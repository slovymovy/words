{
    "word_family": [
        "reception",
        "receiver",
        "receptive",
        "receipt",
        "receivable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "756aaa79-1c83-3025-acf7-ed93fc6ac51b",
                    "sense_definition": "To get or be given something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "get_something",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>received</w> many presents for his birthday.",
                            "target_lang_translations": {
                                "ru": "Он <w>получил</w> много подарков на свой день рождения."
                            }
                        },
                        {
                            "text": "Did you <w>receive</w> the email I sent you yesterday?",
                            "target_lang_translations": {
                                "ru": "Ты <w>получил</w> электронное письмо, которое я отправил тебе вчера?"
                            }
                        }
                    ],
                    "synonyms": [
                        "get",
                        "obtain",
                        "acquire",
                        "gain",
                        "secure"
                    ],
                    "antonyms": [
                        "give"
                    ],
                    "common_phrases": [
                        "receive a letter",
                        "receive information",
                        "receive money",
                        "receive payment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать или принимать что-либо, что дают или посылают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получать",
                                "target_lang_sense_clarification": "несовершенный вид"
                            },
                            {
                                "target_lang_word": "получить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9196365f-a0f0-3c6b-9848-dd1ff5107fe0",
                    "sense_definition": "To formally welcome or greet guests and visitors.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "welcome_guests",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hotel is ready to <w>receive</w> guests for the summer season.",
                            "target_lang_translations": {
                                "ru": "Отель готов <w>принимать</w> гостей в летний сезон."
                            }
                        },
                        {
                            "text": "She stood at the door to <w>receive</w> her visitors warmly.",
                            "target_lang_translations": {
                                "ru": "Она стояла у двери, чтобы тепло <w>принять</w> своих посетителей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "receive guests",
                        "receive visitors"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формально встречать или приветствовать гостей и посетителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "гостей, посетителей"
                            },
                            {
                                "target_lang_word": "встречать",
                                "target_lang_sense_clarification": "приветствовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14e1bacb-fc44-3d01-87bc-84d2adcd5371",
                    "sense_definition": "To catch or take possession of a ball passed by another player in sports.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player ran forward to <w>receive</w> the pass.",
                            "target_lang_translations": {
                                "ru": "Игрок побежал вперед, чтобы <w>принять</w> пас."
                            }
                        },
                        {
                            "text": "He is waiting to <w>receive</w> the serve from his opponent.",
                            "target_lang_translations": {
                                "ru": "Он ждет, чтобы <w>принять</w> подачу от соперника."
                            }
                        }
                    ],
                    "synonyms": [
                        "take",
                        "get"
                    ],
                    "common_phrases": [
                        "receive the ball",
                        "receive a pass",
                        "receive a serve"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ловить или овладевать мячом, переданным другим игроком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "пас, подачу"
                            },
                            {
                                "target_lang_word": "ловить",
                                "target_lang_sense_clarification": "мяч"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d612ab9-9fec-3af2-8ca2-79f60ad27691",
                    "sense_definition": "To formally accept something that is offered.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "accept_offer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had the offer of employment, but he would not <w>receive</w> it.",
                            "target_lang_translations": {
                                "ru": "У него было предложение о работе, но он не хотел его <w>принимать</w>."
                            }
                        },
                        {
                            "text": "The committee was willing to <w>receive</w> their suggestions.",
                            "target_lang_translations": {
                                "ru": "Комитет был готов <w>принять</w> их предложения."
                            }
                        }
                    ],
                    "synonyms": [
                        "accept",
                        "take"
                    ],
                    "common_phrases": [
                        "receive an offer",
                        "receive advice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формально соглашаться на что-либо предложенное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "предложение, совет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0731ad5-fc3e-316b-ba9f-6e88993b1df5",
                    "sense_definition": "To detect or pick up electronic signals or broadcasts.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "detect_signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antenna is designed to <w>receive</w> digital signals from satellites.",
                            "target_lang_translations": {
                                "ru": "Антенна предназначена для того, чтобы <w>принимать</w> цифровые сигналы со спутников."
                            }
                        },
                        {
                            "text": "This old radio can still <w>receive</w> stations from far away.",
                            "target_lang_translations": {
                                "ru": "Это старое радио все еще может <w>ловить</w> станции издалека."
                            }
                        }
                    ],
                    "synonyms": [
                        "get"
                    ],
                    "antonyms": [
                        "lose"
                    ],
                    "common_phrases": [
                        "receive a signal",
                        "receive a message"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнаруживать или улавливать электронные сигналы или трансляции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "сигнал"
                            },
                            {
                                "target_lang_word": "ловить",
                                "target_lang_sense_clarification": "радиостанции, сигнал (разг.)"
                            },
                            {
                                "target_lang_word": "улавливать",
                                "target_lang_sense_clarification": "слабый сигнал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b8f995b-66ae-3e3c-87b1-ca27cb5f1f86",
                    "sense_definition": "To suffer or sustain an injury or blow.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "suffer_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>received</w> a bloody nose from the collision.",
                            "target_lang_translations": {
                                "ru": "Я <w>получил</w> удар по носу в столкновении."
                            }
                        },
                        {
                            "text": "He <w>received</w> a serious injury during the match.",
                            "target_lang_translations": {
                                "ru": "Он <w>получил</w> серьезную травму во время матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "take",
                        "get"
                    ],
                    "common_phrases": [
                        "receive a blow",
                        "receive an injury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пострадать от травмы или удара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получить",
                                "target_lang_sense_clarification": "травму, удар"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e5749de8-0ec5-3f92-8685-59243fb3e69b",
                    "sense_definition": "An operation in which data is accepted by a system.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computing_data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The server logs all sends and <w>receives</w> for security purposes.",
                            "target_lang_translations": {
                                "ru": "Сервер регистрирует все отправки и <w>приёмы</w> в целях безопасности."
                            }
                        },
                        {
                            "text": "We need to optimize the <w>receives</w> to reduce network latency.",
                            "target_lang_translations": {
                                "ru": "Нам нужно оптимизировать <w>приём</w> данных, чтобы уменьшить задержку сети."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Операция, при которой данные принимаются системой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "данных"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
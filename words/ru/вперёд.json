    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "408062dd-ddc3-3499-b815-aaa94321a657",
                    "sense_definition": "В направлении перед собой, куда обращено лицо или взгляд; по ходу движения.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир приказал солдатам двигаться только <вперёд>.",
                            "target_lang_translations": {
                                "en": "The commander ordered the soldiers to move only <w>forward</w>."
                            }
                        },
                        {
                            "text": "Сделай шаг <вперёд> и возьми свою награду.",
                            "target_lang_translations": {
                                "en": "Take a step <w>forward</w> and take your reward."
                            }
                        }
                    ],
                    "antonyms": [
                        "наза́д"
                    ],
                    "common_phrases": [
                        "шаг вперёд",
                        "смотреть вперёд",
                        "идти вперёд",
                        "только вперёд"
                    ],
                    "target_lang_definitions": {
                        "en": "In the direction one is facing or moving; towards the front."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forward",
                                "target_lang_sense_clarification": "movement direction"
                            },
                            {
                                "target_lang_word": "ahead",
                                "target_lang_sense_clarification": "in front"
                            },
                            {
                                "target_lang_word": "onward",
                                "target_lang_sense_clarification": "continuing forward"
                            }
                        ]

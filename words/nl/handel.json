                {
                    "sense_id": "a15952a8-83d4-35a8-9273-e1c43b689a8d",
                    "sense_definition": "Het kopen en verkopen van goederen en diensten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "trade_activity",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f966ed08-0415-3cc3-ad0f-d61c490be129",
                    "sense_definition": "Mieć fizyczną możliwość, siłę lub sprzyjające okoliczności, aby coś zrobić.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety nie <mogę> dzisiaj przyjść do pracy.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я не <могу> сегодня прийти на работу."
                            }
                        },
                        {
                            "text": "Ona <może> podnieść ten ciężki kamień bez problemu.",
                            "target_lang_translations": {
                                "ru": "Она <может> поднять этот тяжелый камень без проблем."
                            }
                        }
                    ],
                    "synonyms": [
                        "umieć /znać"
                    ],
                    "common_phrases": [
                        "nie móc się doczekać",
                        "móc wszystko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь физическую возможность, силу или благоприятные обстоятельства, чтобы что-то сделать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мочь",
                                "target_lang_sense_clarification": "общая способность или возможность"
                            },
                            {
                                "target_lang_word": "быть в состоянии",
                                "target_lang_sense_clarification": "иметь силы или ресурсы для действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43f854d2-59b3-36fe-adec-ff95da54ef54",
                    "sense_definition": "Mieć pozwolenie lub prawo do wykonania jakiejś czynności.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy <mogę> pożyczyć twój długopis na chwilę?",
                            "target_lang_translations": {
                                "ru": "<Могу> ли я одолжить твою ручку на минутку?"
                            }
                        },
                        {
                            "text": "W tym parku nie <można> wyprowadzać psów.",
                            "target_lang_translations": {
                                "ru": "В этом парке <нельзя> выгуливать собак."
                            }
                        }
                    ],
                    "synonyms": [
                        "wolno"
                    ],
                    "common_phrases": [
                        "czy mogę",
                        "nie można"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь разрешение или право на выполнение какого-либо действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мочь",
                                "target_lang_sense_clarification": "иметь позволение"
                            },
                            {
                                "target_lang_word": "можно",
                                "target_lang_sense_clarification": "безличная форма: разрешено"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2766a5b3-1ee1-3399-9e5a-17e8e5194063",
                    "sense_definition": "Forma używana do wyrażania uprzejmych próśb lub pytań.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy <mógłbyś> podać mi sól, proszę?",
                            "target_lang_translations": {
                                "ru": "Не <мог бы> ты передать мне соль, пожалуйста?"
                            }
                        },
                        {
                            "text": "Czy <moglibyśmy> porozmawiać o tym jutro?",
                            "target_lang_translations": {
                                "ru": "<Могли бы> мы поговорить об этом завтра?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "czy mógłbyś",
                        "czy mógłbym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма, используемая для выражения вежливых просьб или вопросов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мочь",
                                "target_lang_sense_clarification": "в условном наклонении (мог бы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d70c496d-777f-324c-a06c-6b1b9bc76f5b",
                    "sense_definition": "Wyraża przypuszczenie, że coś jest możliwe lub prawdopodobne.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <może> być prawda, ale muszę to sprawdzić.",
                            "target_lang_translations": {
                                "ru": "Это <может> быть правдой, но я должен это проверить."
                            }
                        },
                        {
                            "text": "Jutro po południu <może> padać ulewny deszcz.",
                            "target_lang_translations": {
                                "ru": "Завтра днем <может> пойти проливной дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "może",
                        "być może",
                        "możliwe że"
                    ],
                    "common_phrases": [
                        "to może być",
                        "może się zdarzyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает предположение, что что-то возможно или вероятно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "может быть",
                                "target_lang_sense_clarification": "вводное слово или фраза вероятности"
                            },
                            {
                                "target_lang_word": "возможно",
                                "target_lang_sense_clarification": "наречие вероятности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
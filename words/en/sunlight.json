                    "sense_id": "41ca96ee-dca9-3f8a-893c-222eefe9de92",
                    "sense_definition": "Light and energy that comes from the sun.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The room was filled with bright <w>sunlight</w>.",
                            "target_lang_translations": {
                                "ru": "Комната была залита ярким <w>солнечным светом</w>."
                            }
                        },
                        {
                            "text": "Plants need water and <w>sunlight</w> to grow.",
                            "target_lang_translations": {
                                "ru": "Растениям нужны вода и <w>солнечный свет</w> для роста."
                            }
                        }
                    ],
                    "synonyms": [
                        "sunshine",
                        "daylight",
                        "sun"
                    ],
                    "common_phrases": [
                        "bright sunlight",
                        "direct sunlight",
                        "morning sunlight"
                    ],

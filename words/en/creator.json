{
    "word_family": [
        "create",
        "creation",
        "creative",
        "creativity",
        "creature",
        "creatively",
        "creativeness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cbda5067-3ad0-3d30-beb6-aa70882ebb79",
                    "sense_definition": "Someone or something that brings something into existence, such as an invention, artwork, or idea.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_maker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>creator</w> of the popular comic series announced a new book.",
                            "target_lang_translations": {
                                "ru": "<w>Создатель</w> популярной серии комиксов анонсировал новую книгу."
                            }
                        },
                        {
                            "text": "She is the original <w>creator</w> of this innovative software application.",
                            "target_lang_translations": {
                                "ru": "Она является первоначальным <w>создателем</w> этого инновационного программного приложения."
                            }
                        }
                    ],
                    "synonyms": [
                        "maker",
                        "inventor",
                        "originator",
                        "author",
                        "producer"
                    ],
                    "antonyms": [
                        "destroyer"
                    ],
                    "common_phrases": [
                        "creator of",
                        "original creator",
                        "proud creator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто вызывает что-либо к жизни, например изобретение, произведение искусства или идею."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "создатель",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "творец",
                                "target_lang_sense_clarification": "возвышенное, часто об искусстве"
                            },
                            {
                                "target_lang_word": "автор",
                                "target_lang_sense_clarification": "об идеях, книгах или проектах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5327971e-d010-384f-bacb-ed0140f0fe73",
                    "sense_definition": "A person who regularly produces and publishes material, such as videos or posts, on social media platforms.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "content_producer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a full-time <w>creator</w>, he uploads new videos every week.",
                            "target_lang_translations": {
                                "ru": "Будучи профессиональным <w>создателем контента</w>, он загружает новые видео каждую неделю."
                            }
                        },
                        {
                            "text": "Many brands are looking to partner with a popular content <w>creator</w>.",
                            "target_lang_translations": {
                                "ru": "Многие бренды стремятся сотрудничать с популярным <w>создателем</w> контента."
                            }
                        }
                    ],
                    "synonyms": [
                        "influencer"
                    ],
                    "common_phrases": [
                        "content creator",
                        "digital creator",
                        "video creator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который регулярно создает и публикует материалы, такие как видео или посты, в социальных сетях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "создатель контента",
                                "target_lang_sense_clarification": "интернет-термин"
                            },
                            {
                                "target_lang_word": "автор",
                                "target_lang_sense_clarification": "блогер, ведущий канала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1857d080-3598-300e-9b6c-d75bb861e8dd",
                    "sense_definition": "A player in sports who generates scoring opportunities for their teammates.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team needs a skilled <w>creator</w> in midfield to help the strikers score.",
                            "target_lang_translations": {
                                "ru": "Команде нужен опытный <w>созидатель</w> в полузащите, чтобы помочь нападающим забить."
                            }
                        },
                        {
                            "text": "He is more of a <w>creator</w> than a finisher on the field.",
                            "target_lang_translations": {
                                "ru": "На поле он скорее <w>созидатель</w>, чем завершитель атак."
                            }
                        }
                    ],
                    "synonyms": [
                        "playmaker"
                    ],
                    "common_phrases": [
                        "midfield creator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрок в спорте, который создает голевые моменты для своих товарищей по команде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плеймейкер",
                                "target_lang_sense_clarification": "спортивный термин"
                            },
                            {
                                "target_lang_word": "созидатель",
                                "target_lang_sense_clarification": "игрок, строящий атаку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9cf8e0b7-801c-3391-94d0-b1b60675af7b",
                    "sense_definition": "God, viewed as the originator of the universe.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "deity",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "They offered prayers of gratitude to the <w>Creator</w> for the harvest.",
                            "target_lang_translations": {
                                "ru": "Они вознесли благодарственные молитвы <w>Творцу</w> за урожай."
                            }
                        },
                        {
                            "text": "Many people believe that rights are given to them by their <w>Creator</w>.",
                            "target_lang_translations": {
                                "ru": "Многие люди верят, что права дарованы им их <w>Создателем</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "God",
                        "the Almighty",
                        "the Maker"
                    ],
                    "common_phrases": [
                        "the Creator",
                        "praise the Creator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бог, рассматриваемый как первопричина вселенной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Творец",
                                "target_lang_sense_clarification": "Бог"
                            },
                            {
                                "target_lang_word": "Создатель",
                                "target_lang_sense_clarification": "Бог"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
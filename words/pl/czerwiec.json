    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a19c1323-c7f7-3442-a674-7d446f7ef599",
                    "sense_definition": "Szósty miesiąc roku w kalendarzu gregoriańskim, mający 30 dni, występujący po maju i przed lipcem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "months",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <czerwcu> zaczynają się długo wyczekiwane wakacje szkolne.",
                            "target_lang_translations": {
                                "ru": "В <июне> начинаются долгожданные школьные каникулы."
                            }
                        },
                        {
                            "text": "Mój brat ma urodziny pierwszego <czerwca>.",
                            "target_lang_translations": {
                                "ru": "У моего брата день рождения первого <июня>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w czerwcu",
                        "początek czerwca",
                        "koniec czerwca",
                        "ciepły czerwiec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шестой месяц года в григорианском календаре, следующий за маем и предшествующий июлю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "июнь",
                                "target_lang_sense_clarification": "месяц"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24641485-6121-3246-bda9-0c02f9bf5664",
                    "sense_definition": "Owad z nadrodziny pluskwiaków, z którego dawniej pozyskiwano czerwony barwnik.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "insects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawniej <czerwiec> polski był cennym towarem eksportowym.",
                            "target_lang_translations": {
                                "ru": "Раньше польский <червец> был ценным экспортным товаром."
                            }
                        },
                        {
                            "text": "Biolog badał cykl rozwojowy <czerwca> znalezionego na łące.",
                            "target_lang_translations": {
                                "ru": "Биолог изучал цикл развития <червеца>, найденного на лугу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Насекомое из надсемейства червецов, из которого раньше получали красный краситель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "червец",
                                "target_lang_sense_clarification": "насекомое"
                            },
                            {
                                "target_lang_word": "кошениль",
                                "target_lang_sense_clarification": "насекомое/краситель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff145b5d-b0a9-3457-8d6a-9e725835867a",
                    "sense_definition": "Niewielka roślina zielna z rodziny goździkowatych, występująca na piaszczystych glebach.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "plants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Czerwiec> roczny jest pospolitym chwastem rosnącym na polach.",
                            "target_lang_translations": {
                                "ru": "<Дивала> однолетняя — распространённый сорняк, растущий на полях."
                            }
                        },
                        {
                            "text": "W atlasie botanicznym znalazłem dokładny opis <czerwca>.",
                            "target_lang_translations": {
                                "ru": "В ботаническом атласе я нашёл подробное описание <дивалы>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое травянистое растение семейства гвоздичных, растущее на песчаных почвах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дивала",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f7730208-0609-3056-af10-44388ebc8388",
                    "sense_definition": "Polskie nazwisko męskie lub żeńskie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Czerwiec> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <Червец> был избран председателем совета микрорайона."
                            }
                        },
                        {
                            "text": "Wczoraj listonosz przyniósł paczkę dla państwa <Czerwców>.",
                            "target_lang_translations": {
                                "ru": "Вчера почтальон принёс посылку для четы <Червецов>."
                            }
                        }

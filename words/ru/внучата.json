    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ca87114c-b649-3d6c-ac20-cb9a0eaefc79",
                    "sense_definition": "Разговорное, часто ласковое название для внуков и внучек (детей сына или дочери).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "descendants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка всегда с нетерпением ждёт, когда к ней в деревню приедут любимые <w>внучата</w>.",
                            "target_lang_translations": {
                                "en": "Grandma always looks forward to when her beloved <w>grandkids</w> come to visit her in the village."
                            }
                        },
                        {
                            "text": "Мать души в Дарье не чаяла, до гроба была для неё и для <w>внучат</w> добровольной, покорной рабой.",
                            "target_lang_translations": {
                                "en": "The mother doted on Darya and was a willing, submissive slave for her and the <w>grandchildren</w> to her dying day."
                            }
                        }
                    ],
                    "synonyms": [
                        "внуки"
                    ],
                    "common_phrases": [
                        "мои внучата",
                        "любимые внучата",
                        "малые внучата"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A colloquial, often affectionate term for grandsons and granddaughters (children of a son or daughter)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grandkids",
                                "target_lang_sense_clarification": "colloquial, affectionate"
                            },
                            {
                                "target_lang_word": "grandchildren",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a63445c-d412-30c0-a65b-5461f325e068",
                    "sense_definition": "Множественное число слова <w>внучонок</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "descendants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Один <w>внучонок</w> играл в песочнице, а остальные <w>внучата</w> катались на качелях.",
                            "target_lang_translations": {
                                "en": "One little grandchild was playing in the sandbox, while the other <w>grandkids</w> were swinging on the swings."
                            }
                        },
                        {
                            "text": "Пока кошка кормила котят, наши <w>внучата</w> с интересом наблюдали за ними.",
                            "target_lang_translations": {
                                "en": "While the cat was feeding the kittens, our <w>grandkids</w> watched them with interest."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural form of the word \"vnuchonok\" (diminutive for grandchild)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "little grandchildren",
                                "target_lang_sense_clarification": "diminutive plural"
                            },
                            {
                                "target_lang_word": "grandkids",
                                "target_lang_sense_clarification": "colloquial"
                            }
                        ]
                    }
                }
            ]
        }
    ]

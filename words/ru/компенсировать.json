{
    "word_family": [
        "компенсация",
        "компенсатор",
        "компенсаторный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9404493b-25ed-3f5e-af72-a9344b9df92f",
                    "sense_definition": "Возмещать потраченные средства, убытки или причинённый ущерб.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания согласилась полностью <компенсировать> сотрудникам расходы на командировку.",
                            "target_lang_translations": {
                                "en": "The company agreed to fully <reimburse> the employees for their business trip expenses."
                            }
                        },
                        {
                            "text": "Страховая фирма отказалась <компенсировать> ущерб после аварии.",
                            "target_lang_translations": {
                                "en": "The insurance firm refused to <compensate> for the damage after the accident."
                            }
                        }
                    ],
                    "synonyms": [
                        "возмещать"
                    ],
                    "common_phrases": [
                        "компенсировать расходы",
                        "компенсировать ущерб",
                        "компенсировать затраты"
                    ],
                    "target_lang_definitions": {
                        "en": "To pay someone money because they have suffered injury, loss, or damage, or to repay expenses."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "compensate",
                                "target_lang_sense_clarification": "to pay for loss or damage"
                            },
                            {
                                "target_lang_word": "repay",
                                "target_lang_sense_clarification": "to pay back money spent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aaf865a1-2321-3b9a-839e-eb7a1d397772",
                    "sense_definition": "Сглаживать недостатки, замещать отсутствие чего-либо или уравновешивать негативное влияние.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент пытался <компенсировать> недостаток знаний своей активностью на лекциях.",
                            "target_lang_translations": {
                                "en": "The student tried to <make up for> the lack of knowledge with his activity at lectures."
                            }
                        },
                        {
                            "text": "Организм умеет <компенсировать> небольшие сбои в работе внутренних систем.",
                            "target_lang_translations": {
                                "en": "The body is able to <compensate for> small malfunctions in the internal systems."
                            }
                        }
                    ],
                    "synonyms": [
                        "восполнять",
                        "уравновешивать"
                    ],
                    "common_phrases": [
                        "компенсировать недостаток",
                        "компенсировать отсутствие",
                        "компенсировать слабость"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide something good to balance or reduce the bad effects of damage, loss, or a lack of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "compensate",
                                "target_lang_sense_clarification": "to counterbalance a deficiency"
                            },
                            {
                                "target_lang_word": "make up",
                                "target_lang_sense_clarification": "to fill a gap or deficiency (usually with 'for')"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
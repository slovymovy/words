{
    "word_family": [
        "hailstorm",
        "hailstone"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "833ae873-7571-3578-ae8a-cdb8190e2bc4",
                    "sense_definition": "Small balls or pieces of ice that fall from the sky as precipitation, usually during thunderstorms.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weather_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy <w>hail</w> damaged the roof of the car.",
                            "target_lang_translations": {
                                "ru": "Сильный <w>град</w> повредил крышу автомобиля."
                            }
                        },
                        {
                            "text": "We ran for cover as soon as the <w>hail</w> started falling.",
                            "target_lang_translations": {
                                "ru": "Мы побежали в укрытие, как только пошел <w>град</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heavy hail",
                        "hail damage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мелкие шарики или кусочки льда, падающие с неба в виде осадков, обычно во время грозы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "град",
                                "target_lang_sense_clarification": "атмосферные осадки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffa83c23-2d90-3dd8-a55f-7e62982859a8",
                    "sense_definition": "A large number of small objects or projectiles thrown or falling rapidly at the same time.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "barrage_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers advanced despite the <w>hail</w> of arrows.",
                            "target_lang_translations": {
                                "ru": "Солдаты наступали, несмотря на <w>град</w> стрел."
                            }
                        },
                        {
                            "text": "The politician faced a <w>hail</w> of questions from the reporters.",
                            "target_lang_translations": {
                                "ru": "Политик столкнулся с <w>градом</w> вопросов от репортеров."
                            }
                        }
                    ],
                    "synonyms": [
                        "barrage",
                        "volley",
                        "shower",
                        "storm"
                    ],
                    "common_phrases": [
                        "hail of bullets",
                        "hail of arrows",
                        "hail of abuse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество мелких предметов или снарядов, брошенных или падающих быстро и одновременно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "град",
                                "target_lang_sense_clarification": "переносное значение: поток, обилие"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "множество чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0ea808f5-a11f-3e4b-87b8-e6a4369c0164",
                    "sense_definition": "To rain small balls of ice from the sky.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weather_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It started to <w>hail</w> just as we left the house.",
                            "target_lang_translations": {
                                "ru": "Как только мы вышли из дома, пошел <w>град</w>."
                            }
                        },
                        {
                            "text": "If it <w>hails</w> later, we should move the plants inside.",
                            "target_lang_translations": {
                                "ru": "Если позже пойдет <w>град</w>, нам следует занести растения в дом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "start to hail"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпадать в виде града (об осадках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "о граде"
                            },
                            {
                                "target_lang_word": "пойти",
                                "target_lang_sense_clarification": "начаться (о граде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51cfc8e1-3d05-384b-b5e0-91576804eece",
                    "sense_definition": "To signal to a taxi or bus driver to stop so that one can get on.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "signal_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>hail</w> a taxi to get to the airport on time.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>поймать</w> такси, чтобы вовремя попасть в аэропорт."
                            }
                        },
                        {
                            "text": "She stood on the corner <w>hailing</w> a cab in the rain.",
                            "target_lang_translations": {
                                "ru": "Она стояла на углу, <w>пытаясь поймать</w> такси под дождем."
                            }
                        }
                    ],
                    "synonyms": [
                        "flag down",
                        "signal"
                    ],
                    "common_phrases": [
                        "hail a taxi",
                        "hail a cab"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сигнализировать водителю такси или автобуса с просьбой остановиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ловить",
                                "target_lang_sense_clarification": "такси или машину"
                            },
                            {
                                "target_lang_word": "останавливать",
                                "target_lang_sense_clarification": "транспорт жестом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7a07332-e261-3fa5-85a8-de345b2f55bb",
                    "sense_definition": "To praise or describe someone or something enthusiastically, often assigning them a specific title or status.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "acclaim_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new movie was <w>hailed</w> as a masterpiece by critics.",
                            "target_lang_translations": {
                                "ru": "Новый фильм был <w>провозглашен</w> критиками шедевром."
                            }
                        },
                        {
                            "text": "He was <w>hailed</w> as a hero for saving the child from the fire.",
                            "target_lang_translations": {
                                "ru": "Его <w>назвали</w> героем за спасение ребенка из огня."
                            }
                        }
                    ],
                    "synonyms": [
                        "acclaim",
                        "praise",
                        "laud"
                    ],
                    "antonyms": [
                        "criticize",
                        "condemn"
                    ],
                    "common_phrases": [
                        "hailed as a hero",
                        "hailed as a success"
                    ],
                    "target_lang_definitions": {
                        "ru": "С энтузиазмом хвалить или описывать кого-то, часто присваивая определенный титул или статус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провозглашать",
                                "target_lang_sense_clarification": "объявлять с похвалой"
                            },
                            {
                                "target_lang_word": "называть",
                                "target_lang_sense_clarification": "давать звание или статус"
                            },
                            {
                                "target_lang_word": "признавать",
                                "target_lang_sense_clarification": "считать кем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf61c9b4-02d4-371a-80c0-f82eea43bc01",
                    "sense_definition": "To come from or originate from a specific place.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "origin_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandfather <w>hails</w> from a small village in Italy.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка <w>родом</w> из маленькой деревни в Италии."
                            }
                        },
                        {
                            "text": "The band <w>hails</w> from London.",
                            "target_lang_translations": {
                                "ru": "Группа <w>родом</w> из Лондона."
                            }
                        }
                    ],
                    "synonyms": [
                        "originate",
                        "come"
                    ],
                    "common_phrases": [
                        "hail from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить или быть родом из определенного места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть родом",
                                "target_lang_sense_clarification": "происходить откуда-либо"
                            },
                            {
                                "target_lang_word": "происходить",
                                "target_lang_sense_clarification": "иметь истоки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79803ee0-9586-3c4e-9a9d-39d2c2f4f595",
                    "sense_definition": "To greet or salute someone with respect or enthusiasm.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "greet_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd <w>hailed</w> the king as he entered the city.",
                            "target_lang_translations": {
                                "ru": "Толпа <w>приветствовала</w> короля, когда он въезжал в город."
                            }
                        },
                        {
                            "text": "We <w>hail</w> thee, emperor!",
                            "target_lang_translations": {
                                "ru": "Мы <w>приветствуем</w> тебя, император!"
                            }
                        }
                    ],
                    "synonyms": [
                        "greet",
                        "salute",
                        "welcome"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приветствовать или салютовать кому-либо с уважением или энтузиазмом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приветствовать",
                                "target_lang_sense_clarification": "здороваться с уважением"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "99f5b171-6663-32c5-8bdb-e0588a7087dd",
                    "sense_definition": "An exclamation used to express greeting, acclaim, or respect.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "greet_intj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hail</w>, Caesar!",
                            "target_lang_translations": {
                                "ru": "<w>Да здравствует</w> Цезарь!"
                            }
                        },
                        {
                            "text": "<w>Hail</w> to the victors.",
                            "target_lang_translations": {
                                "ru": "<w>Слава</w> победителям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hail to the chief"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, используемый для выражения приветствия, одобрения или уважения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да здравствует",
                                "target_lang_sense_clarification": "приветственный возглас"
                            },
                            {
                                "target_lang_word": "слава",
                                "target_lang_sense_clarification": "возглас почтения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "aba028fb-56d4-3fda-bb6d-8850c3067694",
                    "sense_definition": "A province and city in Saudi Arabia.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "place_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "He traveled to <w>Hail</w> for business.",
                            "target_lang_translations": {
                                "ru": "Он поехал в <w>Хаиль</w> по делам."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Провинция и город в Саудовской Аравии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хаиль",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14038959-0ac5-3de3-8794-59198e299de0",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Hail</w> will see you now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Хейл</w> примет вас сейчас."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хейл",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cfc9f746-46bf-397c-a3e5-073f029c11d6",
                    "sense_definition": "Healthy, whole, or safe.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "healthy_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He returned from the war <w>hail</w> and strong.",
                            "target_lang_translations": {
                                "ru": "Он вернулся с войны <w>здоровым</w> и сильным."
                            }
                        }
                    ],
                    "synonyms": [
                        "hale",
                        "healthy",
                        "robust"
                    ],
                    "antonyms": [
                        "ill",
                        "sick"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Usually spelled 'hale' in modern English."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Здоровый, целый или невредимый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здоровый",
                                "target_lang_sense_clarification": "физически благополучный"
                            },
                            {
                                "target_lang_word": "крепкий",
                                "target_lang_sense_clarification": "сильный и здоровый"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
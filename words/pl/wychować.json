    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9edcf1e3-a2c3-3c89-88ea-95dec3af11a5",
                    "sense_definition": "Zapewnić opiekę dziecku lub młodemu zwierzęciu, dbając o jego rozwój fizyczny i psychiczny aż do osiągnięcia samodzielności.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice ciężko pracowali, aby godnie <w>wychować</w> swoje dzieci.",
                            "target_lang_translations": {
                                "ru": "Родители много работали, чтобы достойно <w>воспитать</w> своих детей."
                            }
                        },
                        {
                            "text": "Samotna matka zdołała <w>wychować</w> trzech synów na porządnych ludzi.",
                            "target_lang_translations": {
                                "ru": "Одинокая мать сумела <w>вырастить</w> трех сыновей порядочными людьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykształcić"
                    ],
                    "common_phrases": [
                        "wychować dzieci",
                        "wychować syna",
                        "samotnie wychować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечить уход за ребенком или детенышем, заботясь о его физическом и психическом развитии до достижения самостоятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырастить",
                                "target_lang_sense_clarification": "вскормить, вырастить до взрослого возраста"
                            },
                            {
                                "target_lang_word": "воспитать",
                                "target_lang_sense_clarification": "взрастить, дать воспитание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2f64fb9-9bfd-3b88-9258-179b62a42f53",
                    "sense_definition": "Ukształtować czyjś charakter, wpoić zasady dobrego zachowania, kulturę osobistą lub przekazać wiedzę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel starał się <w>wychować</w> uczniów w duchu patriotyzmu.",
                            "target_lang_translations": {
                                "ru": "Учитель старался <w>воспитать</w> учеников в духе патриотизма."
                            }
                        },
                        {
                            "text": "Trudno jest dobrze <w>wychować</w> człowieka bez dawania mu dobrego przykładu.",
                            "target_lang_translations": {
                                "ru": "Трудно хорошо <w>воспитать</w> человека, не подавая ему хорошего примера."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyedukować",
                        "nauczyć",
                        "wykształcić"
                    ],
                    "common_phrases": [
                        "dobrze wychować",
                        "źle wychować",
                        "wychować pokolenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сформировать чей-либо характер, привить принципы хорошего поведения, культуру или передать знания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воспитать",
                                "target_lang_sense_clarification": "привить навыки поведения и черты характера"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "924e479e-1c86-3914-8ee6-8b3737b2e878",
                    "sense_definition": "Spędzić dzieciństwo i okres dorastania w określonym środowisku lub warunkach.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek <w>wychował</w> się w małej wiosce na południu Polski.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка <w>вырос</w> в маленькой деревне на юге Польши."
                            }
                        },
                        {
                            "text": "Oni <w>wychowali</w> się na tym samym podwórku i znają się od lat.",
                            "target_lang_translations": {
                                "ru": "Они <w>выросли</w> в одном дворе и знают друг друга много лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykształcić",
                        "nauczyć"
                    ],
                    "common_phrases": [
                        "wychować się na wsi",
                        "wychować się w mieście",
                        "wychować się w biedzie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Провести детство и период взросления в определенной среде или условиях (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырасти",
                                "target_lang_sense_clarification": "стать взрослым, провести детство"
                            },
                            {
                                "target_lang_word": "воспитаться",
                                "target_lang_sense_clarification": "получить воспитание в определенной среде"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
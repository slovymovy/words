    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1d9548e-43b1-3e37-8c21-931521b7418d",
                    "sense_definition": "A person who observes and identifies wild birds as a hobby.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>birder</w> used high-quality binoculars to spot the eagle in the distance.",
                            "target_lang_translations": {
                                "ru": "<w>Орнитолог-любитель</w> использовал высококачественный бинокль, чтобы заметить орла вдалеке."
                            }
                        },
                        {
                            "text": "As an avid <w>birder</w>, she spends every weekend in the forest looking for rare owls.",
                            "target_lang_translations": {
                                "ru": "Будучи заядлым <w>бёрдвотчером</w>, она проводит каждые выходные в лесу в поисках редких сов."
                            }
                        }
                    ],
                    "synonyms": [
                        "birdwatcher"
                    ],
                    "common_phrases": [
                        "avid birder",
                        "keen birder",
                        "experienced birder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который наблюдает за дикими птицами и определяет их виды в качестве хобби."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орнитолог-любитель",
                                "target_lang_sense_clarification": "любитель, изучающий птиц"
                            },
                            {
                                "target_lang_word": "бёрдвотчер",
                                "target_lang_sense_clarification": "современный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8166df2-9ee7-36e9-8fab-f3c08a5fc9c6",
                    "sense_definition": "A person who hunts or catches birds.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>birder</w> returned from the marsh with two ducks for dinner.",
                            "target_lang_translations": {
                                "ru": "<w>Охотник на птиц</w> вернулся с болота с двумя утками к ужину."
                            }
                        },
                        {
                            "text": "In the past, a professional <w>birder</w> would catch wild game to sell at the market.",
                            "target_lang_translations": {
                                "ru": "В прошлом профессиональный <w>птицелов</w> ловил дичь для продажи на рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "fowler",
                        "bird hunter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который охотится на птиц или ловит их."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "птицелов",
                                "target_lang_sense_clarification": "ловец птиц"
                            },
                            {
                                "target_lang_word": "охотник на птиц",
                                "target_lang_sense_clarification": "охотник"
                            }
                        ]
                    }
                }
            ]
        }
    ]

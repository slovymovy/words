                {
                    "sense_id": "98a165f7-5b1c-321d-92e5-915aed48969a",
                    "sense_definition": "Een lijst van kerkelijke feesten en heiligendagen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_ecclesiastical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De priester raadpleegde de <w>kalender</w> voor de viering van de heilige.",
                            "target_lang_translations": {
                                "en": "The priest consulted the <w>calendar</w> for the celebration of the saint.",
                                "ru": "Священник сверился с <w>календарем</w> для празднования дня святого."
                            }

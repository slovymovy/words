                        "узелковое письмо",
                        "предметное письмо",
                        "русское письмо"
                    ],
                    "target_lang_definitions": {
                        "en": "A system of graphic symbols used to represent speech in a language."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "writing",
                                "target_lang_sense_clarification": "system of recording language"
                            },

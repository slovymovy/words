    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c25ce4d-2d37-3a26-95af-67252c100885",
                    "sense_definition": "A system that allows a customer to obtain goods or services before payment, based on the trust that payment will be made in the future.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "finance_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not have enough cash with me, so I will pay for the groceries by <w>credit</w> card.",
                            "target_lang_translations": {
                                "ru": "У меня с собой недостаточно наличных, поэтому я расплачусь за продукты <w>кредитной</w> картой."
                            }
                        },
                        {
                            "text": "The shop refused to extend <w>credit</w> to customers who had a history of not paying their bills.",
                            "target_lang_translations": {
                                "ru": "Магазин отказался предоставлять <w>кредит</w> покупателям, у которых была история неуплаты счетов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "credit card",
                        "buy on credit",
                        "extend credit",
                        "store credit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система, позволяющая клиенту получать товары или услуги до оплаты, основанная на доверии, что оплата будет произведена в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "креди́т",
                                "target_lang_sense_clarification": "финансы"
                            },
                            {
                                "target_lang_word": "рассро́чка",
                                "target_lang_sense_clarification": "оплата частями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1641bd2-deb5-30d7-936c-3ba57cf87302",
                    "sense_definition": "A list of the people who worked on a movie, television program, or other production, usually shown at the beginning or end.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "media_recognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We stayed in the movie theater until the lights came on to watch the closing <w>credits</w>.",
                            "target_lang_translations": {
                                "ru": "Мы оставались в кинотеатре, пока не включили свет, чтобы посмотреть финальные <w>титры</w>."
                            }
                        },
                        {
                            "text": "Her name appeared in the opening <w>credits</w> because she was the lead actress.",
                            "target_lang_translations": {
                                "ru": "Её имя появилось в начальных <w>титрах</w>, потому что она была исполнительницей главной роли."
                            }
                        }
                    ],
                    "common_phrases": [
                        "closing credits",
                        "opening credits",
                        "roll the credits"
                    ],
                    "target_lang_definitions": {
                        "ru": "Список людей, работавших над фильмом или телепрограммой, обычно показываемый в начале или в конце."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ти́тры",
                                "target_lang_sense_clarification": "в кино"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85cd8128-6c75-323a-9d8e-b072a9ebbf96",
                    "sense_definition": "A unit of study at a school or university that represents the successful completion of a course.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to earn three more <w>credits</w> in science to graduate from the university.",
                            "target_lang_translations": {
                                "ru": "Мне нужно получить ещё три <w>зачётные единицы</w> по естественным наукам, чтобы закончить университет."
                            }
                        },
                        {
                            "text": "The teacher gave the student extra <w>credit</w> for completing the optional assignment.",
                            "target_lang_translations": {
                                "ru": "Учитель поставил студенту дополнительный <w>балл</w> за выполнение необязательного задания."
                            }
                        }
                    ],
                    "synonyms": [
                        "unit"
                    ],
                    "common_phrases": [
                        "extra credit",
                        "course credit",
                        "earn credit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица обучения в школе или университете, обозначающая успешное завершение курса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачётная едини́ца",
                                "target_lang_sense_clarification": "учёба"
                            },
                            {
                                "target_lang_word": "креди́т",
                                "target_lang_sense_clarification": "образование"
                            },
                            {
                                "target_lang_word": "зачёт",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82c25647-c64b-3154-83b3-ead39f30803a",
                    "sense_definition": "Praise, approval, or recognition given to someone for doing something good or achieving something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "praise_recognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must give her <w>credit</w> for managing such a difficult project all by herself.",
                            "target_lang_translations": {
                                "ru": "Вы должны отдать ей <w>должное</w> за то, что она справилась с таким сложным проектом совершенно самостоятельно."
                            }
                        },
                        {
                            "text": "To his <w>credit</w>, he admitted that he was wrong and apologized immediately.",
                            "target_lang_translations": {
                                "ru": "К его <w>чести</w>, он признал, что был неправ, и немедленно извинился."
                            }
                        }
                    ],
                    "common_phrases": [
                        "give credit",
                        "take credit",
                        "to his credit",
                        "get credit for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Похвала, одобрение или признание, выражаемые кому-либо за хороший поступок или достижение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до́лжное",
                                "target_lang_sense_clarification": "признание заслуг"
                            },
                            {
                                "target_lang_word": "заслу́га",
                                "target_lang_sense_clarification": "достижение"
                            },
                            {
                                "target_lang_word": "призна́ние",
                                "target_lang_sense_clarification": "одобрение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "323d7949-ed4f-326b-948b-6f9fe64e94b0",
                    "sense_definition": "A person's financial reputation, specifically their history of paying debts and their ability to borrow money.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "finance_payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Having a low <w>credit</w> score can make it very difficult to get a mortgage for a house.",
                            "target_lang_translations": {
                                "ru": "Низкий <w>кредитный</w> рейтинг может сильно затруднить получение ипотеки на дом."
                            }
                        },
                        {
                            "text": "He worked hard to repair his bad <w>credit</w> after years of financial struggle.",
                            "target_lang_translations": {
                                "ru": "Он усердно работал над исправлением своей плохой <w>кредитной истории</w> после долгих лет финансовых трудностей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "credit rating",
                        "credit score",
                        "bad credit",
                        "good credit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Финансовая репутация человека, в частности история выплаты долгов и способность брать деньги в долг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "креди́тная исто́рия",
                                "target_lang_sense_clarification": "репутация"
                            },
                            {
                                "target_lang_word": "кредитоспосо́бность",
                                "target_lang_sense_clarification": "финансы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be94040f-ffb2-3348-a50c-a2394880325d",
                    "sense_definition": "A unit of value or currency used in games, virtual systems, or fictional worlds.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gaming_currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to insert two <w>credits</w> into the machine to play the arcade game.",
                            "target_lang_translations": {
                                "ru": "Чтобы сыграть в аркадную игру, нужно внести в автомат два <w>кредита</w>."
                            }
                        },
                        {
                            "text": "The repair of the starship will cost the captain fifty thousand galactic <w>credits</w>.",
                            "target_lang_translations": {
                                "ru": "Ремонт звездолёта обойдется капитану в пятьдесят тысяч галактических <w>кредитов</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "insert credits",
                        "virtual credits"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица стоимости или валюты, используемая в играх, виртуальных системах или вымышленных мирах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "креди́т",
                                "target_lang_sense_clarification": "в играх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9be3af0-82fe-3527-b180-2afdd5822c60",
                    "sense_definition": "An entry recording a sum of money received, usually listed on the right-hand side of an account.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "finance_accounting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accountant noticed a large <w>credit</w> in the company ledger that had not been explained.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер заметил в гроссбухе компании крупный <w>кредит</w>, который не был объяснён."
                            }
                        },
                        {
                            "text": "Please check the <w>credit</w> column to see all the payments that have come in this month.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте графу <w>кредита</w>, чтобы увидеть все платежи, поступившие в этом месяце."
                            }
                        }
                    ],
                    "antonyms": [
                        "debit"
                    ],
                    "common_phrases": [
                        "credit side",
                        "credit balance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запись о полученной сумме денег, обычно расположенная на правой стороне счёта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "креди́т",
                                "target_lang_sense_clarification": "бухгалтерия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71701553-f455-3876-8ae0-88595aad9c9d",
                    "sense_definition": "A person or thing that brings honor, respect, or a good reputation to a group or organization.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "praise_recognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your intelligent and polite daughter is a real <w>credit</w> to your family.",
                            "target_lang_translations": {
                                "ru": "Ваша умная и вежливая дочь делает настоящую <w>честь</w> вашей семье."
                            }
                        },
                        {
                            "text": "He is a hardworking player and a <w>credit</w> to the entire football team.",
                            "target_lang_translations": {
                                "ru": "Он трудолюбивый игрок и <w>честь</w> для всей футбольной команды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "credit to the team",
                        "credit to the community"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или вещь, приносящие честь, уважение или хорошую репутацию группе или организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "честь",
                                "target_lang_sense_clarification": "гордость"
                            },
                            {
                                "target_lang_word": "украше́ние",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1fcc430-23d6-385f-bc1e-b98e58d86bbb",
                    "sense_definition": "Belief, faith, or trust in the truth of something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "belief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police gave no <w>credit</w> to the suspect's story because he had lied so many times before.",
                            "target_lang_translations": {
                                "ru": "Полиция не придала <w>веры</w> рассказу подозреваемого, потому что он уже много раз лгал."
                            }
                        },
                        {
                            "text": "I place little <w>credit</w> in rumors that are spread through social media.",
                            "target_lang_translations": {
                                "ru": "Я мало <w>верю</w> слухам, которые распространяются через социальные сети."
                            }
                        }
                    ],
                    "common_phrases": [
                        "give credit to",
                        "place credit in"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often used in negative constructions"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вера или доверие к истинности чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ве́ра",
                                "target_lang_sense_clarification": "убеждённость"
                            },
                            {
                                "target_lang_word": "дове́рие",
                                "target_lang_sense_clarification": "к словам"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2d84dccb-ae36-3816-b2d1-096828f46189",
                    "sense_definition": "To publicly acknowledge that someone is responsible for doing something or has a particular quality.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "praise_recognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist is <w>credited</w> with discovering a cure for the rare disease.",
                            "target_lang_translations": {
                                "ru": "Этому ученому <w>приписывают</w> открытие лекарства от редкой болезни."
                            }
                        },
                        {
                            "text": "I <w>credit</w> my parents for teaching me the value of hard work and honesty.",
                            "target_lang_translations": {
                                "ru": "Я <w>благодарен</w> своим родителям за то, что они научили меня ценить труд и честность."
                            }
                        }
                    ],
                    "common_phrases": [
                        "credit someone with",
                        "be credited with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публично признавать, что кто-то несет ответственность за совершение чего-либо или обладает определенным качеством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "припи́сывать",
                                "target_lang_sense_clarification": "авторство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1113921-958b-3854-8453-f871aec307fd",
                    "sense_definition": "To add money to a bank account or record a payment in a financial record.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "finance_accounting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bank will <w>credit</w> your account with the refund within three business days.",
                            "target_lang_translations": {
                                "ru": "Банк <w>зачислит</w> возврат средств на ваш счет в течение трех рабочих дней."
                            }
                        },
                        {
                            "text": "We have <w>credited</w> the full amount of the return to your monthly bill.",
                            "target_lang_translations": {
                                "ru": "Мы <w>зачислили</w> полную сумму возврата на ваш ежемесячный счет."
                            }
                        }
                    ],
                    "antonyms": [
                        "debit"
                    ],
                    "common_phrases": [
                        "credit the account",
                        "credit your account"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавлять деньги на банковский счет или записывать платеж в финансовом отчете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачисля́ть",
                                "target_lang_sense_clarification": "на счёт"
                            },
                            {
                                "target_lang_word": "кредитова́ть",
                                "target_lang_sense_clarification": "счёт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b07105fe-04ff-3dc9-abf2-2d8673532d5a",
                    "sense_definition": "To believe that something is true, especially when it is surprising or difficult to accept.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "belief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I could hardly <w>credit</w> the news when I heard that they had won the lottery.",
                            "target_lang_translations": {
                                "ru": "Я едва мог <w>поверить</w> в эту новость, когда услышал, что они выиграли в лотерею."
                            }
                        },
                        {
                            "text": "She found it difficult to <w>credit</w> his strange explanation for being late.",
                            "target_lang_translations": {
                                "ru": "Ей было трудно <w>поверить</w> в его странное объяснение опоздания."
                            }
                        }
                    ],
                    "synonyms": [
                        "believe",
                        "accept"
                    ],
                    "common_phrases": [
                        "hardly credit",
                        "scarcely credit"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Верить, что что-то является правдой, особенно когда это удивительно или трудно принять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ве́рить",
                                "target_lang_sense_clarification": "считать правдой"
                            },
                            {
                                "target_lang_word": "пове́рить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a58ff5a1-d42e-3a93-9972-52bc24d528de",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Credit</w> signed the guestbook at the hotel reception.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Кредит</w> расписался в гостевой книге на стойке регистрации отеля."
                            }
                        },
                        {
                            "text": "We looked up the history of the <w>Credit</w> family in the local archives.",
                            "target_lang_translations": {
                                "ru": "Мы искали историю семьи <w>Кредит</w> в местных архивах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кре́дит",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
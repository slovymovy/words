{
    "word_family": [
        "sercowy",
        "serduszko",
        "serducho"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0806c78a-0078-3863-88bc-35004b57665c",
                    "sense_definition": "Centralny narząd układu krwionośnego, który pompuje krew w organizmie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz uważnie osłuchał moje <serce> stetoskopem.",
                            "target_lang_translations": {
                                "ru": "Врач внимательно послушал моё <сердце> стетоскопом."
                            }
                        },
                        {
                            "text": "Zdrowe <serce> jest kluczowe dla długiego życia.",
                            "target_lang_translations": {
                                "ru": "Здоровое <сердце> — залог долгой жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "pikawa"
                    ],
                    "common_phrases": [
                        "bicie serca",
                        "zawał serca",
                        "przeszczep serca",
                        "choroba serca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Центральный орган кровеносной системы, который перекачивает кровь в организме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сердце",
                                "target_lang_sense_clarification": "орган"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3b0a6a3-5fa3-33c1-81ce-c2653e06ae4d",
                    "sense_definition": "Symboliczny kształt oznaczający miłość, zwykle czerwony, z dwoma łukami u góry i szpicem u dołu.",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko narysowało wielkie czerwone <serce> na laurce dla mamy.",
                            "target_lang_translations": {
                                "ru": "Ребёнок нарисовал большое красное <сердце> на открытке для мамы."
                            }
                        },
                        {
                            "text": "Kupiłem jej naszyjnik w kształcie małego <serca>.",
                            "target_lang_translations": {
                                "ru": "Я купил ей ожерелье в форме маленького <сердца>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w kształcie serca",
                        "piernikowe serce",
                        "rysować serca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Символическая форма, обозначающая любовь, обычно красного цвета, с двумя дугами сверху и острым углом снизу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сердце",
                                "target_lang_sense_clarification": "символ"
                            },
                            {
                                "target_lang_word": "сердечко",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f26bee8-8885-36f8-beb9-ac6835de5f24",
                    "sense_definition": "Siedlisko uczuć, emocji i nastrojów człowieka, zwłaszcza miłości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej słowa głęboko zapadły mi w <serce>.",
                            "target_lang_translations": {
                                "ru": "Её слова глубоко запали мне в <сердце>."
                            }
                        },
                        {
                            "text": "Mimo upływu lat, wciąż noszę cię w moim <sercu>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на прошедшие годы, я всё ещё ношу тебя в своём <сердце>."
                            }
                        }
                    ],
                    "synonyms": [
                        "psychika",
                        "charakter"
                    ],
                    "common_phrases": [
                        "złamać serce",
                        "z całego serca",
                        "w głębi serca",
                        "słuchać głosu serca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Средоточие чувств, эмоций и настроений человека, особенно любви."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сердце",
                                "target_lang_sense_clarification": "чувства"
                            },
                            {
                                "target_lang_word": "душа",
                                "target_lang_sense_clarification": "внутренний мир"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00a5839c-22d2-35de-b4ea-ceca9942979b",
                    "sense_definition": "Pieszczotliwe określenie ukochanej osoby.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie płacz, moje <serce>, wszystko się ułoży.",
                            "target_lang_translations": {
                                "ru": "Не плачь, моё <сердце>, всё образуется."
                            }
                        },
                        {
                            "text": "Witaj, <serce>, jak minął ci dzień w pracy?",
                            "target_lang_translations": {
                                "ru": "Привет, <сердце> моё, как прошёл твой день на работе?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "moje serce"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used as a term of endearment"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое обращение к любимому человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сердце моё",
                                "target_lang_sense_clarification": "обращение"
                            },
                            {
                                "target_lang_word": "душа моя",
                                "target_lang_sense_clarification": "ласковое обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "529bbcbd-a15c-3029-a409-7a022bf3fb8e",
                    "sense_definition": "Zdolność do odczuwania litości, współczucia i chęć pomagania innym.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest człowiek o wielkim <sercu>, który zawsze pomaga biednym.",
                            "target_lang_translations": {
                                "ru": "Это человек с большим <сердцем>, который всегда помогает бедным."
                            }
                        },
                        {
                            "text": "Trzeba nie mieć <serca>, żeby wyrzucić psa na ulicę.",
                            "target_lang_translations": {
                                "ru": "Нужно не иметь <сердца>, чтобы выбросить собаку на улицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "życzliwość"
                    ],
                    "common_phrases": [
                        "mieć serce",
                        "nie mieć serca",
                        "okazać serce",
                        "gołębie serce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность испытывать жалость, сострадание и желание помогать другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сердце",
                                "target_lang_sense_clarification": "доброта, отзывчивость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b331c23-88d9-316e-8626-cae7e5d8719b",
                    "sense_definition": "Najważniejsza, środkowa część czegoś; centrum.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz hotel znajduje się w samym <sercu> starego miasta.",
                            "target_lang_translations": {
                                "ru": "Наш отель находится в самом <сердце> старого города."
                            }
                        },
                        {
                            "text": "Silnik jest <sercem> każdego samochodu.",
                            "target_lang_translations": {
                                "ru": "Двигатель — это <сердце> каждого автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "centrum",
                        "środek",
                        "trzon"
                    ],
                    "common_phrases": [
                        "w sercu miasta",
                        "serce problemu",
                        "w sercu puszczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самая важная, центральная часть чего-либо; центр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сердце",
                                "target_lang_sense_clarification": "центр"
                            },
                            {
                                "target_lang_word": "центр",
                                "target_lang_sense_clarification": "середина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f03a09c1-cd79-308c-8b74-20d7a2360b3c",
                    "sense_definition": "Jeden z czterech kolorów w kartach, oznaczony czerwonym symbolem serca.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do wygrania tego rozdania potrzebny mi był as <serce>.",
                            "target_lang_translations": {
                                "ru": "Для победы в этой раздаче мне нужен был туз <червей>."
                            }
                        },
                        {
                            "text": "W tej grze <serce> jest kolorem atutowym.",
                            "target_lang_translations": {
                                "ru": "В этой игре <черви> — козырная масть."
                            }
                        }
                    ],
                    "synonyms": [
                        "kier",
                        "czerwień",
                        "herc"
                    ],
                    "antonyms": [
                        "wino",
                        "żołądź",
                        "dzwonek"
                    ],
                    "common_phrases": [
                        "król serce",
                        "dama serce",
                        "walet serce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из четырех карточных мастей, обозначаемая красным символом сердца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черви",
                                "target_lang_sense_clarification": "масть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63ed7473-aaf8-3363-8680-520a63b8033e",
                    "sense_definition": "Ruchomy element wewnątrz dzwonu, który uderzając o jego ściany, wydaje dźwięk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stare <serce> dzwonu urwało się i spadło na ziemię.",
                            "target_lang_translations": {
                                "ru": "Старый <язык> колокола оторвался и упал на землю."
                            }
                        },
                        {
                            "text": "Dźwięk powstaje, gdy metalowe <serce> uderza o klosz.",
                            "target_lang_translations": {
                                "ru": "Звук возникает, когда металлический <язык> ударяет о купол."
                            }
                        }
                    ],
                    "synonyms": [
                        "bijak",
                        "język",
                        "tłuk"
                    ],
                    "common_phrases": [
                        "serce dzwonu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижный элемент внутри колокола, который при ударе о его стенки издает звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык",
                                "target_lang_sense_clarification": "часть колокола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4aeb77c5-2f63-3591-b722-2be7d40f3f72",
                    "sense_definition": "Element rozjazdu kolejowego, w którym przecinają się szyny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pociąg z dużą prędkością przejechał przez <serce> rozjazdu.",
                            "target_lang_translations": {
                                "ru": "Поезд на большой скорости проехал через <крестовину> стрелочного перевода."
                            }
                        },
                        {
                            "text": "Technicy muszą wymienić zużyte <serce> na torach.",
                            "target_lang_translations": {
                                "ru": "Техники должны заменить изношенную <крестовину> на путях."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozjazd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент железнодорожной стрелки, в котором пересекаются рельсы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крестовина",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "9311a01a-7e6e-3303-ac0f-9dc70beedf76",
                    "sense_definition": "De evangelist die traditioneel wordt beschouwd als de auteur van het tweede evangelie in het Nieuwe Testament.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "biblical_marcus",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "De evangelist <w>Marcus</w> is de beschermheilige van de stad Venetië.",
                            "target_lang_translations": {
                                "en": "The Evangelist <w>Mark</w> is the patron saint of the city of Venice.",
                                "ru": "Евангелист <w>Марк</w> — покровитель города Венеции."
                            }
                        },
                        {
                            "text": "In de christelijke kunst wordt <w>Marcus</w> vaak afgebeeld met een leeuw.",
                            "target_lang_translations": {
                                "en": "In Christian art, <w>Mark</w> is often depicted with a lion.",
                                "ru": "В христианском искусстве <w>Марк</w> часто изображается со львом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Sint-Marcus"
                    ],
                    "target_lang_definitions": {
                        "en": "The evangelist traditionally considered the author of the second gospel in the New Testament.",
                        "ru": "Евангелист, который традиционно считается автором второго Евангелия в Новом Завете."
                    },

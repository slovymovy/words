    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "28c58f12-166f-32d8-bcb6-5b08e02580f9",
                    "sense_definition": "Een computerprogramma dat is ontworpen om een specifieke taak uit te voeren voor de gebruiker.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "software",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb jij die nieuwe <w>applicatie</w> al op je telefoon geïnstalleerd?",
                            "target_lang_translations": {
                                "ru": "Ты уже установил это новое <w>приложение</w> на свой телефон?"
                            }
                        },
                        {
                            "text": "Voor het bewerken van foto's gebruik ik liever een professionele <w>applicatie</w> op mijn laptop.",
                            "target_lang_translations": {
                                "ru": "Для редактирования фотографий я предпочитаю использовать профессиональное <w>приложение</w> на своем ноутбуке."
                            }
                        }
                    ],
                    "synonyms": [
                        "programma",
                        "computerprogramma"
                    ],
                    "common_phrases": [
                        "nieuwe applicatie",
                        "mobiele applicatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа, предназначенная для выполнения конкретной задачи для пользователя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приложение",
                                "target_lang_sense_clarification": "программное обеспечение"
                            },
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ae64cb4-11d4-3a45-8c95-9f2b3e0e699e",
                    "sense_definition": "Het toepassen of in de praktijk brengen van een middel, techniek of regel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "act_of_applying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>applicatie</w> van nieuwe technieken zorgde voor het oplossen van het probleem.",
                            "target_lang_translations": {
                                "ru": "<w>Применение</w> новых методов помогло решить проблему."
                            }
                        },
                        {
                            "text": "Zonder de juiste <w>applicatie</w> van de veiligheidsregels kunnen er ongelukken gebeuren.",
                            "target_lang_translations": {
                                "ru": "Без правильного <w>применения</w> правил безопасности могут произойти несчастные случаи."
                            }
                        }
                    ],
                    "synonyms": [
                        "toepassing"
                    ],
                    "common_phrases": [
                        "applicatie van regels"
                    ],
                    "target_lang_definitions": {
                        "ru": "Применение на практике метода, техники или правила."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "применение",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "использование",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e218b72e-741b-30f6-9337-85aa850985ec",
                    "sense_definition": "Een stukje stof, leer of ander materiaal dat ter versiering of reparatie op kleding wordt genaaid.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "textile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal een hippe <w>applicatie</w> over die dunne plek in de stof naaien.",
                            "target_lang_translations": {
                                "ru": "Я пришью модную <w>аппликацию</w> поверх этого тонкого места на ткани."
                            }
                        },
                        {
                            "text": "Op de rug van haar spijkerjasje zit een kleurrijke <w>applicatie</w> in de vorm van een roos.",
                            "target_lang_translations": {
                                "ru": "На спине её джинсовой куртки красуется яркая <w>аппликация</w> в форме розы."
                            }
                        }
                    ],
                    "synonyms": [
                        "appliqué",
                        "oplegwerk"
                    ],
                    "common_phrases": [
                        "geborduurde applicatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок ткани, кожи или другого материала, пришитый к одежде для украшения или ремонта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аппликация",
                                "target_lang_sense_clarification": "декор на ткани"
                            },
                            {
                                "target_lang_word": "нашивка",
                                "target_lang_sense_clarification": "деталь одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fad700f-5d33-369d-b148-5e5726b68a54",
                    "sense_definition": "De manier waarop men de vingers plaatst en gebruikt bij het bespelen van een muziekinstrument.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De pianoleraar lette goed op de <w>applicatie</w> van de leerling tijdens het moeilijke stuk.",
                            "target_lang_translations": {
                                "ru": "Учитель фортепиано внимательно следил за <w>аппликатурой</w> ученика во время сложного фрагмента."
                            }
                        },
                        {
                            "text": "Een correcte <w>applicatie</w> is essentieel om dit snelle loopje soepel te kunnen spelen.",
                            "target_lang_translations": {
                                "ru": "Правильная <w>аппликатура</w> необходима, чтобы плавно сыграть этот быстрый пассаж."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Порядок расположения и использования пальцев при игре на музыкальном инструменте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аппликатура",
                                "target_lang_sense_clarification": "музыкальный термин"
                            }
                        ]
                    }
                }
            ]
        }

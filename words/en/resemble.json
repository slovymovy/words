            "senses": [
                {
                    "sense_id": "18c5ac69-cfbe-3dc5-9d13-cb0a7a6f5ae0",
                    "sense_definition": "To be like or similar to someone or something, especially in appearance or qualities.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The twins <w>resemble</w> each other so much that it is hard to tell them apart."
                        },
                        {
                            "text": "This new electric car seems to <w>resemble</w> a futuristic spaceship."
                        }
                    ],
                    "synonyms": [
                        "look like",
                        "mirror"
                    ],
                    "antonyms": [
                        "differ"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3a878138-ba4c-34d7-a154-ac879f63d1f5",
                    "sense_definition": "Место, где, по религиозным представлениям, души умерших праведников пребывают в вечном блаженстве",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Верующие люди надеются после смерти обрести вечный покой в <w>раю</w>.",
                            "target_lang_translations": {
                                "en": "Believers hope to find eternal rest in <w>paradise</w> after death."
                            }
                        },
                        {
                            "text": "Бабушка часто рассказывала нам о том, как прекрасен небесный <w>рай</w>.",
                            "target_lang_translations": {
                                "en": "Grandmother often told us how beautiful heavenly <w>paradise</w> is."
                            }
                        }
                    ],
                    "antonyms": [
                        "ад",
                        "преисподняя",
                        "геена"
                    ],
                    "common_phrases": [
                        "попасть в рай",
                        "ключи от рая"
                    ],
                    "target_lang_definitions": {
                        "en": "The place where, according to religious beliefs, the souls of the righteous dead dwell in eternal bliss"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heaven",
                                "target_lang_sense_clarification": "the abode of God and the angels"
                            },
                            {
                                "target_lang_word": "paradise",
                                "target_lang_sense_clarification": "a place of eternal happiness after death"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df40e391-58bc-3cb1-bca6-e41185c69258",
                    "sense_definition": "Красивая местность или место, где можно счастливо и безмятежно жить",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "figurative_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот тропический остров — настоящий <w>рай</w> для любителей пляжного отдыха.",
                            "target_lang_translations": {
                                "en": "This tropical island is a real <w>paradise</w> for beach lovers."
                            }
                        },
                        {
                            "text": "Мы нашли тихий уголок у реки, который показался нам земным <w>раем</w>.",
                            "target_lang_translations": {
                                "en": "We found a quiet corner by the river that seemed like an earthly <w>paradise</w> to us."
                            }
                        }
                    ],
                    "antonyms": [
                        "ад"
                    ],
                    "common_phrases": [
                        "земной рай",
                        "туристический рай",
                        "потерянный рай"
                    ],
                    "target_lang_definitions": {
                        "en": "A beautiful area or place where one can live happily and serenely"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "paradise",
                                "target_lang_sense_clarification": "an idyllic or ideal place"
                            },
                            {
                                "target_lang_word": "heaven",
                                "target_lang_sense_clarification": "a delightful place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a25468da-e85c-35a6-9b4e-45deae73105c",
                    "sense_definition": "Прекрасный сад, в котором, по библейскому преданию, до грехопадения жили Адам и Ева",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_concept",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "На старинной картине изображено изгнание первых людей из <w>рая</w>.",
                            "target_lang_translations": {
                                "en": "The old painting depicts the expulsion of the first people from <w>Paradise</w>."
                            }
                        },
                        {
                            "text": "Змей искусил Еву вкусить запретный плод в центре <w>рая</w>.",
                            "target_lang_translations": {
                                "en": "The serpent tempted Eve to taste the forbidden fruit in the center of <w>Paradise</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "парадиз"
                    ],
                    "antonyms": [
                        "ад"
                    ],
                    "common_phrases": [
                        "изгнание из рая",
                        "врата рая"
                    ],
                    "target_lang_definitions": {
                        "en": "The beautiful garden where, according to biblical tradition, Adam and Eve lived before the Fall"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Paradise",
                                "target_lang_sense_clarification": "the Garden of Eden"
                            },
                            {
                                "target_lang_word": "Garden of Eden",
                                "target_lang_sense_clarification": "biblical garden"
                            },
                            {
                                "target_lang_word": "Eden",
                                "target_lang_sense_clarification": "biblical garden"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2add82da-030e-35a3-b29b-cf03be38e249",
                    "sense_definition": "Состояние высшего удовлетворения, радости и счастья",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После сдачи всех трудных экзаменов для студентов наступил настоящий <w>рай</w>.",
                            "target_lang_translations": {
                                "en": "After passing all the difficult exams, it was absolute <w>paradise</w> for the students."
                            }
                        },
                        {
                            "text": "Для книголюба эта огромная библиотека — просто <w>рай</w>.",
                            "target_lang_translations": {
                                "en": "For a book lover, this huge library is simply <w>heaven</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "ад"
                    ],
                    "common_phrases": [
                        "жизнь как в раю",
                        "просто рай"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A state of supreme satisfaction, joy, and happiness"
                    },
                    "translations": {
                        "en": [
                            {

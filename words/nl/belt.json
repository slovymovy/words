    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f0e4236e-4931-3feb-9dec-f90c822f042b",
                    "sense_definition": "Vervoeging van <w>bellen</w>: tweede en derde persoon enkelvoud onvoltooid tegenwoordige tijd",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_bellen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>belt</w> zijn ouders elke zondag om bij te praten.",
                            "target_lang_translations": {
                                "ru": "Он <w>звонит</w> родителям каждое воскресенье, чтобы поболтать."
                            }
                        },
                        {
                            "text": "Als zij niet opneemt, <w>belt</w> ze later wel terug.",
                            "target_lang_translations": {
                                "ru": "Если она не берет трубку, то <w>перезвонит</w> позже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hij belt",
                        "zij belt",
                        "wie belt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола bellen: второе и третье лицо единственного числа настоящего времени"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонит",
                                "target_lang_sense_clarification": "по телефону или в дверь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f87a301-7ce0-39d5-98a2-d36d3c88d779",
                    "sense_definition": "Vervoeging van <w>bellen</w>: gebiedende wijs meervoud",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_bellen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Belt</w> niet aan deze deur, maar gebruik de bel aan de zijkant.",
                            "target_lang_translations": {
                                "ru": "Не <w>звоните</w> в эту дверь, а используйте звонок сбоку."
                            }
                        },
                        {
                            "text": "<w>Belt</w> en u zult gehoord worden.",
                            "target_lang_translations": {
                                "ru": "<w>Звоните</w>, и вы будете услышаны."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "The plural imperative form ending in -t is archaic in modern Dutch."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола bellen: повелительное наклонение множественного числа"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звоните",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7baef874-ed00-331b-901f-197607944943",
                    "sense_definition": "Een plaats waar afval verzameld en gestort wordt",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_heap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten al dit grofvuil morgen naar de <w>belt</w> brengen.",
                            "target_lang_translations": {
                                "ru": "Мы должны отвезти весь этот крупногабаритный мусор на <w>свалку</w> завтра."
                            }
                        },
                        {
                            "text": "In sommige landen leven mensen en dieren op de <w>belt</w>.",
                            "target_lang_translations": {
                                "ru": "В некоторых странах люди и животные живут на <w>свалке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vuilnisbelt",
                        "stortplaats",
                        "afvalstortplaats",
                        "vuilnishoop",
                        "vuilstortplaats"
                    ],
                    "common_phrases": [
                        "op de belt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где собирают и сбрасывают отходы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свалка",
                                "target_lang_sense_clarification": "место для мусора"
                            },
                            {
                                "target_lang_word": "мусорный полигон",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cdce9eb-ed61-39b3-9949-df1c4debbf21",
                    "sense_definition": "Een kleine heuvel of verhoging in het landschap",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_heap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude boerderij stond veilig op een kleine <w>belt</w>.",
                            "target_lang_translations": {
                                "ru": "Старая ферма надежно стояла на небольшом <w>холме</w>."
                            }
                        },
                        {
                            "text": "Vanaf de <w>belt</w> hadden we een mooi uitzicht over de polder.",
                            "target_lang_translations": {
                                "ru": "С <w>холма</w> у нас был прекрасный вид на польдер."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой холм или возвышенность в ландшафте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "холм",
                                "target_lang_sense_clarification": "возвышенность"
                            },
                            {
                                "target_lang_word": "курган",
                                "target_lang_sense_clarification": "насыпь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7660005c-83e1-3b0f-9093-1fcc6f05369e",
                    "sense_definition": "Een riem die om het middel wordt gedragen (kledingstuk)",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij trok zijn <w>belt</w> strakker aan voordat hij wegging.",
                            "target_lang_translations": {
                                "ru": "Он затянул свой <w>ремень</w> потуже перед уходом."
                            }
                        },
                        {
                            "text": "Deze leren <w>belt</w> past goed bij je nieuwe schoenen.",
                            "target_lang_translations": {
                                "ru": "Этот кожаный <w>ремень</w> хорошо подходит к твоим новым туфлям."
                            }
                        }
                    ],
                    "synonyms": [
                        "riem",
                        "broeksriem",
                        "gordel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремень, который носят на талии (предмет одежды)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ремень",
                                "target_lang_sense_clarification": "аксессуар"
                            },
                            {
                                "target_lang_word": "пояс",
                                "target_lang_sense_clarification": "одежда"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c8476b1b-d726-3f13-80a0-2c78605f5c23",
                    "sense_definition": "Een buurtschap in de gemeente Hollands Kroon, Noord-Holland",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mijn grootouders woonden jarenlang in een huisje in De <w>Belt</w>.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка много лет жили в домике в Де <w>Белт</w>."
                            }
                        },
                        {
                            "text": "Tijdens onze fietstocht door Noord-Holland kwamen we langs De <w>Belt</w>.",
                            "target_lang_translations": {
                                "ru": "Во время нашей велопрогулки по Северной Голландии мы проезжали мимо Де <w>Белт</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хутор в муниципалитете Холландс-Крон, Северная Голландия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Де Белт",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "36d79dfe-707b-3797-a1bd-1106cb2a7567",
                    "sense_definition": "Łączna liczba egzemplarzy jednego wydania książki, czasopisma lub innego druku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "publishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Książka ukazała się w <w>nakładzie</w> pięćdziesięciu tysięcy egzemplarzy.",
                            "target_lang_translations": {
                                "ru": "Книга вышла <w>тиражом</w> в пятьдесят тысяч экземпляров."
                            }
                        },
                        {
                            "text": "Cały <w>nakład</w> porannej gazety został wyprzedany w ciągu godziny.",
                            "target_lang_translations": {
                                "ru": "Весь <w>тираж</w> утренней газеты был распродан за час."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyczerpany nakład",
                        "wysoki nakład",
                        "nakład książki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общее количество экземпляров одного издания книги, журнала или другого печатного материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тираж",
                                "target_lang_sense_clarification": "количество экземпляров издания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e928afe-6f87-3f4e-89db-0a050dab2d59",
                    "sense_definition": "Wkład pracy, energii lub sił konieczny do zrealizowania jakiegoś przedsięwzięcia.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "effort_expense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Remont tego starego domu wymagał ogromnego <w>nakładu</w> pracy.",
                            "target_lang_translations": {
                                "ru": "Ремонт этого старого дома требовал огромных <w>затрат</w> труда."
                            }
                        },
                        {
                            "text": "Udało nam się osiągnąć cel minimalnym <w>nakładem</w> sił.",
                            "target_lang_translations": {
                                "ru": "Нам удалось достичь цели с минимальной <w>затратой</w> сил."
                            }

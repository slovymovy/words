                {
                    "sense_id": "a6c20dca-54c8-333d-b293-3945f43ff9df",
                    "sense_definition": "Een openbare plaats, plein of straat waar handelaren op vaste tijden in kramen hun goederen verkopen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn moeder koopt elke zaterdag verse groenten en fruit op de <markt>.",
                            "target_lang_translations": {
                                "ru": "Моя мама каждую субботу покупает свежие овощи и фрукты на <рынке>.",
                                "en": "My mother buys fresh vegetables and fruit at the <market> every Saturday."
                            }
                        },
                        {
                            "text": "Het is altijd erg gezellig en druk als we samen naar de <markt> gaan.",
                            "target_lang_translations": {
                                "ru": "Всегда очень весело и людно, когда мы вместе идем на <рынок>.",
                                "en": "It is always very cozy and busy when we go to the <market> together."
                            }
                        }
                    ],
                    "synonyms": [
                        "marktplein"
                    ],
                    "common_phrases": [
                        "op de markt",
                        "naar de markt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общественное место, площадь или улица, где торговцы в определенное время продают свои товары с прилавков.",
                        "en": "A public place, square, or street where traders sell their goods in stalls at fixed times."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рынок",
                                "target_lang_sense_clarification": "место торговли"
                            },
                            {
                                "target_lang_word": "базар",
                                "target_lang_sense_clarification": "торговая площадь"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "market",
                                "target_lang_sense_clarification": "place for trading"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c994acb-166f-370a-8e84-23acbd22eb91",
                    "sense_definition": "Het economische geheel van vraag en aanbod waarbij producten of diensten worden verhandeld.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "economy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf wil volgend jaar een innovatief product op de <markt> brengen.",
                            "target_lang_translations": {
                                "ru": "Компания хочет выпустить инновационный продукт на <рынок> в следующем году.",
                                "en": "The company wants to bring an innovative product to the <market> next year."
                            }
                        },
                        {
                            "text": "Door de economische crisis is de <markt> voor luxe goederen ingestort.",
                            "target_lang_translations": {
                                "ru": "Из-за экономического кризиса <рынок> предметов роскоши рухнул.",
                                "en": "Due to the economic crisis, the <market> for luxury goods has collapsed."
                            }
                        }
                    ],
                    "synonyms": [
                        "handel",
                        "afzetgebied"
                    ],
                    "common_phrases": [
                        "op de markt brengen",
                        "in de markt liggen",
                        "van de markt halen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Экономическая система спроса и предложения, в которой происходит обмен товарами или услугами.",
                        "en": "The economic system of supply and demand where products or services are traded."
                    },
                    "translations": {

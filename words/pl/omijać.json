    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "000e4923-1b38-3294-9cc4-b5904f3e92c9",
                    "sense_definition": "Przechodzić lub przejeżdżać obok kogoś lub czegoś, zachowując dystans, aby nie doszło do zderzenia lub spotkania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "avoidance_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca musiał ostrożnie <omijać> głębokie dziury w jezdni.",
                            "target_lang_translations": {
                                "ru": "Водитель должен был осторожно <w>объезжать</w> глубокие ямы на дороге."
                            }
                        },
                        {
                            "text": "Zawsze staram się <omijać> szerokim łukiem agresywne psy na spacerze.",
                            "target_lang_translations": {
                                "ru": "Я всегда стараюсь <w>обходить</w> стороной агрессивных собак на прогулке."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymijać",
                        "okrążać"
                    ],
                    "antonyms": [
                        "potrącać",
                        "napotykać"
                    ],
                    "common_phrases": [
                        "omijać szerokim łukiem",
                        "omijać przeszkody",
                        "omijać korki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить или проезжать мимо кого-то или чего-то, сохраняя дистанцию, чтобы избежать столкновения или встречи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обходить",
                                "target_lang_sense_clarification": "пешком или в общем смысле"
                            },
                            {
                                "target_lang_word": "объезжать",
                                "target_lang_sense_clarification": "на транспортном средстве"
                            },
                            {
                                "target_lang_word": "огибать",
                                "target_lang_sense_clarification": "двигаться вокруг препятствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8eb025c1-57ff-3701-8b5d-a7b2df593a86",
                    "sense_definition": "Świadomie unikać zajmowania się czymś, np. trudnym tematem, lub sprytnie nie przestrzegać jakichś zasad.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "avoidance_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieuczciwi przedsiębiorcy często próbują <omijać> prawo podatkowe.",
                            "target_lang_translations": {
                                "ru": "Нечестные предприниматели часто пытаются <w>обходить</w> налоговое законодательство."
                            }
                        },
                        {
                            "text": "Podczas rozmowy polityk starał się <omijać> trudne tematy gospodarcze.",
                            "target_lang_translations": {
                                "ru": "Во время разговора политик старался <w>обходить</w> сложные экономические темы."
                            }
                        }
                    ],
                    "synonyms": [
                        "unikać",
                        "lekceważyć"
                    ],
                    "antonyms": [
                        "przestrzegać",
                        "poruszać"
                    ],
                    "common_phrases": [
                        "omijać prawo",
                        "omijać przepisy",
                        "omijać temat",
                        "omijać prawdę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сознательно избегать обсуждения чего-либо или хитро уклоняться от соблюдения правил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обходить",
                                "target_lang_sense_clarification": "закон, правила, тему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b6a4d01-dfa3-3824-b461-32559e3196c6",
                    "sense_definition": "Nie przydarzać się komuś; o sytuacji lub zdarzeniu, które nie dotyka danej osoby.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fate_experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam wrażenie, że wielkie szczęście w loterii zawsze mnie <omija>.",
                            "target_lang_translations": {
                                "ru": "У меня впечатление, что крупная удача в лотерее всегда <w>обходит</w> меня стороной."
                            }
                        },
                        {
                            "text": "Cieszę się, że poważne choroby <omijają> moją rodzinę od wielu lat.",
                            "target_lang_translations": {
                                "ru": "Я рад, что серьезные болезни уже много лет <w>обходят</w> мою семью стороной."
                            }
                        }
                    ],
                    "synonyms": [
                        "oszczędzać"
                    ],
                    "antonyms": [
                        "spotykać",
                        "dotykać"
                    ],
                    "common_phrases": [
                        "kłopoty go omijają",
                        "szczęście go omija"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не случаться с кем-то; о ситуации или событии, которое проходит мимо, не затрагивая человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обходить",
                                "target_lang_sense_clarification": "не затрагивать, проходить стороной"
                            },
                            {
                                "target_lang_word": "миновать",
                                "target_lang_sense_clarification": "проходить мимо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

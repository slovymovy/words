    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1d9974a6-e878-3102-89ea-f05f344fd24b",
                    "sense_definition": "Составлять мнение, суждение о ком-либо или чём-либо; делать вывод на основе наблюдений.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я стараюсь беспристрастно <w>судить</w> о людях и их поступках.",
                            "target_lang_translations": {
                                "en": "I try to <w>judge</w> people and their actions impartially."
                            }
                        },
                        {
                            "text": "По его словам можно <w>судить</w> о его добром характере.",
                            "target_lang_translations": {
                                "en": "One can <w>judge</w> his good character by his words."
                            }
                        },
                        {
                            "text": "Мы не можем <w>судить</w> о достоверности этой информации без проверки.",
                            "target_lang_translations": {
                                "en": "We cannot <w>judge</w> the reliability of this information without verification."
                            }
                        }
                    ],
                    "synonyms": [
                        "обслуживать"
                    ],
                    "common_phrases": [
                        "судя по всему",
                        "судить по себе",
                        "судить по одежке"
                    ],
                    "target_lang_definitions": {
                        "en": "To form an opinion or conclusion about someone or something based on observations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "judge",
                                "target_lang_sense_clarification": "form an opinion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f72eb99-d3cd-3b7b-843c-48351bbdaa5e",
                    "sense_definition": "Оценивать чьи-либо дела или поступки, часто выражая неодобрение или критику.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит строго <w>судить</w> человека, если вы не знаете всех обстоятельств.",
                            "target_lang_translations": {
                                "en": "You should not <w>judge</w> a person strictly if you do not know all the circumstances."
                            }
                        },
                        {
                            "text": "Окружающие часто любят <w>судить</w> других за малейшие ошибки.",
                            "target_lang_translations": {
                                "en": "People around often love to <w>judge</w> others for the slightest mistakes."
                            }
                        }
                    ],
                    "synonyms": [
                        "обслуживать"
                    ],
                    "common_phrases": [
                        "не судите строго",
                        "Бог тебе судья"
                    ],
                    "target_lang_definitions": {
                        "en": "To evaluate someone's actions or behavior, often expressing criticism or disapproval."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "judge",
                                "target_lang_sense_clarification": "criticize"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81f949c7-9832-3109-9cad-6fa059a5e19c",
                    "sense_definition": "Рассматривать чьё-либо дело в судебном порядке для выяснения виновности и вынесения приговора.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Преступника будут <w>судить</w> по всей строгости закона.",
                            "target_lang_translations": {
                                "en": "The criminal will be <w>tried</w> to the full extent of the law."
                            }
                        },
                        {
                            "text": "Его <w>судили</w> за хулиганство и приговорили к штрафу.",
                            "target_lang_translations": {
                                "en": "He was <w>tried</w> for hooliganism and sentenced to a fine."
                            }
                        }
                    ],
                    "common_phrases": [
                        "судить по закону",
                        "судить преступника"
                    ],
                    "target_lang_definitions": {
                        "en": "To examine a case in a court of law to determine guilt and pass a sentence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "try",
                                "target_lang_sense_clarification": "legal"
                            },
                            {
                                "target_lang_word": "judge",
                                "target_lang_sense_clarification": "legal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b73c108-0305-3538-bd8c-0f9df6e5241c",
                    "sense_definition": "Следить за соблюдением правил в спортивной игре или состязании в качестве арбитра.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный арбитр будет <w>судить</w> финальный матч чемпионата.",
                            "target_lang_translations": {
                                "en": "An experienced arbiter will <w>referee</w> the final match of the championship."
                            }
                        },
                        {
                            "text": "Мой брат начал <w>судить</w> футбольные игры ещё в университете.",
                            "target_lang_translations": {
                                "en": "My brother started to <w>referee</w> football games back in university."
                            }
                        }
                    ],
                    "common_phrases": [
                        "судить матч",
                        "судить игру"
                    ],
                    "target_lang_definitions": {
                        "en": "To monitor compliance with rules in a sports game or competition as an official."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "referee",
                                "target_lang_sense_clarification": "sports"
                            },
                            {
                                "target_lang_word": "officiate",
                                "target_lang_sense_clarification": "sports"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18483376-3c36-3ffd-85a1-551b9c08fd62",
                    "sense_definition": "Предопределять чью-либо участь или будущее (обычно о судьбе, роке или высших силах).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Судьба <w>судила</w> им встретиться снова через много лет.",
                            "target_lang_translations": {
                                "en": "Fate <w>destined</w> them to meet again many years later."
                            }
                        },
                        {
                            "text": "Рок <w>судил</w> поэту умереть молодым на чужбине.",
                            "target_lang_translations": {
                                "en": "Doom <w>destined</w> the poet to die young in a foreign land."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "высок."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To predetermine someone's fate or future (usually referring to fate or higher powers)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "destine",
                                "target_lang_sense_clarification": "fate"
                            },
                            {
                                "target_lang_word": "predestine",
                                "target_lang_sense_clarification": "fate"
                            }
                        ]
                    }
                }
            ]
        }
    ]

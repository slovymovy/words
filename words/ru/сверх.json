                {
                    "sense_id": "46920b8e-22ab-3590-9063-366026b1b6dd",
                    "sense_definition": "Больше определенной нормы, меры или границы; превышая что-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "excess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы выполнили годовой план <w>сверх</w> нормы.",
                            "target_lang_translations": {
                                "en": "We fulfilled the annual plan <w>above</w> the norm."
                            }
                        },
                        {
                            "text": "Это задание оказалось ему <w>сверх</w> сил.",
                            "target_lang_translations": {
                                "en": "This task turned out to be <w>beyond</w> his strength."
                            }
                        }
                    ],
                    "synonyms": [
                        "более"
                    ],
                    "common_phrases": [
                        "сверх меры",
                        "сверх плана",
                        "сверх сил",
                        "сверх того"
                    ],
                    "target_lang_definitions": {
                        "en": "More than a certain norm, measure, or limit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beyond",
                                "target_lang_sense_clarification": "exceeding a limit, capacity, or scope"
                            },
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "more than a specific amount or number"
                            },
                            {
                                "target_lang_word": "above",
                                "target_lang_sense_clarification": "higher than a standard or level"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da5318fc-e1ce-3b31-ae1c-70ee6fa17404",
                    "sense_definition": "В дополнение к кому-либо или чему-либо; помимо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "addition",
                    "name_type": "no",
                    "examples": [
                        {

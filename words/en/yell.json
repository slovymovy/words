{
    "word_family": [
        "yeller"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0e6bd081-f954-3ddb-8d05-99f5e99a3e2c",
                    "sense_definition": "To shout; to make a loud sound with the voice, especially to express intense emotions such as anger, excitement, or pain.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_make_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>yell</w> inside the library.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>кричите</w> в библиотеке."
                            }
                        },
                        {
                            "text": "The crowd started to <w>yell</w> with excitement when the team scored a goal.",
                            "target_lang_translations": {
                                "ru": "Толпа начала <w>кричать</w> от восторга, когда команда забила гол."
                            }
                        }
                    ],
                    "synonyms": [
                        "shout",
                        "holler",
                        "cry",
                        "call"
                    ],
                    "common_phrases": [
                        "yell for help",
                        "yell out",
                        "yell in pain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кричать; издавать громкий звук голосом, особенно для выражения сильных эмоций, таких как гнев, волнение или боль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кричать",
                                "target_lang_sense_clarification": "издавать громкий звук"
                            },
                            {
                                "target_lang_word": "орать",
                                "target_lang_sense_clarification": "разговорное; кричать очень громко"
                            },
                            {
                                "target_lang_word": "вопить",
                                "target_lang_sense_clarification": "издавать пронзительные крики (от боли или страха)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc157786-b556-33c0-85da-cae6999c4f40",
                    "sense_definition": "To tell someone off or scold them, typically in a loud and angry manner.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_scold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My father used to <w>yell</w> at me when I came home late.",
                            "target_lang_translations": {
                                "ru": "Мой отец обычно <w>кричал</w> на меня, когда я приходил домой поздно."
                            }
                        },
                        {
                            "text": "She did not mean to <w>yell</w> at the children for making a mess.",
                            "target_lang_translations": {
                                "ru": "Она не хотела <w>кричать</w> на детей за то, что они устроили беспорядок."
                            }
                        }
                    ],
                    "synonyms": [
                        "shout"
                    ],
                    "common_phrases": [
                        "yell at someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ругать или отчитывать кого-либо, обычно громко и сердито."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кричать",
                                "target_lang_sense_clarification": "на кого-либо; ругать"
                            },
                            {
                                "target_lang_word": "орать",
                                "target_lang_sense_clarification": "на кого-либо; грубо ругать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f449c945-4136-3b35-b248-d0977a9aee4e",
                    "sense_definition": "To convey information or directions by shouting.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_make_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>yelled</w> directions to the lost tourists from across the street.",
                            "target_lang_translations": {
                                "ru": "Он <w>прокричал</w> дорогу заблудившимся туристам с другой стороны улицы."
                            }
                        },
                        {
                            "text": "The captain <w>yelled</w> orders to the crew during the storm.",
                            "target_lang_translations": {
                                "ru": "Капитан <w>выкрикивал</w> приказы команде во время шторма."
                            }
                        }
                    ],
                    "synonyms": [
                        "shout",
                        "call"
                    ],
                    "common_phrases": [
                        "yell instructions",
                        "yell orders"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать информацию или указания криком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кричать",
                                "target_lang_sense_clarification": "сообщать громким голосом"
                            },
                            {
                                "target_lang_word": "выкрикивать",
                                "target_lang_sense_clarification": "громко произносить слова"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f7fe2527-c68a-3404-8c3c-3c456f720e84",
                    "sense_definition": "A loud shout or cry, often expressing strong emotion.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_sound",
                    "name_type": "no",
                    "examples": [
                        {

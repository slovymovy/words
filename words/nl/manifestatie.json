    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f44fc981-1179-33b9-8cea-034ff34bfa7c",
                    "sense_definition": "Een openbare bijeenkomst om een mening te uiten, vaak als protest.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De studenten organiseerden een grote <w>manifestatie</w> op het plein.",
                            "target_lang_translations": {
                                "en": "The students organized a large <w>demonstration</w> on the square.",
                                "ru": "Студенты организовали на площади крупную <w>демонстрацию</w>."
                            }
                        },
                        {
                            "text": "Tijdens de vreedzame <w>manifestatie</w> riep de menigte leuzen.",
                            "target_lang_translations": {
                                "en": "During the peaceful <w>demonstration</w>, the crowd shouted slogans.",
                                "ru": "Во время мирной <w>манифестации</w> толпа выкрикивала лозунги."
                            }
                        }
                    ],
                    "synonyms": [
                        "demonstratie",
                        "protest"
                    ],
                    "common_phrases": [
                        "vreedzame manifestatie",
                        "politieke manifestatie",
                        "grote manifestatie"
                    ],
                    "target_lang_definitions": {
                        "en": "A public gathering to express an opinion, often as a protest.",
                        "ru": "Публичное собрание для выражения мнения, часто в знак протеста."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demonstration",
                                "target_lang_sense_clarification": "public display of group opinion"
                            },
                            {
                                "target_lang_word": "protest",
                                "target_lang_sense_clarification": "organized public objection"
                            },
                            {
                                "target_lang_word": "manifestation",
                                "target_lang_sense_clarification": "political gathering (formal)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "демонстрация",
                                "target_lang_sense_clarification": "публичное массовое шествие или собрание"
                            },
                            {
                                "target_lang_word": "манифестация",
                                "target_lang_sense_clarification": "массовое выступление, часто политического характера"
                            },
                            {
                                "target_lang_word": "митинг",
                                "target_lang_sense_clarification": "собрание для обсуждения злободневных вопросов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a531476-711b-3e63-8eed-201d309c2482",
                    "sense_definition": "Het zichtbaar worden of de uiting van iets, bijvoorbeeld een ziekte of eigenschap.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Huiduitslag kan een <w>manifestatie</w> zijn van een allergische reactie.",
                            "target_lang_translations": {
                                "en": "A skin rash can be a <w>manifestation</w> of an allergic reaction.",
                                "ru": "Сыпь может быть <w>проявлением</w> аллергической реакции."
                            }
                        },
                        {
                            "text": "Zijn woede was een duidelijke <w>manifestatie</w> van zijn frustratie.",
                            "target_lang_translations": {
                                "en": "His anger was a clear <w>manifestation</w> of his frustration.",
                                "ru": "Его гнев был явным <w>проявлением</w> его разочарования."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fysieke manifestatie",
                        "eerste manifestatie"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of becoming visible or the expression of something, such as a disease or trait.",
                        "ru": "Процесс, когда что-то становится видимым, или выражение чего-либо (например, болезни или свойства)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "manifestation",
                                "target_lang_sense_clarification": "visible sign or embodiment"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проявление",
                                "target_lang_sense_clarification": "внешнее выражение какого-либо свойства или состояния"
                            },
                            {
                                "target_lang_word": "манифестация",
                                "target_lang_sense_clarification": "медицинский или книжный термин для обозначения выраженности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
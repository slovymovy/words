    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "27a5633e-f218-30f2-be12-d3eb99e23b65",
                    "sense_definition": "Het deel van de dag tussen het middaguur en de avond, ongeveer van 12.00 tot 18.00 uur.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen spelen elke <w>middag</w> buiten op het schoolplein.",
                            "target_lang_translations": {
                                "ru": "Дети играют на школьном дворе каждый <w>день после обеда</w>.",
                                "en": "The children play outside in the schoolyard every <w>afternoon</w>."
                            }
                        },
                        {
                            "text": "We hebben *in de middag* een afspraak bij de tandarts.",
                            "target_lang_translations": {
                                "ru": "У нас запись к стоматологу <w>во второй половине дня</w>.",
                                "en": "We have an appointment at the dentist in the <w>afternoon</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "namiddag"
                    ],
                    "antonyms": [
                        "ochtend",
                        "morgen",
                        "voormiddag"
                    ],
                    "common_phrases": [
                        "in de middag",
                        "fijne middag",
                        "hele middag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть дня между полуднем и вечером, примерно с 12:00 до 18:00.",
                        "en": "The part of the day between noon and evening, approximately from 12:00 to 18:00."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "день",
                                "target_lang_sense_clarification": "время суток"
                            },
                            {
                                "target_lang_word": "послеобеденное время",
                                "target_lang_sense_clarification": "время после 12:00"
                            },
                            {
                                "target_lang_word": "вторая половина дня",
                                "target_lang_sense_clarification": "период до вечера"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "afternoon",
                                "target_lang_sense_clarification": "period between noon and evening"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e782fd5-af47-3427-90be-dd2155d0bafd",
                    "sense_definition": "Het tijdstip midden op de dag, rond 12.00 uur.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Nederland eten veel mensen brood *tussen de middag*.",
                            "target_lang_translations": {
                                "ru": "В Нидерландах многие едят хлеб <w>в обеденный перерыв</w>.",
                                "en": "In the Netherlands, many people eat bread at <w>noon</w>."
                            }
                        },
                        {
                            "text": "De zon staat rond de <w>middag</w> op haar hoogste punt.",
                            "target_lang_translations": {
                                "ru": "Солнце находится в высшей точке около <w>полудня</w>.",
                                "en": "The sun is at its highest point around <w>noon</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "noen"
                    ],
                    "common_phrases": [
                        "tussen de middag",
                        "tegen de middag",
                        "rond de middag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время в середине дня, около 12:00.",
                        "en": "The time in the middle of the day, around 12:00."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полдень",
                                "target_lang_sense_clarification": "12:00 часов"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "noon",
                                "target_lang_sense_clarification": "12:00 PM"
                            },
                            {
                                "target_lang_word": "midday",
                                "target_lang_sense_clarification": "middle of the day"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ceecc0a-a36d-3472-86fb-1fd1f36d9cd4",
                    "sense_definition": "Het midden van het leven of een periode van bloei (figuurlijk).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij schreef zijn memoires in de <w>middag</w> van zijn leven.",
                            "target_lang_translations": {
                                "ru": "Он написал свои мемуары в <w>середине</w> своей жизни.",
                                "en": "He wrote his memoirs in the <w>prime</w> of his life."
                            }
                        },
                        {
                            "text": "Het koninkrijk bevond zich in de <w>middag</w> van zijn macht.",
                            "target_lang_translations": {
                                "ru": "Королевство находилось в <w>зените</w> своего могущества.",
                                "en": "The kingdom was at the <w>zenith</w> of its power."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Середина жизни или период расцвета (в переносном смысле).",
                        "en": "The middle of life or a period of flourishing (figurative)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расцвет",
                                "target_lang_sense_clarification": "период наивысшего развития"
                            },
                            {
                                "target_lang_word": "зенит",
                                "target_lang_sense_clarification": "высшая степень"
                            },
                            {
                                "target_lang_word": "середина",
                                "target_lang_sense_clarification": "центральная часть"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "prime",
                                "target_lang_sense_clarification": "period of greatest strength or success"
                            },
                            {
                                "target_lang_word": "zenith",
                                "target_lang_sense_clarification": "highest point of power or success"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4612c673-5de1-3806-a01c-494b3144525b",
                    "sense_definition": "W sposób pikantny, z użyciem piekących przypraw.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "taste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta zupa smakuje zbyt <w>ostro</w> dla małych dzieci.",
                            "target_lang_translations": {
                                "ru": "Этот суп на вкус слишком <w>острый</w> для маленьких детей."
                            }
                        },
                        {
                            "text": "Lubię zjeść <w>ostro</w>, dlatego zawsze dodaję papryczkę chili.",
                            "target_lang_translations": {
                                "ru": "Я люблю поесть <w>остро</w>, поэтому всегда добавляю перец чили."
                            }
                        }
                    ],
                    "antonyms": [
                        "delikatnie"
                    ],
                    "common_phrases": [
                        "przyprawić ostro",
                        "zjeść ostro",
                        "smakować ostro"
                    ],
                    "target_lang_definitions": {
                        "ru": "О вкусе: пикантно, с большим количеством жгучих специй."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остро",
                                "target_lang_sense_clarification": "о вкусе"
                            },
                            {
                                "target_lang_word": "пикантно",
                                "target_lang_sense_clarification": "об умеренной остроте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "376e0d19-c91b-3fa0-9d73-7de536af817b",
                    "sense_definition": "W sposób surowy, bezwzględny lub agresywny (o zachowaniu lub krytyce).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel <w>ostro</w> zareagował na złe zachowanie uczniów.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>резко</w> отреагировал на плохое поведение учеников."
                            }
                        },
                        {
                            "text": "Dziennikarz <w>ostro</w> skrytykował decyzje rządu w swoim artykule.",
                            "target_lang_translations": {
                                "ru": "Журналист <w>жестко</w> раскритиковал решения правительства в своей статье."
                            }
                        }
                    ],
                    "antonyms": [
                        "delikatnie",
                        "subtelnie"
                    ],
                    "common_phrases": [
                        "ostro potraktować",
                        "grać ostro",
                        "ostro zareagować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сурово, резко или агрессивно (о поведении или критике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резко",
                                "target_lang_sense_clarification": "о реакции или критике"
                            },
                            {
                                "target_lang_word": "жестко",
                                "target_lang_sense_clarification": "бескомпромиссно"
                            },
                            {
                                "target_lang_word": "сурово",
                                "target_lang_sense_clarification": "строго"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88671320-b2ad-301c-b69e-dd2970a5d2fe",
                    "sense_definition": "Bardzo mocno, z dużą siłą lub intensywnością.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy się wziąć <w>ostro</w> do pracy, żeby zdążyć przed terminem.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>активно</w> взяться за работу, чтобы успеть к сроку."
                            }
                        },
                        {
                            "text": "Wczorajszej nocy sąsiedzi <w>ostro</w> imprezowali do samego rana.",
                            "target_lang_translations": {
                                "ru": "Вчера ночью соседи <w>бурно</w> веселились до самого утра."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciężko"
                    ],
                    "antonyms": [
                        "lekko"
                    ],
                    "common_phrases": [
                        "ostro pracować",
                        "ostro pić",
                        "ostro trenować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень сильно, интенсивно, с полной отдачей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усердно",
                                "target_lang_sense_clarification": "о работе"
                            },
                            {
                                "target_lang_word": "активно",
                                "target_lang_sense_clarification": "о действиях"
                            },
                            {
                                "target_lang_word": "бурно",
                                "target_lang_sense_clarification": "об эмоциях или веселье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c7e925a-ada4-39b7-8073-e5ba153aaab5",
                    "sense_definition": "W sposób przenikliwy i dokuczliwy (zwykle o zjawiskach atmosferycznych).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zewnątrz <w>ostro</w> wieje i pada marznący deszcz.",
                            "target_lang_translations": {
                                "ru": "На улице <w>сильно</w> дует и идет ледяной дождь."
                            }
                        },
                        {
                            "text": "Słońce świeciło tak <w>ostro</w>, że musieliśmy założyć okulary.",
                            "target_lang_translations": {
                                "ru": "Солнце светило так <w>ярко</w>, что нам пришлось надеть очки."
                            }
                        }
                    ],
                    "antonyms": [
                        "lekko"
                    ],
                    "common_phrases": [
                        "ostro wiać",
                        "ostro świecić",
                        "ostro mrozić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пронзительно и неприятно (обычно о погодных явлениях)."

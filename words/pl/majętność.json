{
    "word_family": [
        "majątek",
        "majętny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "571d2d37-c485-3789-885a-f5edbb15c177",
                    "sense_definition": "To, co jest czyjąś własnością, ogół rzeczy posiadanych przez kogoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "possession_generic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pożar strawił całą ich <w>majętność</w> zgromadzoną przez lata ciężkiej pracy.",
                            "target_lang_translations": {
                                "ru": "Пожар уничтожил всё их <w>имущество</w>, накопленное за годы тяжелого труда."
                            }
                        },
                        {
                            "text": "W testamencie zapisał wszelką swoją ruchomą <w>majętność</w> fundacji charytatywnej.",
                            "target_lang_translations": {
                                "ru": "В завещании он отписал всё своё движимое <w>имущество</w> благотворительному фонду."
                            }
                        }
                    ],
                    "synonyms": [
                        "dobra"
                    ],
                    "common_phrases": [
                        "cała majętność",
                        "utrata majętności",
                        "zgromadzona majętność"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что является чьей-либо собственностью, совокупность вещей, которыми кто-то владеет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "имущество",
                                "target_lang_sense_clarification": "собственность, вещи"
                            },
                            {
                                "target_lang_word": "собственность",
                                "target_lang_sense_clarification": "принадлежность кому-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09093a3b-b2ef-39d3-a005-fc76a1927741",
                    "sense_definition": "Duża posiadłość ziemska, teren wraz z zabudowaniami należący do jednego właściciela.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "possession_land",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hrabia zarządzał ogromną <w>majętnością</w> położoną na wschodzie kraju.",
                            "target_lang_translations": {
                                "ru": "Граф управлял огромным <w>имением</w>, расположенным на востоке страны."
                            }
                        },
                        {
                            "text": "Rodzina postanowiła sprzedać wiejską <w>majętność</w> i przeprowadzić się do stolicy.",
                            "target_lang_translations": {
                                "ru": "Семья решила продать сельское <w>поместье</w> и переехать в столицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "dobra"
                    ],
                    "common_phrases": [
                        "rodowa majętność",
                        "ziemska majętność",
                        "zarządzać majętnością"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое земельное владение, территория с постройками, принадлежащая одному владельцу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "имение",
                                "target_lang_sense_clarification": "земельное владение"
                            },
                            {
                                "target_lang_word": "поместье",
                                "target_lang_sense_clarification": "усадьба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0959898-84c7-3f09-8b65-00333800bc4d",
                    "sense_definition": "Wielkie bogactwo, stan posiadania dużej ilości dóbr materialnych.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "wealth_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>majętność</w> pozwalała mu na podróże po całym świecie bez martwienia się o koszty.",
                            "target_lang_translations": {
                                "ru": "Его <w>состояние</w> позволяло ему путешествовать по всему миру, не беспокоясь о расходах."
                            }
                        },
                        {
                            "text": "Mimo że posiadał znaczną <w>majętność</w>, żył bardzo skromnie i oszczędnie.",
                            "target_lang_translations": {
                                "ru": "Несмотря на то, что он владел значительным <w>состоянием</w>, он жил очень скромно и экономно."
                            }
                        }
                    ],
                    "synonyms": [
                        "bogactwo",
                        "zamożność",
                        "dostatek"
                    ],
                    "common_phrases": [
                        "znaczna majętność",
                        "pomnażać majętność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое богатство, обладание значительным количеством материальных благ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состояние",
                                "target_lang_sense_clarification": "капитал, деньги"
                            },
                            {
                                "target_lang_word": "богатство",
                                "target_lang_sense_clarification": "изобилие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        "uncomfortably"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fd3621be-528f-30c1-a874-0df7c7231b92",
                    "sense_definition": "Causing physical pain or not pleasant to use or wear.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new sofa is hard and <w>uncomfortable</w> to sit on.",
                            "target_lang_translations": {
                                "ru": "Новый диван жёсткий, и на нём <w>неудобно</w> сидеть."
                            }
                        },
                        {
                            "text": "She walked slowly because of her <w>uncomfortable</w> shoes.",
                            "target_lang_translations": {
                                "ru": "Она шла медленно из-за своих <w>неудобных</w> туфель."
                            }
                        }
                    ],
                    "antonyms": [
                        "comfortable",
                        "ergonomic"
                    ],
                    "common_phrases": [
                        "uncomfortable chair",
                        "uncomfortable shoes",
                        "uncomfortable bed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий физическую боль или неудобный при использовании или ношении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неудобный",
                                "target_lang_sense_clarification": "о мебели, одежде, обуви"
                            },
                            {
                                "target_lang_word": "некомфортный",
                                "target_lang_sense_clarification": "об условиях или обстановке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8efd3fbd-bef0-3b57-a9d1-4a9ec15a05a9",
                    "sense_definition": "Feeling slight physical pain or not being able to relax.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I feel <w>uncomfortable</w> sitting in this position for too long.",
                            "target_lang_translations": {
                                "ru": "Я чувствую себя <w>некомфортно</w>, сидя в этой позе слишком долго."
                            }
                        },
                        {
                            "text": "The extreme heat made the passengers feel sticky and <w>uncomfortable</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за сильной жары пассажиры чувствовали липкость и <w>дискомфорт</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "comfortable"
                    ],
                    "common_phrases": [
                        "feel uncomfortable"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ощущающий физический дискомфорт или невозможность расслабиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испытывающий неудобство",
                                "target_lang_sense_clarification": "о физическом состоянии"
                            },
                            {
                                "target_lang_word": "некомфортный",
                                "target_lang_sense_clarification": "неприятный (ощущение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16f89f1b-0239-386a-b7b7-1c895d19f71f",
                    "sense_definition": "Feeling embarrassed, awkward, or anxious.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was an <w>uncomfortable</w> silence when he finished speaking.",
                            "target_lang_translations": {
                                "ru": "Когда он закончил говорить, повисла <w>неловкая</w> тишина."
                            }
                        },
                        {
                            "text": "His rude comment made everyone in the room feel <w>uncomfortable</w>.",
                            "target_lang_translations": {
                                "ru": "Его грубый комментарий заставил всех в комнате почувствовать себя <w>неловко</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ill at ease"
                    ],
                    "antonyms": [
                        "comfortable"
                    ],
                    "common_phrases": [
                        "uncomfortable silence",
                        "feel uncomfortable",
                        "uncomfortable situation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий смущение, неловкость или беспокойство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неловкий",
                                "target_lang_sense_clarification": "вызывающий смущение (о ситуации, молчании)"
                            },
                            {
                                "target_lang_word": "стеснённый",
                                "target_lang_sense_clarification": "смущённый, чувствующий себя не в своей тарелке"
                            },
                            {
                                "target_lang_word": "неудобный",
                                "target_lang_sense_clarification": "затруднительный (о ситуации)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
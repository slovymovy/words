                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спокойно",
                                "target_lang_sense_clarification": "без тревоги"
                            },
                            {
                                "target_lang_word": "беззаботно",
                                "target_lang_sense_clarification": "вольготно"
                            }
                        ]

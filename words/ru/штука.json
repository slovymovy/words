            "senses": [
                {
                    "sense_id": "a579aefb-8a53-389f-a201-d5170d355ff4",
                    "sense_definition": "Отдельный предмет при счёте, экземпляр чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "counting_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, взвесьте мне пять <штук> этих красных яблок.",
                            "target_lang_translations": {
                                "en": "Please weigh five <pieces> of these red apples for me."
                            }
                        },
                        {
                            "text": "В коробке осталась только одна <штука>, остальные мы уже съели.",
                            "target_lang_translations": {
                                "en": "There is only one <piece> left in the box, we already ate the rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "экземпляр",
                        "единица"
                    ],
                    "antonyms": [
                        "совокупность"
                    ],
                    "common_phrases": [
                        "пять штук",
                        "пара штук",
                        "несколько штук",
                        "цена за штуку"
                    ],
                    "target_lang_definitions": {
                        "en": "An individual item when counting, a copy of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "piece",
                                "target_lang_sense_clarification": "single item"
                            },
                            {
                                "target_lang_word": "thing",
                                "target_lang_sense_clarification": "individual object"
                            }

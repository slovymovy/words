        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1cd1c7a9-3f82-311e-8beb-7a57942190f3",
                    "sense_definition": "Kosmetyk w formie płynnej lub kremowej nakładany na twarz w celu wyrównania kolorytu cery i ukrycia niedoskonałości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cosmetics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano nakładam <w>podkład</w>, aby ukryć cienie pod oczami.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я наношу <w>тональный крем</w>, чтобы скрыть круги под глазами."
                            }
                        },
                        {
                            "text": "Musisz dobrać odpowiedni <w>podkład</w> do odcienia swojej skóry.",
                            "target_lang_translations": {
                                "ru": "Ты должна подобрать подходящую <w>основу</w> под оттенок своей кожи."
                            }
                        }
                    ],
                    "synonyms": [
                        "fluid",
                        "make-up"
                    ],
                    "common_phrases": [
                        "podkład kryjący",
                        "nakładać podkład",
                        "podkład pod makijaż",
                        "jasny podkład"
                    ],
                    "target_lang_definitions": {
                        "ru": "Косметическое средство в жидкой или кремовой форме, наносимое на лицо для выравнивания тона кожи и скрытия несовершенств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тональный крем",
                                "target_lang_sense_clarification": "косметическое средство"
                            },
                            {
                                "target_lang_word": "основа",
                                "target_lang_sense_clarification": "база под макияж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3623d012-13ee-31af-871e-dbc12a35841f",
                    "sense_definition": "Ścieżka dźwiękowa lub akompaniament muzyczny, do którego dogrywa się inne instrumenty lub śpiewa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wokalista poprosił o głośniejszy <w>podkład</w> w słuchawkach.",
                            "target_lang_translations": {
                                "ru": "Вокалист попросил сделать <w>фонограмму</w> в наушниках погромче."
                            }

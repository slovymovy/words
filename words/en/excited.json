    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "da93ff11-5860-3e21-976c-c66c55599381",
                    "sense_definition": "Having great enthusiasm, passion, and energy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was very <w>excited</w> about his promotion at work.",
                            "target_lang_translations": {
                                "ru": "Он был очень <w>взволнован</w> своим повышением на работе."
                            }
                        },
                        {
                            "text": "The children were <w>excited</w> to open their birthday presents.",
                            "target_lang_translations": {
                                "ru": "Дети были <w>рады</w> открыть свои подарки на день рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "enthusiastic",
                        "aflutter"
                    ],
                    "antonyms": [
                        "bored",
                        "indifferent"
                    ],
                    "common_phrases": [
                        "excited about",
                        "get excited",
                        "so excited",
                        "very excited"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий сильный энтузиазм, страсть и энергию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взволнованный",
                                "target_lang_sense_clarification": "находящийся в состоянии приятного волнения или ожидания"
                            },
                            {
                                "target_lang_word": "в восторге",
                                "target_lang_sense_clarification": "испытывающий сильную радость и восхищение"
                            },
                            {
                                "target_lang_word": "радостный",
                                "target_lang_sense_clarification": "испытывающий чувство радости"
                            },
                            {
                                "target_lang_word": "взбудораженный",
                                "target_lang_sense_clarification": "находящийся в состоянии возбуждения, беспокойства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "840b47aa-2cd7-317b-b4f6-e8258ff23d4c",
                    "sense_definition": "Sexually aroused.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sexual_arousal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt strangely <w>excited</w> by the romantic scene in the movie.",

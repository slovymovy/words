            "senses": [
                {
                    "sense_id": "2acb9b44-2c29-3990-becb-58825354f38d",
                    "sense_definition": "w każdej sytuacji, za każdym razem, gdy coś się dzieje",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat <zawsze> wstaje wcześnie rano.",
                            "target_lang_translations": {
                                "ru": "Мой брат <всегда> встает рано утром."
                            }
                        },
                        {
                            "text": "Ona <zawsze> pamięta o moich urodzinach.",
                            "target_lang_translations": {
                                "ru": "Она <всегда> помнит о моем дне рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "za każdym razem",
                        "każdorazowo"
                    ],
                    "antonyms": [
                        "nigdy"
                    ],
                    "common_phrases": [
                        "jak zawsze",
                        "prawie zawsze",
                        "zawsze i wszędzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "в любой ситуации, каждый раз, когда что-то происходит"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всегда",
                                "target_lang_sense_clarification": "каждый раз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3b8536f-302e-3463-ab4a-25e071624661",
                    "sense_definition": "przez cały czas, bez przerwy, nieustannie",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Będę cię <zawsze> kochać.",
                            "target_lang_translations": {
                                "ru": "Я буду любить тебя <всегда>."
                            }
                        },
                        {
                            "text": "Drzwi do naszego domu są dla ciebie <zawsze> otwarte.",
                            "target_lang_translations": {
                                "ru": "Двери нашего дома <всегда> открыты для тебя."
                            }
                        }
                    ],
                    "synonyms": [
                        "stale",
                        "ciągle",
                        "wciąż",
                        "bezustannie",
                        "wiecznie",
                        "nieustannie"
                    ],
                    "antonyms": [
                        "nigdy"
                    ],
                    "common_phrases": [
                        "na zawsze",
                        "raz na zawsze"
                    ],
                    "target_lang_definitions": {
                        "ru": "всё время, постоянно, непрерывно"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всегда",
                                "target_lang_sense_clarification": "постоянно"
                            },
                            {
                                "target_lang_word": "вечно",
                                "target_lang_sense_clarification": "бесконечно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "290ebf66-7932-3923-9ce2-e31f7fe99045",
                    "sense_definition": "słowo używane do wyrażenia, że mimo trudności sytuacja ma pewne pozytywne strony; przynajmniej",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "modal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wygraliśmy meczu, ale <zawsze> zdobyliśmy nowe doświadczenie.",
                            "target_lang_translations": {
                                "ru": "Мы не выиграли матч, но <всё же> получили новый опыт."
                            }
                        },
                        {
                            "text": "To niewiele pieniędzy, ale <zawsze> coś.",
                            "target_lang_translations": {
                                "ru": "Это небольшие деньги, но <всё же> кое-что."
                            }
                        }
                    ],
                    "synonyms": [
                        "wciąż",
                        "stale"
                    ],
                    "common_phrases": [
                        "zawsze coś",
                        "to zawsze coś"
                    ],
                    "target_lang_definitions": {
                        "ru": "по крайней мере; слово, указывающее на положительный аспект в сложной ситуации"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всё же",
                                "target_lang_sense_clarification": "тем не менее"
                            },
                            {
                                "target_lang_word": "всё-таки",
                                "target_lang_sense_clarification": "несмотря ни на что"
                            },
                            {
                                "target_lang_word": "по крайней мере",
                                "target_lang_sense_clarification": "как минимум"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0203b456-31b4-3277-9500-b5ae90685368",
                    "sense_definition": "słowo wskazujące na istnienie jakiejś możliwości lub alternatywy, często w ostateczności",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "modal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli nie zdążysz na pociąg, <zawsze> możesz pojechać autobusem.",
                            "target_lang_translations": {
                                "ru": "Если ты не успеешь на поезд, ты <всегда> можешь поехать на автобусе."
                            }
                        },
                        {
                            "text": "<Zawsze> możemy spróbować jeszcze raz.",
                            "target_lang_translations": {
                                "ru": "Мы <всегда> можем попробовать еще раз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zawsze można"
                    ],
                    "target_lang_definitions": {
                        "ru": "слово, указывающее на наличие возможности или альтернативы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всегда",
                                "target_lang_sense_clarification": "в любой момент"
                            }
                        ]
                    }
                }
            ]

                {
                    "sense_id": "27642010-dcbc-3af6-a2cb-c89f55140697",
                    "sense_definition": "Strumień wody płynący naturalnym lub sztucznym korytem; ogólne określenie rzek, potoków, strumieni i kanałów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geography_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie znajduje się niewielki <ciek>, który latem często wysycha.",
                            "target_lang_translations": {
                                "ru": "В лесу находится небольшой <w>водоток</w>, который летом часто пересыхает."
                            }
                        },
                        {
                            "text": "Każdy naturalny <ciek> wodny jest ważnym elementem lokalnego ekosystemu.",
                            "target_lang_translations": {
                                "ru": "Каждый естественный <w>водоток</w> является важным элементом местной экосистемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgrzebło"
                    ],
                    "common_phrases": [
                        "ciek wodny",
                        "ciek naturalny",
                        "ciek sztuczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поток воды, текущий по естественному или искусственному руслу; общее название рек, ручьев и каналов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водоток",
                                "target_lang_sense_clarification": "гидрология"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "течение воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c594107-89bd-38d4-b072-463648b28a93",
                    "sense_definition": "Noga ptaka z rodziny kuraków (np. głuszca lub cietrzewia).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hunting_anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myśliwy rozpoznał gatunek ptaka, patrząc na jego opierzone <cieki>.",
                            "target_lang_translations": {
                                "ru": "Охотник определил вид птицы, глядя на её оперенные <w>лапы</w>."
                            }
                        },
                        {
                            "text": "U głuszca <cieki> są przystosowane do poruszania się po głębokim śniegu.",
                            "target_lang_translations": {
                                "ru": "У глухаря <w>лапы</w> приспособлены для передвижения по глубокому снегу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgrzebło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нога птицы из семейства куриных (например, глухаря или тетерева)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лапа",
                                "target_lang_sense_clarification": "охотничий термин"
                            },
                            {
                                "target_lang_word": "нога",
                                "target_lang_sense_clarification": "анатомия птиц"
                            }
                        ]
                    }
                }

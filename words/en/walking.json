                {
                    "sense_id": "3fbb4267-fac2-31f1-bb89-e4535a0ae11d",
                    "sense_definition": "Used to describe a person who embodies a particular quality or characteristic to an extreme degree, as if they were a living version of it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandfather is a <w>walking</w> dictionary of local history.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка — <w>ходячая</w> энциклопедия местной истории."
                            }

{
    "word_family": [
        "lepelen",
        "theelepel",
        "eetlepel",
        "soeplepel",
        "dessertlepel",
        "opscheplepel",
        "roerlepel"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a8b00e9-d5a1-3767-94f8-902d33b9eb48",
                    "sense_definition": "Een voorwerp bestaande uit een steel en een hol gedeelte, waarmee vloeibaar voedsel wordt gegeten of geroerd.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cutlery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik eet mijn soep altijd met een grote <w>lepel</w>.",
                            "target_lang_translations": {
                                "ru": "Я всегда ем суп большой <w>ложкой</w>."
                            }
                        },
                        {
                            "text": "Hij roert met een <w>lepel</w> de suiker door zijn koffie.",
                            "target_lang_translations": {
                                "ru": "Он размешивает сахар в кофе <w>ложкой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "met een lepel",
                        "zilveren lepel",
                        "met de paplepel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, состоящий из ручки и углубления, которым едят жидкую пищу или размешивают что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ложка",
                                "target_lang_sense_clarification": "столовый прибор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d0685fa-07d4-3cfb-b400-e7ca60157ad7",
                    "sense_definition": "Een hoeveelheid die op een lepel past, vaak gebruikt als maat in recepten.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cutlery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voeg een <w>lepel</w> olijfolie toe aan de salade.",
                            "target_lang_translations": {
                                "ru": "Добавьте <w>ложку</w> оливкового масла в салат."
                            }
                        },
                        {
                            "text": "Neem elke ochtend een <w>lepel</w> van dit medicijn.",
                            "target_lang_translations": {
                                "ru": "Принимайте по <w>ложке</w> этого лекарства каждое утро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een lepel suiker",
                        "halve lepel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество, которое помещается в ложку; часто используется как мера в рецептах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ложка",
                                "target_lang_sense_clarification": "мера объёма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90d1cbeb-78ba-3da0-931e-1e2afc1d079a",
                    "sense_definition": "Een glimmend stuk kunstaas in de vorm van een lepel, gebruikt bij het vissen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De visser bevestigde een zilveren <w>lepel</w> aan zijn hengel.",
                            "target_lang_translations": {
                                "ru": "Рыбак прикрепил серебряную <w>блесну</w> к своей удочке."
                            }
                        },
                        {
                            "text": "Met deze <w>lepel</w> hoopte hij een grote snoek te vangen.",
                            "target_lang_translations": {
                                "ru": "С этой <w>блесной</w> он надеялся поймать большую щуку."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Блестящая искусственная приманка в форме ложки, используемая при рыбной ловле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блесна",
                                "target_lang_sense_clarification": "рыболовная снасть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4ad0ba6-5158-36de-863d-e256d797532a",
                    "sense_definition": "Het oor van een haas of konijn (in jagerstaal).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hunting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De haas bewoog zijn linker <w>lepel</w> om geluiden op te vangen.",
                            "target_lang_translations": {
                                "ru": "Заяц пошевелил левым <w>ухом</w>, чтобы уловить звуки."
                            }
                        },
                        {
                            "text": "Boven het hoge gras staken alleen de <w>lepels</w> van het konijn uit.",
                            "target_lang_translations": {
                                "ru": "Над высокой травой торчали только <w>уши</w> кролика."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ухо зайца или кролика (на охотничьем жаргоне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухо",
                                "target_lang_sense_clarification": "у животного"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0ddc4234-56ce-3992-9550-eaefd700301a",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>lepelen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>lepel</w> de laatste restjes soep uit de kom.",
                            "target_lang_translations": {
                                "ru": "Я <w>вычерпываю</w> остатки супа из миски."
                            }
                        },
                        {
                            "text": "Met veel smaak <w>lepel</w> ik mijn toetje naar binnen.",
                            "target_lang_translations": {
                                "ru": "Я с большим аппетитом <w>ем ложкой</w> свой десерт."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола <w>lepelen</w> (есть ложкой, черпать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ем ложкой",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "вычерпываю",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a794e1a-e841-33e8-8f78-31ad86b28152",
                    "sense_definition": "Gebiedende wijs van <w>lepelen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Lepel</w> je bord maar snel leeg voordat het koud wordt.",
                            "target_lang_translations": {
                                "ru": "<w>Ешь ложкой</w> всё с тарелки побыстрее, пока не остыло."
                            }
                        },
                        {
                            "text": "<w>Lepel</w> de saus voorzichtig over de aardappelen.",
                            "target_lang_translations": {
                                "ru": "<w>Полей ложкой</w> соус осторожно на картофель."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола <w>lepelen</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ешь ложкой",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            },
                            {
                                "target_lang_word": "черпай",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
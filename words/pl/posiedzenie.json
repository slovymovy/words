                {
                    "sense_id": "5c083bc0-d72e-3393-b171-0112765b60ac",
                    "sense_definition": "Oficjalne spotkanie członków jakiegoś gremium (np. sądu, rządu, komisji) w celu omówienia spraw i podjęcia decyzji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "meeting_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro odbędzie się ważne <w>posiedzenie</w> zarządu naszej firmy.",
                            "target_lang_translations": {
                                "ru": "Завтра состоится важное <w>заседание</w> правления нашей фирмы."
                            }
                        },
                        {
                            "text": "Sędzia zamknął <w>posiedzenie</w> po przesłuchaniu wszystkich świadków.",
                            "target_lang_translations": {
                                "ru": "Судья закрыл <w>заседание</w> после допроса всех свидетелей."
                            }
                        }
                    ],
                    "synonyms": [
                        "obrada",
                        "sesja"
                    ],
                    "common_phrases": [
                        "posiedzenie rządu",
                        "posiedzenie sądu",
                        "tajne posiedzenie",
                        "zwołać posiedzenie",
                        "otworzyć posiedzenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное собрание членов какой-либо организации (например, суда, правительства, комиссии) для обсуждения вопросов и принятия решений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заседание",
                                "target_lang_sense_clarification": "собрание, совещание"

{
    "word_family": [
        "hospicyjny",
        "hospicjalny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb27b15c-7ae4-3cf0-b6c8-c237daeffca4",
                    "sense_definition": "Zakład świadczący opiekę medyczną, psychologiczną i duchową osobom nieuleczalnie chorym oraz umierającym.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia trafiła do <w>hospicjum</w>, gdzie ma zapewnioną profesjonalną opiekę.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка попала в <w>хоспис</w>, где ей обеспечен профессиональный уход."
                            }
                        },
                        {
                            "text": "Nasz zakład jest jednym z największych stacjonarnych <w>hospicjów</w> w tym regionie.",
                            "target_lang_translations": {
                                "ru": "Наше учреждение является одним из крупнейших стационарных <w>хосписов</w> в этом регионе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hospicjum domowe",
                        "hospicjum stacjonarne",
                        "wolontariusz w hospicjum",
                        "opieka w hospicjum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медицинское учреждение, предоставляющее уход и поддержку неизлечимо больным людям в конце жизни"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хоспис",
                                "target_lang_sense_clarification": "учреждение для паллиативной помощи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82f5c96c-6fce-3875-a20c-013b1f3c8bea",
                    "sense_definition": "Schronisko dla podróżnych lub dom gościnny, często prowadzony przez zakonników (znaczenie historyczne).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "historical_lodging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W górach zbudowano małe <w>hospicjum</w> dla zmęczonych wędrowców.",
                            "target_lang_translations": {
                                "ru": "В горах построили маленький <w>приют</w> для уставших путников."
                            }
                        },
                        {
                            "text": "W miejscach pielgrzymkowych funkcjonują <w>hospicja</w> dla pątników i podróżnych.",
                            "target_lang_translations": {
                                "ru": "В местах паломничества действуют <w>странноприимные дома</w> для паломников и путешественников."
                            }
                        }
                    ],
                    "synonyms": [
                        "gospoda",
                        "schronisko"
                    ],
                    "common_phrases": [
                        "hospicjum dla pielgrzymów"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приют для путешественников или гостевой дом, часто содержащийся монахами (историческое значение)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "странноприимный дом",
                                "target_lang_sense_clarification": "исторический термин"
                            },
                            {
                                "target_lang_word": "приют",
                                "target_lang_sense_clarification": "убежище для путников"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89d5eb43-488c-35b6-ad02-a3c7963832aa",
                    "sense_definition": "Miejsce zamieszkania dla uczącej się młodzieży, rodzaj stancji (znaczenie historyczne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_lodging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody student znalazł tanie <w>hospicjum</w> w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Молодой студент нашел дешевый <w>пансион</w> в центре города."
                            }
                        },
                        {
                            "text": "W tym samym roku został rektorem <w>hospicjum</w> św. Stanisława w Krakowie.",
                            "target_lang_translations": {
                                "ru": "В том же году он стал ректором <w>общежития</w> св. Станислава в Кракове."
                            }
                        }
                    ],
                    "synonyms": [
                        "stancja"
                    ],
                    "common_phrases": [
                        "rektor hospicjum"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место проживания для учащихся, вид общежития или пансиона (историческое значение)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пансион",
                                "target_lang_sense_clarification": "жилье для студентов"
                            },
                            {
                                "target_lang_word": "общежитие",
                                "target_lang_sense_clarification": "место проживания учащихся"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
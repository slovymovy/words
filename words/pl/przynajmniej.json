    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e96bee2f-ccbe-384c-8099-767132dd049d",
                    "sense_definition": "Oznacza, że podana ilość, liczba lub miara jest najmniejszą możliwą lub akceptowalną; nie mniej niż.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żeby naprawić twój rower, potrzebuję <w>przynajmniej</w> tygodnia.",
                            "target_lang_translations": {
                                "ru": "Чтобы починить твой велосипед, мне нужна <w>по крайней мере</w> неделя."
                            }
                        },
                        {
                            "text": "W tym pokoju musi być <w>przynajmniej</w> jedno wolne krzesło.",
                            "target_lang_translations": {
                                "ru": "В этой комнате должен быть <w>как минимум</w> один свободный стул."
                            }
                        }
                    ],
                    "synonyms": [
                        "co najmniej"
                    ],
                    "antonyms": [
                        "co najwyżej"
                    ],
                    "common_phrases": [
                        "przynajmniej raz",
                        "przynajmniej dwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Означает, что указанное количество, число или мера является наименьшим возможным или приемлемым; не менее чем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как минимум",
                                "target_lang_sense_clarification": "о количестве или времени"
                            },
                            {
                                "target_lang_word": "по меньшей мере",
                                "target_lang_sense_clarification": "формально"
                            },
                            {
                                "target_lang_word": "по крайней мере",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8132b10-3235-3503-ab1e-f03a5c3d048f",
                    "sense_definition": "Słowo używane, gdy mówiący ogranicza swoje oczekiwania do minimum lub wskazuje na jedyny pozytywny aspekt w danej sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mitigation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jak bierzesz mój rower, to mógłbyś <w>przynajmniej</w> zapytać.",
                            "target_lang_translations": {
                                "ru": "Если берешь мой велосипед, мог бы <w>хотя бы</w> спросить."
                            }
                        },
                        {
                            "text": "Nie wygraliśmy meczu, ale <w>przynajmniej</w> dobrze się bawiliśmy.",
                            "target_lang_translations": {
                                "ru": "Мы не выиграли матч, но <w>по крайней мере</w> хорошо провели время."
                            }
                        }
                    ],
                    "synonyms": [
                        "chociaż",
                        "w każdym razie",
                        "choćby"
                    ],
                    "common_phrases": [
                        "a przynajmniej",
                        "przynajmniej tyle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое, когда говорящий ограничивает свои ожидания до минимума или указывает на единственный положительный аспект в данной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по крайней мере",
                                "target_lang_sense_clarification": "указание на положительный аспект"
                            },
                            {
                                "target_lang_word": "хотя бы",
                                "target_lang_sense_clarification": "ограничение ожиданий"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
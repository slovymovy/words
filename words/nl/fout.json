{
    "word_family": [
        "foutief",
        "foutloos"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "de7322d7-5871-3f67-b61f-bd47145b0f5f",
                    "sense_definition": "Een onjuistheid of vergissing; iets wat niet goed is gedaan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "incorrectness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb per ongeluk een <w>fout</w> gemaakt in de berekening.",
                            "target_lang_translations": {
                                "en": "I accidentally made a <w>mistake</w> in the calculation."
                            }
                        },
                        {
                            "text": "Het is niet jouw <w>fout</w> dat we de trein hebben gemist.",
                            "target_lang_translations": {
                                "en": "It is not your <w>fault</w> that we missed the train."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergissing",
                        "miskleun"
                    ],
                    "common_phrases": [
                        "een fout maken",
                        "mijn fout",
                        "grote fout",
                        "bij de fout"
                    ],
                    "target_lang_definitions": {
                        "en": "An inaccuracy or mistake; something that has not been done correctly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mistake",
                                "target_lang_sense_clarification": "an accidental error or incorrect action"
                            },
                            {
                                "target_lang_word": "fault",
                                "target_lang_sense_clarification": "responsibility for a mistake or bad situation"
                            },
                            {
                                "target_lang_word": "error",
                                "target_lang_sense_clarification": "a formal inaccuracy or miscalculation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8203355a-80a1-3fe7-a156-696b1e1a7aa9",
                    "sense_definition": "Een defect of onvolkomenheid in een technisch systeem, apparaat of proces.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "defect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door een technische <w>fout</w> startte de computer niet meer op.",
                            "target_lang_translations": {
                                "en": "Due to a technical <w>error</w>, the computer would no longer start up."
                            }
                        },
                        {
                            "text": "De monteur probeert de <w>fout</w> in de motor te vinden.",
                            "target_lang_translations": {
                                "en": "The mechanic is trying to find the <w>fault</w> in the engine."
                            }
                        }
                    ],
                    "synonyms": [
                        "defect",
                        "mankement",
                        "gebrek"
                    ],
                    "common_phrases": [
                        "technische fout",
                        "fout in het systeem"
                    ],
                    "target_lang_definitions": {
                        "en": "A defect or imperfection in a technical system, device, or process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fault",
                                "target_lang_sense_clarification": "a defect in a mechanism or structure"
                            },
                            {
                                "target_lang_word": "error",
                                "target_lang_sense_clarification": "a failure in a computer system or program"
                            },
                            {
                                "target_lang_word": "defect",
                                "target_lang_sense_clarification": "an imperfection or flaw"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f458444c-a1ea-373f-8a9e-5163ca69c098",
                    "sense_definition": "Niet juist of niet correct; in strijd met de waarheid of de regels.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "incorrectness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is helaas een <w>fout</w> antwoord op de vraag.",
                            "target_lang_translations": {
                                "en": "Unfortunately, that is a <w>wrong</w> answer to the question."
                            }
                        },
                        {
                            "text": "Ik ben bang dat ik het <w>foute</w> adres heb ingevoerd.",
                            "target_lang_translations": {
                                "en": "I am afraid I entered the <w>wrong</w> address."
                            }
                        }
                    ],
                    "synonyms": [
                        "verkeerd",
                        "onjuist",
                        "mis"
                    ],
                    "antonyms": [
                        "goed",
                        "juist",
                        "correct"
                    ],
                    "common_phrases": [
                        "fout antwoord",
                        "helemaal fout",
                        "fout gaan"
                    ],
                    "target_lang_definitions": {
                        "en": "Not right or correct; contrary to truth or rules."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wrong",
                                "target_lang_sense_clarification": "incorrect or not true"
                            },
                            {
                                "target_lang_word": "incorrect",
                                "target_lang_sense_clarification": "erroneous or inaccurate"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d39e006a-262e-3c2f-a4d3-1b3a81ef95af",
                    "sense_definition": "Niet in overeenstemming met de goede smaak of sociale normen; vaak gebruikt voor muziek, kleding of gedrag dat ordinair of kitscherig is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "bad_taste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op het feest draaide de dj de hele avond <w>foute</w> muziek.",
                            "target_lang_translations": {
                                "en": "At the party, the DJ played <w>cheesy</w> music all evening."
                            }
                        },
                        {
                            "text": "Hij droeg een ontzettend <w>foute</w> trui met glitters.",
                            "target_lang_translations": {
                                "en": "He wore a terribly <w>tacky</w> sweater with glitter."
                            }
                        }
                    ],
                    "synonyms": [
                        "ordinair",
                        "kitscherig"
                    ],
                    "common_phrases": [
                        "foute muziek",
                        "foute party",
                        "foute man"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe guilty pleasures or tacky style"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Not in accordance with good taste or social norms; often used for music, clothing, or behavior that is tacky or kitschy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tacky",
                                "target_lang_sense_clarification": "showing poor taste or style"
                            },
                            {
                                "target_lang_word": "cheesy",
                                "target_lang_sense_clarification": "unsophisticated, kitschy, or a guilty pleasure"
                            },
                            {
                                "target_lang_word": "wrong",
                                "target_lang_sense_clarification": "socially inappropriate or bad"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da2aedfb-31f9-3613-af21-62e672914553",
                    "sense_definition": "Collaborerend met de vijand (specifiek de Duitse bezetter) tijdens de Tweede Wereldoorlog.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "war_history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de bevrijding werd onderzocht wie er <w>fout</w> was geweest in de oorlog.",
                            "target_lang_translations": {
                                "en": "After the liberation, it was investigated who had been <w>collaborating</w> during the war."
                            }
                        },
                        {
                            "text": "Het was een publiek geheim dat zijn familie <w>fout</w> was.",
                            "target_lang_translations": {
                                "en": "It was an open secret that his family was <w>on the wrong side</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "goed"
                    ],
                    "common_phrases": [
                        "fout in de oorlog"
                    ],
                    "target_lang_definitions": {
                        "en": "Collaborating with the enemy (specifically the German occupier) during World War II."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "collaborating",
                                "target_lang_sense_clarification": "cooperating traitorously with an enemy"
                            },
                            {
                                "target_lang_word": "wrong",
                                "target_lang_sense_clarification": "on the immoral or enemy side during the war"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "93b12c8d-256c-3fe6-8521-66dfc74a8c67",
                    "sense_definition": "Op een onjuiste of verkeerde manier.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "incorrectness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je hebt mijn achternaam <w>fout</w> geschreven op het formulier.",
                            "target_lang_translations": {
                                "en": "You wrote my surname <w>wrong</w> on the form."
                            }
                        },
                        {
                            "text": "Als je het <w>fout</w> doet, moet je opnieuw beginnen.",
                            "target_lang_translations": {
                                "en": "If you do it <w>wrong</w>, you have to start over."
                            }
                        }
                    ],
                    "synonyms": [
                        "verkeerd",
                        "onjuist"
                    ],
                    "antonyms": [
                        "goed",
                        "correct"
                    ],
                    "common_phrases": [
                        "fout doen",
                        "fout parkeren",
                        "fout gegaan"
                    ],
                    "target_lang_definitions": {
                        "en": "In an incorrect or wrong manner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wrong",
                                "target_lang_sense_clarification": "incorrectly (informal)"
                            },
                            {
                                "target_lang_word": "incorrectly",
                                "target_lang_sense_clarification": "in an inaccurate way"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
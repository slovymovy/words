    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ca11f2de-0595-3e0f-bbe8-d72e0cab4039",
                    "sense_definition": "Ciemny kształt powstający na powierzchni, gdy obiekt zasłania źródło światła.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "optical_shadow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W słoneczny dzień każde drzewo rzuca długi <w>cień</w> na trawę.",
                            "target_lang_translations": {
                                "ru": "В солнечный день каждое дерево отбрасывает длинную <w>тень</w> на траву."
                            }
                        },
                        {
                            "text": "Dziecko bawiło się, próbując złapać swój własny <w>cień</w> na chodniku.",
                            "target_lang_translations": {
                                "ru": "Ребёнок играл, пытаясь поймать свою собственную <w>тень</w> на тротуаре."
                            }
                        }
                    ],
                    "antonyms": [
                        "światło"
                    ],
                    "common_phrases": [
                        "rzucać cień",
                        "długi cień",
                        "własny cień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тёмный силуэт, возникающий на поверхности, когда объект загораживает источник света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тень",
                                "target_lang_sense_clarification": "тёмный силуэт от объекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09e7577e-090e-34ab-bdb6-2c7a06b6e4fb",
                    "sense_definition": "Miejsce osłonięte przed słońcem, gdzie jest chłodniej i ciemniej niż na otwartej przestrzeni.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "optical_shadow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W upalne południe najlepiej odpoczywa się w głębokim <w>cieniu</w>.",
                            "target_lang_translations": {
                                "ru": "В знойный полдень лучше всего отдыхать в глубокой <w>тени</w>."
                            }
                        },
                        {
                            "text": "Usiadłem w <w>cieniu</w> parasola, aby schronić się przed słońcem.",
                            "target_lang_translations": {
                                "ru": "Я сел в <w>тени</w> зонта, чтобы укрыться от солнца."
                            }
                        }
                    ],
                    "antonyms": [
                        "światło",
                        "światło"
                    ],
                    "common_phrases": [
                        "w cieniu",
                        "schować się w cieniu",
                        "głęboki cień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, закрытое от солнца, где прохладнее и темнее, чем на открытом пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тень",
                                "target_lang_sense_clarification": "место, защищённое от солнца"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7aa758b3-dfb4-38e1-bb4b-5e2d9ee76d21",
                    "sense_definition": "Kosmetyk kolorowy nakładany na powieki w celu wykonania makijażu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cosmetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wieczorne wyjście nałożyła błyszczący <w>cień</w> do powiek.",
                            "target_lang_translations": {
                                "ru": "Для вечернего выхода она нанесла блестящие <w>тени</w> для век."
                            }
                        },
                        {
                            "text": "W jej kosmetyczce brakowało pędzelka, by nałożyć <w>cień</w>.",
                            "target_lang_translations": {
                                "ru": "В её косметичке не хватало кисточки, чтобы нанести <w>тени</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cień do powiek"
                    ],
                    "common_phrases": [
                        "cień do powiek",
                        "paleta cieni",
                        "nakładać cienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Косметическое средство, наносимое на веки для макияжа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тени",
                                "target_lang_sense_clarification": "косметика для век"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55d7a843-b70c-37c2-9ee6-c3f9ec7bbd17",
                    "sense_definition": "Bardzo mała ilość, słaba oznaka lub ślad czegoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_trace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie miał nawet <w>cienia</w> wątpliwości, że podjął słuszną decyzję.",
                            "target_lang_translations": {
                                "ru": "У него не было даже <w>тени</w> сомнения, что он принял правильное решение."
                            }
                        },
                        {
                            "text": "Lekarz powiedział, że istnieje <w>cień</w> szansy na pełne wyzdrowienie.",
                            "target_lang_translations": {
                                "ru": "Врач сказал, что есть <w>тень</w> надежды на полное выздоровление."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cień nadziei",
                        "cień szansy",
                        "cień wątpliwości",
                        "cień uśmiechu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень малое количество, слабый признак или след чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тень",
                                "target_lang_sense_clarification": "малейший признак, след"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a63b6f3f-cf9c-37d1-9c00-7274ed1039de",
                    "sense_definition": "Słabo widoczny, niewyraźny zarys postaci lub przedmiotu, zwykle w ciemności.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "optical_shadow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We mgle widziałem tylko niewyraźny <w>cień</w> nadchodzącego człowieka.",
                            "target_lang_translations": {
                                "ru": "В тумане я видел лишь неясную <w>тень</w> приближающегося человека."
                            }
                        },
                        {
                            "text": "Jakiś tajemniczy <w>cień</w> przemknął szybko przez ciemną ulicę.",
                            "target_lang_translations": {
                                "ru": "Какая-то таинственная <w>тень</w> быстро промелькнула по тёмной улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "sylwetka",
                        "zjawa"
                    ],
                    "common_phrases": [
                        "przemykać jak cień",
                        "cienie nocy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слабо видимое, неясное очертание фигуры или предмета, обычно в темноте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тень",
                                "target_lang_sense_clarification": "неясный силуэт"
                            },
                            {
                                "target_lang_word": "силуэт",
                                "target_lang_sense_clarification": "очертание предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bf11aad-bc37-39d9-94b8-fb2cfa10963a",
                    "sense_definition": "Duch osoby zmarłej lub istota nadprzyrodzona.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "supernatural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stare legendy mówią, że nocą po zamku błąkają się <w>cienie</w> przodków.",
                            "target_lang_translations": {
                                "ru": "Старые легенды гласят, что по ночам в замке бродят <w>тени</w> предков."
                            }
                        },
                        {
                            "text": "Bohater powieści próbował rozmawiać z <w>cieniami</w> w zaświatach.",
                            "target_lang_translations": {
                                "ru": "Герой романа пытался разговаривать с <w>тенями</w> в загробном мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "duch",
                        "zjawa",
                        "mara"
                    ],
                    "common_phrases": [
                        "kraina cieni",
                        "wywoływać cienie"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or poetic context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дух умершего человека или сверхъестественное существо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тень",
                                "target_lang_sense_clarification": "дух умершего"
                            },
                            {
                                "target_lang_word": "призрак",
                                "target_lang_sense_clarification": "видение, дух"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1e55afc7-a11f-388a-a2a3-5797727b36ac",
                    "sense_definition": "Used to express hesitation, uncertainty, or to fill a pause in conversation.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "hesitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Um>, I am not sure if we have enough time for that.",
                            "target_lang_translations": {
                                "ru": "<Эм>, я не уверен, что у нас хватит на это времени."
                            }
                        },
                        {
                            "text": "She asked me, <um>, where I bought my jacket.",
                            "target_lang_translations": {
                                "ru": "Она спросила меня, <эм>, где я купил куртку."
                            }
                        }
                    ],
                    "synonyms": [
                        "er",
                        "uh"
                    ],
                    "common_phrases": [
                        "um I don't know",
                        "um let me see",
                        "um well"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения нерешительности, неуверенности или для заполнения паузы в разговоре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эм",
                                "target_lang_sense_clarification": "междометие для заполнения паузы"
                            },
                            {
                                "target_lang_word": "гм",
                                "target_lang_sense_clarification": "выражение сомнения или раздумья"
                            },
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "вводное слово при заминке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71700d1b-5ecf-38a5-b5fa-67c5d2a77132",
                    "sense_definition": "Used to forcefully or sarcastically call attention to something wrong or to interrupt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Um>, excuse me, but I believe you are sitting in my seat.",
                            "target_lang_translations": {
                                "ru": "<Эм>, простите, но мне кажется, вы сидите на моем месте."
                            }
                        },
                        {
                            "text": "<Um>, did you really just say that out loud?",
                            "target_lang_translations": {
                                "ru": "<Эм>, ты правда только что сказал это вслух?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "um excuse me",
                        "um actually"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы саркастически привлечь внимание к чему-то неправильному или прервать собеседника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эм",
                                "target_lang_sense_clarification": "привлечение внимания"
                            },
                            {
                                "target_lang_word": "кхм",
                                "target_lang_sense_clarification": "звук для привлечения внимания"
                            },
                            {
                                "target_lang_word": "извините",
                                "target_lang_sense_clarification": "вежливое или саркастичное прерывание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c11cd6ae-415f-360d-8ec2-b9bfd5bfef48",
                    "sense_definition": "To make the \"um\" sound to express uncertainty or hesitancy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hesitation_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The speaker <ummed> and ahhed throughout the entire presentation.",
                            "target_lang_translations": {
                                "ru": "Докладчик <экал> и мэкал на протяжении всей презентации."
                            }
                        },
                        {
                            "text": "Stop <umming> and give me a straight answer immediately.",
                            "target_lang_translations": {
                                "ru": "Перестань <экать> и дай мне прямой ответ немедленно."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать звук «эм», выражая неуверенность или нерешительность; «экать»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экать",
                                "target_lang_sense_clarification": "произносить звук «э» или «эм»"
                            },
                            {
                                "target_lang_word": "мэкать",
                                "target_lang_sense_clarification": "мычать в нерешительности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b8660234-4885-37c3-af12-90cad089b63b",
                    "sense_definition": "An occurrence of the interjection \"um\".",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hesitation_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His speech was difficult to follow because of all the <ums>.",
                            "target_lang_translations": {
                                "ru": "Его речь было трудно воспринимать из-за постоянных <эканий>."
                            }
                        },
                        {
                            "text": "Try to reduce the number of <ums> when you speak in public.",
                            "target_lang_translations": {
                                "ru": "Постарайтесь уменьшить количество <слов-паразитов «эм»>, когда выступаете на публике."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Употребление междометия «эм» в речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эканье",
                                "target_lang_sense_clarification": "процесс произнесения звуков-паразитов"
                            },
                            {
                                "target_lang_word": "звук «эм»",
                                "target_lang_sense_clarification": "само междометие как существительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "494fbd24-4b4b-3b56-8005-5af41c4818ea",
                    "sense_definition": "Initialism of the University of Michigan.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "university",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "She decided to apply to <UM> for her graduate studies.",
                            "target_lang_translations": {
                                "ru": "Она решила подать документы в <Мичиганский университет> для обучения в магистратуре."
                            }
                        },
                        {
                            "text": "The <UM> football team has a very large fan base.",
                            "target_lang_translations": {
                                "ru": "У футбольной команды <Мичиганского университета> очень большая база фанатов."
                            }
                        }
                    ],
                    "synonyms": [
                        "UMich",
                        "Michigan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура Мичиганского университета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мичиганский университет",
                                "target_lang_sense_clarification": "учебное заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f05b933a-e35a-3617-b35f-85daa1084e2f",
                    "sense_definition": "A surname of Korean origin.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <Um> will be leading the negotiation team today.",
                            "target_lang_translations": {
                                "ru": "Господин <Ом> сегодня возглавит группу переговорщиков."
                            }
                        },
                        {
                            "text": "I was introduced to a professor named <Um> at the conference.",
                            "target_lang_translations": {
                                "ru": "На конференции меня представили профессору по фамилии <Ом>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "Korean origin"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия корейского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ом",
                                "target_lang_sense_clarification": "корейская фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
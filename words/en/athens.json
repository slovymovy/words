                {
                    "sense_id": "a6840adf-b110-3809-9410-2a8e38920856",
                    "sense_definition": "The capital and largest city of Greece, famous for its ancient history and monuments.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "greece_entity",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We plan to visit <w>Athens</w> during our summer holiday in Europe.",
                            "target_lang_translations": {
                                "ru": "Мы планируем посетить <w>Афины</w> во время нашего летнего отпуска в Европе."
                            }
                        },
                        {
                            "text": "The philosophy and art of ancient <w>Athens</w> still influence the world today.",
                            "target_lang_translations": {
                                "ru": "Философия и искусство древних <w>Афин</w> до сих пор оказывают влияние на мир."
                            }
                        }
                    ],
                    "synonyms": [
                        "Athina"
                    ],
                    "common_phrases": [
                        "ancient Athens",
                        "city of Athens",
                        "downtown Athens"
                    ],
                    "target_lang_definitions": {
                        "ru": "Столица и крупнейший город Греции, известный своей древней историей и памятниками."

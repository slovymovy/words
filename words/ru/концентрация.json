    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d85f702-41f1-3305-b1f6-cb5beada7115",
                    "sense_definition": "Состояние, при котором всё внимание человека направлено на какой-либо объект, мысль или действие; сильная сосредоточенность.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Громкий шум на улице сильно мешает моей <w>концентрации</w> во время работы.",
                            "target_lang_translations": {
                                "en": "Loud noise outside greatly interferes with my <w>concentration</w> while working."
                            }
                        },
                        {
                            "text": "Для успешного решения этой сложной задачи необходима полная <w>концентрация</w> внимания.",
                            "target_lang_translations": {
                                "en": "Full <w>concentration</w> is required to successfully solve this complex task."
                            }
                        }
                    ],
                    "synonyms": [
                        "сосредоточенность",
                        "сфокусированность"
                    ],
                    "antonyms": [
                        "рассеянность"
                    ],
                    "common_phrases": [
                        "концентрация внимания",
                        "терять концентрацию",
                        "полная концентрация",
                        "нарушение концентрации"
                    ],
                    "target_lang_definitions": {
                        "en": "A state in which a person's entire attention is directed towards an object, thought, or action; intense focus."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "concentration",
                                "target_lang_sense_clarification": "mental effort"
                            },
                            {
                                "target_lang_word": "focus",
                                "target_lang_sense_clarification": "attention"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c75784a-ff97-3393-a301-c6f7ba08e59b",
                    "sense_definition": "Количество растворенного вещества, содержащееся в определенном объеме или массе раствора или смеси.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом фруктовом соке слишком высокая <w>концентрация</w> сахара.",
                            "target_lang_translations": {
                                "en": "There is too high a <w>concentration</w> of sugar in this fruit juice."
                            }
                        },
                        {
                            "text": "Специальный прибор показал, что <w>концентрация</w> вредных газов в воздухе превышает норму.",
                            "target_lang_translations": {
                                "en": "A special device showed that the <w>concentration</w> of harmful gases in the air exceeds the limit."
                            }
                        }
                    ],
                    "common_phrases": [
                        "высокая концентрация",
                        "низкая концентрация",
                        "концентрация вещества",
                        "допустимая концентрация"
                    ],
                    "target_lang_definitions": {
                        "en": "The quantity of a dissolved substance contained in a specific volume or mass of a solution or mixture."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "concentration",
                                "target_lang_sense_clarification": "chemistry/physics"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe464625-9c31-381e-bb1e-7df769bcdb73",
                    "sense_definition": "Скопление, собирание большого количества кого-либо или чего-либо в одном месте или в одних руках.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На границе двух государств наблюдалась значительная <w>концентрация</w> военных сил.",
                            "target_lang_translations": {
                                "en": "A significant <w>concentration</w> of military forces was observed at the border of the two states."
                            }
                        },
                        {
                            "text": "В последние годы происходит <w>концентрация</w> капитала в крупных промышленных центрах.",
                            "target_lang_translations": {
                                "en": "In recent years, there has been a <w>concentration</w> of capital in large industrial centers."
                            }
                        }
                    ],
                    "synonyms": [
                        "сосредоточение"
                    ],
                    "antonyms": [
                        "рассредоточение"
                    ],
                    "common_phrases": [
                        "концентрация войск",
                        "концентрация власти",
                        "концентрация усилий",
                        "концентрация производства"
                    ],
                    "target_lang_definitions": {
                        "en": "The accumulation or gathering of a large number of people or things in one place or under one control."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "concentration",
                                "target_lang_sense_clarification": "gathering"
                            },
                            {
                                "target_lang_word": "accumulation",
                                "target_lang_sense_clarification": "buildup"
                            }
                        ]
                    }
                }
            ]
        }
    ]

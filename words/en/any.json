{
    "word_family": [
        "anybody",
        "anyone",
        "anything",
        "anywhere",
        "anyway",
        "anyhow",
        "anymore"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "fc7b6b8b-9499-39c9-9d3f-eff98053c984",
                    "sense_definition": "Used to refer to an indefinite quantity or number, usually in questions or negative sentences.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantifier_existential",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you have <w>any</w> brothers or sisters?",
                            "target_lang_translations": {
                                "ru": "У тебя есть <w>какие-нибудь</w> братья или сестры?"
                            }
                        },
                        {
                            "text": "I do not have <w>any</w> money in my wallet.",
                            "target_lang_translations": {
                                "ru": "У меня в кошельке нет <w>никаких</w> денег."
                            }
                        }
                    ],
                    "antonyms": [
                        "no#Determiner"
                    ],
                    "common_phrases": [
                        "any questions",
                        "any idea",
                        "any luck",
                        "any good"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения неопределенного количества или числа, обычно в вопросах или отрицательных предложениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какой-нибудь",
                                "target_lang_sense_clarification": "в вопросительных предложениях"
                            },
                            {
                                "target_lang_word": "никакой",
                                "target_lang_sense_clarification": "в отрицательных предложениях"
                            },
                            {
                                "target_lang_word": "какой-либо",
                                "target_lang_sense_clarification": "формальный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "683ecd69-f1b6-32ed-be64-2d362b98f88d",
                    "sense_definition": "Used to refer to one or more things or people, when it does not matter which one is chosen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantifier_universal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can call me for help at <w>any</w> time.",
                            "target_lang_translations": {
                                "ru": "Ты можешь позвать меня на помощь в <w>любое</w> время."
                            }
                        },
                        {
                            "text": "Pick a card, <w>any</w> card.",
                            "target_lang_translations": {
                                "ru": "Выбери карту, <w>любую</w> карту."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in any case",
                        "at any rate",
                        "any other",
                        "any way"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения одного или нескольких предметов или людей, когда выбор не имеет значения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любой",
                                "target_lang_sense_clarification": "какой угодно"
                            },
                            {
                                "target_lang_word": "всякий",
                                "target_lang_sense_clarification": "разного рода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6caea27-b528-3a52-a824-a86d0ab87ddf",
                    "sense_definition": "Used with units of time to indicate that something will happen very soon.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantifier_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train should be arriving <w>any</w> minute now.",
                            "target_lang_translations": {
                                "ru": "Поезд должен прибыть в <w>любую</w> минуту."
                            }
                        },
                        {
                            "text": "She expects the package to be delivered <w>any</w> day.",
                            "target_lang_translations": {
                                "ru": "Она ожидает, что посылку доставят в <w>любой</w> день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "any day now",
                        "any minute",
                        "any second"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется с единицами измерения времени, чтобы указать, что что-то произойдет очень скоро."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любой",
                                "target_lang_sense_clarification": "в любой момент"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "9c1a50aa-9d91-36b8-984a-ce16d95b0d60",
                    "sense_definition": "Any person, thing, or quantity; one or some out of a group.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I checked the fridge for eggs, but I didn't find <w>any</w>.",
                            "target_lang_translations": {
                                "ru": "Я проверил холодильник на наличие яиц, но не нашел <w>ни одного</w>."
                            }
                        },
                        {
                            "text": "Does <w>any</w> of this make sense to you?",
                            "target_lang_translations": {
                                "ru": "<w>Что-нибудь</w> из этого имеет для тебя смысл?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "any of them",
                        "if any",
                        "hardly any"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой человек, предмет или количество; один или несколько из группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любой",
                                "target_lang_sense_clarification": "кто или что угодно"
                            },
                            {
                                "target_lang_word": "какой-нибудь",
                                "target_lang_sense_clarification": "неопределенный объект"
                            },
                            {
                                "target_lang_word": "никакой",
                                "target_lang_sense_clarification": "ни один (в отрицаниях)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2b8594dc-3a84-31d2-b76c-2a602acaabf4",
                    "sense_definition": "Used to emphasize a comparative adjective or adverb, or in negative sentences to mean 'at all'.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "adverb_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot wait here <w>any</w> longer.",
                            "target_lang_translations": {
                                "ru": "Я не могу здесь <w>больше</w> ждать."
                            }
                        },
                        {
                            "text": "Is your father feeling <w>any</w> better today?",
                            "target_lang_translations": {
                                "ru": "Твой отец чувствует себя сегодня <w>хоть немного</w> лучше?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "any longer",
                        "any more",
                        "any further",
                        "any better"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления сравнительной степени прилагательного или наречия, или в отрицательных предложениях в значении «совсем»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "больше",
                                "target_lang_sense_clarification": "в значении «больше не»"
                            },
                            {
                                "target_lang_word": "ещё",
                                "target_lang_sense_clarification": "усиление сравнения"
                            },
                            {
                                "target_lang_word": "хоть сколько-нибудь",
                                "target_lang_sense_clarification": "в значении «хоть немного»"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
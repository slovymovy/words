                            }
                        },
                        {
                            "text": "We are looking for a family named <w>Race</w> in the phone book.",
                            "target_lang_translations": {
                                "ru": "Мы ищем семью по фамилии <w>Рейс</w> в телефонной книге."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fd549c93-fa74-339d-acb6-ff9a64d5b64c",
                    "sense_definition": "Het vermogen van een persoon of zaak om interesse op te wekken of mensen naar zich toe te trekken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het nieuwe museum heeft een grote <w>aantrekkingskracht</w> op buitenlandse toeristen.",
                            "target_lang_translations": {
                                "ru": "Новый музей обладает большой <w>притягательностью</w> для иностранных туристов.",
                                "en": "The new museum holds a great <w>attraction</w> for foreign tourists."
                            }
                        },
                        {
                            "text": "Ondanks hun verschillen voelden ze een sterke wederzijdse <w>aantrekkingskracht</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на различия, они чувствовали сильное взаимное <w>влечение</w>.",
                                "en": "Despite their differences, they felt a strong mutual <w>attraction</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "charme",
                        "aantrekking",
                        "bekoring",
                        "attractie"
                    ],
                    "antonyms": [
                        "afstoting"
                    ],
                    "common_phrases": [
                        "grote aantrekkingskracht",
                        "seksuele aantrekkingskracht",
                        "wederzijdse aantrekkingskracht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность человека или предмета вызывать интерес или притягивать к себе людей.",
                        "en": "The ability of a person or thing to arouse interest or draw people towards them."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "притягательность",
                                "target_lang_sense_clarification": "свойство вызывать интерес"
                            },
                            {
                                "target_lang_word": "влечение",
                                "target_lang_sense_clarification": "чувство симпатии или страсти к человеку"
                            },
                            {
                                "target_lang_word": "притяжение",
                                "target_lang_sense_clarification": "в переносном смысле: тяга друг к другу"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "attraction",
                                "target_lang_sense_clarification": "power to draw interest or desire"
                            },
                            {
                                "target_lang_word": "appeal",
                                "target_lang_sense_clarification": "quality of being interesting or attractive"
                            },
                            {
                                "target_lang_word": "allure",
                                "target_lang_sense_clarification": "powerful or mysterious attractiveness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aae5fd15-ff64-30d8-9474-738b80b2f9fe",
                    "sense_definition": "Een natuurkundige kracht waarmee materiële lichamen elkaar proberen te naderen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>aantrekkingskracht</w> van de aarde houdt de maan in haar baan.",
                            "target_lang_translations": {
                                "ru": "<w>Сила притяжения</w> Земли удерживает Луну на её орбите.",
                                "en": "The <w>gravitational pull</w> of the Earth keeps the moon in its orbit."
                            }
                        },
                        {
                            "text": "Door de magnetische <w>aantrekkingskracht</w> bleef het metaal aan de magneet kleven.",
                            "target_lang_translations": {
                                "ru": "Из-за магнитного <w>притяжения</w> металл прилип к магниту.",
                                "en": "Due to the magnetic <w>attraction</w>, the metal stuck to the magnet."
                            }
                        }
                    ],
                    "synonyms": [
                        "gravitatie"
                    ],
                    "antonyms": [
                        "afstotingskracht"
                    ],
                    "common_phrases": [
                        "magnetische aantrekkingskracht",
                        "universele aantrekkingskracht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая сила, с которой материальные тела стремятся приблизиться друг к другу.",
                        "en": "A physical force with which material bodies try to approach each other."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сила притяжения",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f84f2e78-4151-3b54-93c2-7f778b002512",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>panować</w>; proces sprawowania władzy lub utrzymywania kontroli.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciągłe <w>panowanie</w> nad własnym gniewem bywa bardzo wyczerpujące.",
                            "target_lang_translations": {
                                "ru": "Постоянный <w>контроль</w> над собственным гневом бывает очень утомительным."
                            }
                        },
                        {
                            "text": "Jego styl <w>panowania</w> nie podobał się wielu poddanym.",
                            "target_lang_translations": {
                                "ru": "Его стиль <w>правления</w> не нравился многим подданным."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rzeczownik odczasownikowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное от <w>panować</w>; процесс осуществления власти или сохранения контроля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "властвование",
                                "target_lang_sense_clarification": "процесс осуществления власти"
                            },
                            {
                                "target_lang_word": "контроль",
                                "target_lang_sense_clarification": "над собой или эмоциями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbbb1d28-5ec5-33f5-9e45-f529a11a4f2c",
                    "sense_definition": "Posiadanie władzy, zwierzchnictwa lub przewagi nad kimś lub czymś; także zdolność kontrolowania sytuacji lub emocji.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "power_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca stracił <w>panowanie</w> nad samochodem i wjechał do rowu.",
                            "target_lang_translations": {
                                "ru": "Водитель потерял <w>управление</w> автомобилем и съехал в кювет."
                            }
                        },
                        {
                            "text": "Imperium rzymskie rozszerzyło swoje <w>panowanie</w> na ogromne terytoria wokół Morza Śródziemnego.",
                            "target_lang_translations": {
                                "ru": "Римская империя распространила свое <w>господство</w> на огромные территории вокруг Средиземного моря."
                            }
                        }
                    ],
                    "synonyms": [
                        "władza",
                        "kontrola",
                        "dominacja",
                        "zwierzchnictwo"
                    ],
                    "antonyms": [
                        "poddaństwo",
                        "uległość"
                    ],
                    "common_phrases": [
                        "panowanie nad sobą",
                        "stracić panowanie",
                        "odzyskać panowanie",
                        "panowanie na morzu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладание властью, верховенством или преимуществом над кем-то или чем-то; также способность контролировать ситуацию или эмоции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "господство",
                                "target_lang_sense_clarification": "верховенство, доминирование"
                            },
                            {
                                "target_lang_word": "управление",
                                "target_lang_sense_clarification": "контроль над техникой"
                            },
                            {
                                "target_lang_word": "контроль",
                                "target_lang_sense_clarification": "над ситуацией"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6814329f-111f-3620-9b0a-cd93908ca655",
                    "sense_definition": "Okres czasu, w którym dany monarcha lub władca sprawuje rządy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za <w>panowania</w> króla Kazimierza Wielkiego Polska znacznie się rozwinęła.",
                            "target_lang_translations": {
                                "ru": "Во времена <w>правления</w> короля Казимира Великого Польша значительно развилась."
                            }
                        },
                        {
                            "text": "To historyczne wydarzenie miało miejsce w piątym roku jego <w>panowania</w>.",
                            "target_lang_translations": {
                                "ru": "Это историческое событие произошло на пятом году его <w>царствования</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rządy"
                    ],
                    "common_phrases": [
                        "za panowania",
                        "okres panowania",
                        "długie panowanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени, в течение которого правит монарх или правитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правление",
                                "target_lang_sense_clarification": "период власти"
                            },
                            {
                                "target_lang_word": "царствование",
                                "target_lang_sense_clarification": "период правления монарха"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "30cd6b6a-55c2-323a-87e5-41cb4be8ada9",
                    "sense_definition": "Het proces van beveiligen en in de gaten houden van personen, gebouwen of voorwerpen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gevangene staat dag en nacht onder strenge <w>bewaking</w>.",
                            "target_lang_translations": {
                                "en": "The prisoner is under strict <w>surveillance</w> day and night.",
                                "ru": "Заключенный находится под строгой <w>охраной</w> днем и ночью."
                            }
                        },
                        {
                            "text": "Voor de <w>bewaking</w> van het museum zijn overal camera's opgehangen.",
                            "target_lang_translations": {
                                "en": "Cameras have been hung everywhere for the <w>security</w> of the museum.",
                                "ru": "Для <w>охраны</w> музея повсюду установлены камеры."
                            }
                        }
                    ],
                    "synonyms": [
                        "toezicht",
                        "surveillance",
                        "beveiliging"
                    ],
                    "common_phrases": [
                        "onder bewaking staan",
                        "strenge bewaking",
                        "intensieve bewaking"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of securing and monitoring people, buildings, or objects.",
                        "ru": "Процесс обеспечения безопасности и наблюдения за людьми, зданиями или предметами."
                    },
                    "translations": {
                        "en": [
                            {

        "pomalutku"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c3472e79-00fd-3a01-bd6f-52b1c10bc52b",
                    "sense_definition": "Z niewielką prędkością, bez pośpiechu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód jechał bardzo <w>pomału</w> przez wąską uliczkę.",
                            "target_lang_translations": {
                                "ru": "Машина ехала очень <w>медленно</w> по узкой улочке."
                            }
                        },
                        {
                            "text": "Musimy iść <w>pomału</w>, żeby nie obudzić śpiącego dziecka.",
                            "target_lang_translations": {
                                "ru": "Мы должны идти <w>потихоньку</w>, чтобы не разбудить спящего ребенка."
                            }
                        }
                    ],
                    "synonyms": [
                        "powoli",
                        "wolno"
                    ],
                    "antonyms": [
                        "szybko"
                    ],
                    "common_phrases": [
                        "iść pomału",
                        "mówić pomału",
                        "jechać pomału"
                    ],
                    "target_lang_definitions": {
                        "ru": "С небольшой скоростью, без спешки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медленно",
                                "target_lang_sense_clarification": "о скорости движения или действия"
                            },
                            {
                                "target_lang_word": "не спеша",
                                "target_lang_sense_clarification": "без суеты, спокойно"
                            },
                            {
                                "target_lang_word": "потихоньку",
                                "target_lang_sense_clarification": "медленно и осторожно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65453765-0682-308c-89b4-2ff0763378f5",
                    "sense_definition": "Stopniowo, etapami, w miarę upływu czasu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "progression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sytuacja w naszej firmie <w>pomału</w> zaczyna się poprawiać.",
                            "target_lang_translations": {
                                "ru": "Ситуация в нашей фирме <w>постепенно</w> начинает улучшаться."
                            }
                        },
                        {
                            "text": "Śnieg za oknem <w>pomału</w> topnieje, bo idzie wiosna.",
                            "target_lang_translations": {
                                "ru": "Снег за окном <w>понемногу</w> тает, потому что идет весна."
                            }
                        }
                    ],
                    "synonyms": [
                        "stopniowo",
                        "powoli"
                    ],
                    "common_phrases": [
                        "wszystko przyjdzie pomału",
                        "pomału się przyzwyczajać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постепенно, шаг за шагом, с течением времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постепенно",
                                "target_lang_sense_clarification": "поэтапно, со временем"
                            },
                            {
                                "target_lang_word": "понемногу",
                                "target_lang_sense_clarification": "малыми частями, не сразу"
                            },
                            {
                                "target_lang_word": "мало-помалу",
                                "target_lang_sense_clarification": "медленно, но верно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02e8dd36-e8c0-3a86-98a4-1addec54a7a2",
                    "sense_definition": "W sposób znośny, przeciętny, ale akceptowalny; często używane w odpowiedzi na pytanie o samopoczucie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "– Co u ciebie słychać? – A dziękuję, jakoś <w>pomału</w> leci.",
                            "target_lang_translations": {
                                "ru": "– Как у тебя дела? – Спасибо, как-то <w>потихоньку</w> идет."
                            }
                        },
                        {
                            "text": "Interes kręci się <w>pomału</w>, więc nie mam na co narzekać.",
                            "target_lang_translations": {
                                "ru": "Бизнес крутится <w>потихоньку</w>, так что мне не на что жаловаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "znośnie"
                    ],
                    "common_phrases": [
                        "jakoś pomału leci",
                        "wszystko pomału"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in casual conversation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сносно, средне, но приемлемо; часто используется в ответе на вопрос о делах или самочувствии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потихоньку",
                                "target_lang_sense_clarification": "о делах или жизни (разговорное)"
                            },
                            {
                                "target_lang_word": "помаленьку",
                                "target_lang_sense_clarification": "неплохо, терпимо (разговорное)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
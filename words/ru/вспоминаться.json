            "senses": [
                {
                    "sense_id": "8f64cd1b-17b7-3e2c-bd42-ea6dc540d5d9",
                    "sense_definition": "Приходить на память, возникать в сознании (о событиях прошлого, образах или мыслях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "memory_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне часто <w>вспоминается</w> моё счастливое и беззаботное детство.",
                            "target_lang_translations": {
                                "en": "My happy and carefree childhood often <w>comes to mind</w>."
                            }
                        },
                        {
                            "text": "В такие тихие вечера мне всегда <w>вспоминается</w> наш старый дом в деревне.",
                            "target_lang_translations": {
                                "en": "On such quiet evenings, our old house in the village always <w>comes to mind</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "припоминаться",
                        "приходить на память"
                    ],
                    "antonyms": [
                        "забываться"
                    ],
                    "common_phrases": [
                        "мне вспоминается",
                        "часто вспоминается",
                        "сразу вспоминается"
                    ],
                    "target_lang_definitions": {
                        "en": "To come to mind, to appear in one's thoughts (regarding past events, images, or ideas)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come to mind",
                                "target_lang_sense_clarification": "spontaneous memory"
                            },
                            {
                                "target_lang_word": "be remembered",
                                "target_lang_sense_clarification": "general recollection"
                            },
                            {
                                "target_lang_word": "recall",
                                "target_lang_sense_clarification": "often translated as active 'I recall'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed6cdcb7-4bfb-3521-8824-e8560cbcce3a",
                    "sense_definition": "Страдательная форма от глагола <w>вспоминать</w>; быть упоминаемым или восстанавливаемым в памяти кем-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "passive_voice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот мудрый правитель до сих пор <w>вспоминается</w> народом с большой благодарностью.",
                            "target_lang_translations": {
                                "en": "This wise ruler is still <w>remembered</w> by the people with great gratitude."
                            }
                        },
                        {
                            "text": "В исторических документах это событие <w>вспоминается</w> довольно редко.",
                            "target_lang_translations": {
                                "en": "In historical documents, this event is <w>recalled</w> quite rarely."
                            }
                        }
                    ],
                    "synonyms": [
                        "поминаться"
                    ],
                    "antonyms": [
                        "забываться"
                    ],
                    "common_phrases": [
                        "вспоминается добрым словом"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {

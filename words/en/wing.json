                    "target_lang_definitions": {
                        "ru": "Одна из невидимых зон по бокам сцены в театре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кулисы",
                                "target_lang_sense_clarification": "театр"
                            }
                        ]

                        {
                            "text": "I will be <w>away</w> on business for a few days.",
                            "target_lang_translations": {
                                "ru": "Я буду <w>в отъезде</w> по делам несколько дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "off",
                        "at bay"
                    ],
                    "common_phrases": [
                        "be away",
                        "away from home"
                    ],
                    "target_lang_definitions": {

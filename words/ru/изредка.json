                    ],
                    "target_lang_definitions": {
                        "en": "Here and there; appearing only in scattered places (referring to spatial distribution)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "here and there",
                                "target_lang_sense_clarification": "found in various places but not everywhere"
                            },
                            {
                                "target_lang_word": "occasionally",
                                "target_lang_sense_clarification": "encountered infrequently across a space"

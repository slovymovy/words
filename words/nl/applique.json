                {
                    "sense_id": "f610377b-95fc-3718-9843-c93696e9f522",
                    "sense_definition": "Een versiering van een oppervlakte in de vorm van een motief dat uit een ander materiaal is gemaakt en erop is aangebracht.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze naaide een kleurrijke <w>applique</w> op de achterkant van haar spijkerjasje.",
                            "target_lang_translations": {
                                "en": "She sewed a colorful <w>appliqué</w> onto the back of her denim jacket.",
                                "ru": "Она пришила яркую <w>аппликацию</w> на спину своей джинсовой куртки."
                            }
                        },
                        {
                            "text": "De antieke kast was prachtig versierd met een vergulde <w>applique</w>.",
                            "target_lang_translations": {
                                "en": "The antique cabinet was beautifully decorated with a gilded <w>mount</w>.",
                                "ru": "Антикварный шкаф был великолепно украшен позолоченной <w>накладкой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "appliqué",
                        "oplegsel",
                        "opnaaisel"
                    ],
                    "common_phrases": [
                        "geborduurde applique",
                        "leren applique",
                        "zilveren applique"
                    ],
                    "target_lang_definitions": {
                        "en": "A decoration or ornament made of one material attached to the surface of another.",
                        "ru": "Украшение поверхности в виде узора из другого материала, наложенного или нашитого на неё."
                    },
                    "translations": {

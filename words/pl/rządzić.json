{
    "word_family": [
        "rząd",
        "rządowy",
        "zarządzać",
        "zarząd",
        "urząd",
        "urzędnik",
        "porządek",
        "rządca",
        "nierząd"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3e0fc142-9840-38c0-aa4d-29df6e8bc728",
                    "sense_definition": "Sprawować władzę nad państwem, terytorium lub grupą ludzi; być u władzy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obecny prezydent <rządzi> krajem od pięciu lat.",
                            "target_lang_translations": {
                                "ru": "Нынешний президент <управляет> страной уже пять лет."
                            }
                        },
                        {
                            "text": "W dawnych czasach król <rządził> sprawiedliwie i dbał o swoich poddanych.",
                            "target_lang_translations": {
                                "ru": "В былые времена король <правил> справедливо и заботился о своих подданных."
                            }
                        }
                    ],
                    "synonyms": [
                        "władać",
                        "panować"
                    ],
                    "antonyms": [
                        "służyć",
                        "poddawać się"
                    ],
                    "common_phrases": [
                        "rządzić krajem",
                        "rządzić państwem",
                        "rządzić twardą ręką"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осуществлять власть над государством, территорией или группой людей; находиться у власти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "править",
                                "target_lang_sense_clarification": "быть правителем, царем"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "осуществлять административную власть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e4a3f2f-1b10-3b4a-b902-c43c035fea5b",
                    "sense_definition": "Mieć decydujący wpływ na funkcjonowanie czegoś; kierować lub zarządzać czymś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój ojciec od lat skutecznie <rządzi> dużą firmą budowlaną.",
                            "target_lang_translations": {
                                "ru": "Мой отец много лет успешно <руководит> крупной строительной фирмой."
                            }
                        },
                        {
                            "text": "Mówi się, że w dzisiejszych czasach pieniądz <rządzi> światem.",
                            "target_lang_translations": {
                                "ru": "Говорят, что в наше время деньги <правят> миром."
                            }
                        }
                    ],
                    "synonyms": [
                        "zarządzać",
                        "kierować się",
                        "administrować",
                        "szefować"
                    ],
                    "common_phrases": [
                        "rządzić firmą",
                        "rządzić światem",
                        "pieniądz rządzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь решающее влияние на функционирование чего-либо; руководить или управлять чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "менеджмент, контроль процессов"
                            },
                            {
                                "target_lang_word": "руководить",
                                "target_lang_sense_clarification": "возглавлять организацию"
                            },
                            {
                                "target_lang_word": "править",
                                "target_lang_sense_clarification": "иметь доминирующее влияние (напр. миром)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04820706-2a14-3e38-8fbb-65790ff828c7",
                    "sense_definition": "Wydawać polecenia i narzucać innym swoją wolę, często w sposób apodyktyczny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie pozwolę, żebyś mną <rządził> i mówił mi, co mam robić.",
                            "target_lang_translations": {
                                "ru": "Я не позволю тебе мною <командовать> и говорить мне, что делать."
                            }
                        },
                        {
                            "text": "Starsza siostra zawsze lubiła <rządzić> młodszym rodzeństwem podczas zabawy.",
                            "target_lang_translations": {
                                "ru": "Старшая сестра всегда любила <командовать> младшими во время игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "szefować",
                        "dowodzić",
                        "panować"
                    ],
                    "antonyms": [
                        "słuchać"
                    ],
                    "common_phrases": [
                        "lubić rządzić",
                        "rządzić wszystkimi"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used when someone is bossy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Раздавать приказы и навязывать другим свою волю, часто в безапелляционной манере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командовать",
                                "target_lang_sense_clarification": "отдавать приказы, вести себя как начальник"
                            },
                            {
                                "target_lang_word": "распоряжаться",
                                "target_lang_sense_clarification": "указывать, что делать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6c52bcf-b1fe-3f6d-82b8-a348b9ac23e1",
                    "sense_definition": "Kierować się czymś w swoim postępowaniu; podejmować decyzje w oparciu o określone zasady lub motywy (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W życiu zawodowym należy <rządzić> się logiką, a nie emocjami.",
                            "target_lang_translations": {
                                "ru": "В профессиональной жизни следует <руководствоваться> логикой, а не эмоциями."
                            }
                        },
                        {
                            "text": "Przyroda <rządzi> się swoimi własnymi, niezmiennymi prawami.",
                            "target_lang_translations": {
                                "ru": "Природа <подчиняется> своим собственным, неизменным законам."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierować się"
                    ],
                    "common_phrases": [
                        "rządzić się prawami",
                        "rządzić się logiką",
                        "rządzić się emocjami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Руководствоваться чем-либо в своих действиях; принимать решения на основе определенных принципов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руководствоваться",
                                "target_lang_sense_clarification": "следовать принципам или логике"
                            },
                            {
                                "target_lang_word": "подчиняться",
                                "target_lang_sense_clarification": "следовать законам (напр. природы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "feadc6bf-4441-366d-bc70-b13a244144d7",
                    "sense_definition": "Postępować swobodnie według własnego uznania, często nie zważając na innych (używane z zaimkiem „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy lokator <rządzi> się w kuchni, jakby mieszkał tu od zawsze.",
                            "target_lang_translations": {
                                "ru": "Новый жилец <хозяйничает> на кухне, как будто жил здесь всегда."
                            }
                        },
                        {
                            "text": "Nie możesz przychodzić do cudzego biura i <rządzić> się bez pytania.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь приходить в чужой офис и <распоряжаться> без спроса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rządzić się jak szara gęś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поступать по своему усмотрению, часто не считаясь с другими; вести себя как хозяин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хозяйничать",
                                "target_lang_sense_clarification": "вести себя как дома (часто неодобрительно)"
                            },
                            {
                                "target_lang_word": "распоряжаться",
                                "target_lang_sense_clarification": "самовольничать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3663a42d-02f0-32b1-90c2-e995e6b580ce",
                    "sense_definition": "Wymagać połączenia z inną częścią mowy w określonym przypadku gramatycznym.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W języku polskim ten czasownik zawsze <rządzi> dopełniaczem.",
                            "target_lang_translations": {
                                "ru": "В польском языке этот глагол всегда <управляет> родительным падежом."
                            }
                        },
                        {
                            "text": "Nauczyciel wyjaśnił studentom, jaki przyimek <rządzi> tym rzeczownikiem.",
                            "target_lang_translations": {
                                "ru": "Учитель объяснил студентам, какой предлог <управляет> этим существительным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rządzić przypadkiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "linguistic term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Требовать употребления зависимого слова в определенном падеже (грамматический термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "синтаксическая связь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
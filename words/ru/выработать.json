    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a49ab464-169d-3633-b14a-e69018fb2632",
                    "sense_definition": "Создать, подготовить или изготовить что-либо в процессе труда (план, стратегию, энергию или продукцию).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Команда должна <w>выработать</w> новую стратегию развития компании до конца недели.",
                            "target_lang_translations": {
                                "en": "The team needs to <w>develop</w> a new company development strategy by the end of the week."
                            }
                        },
                        {
                            "text": "Электростанция может <w>выработать</w> достаточно энергии для всего города.",
                            "target_lang_translations": {
                                "en": "The power plant can <w>generate</w> enough energy for the whole city."
                            }
                        }
                    ],
                    "synonyms": [
                        "разработать",
                        "произвести",
                        "изготовить"
                    ],
                    "common_phrases": [
                        "выработать стратегию",
                        "выработать план",
                        "выработать решение",
                        "выработать электроэнергию"
                    ],
                    "target_lang_definitions": {
                        "en": "To create, prepare, or manufacture something through work (a plan, strategy, energy, or products)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "a strategy, plan, or system"
                            },
                            {
                                "target_lang_word": "generate",
                                "target_lang_sense_clarification": "energy or electricity"
                            },
                            {
                                "target_lang_word": "work out",
                                "target_lang_sense_clarification": "a solution or plan"
                            },
                            {
                                "target_lang_word": "produce",
                                "target_lang_sense_clarification": "substances or goods"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a02b601-90d2-38dd-a832-7ee285ac877c",
                    "sense_definition": "Развить или сформировать в себе какие-либо качества, привычки или навыки путём усилий.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему удалось <w>выработать</w> в себе терпение и спокойствие.",
                            "target_lang_translations": {
                                "en": "He managed to <w>cultivate</w> patience and calmness in himself."
                            }
                        },
                        {
                            "text": "Чтобы <w>выработать</w> полезную привычку, нужно повторять действие каждый день.",
                            "target_lang_translations": {
                                "en": "To <w>form</w> a useful habit, you need to repeat the action every day."
                            }
                        }
                    ],
                    "synonyms": [
                        "сформировать"
                    ],
                    "antonyms": [
                        "утратить"
                    ],
                    "common_phrases": [
                        "выработать характер",
                        "выработать привычку",
                        "выработать иммунитет",
                        "выработать навыки"
                    ],
                    "target_lang_definitions": {
                        "en": "To develop or form qualities, habits, or skills in oneself through effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "a habit, character, or skill"
                            },
                            {
                                "target_lang_word": "cultivate",
                                "target_lang_sense_clarification": "patience or personal qualities"
                            },
                            {
                                "target_lang_word": "build up",
                                "target_lang_sense_clarification": "immunity or resistance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4002e472-ba29-38a4-aaeb-60f516408cd3",
                    "sense_definition": "Полностью использовать ресурс, запас или возможности чего-либо в процессе эксплуатации.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старый двигатель полностью <w>выработал</w> свой ресурс и требует замены.",
                            "target_lang_translations": {
                                "en": "The old engine has completely <w>exhausted</w> its service life and requires replacement."
                            }
                        },
                        {
                            "text": "Шахта <w>выработала</w> все запасы угля и была закрыта в прошлом году.",
                            "target_lang_translations": {
                                "en": "The mine <w>depleted</w> all coal reserves and was closed last year."
                            }
                        }
                    ],
                    "synonyms": [
                        "исчерпать"
                    ],
                    "antonyms": [
                        "восполнить"
                    ],
                    "common_phrases": [
                        "выработать ресурс",
                        "выработать топливо",
                        "выработать запас"
                    ],
                    "target_lang_definitions": {
                        "en": "To fully use up a resource, reserve, or capacity during operation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exhaust",
                                "target_lang_sense_clarification": "resources or reserves"
                            },
                            {
                                "target_lang_word": "use up",
                                "target_lang_sense_clarification": "fuel or supplies"
                            },
                            {
                                "target_lang_word": "deplete",
                                "target_lang_sense_clarification": "a natural reserve"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "980faa25-52a4-3641-89f4-8b6989f6a51d",
                    "sense_definition": "Получить право на что-либо (обычно пенсию или стаж) или заработать деньги своим трудом.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой отец уже <w>выработал</w> необходимый стаж для получения государственной пенсии.",
                            "target_lang_translations": {
                                "en": "My father has already <w>earned</w> the necessary seniority to receive a state pension."
                            }
                        },
                        {
                            "text": "Рабочие старались <w>выработать</w> норму, чтобы получить премию.",
                            "target_lang_translations": {
                                "en": "The workers tried to <w>fulfill</w> the quota to get a bonus."
                            }
                        }
                    ],
                    "synonyms": [
                        "заработать"
                    ],
                    "common_phrases": [
                        "выработать стаж",
                        "выработать пенсию",
                        "выработать норму"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "в значении заработать деньги или получить стаж"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To earn the right to something (usually a pension or seniority) or to earn money/meet a quota through work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "earn",
                                "target_lang_sense_clarification": "a pension or seniority"
                            },
                            {
                                "target_lang_word": "fulfill",
                                "target_lang_sense_clarification": "a quota or norm"
                            },
                            {
                                "target_lang_word": "complete",
                                "target_lang_sense_clarification": "years of service"
                            }
                        ]
                    }
                }
            ]
        }
    ]

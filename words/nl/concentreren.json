{
    "word_family": [
        "concentratie",
        "concentraat"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "21602d31-b0aa-3682-9955-32bc808b449f",
                    "sense_definition": "Zich met volledige aandacht op één taak of onderwerp richten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is erg stil in de bibliotheek, zodat iedereen zich goed kan <w>concentreren</w>.",
                            "target_lang_translations": {
                                "ru": "В библиотеке очень тихо, чтобы все могли хорошо <w>сосредоточиться</w>."
                            }
                        },
                        {
                            "text": "Hij probeerde zich te <w>concentreren</w> op zijn huiswerk, maar hij was te moe.",
                            "target_lang_translations": {
                                "ru": "Он пытался <w>сосредоточиться</w> на домашнем задании, но слишком устал."
                            }
                        }
                    ],
                    "synonyms": [
                        "focussen",
                        "toespitsen"
                    ],
                    "antonyms": [
                        "afleiden"
                    ],
                    "common_phrases": [
                        "zich concentreren op",
                        "zich kunnen concentreren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосредоточить все внимание на одной задаче или предмете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сосредоточиваться",
                                "target_lang_sense_clarification": "направлять внимание"
                            },
                            {
                                "target_lang_word": "концентрироваться",
                                "target_lang_sense_clarification": "фокусироваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbe5a546-e36a-3b2b-8cba-ccf37573ca3c",
                    "sense_definition": "Mensen, dingen of macht op één plek of punt samenbrengen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De generaal besloot zijn leger in het noorden van het land te <w>concentreren</w>.",
                            "target_lang_translations": {
                                "ru": "Генерал решил <w>сосредоточить</w> свою армию на севере страны."
                            }
                        },
                        {
                            "text": "Veel grote bedrijven hebben hun activiteiten in de hoofdstad <w>geconcentreerd</w>.",
                            "target_lang_translations": {
                                "ru": "Многие крупные компании <w>сосредоточили</w> свою деятельность в столице."
                            }
                        }
                    ],
                    "synonyms": [
                        "samenbrengen",
                        "centraliseren",
                        "bundelen"
                    ],
                    "antonyms": [
                        "verspreiden",
                        "spreiden"
                    ],
                    "common_phrases": [
                        "troepen concentreren",
                        "macht concentreren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собирать людей, предметы или власть в одном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сосредоточивать",
                                "target_lang_sense_clarification": "собирать в одном месте"
                            },
                            {
                                "target_lang_word": "концентрировать",
                                "target_lang_sense_clarification": "централизовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fe3bc65-7af6-35ef-bea2-d3bba9efbc50",
                    "sense_definition": "De hoeveelheid opgeloste stof in een vloeistof verhogen door een deel van het oplosmiddel te verwijderen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de fabriek wordt het vruchtensap <w>geconcentreerd</w> voordat het wordt vervoerd.",
                            "target_lang_translations": {
                                "ru": "На заводе фруктовый сок <w>концентрируют</w> перед транспортировкой."
                            }
                        },
                        {
                            "text": "Door het mengsel langzaam te koken, kun je de vloeistof <w>concentreren</w>.",
                            "target_lang_translations": {
                                "ru": "Медленно кипятя смесь, можно <w>сгустить</w> жидкость."
                            }
                        }
                    ],
                    "synonyms": [
                        "indikken"
                    ],
                    "antonyms": [
                        "verdunnen"
                    ],
                    "common_phrases": [
                        "een oplossing concentreren"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "scheikunde"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличивать содержание растворенного вещества в жидкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сгущать",
                                "target_lang_sense_clarification": "делать более густым"
                            },
                            {
                                "target_lang_word": "концентрировать",
                                "target_lang_sense_clarification": "повышать насыщенность"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "250f206e-4822-303e-b68d-668b5966d6f6",
                    "sense_definition": "Ostra przyprawa kuchenna otrzymywana z suszonych nasion (ziaren) rośliny o tej samej nazwie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sem_pepper_plant_spice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do smaku potrzebna jest tylko sól i <w>pieprz</w>.",
                            "target_lang_translations": {
                                "ru": "Для вкуса нужны только соль и <w>перец</w>."
                            }
                        },
                        {
                            "text": "Kelner przyniósł nam świeżo mielony <w>pieprz</w>.",
                            "target_lang_translations": {
                                "ru": "Официант принес нам свежемолотый <w>перец</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pieprz czarny",
                        "reg. śl. fefer"
                    ],
                    "common_phrases": [
                        "sól i pieprz",
                        "pieprz czarny",
                        "pieprz ziołowy",
                        "szczypta pieprzu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Острая кухонная приправа, получаемая из сушеных семян (зерен) одноименного растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перец",
                                "target_lang_sense_clarification": "приправа"
                            },
                            {
                                "target_lang_word": "чёрный перец",
                                "target_lang_sense_clarification": "разновидность приправы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a83c04df-c8ad-3fb6-9556-d7564c56d576",
                    "sense_definition": "Roślina pnąca lub krzew z rodziny pieprzowatych, uprawiana dla owoców używanych jako przyprawa.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sem_pepper_plant_spice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Pieprz</w> jest rośliną pochodzącą z terenów tropikalnych.",
                            "target_lang_translations": {
                                "ru": "<w>Перец</w> — это растение родом из тропиков."
                            }
                        },
                        {
                            "text": "W tej szklarni rośnie <w>pieprz</w> oraz inne egzotyczne rośliny.",
                            "target_lang_translations": {
                                "ru": "В этой теплице растет <w>перец</w> и другие экзотические растения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uprawa pieprzu",
                        "ziarna pieprzu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вьющееся растение или кустарник из семейства перечных, выращиваемое ради плодов, используемых в качестве приправы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перец",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "be6f4ed6-70e7-375e-b812-0d825f704d58",
                    "sense_definition": "Druga osoba liczby pojedynczej trybu rozkazującego czasownika <w>pieprzyć</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>pieprz</w> tej zupy, bo będzie za ostra.",
                            "target_lang_translations": {
                                "ru": "Не <w>перчи</w> этот суп, иначе он будет слишком острым."
                            }
                        },
                        {
                            "text": "Najpierw posól ziemniaki, a potem je <w>pieprz</w>.",
                            "target_lang_translations": {
                                "ru": "Сначала посоли картошку, а потом <w>перчи</w> её."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "tryb rozkazujący od: pieprzyć"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Второе лицо единственного числа повелительного наклонения глагола <w>pieprzyć</w> (перчить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перчи",
                                "target_lang_sense_clarification": "глагол, повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "description",
        "descriptive",
        "indescribable",
        "nondescript"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "904a72d9-40b5-3d37-a321-4d8b76a2e86b",
                    "sense_definition": "To give a detailed account or representation of someone or something in words.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>describe</w> the man you saw running away?",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>описать</w> мужчину, которого видели убегающим?"
                            }
                        },
                        {
                            "text": "She <w>described</w> her holiday in detail to her friends.",
                            "target_lang_translations": {
                                "ru": "Она подробно <w>описала</w> свой отпуск друзьям."
                            }
                        }
                    ],
                    "synonyms": [
                        "portray",
                        "depict",
                        "explain",
                        "narrate",
                        "recount",
                        "relate",
                        "report",
                        "represent"
                    ],
                    "antonyms": [
                        "distort",
                        "misrepresent"
                    ],
                    "common_phrases": [
                        "describe in detail",
                        "describe exactly",
                        "hard to describe",
                        "describe as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать подробное описание или представление о ком-либо или чём-либо словами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "описывать",
                                "target_lang_sense_clarification": "рассказывать подробно, изображать словами (несов. вид)"
                            },
                            {
                                "target_lang_word": "описать",
                                "target_lang_sense_clarification": "рассказать подробно, изобразить словами (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f96d027a-fcfe-39ab-9f10-dd4ab0d06b0a",
                    "sense_definition": "To move in a way that forms a particular shape or line, or to draw such a shape.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dancer's arm <w>described</w> a graceful arc through the air.",
                            "target_lang_translations": {
                                "ru": "Рука танцора <w>описала</w> в воздухе изящную дугу."
                            }
                        },
                        {
                            "text": "The planet <w>describes</w> an elliptical orbit around the star.",
                            "target_lang_translations": {
                                "ru": "Планета <w>описывает</w> эллиптическую орбиту вокруг звезды."
                            }
                        }
                    ],
                    "synonyms": [
                        "draw",
                        "delineate"
                    ],
                    "common_phrases": [
                        "describe a circle",
                        "describe an arc",
                        "describe an orbit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться по траектории, образующей определённую фигуру или линию, или чертить такую фигуру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "описывать",
                                "target_lang_sense_clarification": "двигаться по кривой (напр. круги, дуги)"
                            },
                            {
                                "target_lang_word": "очерчивать",
                                "target_lang_sense_clarification": "обводить контуром, создавать очертания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6966003e-eef9-331c-9273-a3446ba3237a",
                    "sense_definition": "To officially discover, name, and classify a new species or taxon in science.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The biologist <w>described</w> a new species of frog found in the rainforest.",
                            "target_lang_translations": {
                                "ru": "Биолог <w>описал</w> новый вид лягушек, найденный в тропическом лесу."
                            }
                        },
                        {
                            "text": "This rare fungus was first <w>described</w> by scientists in the nineteenth century.",
                            "target_lang_translations": {
                                "ru": "Этот редкий гриб был впервые <w>описан</w> учёными в девятнадцатом веке."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально открывать, классифицировать и давать характеристику новому виду в науке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "описывать",
                                "target_lang_sense_clarification": "давать научное описание вида"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
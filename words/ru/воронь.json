    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2f76fa2b-65b6-385e-81a3-fcd76025541e",
                    "sense_definition": "Густая чернота, чёрный цвет (обычно о волосах, оперении или ночной темноте).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "color_blackness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её длинные волосы отливали на солнце синей <w>воронью</w>.",
                            "target_lang_translations": {
                                "en": "Her long hair shimmered in the sun with a blue <w>raven blackness</w>."
                            }
                        },
                        {
                            "text": "Непроглядная <w>воронь</w> южной ночи быстро скрыла очертания гор.",
                            "target_lang_translations": {
                                "en": "The impenetrable <w>blackness</w> of the southern night quickly hid the outlines of the mountains."
                            }
                        }
                    ],
                    "synonyms": [
                        "чернота"
                    ],
                    "antonyms": [
                        "белизна",
                        "бель"
                    ],
                    "common_phrases": [
                        "синяя воронь",
                        "ночная воронь",
                        "воронь волос"
                    ],
                    "target_lang_definitions": {
                        "en": "Deep black color or darkness, often referring to hair, feathers, or the night sky."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "raven blackness",
                                "target_lang_sense_clarification": "referring to color, especially hair or feathers"
                            },
                            {
                                "target_lang_word": "pitch blackness",
                                "target_lang_sense_clarification": "referring to deep darkness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7867b82a-2cea-3c34-a794-2da064a4fe78",
                    "sense_definition": "Тёмное оксидное покрытие на металле, защищающее его от коррозии; чернение.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metal_coating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На стволе старого охотничьего ружья ещё сохранилась заводская <w>воронь</w>.",
                            "target_lang_translations": {
                                "en": "The factory <w>bluing</w> was still preserved on the barrel of the old hunting rifle."
                            }
                        },
                        {
                            "text": "Мастер решил обновить <w>воронь</w> на рукоятке наградного пистолета.",
                            "target_lang_translations": {
                                "en": "The craftsman decided to renew the <w>bluing</w> on the handle of the award pistol."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пистолетная воронь",
                        "сношенная воронь",
                        "навести воронь"
                    ],
                    "target_lang_definitions": {
                        "en": "A dark oxide coating applied to metal for protection against corrosion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bluing",
                                "target_lang_sense_clarification": "protective metal coating"
                            },
                            {
                                "target_lang_word": "blackening",
                                "target_lang_sense_clarification": "metal treatment process"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d226bd8-d220-3eaa-a7d4-efffc51ebbbc",
                    "sense_definition": "Форма повелительного наклонения глагола <w>воронить</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не <w>воронь</w> эту деталь, она должна остаться серебристой.",
                            "target_lang_translations": {
                                "en": "Do not <w>blue</w> this part; it must remain silvery."
                            }
                        },
                        {
                            "text": "<w>Воронь</w> сталь аккуратно, чтобы защитный слой лёг ровно.",
                            "target_lang_translations": {
                                "en": "<w>Blue</w> the steel carefully so that the protective layer is applied evenly."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative form of the verb meaning to apply a dark protective coating to metal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blue",
                                "target_lang_sense_clarification": "imperative verb: to treat metal"
                            },
                            {
                                "target_lang_word": "blacken",
                                "target_lang_sense_clarification": "imperative verb: to coat metal"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bfe1561e-5bb9-3f93-90f2-82e9e5dbcfb1",
                    "sense_definition": "Het schip verlaten en aan wal gaan.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een lange reis mochten de passagiers eindelijk <w>ontschepen</w>.",
                            "target_lang_translations": {
                                "ru": "После долгого путешествия пассажирам наконец разрешили <w>высадиться</w>.",
                                "en": "After a long journey, the passengers were finally allowed to <w>disembark</w>."
                            }
                        },
                        {
                            "text": "Wij zullen morgenochtend vroeg in de haven van Rotterdam <w>ontschepen</w>.",
                            "target_lang_translations": {
                                "ru": "Мы <w>высадимся</w> завтра рано утром в порту Роттердама.",
                                "en": "We will <w>disembark</w> early tomorrow morning in the port of Rotterdam."
                            }
                        }
                    ],
                    "synonyms": [
                        "aan land gaan",
                        "uitstappen"
                    ],
                    "antonyms": [
                        "inschepen",
                        "aan boord gaan"
                    ],
                    "common_phrases": [
                        "passagiers ontschepen",
                        "in de haven ontschepen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покинуть корабль и сойти на берег.",
                        "en": "To leave the ship and go ashore."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высаживаться",
                                "target_lang_sense_clarification": "о пассажирах"
                            },
                            {
                                "target_lang_word": "сходить на берег",
                                "target_lang_sense_clarification": "покидать судно"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "disembark",
                                "target_lang_sense_clarification": "passengers leaving a ship"
                            },
                            {
                                "target_lang_word": "go ashore",
                                "target_lang_sense_clarification": "leaving the ship for land"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0ca832c-5ce3-3699-9fa4-e76bb8cf3ec8",
                    "sense_definition": "Goederen of personen uit een schip halen en aan land brengen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De havenarbeiders begonnen direct de zware lading te <w>ontschepen</w>.",
                            "target_lang_translations": {
                                "ru": "Портовые рабочие сразу начали <w>выгружать</w> тяжелый груз.",
                                "en": "The dockworkers immediately started to <w>unload</w> the heavy cargo."
                            }
                        },
                        {
                            "text": "Het leger moest al het materieel veilig <w>ontschepen</w> voor de missie begon.",
                            "target_lang_translations": {
                                "ru": "Армия должна была безопасно <w>выгрузить</w> всё оборудование до начала миссии.",
                                "en": "The army had to safely <w>offload</w> all equipment before the mission started."
                            }
                        }
                    ],
                    "synonyms": [
                        "lossen",
                        "uitladen"
                    ],
                    "antonyms": [
                        "inladen",
                        "inschepen"
                    ],
                    "common_phrases": [
                        "goederen ontschepen",
                        "lading ontschepen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выгружать товары или высаживать людей с корабля на берег.",
                        "en": "To take goods or people off a ship and bring them to land."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выгружать",
                                "target_lang_sense_clarification": "о грузах"
                            },
                            {
                                "target_lang_word": "высаживать",
                                "target_lang_sense_clarification": "о людях (переходный глагол)"
                            },
                            {
                                "target_lang_word": "разгружать",
                                "target_lang_sense_clarification": "освобождать судно от груза"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "unload",
                                "target_lang_sense_clarification": "removing cargo"
                            },
                            {
                                "target_lang_word": "offload",
                                "target_lang_sense_clarification": "removing equipment or cargo"
                            },
                            {
                                "target_lang_word": "disembark",
                                "target_lang_sense_clarification": "landing troops or passengers"
                            }
                        ]
                    }
                }
            ]
        }

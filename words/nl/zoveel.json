    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "2e6ca343-feb1-3040-bbc6-6346438ab846",
                    "sense_definition": "Een aanduiding voor een grote, maar vaak niet nader gespecificeerde hoeveelheid of aantal.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_large",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er staan <w>zoveel</w> fietsen voor de deur dat ik er niet langs kan.",
                            "target_lang_translations": {
                                "ru": "Перед дверью стоит <w>столько</w> велосипедов, что я не могу пройти."
                            }
                        },
                        {
                            "text": "Ik heb nog nooit <w>zoveel</w> regen gezien in één week.",
                            "target_lang_translations": {
                                "ru": "Я никогда не видел <w>столько</w> дождя за одну неделю."
                            }
                        }
                    ],
                    "synonyms": [
                        "veel",
                        "talrijke"
                    ],
                    "antonyms": [
                        "weinig"
                    ],
                    "common_phrases": [
                        "zoveel mogelijk",
                        "niet zoveel",
                        "net zoveel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указание на большое, но часто не уточненное количество или число."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столько",
                                "target_lang_sense_clarification": "указание на количество"
                            },
                            {
                                "target_lang_word": "так много",
                                "target_lang_sense_clarification": "большое количество"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "0ba2daf9-4a87-3e89-915b-c60e62a3f87b",
                    "sense_definition": "Een groot aantal personen of dingen, zelfstandig gebruikt zonder dat het zelfstandig naamwoord wordt genoemd.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er waren er <w>zoveel</w> dat we niet naar binnen konden.",
                            "target_lang_translations": {
                                "ru": "Их было <w>так много</w>, что мы не могли войти."
                            }
                        },
                        {
                            "text": "Waarom zijn er <w>zoveel</w> niet op het feest gekomen?",
                            "target_lang_translations": {
                                "ru": "Почему <w>так много</w> людей не пришло на вечеринку?"
                            }
                        }
                    ],
                    "synonyms": [
                        "velen"
                    ],
                    "antonyms": [
                        "weinigen"
                    ],
                    "common_phrases": [
                        "met zovelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество людей или предметов, используется самостоятельно без существительного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столько",
                                "target_lang_sense_clarification": "самостоятельное употребление"
                            },
                            {
                                "target_lang_word": "так много",
                                "target_lang_sense_clarification": "большое число"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "2edc4dc1-32bc-3ed9-b6ad-7cfc5f423b68",
                    "sense_definition": "Die specifieke, vaak aanzienlijke hoeveelheid of graad, vaak gebruikt om nadruk te leggen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_emphatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zoveel</w> geld had hij nog nooit gehad.",
                            "target_lang_translations": {
                                "ru": "У него никогда не было <w>столько</w> денег."
                            }
                        },
                        {
                            "text": "Hierdoor had ik steeds <w>zoveel</w> dorst dat mijn watervoorraad snel opraakte.",
                            "target_lang_translations": {
                                "ru": "Из-за этого у меня была <w>такая</w> жажда, что мой запас воды быстро иссяк."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное значительное количество или степень, часто используется для усиления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столько",
                                "target_lang_sense_clarification": "количество"
                            },
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "степень проявления"
                            },
                            {
                                "target_lang_word": "такой",
                                "target_lang_sense_clarification": "усиление качества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6844cd4-fc2c-3552-b1f8-b2745de0fafe",
                    "sense_definition": "Een specifieke hoeveelheid die gelijk is aan een genoemde waarde of maat, vaak in vergelijkingen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity_specific",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gooide <w>zoveel</w> als 15 gram magnesium bij het mengsel.",
                            "target_lang_translations": {
                                "ru": "Он добавил в смесь <w>целых</w> 15 граммов магния."
                            }
                        },
                        {
                            "text": "Je mag <w>zoveel</w> pakken als je wilt.",
                            "target_lang_translations": {
                                "ru": "Ты можешь взять <w>столько</w>, сколько хочешь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zoveel als"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенное количество, равное названному значению или мере, часто в сравнениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столько",
                                "target_lang_sense_clarification": "в сравнениях"
                            },
                            {
                                "target_lang_word": "целых",
                                "target_lang_sense_clarification": "подчеркивание величины числа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ef2fcd5-e47c-378d-b591-e1b4b929b018",
                    "sense_definition": "Een onbepaald klein aantal of bedrag dat volgt op een groter rond getal.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "quantity_indefinite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moest nog tweehonderd euro <w>zoveel</w> betalen.",
                            "target_lang_translations": {
                                "ru": "Мне нужно было заплатить двести <w>с лишним</w> евро."
                            }
                        },
                        {
                            "text": "Het was in negentienhonderd <w>zoveel</w> dat we elkaar voor het eerst ontmoetten.",
                            "target_lang_translations": {
                                "ru": "Мы впервые встретились в тысяча девятьсот <w>каком-то</w> году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "en zoveel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопределенное небольшое число или сумма, следующая за круглым числом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с лишним",
                                "target_lang_sense_clarification": "добавочное количество"
                            },
                            {
                                "target_lang_word": "какой-то",
                                "target_lang_sense_clarification": "неопределенность (о датах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
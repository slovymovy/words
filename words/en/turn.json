    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b20608d3-6634-35b8-a87d-6f57a2624cc2",
                    "sense_definition": "To move your body or an object so that it faces a different direction, or to move around a central point.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>turn</w> around so I can see the back of your dress.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>повернись</w>, чтобы я могла увидеть твое платье сзади."
                            }
                        },
                        {
                            "text": "He <w>turned</w> the key in the lock to open the front door.",
                            "target_lang_translations": {
                                "ru": "Он <w>повернул</w> ключ в замке, чтобы открыть входную дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "rotate",
                        "spin",
                        "twirl"
                    ],
                    "common_phrases": [
                        "turn around",
                        "turn over",
                        "turn back"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигать телом или предметом так, чтобы он был направлен в другую сторону, или двигаться вокруг центральной точки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поворачиваться",
                                "target_lang_sense_clarification": "изменять положение тела"
                            },
                            {
                                "target_lang_word": "поворачивать",
                                "target_lang_sense_clarification": "изменять положение предмета"
                            },
                            {
                                "target_lang_word": "вертеть",
                                "target_lang_sense_clarification": "вращать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9029b84-a671-3e58-a9a2-b96ce68c1e1e",
                    "sense_definition": "To change the direction in which you are moving or traveling.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>turn</w> left at the next traffic light.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>повернуть</w> налево на следующем светофоре."
                            }
                        },
                        {
                            "text": "The car <w>turned</w> into the driveway very slowly to avoid the ice.",
                            "target_lang_translations": {
                                "ru": "Машина очень медленно <w>свернула</w> на подъездную дорожку, чтобы избежать льда."
                            }
                        }
                    ],
                    "synonyms": [
                        "steer",
                        "swerve"
                    ],
                    "common_phrases": [
                        "turn left",
                        "turn right",
                        "turn at the corner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменять направление движения или пути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поворачивать",
                                "target_lang_sense_clarification": "менять направление"
                            },
                            {
                                "target_lang_word": "сворачивать",
                                "target_lang_sense_clarification": "съезжать с дороги в сторону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d908f17-841d-3724-bd37-c84fdb6e0a2b",
                    "sense_definition": "To move the pages of a book or magazine to see the next one.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>turn</w> to page ten in your textbooks.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>откройте</w> ваши учебники на десятой странице."
                            }
                        },
                        {
                            "text": "I cannot wait to <w>turn</w> the page and see what happens next in the story.",
                            "target_lang_translations": {
                                "ru": "Я не могу дождаться, когда <w>переверну</w> страницу и увижу, что произойдет дальше в истории."
                            }
                        }
                    ],
                    "common_phrases": [
                        "turn the page",
                        "turn to page"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать страницы книги или журнала, чтобы увидеть следующую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переворачивать",
                                "target_lang_sense_clarification": "страницу"
                            },
                            {
                                "target_lang_word": "перелистывать",
                                "target_lang_sense_clarification": "листать страницы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "900ae57a-f729-3dc6-bfd4-25490d564499",
                    "sense_definition": "To change and become something else; to transform into a different state or condition.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The leaves on the trees <w>turn</w> brown in the autumn.",
                            "target_lang_translations": {
                                "ru": "Листья на деревьях <w>становятся</w> коричневыми осенью."
                            }
                        },
                        {
                            "text": "Her face <w>turned</w> red with embarrassment when they laughed at her.",
                            "target_lang_translations": {
                                "ru": "Ее лицо <w>покраснело</w> от смущения, когда они посмеялись над ней."
                            }
                        }
                    ],
                    "synonyms": [
                        "become",
                        "get",
                        "grow",
                        "go"
                    ],
                    "common_phrases": [
                        "turn red",
                        "turn into",
                        "turn cold",
                        "turn bad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменяться и становиться чем-то другим; переходить в другое состояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "становиться",
                                "target_lang_sense_clarification": "приобретать свойство"
                            },
                            {
                                "target_lang_word": "превращаться",
                                "target_lang_sense_clarification": "трансформироваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7562b807-5637-38bd-97ff-30546b923ddb",
                    "sense_definition": "To reach a specific age.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My little brother will <w>turn</w> six years old next week.",
                            "target_lang_translations": {
                                "ru": "Моему младшему брату на следующей неделе <w>исполнится</w> шесть лет."
                            }
                        },
                        {
                            "text": "She is going to <w>turn</w> thirty on her next birthday.",
                            "target_lang_translations": {
                                "ru": "Ей <w>исполнится</w> тридцать на следующий день рождения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "turn twenty",
                        "turn fifty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достигать определенного возраста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исполняться",
                                "target_lang_sense_clarification": "о возрасте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "839358ef-5c36-3f70-9375-937c90e2af00",
                    "sense_definition": "To suddenly attack or show hostility towards someone you were previously friendly with.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dog suddenly <w>turned</w> on its owner without any warning.",
                            "target_lang_translations": {
                                "ru": "Собака внезапно <w>набросилась</w> на своего хозяина без всякого предупреждения."
                            }
                        },
                        {
                            "text": "The crowd <w>turned</w> against the speaker when he insulted them.",
                            "target_lang_translations": {
                                "ru": "Толпа <w>ополчилась</w> против оратора, когда он оскорбил их."
                            }
                        }
                    ],
                    "synonyms": [
                        "rebel",
                        "revolt"
                    ],
                    "common_phrases": [
                        "turn on someone",
                        "turn against"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно напасть или проявить враждебность к кому-то, с кем раньше были дружеские отношения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набрасываться",
                                "target_lang_sense_clarification": "атаковать"
                            },
                            {
                                "target_lang_word": "ополчиться",
                                "target_lang_sense_clarification": "настроиться против"
                            },
                            {
                                "target_lang_word": "восставать",
                                "target_lang_sense_clarification": "бунтовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d69f7da6-adab-3330-a0bf-c9594dce9dad",
                    "sense_definition": "To twist a joint, such as an ankle, causing injury.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>turned</w> my ankle while running in the park yesterday.",
                            "target_lang_translations": {
                                "ru": "Я <w>подвернул</w> лодыжку, когда бегал вчера в парке."
                            }
                        },
                        {
                            "text": "She had to stop dancing because she <w>turned</w> her foot during the performance.",
                            "target_lang_translations": {
                                "ru": "Ей пришлось прекратить танцевать, потому что она <w>подвернула</w> ногу во время выступления."
                            }
                        }
                    ],
                    "synonyms": [
                        "twist",
                        "sprain"
                    ],
                    "common_phrases": [
                        "turn an ankle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвернуть сустав, например лодыжку, причинив травму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвернуть",
                                "target_lang_sense_clarification": "травмировать сустав"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "443f9906-5e82-3421-ab21-0b1d8527e970",
                    "sense_definition": "To shape wood or metal using a rotating machine called a lathe.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter <w>turned</w> the table legs with great precision.",
                            "target_lang_translations": {
                                "ru": "Плотник <w>выточил</w> ножки стола с большой точностью."
                            }
                        },
                        {
                            "text": "He learned how to <w>turn</w> a wooden bowl in the workshop.",
                            "target_lang_translations": {
                                "ru": "Он научился <w>точить</w> деревянную чашу в мастерской."
                            }
                        }
                    ],
                    "synonyms": [
                        "lathe"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать форму дереву или металлу с помощью вращающегося станка (токарного станка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точить",
                                "target_lang_sense_clarification": "обрабатывать на станке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cdf4015b-6a38-34fe-83bc-192a9efb430e",
                    "sense_definition": "The time when it is your chance to do something in a sequence with other people.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "noun_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is your <w>turn</w> to wash the dishes tonight.",
                            "target_lang_translations": {
                                "ru": "Сегодня твоя <w>очередь</w> мыть посуду."
                            }
                        },
                        {
                            "text": "The children took <w>turns</w> riding the new bicycle.",
                            "target_lang_translations": {
                                "ru": "Дети по <w>очереди</w> катались на новом велосипеде."
                            }
                        }
                    ],
                    "synonyms": [
                        "go",
                        "move",
                        "chance"
                    ],
                    "common_phrases": [
                        "my turn",
                        "your turn",
                        "take turns",
                        "wait your turn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время, когда наступает ваша возможность сделать что-то в порядке очередности с другими людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очередь",
                                "target_lang_sense_clarification": "порядок действий"
                            },
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "в игре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9acfc291-4559-3c1f-b4f2-e8f8913fc259",
                    "sense_definition": "An act of moving around a central point or changing direction.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Give the handle a <w>turn</w> to open the window.",
                            "target_lang_translations": {
                                "ru": "Сделай <w>оборот</w> ручки, чтобы открыть окно."
                            }
                        },
                        {
                            "text": "We took a wrong <w>turn</w> and got lost in the city.",
                            "target_lang_translations": {
                                "ru": "Мы сделали неверный <w>поворот</w> и заблудились в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "rotation",
                        "spin",
                        "twirl"
                    ],
                    "common_phrases": [
                        "a sharp turn",
                        "make a turn",
                        "wrong turn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие движения вокруг центральной точки или изменения направления."
                    },
                    "translations": {

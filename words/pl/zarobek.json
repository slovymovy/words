    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc0c7f52-56f5-392b-ad15-9c2ca5d85efe",
                    "sense_definition": "Pieniądze otrzymywane jako wynagrodzenie za wykonaną pracę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "money_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego miesięczny <w>zarobek</w> pozwala na spokojne i wygodne życie.",
                            "target_lang_translations": {
                                "ru": "Его месячный <w>заработок</w> позволяет вести спокойную и комфортную жизнь."
                            }
                        },
                        {
                            "text": "W tej branży przeciętne <w>zarobki</w> są znacznie wyższe niż średnia krajowa.",
                            "target_lang_translations": {
                                "ru": "В этой отрасли средние <w>заработки</w> значительно выше, чем в среднем по стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "wynagrodzenie"
                    ],
                    "common_phrases": [
                        "wysokie zarobki",
                        "stały zarobek",
                        "przeciętny zarobek",
                        "niskie zarobki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деньги, получаемые в качестве оплаты за выполненную работу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заработок",
                                "target_lang_sense_clarification": "плата за труд"
                            },
                            {
                                "target_lang_word": "доход",
                                "target_lang_sense_clarification": "денежные поступления"
                            },
                            {
                                "target_lang_word": "зарплата",
                                "target_lang_sense_clarification": "регулярное вознаграждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b088974-7b7d-3cd1-830d-5dc6bebe069d",
                    "sense_definition": "Zysk finansowy uzyskany z jednorazowej transakcji, handlu lub operacji finansowej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "money_profit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprzedaż starego samochodu przyniosła mu niespodziewany <w>zarobek</w>.",
                            "target_lang_translations": {
                                "ru": "Продажа старого автомобиля принесла ему неожиданную <w>прибыль</w>."
                            }
                        },
                        {
                            "text": "To był dla nas czysty <w>zarobek</w>, ponieważ nie ponieśliśmy żadnych kosztów własnych.",
                            "target_lang_translations": {
                                "ru": "Это была для нас чистая <w>прибыль</w>, так как мы не понесли никаких собственных расходов."
                            }
                        }
                    ],
                    "synonyms": [
                        "wynagrodzenie"
                    ],
                    "antonyms": [
                        "strata"
                    ],
                    "common_phrases": [
                        "czysty zarobek",
                        "łatwy zarobek",
                        "szybki zarobek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Финансовая выгода, полученная от разовой сделки, торговли или финансовой операции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прибыль",
                                "target_lang_sense_clarification": "финансовая выгода"
                            },
                            {
                                "target_lang_word": "выгода",
                                "target_lang_sense_clarification": "польза, доход"
                            },
                            {
                                "target_lang_word": "барыш",
                                "target_lang_sense_clarification": "разг. прибыль от торговли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c832e169-bf16-3e0f-8239-34ea242f575d",
                    "sense_definition": "Możliwość uzyskania pieniędzy poprzez pracę, często w kontekście wyjazdu lub pracy dorywczej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "money_opportunity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu mieszkańców wsi wyjeżdżało do miasta na <w>zarobek</w>, aby utrzymać rodziny.",
                            "target_lang_translations": {
                                "ru": "Многие жители деревни уезжали в город на <w>заработки</w>, чтобы содержать семьи."
                            }
                        },
                        {
                            "text": "Muszę znaleźć jakiś dodatkowy <w>zarobek</w>, żeby opłacić zaległe rachunki.",
                            "target_lang_translations": {
                                "ru": "Мне нужно найти какой-нибудь дополнительный <w>заработок</w>, чтобы оплатить просроченные счета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jechać na zarobek",
                        "szukać zarobku",
                        "dodatkowy zarobek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность заработать деньги, часто в контексте выезда на работу или подработки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заработки",
                                "target_lang_sense_clarification": "работа, обычно временная или выездная"
                            },
                            {
                                "target_lang_word": "подработка",
                                "target_lang_sense_clarification": "дополнительная работа"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "repertuarowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d4f52399-1b0e-3d40-a324-0df02dd24162",
                    "sense_definition": "Zestaw utworów (sztuk teatralnych, filmów, koncertów) granych w danej instytucji kultury w określonym czasie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym tygodniu <w>repertuar</w> kina jest bardzo interesujący.",
                            "target_lang_translations": {
                                "ru": "На этой неделе <w>репертуар</w> кинотеатра очень интересный."
                            }
                        },
                        {
                            "text": "Teatr ogłosił nowy <w>repertuar</w> na nadchodzący sezon jesienny.",
                            "target_lang_translations": {
                                "ru": "Театр объявил новый <w>репертуар</w> на предстоящий осенний сезон."
                            }
                        }
                    ],
                    "synonyms": [
                        "program"
                    ],
                    "common_phrases": [
                        "repertuar kin",
                        "bieżący repertuar",
                        "wchodzić do repertuaru",
                        "zmienić repertuar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность произведений (пьес, фильмов, концертов), исполняемых в данном учреждении культуры в определенное время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репертуар",
                                "target_lang_sense_clarification": "совокупность постановок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d52381a-32cf-317e-a57b-dda68916917b",
                    "sense_definition": "Wydrukowany spis lub informacja z programem instytucji kultury na dany okres.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wzięłam papierowy <w>repertuar</w>, aby sprawdzić godziny seansów.",
                            "target_lang_translations": {
                                "ru": "Я взяла бумажную <w>программу</w>, чтобы проверить время сеансов."
                            }
                        },
                        {
                            "text": "Spójrz w <w>repertuar</w> i powiedz mi, co grają w sobotę.",
                            "target_lang_translations": {
                                "ru": "Посмотри в <w>афишу</w> и скажи мне, что идет в субботу."
                            }
                        }
                    ],
                    "synonyms": [
                        "program"
                    ],
                    "common_phrases": [
                        "sprawdzić w repertuarze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напечатанный список или информация с программой учреждения культуры на данный период."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "печатный список мероприятий"
                            },
                            {
                                "target_lang_word": "афиша",
                                "target_lang_sense_clarification": "расписание спектаклей или концертов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fd9a72f-fbea-3e7a-80eb-eba9ee0bc29d",
                    "sense_definition": "Zbiór utworów, ról lub numerów, które dany artysta lub zespół ma opanowane i gotowe do wykonania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pianista ma w swoim <w>repertuarze</w> wszystkie sonaty Beethovena.",
                            "target_lang_translations": {
                                "ru": "У пианиста в <w>репертуаре</w> есть все сонаты Бетховена."
                            }
                        },
                        {
                            "text": "Zespół rockowy włączył do <w>repertuaru</w> kilka ballad.",
                            "target_lang_translations": {
                                "ru": "Рок-группа включила в свой <w>репертуар</w> несколько баллад."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć w repertuarze",
                        "bogaty repertuar",
                        "repertuar zespołu",
                        "poszerzyć repertuar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собрание произведений, ролей или номеров, которые артист или коллектив знает и готов исполнить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репертуар",
                                "target_lang_sense_clarification": "запас готовых произведений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d68f3055-bbcf-35e7-8fcb-8bff433d0bd6",
                    "sense_definition": "Zestaw typowych dla kogoś zachowań, metod działania lub środków wyrazu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>repertuar</w> wymówek na spóźnienia jest nieskończony.",
                            "target_lang_translations": {
                                "ru": "Его <w>репертуар</w> отговорок за опоздания бесконечен."
                            }
                        },
                        {
                            "text": "Aktor użył całego swojego <w>repertuaru</w> min, by rozśmieszyć publiczność.",
                            "target_lang_translations": {
                                "ru": "Актер использовал весь свой <w>репертуар</w> гримас, чтобы рассмешить публику."
                            }
                        }
                    ],
                    "common_phrases": [
                        "repertuar zachowań",
                        "repertuar min",
                        "szeroki repertuar",
                        "cały repertuar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор типичных для кого-то форм поведения, методов действий или средств выражения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репертуар",
                                "target_lang_sense_clarification": "перен. набор приемов"
                            },
                            {
                                "target_lang_word": "арсенал",
                                "target_lang_sense_clarification": "перен. запас средств"
                            }
                        ]
                    }
                }
            ]
        }

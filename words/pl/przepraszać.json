{
    "word_family": [
        "przeprosiny",
        "przeprosić",
        "przepraszający"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "36468ac8-6b20-3291-9ce1-4d5cedef66e9",
                    "sense_definition": "Wyrażać żal z powodu wyrządzonej komuś przykrości, błędu lub niestosownego zachowania; prosić o wybaczenie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "apology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym cię <w>przeprosić</w> za moje wczorajsze spóźnienie.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>извиниться</w> перед тобой за мое вчерашнее опоздание."
                            }
                        },
                        {
                            "text": "Jeśli zrobisz coś złego, powinieneś natychmiast <w>przeprosić</w>.",
                            "target_lang_translations": {
                                "ru": "Если ты сделаешь что-то плохое, тебе следует немедленно <w>извиниться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kajać się",
                        "prosić o wybaczenie"
                    ],
                    "antonyms": [
                        "obrażać"
                    ],
                    "common_phrases": [
                        "przepraszam za spóźnienie",
                        "bardzo przepraszam",
                        "z góry przepraszam",
                        "przepraszam za kłopot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать сожаление по поводу причиненной неприятности, ошибки или неуместного поведения; просить прощения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извиняться",
                                "target_lang_sense_clarification": "просить прощения за проступок"
                            },
                            {
                                "target_lang_word": "просить прощения",
                                "target_lang_sense_clarification": "устойчивое выражение, более формальное или искреннее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb75d0a0-dfb1-364a-a1db-d80217baf178",
                    "sense_definition": "Godzić się ze sobą po kłótni, wyjaśniać sobie nieporozumienia i wybaczać wzajemnie urazy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reconciliation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci pokłóciły się o zabawkę, ale szybko się <w>przeprosiły</w>.",
                            "target_lang_translations": {
                                "ru": "Дети поссорились из-за игрушки, но быстро <w>помирились</w>."
                            }
                        },
                        {
                            "text": "Sąsiedzi w końcu się <w>przeprosili</w> po wieloletnim sporze o miedzę.",
                            "target_lang_translations": {
                                "ru": "Соседи в конце концов <w>помирились</w> после многолетнего спора о меже."
                            }
                        }
                    ],
                    "synonyms": [
                        "godzić się",
                        "jednać się"
                    ],
                    "antonyms": [
                        "kłócić się",
                        "spierać się"
                    ],
                    "common_phrases": [
                        "przepraszać się nawzajem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мириться после ссоры, выяснять недоразумения и прощать друг другу обиды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мириться",
                                "target_lang_sense_clarification": "восстанавливать мирные отношения"
                            },
                            {
                                "target_lang_word": "помириться",
                                "target_lang_sense_clarification": "совершенный вид, прийти к согласию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff8089c7-6df5-35fe-92a3-54c82f6bf204",
                    "sense_definition": "Zacząć ponownie używać czegoś lub akceptować kogoś po dłuższej przerwie lub okresie niechęci.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiomatic_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy nadeszła zima, musiałem <w>przeprosić</w> się z ciepłą czapką.",
                            "target_lang_translations": {
                                "ru": "Когда наступила зима, мне пришлось <w>вернуться</w> к теплой шапке."
                            }
                        },
                        {
                            "text": "Po awarii samochodu Jan <w>przeprosił</w> się ze starym rowerem.",
                            "target_lang_translations": {
                                "ru": "После поломки машины Ян <w>вернулся</w> к старому велосипеду."
                            }
                        }
                    ],
                    "synonyms": [
                        "wracać do czegoś",
                        "przekonywać się"
                    ],
                    "antonyms": [
                        "odrzucać",
                        "porzucać"
                    ],
                    "common_phrases": [
                        "przeprosić się z czymś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "łączy się z przyimkiem „z”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Снова начать использовать что-то или принимать кого-то после долгого перерыва или периода неприязни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуться к",
                                "target_lang_sense_clarification": "снова начать использовать что-либо"
                            },
                            {
                                "target_lang_word": "смириться с",
                                "target_lang_sense_clarification": "принять необходимость использования чего-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
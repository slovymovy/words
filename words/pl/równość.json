            "senses": [
                {
                    "sense_id": "8d5f2341-7b14-30d3-92c7-f2171c2f51d3",
                    "sense_definition": "Stan, w którym wszyscy ludzie są traktowani tak samo i mają te same prawa, bez względu na pochodzenie, płeć czy status społeczny",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy obywatele walczą o wolność i <w>równość</w> wobec prawa.",
                            "target_lang_translations": {
                                "ru": "Все граждане борются за свободу и <w>равенство</w> перед законом."
                            }
                        },
                        {
                            "text": "Konstytucja tego kraju gwarantuje pełną <w>równość</w> płci w życiu zawodowym.",
                            "target_lang_translations": {
                                "ru": "Конституция этой страны гарантирует полное <w>равенство</w> полов в профессиональной жизни."
                            }
                        }
                    ],
                    "antonyms": [
                        "nierówność"
                    ],
                    "common_phrases": [
                        "równość wobec prawa",
                        "równość płci",
                        "wolność i równość",
                        "równość szans"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором ко всем людям относятся одинаково и они имеют равные права, независимо от происхождения, пола или социального статуса"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равенство",
                                "target_lang_sense_clarification": "социальное, правовое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84100130-438b-3f65-a4b2-d345a049987d",
                    "sense_definition": "Relacja w matematyce wskazująca, że dwa wyrażenia lub wartości są identyczne, zapisywana znakiem „=”",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "math_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił ucznia, aby zapisał na tablicy poprawną <w>równość</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика записать на доске верное <w>равенство</w>."
                            }
                        },
                        {
                            "text": "Musimy sprawdzić, czy ta <w>równość</w> zachodzi dla wszystkich liczb rzeczywistych.",
                            "target_lang_translations": {
                                "ru": "Мы должны проверить, выполняется ли это <w>равенство</w> для всех действительных чисел."
                            }
                        }
                    ],
                    "antonyms": [
                        "nierówność"
                    ],
                    "common_phrases": [
                        "znak równości",
                        "równość matematyczna",
                        "prawdziwa równość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Математическое соотношение, указывающее, что два выражения или значения идентичны; записывается знаком «=»"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равенство",
                                "target_lang_sense_clarification": "математическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f027694-3f8b-386c-a8db-fa0fb0e6c369",
                    "sense_definition": "Cecha tego, co jest takie samo pod względem wielkości, ilości, wartości lub jakości",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_attribute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie można postawić znaku <w>równości</w> między tymi dwoma zupełnie różnymi problemami.",
                            "target_lang_translations": {
                                "ru": "Нельзя ставить знак <w>равенства</w> между этими двумя совершенно разными проблемами."
                            }
                        },
                        {
                            "text": "Trener podkreślał <w>równość</w> poziomu obu drużyn przed finałowym meczem.",
                            "target_lang_translations": {
                                "ru": "Тренер подчеркивал <w>равенство</w> уровня обеих команд перед финальным матчем."

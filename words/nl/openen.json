{
    "word_family": [
        "open",
        "opening",
        "opener",
        "heropenen",
        "openheid",
        "openlijk"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d320625-100f-3f20-ac2d-9c542ece1a4e",
                    "sense_definition": "Toegankelijk maken wat gesloten is, zoals een deur, raam, slot of verpakking.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je alsjeblieft het raam even <w>openen</w>?",
                            "target_lang_translations": {
                                "en": "Can you please <w>open</w> the window for a moment?",
                                "ru": "Ты можешь, пожалуйста, <w>открыть</w> окно?"
                            }
                        },
                        {
                            "text": "Hij <w>opende</w> de deur voor de gasten.",
                            "target_lang_translations": {
                                "en": "He <w>opened</w> the door for the guests.",
                                "ru": "Он <w>открыл</w> дверь для гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "openmaken",
                        "ontsluiten"
                    ],
                    "antonyms": [
                        "sluiten"
                    ],
                    "common_phrases": [
                        "de deur openen",
                        "de ogen openen",
                        "een fles openen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make accessible what is closed, such as a door, window, lock, or package.",
                        "ru": "Сделать доступным то, что закрыто, например, дверь, окно или упаковку."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "open",
                                "target_lang_sense_clarification": "general physical meaning"
                            },
                            {
                                "target_lang_word": "unlock",
                                "target_lang_sense_clarification": "to open a lock"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "открыть",
                                "target_lang_sense_clarification": "физическое действие"
                            },
                            {
                                "target_lang_word": "открывать",
                                "target_lang_sense_clarification": "несовершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84463608-ccb1-3f3b-8cc1-8635aba6754c",
                    "sense_definition": "Een digitaal bestand, programma of applicatie activeren op een computer of telefoon.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "digital",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klik dubbel om de map te <w>openen</w>.",
                            "target_lang_translations": {
                                "en": "Double-click to <w>open</w> the folder.",
                                "ru": "Дважды щелкните, чтобы <w>открыть</w> папку."
                            }
                        },
                        {
                            "text": "Ik kan het document niet <w>openen</w> op mijn mobiel.",
                            "target_lang_translations": {
                                "en": "I cannot <w>open</w> the document on my mobile.",
                                "ru": "Я не могу <w>открыть</w> документ на своем мобильном."
                            }
                        }
                    ],
                    "antonyms": [
                        "sluiten"
                    ],
                    "common_phrases": [
                        "een bestand openen",
                        "een link openen",
                        "een app openen"
                    ],
                    "target_lang_definitions": {
                        "en": "To activate a digital file, program, or application on a computer or phone.",
                        "ru": "Запустить цифровой файл, программу или приложение на компьютере или телефоне."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "open",
                                "target_lang_sense_clarification": "digital files or apps"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "открыть",
                                "target_lang_sense_clarification": "файл, программу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cf214ba-9859-3531-95c0-7ade33fc5114",
                    "sense_definition": "Laten beginnen of officieel van start laten gaan, zoals een bedrijf, evenement of handeling.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "start_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn oom gaat volgende maand een nieuwe winkel <w>openen</w>.",
                            "target_lang_translations": {
                                "en": "My uncle is going to <w>open</w> a new shop next month.",
                                "ru": "Мой дядя собирается <w>открыть</w> новый магазин в следующем месяце."
                            }
                        },
                        {
                            "text": "De soldaten <w>openden</w> direct het vuur op de vijand.",
                            "target_lang_translations": {
                                "en": "The soldiers immediately <w>opened</w> fire on the enemy.",
                                "ru": "Солдаты немедленно <w>открыли</w> огонь по врагу."
                            }
                        }
                    ],
                    "synonyms": [
                        "starten",
                        "beginnen"
                    ],
                    "antonyms": [
                        "sluiten"
                    ],
                    "common_phrases": [
                        "het vuur openen",
                        "een rekening openen",
                        "de vergadering openen"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause to start or officially begin, such as a business, event, or action.",
                        "ru": "Начать или официально запустить, например, бизнес, мероприятие или действие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "open",
                                "target_lang_sense_clarification": "a business or event"
                            },
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "an action or fire"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "открыть",
                                "target_lang_sense_clarification": "бизнес, счет"
                            },
                            {
                                "target_lang_word": "начать",
                                "target_lang_sense_clarification": "действие, огонь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4214dcc7-71d8-3c19-bc6e-445d0bedaca7",
                    "sense_definition": "Zich ontvouwen of wijder worden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_unfold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bloemen <w>openen</w> zich zodra de zon opkomt.",
                            "target_lang_translations": {
                                "en": "The flowers <w>open</w> as soon as the sun comes up.",
                                "ru": "Цветы <w>раскрываются</w>, как только восходит солнце."
                            }
                        },
                        {
                            "text": "Voor ons <w>opende</w> zich een prachtig uitzicht over de vallei.",
                            "target_lang_translations": {
                                "en": "A beautiful view over the valley <w>opened</w> up before us.",
                                "ru": "Перед нами <w>открылся</w> прекрасный вид на долину."
                            }
                        }
                    ],
                    "antonyms": [
                        "sluiten"
                    ],
                    "common_phrases": [
                        "zich openen"
                    ],
                    "target_lang_definitions": {
                        "en": "To unfold itself or become wider.",
                        "ru": "Раскрываться или становиться шире."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "open",
                                "target_lang_sense_clarification": "flowers or eyes"
                            },
                            {
                                "target_lang_word": "open up",
                                "target_lang_sense_clarification": "a view or landscape"
                            },
                            {
                                "target_lang_word": "unfold",
                                "target_lang_sense_clarification": "to spread out"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раскрываться",
                                "target_lang_sense_clarification": "о цветах"
                            },
                            {
                                "target_lang_word": "открываться",
                                "target_lang_sense_clarification": "о виде, панораме"
                            }
                        ]
                    }
                }
            ]
        }
    ]

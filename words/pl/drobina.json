    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cf965fcc-ce39-314a-8540-c6dfaa708421",
                    "sense_definition": "Bardzo mały kawałek materii lub niewielka ilość czegoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "small_piece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wpadła mi do oka jakaś <w>drobina</w> i strasznie mnie uwiera.",
                            "target_lang_translations": {
                                "ru": "Мне в глаз попала какая-то <w>соринка</w>, и она ужасно мешает."
                            }
                        },
                        {
                            "text": "W jego głosie nie było słychać ani <w>drobiny</w> strachu.",
                            "target_lang_translations": {
                                "ru": "В его голосе не было слышно ни <w>капли</w> страха."
                            }
                        }
                    ],
                    "synonyms": [
                        "okruszyna",
                        "kawałeczek",
                        "odrobina",
                        "cząstka"
                    ],
                    "common_phrases": [
                        "drobina kurzu",
                        "ani drobiny",
                        "drobina prawdy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень маленький кусочек материи или небольшое количество чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крупица",
                                "target_lang_sense_clarification": "маленькая частица"
                            },
                            {
                                "target_lang_word": "частица",
                                "target_lang_sense_clarification": "фрагмент материи"
                            },
                            {
                                "target_lang_word": "соринка",
                                "target_lang_sense_clarification": "мелкая частица, обычно в глазу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4abc0dea-8b23-3b49-8965-6c1baad311cc",
                    "sense_definition": "Najmniejsza cząstka substancji, która zachowuje jej właściwości chemiczne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "science_particle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każda <w>drobina</w> wody składa się z dwóch atomów wodoru i jednego tlenu.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>молекула</w> воды состоит из двух атомов водорода и одного атома кислорода."
                            }
                        },
                        {
                            "text": "W wysokiej temperaturze <w>drobina</w> porusza się znacznie szybciej.",
                            "target_lang_translations": {
                                "ru": "При высокой температуре <w>частица</w> движется значительно быстрее."
                            }
                        }
                    ],
                    "synonyms": [
                        "cząsteczka"
                    ],
                    "common_phrases": [
                        "budowa drobiny",
                        "oddziaływania między drobinami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мельчайшая частица вещества, сохраняющая его химические свойства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молекула",
                                "target_lang_sense_clarification": "химическая"
                            },
                            {
                                "target_lang_word": "частица",
                                "target_lang_sense_clarification": "физическая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe4e5344-071f-34a0-a10e-9800c018c0d1",
                    "sense_definition": "Mała, delikatna istota żywa, o której mówi się z czułością lub litością.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "living_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta mała <w>drobina</w> trzęsła się z zimna na deszczu.",
                            "target_lang_translations": {
                                "ru": "Эта маленькая <w>кроха</w> тряслась от холода под дождем."
                            }
                        },
                        {
                            "text": "Spójrz na tego kotka, to taka bezbronna <w>drobina</w>.",
                            "target_lang_translations": {
                                "ru": "Посмотри на этого котенка, это такая беззащитная <w>малютка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kruszyna"
                    ],
                    "common_phrases": [
                        "biedna drobina",
                        "kochana drobina"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькое, хрупкое живое существо, о котором говорят с нежностью или жалостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроха",
                                "target_lang_sense_clarification": "о ребенке или животном"
                            },
                            {
                                "target_lang_word": "малютка",
                                "target_lang_sense_clarification": "ласковое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
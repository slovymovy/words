        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dc237ffb-2e8b-394f-9aad-34c1658fd93a",
                    "sense_definition": "To suppress, conceal, or hush up something, such as an inquiry or a rumor, to prevent it from becoming public.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "suppress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee attempted to <w>burke</w> the scandal before it reached the newspapers.",
                            "target_lang_translations": {
                                "ru": "Комитет попытался <w>замять</w> скандал, пока он не попал в газеты."
                            }
                        },
                        {
                            "text": "He tried to <w>burke</w> the issue by changing the subject immediately.",
                            "target_lang_translations": {
                                "ru": "Он попытался <w>заглушить</w> проблему, немедленно сменив тему."
                            }
                        }
                    ],
                    "synonyms": [
                        "suppress",
                        "hush up",
                        "stifle",
                        "quash",
                        "silence"
                    ],
                    "antonyms": [
                        "reveal",
                        "expose",
                        "publicize",
                        "disclose"
                    ],
                    "common_phrases": [
                        "burke an issue",
                        "burke a scandal",
                        "burke the truth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подавлять, скрывать или замалчивать что-либо (например, расследование или слух), чтобы предотвратить огласку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замять",
                                "target_lang_sense_clarification": "скандал, дело"
                            },
                            {
                                "target_lang_word": "заглушить",
                                "target_lang_sense_clarification": "вопрос, обсуждение"
                            },
                            {
                                "target_lang_word": "утаить",
                                "target_lang_sense_clarification": "скрыть факты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15171fd2-14d9-3442-aa77-5d6ef49ad781",
                    "sense_definition": "To murder someone, typically by suffocation, in a way that leaves no visible marks of violence.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "kill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villain planned to <w>burke</w> his victim in the dark alleyway.",
                            "target_lang_translations": {
                                "ru": "Злодей планировал <w>задушить</w> свою жертву в темном переулке."
                            }
                        },
                        {
                            "text": "In the story, the unsuspecting traveler was <w>burked</w> for his valuables.",
                            "target_lang_translations": {
                                "ru": "В рассказе ничего не подозревающий путешественник был <w>удушен</w> ради его ценностей."
                            }
                        }
                    ],
                    "synonyms": [
                        "suffocate",
                        "smother",
                        "strangle",
                        "asphyxiate"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often used in historical contexts referring to the crimes of Burke and Hare"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Убить кого-либо, как правило, путем удушения, не оставляя видимых следов насилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задушить",
                                "target_lang_sense_clarification": "убить удушением"
                            },
                            {
                                "target_lang_word": "удушить",
                                "target_lang_sense_clarification": "лишить жизни"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "equality",
        "equalize",
        "equally",
        "inequality",
        "unequal"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e1d2777f-ac6d-38ac-8c36-7eec37593f74",
                    "sense_definition": "The same in quantity, size, degree, value, or quality.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "same_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cut the cake into four <w>equal</w> pieces.",
                            "target_lang_translations": {
                                "ru": "Разрежь торт на четыре <w>равные</w> части."
                            }
                        },
                        {
                            "text": "The two boxes are of <w>equal</w> weight.",
                            "target_lang_translations": {
                                "ru": "Эти две коробки <w>одинакового</w> веса."
                            }
                        }
                    ],
                    "synonyms": [
                        "identical",
                        "same",
                        "uniform"
                    ],
                    "antonyms": [
                        "unequal",
                        "unequal"
                    ],
                    "common_phrases": [
                        "of equal value",
                        "equal size",
                        "all things being equal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одинаковый по количеству, размеру, степени, ценности или качеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равный",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "одинаковый",
                                "target_lang_sense_clarification": "идентичный по виду или размеру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40be55b2-2c5f-3381-bf52-1ae7f0e3e1aa",
                    "sense_definition": "Having the same status, rights, or opportunities as others.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All citizens should be <w>equal</w> under the law.",
                            "target_lang_translations": {
                                "ru": "Все граждане должны быть <w>равны</w> перед законом."
                            }
                        },
                        {
                            "text": "Men and women must receive <w>equal</w> pay for the same work.",
                            "target_lang_translations": {
                                "ru": "Мужчины и женщины должны получать <w>равную</w> оплату за один и тот же труд."
                            }
                        }
                    ],
                    "synonyms": [
                        "equivalent",
                        "equitable"
                    ],
                    "antonyms": [
                        "unequal"
                    ],
                    "common_phrases": [
                        "equal rights",
                        "equal opportunity",
                        "created equal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий тот же статус, права или возможности, что и другие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равный",
                                "target_lang_sense_clarification": "по статусу"
                            },
                            {
                                "target_lang_word": "равноправный",
                                "target_lang_sense_clarification": "обладающий равными правами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a6c4ec4-6c06-38d8-b25a-0c022c0d6870",
                    "sense_definition": "Having the necessary ability, strength, or determination for a specific task or situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "capability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I hope he is <w>equal</w> to the challenge.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, он <w>готов</w> к этому испытанию."
                            }
                        },
                        {
                            "text": "She did not feel <w>equal</w> to receiving visitors after her long journey.",
                            "target_lang_translations": {
                                "ru": "Она не чувствовала себя <w>в силах</w> принимать гостей после долгого путешествия."
                            }
                        }
                    ],
                    "synonyms": [
                        "capable",
                        "adequate",
                        "up to"
                    ],
                    "common_phrases": [
                        "equal to the task",
                        "equal to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий необходимыми способностями, силой или решимостью для выполнения конкретной задачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "способный",
                                "target_lang_sense_clarification": "имеющий способность сделать что-то"
                            },
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "подготовленный к чему-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "579220a5-1168-3a87-b32c-1e7c09de6f61",
                    "sense_definition": "Fair and impartial; not showing favoritism.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fairness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge promised to give <w>equal</w> consideration to both sides of the argument.",
                            "target_lang_translations": {
                                "ru": "Судья обещал уделить <w>равное</w> внимание обеим сторонам спора."
                            }
                        },
                        {
                            "text": "They reached an <w>equal</w> agreement that satisfied everyone involved.",
                            "target_lang_translations": {
                                "ru": "Они достигли <w>равноправного</w> соглашения, которое удовлетворило всех участников."
                            }
                        }
                    ],
                    "synonyms": [
                        "fair",
                        "impartial",
                        "just",
                        "unbiased"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often used in formal or literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Справедливый и беспристрастный; не выказывающий предпочтения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "справедливый",
                                "target_lang_sense_clarification": "честный"
                            },
                            {
                                "target_lang_word": "равноправный",
                                "target_lang_sense_clarification": "на равных условиях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b3f2302f-4d6e-3cf3-ac0b-ac456ede5b45",
                    "sense_definition": "To be the same in number, amount, or value.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "math_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Two plus two <w>equals</w> four.",
                            "target_lang_translations": {
                                "ru": "Два плюс два <w>равняется</w> четырем."
                            }
                        },
                        {
                            "text": "In this equation, x <w>equals</w> y.",
                            "target_lang_translations": {
                                "ru": "В этом уравнении икс <w>равен</w> игреку."
                            }
                        }
                    ],
                    "synonyms": [
                        "be",
                        "amount to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть одинаковым по числу, количеству или значению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равняться",
                                "target_lang_sense_clarification": "математически"
                            },
                            {
                                "target_lang_word": "быть равным",
                                "target_lang_sense_clarification": "составлять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91d75f18-bd4e-3be0-ad15-109541e9eae4",
                    "sense_definition": "To be as good as or achieve the same standard as someone or something else.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "match_standard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "No one has <w>equaled</w> her record in the 100 meters.",
                            "target_lang_translations": {
                                "ru": "Никто не <w>повторил</w> её рекорд в беге на 100 метров."
                            }
                        },
                        {
                            "text": "His latest book does not <w>equal</w> the success of his first one.",
                            "target_lang_translations": {
                                "ru": "Его последняя книга не <w>сравнится</w> по успеху с первой."
                            }
                        }
                    ],
                    "synonyms": [
                        "match",
                        "rival",
                        "parallel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть таким же хорошим или достичь того же уровня, что и кто-то или что-то другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сравниться",
                                "target_lang_sense_clarification": "быть на одном уровне"
                            },
                            {
                                "target_lang_word": "повторить",
                                "target_lang_sense_clarification": "достичь того же результата (о рекорде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9f1d6aa-b168-32f3-8388-2822518c4fe0",
                    "sense_definition": "To result in or mean something; to have as a consequence.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Improving quality often <w>equals</w> higher costs.",
                            "target_lang_translations": {
                                "ru": "Улучшение качества часто <w>означает</w> более высокие затраты."
                            }
                        },
                        {
                            "text": "Silence does not always <w>equal</w> agreement.",
                            "target_lang_translations": {
                                "ru": "Молчание не всегда <w>означает</w> согласие."
                            }
                        }
                    ],
                    "synonyms": [
                        "mean",
                        "entail",
                        "imply"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить к чему-либо или означать что-либо; иметь как следствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "означать",
                                "target_lang_sense_clarification": "значить"
                            },
                            {
                                "target_lang_word": "быть равносильным",
                                "target_lang_sense_clarification": "быть эквивалентным"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "384001c5-58f2-3427-b0b4-c82613418beb",
                    "sense_definition": "A person or thing that has the same status, ability, or quality as another.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_thing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She treats her staff as <w>equals</w> rather than subordinates.",
                            "target_lang_translations": {
                                "ru": "Она относится к своим сотрудникам как к <w>равным</w>, а не как к подчиненным."
                            }
                        },
                        {
                            "text": "As a musician, he has no <w>equal</w> in this country.",
                            "target_lang_translations": {
                                "ru": "Как музыканту, ему нет <w>равных</w> в этой стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "peer",
                        "match",
                        "equivalent"
                    ],
                    "common_phrases": [
                        "without equal",
                        "among equals"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или вещь, имеющие тот же статус, способности или качества, что и другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ровня",
                                "target_lang_sense_clarification": "разговорное, человек равного положения"
                            },
                            {
                                "target_lang_word": "равный",
                                "target_lang_sense_clarification": "человек равного статуса"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
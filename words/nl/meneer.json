{
    "word_family": [],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "efa8405b-dde2-3ee2-834e-fd10e12b133c",
                    "sense_definition": "Een beleefde manier om een man aan te spreken of naar hem te verwijzen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "address_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dag, <w>meneer</w> De Vries!",
                            "target_lang_translations": {
                                "ru": "Здравствуйте, <w>господин</w> Де Врис!",
                                "en": "Hello, <w>Mr.</w> De Vries!"
                            }
                        },
                        {
                            "text": "Kunt u mij misschien helpen, <w>meneer</w>?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы мне помочь, <w>господин</w>?",
                                "en": "Could you perhaps help me, <w>sir</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "mijnheer",
                        "heer"
                    ],
                    "antonyms": [
                        "mevrouw"
                    ],
                    "common_phrases": [
                        "dag meneer",
                        "dank u wel meneer",
                        "meneer de voorzitter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливая форма обращения к мужчине или упоминания о нем.",
                        "en": "A polite way to address a man or to refer to him."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "господин",
                                "target_lang_sense_clarification": "вежливое обращение"
                            },
                            {
                                "target_lang_word": "сударь",
                                "target_lang_sense_clarification": "устаревшее или ироничное обращение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "sir",
                                "target_lang_sense_clarification": "polite form of address without a name"
                            },
                            {
                                "target_lang_word": "Mr.",
                                "target_lang_sense_clarification": "title used before a surname"
                            },
                            {
                                "target_lang_word": "mister",
                                "target_lang_sense_clarification": "informal address or title"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34039ce8-97d3-3380-84f9-be9bf703fde8",
                    "sense_definition": "Een man die er deftig uitziet of zich voornaam gedraagt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "gentleman",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je bent een hele <w>meneer</w> in dat nieuwe pak!",
                            "target_lang_translations": {
                                "ru": "Ты выглядишь как настоящий <w>господин</w> в этом новом костюме!",
                                "en": "You look like quite the <w>gentleman</w> in that new suit!"
                            }
                        },
                        {
                            "text": "Hij voelt zich een hele <w>meneer</w> sinds hij die dure auto heeft gekocht.",
                            "target_lang_translations": {
                                "ru": "Он чувствует себя важным <w>господином</w> с тех пор, как купил эту дорогую машину.",
                                "en": "He feels like quite the <w>gentleman</w> since he bought that expensive car."
                            }
                        }
                    ],
                    "synonyms": [
                        "heer"
                    ],
                    "common_phrases": [
                        "een hele meneer",
                        "een grote meneer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, который выглядит представительно или ведет себя важно.",
                        "en": "A man who looks dignified or behaves importantly."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "господин",
                                "target_lang_sense_clarification": "важный человек, джентльмен"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "gentleman",
                                "target_lang_sense_clarification": "a dignified or well-mannered man"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d6cf4776-b85d-354a-8147-1a4858cea900",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>meneren</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>meneer</w> iedereen die ouder is dan ik uit beleefdheid.",
                            "target_lang_translations": {
                                "ru": "Я из вежливости <w>называю господином</w> всех, кто старше меня.",
                                "en": "I <w>call</w> everyone older than me <w>sir</w> out of politeness."
                            }
                        },
                        {
                            "text": "Als ik <w>meneer</w>, doe ik dat uit respect voor de klant.",
                            "target_lang_translations": {
                                "ru": "Когда я <w>называю господином</w>, я делаю это из уважения к клиенту.",
                                "en": "When I <w>say sir</w>, I do so out of respect for the customer."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени глагола <w>meneren</w>.",
                        "en": "First person singular present tense of <w>meneren</w>."
                    },
                    "translations": {
                        "ru": [],
                        "en": []
                    }
                },
                {
                    "sense_id": "dc442403-2cf8-3fc5-9112-55ad452f70ed",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>meneren</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Meneer</w> je de leraar altijd als je hem iets vraagt?",
                            "target_lang_translations": {
                                "ru": "Ты всегда <w>называешь господином</w> учителя, когда о чем-то его спрашиваешь?",
                                "en": "Do you always <w>call</w> the teacher <w>sir</w> when you ask him something?"
                            }
                        },
                        {
                            "text": "Waarom <w>meneer</w> je mij steeds terwijl we even oud zijn?",
                            "target_lang_translations": {
                                "ru": "Почему ты все время <w>называешь меня господином</w>, хотя мы ровесники?",
                                "en": "Why do you keep <w>calling</w> me <w>sir</w> even though we are the same age?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Второе лицо единственного числа настоящего времени глагола <w>meneren</w>.",
                        "en": "Second person singular present tense of <w>meneren</w>."
                    },
                    "translations": {
                        "ru": [],
                        "en": []
                    }
                },
                {
                    "sense_id": "52a36431-6226-3e74-8d48-31537fbb2552",
                    "sense_definition": "Gebiedende wijs van <w>meneren</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Meneer</w> niet zo overdreven, noem me gewoon bij mijn naam.",
                            "target_lang_translations": {
                                "ru": "Не <w>называй меня господином</w> так нарочито, зови меня просто по имени.",
                                "en": "Don't <w>sir</w> me so excessively, just call me by my name."
                            }
                        },
                        {
                            "text": "<w>Meneer</w> en mevrouw er maar op los als je dat wilt.",
                            "target_lang_translations": {
                                "ru": "<w>Называй господином</w> и госпожой сколько влезет, если хочешь.",
                                "en": "You can <w>sir</w> and madam all you want."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола <w>meneren</w>.",
                        "en": "Imperative mood of <w>meneren</w>."
                    },

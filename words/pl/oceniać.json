            "pos": "verb",
            "senses": [
                {
                    "sense_id": "19baf796-af5c-384d-92a7-880aaa814a15",
                    "sense_definition": "Wydawać opinię o kimś lub o czymś, określając jego wartość, jakość, znaczenie lub cechy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "evaluation_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinieneś <oceniać> ludzi tylko po ich wyglądzie zewnętrznym.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <w>оценивать</w> людей только по их внешности."
                            }
                        },
                        {
                            "text": "Eksperci muszą dokładnie <oceniać> ryzyko przed rozpoczęciem każdego nowego projektu.",
                            "target_lang_translations": {
                                "ru": "Эксперты должны тщательно <w>оценивать</w> риски перед началом каждого нового проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "sądzić",
                        "osądzać"
                    ],
                    "common_phrases": [
                        "oceniać sytuację",
                        "oceniać ryzyko",
                        "oceniać krytycznie",
                        "oceniać ludzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высказывать мнение о ком-либо или чем-либо, определяя его ценность, качество, значение или черты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "определять качество, судить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63edda2b-f492-3462-9728-f228ecfb24c5",
                    "sense_definition": "Sprawdzać wiedzę lub umiejętności i wystawiać za nie stopnie, punkty lub noty (zwykle w szkole lub sporcie).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "evaluation_grading",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciele w naszej szkole sprawiedliwie <oceniają> postępy wszystkich uczniów.",
                            "target_lang_translations": {
                                "ru": "Учителя в нашей школе справедливо <w>оценивают</w> успехи всех учеников."
                            }
                        },
                        {
                            "text": "Jury będzie <oceniać> występy artystów w skali od jednego do dziesięciu.",
                            "target_lang_translations": {
                                "ru": "Жюри будет <w>оценивать</w> выступления артистов по шкале от одного до десяти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oceniać uczniów",
                        "oceniać prace",
                        "surowo oceniać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверять знания или навыки и выставлять за них оценки, баллы или отметки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "ставить оценки, баллы"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "07e7afdc-49f9-3e56-8de9-9ae3ee988a5e",
                    "sense_definition": "An artist’s or photographer’s workshop or the room in which an artist works.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "workspace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The painter spends every morning working in her <w>studio</w>.",
                            "target_lang_translations": {
                                "ru": "Художница проводит каждое утро, работая в своей <w>мастерской</w>."
                            }
                        },
                        {
                            "text": "He invited the model to his photography <w>studio</w> for the session.",
                            "target_lang_translations": {
                                "ru": "Он пригласил модель в свою <w>студию</w> фотографии для проведения сессии."
                            }
                        }
                    ],
                    "synonyms": [
                        "workshop",
                        "atelier"
                    ],
                    "common_phrases": [
                        "art studio",
                        "photography studio",
                        "artist's studio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мастерская художника или фотографа, или комната, в которой работает художник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастерская",
                                "target_lang_sense_clarification": "рабочее место художника"
                            },
                            {
                                "target_lang_word": "студия",
                                "target_lang_sense_clarification": "рабочее место фотографа или художника"
                            },
                            {
                                "target_lang_word": "ателье",
                                "target_lang_sense_clarification": "фотоателье или мастерская"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82ca99f4-1ed9-3a6a-915b-3f727b716b96",
                    "sense_definition": "A room or building where radio or television programs, records, or films are made.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "workspace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band spent three weeks in the <w>studio</w> recording their new album.",
                            "target_lang_translations": {
                                "ru": "Группа провела три недели в <w>студии</w>, записывая свой новый альбом."
                            }
                        },
                        {
                            "text": "The news is broadcast live from our London <w>studio</w> every evening.",
                            "target_lang_translations": {
                                "ru": "Новости транслируются в прямом эфире из нашей лондонской <w>студии</w> каждый вечер."
                            }
                        }
                    ],
                    "common_phrases": [
                        "recording studio",
                        "television studio",
                        "film studio",
                        "in the studio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещение или здание, где создаются радио- или телепрограммы, музыкальные записи или фильмы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "студия",
                                "target_lang_sense_clarification": "помещение для записи или съемки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6753a559-9530-3f83-a2cf-0cf47c3c60e9",
                    "sense_definition": "A small apartment consisting of a single main room that functions as living room, bedroom, and kitchen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "housing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is much cheaper to rent a <w>studio</w> than a one-bedroom apartment.",
                            "target_lang_translations": {
                                "ru": "Арендовать <w>студию</w> намного дешевле, чем однокомнатную квартиру."
                            }
                        },
                        {
                            "text": "He lives in a tiny <w>studio</w> in the city center near his office.",
                            "target_lang_translations": {
                                "ru": "Он живет в крошечной <w>студии</w> в центре города, недалеко от офиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "studio apartment",
                        "bedsit"
                    ],
                    "common_phrases": [
                        "rent a studio",
                        "live in a studio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая квартира, состоящая из одной главной комнаты, которая служит гостиной, спальней и кухней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "студия",
                                "target_lang_sense_clarification": "тип квартиры"
                            },
                            {
                                "target_lang_word": "квартира-студия",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0577c3be-8d75-39d5-ae2c-c39c28fc1306",
                    "sense_definition": "A company or organization that makes films, records, or other artistic works.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>studio</w> announced the release date for the new blockbuster movie.",
                            "target_lang_translations": {
                                "ru": "<w>Студия</w> объявила дату выхода нового блокбастера."
                            }
                        },
                        {
                            "text": "She signed a multi-year contract with a major Hollywood <w>studio</w>.",
                            "target_lang_translations": {
                                "ru": "Она подписала многолетний контракт с крупной голливудской <w>студией</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "production company"
                    ],
                    "common_phrases": [
                        "movie studio",
                        "major studio",
                        "film studio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компания или организация, занимающаяся производством фильмов, музыкальных записей или других художественных произведений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "студия",
                                "target_lang_sense_clarification": "организация"
                            },
                            {
                                "target_lang_word": "киностудия",
                                "target_lang_sense_clarification": "компания по производству фильмов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cbb2e01-85c1-317c-9f52-09b116226f89",
                    "sense_definition": "An establishment where an art, such as dance or martial arts, is taught or practiced.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "workspace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My daughter attends ballet classes at a local dance <w>studio</w>.",
                            "target_lang_translations": {
                                "ru": "Моя дочь посещает уроки балета в местной танцевальной <w>студии</w>."
                            }
                        },
                        {
                            "text": "The yoga <w>studio</w> is located on the second floor of the building.",
                            "target_lang_translations": {
                                "ru": "<w>Студия</w> йоги находится на втором этаже здания."
                            }
                        }
                    ],
                    "synonyms": [
                        "school"
                    ],
                    "common_phrases": [
                        "dance studio",
                        "yoga studio"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение, где обучают искусству (например, танцам или боевым искусствам) или занимаются им."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "студия",
                                "target_lang_sense_clarification": "школа танцев или искусств"
                            }
                        ]
                    }
                }
            ]
        }
    ]

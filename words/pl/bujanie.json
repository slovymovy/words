            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a77a308-81c6-3b6f-9caf-96d009afce61",
                    "sense_definition": "Czynność polegająca na rytmicznym poruszaniu kimś lub czymś w jedną i drugą stronę, na przykład w celu uśpienia lub zabawy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rozpieściliście dziecko tym ciągłym <w>bujaniem</w>.",
                            "target_lang_translations": {
                                "ru": "Вы избаловали ребенка этим постоянным <w>укачиванием</w>."
                            }
                        },
                        {
                            "text": "Delikatne <w>bujanie</w> hamaka pomogło mi szybko zasnąć.",
                            "target_lang_translations": {
                                "ru": "Легкое <w>покачивание</w> гамака помогло мне быстро уснуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "kołysanie",
                        "huśtanie"
                    ],
                    "antonyms": [
                        "bezruch"
                    ],
                    "common_phrases": [
                        "bujanie dziecka",
                        "bujanie nogą",
                        "bujanie krzesłem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в ритмичном движении кого-либо или чего-либо из стороны в сторону, например, для усыпления или развлечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "качание",
                                "target_lang_sense_clarification": "ритмичное движение из стороны в сторону"
                            },
                            {
                                "target_lang_word": "укачивание",
                                "target_lang_sense_clarification": "ребенка перед сном"
                            },
                            {
                                "target_lang_word": "раскачивание",
                                "target_lang_sense_clarification": "увеличение амплитуды движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d985f23-9ea5-33cc-9fff-a961c1b55341",
                    "sense_definition": "Stan lub czynność polegająca na swobodnym unoszeniu się w powietrzu lub na wodzie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego ciągłe <w>bujanie</w> w obłokach irytuje wszystkich nauczycieli.",
                            "target_lang_translations": {
                                "ru": "Его постоянное <w>витание</w> в облаках раздражает всех учителей."
                            }
                        },
                        {
                            "text": "Przez okno obserwowaliśmy powolne <w>bujanie</w> latawca na wietrze.",
                            "target_lang_translations": {
                                "ru": "Через окно мы наблюдали за медленным <w>качанием</w> воздушного змея на ветру."
                            }
                        }
                    ],
                    "synonyms": [
                        "szybowanie",
                        "unoszenie się"
                    ],
                    "antonyms": [
                        "opadanie"
                    ],
                    "common_phrases": [
                        "bujanie w obłokach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние или действие, заключающееся в свободном парении в воздухе или на воде; часто используется в переносном смысле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "витание",
                                "target_lang_sense_clarification": "в облаках, мечтах"
                            },
                            {
                                "target_lang_word": "парение",
                                "target_lang_sense_clarification": "свободное движение в воздухе"
                            }
                        ]
                    }
                }
            ]
        }
    ]

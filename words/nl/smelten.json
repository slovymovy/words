    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "95163ea9-06ac-3558-a570-5fa30d23d37a",
                    "sense_definition": "Van vaste in vloeibare vorm overgaan door warmte.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De sneeuw <w>smolt</w> snel toen de zon begon te schijnen.",
                            "target_lang_translations": {
                                "en": "The snow <w>melted</w> quickly when the sun started to shine.",
                                "ru": "Снег быстро <w>таял</w>, когда выглянуло солнце."
                            }
                        },
                        {
                            "text": "Mijn ijsje <w>smelt</w> in mijn hand omdat het buiten zo heet is.",
                            "target_lang_translations": {
                                "en": "My ice cream is <w>melting</w> in my hand because it is so hot outside.",
                                "ru": "Мое мороженое <w>тает</w> в руке, потому что на улице так жарко."
                            }
                        }
                    ],
                    "antonyms": [
                        "stollen",
                        "bevriezen"
                    ],
                    "common_phrases": [
                        "ijs smelt",
                        "sneeuw smelt",
                        "kaas smelt"
                    ],
                    "target_lang_definitions": {
                        "en": "To change from a solid to a liquid form due to heat.",
                        "ru": "Переходить из твердого состояния в жидкое под воздействием тепла"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "melt",
                                "target_lang_sense_clarification": "to become liquid"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "таять",
                                "target_lang_sense_clarification": "о снеге, льде, мороженом"
                            },
                            {
                                "target_lang_word": "плавиться",
                                "target_lang_sense_clarification": "о металле, сыре, пластике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94fe7808-4019-340b-9db7-47dadcd8721c",
                    "sense_definition": "Iets van vaste in vloeibare vorm doen overgaan door verhitting.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kok <w>smolt</w> de boter in de pan voordat hij het vlees toevoegde.",
                            "target_lang_translations": {
                                "en": "The cook <w>melted</w> the butter in the pan before adding the meat.",
                                "ru": "Повар <w>растопил</w> масло на сковороде перед тем, как добавить мясо."
                            }
                        },
                        {
                            "text": "In de fabriek <w>smelten</w> ze het metaal om er nieuwe vormen van te maken.",
                            "target_lang_translations": {
                                "en": "In the factory they <w>melt</w> the metal to make new shapes from it.",
                                "ru": "На заводе <w>плавят</w> металл, чтобы изготавливать из него новые формы."
                            }
                        }
                    ],
                    "antonyms": [
                        "bevriezen"
                    ],
                    "common_phrases": [
                        "boter smelten",
                        "chocolade smelten",
                        "goud smelten"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause something to change from a solid to a liquid form by heating.",
                        "ru": "Превращать что-либо из твердого состояния в жидкое путем нагревания"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "melt",
                                "target_lang_sense_clarification": "to make liquid"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "растапливать",
                                "target_lang_sense_clarification": "масло, жир, шоколад"
                            },
                            {
                                "target_lang_word": "плавить",
                                "target_lang_sense_clarification": "металл, стекло"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

                {
                    "sense_id": "f9d9280f-f618-37b8-aeec-e483d88f9006",
                    "sense_definition": "Military personnel in uniform.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "military_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government decided to withdraw its <w>troops</w> from the conflict zone.",
                            "target_lang_translations": {
                                "ru": "Правительство решило вывести свои <w>войска</w> из зоны конфликта."
                            }
                        },
                        {
                            "text": "Thousands of foreign <w>troops</w> are stationed in the region to maintain peace.",
                            "target_lang_translations": {
                                "ru": "Тысячи иностранных <w>солдат</w> размещены в регионе для поддержания мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "soldiers"
                    ],
                    "common_phrases": [
                        "ground troops",
                        "deploy troops",
                        "withdraw troops",
                        "send in troops"
                    ],
                    "target_lang_definitions": {
                        "ru": "Военнослужащие, вооруженные силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "войска",
                                "target_lang_sense_clarification": "вооруженные силы"
                            },
                            {
                                "target_lang_word": "солдаты",
                                "target_lang_sense_clarification": "военнослужащие"
                            }
                        ]
                    }
                },
                {

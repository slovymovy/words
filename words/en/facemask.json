{
    "word_family": [
        "mask",
        "unmask",
        "masking",
        "masked"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0e7e1f87-61ed-3eca-9bdc-4d1c5131a46f",
                    "sense_definition": "A covering worn over the mouth and nose to prevent the spread of infection or to protect against dust and pollutants.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "protective_gear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must wear a <w>facemask</w> when entering the hospital.",
                            "target_lang_translations": {
                                "ru": "Вы должны надеть <w>маску</w> при входе в больницу."
                            }
                        },
                        {
                            "text": "The doctor put on a sterile <w>facemask</w> before starting the surgery.",
                            "target_lang_translations": {
                                "ru": "Врач надел стерильную <w>маску</w> перед началом операции."
                            }
                        }
                    ],
                    "synonyms": [
                        "mask",
                        "surgical mask",
                        "respirator"
                    ],
                    "common_phrases": [
                        "wear a facemask",
                        "put on a facemask",
                        "protective facemask"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытие, закрывающее рот и нос для защиты от инфекций, пыли или загрязняющих веществ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маска",
                                "target_lang_sense_clarification": "средство защиты"
                            },
                            {
                                "target_lang_word": "медицинская маска",
                                "target_lang_sense_clarification": "для врачей и больных"
                            },
                            {
                                "target_lang_word": "защитная маска",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8b7d884-4935-3549-9dcd-3c68ad2fd6f8",
                    "sense_definition": "A substance like cream or clay applied to the face for a short time to clean or improve the skin.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cosmetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She relaxed in the bath with a green clay <w>facemask</w> on her skin.",
                            "target_lang_translations": {
                                "ru": "Она расслаблялась в ванной с зеленой глиняной <w>маской</w> на лице."
                            }
                        },
                        {
                            "text": "Using a hydrating <w>facemask</w> once a week helps keep your face smooth.",
                            "target_lang_translations": {
                                "ru": "Использование увлажняющей <w>маски для лица</w> раз в неделю помогает сохранить кожу гладкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "face pack",
                        "facial"
                    ],
                    "common_phrases": [
                        "apply a facemask",
                        "clay facemask",
                        "hydrating facemask"
                    ],
                    "target_lang_definitions": {
                        "ru": "Косметическое средство (крем, глина), наносимое на лицо для очищения или ухода за кожей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маска для лица",
                                "target_lang_sense_clarification": "косметическая процедура"
                            },
                            {
                                "target_lang_word": "косметическая маска",
                                "target_lang_sense_clarification": "средство ухода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35033e45-b77b-3139-8fce-ba65e3701839",
                    "sense_definition": "A protective grille or shield attached to the front of a helmet in sports like American football or hockey.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_gear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player's <w>facemask</w> was bent after the heavy collision.",
                            "target_lang_translations": {
                                "ru": "<w>Маска</w> игрока погнулась после сильного столкновения."
                            }
                        },
                        {
                            "text": "He grabbed the opponent's <w>facemask</w> while trying to make the tackle.",
                            "target_lang_translations": {
                                "ru": "Он схватил соперника за <w>маску</w>, пытаясь выполнить захват."
                            }
                        }
                    ],
                    "synonyms": [
                        "visor",
                        "face guard"
                    ],
                    "common_phrases": [
                        "grab the facemask",
                        "helmet facemask"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитная решетка или щиток на шлеме в контактных видах спорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маска",
                                "target_lang_sense_clarification": "часть шлема"
                            },
                            {
                                "target_lang_word": "защитная сетка",
                                "target_lang_sense_clarification": "на шлеме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "395a9c97-06c2-3ed2-86d4-c56614486f17",
                    "sense_definition": "A penalty in American football called when a player illegally grasps the face guard of another player's helmet.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_rule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The referee threw a yellow flag to signal a <w>facemask</w> on the defense.",
                            "target_lang_translations": {
                                "ru": "Судья выбросил желтый флаг, сигнализируя о <w>захвате маски</w> защитой."
                            }
                        },
                        {
                            "text": "A fifteen-yard penalty was assessed for the <w>facemask</w>.",
                            "target_lang_translations": {
                                "ru": "За <w>захват маски</w> был назначен штраф в пятнадцать ярдов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "call a facemask",
                        "facemask penalty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушение правил в американском футболе, заключающееся в захвате защитной маски шлема соперника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "захват маски",
                                "target_lang_sense_clarification": "нарушение правил"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "13a08cec-84ee-3a5e-91fa-6d44a5da8437",
                    "sense_definition": "Не проснуться в нужное время; проснуться позже, чем планировалось.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sleep_issues",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я боюсь <w>просыпать</w> на работу, поэтому ставлю два будильника.",
                            "target_lang_translations": {
                                "en": "I am afraid to <w>oversleep</w> for work, so I set two alarms."
                            }
                        },
                        {
                            "text": "Студент часто <w>просыпал</w> первую пару и приходил в университет к обеду.",
                            "target_lang_translations": {
                                "en": "The student often <w>overslept</w> the first class and came to the university by lunchtime."
                            }
                        }
                    ],
                    "synonyms": [
                        "проспать"
                    ],
                    "common_phrases": [
                        "просыпать на работу",
                        "просыпать в школу",
                        "вечно просыпать"
                    ],
                    "target_lang_definitions": {
                        "en": "To not wake up at the required time; to wake up later than planned."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oversleep",
                                "target_lang_sense_clarification": "wake up later than intended"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "652bd125-4713-3e3f-958e-f1beb97a3198",
                    "sense_definition": "Пропустить какое-либо событие или возможность из-за того, что спал.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sleep_issues",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <w>просыпали</w> завтрак в отеле и остались голодными.",
                            "target_lang_translations": {
                                "en": "We <w>slept through</w> breakfast at the hotel and stayed hungry."
                            }
                        },
                        {
                            "text": "Не хочется <w>просыпать</w> такой чудесный солнечный день.",
                            "target_lang_translations": {
                                "en": "I don't want to <w>sleep through</w> such a wonderful sunny day."
                            }
                        }
                    ],
                    "synonyms": [
                        "пропустить"
                    ],
                    "common_phrases": [
                        "просыпать рассвет",
                        "просыпать всё самое интересное",
                        "просыпать завтрак"
                    ],
                    "target_lang_definitions": {
                        "en": "To miss an event or opportunity due to sleeping."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sleep through",
                                "target_lang_sense_clarification": "miss something because of sleep"
                            },
                            {
                                "target_lang_word": "miss",
                                "target_lang_sense_clarification": "fail to attend because of sleeping"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3109d5d1-a4ec-3d00-9601-b402221ed762",
                    "sense_definition": "Нечаянно уронить или рассыпать что-либо мелкое и сыпучее (соль, крупу, порошок).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spill_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Постарайся не <w>просыпать</w> муку на чистый пол.",
                            "target_lang_translations": {
                                "en": "Try not to <w>spill</w> flour on the clean floor."
                            }
                        },
                        {
                            "text": "Он неловко повернулся и <w>просыпал</w> сахар мимо чашки.",
                            "target_lang_translations": {
                                "en": "He turned awkwardly and <w>spilled</w> sugar past the cup."
                            }
                        }
                    ],
                    "synonyms": [
                        "рассыпать",
                        "уронить"
                    ],
                    "antonyms": [
                        "собрать"
                    ],
                    "common_phrases": [
                        "просыпать соль",
                        "просыпать сахар",
                        "просыпать на пол"
                    ],
                    "target_lang_definitions": {
                        "en": "To accidentally drop or scatter something small and loose (salt, grains, powder)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spill",
                                "target_lang_sense_clarification": "accidentally scatter a substance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8659455e-6ad0-389c-81fd-cc8c8105568a",
                    "sense_definition": "Провести определённое количество времени во сне.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sleep_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В выходной день я могу <w>просыпать</w> до самого обеда.",
                            "target_lang_translations": {
                                "en": "On a day off, I can <w>sleep</w> until lunch."
                            }
                        },
                        {
                            "text": "Уставший путник <w>просыпал</w> почти сутки, восстанавливая силы.",
                            "target_lang_translations": {
                                "en": "The tired traveler <w>slept</w> for almost twenty-four hours, recovering his strength."
                            }
                        }
                    ],
                    "common_phrases": [
                        "просыпать весь день",
                        "просыпать до обеда"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend a specific amount of time sleeping."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sleep",
                                "target_lang_sense_clarification": "spend time in a state of sleep"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
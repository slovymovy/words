    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c196687b-050f-3ac1-88e0-68faf2ee6ff9",
                    "sense_definition": "Nawiązać z kimś kontakt w celu wymiany informacji (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "contact_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę pilnie <w>skomunikować</w> się z moim szefem w sprawie projektu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно срочно <w>связаться</w> с моим начальником по поводу проекта."
                            }
                        },
                        {
                            "text": "Czy udało ci się w końcu <w>skomunikować</w> się telefonicznie z biurem obsługi klienta?",
                            "target_lang_translations": {
                                "ru": "Тебе удалось в конце концов <w>связаться</w> по телефону с отделом обслуживания клиентов?"
                            }
                        }
                    ],
                    "synonyms": [
                        "skontaktować się"
                    ],
                    "common_phrases": [
                        "skomunikować się telefonicznie",
                        "skomunikować się mailowo",
                        "skomunikować się z bazą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установить контакт с кем-либо для обмена информацией (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связаться",
                                "target_lang_sense_clarification": "установить контакт"
                            },
                            {
                                "target_lang_word": "списаться",
                                "target_lang_sense_clarification": "связаться письменно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e008861-00b0-3e7a-9f7d-68586e9f6007",
                    "sense_definition": "Zapewnić połączenie transportowe umożliwiające dojazd do określonego miejsca.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy hotel jest świetnie <w>skomunikowany</w> z głównym dworcem kolejowym.",
                            "target_lang_translations": {
                                "ru": "Этот новый отель отлично <w>связан</w> с главным железнодорожным вокзалом."
                            }
                        },
                        {
                            "text": "Władze miasta planują lepiej <w>skomunikować</w> odległe dzielnice z centrum.",
                            "target_lang_translations": {
                                "ru": "Власти города планируют лучше <w>связать</w> отдаленные районы с центром."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dobrze skomunikowany",
                        "słabo skomunikowany",
                        "świetnie skomunikowany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечить транспортное сообщение, позволяющее добраться до определенного места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "транспортным сообщением"
                            },
                            {
                                "target_lang_word": "соединить",
                                "target_lang_sense_clarification": "маршрутами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52ee2a0f-c218-3929-9c46-7745f80a1bea",
                    "sense_definition": "Połączyć urządzenia lub systemy w taki sposób, aby mogły wymieniać między sobą dane.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Informatyk musi poprawnie <w>skomunikować</w> serwer z nową bazą danych.",
                            "target_lang_translations": {
                                "ru": "Информатик должен правильно <w>соединить</w> сервер с новой базой данных."
                            }
                        },
                        {
                            "text": "Udało nam się <w>skomunikować</w> urządzenia pomiarowe z centralnym komputerem.",
                            "target_lang_translations": {
                                "ru": "Нам удалось <w>сопрячь</w> измерительные приборы с центральным компьютером."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skomunikować systemy",
                        "skomunikować urządzenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединить устройства или системы так, чтобы они могли обмениваться данными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопрячь",
                                "target_lang_sense_clarification": "технически"
                            },
                            {
                                "target_lang_word": "соединить",
                                "target_lang_sense_clarification": "подключить друг к другу"
                            }
                        ]
                    }
                }
            ]
        }
    ]

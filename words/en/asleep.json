{
    "word_family": [
        "sleep",
        "sleepy",
        "sleeper",
        "sleepless",
        "oversleep"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d703fc07-1016-3d86-adbc-676c0acbae09",
                    "sense_definition": "In a state of sleep; resting and unconscious.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was <w>asleep</w> when you called.",
                            "target_lang_translations": {
                                "ru": "Я <w>спал</w>, когда ты позвонил."
                            }
                        },
                        {
                            "text": "The baby is fast <w>asleep</w> in the crib.",
                            "target_lang_translations": {
                                "ru": "Ребенок крепко <w>спит</w> в кроватке."
                            }
                        }
                    ],
                    "synonyms": [
                        "sleeping",
                        "dead to the world",
                        "out cold",
                        "out like a light"
                    ],
                    "antonyms": [
                        "awake"
                    ],
                    "common_phrases": [
                        "fall asleep",
                        "fast asleep",
                        "sound asleep",
                        "half asleep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в состоянии сна; спящий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спящий",
                                "target_lang_sense_clarification": "находящийся во сне"
                            },
                            {
                                "target_lang_word": "уснувший",
                                "target_lang_sense_clarification": "погрузившийся в сон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85434181-347f-3bac-b331-4b452ef54c3c",
                    "sense_definition": "Having a numb or prickling sensation in a body part, usually due to lack of blood flow.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My arm fell <w>asleep</w> after I leaned on it for too long.",
                            "target_lang_translations": {
                                "ru": "У меня <w>затекла</w> рука после того, как я слишком долго на нее опирался."
                            }
                        },
                        {
                            "text": "She stood up and realized her foot was <w>asleep</w>.",
                            "target_lang_translations": {
                                "ru": "Она встала и поняла, что у нее <w>онемела</w> нога."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fall asleep",
                        "leg fell asleep",
                        "foot asleep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Онемевший, потерявший чувствительность (о части тела)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затёкший",
                                "target_lang_sense_clarification": "онемевший от неподвижности"
                            },
                            {
                                "target_lang_word": "онемевший",
                                "target_lang_sense_clarification": "потерявший чувствительность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e8d9321-ee37-39e7-bdd0-d8cbaddaf3d1",
                    "sense_definition": "Not paying attention; inattentive to what is happening.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The security guard was <w>asleep</w> at the wheel and missed the incident.",
                            "target_lang_translations": {
                                "ru": "Охранник <w>спал на посту</w> и пропустил инцидент."
                            }
                        },
                        {
                            "text": "You must have been <w>asleep</w> to miss such an obvious mistake.",
                            "target_lang_translations": {
                                "ru": "Ты, должно быть, <w>спал</w>, раз пропустил такую очевидную ошибку."
                            }
                        }
                    ],
                    "antonyms": [
                        "awake"
                    ],
                    "common_phrases": [
                        "asleep at the wheel",
                        "asleep on the job"
                    ],
                    "target_lang_definitions": {
                        "ru": "Невнимательный; не следящий за происходящим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невнимательный",
                                "target_lang_sense_clarification": "рассеянный, потерявший бдительность"
                            },
                            {
                                "target_lang_word": "вялый",
                                "target_lang_sense_clarification": "заторможенный, неактивный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ca9549f-2ec5-38bc-885d-f46160d71dbe",
                    "sense_definition": "Dead.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old king is now <w>asleep</w> in the royal tomb.",
                            "target_lang_translations": {
                                "ru": "Старый король теперь <w>покоится</w> в королевской гробнице."
                            }
                        },
                        {
                            "text": "She spoke softly of those who are <w>asleep</w> in the churchyard.",
                            "target_lang_translations": {
                                "ru": "Она тихо говорила о тех, кто <w>почил</w> на церковном кладбище."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used as a euphemism for dead"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Умерший; усопший (используется как эвфемизм)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почивший",
                                "target_lang_sense_clarification": "умерший (высокий стиль)"
                            },
                            {
                                "target_lang_word": "усопший",
                                "target_lang_sense_clarification": "покойный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e07a7418-f536-3555-a766-f54b6277c5b1",
                    "sense_definition": "The language of the ancient Romans, widely used in the Roman Catholic Church and in scientific classification.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "language_sense",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "She decided to study <w>Latin</w> to understand the roots of English words.",
                            "target_lang_translations": {
                                "ru": "Она решила изучать <w>латынь</w>, чтобы понять корни английских слов."
                            }
                        },
                        {
                            "text": "Many medical and legal terms are still written in <w>Latin</w>.",
                            "target_lang_translations": {
                                "ru": "Многие медицинские и юридические термины до сих пор пишутся на <w>латыни</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Classical Latin",
                        "Vulgar Latin",
                        "speak Latin",
                        "Pig Latin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Язык древних римлян, широко используемый в Римско-католической церкви и в научной классификации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "латы́нь",
                                "target_lang_sense_clarification": "разговорное, общеупотребительное"
                            },
                            {
                                "target_lang_word": "лати́нский язы́к",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a5a2858-52df-386b-8837-57633d8c9d7c",
                    "sense_definition": "The alphabet or writing system used for English and many other languages.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "script_sense",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "Most European languages are written using <w>Latin</w> characters.",
                            "target_lang_translations": {
                                "ru": "Большинство европейских языков используют <w>латинские</w> буквы."
                            }
                        },
                        {
                            "text": "Please write your name clearly in the <w>Latin</w> script.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, напишите свое имя разборчиво <w>латиницей</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Latin alphabet",
                        "Latin script",
                        "Latin characters"
                    ],
                    "target_lang_definitions": {
                        "ru": "Алфавит или система письма, используемая для английского и многих других языков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лати́ница",
                                "target_lang_sense_clarification": "система письма"
                            },
                            {
                                "target_lang_word": "лати́нский алфави́т",
                                "target_lang_sense_clarification": "алфавит"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {

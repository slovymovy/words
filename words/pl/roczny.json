                {
                    "sense_id": "73885506-8fe5-31d4-9b4e-7621ba4330f3",
                    "sense_definition": "Mający, liczący jeden rok (zwykle o wieku osoby, zwierzęcia lub rzeczy).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja kuzynka ma <w>roczne</w> dziecko, które właśnie stawia pierwsze kroki.",
                            "target_lang_translations": {
                                "ru": "У моей кузины есть <w>годовалый</w> ребёнок, который как раз делает первые шаги."
                            }
                        },
                        {
                            "text": "Ten <w>roczny</w> samochód wygląda jak nowy i ma mały przebieg.",
                            "target_lang_translations": {
                                "ru": "Этот <w>годовалый</w> автомобиль выглядит как новый, и у него маленький пробег."
                            }
                        }
                    ],
                    "synonyms": [
                        "jednoroczny"
                    ],
                    "common_phrases": [
                        "roczne dziecko",
                        "roczny pies",
                        "roczna roślina"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий возраст один год (обычно о людях, животных или вещах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "годова́лый",
                                "target_lang_sense_clarification": "о возрасте людей и животных"
                            },
                            {
                                "target_lang_word": "одноле́тний",
                                "target_lang_sense_clarification": "о возрасте растений или предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52720578-5236-3f8e-87b6-1c6567ec46c8",
                    "sense_definition": "Trwający przez rok lub ważny przez okres dwunastu miesięcy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wykupiłem <w>roczny</w> abonament na telewizję, bo był w promocyjnej cenie.",
                            "target_lang_translations": {
                                "ru": "Я купил <w>годовой</w> абонемент на телевидение, потому что он был по скидке."
                            }
                        },
                        {
                            "text": "Mój <w>roczny</w> pobyt w Anglii bardzo poprawił moją znajomość języka.",
                            "target_lang_translations": {
                                "ru": "Моё <w>годичное</w> пребывание в Англии очень улучшило моё знание языка."
                            }
                        }
                    ],
                    "synonyms": [
                        "dwunastomiesięczny"
                    ],
                    "common_phrases": [
                        "roczny abonament",
                        "roczny kontrakt",
                        "roczny urlop",
                        "roczny plan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длящийся год или действительный в течение двенадцати месяцев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "годово́й",
                                "target_lang_sense_clarification": "действительный в течение года"
                            },
                            {
                                "target_lang_word": "годи́чный",
                                "target_lang_sense_clarification": "длящийся один год"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb8fd12a-9d13-305c-9329-6db07bc9eeb1",
                    "sense_definition": "Obejmujący czas roku, dotyczący sumy lub statystyk z tego okresu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Opady <w>roczne</w> w tym regionie są zbyt niskie, by uprawiać kukurydzę.",
                            "target_lang_translations": {
                                "ru": "<w>Годовые</w> осадки в этом регионе слишком низкие, чтобы выращивать кукурузу."
                            }
                        },
                        {
                            "text": "Firma przedstawiła <w>roczny</w> bilans zysków i strat na zebraniu zarządu.",
                            "target_lang_translations": {
                                "ru": "Компания представила <w>годовой</w> баланс прибылей и убытков на собрании правления."
                            }
                        }
                    ],
                    "synonyms": [
                        "całoroczny"
                    ],
                    "common_phrases": [
                        "roczny dochód",
                        "roczny bilans",
                        "roczny raport",
                        "roczny przyrost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охватывающий период одного года, касающийся итогов или статистики за этот период."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "годово́й",
                                "target_lang_sense_clarification": "относящийся к итогам года"

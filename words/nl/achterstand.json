{
    "word_family": [
        "achterstaan",
        "achterstallig"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6c9fec9b-7515-3d6a-af10-439a1f1b9eb1",
                    "sense_definition": "Een situatie waarin men minder ver is gevorderd dan gepland of dan anderen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door zijn ziekte heeft de leerling een grote <w>achterstand</w> opgelopen op school.",
                            "target_lang_translations": {
                                "en": "Due to his illness, the student has fallen far <w>behind</w> at school."
                            }
                        },
                        {
                            "text": "We moeten hard werken om de <w>achterstand</w> in te halen en weer op schema te komen.",
                            "target_lang_translations": {
                                "en": "We must work hard to catch up on the <w>lag</w> and get back on schedule."
                            }
                        }
                    ],
                    "antonyms": [
                        "voorsprong"
                    ],
                    "common_phrases": [
                        "achterstand oplopen",
                        "achterstand inhalen",
                        "achterstand wegwerken"
                    ],
                    "target_lang_definitions": {
                        "en": "A situation where one is less advanced than planned or than others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lag",
                                "target_lang_sense_clarification": "general delay in progress"
                            },
                            {
                                "target_lang_word": "delay",
                                "target_lang_sense_clarification": "time-based retardation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bef0c9bf-e8b9-3372-8148-c12b8db857cd",
                    "sense_definition": "De toestand waarin men minder punten heeft of zich achter een tegenstander bevindt in een wedstrijd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De voetbalclub kwam al snel in de eerste helft op een 1-0 <w>achterstand</w>.",
                            "target_lang_translations": {
                                "en": "The football club quickly fell <w>behind</w> 1-0 in the first half."
                            }
                        },
                        {
                            "text": "Ondanks dat ze <w>op achterstand</w> stonden, bleven de spelers vechten voor de overwinning.",
                            "target_lang_translations": {
                                "en": "Despite being <w>behind</w>, the players kept fighting for the victory."
                            }
                        }
                    ],
                    "antonyms": [
                        "voorsprong"
                    ],
                    "common_phrases": [
                        "op achterstand komen",
                        "op achterstand staan",
                        "tegen een achterstand aankijken"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of having fewer points or being behind an opponent in a match."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deficit",
                                "target_lang_sense_clarification": "in score or points"
                            },
                            {
                                "target_lang_word": "behind",
                                "target_lang_sense_clarification": "position in a competition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e74fb9f-6724-38f8-963a-1f2633ec8ca2",
                    "sense_definition": "Een hoeveelheid werk of betalingen die nog afgehandeld moet worden en zich heeft opgehoopt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf heeft een enorme <w>achterstand</w> in de verwerking van de bestellingen.",
                            "target_lang_translations": {
                                "en": "The company has a huge <w>backlog</w> in processing orders."
                            }
                        },
                        {
                            "text": "Door personeelstekort is er een <w>achterstand</w> ontstaan in het onderhoud van de gebouwen.",
                            "target_lang_translations": {
                                "en": "Due to staff shortages, a <w>backlog</w> has arisen in the maintenance of the buildings."
                            }
                        }
                    ],
                    "common_phrases": [
                        "achterstand in betalingen",
                        "administratieve achterstand"
                    ],
                    "target_lang_definitions": {
                        "en": "An amount of work or payments that still needs to be handled and has accumulated."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "backlog",
                                "target_lang_sense_clarification": "accumulated work"
                            },
                            {
                                "target_lang_word": "arrears",
                                "target_lang_sense_clarification": "overdue payments"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
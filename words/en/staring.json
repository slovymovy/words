    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1c5d298d-253b-3b6b-a5ec-0d5a0294d525",
                    "sense_definition": "Present participle and gerund of <w>stare</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was <w>staring</w> out the window and did not hear me enter the room.",
                            "target_lang_translations": {
                                "ru": "Она <w>пристально смотрела</w> в окно и не услышала, как я вошел в комнату."
                            }
                        },
                        {
                            "text": "It is rude to keep <w>staring</w> at people you do not know.",
                            "target_lang_translations": {
                                "ru": "Невежливо продолжать <w>глазеть</w> на людей, которых вы не знаете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "staring at",
                        "staring into space",
                        "staring blankly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола stare (пристально смотреть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пристально смотреть",
                                "target_lang_sense_clarification": "смотреть внимательно, не отрываясь"
                            },
                            {
                                "target_lang_word": "уставиться",
                                "target_lang_sense_clarification": "смотреть в упор (часто разговорное)"
                            },
                            {
                                "target_lang_word": "глазеть",
                                "target_lang_sense_clarification": "смотреть с праздным любопытством (разговорное)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d894f00e-92f9-39af-834a-1a5bf44bfdd2",
                    "sense_definition": "Looking fixedly with wide-open eyes.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doll had large, <w>staring</w> eyes that seemed to follow me around the room.",
                            "target_lang_translations": {
                                "ru": "У куклы были большие, <w>широко раскрытые</w> глаза, которые, казалось, следили за мной по всей комнате."
                            }
                        },
                        {
                            "text": "He gave me a <w>staring</w> look that made me feel very uncomfortable.",
                            "target_lang_translations": {
                                "ru": "Он бросил на меня <w>пристальный</w> взгляд, от которого мне стало очень не по себе."
                            }
                        }
                    ],
                    "synonyms": [
                        "unblinking",
                        "fixed",
                        "gazing"

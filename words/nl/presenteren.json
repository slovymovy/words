            "senses": [
                {
                    "sense_id": "d2c39e3c-7d48-3584-9ca7-0f9e9bf6d491",
                    "sense_definition": "Iets officieel aan anderen laten zien, uitleggen of aanbieden",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De architect zal morgen het nieuwe ontwerp aan de klanten <w>presenteren</w>.",
                            "target_lang_translations": {
                                "en": "The architect will <w>present</w> the new design to the clients tomorrow.",
                                "ru": "Архитектор завтра <w>представит</w> клиентам новый проект."
                            }
                        },
                        {
                            "text": "Zij <w>presenteerde</w> haar werk op een internationale conferentie.",
                            "target_lang_translations": {
                                "en": "She <w>presented</w> her work at an international conference.",
                                "ru": "Она <w>представила</w> свою работу на международной конференции."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorstellen",
                        "introduceren",
                        "tonen",
                        "aanbieden"
                    ],
                    "antonyms": [
                        "verbergen",
                        "achterhouden"
                    ],
                    "common_phrases": [
                        "een plan presenteren",
                        "zich presenteren",
                        "een oplossing presenteren",
                        "resultaten presenteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To officially show, explain, or offer something to others",
                        "ru": "Официально показывать, объяснять или предлагать что-либо другим"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "to show or offer formally"
                            },
                            {
                                "target_lang_word": "introduce",
                                "target_lang_sense_clarification": "to bring something into notice for the first time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "представлять",
                                "target_lang_sense_clarification": "официально знакомить или показывать"
                            },
                            {
                                "target_lang_word": "презентовать",
                                "target_lang_sense_clarification": "проводить презентацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39719a52-d629-3535-951b-01accee3fe2f",
                    "sense_definition": "Als gastheer of gastvrouw optreden bij een televisieprogramma, radiouitzending of evenement",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>presenteert</w> al jaren het populaire radioprogramma in de ochtend.",
                            "target_lang_translations": {
                                "en": "He has been <w>presenting</w> the popular morning radio program for years.",
                                "ru": "Он уже много лет <w>ведет</w> популярную утреннюю радиопрограмму."
                            }
                        },
                        {
                            "text": "Wie gaat het grote muziekfestival dit jaar <w>presenteren</w>?",
                            "target_lang_translations": {
                                "en": "Who is going to <w>host</w> the big music festival this year?",
                                "ru": "Кто будет <w>вести</w> большой музыкальный фестиваль в этом году?"
                            }
                        }
                    ],
                    "synonyms": [
                        "leiden",
                        "hosten"
                    ],
                    "common_phrases": [
                        "een programma presenteren",
                        "het nieuws presenteren",
                        "een show presenteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To act as a host for a television program, radio broadcast, or event",
                        "ru": "Выступать в качестве ведущего телепрограммы, радиопередачи или мероприятия"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "host",
                                "target_lang_sense_clarification": "to act as the master of ceremonies"
                            },
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "to introduce and appear in a programme"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "быть ведущим программы или мероприятия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

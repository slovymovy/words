                    "common_phrases": [
                        "River Alt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Река в Англии, впадающая в Ирландское море."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Альт",
                                "target_lang_sense_clarification": "река"
                            }

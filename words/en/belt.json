    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e13e8526-e083-3851-a0c2-13523ffd04e1",
                    "sense_definition": "A band worn around the waist to hold clothing to one's body (usually pants), hold weapons, or serve as a decorative piece.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He wore a brown leather <w>belt</w> with his jeans.",
                            "target_lang_translations": {
                                "ru": "Он носил коричневый кожаный <w>ремень</w> с джинсами."
                            }
                        },
                        {
                            "text": "She tightened her <w>belt</w> because the trousers were too loose.",
                            "target_lang_translations": {
                                "ru": "Она затянула <w>ремень</w>, потому что брюки были слишком свободными."
                            }
                        }
                    ],
                    "synonyms": [
                        "waistband",
                        "sash",
                        "girdle",
                        "strap"
                    ],
                    "common_phrases": [
                        "leather belt",
                        "tighten your belt",
                        "buckle your belt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремень или пояс, который носят на талии для поддержания одежды или как украшение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ремень",
                                "target_lang_sense_clarification": "кожаный или для брюк"
                            },
                            {
                                "target_lang_word": "пояс",
                                "target_lang_sense_clarification": "из ткани или общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0507463-3474-3c57-8b61-f3e2ea6a930d",
                    "sense_definition": "A band used as a restraint for safety purposes, such as in a vehicle or aircraft.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "safety_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please fasten your <w>belt</w> before the plane takes off.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, пристегните <w>ремень</w> перед взлетом самолета."
                            }
                        },
                        {
                            "text": "The driver was saved by his safety <w>belt</w> during the crash.",
                            "target_lang_translations": {
                                "ru": "Водителя спас <w>ремень</w> безопасности во время аварии."
                            }

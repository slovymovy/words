{
    "word_family": [
        "moeten",
        "moetje"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3fb2dd42-de80-324a-aa69-a2e2baa37d0d",
                    "sense_definition": "Eerste, tweede en derde persoon enkelvoud onvoltooid tegenwoordige tijd van <w>moeten</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <moet> nu echt naar huis gaan omdat het al laat is.",
                            "target_lang_translations": {
                                "ru": "Мне <нужно> сейчас же идти домой, так как уже поздно."
                            }
                        },
                        {
                            "text": "Hij <moet> elke dag hard werken om zijn gezin te onderhouden.",
                            "target_lang_translations": {
                                "ru": "Он <должен> много работать каждый день, чтобы содержать свою семью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dat moet",
                        "ik moet weg",
                        "het moet wel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое, второе и третье лицо единственного числа настоящего времени глагола <w>moeten</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "должен",
                                "target_lang_sense_clarification": "обязанность (согласуется с родом)"
                            },
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "необходимость (безличная форма)"
                            },
                            {
                                "target_lang_word": "надо",
                                "target_lang_sense_clarification": "разговорная форма необходимости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "635c0145-b07e-334d-ba3c-13ae99d64596",
                    "sense_definition": "Gebiedende wijs van <w>moeten</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Moet> dat nou echt op deze onhandige manier gebeuren?",
                            "target_lang_translations": {
                                "ru": "<Обязательно> ли это должно происходить таким неудобным способом?"
                            }
                        },
                        {
                            "text": "<Moet> niet denken dat je zomaar alles kunt maken.",
                            "target_lang_translations": {
                                "ru": "<Не нужно> думать, что тебе всё сойдет с рук."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола <w>moeten</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "в риторических вопросах или побуждениях"
                            },
                            {
                                "target_lang_word": "обязательно",
                                "target_lang_sense_clarification": "в значении неизбежности действия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c93f198a-73b5-3e09-a774-42809243c0cd",
                    "sense_definition": "Een achtergebleven indruk, kreuk of vlek in een oppervlak.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zware kast heeft een diepe <moet> in het tapijt achtergelaten.",
                            "target_lang_translations": {
                                "ru": "Тяжелый шкаф оставил глубокую <вмятину> на ковре."
                            }
                        },
                        {
                            "text": "De strakke sokken lieten een rode <moet> achter op zijn enkels.",
                            "target_lang_translations": {
                                "ru": "Тесные носки оставили красный <след> на его лодыжках."
                            }
                        }
                    ],
                    "synonyms": [
                        "afdruk",
                        "deuk",
                        "spoor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставшийся след, складка или пятно на поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "след",
                                "target_lang_sense_clarification": "отпечаток на коже или поверхности"
                            },
                            {
                                "target_lang_word": "вмятина",
                                "target_lang_sense_clarification": "углубление от давления"
                            },
                            {
                                "target_lang_word": "отпечаток",
                                "target_lang_sense_clarification": "видимый след"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f0b89ed-02c5-3945-9843-2cedc07491f6",
                    "sense_definition": "Iets wat men verplicht is te doen; een dwang of noodzaak.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_compulsion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor ons is het een plezierige hobby en absoluut geen <moet>.",
                            "target_lang_translations": {
                                "ru": "Для нас это приятное хобби, а вовсе не <обязаловка>."
                            }
                        },
                        {
                            "text": "Zoals het oude spreekwoord zegt: <moet> is een bitter kruid.",
                            "target_lang_translations": {
                                "ru": "Как гласит старая пословица: <необходимость> — горькая трава."
                            }
                        }
                    ],
                    "synonyms": [
                        "dwang",
                        "verplichting",
                        "noodzaak"
                    ],
                    "antonyms": [
                        "keuze",
                        "vrijheid"
                    ],
                    "common_phrases": [
                        "geen moet",
                        "een moetje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то, что человек обязан сделать; принуждение или необходимость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обязаловка",
                                "target_lang_sense_clarification": "разговорное, навязанная обязанность"
                            },
                            {
                                "target_lang_word": "принуждение",
                                "target_lang_sense_clarification": "давление обстоятельств"
                            },
                            {
                                "target_lang_word": "необходимость",
                                "target_lang_sense_clarification": "потребность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
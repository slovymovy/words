                {
                    "sense_id": "503b4ad6-e42e-3c10-a1dc-189123f8513d",
                    "sense_definition": "Форма глагола <w>навестить</w> (повелительное наклонение, единственное число).",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, <w>навести</w> бабушку в больнице.",
                            "target_lang_translations": {
                                "en": "Please <w>visit</w> grandma in the hospital."
                            }
                        },
                        {
                            "text": "<w>Навести</w> нас, когда будешь в городе.",
                            "target_lang_translations": {
                                "en": "<w>Visit</w> us when you are in town."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative of навестить"
                        }
                    ],
                    "target_lang_definitions": {

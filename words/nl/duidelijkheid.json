    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "afc1a134-1f78-3e5a-9d9a-3940f3727dfe",
                    "sense_definition": "De toestand waarin iets zeker, evident of vanzelfsprekend is, zodat er geen twijfel meer over bestaat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De werknemers willen graag <w>duidelijkheid</w> over hun nieuwe contracten.",
                            "target_lang_translations": {
                                "ru": "Сотрудники хотят <w>ясности</w> по поводу своих новых контрактов."
                            }
                        },
                        {
                            "text": "De minister kon nog geen <w>duidelijkheid</w> verschaffen over de nieuwe maatregelen.",
                            "target_lang_translations": {
                                "ru": "Министр пока не смог внести <w>ясность</w> по поводу новых мер."
                            }
                        }
                    ],
                    "synonyms": [
                        "evidentie",
                        "klaarblijkelijkheid"
                    ],
                    "antonyms": [
                        "onduidelijkheid"
                    ],
                    "common_phrases": [
                        "duidelijkheid scheppen",
                        "duidelijkheid verschaffen",
                        "duidelijkheid krijgen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором что-либо является определенным, очевидным или само собой разумеющимся, так что не остается никаких сомнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определённость",
                                "target_lang_sense_clarification": "отсутствие сомнений"
                            },
                            {
                                "target_lang_word": "ясность",
                                "target_lang_sense_clarification": "понимание ситуации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be3a48a9-36c9-397a-b3e2-7e8fc837f97a",
                    "sense_definition": "De eigenschap dat iets goed waarneembaar, begrijpelijk of helder is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "comprehensibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor de <w>duidelijkheid</w> zal ik het antwoord nog een keer herhalen.",
                            "target_lang_translations": {
                                "ru": "Для <w>ясности</w> я повторю ответ еще раз."
                            }
                        },
                        {
                            "text": "De leraar werd bedankt voor de <w>duidelijkheid</w> van zijn uitleg.",
                            "target_lang_translations": {
                                "ru": "Учителя поблагодарили за <w>ясность</w> его объяснений."
                            }
                        }
                    ],
                    "synonyms": [
                        "helderheid",
                        "begrijpelijkheid",
                        "transparantie",
                        "klaarheid",
                        "toegankelijkheid"
                    ],
                    "antonyms": [
                        "onduidelijkheid"
                    ],
                    "common_phrases": [
                        "voor de duidelijkheid",
                        "gebrek aan duidelijkheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство быть хорошо воспринимаемым, понятным или ясным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ясность",

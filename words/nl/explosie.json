{
    "word_family": [
        "exploderen",
        "explosief",
        "explosiviteit"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "98bd278a-76e8-3929-9154-f23fe2a7f0cb",
                    "sense_definition": "Een plotselinge, hevige uitbarsting van energie, vaak gepaard met een harde knal en vuur.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>explosie</w> veroorzaakte veel schade aan de omliggende huizen.",
                            "target_lang_translations": {
                                "ru": "<w>Взрыв</w> причинил значительный ущерб близлежащим домам."
                            }
                        },
                        {
                            "text": "Door een gaslek vond er een zware <w>explosie</w> plaats in het gebouw.",
                            "target_lang_translations": {
                                "ru": "Из-за утечки газа в здании произошел сильный <w>взрыв</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontploffing",
                        "detonatie"
                    ],
                    "antonyms": [
                        "implosie"
                    ],
                    "common_phrases": [
                        "zware explosie",
                        "luide explosie",
                        "explosie veroorzaken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапный, сильный выброс энергии, часто сопровождающийся громким звуком и огнем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрыв",
                                "target_lang_sense_clarification": "физическое явление"
                            },
                            {
                                "target_lang_word": "детонация",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c03e8c41-2985-35c1-9c76-bc1907f8f1ee",
                    "sense_definition": "Een plotselinge en hevige uiting van sterke emoties, zoals woede of vreugde.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn ontslag leidde tot een <w>explosie</w> van woede bij de werknemers.",
                            "target_lang_translations": {
                                "ru": "Его увольнение вызвало <w>взрыв</w> гнева у сотрудников."
                            }
                        },
                        {
                            "text": "Na het winnende doelpunt volgde een <w>explosie</w> van vreugde op de tribune.",
                            "target_lang_translations": {
                                "ru": "После победного гола на трибунах последовал <w>взрыв</w> радости."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitbarsting"
                    ],
                    "common_phrases": [
                        "explosie van woede",
                        "explosie van vreugde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное и бурное проявление сильных эмоций, таких как гнев или радость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрыв",
                                "target_lang_sense_clarification": "бурное проявление чувств"
                            },
                            {
                                "target_lang_word": "вспышка",
                                "target_lang_sense_clarification": "резкое проявление эмоций"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
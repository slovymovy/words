                    }
                },
                {
                    "sense_id": "4c6fae45-4709-3494-a5ae-c24e9f8619f8",
                    "sense_definition": "postać z mitologii greckiej, córka Zeusa i Ledy, uznawana za najpiękniejszą kobietę świata, której uprowadzenie stało się przyczyną wojny trojańskiej",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "name_myth",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Porwanie <w>Heleny</w> stało się bezpośrednią przyczyną wybuchu wojny trojańskiej.",
                            "target_lang_translations": {
                                "ru": "Похищение <w>Елены</w> стало непосредственной причиной начала Троянской войны."
                            }
                        },
                        {
                            "text": "Mityczna <w>Helena</w> była żoną króla Sparty, Menelaosa.",
                            "target_lang_translations": {
                                "ru": "Мифическая <w>Елена</w> была женой царя Спарты Менелая."
                            }
                        }
                    ],
                    "common_phrases": [
                        "piękna Helena",
                        "porwanie Heleny"
                    ],
                    "target_lang_definitions": {
                        "ru": "персонаж древнегреческой мифологии, дочь Зевса и Леды, признанная самой красивой женщиной в мире, похищение которой стало причиной Троянской войны"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Елена",
                                "target_lang_sense_clarification": "мифологический персонаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d26c695d-3182-3981-9aa6-02afc5765bce",
                    "sense_definition": "jeden z naturalnych satelitów Saturna",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_astro",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Astronomowie obserwują orbitę <w>Heleny</w> wokół planety Saturn.",
                            "target_lang_translations": {
                                "ru": "Астрономы наблюдают за орбитой <w>Елены</w> вокруг планеты Сатурн."
                            }
                        },
                        {
                            "text": "Mała <w>Helena</w> została odkryta w 1980 roku przez francuskich astronomów.",
                            "target_lang_translations": {
                                "ru": "Малая <w>Елена</w> была открыта в 1980 году французскими астрономами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "księżyc Helena"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6f4fee78-dc12-317a-b98b-fb0fd45d61a0",
                    "sense_definition": "A politician, especially one who is active and experienced in party politics.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "political_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The veteran <w>politico</w> knew exactly how to negotiate the new treaty.",
                            "target_lang_translations": {
                                "ru": "Опытный <w>политик</w> точно знал, как вести переговоры о новом договоре."
                            }
                        },
                        {
                            "text": "Local <w>politicos</w> gathered at the town hall to discuss the budget cuts.",
                            "target_lang_translations": {
                                "ru": "Местные <w>политиканы</w> собрались в мэрии, чтобы обсудить сокращение бюджета."
                            }
                        }
                    ],
                    "synonyms": [
                        "politician"
                    ],
                    "common_phrases": [
                        "veteran politico",
                        "seasoned politico",
                        "local politico"
                    ],
                    "target_lang_definitions": {
                        "ru": "Политик, особенно тот, кто активен и опытен в партийной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "политик",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "политикан",
                                "target_lang_sense_clarification": "разговорное, часто с негативным оттенком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3e5277b-2a4b-331e-abc7-0ce9da8d9518",
                    "sense_definition": "Someone involved in the professional life of a politician or a political campaign, such as a strategist or advisor.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "political_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The campaign hired a savvy <w>politico</w> to manage their public image.",
                            "target_lang_translations": {
                                "ru": "Избирательный штаб нанял смекалистого <w>политтехнолога</w> для управления общественным имиджем."
                            }
                        },
                        {
                            "text": "As a career <w>politico</w>, she understood the complexities of lobbying better than anyone.",
                            "target_lang_translations": {
                                "ru": "Будучи профессиональным <w>политическим консультантом</w>, она понимала сложности лоббирования лучше, чем кто-либо другой."
                            }
                        }
                    ],
                    "synonyms": [
                        "strategist",
                        "operative"
                    ],
                    "common_phrases": [
                        "campaign politico",
                        "career politico"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, профессионально занимающийся политической жизнью или кампаниями, например, стратег или советник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "политтехнолог",
                                "target_lang_sense_clarification": "специалист по выборам и имиджу"
                            },
                            {
                                "target_lang_word": "политический консультант",
                                "target_lang_sense_clarification": "советник"
                            }
                        ]
                    }
                }
            ]
        }
    ]

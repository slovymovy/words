    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "66c6d56b-7aeb-3e51-beef-2362bb27d835",
                    "sense_definition": "Руководство, описывающее способ приготовления кулинарного блюда или напитка, включая список ингредиентов.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "instruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я нашёл в интернете простой <w>рецепт</w> вкусного яблочного пирога.",
                            "target_lang_translations": {
                                "en": "I found a simple <w>recipe</w> for a delicious apple pie on the internet."
                            }
                        },
                        {
                            "text": "Моя бабушка всегда готовит борщ по своему секретному <w>рецепту</w>.",
                            "target_lang_translations": {
                                "en": "My grandmother always cooks borscht according to her secret <w>recipe</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "книга рецептов",
                        "старинный рецепт",
                        "поделиться рецептом",
                        "следовать рецепту"
                    ],
                    "target_lang_definitions": {
                        "en": "A set of instructions for preparing a culinary dish or drink, including a list of ingredients."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recipe",
                                "target_lang_sense_clarification": "culinary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "758e486c-3c66-33de-8676-b484dafdc794",
                    "sense_definition": "Письменное распоряжение врача аптеке об изготовлении и выдаче лекарства больному.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач выписал мне <w>рецепт</w> на сильное обезболивающее средство.",
                            "target_lang_translations": {
                                "en": "The doctor wrote me a <w>prescription</w> for a strong painkiller."
                            }
                        },
                        {
                            "text": "Это лекарство продаётся в аптеке только по <w>рецепту</w>.",
                            "target_lang_translations": {
                                "en": "This medicine is sold in the pharmacy only by <w>prescription</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "назначение"
                    ],
                    "common_phrases": [
                        "выписать рецепт",
                        "получить рецепт",
                        "лекарство по рецепту",
                        "рецепт врача"
                    ],
                    "target_lang_definitions": {
                        "en": "A written order from a doctor to a pharmacy for the preparation and dispensing of medicine to a patient."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prescription",
                                "target_lang_sense_clarification": "medical"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5094980-a9f1-36c6-a675-90786695b306",
                    "sense_definition": "Совет, рекомендация или способ действия для достижения желаемого результата или решения проблемы.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "instruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У каждого человека есть свой собственный <w>рецепт</w> счастья и успеха.",
                            "target_lang_translations": {
                                "en": "Every person has their own <w>recipe</w> for happiness and success."
                            }
                        },
                        {
                            "text": "Не существует универсального <w>рецепта</w> для решения всех жизненных трудностей.",
                            "target_lang_translations": {
                                "en": "There is no universal <w>recipe</w> for solving all of life's difficulties."
                            }
                        }
                    ],
                    "synonyms": [
                        "способ",
                        "метод",
                        "секрет"
                    ],
                    "common_phrases": [
                        "рецепт успеха",
                        "рецепт молодости",
                        "готовый рецепт",
                        "универсальный рецепт"
                    ],
                    "target_lang_definitions": {
                        "en": "Advice, recommendation, or a method of action for achieving a desired result or solving a problem."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recipe",
                                "target_lang_sense_clarification": "method for achieving a result"
                            },
                            {
                                "target_lang_word": "formula",
                                "target_lang_sense_clarification": "figurative"
                            }
                        ]
                    }
                }
            ]
        }
    ]

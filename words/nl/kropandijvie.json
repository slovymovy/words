    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5e184f15-a18b-37b9-a70d-0e7ad5c1befd",
                    "sense_definition": "De brede bladeren van een specifieke andijviesoort die als groente of in salades worden gegeten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor dit recept heb je een verse struik <w>kropandijvie</w> nodig.",
                            "target_lang_translations": {
                                "ru": "Для этого рецепта вам понадобится свежий кочан <w>эндивия</w>."
                            }
                        },
                        {
                            "text": "We eten vanavond stamppot met rauwe <w>kropandijvie</w> en spekjes.",
                            "target_lang_translations": {
                                "ru": "Сегодня вечером мы едим стамппот с сырым <w>эндивием</w> и кусочками бекона."
                            }
                        }
                    ],
                    "synonyms": [
                        "breedbladige andijvie"
                    ],
                    "common_phrases": [
                        "rauwe kropandijvie",
                        "kropandijvie met spekjes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Широкие листья определенного вида эндивия, которые употребляют в пищу как овощ или в салатах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эндивий",
                                "target_lang_sense_clarification": "листовой овощ"
                            },
                            {
                                "target_lang_word": "эскариол",
                                "target_lang_sense_clarification": "сорт с широкими листьями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da42f3d3-91c2-333c-b44b-fa4e8f430366",
                    "sense_definition": "Een eenjarige plant met brede bladeren, een variëteit van de andijvieplant.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer oogst de <w>kropandijvie</w> meestal in de late herfst.",
                            "target_lang_translations": {
                                "ru": "Фермер обычно собирает урожай <w>эндивия</w> поздней осенью."
                            }
                        },
                        {
                            "text": "Deze soort <w>kropandijvie</w> is minder gevoelig voor vorst dan andere soorten.",
                            "target_lang_translations": {
                                "ru": "Этот сорт <w>эндивия</w> менее чувствителен к морозу, чем другие виды."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Однолетнее растение с широкими листьями, разновидность эндивия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эндивий",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

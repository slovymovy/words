{
    "word_family": [
        "mentee",
        "mentorship"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c28a9890-5611-34df-a1b2-28969a2c5b61",
                    "sense_definition": "A wise and trusted counselor or teacher.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She considers her former boss to be her most important <w>mentor</w>.",
                            "target_lang_translations": {
                                "ru": "Она считает своего бывшего начальника своим самым важным <w>наставником</w>."
                            }
                        },
                        {
                            "text": "A good <w>mentor</w> can help you navigate your career path.",
                            "target_lang_translations": {
                                "ru": "Хороший <w>наставник</w> может помочь вам сориентироваться в карьерном пути."
                            }
                        }
                    ],
                    "synonyms": [
                        "adviser",
                        "guide",
                        "coach",
                        "tutor",
                        "counselor"
                    ],
                    "antonyms": [
                        "mentee",
                        "protégé"
                    ],
                    "common_phrases": [
                        "find a mentor",
                        "act as a mentor",
                        "career mentor",
                        "spiritual mentor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мудрый и доверенный советник или учитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наставник",
                                "target_lang_sense_clarification": "опытный руководитель, учитель или воспитатель"
                            },
                            {
                                "target_lang_word": "ментор",
                                "target_lang_sense_clarification": "куратор, советник (часто в бизнесе или стартапах)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6dff8143-6bbf-342b-9fee-f306af98e61a",
                    "sense_definition": "To act as a wise and trusted counselor or teacher to someone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He volunteered to <w>mentor</w> young students interested in science.",
                            "target_lang_translations": {
                                "ru": "Он вызвался <w>быть наставником</w> для молодых студентов, интересующихся наукой."
                            }
                        },
                        {
                            "text": "She was <w>mentored</w> by a leading expert in the field.",
                            "target_lang_translations": {
                                "ru": "Ее <w>курировал</w> ведущий эксперт в этой области."
                            }
                        }
                    ],
                    "synonyms": [
                        "guide",
                        "coach",
                        "advise",
                        "train"
                    ],
                    "common_phrases": [
                        "mentor a student",
                        "mentor someone",
                        "be mentored by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступать в роли мудрого и доверенного советника или учителя для кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть наставником",
                                "target_lang_sense_clarification": "выполнять роль учителя или советника"
                            },
                            {
                                "target_lang_word": "курировать",
                                "target_lang_sense_clarification": "опекать, руководить обучением или работой"
                            },
                            {
                                "target_lang_word": "наставлять",
                                "target_lang_sense_clarification": "поучать, давать наставления (более книжное)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "dd76e81b-4f42-35ad-ac05-820d8c7c202c",
                    "sense_definition": "Odysseus's trusted counselor who raised his son Telemachus.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Athena often took the form of <w>Mentor</w> to give advice.",
                            "target_lang_translations": {
                                "ru": "Афина часто принимала облик <w>Ментора</w>, чтобы дать совет."
                            }
                        },
                        {
                            "text": "In the Odyssey, <w>Mentor</w> is a loyal friend to the hero.",
                            "target_lang_translations": {
                                "ru": "В «Одиссее» <w>Ментор</w> — верный друг героя."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Доверенный советник Одиссея, воспитавший его сына Телемаха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ментор",
                                "target_lang_sense_clarification": "персонаж древнегреческой мифологии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d74a1ae-2aa1-3ff9-9868-bcd04e5b7182",
                    "sense_definition": "A male given name from Ancient Greek.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "He was named <w>Mentor</w> after the famous mythological figure.",
                            "target_lang_translations": {
                                "ru": "Его назвали <w>Ментором</w> в честь известного мифологического персонажа."
                            }
                        },
                        {
                            "text": "I have a friend from Greece whose name is <w>Mentor</w>.",
                            "target_lang_translations": {
                                "ru": "У меня есть друг из Греции, которого зовут <w>Ментор</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя древнегреческого происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ментор",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "81086760-70da-3036-88b8-dcc023f7314b",
                    "sense_definition": "Een bepaalde mening hebben of geloven dat iets waar is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>denk</w> dat het morgen gaat regenen.",
                            "target_lang_translations": {
                                "en": "I <w>think</w> it is going to rain tomorrow."
                            }
                        },
                        {
                            "text": "Wat <w>denk</w> jij van dit nieuwe plan?",
                            "target_lang_translations": {
                                "en": "What do you <w>think</w> of this new plan?"
                            }
                        }
                    ],
                    "synonyms": [
                        "geloven",
                        "menen"
                    ],
                    "common_phrases": [
                        "ik denk van wel",
                        "ik denk van niet",
                        "niet te geloven"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a specific opinion or believe that something is true."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "think",
                                "target_lang_sense_clarification": "to have an opinion or belief"
                            },
                            {
                                "target_lang_word": "believe",
                                "target_lang_sense_clarification": "to hold as true"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "553beb15-ecb6-3c6e-86cd-f01e470c1465",
                    "sense_definition": "Iemand of iets in gedachten hebben (meestal gebruikt met 'aan').",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "focus",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>denk</w> vaak aan mijn vriendin.",
                            "target_lang_translations": {
                                "en": "I often <w>think</w> of my girlfriend."
                            }
                        },
                        {
                            "text": "Tijdens de vakantie moet ik niet aan mijn werk <w>denken</w>.",
                            "target_lang_translations": {
                                "en": "During the holiday I shouldn't <w>think</w> about my work."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan iemand denken",
                        "nergens aan denken"
                    ],
                    "target_lang_definitions": {
                        "en": "To have someone or something in one's thoughts (usually used with 'aan')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "think of",
                                "target_lang_sense_clarification": "to direct thoughts towards"
                            },
                            {
                                "target_lang_word": "think about",
                                "target_lang_sense_clarification": "to contemplate"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f266c87b-e5e9-328f-97d6-c4e6803eaa6d",
                    "sense_definition": "De hersenen gebruiken om iets te begrijpen, op te lossen of te overwegen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet eerst goed <w>denken</w> voordat je antwoord geeft.",
                            "target_lang_translations": {
                                "en": "You must <w>think</w> carefully before you answer."
                            }
                        },
                        {
                            "text": "Na lang <w>denken</w> wist hij eindelijk de oplossing.",
                            "target_lang_translations": {
                                "en": "After <w>thinking</w> for a long time, he finally knew the solution."
                            }
                        }
                    ],
                    "synonyms": [
                        "peinzen"
                    ],
                    "common_phrases": [
                        "logisch denken",
                        "hardop denken",
                        "eerst denken dan doen"
                    ],
                    "target_lang_definitions": {
                        "en": "To use the brain to understand, solve, or consider something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "think",
                                "target_lang_sense_clarification": "mental process"
                            },
                            {
                                "target_lang_word": "ponder",
                                "target_lang_sense_clarification": "to think deeply"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4f8051c-1653-3f13-970b-b22921cbb23d",
                    "sense_definition": "Niet vergeten om iets te doen (meestal gebruikt met 'aan').",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "memory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Denk</w> eraan dat je morgen de vuilnisbak buiten zet.",
                            "target_lang_translations": {
                                "en": "<w>Remember</w> to put the trash can out tomorrow."
                            }
                        },
                        {
                            "text": "Heb je wel aan de boodschappen <w>gedacht</w>?",
                            "target_lang_translations": {
                                "en": "Did you <w>remember</w> the groceries?"
                            }
                        }
                    ],
                    "synonyms": [
                        "herinneren"
                    ],
                    "common_phrases": [
                        "denk eraan dat",
                        "overal aan denken"
                    ],
                    "target_lang_definitions": {
                        "en": "To not forget to do something (usually used with 'aan')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remember",
                                "target_lang_sense_clarification": "not to forget"
                            },
                            {
                                "target_lang_word": "keep in mind",
                                "target_lang_sense_clarification": "to retain in memory"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b88f30f-5e92-382c-8320-d86ac567b35f",
                    "sense_definition": "Het plan hebben om iets te doen en dit overwegen (meestal gebruikt met 'over').",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "consideration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>denkt</w> erover om naar het buitenland te emigreren.",
                            "target_lang_translations": {
                                "en": "He is <w>thinking</w> about emigrating abroad."
                            }
                        },
                        {
                            "text": "Wij <w>denken</w> erover een nieuwe auto te kopen.",
                            "target_lang_translations": {
                                "en": "We are <w>considering</w> buying a new car."
                            }
                        }
                    ],
                    "synonyms": [
                        "overwegen"
                    ],
                    "common_phrases": [
                        "erover denken om",
                        "er anders over denken"
                    ],
                    "target_lang_definitions": {
                        "en": "To have the plan to do something and consider it (usually used with 'over')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "consider",
                                "target_lang_sense_clarification": "to think about doing"
                            },
                            {
                                "target_lang_word": "think about",
                                "target_lang_sense_clarification": "to contemplate a plan"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c62ddd04-fd61-3197-8043-a81dc9a9221a",
                    "sense_definition": "Rekening houden met of voorzichtig zijn met (meestal gebruikt met 'om').",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moeder is ziek, dus <w>denk</w> erom stil te zijn.",
                            "target_lang_translations": {
                                "en": "Your mother is sick, so <w>remember</w> to be quiet."
                            }
                        },
                        {
                            "text": "<w>Denk</w> om het afstapje als je naar buiten loopt.",
                            "target_lang_translations": {
                                "en": "<w>Mind</w> the step when you walk outside."
                            }
                        }
                    ],
                    "common_phrases": [
                        "denk om jezelf",
                        "denk om de buren"
                    ],
                    "target_lang_definitions": {
                        "en": "To take into account or be careful with (usually used with 'om')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mind",
                                "target_lang_sense_clarification": "to be careful of"
                            },
                            {
                                "target_lang_word": "watch out for",
                                "target_lang_sense_clarification": "to be cautious"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
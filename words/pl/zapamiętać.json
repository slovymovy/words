            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2f5a93e7-8545-340a-a0ce-f6e48c774a99",
                    "sense_definition": "utrwalić coś w pamięci; sprawić, że informacja pozostanie w umyśle, aby móc ją później odtworzyć",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "memory_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <zapamiętać> ten numer telefonu, bo jest bardzo ważny.",
                            "target_lang_translations": {
                                "ru": "Ты должен <запомнить> этот номер телефона, потому что он очень важен."
                            }
                        },
                        {
                            "text": "Mam nadzieję, że <zapamiętasz> tę lekcję na zawsze.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, ты <запомнишь> этот урок навсегда."
                            }
                        },
                        {
                            "text": "Uczniowie musieli <zapamiętać> daty wszystkich bitew.",
                            "target_lang_translations": {
                                "ru": "Ученики должны были <запомнить> даты всех битв."
                            }
                        }
                    ],
                    "synonyms": [
                        "wściec się"
                    ],
                    "antonyms": [
                        "zapomnieć"
                    ],
                    "common_phrases": [
                        "zapamiętać na zawsze",
                        "dobrze zapamiętać",
                        "muszę to zapamiętać",
                        "łatwo zapamiętać"
                    ],
                    "target_lang_definitions": {
                        "ru": "зафиксировать что-либо в памяти; сделать так, чтобы информация осталась в уме для последующего воспроизведения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запомнить",
                                "target_lang_sense_clarification": "сохранить в памяти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "116a644f-d92f-3e1c-a929-4ee48eed054f",
                    "sense_definition": "stracić panowanie nad sobą lub świadomość otoczenia pod wpływem silnych emocji albo intensywnego zajęcia (używane z zaimkiem zwrotnym „się”)",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "loss_of_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W kłótni tak się <zapamiętał>, że zaczął krzyczeć na wszystkich.",
                            "target_lang_translations": {
                                "ru": "В ссоре он так <забылся>, что начал кричать на всех."
                            }
                        },
                        {
                            "text": "Artysta <zapamiętał> się w pracy nad nowym obrazem i zapomniał o jedzeniu.",
                            "target_lang_translations": {
                                "ru": "Художник <забылся> за работой над новой картиной и забыл о еде."
                            }
                        }
                    ],
                    "synonyms": [
                        "wściec się"
                    ],
                    "common_phrases": [
                        "zapamiętać się w gniewie",
                        "zapamiętać się w bólu"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym „się”"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literackie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "потерять самообладание или осознание окружающего под влиянием сильных эмоций или интенсивного занятия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забыться",
                                "target_lang_sense_clarification": "потерять контроль над собой"
                            },
                            {
                                "target_lang_word": "не помнить себя",
                                "target_lang_sense_clarification": "действовать бессознательно от эмоций"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2adc0c53-e306-3b42-9b1e-6b226e058042",
                    "sense_definition": "wpaść w szał lub wielką złość, często o zwierzętach (używane z zaimkiem zwrotnym „się”)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "loss_of_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pies <zapamiętał> się w ataku i nie reagował na komendy właściciela.",
                            "target_lang_translations": {
                                "ru": "Пес <остервенел> в атаке и не реагировал на команды владельца."
                            }
                        },
                        {
                            "text": "Rozjuszony dzik <zapamiętał> się i szarżował na myśliwych.",
                            "target_lang_translations": {
                                "ru": "Разъяренный кабан <остервенел> и бросился на охотников."
                            }
                        }
                    ],
                    "synonyms": [
                        "wściec się"
                    ],
                    "traits": [

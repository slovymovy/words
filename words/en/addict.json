{
    "word_family": [
        "addiction",
        "addictive",
        "addicted"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3f8a683-06c1-3ee0-be77-fe1d32b8962e",
                    "sense_definition": "A person who is physically or mentally dependent on a harmful drug or substance.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "substance_dependence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The recovering <w>addict</w> has been clean for over five years.",
                            "target_lang_translations": {
                                "ru": "Выздоравливающий <w>наркоман</w> не употребляет уже более пяти лет."
                            }
                        },
                        {
                            "text": "He admitted he was a gambling <w>addict</w> and sought professional help.",
                            "target_lang_translations": {
                                "ru": "Он признал, что он <w>игроман</w>, и обратился за профессиональной помощью."
                            }
                        }
                    ],
                    "synonyms": [
                        "junkie",
                        "abuser",
                        "fiend"
                    ],
                    "antonyms": [
                        "nonaddict",
                        "abstainer"
                    ],
                    "common_phrases": [
                        "drug addict",
                        "recovering addict",
                        "gambling addict"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который физически или психологически зависит от вредного наркотика или вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наркоман",
                                "target_lang_sense_clarification": "человек, зависимый от наркотиков"
                            },
                            {
                                "target_lang_word": "зависимый",
                                "target_lang_sense_clarification": "более общий или медицинский термин"
                            },
                            {
                                "target_lang_word": "игроман",
                                "target_lang_sense_clarification": "зависимый от азартных игр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "510fa9f7-877a-31de-9fd0-13d5b578cefa",
                    "sense_definition": "An enthusiastic devotee or fan of a specific interest or activity.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "enthusiasm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother is a total video game <w>addict</w> and plays every weekend.",
                            "target_lang_translations": {
                                "ru": "Мой брат — полный <w>фанат</w> видеоигр и играет каждые выходные."
                            }
                        },
                        {
                            "text": "As a chocolate <w>addict</w>, she cannot resist buying a bar at the store.",
                            "target_lang_translations": {
                                "ru": "Как <w>любительница</w> шоколада, она не может удержаться от покупки плитки в магазине."
                            }
                        }
                    ],
                    "synonyms": [
                        "enthusiast",
                        "fan",
                        "devotee",
                        "aficionado",
                        "junkie"
                    ],
                    "common_phrases": [
                        "chocolate addict",
                        "news addict",
                        "tv addict"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Used to describe enthusiasm rather than clinical dependence."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Страстный поклонник или фанат определенного интереса или занятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фанат",
                                "target_lang_sense_clarification": "разговорное, увлеченный человек"
                            },
                            {
                                "target_lang_word": "любитель",
                                "target_lang_sense_clarification": "человек, имеющий пристрастие к чему-либо"
                            },
                            {
                                "target_lang_word": "приверженец",
                                "target_lang_sense_clarification": "последователь идеи или учения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "01e2e569-6e1e-367b-9464-6131c97b5f41",
                    "sense_definition": "To cause someone to become dependent on a substance or habit.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cause_dependence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company designed the app to <w>addict</w> users to its reward system.",
                            "target_lang_translations": {
                                "ru": "Компания разработала приложение, чтобы <w>подсадить</w> пользователей на свою систему вознаграждений."
                            }
                        },
                        {
                            "text": "Certain painkillers can <w>addict</w> patients if they are not monitored carefully.",
                            "target_lang_translations": {
                                "ru": "Определенные обезболивающие могут <w>вызвать зависимость</w> у пациентов, если их не контролировать тщательно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "addict someone to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-либо зависимость от вещества или привычки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать зависимость",
                                "target_lang_sense_clarification": "провоцировать привыкание"
                            },
                            {
                                "target_lang_word": "пристрастить",
                                "target_lang_sense_clarification": "заставить привыкнуть к чему-либо"
                            },
                            {
                                "target_lang_word": "подсадить",
                                "target_lang_sense_clarification": "слэнг, сделать зависимым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbae373c-776f-353d-b4d2-6ace42e7f961",
                    "sense_definition": "To devote oneself entirely to a specific activity or cause.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "devotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to <w>addict</w> himself to the study of ancient history.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>посвятить</w> себя изучению древней истории."
                            }
                        },
                        {
                            "text": "They <w>addict</w> themselves to their work with great passion and discipline.",
                            "target_lang_translations": {
                                "ru": "Они <w>отдаются</w> своей работе с большой страстью и дисциплиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "devote",
                        "dedicate",
                        "consecrate"
                    ],
                    "common_phrases": [
                        "addict oneself to"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern usage prefers 'devote' or 'dedicate'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью посвятить себя определенному занятию или делу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посвящать",
                                "target_lang_sense_clarification": "отдавать себя делу"
                            },
                            {
                                "target_lang_word": "предаваться",
                                "target_lang_sense_clarification": "устаревшее или книжное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d05dfa41-1543-3a98-9da0-ebdbfc21eea0",
                    "sense_definition": "Przedstawić dowody potwierdzające, że coś jest prawdą lub że fakt miał miejsce.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "logic_proof",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Najnowsze badania <wykazały>, że ten lek jest bardzo skuteczny.",
                            "target_lang_translations": {
                                "ru": "Последние исследования <показали>, что это лекарство очень эффективно."
                            }
                        },
                        {
                            "text": "Musisz <wykazać>, że twoje obliczenia nie zawierają błędów.",
                            "target_lang_translations": {
                                "ru": "Ты должен <доказать>, что твои расчеты не содержат ошибок."
                            }
                        }
                    ],
                    "synonyms": [
                        "udowodnić",
                        "dowieść",
                        "uzasadnić"
                    ],
                    "antonyms": [
                        "obalić"
                    ],
                    "common_phrases": [
                        "wykazać błąd",
                        "wykazać słuszność",
                        "badania wykazały",
                        "wykazać związek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представить доказательства, подтверждающие истинность чего-либо или факт события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доказать",
                                "target_lang_sense_clarification": "подтвердить истинность"
                            },
                            {
                                "target_lang_word": "показать",
                                "target_lang_sense_clarification": "о результатах исследований, анализов"
                            },
                            {
                                "target_lang_word": "выявить",
                                "target_lang_sense_clarification": "обнаружить, установить наличие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "524b8847-75f5-3e75-af27-86b9906a0a2f",
                    "sense_definition": "Uzewnętrznić jakąś cechę, uczucie lub postawę; dać czemuś wyraz.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "expression_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel <wykazał> dużo cierpliwości, tłumacząc trudne zadanie.",
                            "target_lang_translations": {
                                "ru": "Учитель <проявил> много терпения, объясняя трудную задачу."
                            }
                        },
                        {
                            "text": "Firma nie <wykazała> zainteresowania naszą ofertą współpracy.",
                            "target_lang_translations": {
                                "ru": "Компания не <проявила> интереса к нашему предложению о сотрудничестве."
                            }
                        }
                    ],
                    "synonyms": [
                        "okazać",
                        "przejawić",
                        "zaprezentować"
                    ],
                    "antonyms": [
                        "ukryć",
                        "zataić"
                    ],
                    "common_phrases": [
                        "wykazać zainteresowanie",
                        "wykazać zrozumienie",
                        "wykazać cierpliwość",
                        "wykazać chęci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявить какое-либо качество, чувство или отношение; выразить что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проявить",
                                "target_lang_sense_clarification": "обнаружить качество или чувство"
                            },
                            {
                                "target_lang_word": "выказать",
                                "target_lang_sense_clarification": "выразить отношение (книжн.)"
                            },
                            {
                                "target_lang_word": "продемонстрировать",
                                "target_lang_sense_clarification": "наглядно показать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03539dd1-d147-3091-9df9-bbfd4f694746",
                    "sense_definition": "Udowodnić posiadanie pewnych cech, umiejętności lub wiedzy poprzez swoje działanie (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "expression_attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej trudnej sytuacji strażak <wykazał się> niezwykłą odwagą.",
                            "target_lang_translations": {
                                "ru": "В этой сложной ситуации пожарный <проявил> необычайную отвагу."
                            }
                        },
                        {
                            "text": "Kandydat do pracy musi <wykazać się> biegłą znajomością języka angielskiego.",
                            "target_lang_translations": {
                                "ru": "Кандидат на работу должен <продемонстрировать> свободное владение английским языком."
                            }
                        }
                    ],
                    "synonyms": [
                        "popisać się",
                        "sprawdzić się"
                    ],
                    "common_phrases": [
                        "wykazać się wiedzą",
                        "wykazać się odwagą",
                        "wykazać się inicjatywą",
                        "wykazać się sprytem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продемонстрировать наличие определенных качеств, навыков или знаний на деле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проявить",
                                "target_lang_sense_clarification": "показать наличие качества (смелость, инициативу)"
                            },
                            {
                                "target_lang_word": "продемонстрировать",
                                "target_lang_sense_clarification": "показать умения или знания"
                            },
                            {
                                "target_lang_word": "отличиться",
                                "target_lang_sense_clarification": "выделиться каким-либо качеством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd778059-8b8e-38a1-8d24-6649c735a520",
                    "sense_definition": "Podać informacje do oficjalnej wiadomości, ująć coś w spisie, raporcie lub dokumencie finansowym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "administrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W rocznym zeznaniu podatkowym należy <wykazać> wszystkie dodatkowe dochody.",
                            "target_lang_translations": {
                                "ru": "В годовой налоговой декларации следует <указать> все дополнительные доходы."
                            }

{
    "word_family": [
        "балдеть",
        "балда",
        "обалделый",
        "обалдение",
        "прибалдеть",
        "забалдеть"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "55215a38-3033-36a6-adb6-422a91a50431",
                    "sense_definition": "Сильно удивиться, испытать крайнюю степень изумления или потрясения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "surprise_shock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты <обалдеешь>, когда узнаешь последние новости.",
                            "target_lang_translations": {
                                "en": "You will <be blown away> when you find out the latest news."
                            }
                        },
                        {
                            "text": "Мы все просто <обалдели> от его неожиданного поступка.",
                            "target_lang_translations": {
                                "en": "We were all just <stunned> by his unexpected action."
                            }
                        }
                    ],
                    "synonyms": [
                        "изумиться",
                        "поразиться",
                        "ошалеть",
                        "удивиться"
                    ],
                    "common_phrases": [
                        "можно обалдеть",
                        "просто обалдеть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be extremely surprised, shocked, or amazed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be stunned",
                                "target_lang_sense_clarification": "to be extremely surprised or shocked"
                            },
                            {
                                "target_lang_word": "be blown away",
                                "target_lang_sense_clarification": "colloquial, to be very impressed or surprised"
                            },
                            {
                                "target_lang_word": "freak out",
                                "target_lang_sense_clarification": "slang, to react strongly with surprise or emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "878f172f-11c0-33d3-88e5-b10f103d99d0",
                    "sense_definition": "Потерять способность ясно соображать, одуреть от усталости, шума или скуки.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я совсем <обалдел> от этой невыносимой жары.",
                            "target_lang_translations": {
                                "en": "I've completely <lost my mind> from this unbearable heat."
                            }
                        },
                        {
                            "text": "К концу рабочего дня можно <обалдеть> от количества звонков.",
                            "target_lang_translations": {
                                "en": "By the end of the workday, you can <go crazy> from the number of calls."
                            }
                        }
                    ],
                    "synonyms": [
                        "одуреть",
                        "отупеть",
                        "ошалеть"
                    ],
                    "common_phrases": [
                        "обалдеть от скуки",
                        "совсем обалдеть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become dazed, stupefied, or lose the ability to think clearly due to fatigue, noise, or boredom."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go crazy",
                                "target_lang_sense_clarification": "to become mentally exhausted or annoyed"
                            },
                            {
                                "target_lang_word": "lose one's mind",
                                "target_lang_sense_clarification": "to become confused or irrational from stress/environment"
                            },
                            {
                                "target_lang_word": "be stupefied",
                                "target_lang_sense_clarification": "to become groggy or unable to think"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "0544c30b-cee4-34fa-aea6-36ee57e1261d",
                    "sense_definition": "Возглас, выражающий сильное удивление, восхищение или негодование.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "surprise_shock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Обалдеть>, какое красивое платье ты купила!",
                            "target_lang_translations": {
                                "en": "<Wow>, what a beautiful dress you bought!"
                            }
                        },
                        {
                            "text": "<Обалдеть>, я даже не знал, что так бывает.",
                            "target_lang_translations": {
                                "en": "<No way>, I didn't even know that happens."
                            }
                        }
                    ],
                    "synonyms": [
                        "ничего себе",
                        "вот это да",
                        "с ума сойти"
                    ],
                    "common_phrases": [
                        "просто обалдеть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "very common in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation expressing strong surprise, admiration, or shock."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wow",
                                "target_lang_sense_clarification": "general expression of amazement"
                            },
                            {
                                "target_lang_word": "no way",
                                "target_lang_sense_clarification": "expression of disbelief or surprise"
                            },
                            {
                                "target_lang_word": "that's crazy",
                                "target_lang_sense_clarification": "reaction to shocking or surprising information"
                            },
                            {
                                "target_lang_word": "holy cow",
                                "target_lang_sense_clarification": "informal exclamation of surprise"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
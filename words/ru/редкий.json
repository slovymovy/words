    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b770204a-a035-3973-8334-1bf7863760aa",
                    "sense_definition": "Происходящий или повторяющийся через большие промежутки времени; нечастый.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом году снег — <редкое> явление для нашего региона.",
                            "target_lang_translations": {
                                "en": "This year snow is a <rare> phenomenon for our region."
                            }
                        },
                        {
                            "text": "Мы ценим наши <редкие> встречи со школьными друзьями.",
                            "target_lang_translations": {
                                "en": "We value our <infrequent> meetings with school friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "нечастый"
                    ],
                    "antonyms": [
                        "частый"
                    ],
                    "common_phrases": [
                        "редкие встречи",
                        "редкий случай",
                        "редкий гость"
                    ],
                    "target_lang_definitions": {
                        "en": "Occurring or repeating at long intervals; infrequent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rare",
                                "target_lang_sense_clarification": "happening very seldom"
                            },
                            {
                                "target_lang_word": "infrequent",
                                "target_lang_sense_clarification": "not occurring often"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d934b1a-230b-30f7-8802-d0a44d0af858",
                    "sense_definition": "Встречающийся очень нечасто; уникальный, ценный или труднодоступный.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "uniqueness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В музее хранится <редкая> книга шестнадцатого века.",
                            "target_lang_translations": {
                                "en": "A <rare> sixteenth-century book is kept in the museum."
                            }
                        },
                        {
                            "text": "У этого музыканта <редкий> талант чувствовать ритм.",
                            "target_lang_translations": {
                                "en": "This musician has a <rare> talent for feeling rhythm."
                            }
                        }
                    ],
                    "antonyms": [
                        "распространённый"
                    ],
                    "common_phrases": [
                        "редкая книга",
                        "редкий вид",
                        "редкий талант",
                        "редкая вещь"
                    ],
                    "target_lang_definitions": {
                        "en": "Encountered very infrequently; unique, valuable, or hard to access."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rare",
                                "target_lang_sense_clarification": "uncommon and valuable"
                            },
                            {
                                "target_lang_word": "scarce",
                                "target_lang_sense_clarification": "hard to find; in short supply"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55eeddaf-2014-3ce5-b3e9-73ed1b54fc16",
                    "sense_definition": "Состоящий из далеко расположенных друг от друга частей; негустой.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "density",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Через <редкий> лес пробивались яркие лучи солнца.",
                            "target_lang_translations": {
                                "en": "Bright rays of sun broke through the <sparse> forest."
                            }
                        },
                        {
                            "text": "У старика были седые и <редкие> волосы.",
                            "target_lang_translations": {
                                "en": "The old man had grey and <thin> hair."
                            }
                        }
                    ],
                    "antonyms": [
                        "плотный",
                        "частый"
                    ],
                    "common_phrases": [
                        "редкий лес",
                        "редкие волосы",
                        "редкие зубы",
                        "редкая ткань"
                    ],
                    "target_lang_definitions": {
                        "en": "Consisting of parts located far from each other; not dense."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sparse",
                                "target_lang_sense_clarification": "thinly scattered (e.g. vegetation)"
                            },
                            {
                                "target_lang_word": "thin",
                                "target_lang_sense_clarification": "not thick (e.g. hair)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5335a93f-1f6f-3e20-81b0-50deb502ef11",
                    "sense_definition": "Мало какой; лишь немногие (указывает на малое количество людей или предметов, обладающих свойством).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quantifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Редкая> птица долетит до середины широкой реки.",
                            "target_lang_translations": {
                                "en": "A <rare> bird will fly to the middle of the broad river."
                            }
                        },
                        {
                            "text": "<Редкий> человек согласится работать без выходных.",
                            "target_lang_translations": {
                                "en": "A <rare> person would agree to work without days off."
                            }
                        }
                    ],
                    "antonyms": [
                        "всякий"
                    ],
                    "common_phrases": [
                        "редкая птица",
                        "редкий человек"
                    ],
                    "target_lang_definitions": {
                        "en": "Few; only a few (indicating a small number of people or objects possessing a property)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rare",
                                "target_lang_sense_clarification": "few and far between"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d0359e8-1916-3ee4-be4c-bf8dae156c51",
                    "sense_definition": "Исключительный по степени проявления качества (часто используется для эмоционального усиления).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот ваш знакомый — <редкий> плут и обманщик.",
                            "target_lang_translations": {
                                "en": "This acquaintance of yours is a <rare> rogue and deceiver."
                            }
                        },
                        {
                            "text": "Нам выпала <редкая> удача найти билеты на этот концерт.",
                            "target_lang_translations": {
                                "en": "We had the <rare> luck to find tickets for this concert."
                            }
                        }
                    ],
                    "common_phrases": [
                        "редкий негодяй",
                        "редкая удача",
                        "редкий ум"
                    ],
                    "target_lang_definitions": {
                        "en": "Exceptional in degree of quality; extraordinary."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rare",
                                "target_lang_sense_clarification": "exceptional or extraordinary"
                            },
                            {
                                "target_lang_word": "exceptional",
                                "target_lang_sense_clarification": "unusually good or extreme"
                            }
                        ]
                    }
                }
            ]
        }
    ]

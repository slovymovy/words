    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "37d7f7ba-36c5-3ef8-bf3b-784a8157b200",
                    "sense_definition": "Een handwerktechniek uitvoeren waarbij men met een speciale naald lussen maakt van garen om textiel te vormen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "crafting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn oma leerde mij <haken> toen ik nog heel jong was.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка научила меня <вязать крючком>, когда я был совсем маленьким."
                            }
                        },
                        {
                            "text": "Zij zit 's avonds graag op de bank te <haken> aan een nieuwe deken.",
                            "target_lang_translations": {
                                "ru": "По вечерам она любит сидеть на диване и <вязать крючком> новое одеяло."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leren haken",
                        "haken en breien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Техника рукоделия, при которой с помощью специального крючка из пряжи создают петли для формирования ткани."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вязать крючком",
                                "target_lang_sense_clarification": "рукоделие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13b144d8-2444-36fa-aba5-75d938725986",
                    "sense_definition": "Ergens achter vast blijven zitten, vaak onbedoeld en met het risico dat het scheurt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "snagging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn jas bleef aan de deurklink <haken> toen ik de kamer uitliep.",
                            "target_lang_translations": {
                                "ru": "Моя куртка <зацепилась> за дверную ручку, когда я выходил из комнаты."
                            }
                        },
                        {
                            "text": "Pas op dat je trui niet aan die scherpe spijker blijft <haken>.",
                            "target_lang_translations": {
                                "ru": "Осторожно, чтобы твой свитер не <зацепился> за тот острый гвоздь."
                            }
                        }
                    ],
                    "synonyms": [
                        "vastraken",
                        "blijven hangen"
                    ],
                    "antonyms": [
                        "loskomen"
                    ],
                    "common_phrases": [
                        "blijven haken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зацепиться за что-то, часто случайно, с риском порвать вещь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цепляться",
                                "target_lang_sense_clarification": "застревать"
                            },
                            {
                                "target_lang_word": "зацепиться",
                                "target_lang_sense_clarification": "застрять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32704917-daa2-33a8-85ba-2e27b6adc7dd",
                    "sense_definition": "Iets vastmaken of verbinden door middel van een haak.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fastening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de ketting goed aan elkaar <haken> zodat hij niet loslaat.",
                            "target_lang_translations": {
                                "ru": "Нужно хорошо <сцепить> цепь, чтобы она не рассоединилась."
                            }
                        },
                        {
                            "text": "Hij <haakte> het touw stevig aan de ring in de muur.",
                            "target_lang_translations": {
                                "ru": "Он крепко <прицепил> веревку к кольцу в стене."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanhaken",
                        "vasthaken",
                        "vastmaken"
                    ],
                    "antonyms": [
                        "losmaken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикреплять или соединять что-либо с помощью крюка."
                    },
                    "translations": {

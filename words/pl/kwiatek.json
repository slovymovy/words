                {
                    "sense_id": "c26a330a-4d87-31dc-b3bd-8baffc09017e",
                    "sense_definition": "Zdrobnienie od słowa <w>kwiat</w>; mała, delikatna roślina kwitnąca lub sam jej kielich.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "plant_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko narysowało kolorowy <w>kwiatek</w> na laurce dla swojej mamy.",
                            "target_lang_translations": {
                                "ru": "Ребенок нарисовал разноцветный <w>цветочек</w> на открытке для мамы."
                            }
                        },
                        {
                            "text": "Ten mały, niebieski <w>kwiatek</w> rośnie dziko na górskich łąkach.",
                            "target_lang_translations": {
                                "ru": "Этот маленький голубой <w>цветочек</w> растет в диком виде на горных лугах."
                            }
                        }
                    ],
                    "synonyms": [
                        "kwiatuszek"
                    ],
                    "common_phrases": [
                        "polny kwiatek",
                        "sztuczny kwiatek",
                        "dać kwiatek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова «цветок»; маленькое, нежное цветущее растение или его бутон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цветочек",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"

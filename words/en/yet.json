                },
                {
                    "sense_id": "4fae1acd-36bf-395a-9d4c-649a6543d387",
                    "sense_definition": "Continuously up to the present time; still.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "time_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "While he was <w>yet</w> a young man, he became famous throughout the land.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>ещё</w> молодым человеком, он прославился на всю страну."
                            }

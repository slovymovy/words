            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d00f3a63-c07b-3612-b802-153d10a9614b",
                    "sense_definition": "Удариться обо что-либо, обычно издавая короткий глухой звук.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В темноте я случайно <тюкнулся> коленом об угол стола.",
                            "target_lang_translations": {
                                "en": "In the dark, I accidentally <bumped> my knee against the corner of the table."
                            }
                        },
                        {
                            "text": "Птица с разгона <тюкнулась> в оконное стекло.",
                            "target_lang_translations": {
                                "en": "The bird flew at full speed and <hit> the window pane."
                            }
                        }
                    ],
                    "synonyms": [
                        "удариться",
                        "стукнуться"
                    ],
                    "common_phrases": [
                        "тюкнуться лбом",
                        "тюкнуться носом",
                        "больно тюкнуться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To hit against something, usually making a short dull sound."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bump",
                                "target_lang_sense_clarification": "to knock against something with a light impact"
                            },
                            {
                                "target_lang_word": "bang",
                                "target_lang_sense_clarification": "to hit a body part against something hard"
                            },
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "general impact"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad993e1d-2cc5-3895-9117-b2bb3f355a73",
                    "sense_definition": "Упасть или повалиться, издав при этом звук удара.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уставший ребёнок <тюкнулся> на подушку и сразу уснул.",
                            "target_lang_translations": {
                                "en": "The tired child <flopped> onto the pillow and fell asleep immediately."
                            }
                        },
                        {
                            "text": "Тяжёлый мешок с картошкой <тюкнулся> на пол.",
                            "target_lang_translations": {
                                "en": "The heavy sack of potatoes <thudded> onto the floor."
                            }
                        }
                    ],
                    "synonyms": [
                        "упасть",
                        "грохнуться",
                        "шлёпнуться"
                    ],
                    "antonyms": [
                        "встать",
                        "подняться"
                    ],
                    "common_phrases": [
                        "тюкнуться на кровать",
                        "тюкнуться спать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To fall or drop down, making a sound of impact."
                    },
                    "translations": {

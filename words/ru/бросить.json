                    ],
                    "synonyms": [
                        "покинуть",
                        "оставить"
                    ],
                    "common_phrases": [
                        "бросить семью",
                        "бросить жену",
                        "бросить мужа",
                        "бросить в беде",
                        "бросить на произвол судьбы"

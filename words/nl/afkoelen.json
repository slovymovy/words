    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4cb626af-bf2a-3748-a5dd-84eb53e1e193",
                    "sense_definition": "Kouder worden; in temperatuur dalen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_temp_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het hete water <w>koelde</w> langzaam <w>af</w>.",
                            "target_lang_translations": {
                                "en": "The hot water slowly <w>cooled</w> <w>down</w>.",
                                "ru": "Горячая вода медленно <w>остывала</w>."
                            }
                        },
                        {
                            "text": "Je moet de soep eerst laten <w>afkoelen</w> voordat je hem eet.",
                            "target_lang_translations": {
                                "en": "You must let the soup <w>cool</w> <w>down</w> first before you eat it.",
                                "ru": "Нужно сначала дать супу <w>остыть</w>, прежде чем есть его."
                            }
                        }
                    ],
                    "antonyms": [
                        "opwarmen"
                    ],
                    "common_phrases": [
                        "even afkoelen",
                        "snel afkoelen",
                        "laten afkoelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To become colder; to decrease in temperature.",
                        "ru": "Становиться холоднее; понижаться в температуре."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cool down",
                                "target_lang_sense_clarification": "to lose heat"
                            },
                            {
                                "target_lang_word": "cool off",
                                "target_lang_sense_clarification": "to become cooler"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "остывать",
                                "target_lang_sense_clarification": "становиться холодным (о еде, жидкости)"
                            },
                            {
                                "target_lang_word": "охлаждаться",
                                "target_lang_sense_clarification": "понижать температуру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6235135f-5331-3b9e-af07-70bce5b3b4ac",
                    "sense_definition": "Iets kouder maken; de temperatuur van iets verlagen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_temp_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>koelde</w> zijn verbrande vinger <w>af</w> onder de koude kraan.",
                            "target_lang_translations": {
                                "en": "He <w>cooled</w> his burnt finger <w>down</w> under the cold tap.",
                                "ru": "Он <w>охладил</w> свой обожженный палец под холодным краном."
                            }
                        },
                        {
                            "text": "De ventilator <w>koelt</w> de kamer een beetje <w>af</w>.",
                            "target_lang_translations": {
                                "en": "The fan <w>cools</w> the room <w>down</w> a bit.",
                                "ru": "Вентилятор немного <w>охлаждает</w> комнату."
                            }
                        }
                    ],
                    "synonyms": [
                        "koelen"
                    ],
                    "antonyms": [
                        "verhitten",
                        "opwarmen"
                    ],
                    "common_phrases": [
                        "met water afkoelen",
                        "de motor afkoelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something colder; to lower the temperature of something.",
                        "ru": "Делать что-то холоднее; понижать температуру чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cool down",
                                "target_lang_sense_clarification": "to make something cooler"
                            },
                            {
                                "target_lang_word": "cool",
                                "target_lang_sense_clarification": "to reduce temperature"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "охлаждать",
                                "target_lang_sense_clarification": "делать холодным"
                            },
                            {
                                "target_lang_word": "остужать",
                                "target_lang_sense_clarification": "давать остыть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc16e017-9f83-3a53-b5ab-a8af7f75c4fc",
                    "sense_definition": "Kalmeren; minder boos of opgewonden worden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de ruzie liep hij een rondje buiten om even <w>af</w> te <w>koelen</w>.",
                            "target_lang_translations": {
                                "en": "After the argument, he walked a lap outside to <w>cool</w> <w>off</w> for a moment.",
                                "ru": "После ссоры он прошелся по улице, чтобы немного <w>остыть</w>."
                            }
                        },
                        {
                            "text": "De politie probeerde de verhitte gemoederen op straat <w>af</w> te <w>koelen</w>.",
                            "target_lang_translations": {
                                "en": "The police tried to <w>calm</w> <w>down</w> the heated tempers on the street.",
                                "ru": "Полиция пыталась <w>остудить</w> накалившиеся страсти на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedaren",
                        "kalmeren"
                    ],
                    "common_phrases": [
                        "de gemoederen afkoelen",
                        "even moeten afkoelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To calm down; to become less angry or excited.",
                        "ru": "Успокаиваться; становиться менее сердитым или возбужденным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "calm down",
                                "target_lang_sense_clarification": "to become less angry"
                            },
                            {
                                "target_lang_word": "cool down",
                                "target_lang_sense_clarification": "to regain composure"
                            },
                            {
                                "target_lang_word": "cool off",
                                "target_lang_sense_clarification": "to let anger subside"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "остывать",
                                "target_lang_sense_clarification": "перен. успокаиваться"
                            },
                            {
                                "target_lang_word": "успокаиваться",
                                "target_lang_sense_clarification": "приходить в равновесие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

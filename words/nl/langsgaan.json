                {
                    "sense_id": "409431e2-6015-30c0-b95d-289854754922",
                    "sense_definition": "Zich langs een bepaald punt, object of persoon bewegen; voorbijgaan.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "pass",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De feestelijke optocht <w>ging</w> recht voor ons huis <w>langs</w>.",
                            "target_lang_translations": {
                                "en": "The festive parade <w>passed</w> right in front of our house.",
                                "ru": "Праздничное шествие <w>прошло</w> прямо перед нашим домом."
                            }
                        },
                        {
                            "text": "De bus <w>gaat</w> hier elk half uur <w>langs</w>.",
                            "target_lang_translations": {
                                "en": "The bus <w>passes by</w> here every half hour.",
                                "ru": "Автобус <w>проезжает</w> здесь каждые полчаса."
                            }
                        }
                    ],
                    "synonyms": [
                        "passeren",
                        "voorbijgaan",
                        "langslopen"
                    ],
                    "common_phrases": [
                        "langs de deuren gaan",
                        "ergens langsgaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To move past a certain point, object, or person; to pass by.",
                        "ru": "Двигаться мимо определенной точки, объекта или человека; проходить мимо."
                    },

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "92203965-9aec-382b-8865-e84d31751027",
                    "sense_definition": "Pertaining to or originating from Ireland or the Irish people.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We enjoyed a traditional <w>Irish</w> breakfast during our holiday.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска мы наслаждались традиционным <w>ирландским</w> завтраком."
                            }
                        },
                        {
                            "text": "The <w>Irish</w> landscape is famous for its lush green fields.",
                            "target_lang_translations": {
                                "ru": "<w>Ирландский</w> пейзаж славится своими сочными зелеными полями."
                            }
                        }
                    ],
                    "synonyms": [
                        "Hibernian"
                    ],
                    "common_phrases": [
                        "Irish coffee",
                        "Irish stew",
                        "Irish wolfhound",
                        "Irish luck"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Ирландии или ирландскому народу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ирландский",
                                "target_lang_sense_clarification": "относящийся к стране или людям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84a4dffc-d8eb-3b08-9cfb-844a5cb8bf1e",
                    "sense_definition": "Pertaining to the Irish language.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_lang_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He bought a book on <w>Irish</w> grammar to help with his studies.",
                            "target_lang_translations": {
                                "ru": "Он купил книгу по <w>ирландской</w> грамматике в помощь своей учебе."
                            }
                        },
                        {
                            "text": "The singer performed several traditional <w>Irish</w> songs.",
                            "target_lang_translations": {
                                "ru": "Певец исполнил несколько традиционных <w>ирландских</w> песен."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6c2815c8-6df7-3d79-af3a-1a69cfac9968",
                    "sense_definition": "Характер человека, его темперамент и врождённые душевные свойства.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У моего друга очень добрая и отзывчивая <w>натура</w>.",
                            "target_lang_translations": {
                                "en": "My friend has a very kind and responsive <w>nature</w>."
                            }
                        },
                        {
                            "text": "Она по <w>натуре</w> оптимист и никогда не унывает из-за мелких неудач.",
                            "target_lang_translations": {
                                "en": "She is an optimist by <w>nature</w> and never loses heart over minor setbacks."
                            }
                        }
                    ],
                    "synonyms": [
                        "характер",
                        "нрав",
                        "темперамент"
                    ],
                    "common_phrases": [
                        "широкая натура",
                        "тонкая натура",
                        "творческая натура",
                        "по натуре"
                    ],
                    "target_lang_definitions": {
                        "en": "A person's character, temperament, and innate spiritual qualities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nature",
                                "target_lang_sense_clarification": "innate character or disposition"
                            },
                            {
                                "target_lang_word": "temperament",
                                "target_lang_sense_clarification": "emotional disposition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfd83d27-54e0-3642-8a9f-d7bce6a86cd8",
                    "sense_definition": "Реальный объект (человек, предмет, пейзаж), который художник наблюдает и изображает в своём произведении.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студенты художественной школы часто выезжают в парк писать этюды с <w>натуры</w>.",
                            "target_lang_translations": {
                                "en": "Art school students often go to the park to paint studies from <w>nature</w>."
                            }
                        },
                        {
                            "text": "Для этой картины художнику позировала профессиональная <w>натура</w>.",
                            "target_lang_translations": {
                                "en": "A professional <w>model</w> posed for the artist for this painting."
                            }
                        }
                    ],
                    "common_phrases": [
                        "рисовать с натуры",
                        "писать с натуры",
                        "выезд на натуру",
                        "живая натура"
                    ],
                    "target_lang_definitions": {
                        "en": "A real object (person, item, landscape) that an artist observes and depicts in their work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "life",
                                "target_lang_sense_clarification": "in the phrase 'from life' (artistic observation)"
                            },
                            {
                                "target_lang_word": "model",
                                "target_lang_sense_clarification": "person posing for an artist"
                            },
                            {
                                "target_lang_word": "nature",
                                "target_lang_sense_clarification": "natural scenery as a subject for art"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba3dc607-7221-315e-ade5-31b79c947218",
                    "sense_definition": "Товары, продукты или услуги как средство оплаты вместо денег.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "economics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В трудные времена работникам завода иногда выдавали зарплату <w>натурой</w>.",
                            "target_lang_translations": {
                                "en": "In difficult times, factory workers were sometimes paid their wages in <w>kind</w>."
                            }
                        },
                        {
                            "text": "Фермер решил расплатиться за помощь соседа <w>натурой</w>, отдав ему мешок картошки.",
                            "target_lang_translations": {
                                "en": "The farmer decided to pay his neighbor for the help in <w>kind</w>, giving him a sack of potatoes."
                            }
                        }
                    ],
                    "antonyms": [
                        "деньги"
                    ],
                    "common_phrases": [
                        "платить натурой",
                        "оплата натурой",
                        "расчёт натурой"
                    ],
                    "target_lang_definitions": {
                        "en": "Goods, products, or services used as a means of payment instead of money."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kind",
                                "target_lang_sense_clarification": "in the phrase 'in kind' (payment in goods)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4939f729-a25f-3c19-b790-0be5ccd587be",
                    "sense_definition": "То, что существует в действительности; реальность (в отличие от изображения или описания).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот пейзаж на картине выглядит совсем как в <w>натуре</w>.",
                            "target_lang_translations": {
                                "en": "This landscape in the painting looks just like in <w>reality</w>."
                            }
                        },
                        {
                            "text": "Мы хотели увидеть, как этот сложный механизм работает в <w>натуре</w>, а не на чертеже.",
                            "target_lang_translations": {
                                "en": "We wanted to see how this complex mechanism works in <w>reality</w>, not on a blueprint."
                            }
                        }
                    ],
                    "synonyms": [
                        "действительность",
                        "реа́льность"
                    ],
                    "antonyms": [
                        "вымысел"
                    ],
                    "common_phrases": [
                        "в натуре",
                        "как в натуре"
                    ],
                    "target_lang_definitions": {
                        "en": "That which exists in actuality; reality (as opposed to a depiction or description)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reality",
                                "target_lang_sense_clarification": "actual existence"
                            },
                            {
                                "target_lang_word": "real life",
                                "target_lang_sense_clarification": "tangible existence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ddca271-c40d-3b03-99f2-5b3afca6c84a",
                    "sense_definition": "Природа, окружающий мир (обычно используется в устаревшем или шутливом контексте).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили провести выходные на лоне <w>натуры</w>, подальше от городского шума.",
                            "target_lang_translations": {
                                "en": "We decided to spend the weekend in the bosom of <w>nature</w>, far from the city noise."
                            }
                        },
                        {
                            "text": "Привычка — это вторая <w>натура</w>, от которой очень трудно избавиться.",
                            "target_lang_translations": {
                                "en": "Habit is second <w>nature</w>, which is very hard to get rid of."
                            }
                        }
                    ],
                    "synonyms": [
                        "приро́да"
                    ],
                    "common_phrases": [
                        "лоно натуры",
                        "игра натуры",
                        "привычка — вторая натура"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'природа'"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "often used ironically"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Nature, the surrounding world (usually used in archaic or humorous contexts)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nature",
                                "target_lang_sense_clarification": "the physical world and environment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60a0702b-9753-3cfe-a713-01c9fd2ca55d",
                    "sense_definition": "Организм человека, его физическое состояние или телосложение.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его крепкая <w>натура</w> быстро справилась с простудой без всяких лекарств.",
                            "target_lang_translations": {
                                "en": "His strong <w>constitution</w> quickly fought off the cold without any medicine."
                            }
                        },
                        {
                            "text": "Голод давал о себе знать, и <w>натура</w> настойчиво требовала пищи.",
                            "target_lang_translations": {
                                "en": "Hunger made itself felt, and <w>nature</w> insistently demanded food."
                            }
                        }
                    ],
                    "synonyms": [
                        "органи́зм"
                    ],
                    "common_phrases": [
                        "крепкая натура",
                        "требование натуры"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person's organism, physical condition, or constitution."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "constitution",
                                "target_lang_sense_clarification": "physical health and strength"
                            },
                            {
                                "target_lang_word": "nature",
                                "target_lang_sense_clarification": "vital bodily functions or needs"
                            }
                        ]
                    }
                }
            ]
        }
    ]

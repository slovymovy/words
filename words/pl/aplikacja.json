{
    "word_family": [
        "aplikować",
        "aplikant",
        "aplikacyjny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e5d4d42-8938-33ba-aed9-e2edc5dc816a",
                    "sense_definition": "Program komputerowy lub oprogramowanie instalowane na urządzeniach mobilnych (telefonach, tabletach) służące do wykonywania określonych zadań.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "software",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta <aplikacja> pomaga w szybkiej nauce języków obcych.",
                            "target_lang_translations": {
                                "ru": "Это <приложение> помогает быстро учить иностранные языки."
                            }
                        },
                        {
                            "text": "Musisz zaktualizować tę <aplikację>, żeby działała poprawnie na twoim telefonie.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно обновить это <приложение>, чтобы оно работало корректно на твоем телефоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "program",
                        "apka"
                    ],
                    "common_phrases": [
                        "aplikacja mobilna",
                        "pobrać aplikację",
                        "zainstalować aplikację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа или программное обеспечение для мобильных устройств (телефонов, планшетов), предназначенное для выполнения определенных задач."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приложение",
                                "target_lang_sense_clarification": "программа для смартфона или компьютера"
                            },
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "общее название программного обеспечения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b6bd2db-fe7f-3cc5-9061-c3dfb65b57d9",
                    "sense_definition": "Oficjalne pismo, zgłoszenie lub wniosek, składane w celu uzyskania czegoś, na przykład pracy lub miejsca na uczelni.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja <aplikacja> o pracę została niestety odrzucona przez firmę.",
                            "target_lang_translations": {
                                "ru": "Моя <заявка> на работу была, к сожалению, отклонена компанией."
                            }
                        },
                        {
                            "text": "Termin składania <aplikacji> na studia mija w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Срок подачи <заявлений> на учебу истекает на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "wniosek",
                        "program"
                    ],
                    "common_phrases": [
                        "złożyć aplikację",
                        "wysłać aplikację",
                        "aplikacja o pracę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное письмо, заявка или заявление, подаваемое с целью получения чего-либо, например, работы или места в университете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заявление",
                                "target_lang_sense_clarification": "официальный документ"
                            },
                            {
                                "target_lang_word": "заявка",
                                "target_lang_sense_clarification": "просьба или требование на участие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e348ae9-7775-3c5d-9571-b53d3995a473",
                    "sense_definition": "Praktyka zawodowa absolwenta studiów prawniczych, przygotowująca do wykonywania zawodu sędziego, prokuratora, adwokata lub radcy prawnego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_training",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ukończeniu studiów prawniczych Marek dostał się na <aplikację> sędziowską.",
                            "target_lang_translations": {
                                "ru": "После окончания юридического факультета Марек попал на судейскую <стажировку>."
                            }
                        },
                        {
                            "text": "Egzamin wstępny na <aplikację> adwokacką jest bardzo wymagający.",
                            "target_lang_translations": {
                                "ru": "Вступительный экзамен на адвокатскую <стажировку> очень сложный."
                            }
                        }
                    ],
                    "synonyms": [
                        "aplikantura"
                    ],
                    "common_phrases": [
                        "aplikacja adwokacka",
                        "aplikacja radcowska",
                        "dostać się na aplikację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Профессиональная стажировка выпускников юридических факультетов, подготавливающая к профессии судьи, прокурора или адвоката."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стажировка",
                                "target_lang_sense_clarification": "период обучения на рабочем месте"
                            },
                            {
                                "target_lang_word": "практика",
                                "target_lang_sense_clarification": "приобретение опыта работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1b1bc49-1f13-3b10-a89b-d58655ef953d",
                    "sense_definition": "Ozdobny element z innego materiału, naszyty lub naklejony na tkaninę, ubranie lub inny przedmiot.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "decoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiła dżinsową kurtkę z kolorową <aplikacją> w kształcie kwiatu na plecach.",
                            "target_lang_translations": {
                                "ru": "Она купила джинсовую куртку с цветной <аппликацией> в форме цветка на спине."
                            }
                        },
                        {
                            "text": "Ta koronkowa <aplikacja> sprawia, że sukienka wygląda bardzo elegancko.",
                            "target_lang_translations": {
                                "ru": "Эта кружевная <аппликация> придает платью очень элегантный вид."
                            }
                        }
                    ],
                    "synonyms": [
                        "naszywka"
                    ],
                    "common_phrases": [
                        "aplikacja z koronki",
                        "ozdobna aplikacja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный элемент из другого материала, нашитый или наклеенный на ткань, одежду или другой предмет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аппликация",
                                "target_lang_sense_clarification": "нашитое или наклеенное украшение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9f62fe9-840e-3ab0-be97-77f33f67ccaa",
                    "sense_definition": "Czynność stosowania czegoś w praktyce lub podawania czegoś, na przykład leku.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "usage_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bezpośrednia <aplikacja> tego leku na skórę może wywołać podrażnienia.",
                            "target_lang_translations": {
                                "ru": "Непосредственное <нанесение> этого лекарства на кожу может вызвать раздражение."
                            }
                        },
                        {
                            "text": "Szeroka <aplikacja> nowych przepisów budzi wiele wątpliwości prawnych.",
                            "target_lang_translations": {
                                "ru": "Широкое <применение> новых правил вызывает много правовых сомнений."
                            }
                        }
                    ],
                    "synonyms": [
                        "zastosowanie",
                        "stosowanie"
                    ],
                    "common_phrases": [
                        "aplikacja leku",
                        "sposób aplikacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по применению чего-либо на практике или введению/нанесению чего-либо (например, лекарства)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нанесение",
                                "target_lang_sense_clarification": "лекарства или вещества на поверхность"
                            },
                            {
                                "target_lang_word": "применение",
                                "target_lang_sense_clarification": "использование на практике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12bba47c-58cd-3c8f-90da-9b4fc918e014",
                    "sense_definition": "Dawne określenie na pilność, staranność i przyłożenie się do wykonywanej pracy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "diligence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego wielka <aplikacja> w nauce przyniosła w końcu oczekiwane rezultaty.",
                            "target_lang_translations": {
                                "ru": "Его огромное <усердие> в учебе принесло наконец ожидаемые результаты."
                            }
                        },
                        {
                            "text": "Mistrz pochwalił ucznia za jego <aplikację> i dokładność w rzemiośle.",
                            "target_lang_translations": {
                                "ru": "Мастер похвалил ученика за его <прилежание> и точность в ремесле."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo w tym znaczeniu jest już rzadko używane"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее обозначение усердия, старательности и прилежания в выполняемой работе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усердие",
                                "target_lang_sense_clarification": "большое старание"
                            },
                            {
                                "target_lang_word": "прилежание",
                                "target_lang_sense_clarification": "старательность в учебе или труде"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
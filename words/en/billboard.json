    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6afa6811-9c98-3f45-8d65-e4f41879f36b",
                    "sense_definition": "A very large outdoor sign, generally used for advertising.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "advertising_sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We saw a huge <w>billboard</w> advertising the new movie on the highway.",
                            "target_lang_translations": {
                                "ru": "Мы увидели огромный <w>рекламный щит</w> с рекламой нового фильма на шоссе."
                            }
                        },
                        {
                            "text": "The company rented a <w>billboard</w> in the city center to promote their brand.",
                            "target_lang_translations": {
                                "ru": "Компания арендовала <w>билборд</w> в центре города для продвижения своего бренда."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoarding",
                        "sign"
                    ],
                    "common_phrases": [
                        "roadside billboard",
                        "electronic billboard",
                        "billboard advertising"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень большой уличный щит, обычно используемый для размещения рекламы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рекламный щит",
                                "target_lang_sense_clarification": "наружная реклама"
                            },
                            {
                                "target_lang_word": "билборд",
                                "target_lang_sense_clarification": "наружная реклама"
                            },
                            {
                                "target_lang_word": "щит",
                                "target_lang_sense_clarification": "краткая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50648352-75e5-346d-bd11-061a7e088919",
                    "sense_definition": "A flat surface, such as a panel or fence, on which bills or notices are posted.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She pinned the flyer to the <w>billboard</w> in the community hall.",
                            "target_lang_translations": {
                                "ru": "Она прикрепила листовку на <w>доску объявлений</w> в общественном центре."
                            }
                        },
                        {
                            "text": "Students checked the <w>billboard</w> every morning for new announcements.",
                            "target_lang_translations": {
                                "ru": "Студенты каждое утро проверяли <w>стенд</w> в поисках новых объявлений."
                            }
                        }
                    ],
                    "synonyms": [
                        "bulletin board",
                        "noticeboard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская поверхность, например панель или стенд, на которой вывешиваются объявления или уведомления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доска объявлений",
                                "target_lang_sense_clarification": "информационная"
                            },
                            {
                                "target_lang_word": "стенд",
                                "target_lang_sense_clarification": "информационный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "797a6267-2f67-3959-9425-5dc9c25ce7dd",
                    "sense_definition": "An opening or closing portion of a broadcast program, often including credits and sponsors' names.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sponsor's logo appeared briefly in the opening <w>billboard</w> of the show.",
                            "target_lang_translations": {
                                "ru": "Логотип спонсора ненадолго появился в начальной <w>заставке</w> шоу."
                            }
                        },
                        {
                            "text": "They need to edit the closing <w>billboard</w> to include the new producer.",
                            "target_lang_translations": {
                                "ru": "Им нужно отредактировать финальную <w>заставку</w>, чтобы включить нового продюсера."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "broadcasting industry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступительная или заключительная часть теле- или радиопрограммы, часто содержащая титры и имена спонсоров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заставка",
                                "target_lang_sense_clarification": "телевидение"
                            },
                            {
                                "target_lang_word": "спонсорская заставка",
                                "target_lang_sense_clarification": "реклама в эфире"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a583c680-99b2-3e38-9e64-aae86e8c32b1",
                    "sense_definition": "A two-dimensional image or sprite in a 3D environment that always faces the screen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The distant trees were rendered as a simple <w>billboard</w> to save memory.",
                            "target_lang_translations": {
                                "ru": "Далекие деревья были отрисованы как простой <w>билборд</w> для экономии памяти."
                            }
                        },
                        {
                            "text": "Using a <w>billboard</w> for the crowd effect makes the game run smoother.",
                            "target_lang_translations": {
                                "ru": "Использование <w>билбордов</w> для создания эффекта толпы делает игру более плавной."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprite"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computer graphics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Двумерное изображение или спрайт в 3D-среде, которое всегда обращено лицевой стороной к экрану."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "билборд",
                                "target_lang_sense_clarification": "компьютерная графика"
                            },
                            {
                                "target_lang_word": "спрайт",
                                "target_lang_sense_clarification": "графический объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e6e27e0-ec8a-3587-b2ac-f48388260dab",
                    "sense_definition": "A piece of thick plank on the bow of a vessel for the anchor fluke to rest on.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailor secured the heavy anchor against the <w>billboard</w>.",
                            "target_lang_translations": {
                                "ru": "Матрос закрепил тяжелый якорь на <w>якорной подушке</w>."
                            }
                        },
                        {
                            "text": "Damage to the <w>billboard</w> required immediate repair before the ship could sail.",
                            "target_lang_translations": {
                                "ru": "Повреждение <w>якорной подушки</w> требовало немедленного ремонта перед отплытием корабля."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть толстой обшивки на носу судна, на которую опирается лапа якоря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "якорная подушка",
                                "target_lang_sense_clarification": "морской термин"
                            },
                            {
                                "target_lang_word": "билборд",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "53fcbf28-a94a-3de8-9227-50d1730cf35f",
                    "sense_definition": "To advertise something on a billboard.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "advertising_sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The agency decided to <w>billboard</w> the new product all over the city.",
                            "target_lang_translations": {
                                "ru": "Агентство решило <w>рекламировать</w> новый продукт <w>на щитах</w> по всему городу."
                            }
                        },
                        {
                            "text": "The upcoming concert was <w>billboarded</w> near every major exit.",
                            "target_lang_translations": {
                                "ru": "Предстоящий концерт <w>рекламировался на щитах</w> у каждого крупного выезда."
                            }
                        }
                    ],
                    "synonyms": [
                        "advertise",
                        "publicize"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рекламировать что-либо на рекламных щитах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рекламировать на щитах",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "размещать на билбордах",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        },
        {

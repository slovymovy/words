{
    "word_family": [
        "początek",
        "początkowy",
        "rozpoczynać",
        "początkowo"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "260e4a6d-91bf-3a1d-bbb0-f07b00e10a52",
                    "sense_definition": "Zacząć coś robić, przystąpić do wykonywania jakiejś czynności lub zainicjować jakieś wydarzenie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_start_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy natychmiast <rozpocząć> przygotowania do podróży.",
                            "target_lang_translations": {
                                "ru": "Мы должны немедленно <начать> подготовку к путешествию."
                            }
                        },
                        {
                            "text": "Nauczyciel <rozpoczął> lekcję od sprawdzenia listy obecności.",
                            "target_lang_translations": {
                                "ru": "Учитель <начал> урок с проверки списка присутствующих."
                            }
                        }
                    ],
                    "synonyms": [
                        "zacząć",
                        "wszcząć",
                        "zacząć"
                    ],
                    "antonyms": [
                        "zakończyć",
                        "skończyć się"
                    ],
                    "common_phrases": [
                        "rozpocząć pracę",
                        "rozpocząć naukę",
                        "rozpocząć spotkanie",
                        "rozpocząć działalność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать что-то делать, приступить к выполнению какого-либо действия или инициировать какое-либо событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начать",
                                "target_lang_sense_clarification": "приступить к действию"
                            },
                            {
                                "target_lang_word": "приступить",
                                "target_lang_sense_clarification": "начать дело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3a99e89-2a44-3516-a957-6f47d6453b67",
                    "sense_definition": "Zacząć się dziać, mieć swój początek (o wydarzeniach, okresach czasu); używane z zaimkiem zwrotnym „się”.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_start_event_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rok szkolny <rozpoczął> się pierwszego września uroczystym apelem.",
                            "target_lang_translations": {
                                "ru": "Учебный год <начался> первого сентября торжественной линейкой."
                            }
                        },
                        {
                            "text": "Koncert <rozpoczął> się z małym opóźnieniem z powodu problemów technicznych.",
                            "target_lang_translations": {
                                "ru": "Концерт <начался> с небольшой задержкой из-за технических проблем."
                            }
                        }
                    ],
                    "synonyms": [
                        "zacząć się",
                        "nastać"
                    ],
                    "antonyms": [
                        "skończyć się",
                        "zakończyć"
                    ],
                    "common_phrases": [
                        "rozpocząć się o czasie",
                        "rozpocząć się wkrótce",
                        "rozpocząć się od nowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать происходить, иметь своё начало (о событиях, периодах времени); используется с возвратным местоимением «się»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начаться",
                                "target_lang_sense_clarification": "о событиях, времени"
                            },
                            {
                                "target_lang_word": "настать",
                                "target_lang_sense_clarification": "наступить (о времени)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfdf3398-06e2-388a-917c-156c87b86b1a",
                    "sense_definition": "Odezwać się jako pierwszy, zacząć mówić lub prowadzić rozmowę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_start_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor <rozpoczął> swoje przemówienie od powitania wszystkich gości.",
                            "target_lang_translations": {
                                "ru": "Директор <начал> свою речь с приветствия всех гостей."
                            }
                        },
                        {
                            "text": "Nie wiedziałem, jak <rozpocząć> tę trudną rozmowę z rodzicami.",
                            "target_lang_translations": {
                                "ru": "Я не знал, как <начать> этот трудный разговор с родителями."
                            }
                        }
                    ],
                    "synonyms": [
                        "zacząć"
                    ],
                    "antonyms": [
                        "zakończyć"
                    ],
                    "common_phrases": [
                        "rozpocząć dyskusję",
                        "rozpocząć temat",
                        "rozpocząć wątek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заговорить первым, начать говорить или вести беседу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начать",
                                "target_lang_sense_clarification": "заговорить, начать речь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9ed8642-b1c3-346b-9273-d4e0ac57c789",
                    "sense_definition": "Otworzyć coś i zacząć tego używać lub to spożywać (np. jedzenie, napój).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_start_consume",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Postanowiliśmy <rozpocząć> słoik domowego dżemu do śniadania.",
                            "target_lang_translations": {
                                "ru": "Мы решили <начать> банку домашнего варенья к завтраку."
                            }
                        },
                        {
                            "text": "Czy mogę <rozpocząć> nowe opakowanie kawy, czy jest jeszcze stara?",
                            "target_lang_translations": {
                                "ru": "Могу я <начать> новую пачку кофе, или есть ещё старая?"
                            }
                        }
                    ],
                    "synonyms": [
                        "napocząć"
                    ],
                    "common_phrases": [
                        "rozpocząć butelkę",
                        "rozpocząć bochenek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открыть что-то и начать использовать или употреблять (например, еду, напиток)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начать",
                                "target_lang_sense_clarification": "приступить к использованию"
                            },
                            {
                                "target_lang_word": "открыть",
                                "target_lang_sense_clarification": "распечатать упаковку"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2bf098b0-ccf4-3d79-a318-a887ae11d269",
                    "sense_definition": "Со знанием дела, обладая большим опытом и профессиональными навыками.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Капитан <опытно> вёл судно через опасные рифы.",
                            "target_lang_translations": {
                                "en": "The captain <expertly> guided the ship through the dangerous reefs."
                            }
                        },
                        {
                            "text": "Врач <опытно> наложил повязку на рану пациента.",
                            "target_lang_translations": {
                                "en": "The doctor <skillfully> applied a bandage to the patient's wound."
                            }
                        }
                    ],
                    "synonyms": [
                        "умело",
                        "профессионально",
                        "мастерски",
                        "искусно",
                        "квалифицированно"
                    ],
                    "antonyms": [
                        "неопытно",
                        "неумело"
                    ],
                    "common_phrases": [
                        "опытно руководить",
                        "опытно использовать",
                        "опытно действовать"
                    ],
                    "target_lang_definitions": {
                        "en": "With expertise, possessing great experience and professional skills."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expertly",
                                "target_lang_sense_clarification": "in a highly skillful or knowledgeable manner"
                            },
                            {
                                "target_lang_word": "skillfully",
                                "target_lang_sense_clarification": "with skill or cleverness"
                            },
                            {
                                "target_lang_word": "proficiently",
                                "target_lang_sense_clarification": "competently and efficiently"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c27169f8-1b2b-3050-9e23-88b853407430",
                    "sense_definition": "Посредством опытов и экспериментов; на практике, а не в теории.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "experiment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учёные <опытно> доказали эффективность нового лекарства.",
                            "target_lang_translations": {
                                "en": "Scientists <experimentally> proved the effectiveness of the new drug."
                            }
                        },
                        {
                            "text": "Это предположение необходимо проверить <опытно> в лаборатории.",
                            "target_lang_translations": {
                                "en": "This assumption must be verified <experimentally> in the laboratory."
                            }
                        }
                    ],
                    "synonyms": [
                        "экспериментально",
                        "эмпирически",
                        "практически"
                    ],
                    "antonyms": [
                        "теоретически",
                        "умозрительно"
                    ],
                    "common_phrases": [
                        "опытно проверить",
                        "опытно установить",
                        "опытно подтвердить",
                        "опытно доказать"
                    ],
                    "target_lang_definitions": {
                        "en": "By means of tests and experiments; in practice rather than theory."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "experimentally",
                                "target_lang_sense_clarification": "by means of scientific experiments"
                            },
                            {
                                "target_lang_word": "empirically",
                                "target_lang_sense_clarification": "based on observation or experience rather than theory"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "good",
        "goodly"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1efbb148-4194-3677-ba2c-063792d30b20",
                    "sense_definition": "A word used instead of 'God' to express strong emotion or relief.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "euphemism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thank <w>goodness</w> you arrived safely before the storm started.",
                            "target_lang_translations": {
                                "ru": "Слава <w>богу</w>, ты добрался благополучно, до того как началась буря."
                            }
                        },
                        {
                            "text": "For <w>goodness</w>' sake, please stop making that loud noise!",
                            "target_lang_translations": {
                                "ru": "Ради <w>бога</w>, пожалуйста, перестань так шуметь!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "thank goodness",
                        "for goodness' sake"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used as a mild substitute for God"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое вместо слова «Бог» для выражения сильных эмоций или облегчения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бог",
                                "target_lang_sense_clarification": "в устойчивых выражениях (thank goodness, for goodness' sake)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce3488d4-485c-39e2-aac5-048bb9ddbd97",
                    "sense_definition": "The quality of being morally good, kind, or virtuous.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "virtue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She helped the neighbors out of the <w>goodness</w> of her heart.",
                            "target_lang_translations": {
                                "ru": "Она помогла соседям по <w>доброте</w> душевной."
                            }
                        },
                        {
                            "text": "We should always try to see the <w>goodness</w> in other people.",
                            "target_lang_translations": {
                                "ru": "Мы всегда должны стараться видеть <w>добро</w> в других людях."
                            }
                        }
                    ],
                    "synonyms": [
                        "virtue",
                        "virtuousness",
                        "good",
                        "beneficence"
                    ],
                    "antonyms": [
                        "evil",
                        "wickedness",
                        "immorality",
                        "evilness",
                        "depravity",
                        "villainy",
                        "corruptness",
                        "vileness"
                    ],
                    "common_phrases": [
                        "out of the goodness of one's heart",
                        "honest-to-goodness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество, заключающееся в моральной чистоте, доброте или добродетели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доброта",
                                "target_lang_sense_clarification": "душевное качество, отзывчивость"
                            },
                            {
                                "target_lang_word": "добродетель",
                                "target_lang_sense_clarification": "положительное нравственное качество"
                            },
                            {
                                "target_lang_word": "добро",
                                "target_lang_sense_clarification": "нравственное благо, положительное начало"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64818e77-f431-3789-af4b-63c9db7a7362",
                    "sense_definition": "The healthy and nutritious part of food or drink.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nutrition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This homemade soup is full of natural <w>goodness</w>.",
                            "target_lang_translations": {
                                "ru": "Этот домашний суп полон природной <w>пользы</w>."
                            }
                        },
                        {
                            "text": "Do not cook the vegetables for too long or you will lose all the <w>goodness</w>.",
                            "target_lang_translations": {
                                "ru": "Не варите овощи слишком долго, иначе вы потеряете всю <w>пользу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "full of goodness",
                        "natural goodness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полезные и питательные свойства пищи или напитков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "польза",
                                "target_lang_sense_clarification": "полезные свойства продукта"
                            },
                            {
                                "target_lang_word": "полезные вещества",
                                "target_lang_sense_clarification": "питательные элементы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "0bb3cc76-2572-3639-ae58-40d04f87e235",
                    "sense_definition": "An exclamation used to express surprise, shock, or relief.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "euphemism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>goodness</w>, look at how much you have grown!",
                            "target_lang_translations": {
                                "ru": "<w>Боже мой</w>, посмотри, как ты вырос!"
                            }
                        },
                        {
                            "text": "<w>Goodness</w> gracious, I completely forgot about our meeting today.",
                            "target_lang_translations": {
                                "ru": "<w>Боже правый</w>, я совершенно забыл о нашей сегодняшней встрече."
                            }
                        }
                    ],
                    "common_phrases": [
                        "my goodness",
                        "goodness gracious",
                        "goodness me"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "expresses surprise or annoyance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, выражающее удивление, шок или облегчение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Боже мой",
                                "target_lang_sense_clarification": "выражение удивления или испуга"
                            },
                            {
                                "target_lang_word": "Господи",
                                "target_lang_sense_clarification": "эмоциональное восклицание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "8ee4cd28-bb71-3b6a-81bb-81f7cb6fc872",
                    "sense_definition": "Łaciński przyimek oznaczający \"do\", \"ku\" lub \"w sprawie\", używany w języku polskim wyłącznie w utartych zwrotach i wyrażeniach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Decyzja o zmianie planów została podjęta <w>ad</w> hoc.",
                            "target_lang_translations": {
                                "ru": "Решение об изменении планов было принято <w>ad</w> hoc (ситуативно)."
                            }
                        },
                        {
                            "text": "Proszę odnosić się <w>ad</w> rem do omawianego zagadnienia.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, говорите <w>ad</w> rem (по существу) касательно обсуждаемого вопроса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ad hoc",
                        "ad rem",
                        "ad acta",
                        "ad vocem",
                        "ad personam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Латинский предлог, означающий «к», «до» или «по делу», используемый в польском языке исключительно в устойчивых выражениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ad",
                                "target_lang_sense_clarification": "в латинских выражениях (напр. ad hoc)"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "значение латинского предлога"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e5ab8e03-de78-348b-90b6-c56bccd096c0",
                    "sense_definition": "Skrót od łacińskiego wyrażenia \"anno Domini\" oznaczający \"roku Pańskiego\", stosowany przy datach wskazujących na naszą erę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na starym dokumencie widniała data 1500 <w>ad</w>.",
                            "target_lang_translations": {
                                "ru": "На старом документе стояла дата 1500 г. <w>н.э.</w>"
                            }
                        },
                        {
                            "text": "Wydarzenie to miało miejsce w piątym wieku <w>ad</w>.",
                            "target_lang_translations": {
                                "ru": "Это событие произошло в пятом веке <w>н.э.</w>"
                            }
                        }
                    ],
                    "synonyms": [
                        "n.e."
                    ],
                    "antonyms": [
                        "AC",
                        "a.a.C"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "skrót"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от латинского выражения «anno Domini», означающее «год Господень»; используется при указании дат нашей эры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "н.э.",
                                "target_lang_sense_clarification": "нашей эры"
                            },
                            {
                                "target_lang_word": "от Р.Х.",
                                "target_lang_sense_clarification": "от Рождества Христова"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e5687b9-41c8-3aea-8638-26fbd4d81787",
                    "sense_definition": "Skrót oznaczający autoryzowanego doradcę, pełniącego funkcję w alternatywnym systemie obrotu giełdowego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spółka musi wybrać swojego <w>ad</w> przed wejściem na rynek.",
                            "target_lang_translations": {
                                "ru": "Компания должна выбрать своего <w>авторизованного советника</w> перед выходом на рынок."
                            }
                        },
                        {
                            "text": "Rola <w>ad</w> polega na wspieraniu emitenta w wypełnianiu obowiązków informacyjnych.",
                            "target_lang_translations": {
                                "ru": "Роль <w>авторизованного советника</w> заключается в поддержке эмитента при выполнении информационных обязательств."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "skrót"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение, означающее авторизованного советника, выполняющего функции в альтернативной системе биржевой торговли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "авторизованный советник",
                                "target_lang_sense_clarification": "на бирже"
                            }
                        ]
                    }
                }
            ]
        }
    ]

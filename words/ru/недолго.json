                    "sense_id": "c9e69f4b-9463-375c-9162-798481acd900",
                    "sense_definition": "В течение короткого времени; мало по времени.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы гуляли в парке совсем <w>недолго</w>, потому что начался дождь.",
                            "target_lang_translations": {
                                "en": "We walked in the park for quite <w>a short time</w> because it started to rain."
                            }
                        },
                        {
                            "text": "Ждать автобуса пришлось <w>недолго</w>.",
                            "target_lang_translations": {
                                "en": "We had to wait <w>only a short while</w> for the bus."
                            }
                        }
                    ],
                    "antonyms": [
                        "долго"
                    ],
                    "common_phrases": [
                        "совсем недолго",
                        "сравнительно недолго"
                    ],
                    "target_lang_definitions": {
                        "en": "For a short period of time; briefly."
                    },
                    "translations": {

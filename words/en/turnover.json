{
    "word_family": [
        "turn",
        "overturn",
        "turnout",
        "turnaround",
        "return"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "02afa17d-230c-36d8-965a-5433a608f2f0",
                    "sense_definition": "The total amount of money that a company receives from sales during a particular period.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_metrics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company had an annual <w>turnover</w> of $500,000.",
                            "target_lang_translations": {
                                "ru": "У компании был годовой <w>оборот</w> в 500 000 долларов."
                            }
                        },
                        {
                            "text": "They hope to increase their total <w>turnover</w> by ten percent next year.",
                            "target_lang_translations": {
                                "ru": "Они надеются увеличить свой общий <w>оборот</w> на десять процентов в следующем году."
                            }
                        }
                    ],
                    "synonyms": [
                        "runover"
                    ],
                    "common_phrases": [
                        "annual turnover",
                        "high turnover",
                        "total turnover"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in British English; 'revenue' is often used in US English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Общая сумма денег, полученная компанией от продаж за определенный период."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оборо́т",
                                "target_lang_sense_clarification": "общая сумма продаж"
                            },
                            {
                                "target_lang_word": "товарооборо́т",
                                "target_lang_sense_clarification": "объем продажи товаров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f25af035-b330-3354-94bf-6a506a19245c",
                    "sense_definition": "The rate at which employees leave a workforce and are replaced, or the rate at which goods are sold and replaced.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "business_metrics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "High staff <w>turnover</w> can lead to low morale amongst employees.",
                            "target_lang_translations": {
                                "ru": "Высокая <w>текучесть</w> кадров может привести к низкому моральному духу среди сотрудников."
                            }
                        },
                        {
                            "text": "Supermarkets rely on a rapid <w>turnover</w> of fresh produce to ensure quality.",
                            "target_lang_translations": {
                                "ru": "Супермаркеты полагаются на быстрый <w>оборот</w> свежих продуктов для обеспечения качества."
                            }
                        }
                    ],
                    "common_phrases": [
                        "staff turnover",
                        "employee turnover",
                        "stock turnover",
                        "high turnover"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показатель скорости, с которой сотрудники покидают компанию и заменяются новыми, или скорости реализации товаров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "теку́честь",
                                "target_lang_sense_clarification": "кадров или персонала"
                            },
                            {
                                "target_lang_word": "оборо́т",
                                "target_lang_sense_clarification": "товаров или запасов"
                            },
                            {
                                "target_lang_word": "обора́чиваемость",
                                "target_lang_sense_clarification": "экономический показатель скорости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8c85720-d8e2-389d-ad6b-18b12eb14d52",
                    "sense_definition": "A small pie made by folding a piece of pastry over a filling, such as fruit.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They only served me one apple <w>turnover</w> for breakfast.",
                            "target_lang_translations": {
                                "ru": "На завтрак мне подали только одну яблочную <w>слойку</w>."
                            }
                        },
                        {
                            "text": "She baked a delicious cherry <w>turnover</w> for dessert.",
                            "target_lang_translations": {
                                "ru": "Она испекла вкусную вишневую <w>слойку</w> на десерт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "apple turnover"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой пирог, обычно треугольной формы, сделанный путем складывания теста поверх начинки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сло́йка",
                                "target_lang_sense_clarification": "выпечка, обычно из слоеного теста"
                            },
                            {
                                "target_lang_word": "пирожо́к",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "853845fb-2035-33d0-8de1-5e08ce61824c",
                    "sense_definition": "An instance of losing possession of the ball to the opposing team during a game.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team committed a costly <w>turnover</w> late in the game.",
                            "target_lang_translations": {
                                "ru": "Команда совершила дорогостоящую <w>потерю</w> в конце игры."
                            }
                        },
                        {
                            "text": "Their strong defense forced three <w>turnovers</w> in the first half.",
                            "target_lang_translations": {
                                "ru": "Их сильная защита вынудила соперника совершить три <w>потери</w> в первой половине матча."
                            }
                        }
                    ],
                    "common_phrases": [
                        "commit a turnover",
                        "force a turnover"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случай потери владения мячом в пользу команды соперника во время игры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поте́ря",
                                "target_lang_sense_clarification": "мяча в спорте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3d76288b-999d-31b4-b5a7-fef8cd6cebb0",
                    "sense_definition": "Designed to be folded or turned over, such as a collar or cuff.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He wore a formal shirt with a stiff <w>turnover</w> collar.",
                            "target_lang_translations": {
                                "ru": "Он был в строгой рубашке с жестким <w>отложным</w> воротником."
                            }
                        },
                        {
                            "text": "The coat features <w>turnover</w> cuffs that reveal a contrasting lining.",
                            "target_lang_translations": {
                                "ru": "Пальто имеет манжеты <w>с отворотами</w>, открывающими контрастную подкладку."
                            }
                        }
                    ],
                    "common_phrases": [

            "senses": [
                {
                    "sense_id": "ee73b2f7-373c-3fee-afa0-9fbfcc7e089c",
                    "sense_definition": "De tijd die nu is; het huidige moment.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten in het <w>heden</w> leven, niet in het verleden.",
                            "target_lang_translations": {
                                "en": "We must live in the <w>present</w>, not in the past.",
                                "ru": "Мы должны жить в <w>настоящем</w>, а не в прошлом."
                            }
                        },
                        {
                            "text": "Het <w>heden</w> is het enige moment waarop je echt invloed hebt.",
                            "target_lang_translations": {
                                "en": "The <w>present</w> is the only moment you really have influence over.",
                                "ru": "<w>Настоящее</w> — это единственный момент, на который вы действительно можете повлиять."
                            }
                        }
                    ],
                    "synonyms": [
                        "nu"
                    ],
                    "antonyms": [
                        "verleden",
                        "toekomst"
                    ],
                    "common_phrases": [
                        "in het heden",
                        "heden en verleden"
                    ],
                    "target_lang_definitions": {
                        "en": "The time that is now; the current moment.",
                        "ru": "Время, которое происходит сейчас; настоящий момент."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "the current time period"
                            },
                            {
                                "target_lang_word": "now",
                                "target_lang_sense_clarification": "the current moment"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "настоящее",
                                "target_lang_sense_clarification": "текущий период времени"
                            },
                            {
                                "target_lang_word": "настоящее время",
                                "target_lang_sense_clarification": "нынешний момент"
                            }
                        ]
                    }
                }
            ]
        },
        {

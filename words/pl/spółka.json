                    "sense_id": "1627cf81-7c02-33da-900d-59ac89fe30c4",
                    "sense_definition": "Podmiot prawny lub organizacja utworzona przez wspólników w celu prowadzenia działalności gospodarczej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "business_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat prowadzi małą <w>spółkę</w> budowlaną, która remontuje mieszkania.",
                            "target_lang_translations": {
                                "ru": "Мой брат руководит небольшой строительной <w>компанией</w>, которая ремонтирует квартиры."
                            }
                        },
                        {
                            "text": "Ta <w>spółka</w> jest notowana na giełdzie papierów wartościowych od wielu lat.",
                            "target_lang_translations": {
                                "ru": "Эта <w>компания</w> котируется на фондовой бирже уже много лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "s-ka",
                        "sp."
                    ],
                    "common_phrases": [
                        "spółka akcyjna",
                        "spółka z ograniczoną odpowiedzialnością",
                        "spółka cywilna",
                        "założyć spółkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридическое лицо или организация, созданная партнерами для ведения хозяйственной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компания",

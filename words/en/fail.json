{
    "word_family": [
        "failure",
        "failing",
        "unfailing"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c8817c96-0b34-3d48-a6a5-132e6060d3a8",
                    "sense_definition": "To not succeed in what you are trying to achieve.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to lift the heavy box but <w>failed</w>.",
                            "target_lang_translations": {
                                "ru": "Он попытался поднять тяжелую коробку, но <w>не справился</w>."
                            }
                        },
                        {
                            "text": "If you work hard, you will not <w>fail</w>.",
                            "target_lang_translations": {
                                "ru": "Если будешь усердно работать, ты не <w>потерпишь неудачу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fall flat",
                        "come to nothing",
                        "miscarry"
                    ],
                    "antonyms": [
                        "succeed"
                    ],
                    "common_phrases": [
                        "fail miserably",
                        "without fail"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не преуспеть в том, чего вы пытаетесь достичь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потерпеть неудачу",
                                "target_lang_sense_clarification": "формальное выражение"
                            },
                            {
                                "target_lang_word": "не справиться",
                                "target_lang_sense_clarification": "не суметь выполнить задачу"
                            },
                            {
                                "target_lang_word": "провалиться",
                                "target_lang_sense_clarification": "разговорное, потерпеть полный крах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5539c4e1-9509-3259-bac6-42e99fdb5fc7",
                    "sense_definition": "To not pass a test, exam, or course.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "academic_fail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I studied all night because I didn't want to <w>fail</w> the exam.",
                            "target_lang_translations": {
                                "ru": "Я учился всю ночь, потому что не хотел <w>провалить</w> экзамен."
                            }
                        },
                        {
                            "text": "She <w>failed</w> math last semester.",
                            "target_lang_translations": {
                                "ru": "Она <w>завалила</w> математику в прошлом семестре."
                            }
                        }
                    ],
                    "synonyms": [
                        "flunk",
                        "tank",
                        "bomb"
                    ],
                    "antonyms": [
                        "succeed"
                    ],
                    "common_phrases": [
                        "fail an exam",
                        "fail a test"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не сдать тест, экзамен или курс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провалить",
                                "target_lang_sense_clarification": "получить неудовлетворительную оценку"
                            },
                            {
                                "target_lang_word": "не сдать",
                                "target_lang_sense_clarification": "нейтральное выражение"
                            },
                            {
                                "target_lang_word": "завалить",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "865353ba-75eb-3897-90db-986672d02eb5",
                    "sense_definition": "To not do something that you should do or are expected to do.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The driver <w>failed</w> to stop at the red light.",
                            "target_lang_translations": {
                                "ru": "Водитель <w>не остановился</w> на красный свет."
                            }
                        },
                        {
                            "text": "He <w>failed</w> to mention that he was leaving early.",
                            "target_lang_translations": {
                                "ru": "Он <w>не упомянул</w>, что уходит рано."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fail to understand",
                        "fail to see",
                        "fail to mention"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не сделать того, что нужно или ожидается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не сделать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "не выполнить",
                                "target_lang_sense_clarification": "об обязанности или обещании"
                            },
                            {
                                "target_lang_word": "упустить",
                                "target_lang_sense_clarification": "о возможности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b5a7897-efac-323c-8829-9cf53a6470b3",
                    "sense_definition": "To stop working properly (used for machines or parts of the body).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cease_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engine <w>failed</w> shortly after takeoff.",
                            "target_lang_translations": {
                                "ru": "Двигатель <w>отказал</w> вскоре после взлета."
                            }
                        },
                        {
                            "text": "The brakes <w>failed</w> on the steep hill, causing an accident.",
                            "target_lang_translations": {
                                "ru": "Тормоза <w>отказали</w> на крутом склоне, что привело к аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "conk",
                        "bust"
                    ],
                    "common_phrases": [
                        "engine failed",
                        "brakes failed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать работать должным образом (о машинах или частях тела)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказать",
                                "target_lang_sense_clarification": "о механизмах или органах"
                            },
                            {
                                "target_lang_word": "выйти из строя",
                                "target_lang_sense_clarification": "техническое, сломаться"
                            },
                            {
                                "target_lang_word": "сломаться",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51a21b16-938b-3c70-a5d9-d1930ba815c7",
                    "sense_definition": "To disappoint someone or not be of help when needed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disappoint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I feel like I have <w>failed</w> my parents by not finishing school.",
                            "target_lang_translations": {
                                "ru": "Я чувствую, что <w>подвел</w> родителей, не закончив школу."
                            }
                        },
                        {
                            "text": "Words <w>fail</w> me when I try to describe how beautiful it was.",
                            "target_lang_translations": {
                                "ru": "Мне <w>не хватает</w> слов, чтобы описать, как это было красиво."
                            }
                        }
                    ],
                    "common_phrases": [
                        "words fail me",
                        "fail someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разочаровать кого-то или не помочь, когда это необходимо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвести",
                                "target_lang_sense_clarification": "обмануть ожидания"
                            },
                            {
                                "target_lang_word": "разочаровать",
                                "target_lang_sense_clarification": "вызвать чувство разочарования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29a92d16-32fc-38b7-a384-2059bb52e51f",
                    "sense_definition": "To give a student a non-passing grade.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "academic_fail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor <w>failed</w> me because I missed the final exam.",
                            "target_lang_translations": {
                                "ru": "Профессор <w>завалил</w> меня, потому что я пропустил финальный экзамен."
                            }
                        },
                        {
                            "text": "If you don't hand in your work, the teacher will <w>fail</w> you.",
                            "target_lang_translations": {
                                "ru": "Если не сдашь работу, учитель тебя <w>завалит</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "flunk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поставить студенту неудовлетворительную оценку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завалить",
                                "target_lang_sense_clarification": "разговорное, намеренно поставить плохую оценку"
                            },
                            {
                                "target_lang_word": "поставить «неуд»",
                                "target_lang_sense_clarification": "формальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6bfedcf-60d0-31a5-8c0d-5e3052d43b33",
                    "sense_definition": "To become insufficient or stop being produced (often used for crops or supplies).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cease_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crops <w>failed</w> this year due to the severe drought.",
                            "target_lang_translations": {
                                "ru": "Урожай <w>погиб</w> в этом году из-за сильной засухи."
                            }
                        },
                        {
                            "text": "As the light began to <w>fail</w>, we decided to head back home.",
                            "target_lang_translations": {
                                "ru": "Когда свет начал <w>меркнуть</w>, мы решили вернуться домой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "crops failed",
                        "light failed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать недостаточным или перестать производиться (часто об урожае или припасах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погибнуть",
                                "target_lang_sense_clarification": "об урожае"
                            },
                            {
                                "target_lang_word": "не удаться",
                                "target_lang_sense_clarification": "об урожае"
                            },
                            {
                                "target_lang_word": "иссякать",
                                "target_lang_sense_clarification": "о запасах или свете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b5f5123-7ef6-3615-9fbb-3c1bab9c35d0",
                    "sense_definition": "To become weaker or lose health.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cease_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His health began to <w>fail</w> rapidly after the accident.",
                            "target_lang_translations": {
                                "ru": "Его здоровье начало быстро <w>сдавать</w> после аварии."
                            }
                        },
                        {
                            "text": "My grandfather is <w>failing</w> fast and may not last the winter.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка быстро <w>сдает</w> и может не пережить зиму."
                            }
                        }
                    ],
                    "common_phrases": [
                        "failing health",
                        "failing eyesight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать слабее или потерять здоровье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "разговорное, о здоровье или силах"
                            },
                            {
                                "target_lang_word": "слабеть",
                                "target_lang_sense_clarification": "становиться слабым"
                            },
                            {
                                "target_lang_word": "ухудшаться",
                                "target_lang_sense_clarification": "о состоянии здоровья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "766e80b9-bea8-32cd-bd01-fb9e2c1969de",
                    "sense_definition": "To become unable to pay debts; to go bankrupt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cease_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many small businesses <w>failed</w> during the economic recession.",
                            "target_lang_translations": {
                                "ru": "Многие малые предприятия <w>разорились</w> во время экономического спада."
                            }
                        },
                        {
                            "text": "The bank <w>failed</w> and had to be rescued by the government.",
                            "target_lang_translations": {
                                "ru": "Банк <w>лопнул</w>, и правительству пришлось его спасать."
                            }
                        }
                    ],
                    "synonyms": [
                        "bust"
                    ],
                    "antonyms": [
                        "succeed"
                    ],
                    "common_phrases": [
                        "banks failed",
                        "business failed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать неплатежеспособным; обанкротиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разориться",
                                "target_lang_sense_clarification": "потерять все деньги"
                            },
                            {
                                "target_lang_word": "банкротиться",
                                "target_lang_sense_clarification": "юридический термин"
                            },
                            {
                                "target_lang_word": "потерпеть крах",
                                "target_lang_sense_clarification": "о бизнесе или банке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "99bcb16a-9396-30d7-a73f-b7655f13476d",
                    "sense_definition": "A grade given to a student who has not passed an examination or course.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "academic_fail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was disappointed to see a <w>fail</w> on his report card.",
                            "target_lang_translations": {
                                "ru": "Он был разочарован, увидев <w>неуд</w> в своем табеле."
                            }
                        },
                        {
                            "text": "The exam is graded simply as a pass or a <w>fail</w>.",
                            "target_lang_translations": {
                                "ru": "Экзамен оценивается просто: зачет или <w>незачет</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pass or fail"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценка, выставляемая студенту, который не сдал экзамен или курс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неуд",
                                "target_lang_sense_clarification": "сокращение от неудовлетворительно"
                            },
                            {
                                "target_lang_word": "незачёт",
                                "target_lang_sense_clarification": "в системе зачет/незачет"
                            },
                            {
                                "target_lang_word": "двойка",
                                "target_lang_sense_clarification": "школьная оценка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96e13eae-1f1f-321c-8457-c29f451af185",
                    "sense_definition": "An embarrassing or humorous mistake or failure.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That attempt to jump the fence was an epic <w>fail</w>.",
                            "target_lang_translations": {
                                "ru": "Эта попытка перепрыгнуть через забор была эпичным <w>провалом</w>."
                            }
                        },
                        {
                            "text": "The whole project was a total <w>fail</w> from start to finish.",
                            "target_lang_translations": {
                                "ru": "Весь проект был полным <w>провалом</w> от начала до конца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "epic fail"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in internet culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неловкая или смешная ошибка или неудача."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провал",
                                "target_lang_sense_clarification": "полная неудача"
                            },
                            {
                                "target_lang_word": "фейл",
                                "target_lang_sense_clarification": "сленг, заимствование"
                            },
                            {
                                "target_lang_word": "облом",
                                "target_lang_sense_clarification": "сленг, неудача"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "89361c99-1fd5-336d-bd1f-796220431a16",
                    "sense_definition": "Unsuccessful or inadequate.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "general_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That party was so <w>fail</w>.",
                            "target_lang_translations": {
                                "ru": "Эта вечеринка была такой <w>отстойной</w>."
                            }
                        },
                        {
                            "text": "His attempt at a joke was completely <w>fail</w>.",
                            "target_lang_translations": {
                                "ru": "Его попытка пошутить была полным <w>провалом</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неудачный или неподходящий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провальный",
                                "target_lang_sense_clarification": "обреченный на неудачу"
                            },
                            {
                                "target_lang_word": "отстойный",
                                "target_lang_sense_clarification": "сленг, плохой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "055fb943-79cf-35d7-9626-e0b22e212052",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Fails</w> has lived in this town for fifty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Фейлс</w> живет в этом городе пятьдесят лет."
                            }
                        },
                        {
                            "text": "I have an appointment with Dr. <w>Fails</w> tomorrow.",
                            "target_lang_translations": {
                                "ru": "У меня прием у доктора <w>Фейлса</w> завтра."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Фейлс",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
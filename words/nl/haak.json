                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Haak</w> de deur vast zodat hij niet openwaait.",
                            "target_lang_translations": {
                                "ru": "<w>Зацепи</w> дверь, чтобы она не распахнулась."
                            }

                {
                    "sense_id": "6ee42d79-a7e7-3cae-80d9-e780ddf9a995",
                    "sense_definition": "Osiągnąć pełnię rozwoju fizycznego lub stać się zdatnym do zbioru i spożycia (o roślinach, owocach).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "maturation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jabłka w naszym sadzie w końcu <w>dojrzały</w> i są bardzo słodkie.",
                            "target_lang_translations": {
                                "ru": "Яблоки в нашем саду наконец <w>созрели</w> и стали очень сладкими."
                            }
                        },
                        {
                            "text": "Musimy poczekać, aż pomidory <w>dojrzeją</w> na słońcu.",
                            "target_lang_translations": {
                                "ru": "Мы должны подождать, пока помидоры <w>поспеют</w> на солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "dorosnąć",
                        "wyrosnąć"
                    ],
                    "common_phrases": [
                        "owoce dojrzały",
                        "szybko dojrzeć",
                        "dojrzeć na słońcu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достичь полного физического развития или стать пригодным для сбора и употребления (о растениях, плодах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "созреть",
                                "target_lang_sense_clarification": "о фруктах, овощах, растениях"
                            },
                            {
                                "target_lang_word": "поспеть",
                                "target_lang_sense_clarification": "разговорное; о плодах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9404a348-efc9-3092-b86a-bdc3fddd6a22",
                    "sense_definition": "Zobaczyć lub zauważyć kogoś lub coś, co jest trudne do zauważenia, oddalone lub ukryte.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We mgle trudno było <w>dojrzeć</w> nadjeżdżający samochód.",
                            "target_lang_translations": {
                                "ru": "В тумане было трудно <w>разглядеть</w> приближающуюся машину."
                            }
                        },
                        {
                            "text": "Próbowałem <w>dojrzeć</w> znajomą twarz w gęstym tłumie.",
                            "target_lang_translations": {
                                "ru": "Я пытался <w>разглядеть</w> знакомое лицо в густой толпе."
                            }

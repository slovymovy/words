{
    "word_family": [
        "barony",
        "baroness",
        "baronial",
        "baronet"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1f112af9-93dd-3736-b3ff-52d88622cb3f",
                    "sense_definition": "A person of great power and influence in a particular industry or sphere of life.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "powerful_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The media <w>baron</w> owns several major newspapers and television channels.",
                            "target_lang_translations": {
                                "ru": "Этот медиа<w>магнат</w> владеет несколькими крупными газетами и телеканалами."
                            }
                        },
                        {
                            "text": "He became a wealthy oil <w>baron</w> after discovering new reserves in the desert.",
                            "target_lang_translations": {
                                "ru": "Он стал богатым нефтяным <w>магнатом</w> после открытия новых месторождений в пустыне."
                            }
                        }
                    ],
                    "synonyms": [
                        "magnate",
                        "tycoon",
                        "mogul",
                        "kingpin"
                    ],
                    "common_phrases": [
                        "media baron",
                        "oil baron",
                        "press baron",
                        "drug baron"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обладающий большой властью и влиянием в определенной отрасли или сфере жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магнат",
                                "target_lang_sense_clarification": "крупный промышленник или финансист"
                            },
                            {
                                "target_lang_word": "барон",
                                "target_lang_sense_clarification": "в устойчивых сочетаниях (напр. наркобарон)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f177b4d-1e95-3510-b377-892a389ceba9",
                    "sense_definition": "A nobleman of the lowest rank in the British peerage system, or historically a nobleman who held land directly from the king.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "nobility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The King granted the title of <w>baron</w> to the general for his loyal service.",
                            "target_lang_translations": {
                                "ru": "Король пожаловал генералу титул <w>барона</w> за его верную службу."
                            }
                        },
                        {
                            "text": "Every <w>baron</w> and earl was summoned to attend the royal ceremony.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>барон</w> и граф был призван присутствовать на королевской церемонии."
                            }
                        }
                    ],
                    "synonyms": [
                        "nobleman",
                        "peer",
                        "lord"
                    ],
                    "common_phrases": [
                        "robber baron"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дворянский титул низшего ранга в системе пэрства, а также носитель этого титула."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "барон",
                                "target_lang_sense_clarification": "дворянский титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6a92b22-ff81-3b80-969b-b8f3cff4c495",
                    "sense_definition": "A prisoner who gains power and authority over other inmates by trading illicit goods, such as tobacco.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "powerful_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tobacco <w>baron</w> controlled all the trade within the prison wing.",
                            "target_lang_translations": {
                                "ru": "Этот табачный <w>воротила</w> контролировал всю торговлю в тюремном крыле."
                            }
                        },
                        {
                            "text": "New inmates quickly learned not to cross the local <w>baron</w> if they wanted to stay safe.",
                            "target_lang_translations": {
                                "ru": "Новые заключенные быстро усваивали, что не стоит переходить дорогу местному <w>авторитету</w>, если они хотят остаться в безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tobacco baron"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "prison context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заключенный, получивший власть и авторитет над другими благодаря торговле запрещенными товарами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "авторитет",
                                "target_lang_sense_clarification": "в криминальной среде"
                            },
                            {
                                "target_lang_word": "воротила",
                                "target_lang_sense_clarification": "разг. влиятельный делец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bde39a7-2617-39e1-9808-98280289bf80",
                    "sense_definition": "A large cut of beef consisting of two sirloins joined at the backbone.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chef prepared a roasted <w>baron</w> of beef for the grand banquet.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар приготовил жареного говяжьего <w>барона</w> для грандиозного банкета."
                            }
                        },
                        {
                            "text": "It took several hours to properly cook the massive <w>baron</w>.",
                            "target_lang_translations": {
                                "ru": "Потребовалось несколько часов, чтобы должным образом приготовить этот массивный <w>барон</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "baron of beef"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой отруб говядины, состоящий из двух филейных частей, соединенных позвоночником."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "барон",
                                "target_lang_sense_clarification": "кулинарный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "dc06cfe3-d6f3-3aad-9bfc-6567a6d6ba80",
                    "sense_definition": "A surname of French, German, Polish, or Ukrainian origin.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Baron</w> arrived late for the meeting.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Бэрон</w> опоздал на встречу."
                            }
                        },
                        {
                            "text": "The works of the artist Jean-Michel <w>Baron</w> are displayed in the gallery.",
                            "target_lang_translations": {
                                "ru": "Работы художника Жан-Мишеля <w>Барона</w> выставлены в галерее."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия французского, немецкого, польского или украинского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Барон",
                                "target_lang_sense_clarification": "фамилия"
                            },
                            {
                                "target_lang_word": "Бэрон",
                                "target_lang_sense_clarification": "транскрипция английской фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
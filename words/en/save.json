                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сохранение",
                                "target_lang_sense_clarification": "процесс записи"
                            },
                            {
                                "target_lang_word": "запись",
                                "target_lang_sense_clarification": "данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "295a4dac-4fa5-3f70-9a31-b8c4a6d55421",
                    "sense_definition": "A block by a goalkeeper that prevents an opponent from scoring.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd cheered for the amazing <w>save</w> by the keeper.",
                            "target_lang_translations": {
                                "ru": "Толпа приветствовала потрясающий <w>сейв</w> вратаря."
                            }

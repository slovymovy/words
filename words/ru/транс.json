    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "17578663-3564-3b6a-bbc6-845a83b64458",
                    "sense_definition": "Стиль электронной танцевальной музыки с высоким темпом и повторяющимися мелодиями.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "music_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На вечеринке всю ночь играл громкий <w>транс</w>.",
                            "target_lang_translations": {
                                "en": "Loud <w>trance</w> played all night at the party."
                            }
                        },
                        {
                            "text": "Этот диджей пишет качественный <w>транс</w> уже много лет.",
                            "target_lang_translations": {
                                "en": "This DJ has been producing high-quality <w>trance</w> for many years."
                            }
                        }
                    ],
                    "common_phrases": [
                        "слушать транс",
                        "фестиваль транса",
                        "психоделический транс"
                    ],
                    "target_lang_definitions": {
                        "en": "A style of electronic dance music characterized by a high tempo and repetitive melodies."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trance",
                                "target_lang_sense_clarification": "music genre"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4c37ffc-9c48-31e6-b1a0-b0bf71bea94d",
                    "sense_definition": "Психологическое состояние изменённого сознания, при котором человек частично теряет контроль над реальностью или погружён в себя.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Медиум вошёл в глубокий <w>транс</w> во время спиритического сеанса.",
                            "target_lang_translations": {
                                "en": "The medium went into a deep <w>trance</w> during the seance."
                            }
                        },
                        {
                            "text": "От сильной усталости я словно впал в <w>транс</w> и не слышал вопроса.",
                            "target_lang_translations": {
                                "en": "From severe fatigue, I fell into a sort of <w>trance</w> and didn't hear the question."
                            }
                        }
                    ],
                    "synonyms": [
                        "прострация"
                    ],
                    "common_phrases": [
                        "впасть в транс",
                        "выйти из транса",
                        "глубокий транс",
                        "гипнотический транс"
                    ],
                    "target_lang_definitions": {
                        "en": "A psychological state of altered consciousness in which a person partially loses control over reality or is deeply absorbed in themselves."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trance",
                                "target_lang_sense_clarification": "mental state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "583d3f8f-07e0-31bf-becf-91e20a0bfbeb",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "75b707e3-a8b1-3635-99e9-8af8187106dc",
                    "sense_definition": "A leash or strap used for leading an animal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The trainer clipped the <w>leadline</w> onto the pony's bridle before walking it out.",
                            "target_lang_translations": {
                                "ru": "Тренер пристегнул <w>чомбур</w> к уздечке пони, прежде чем вывести его."
                            }
                        },
                        {
                            "text": "You must hold the <w>leadline</w> firmly to keep the dog under control.",
                            "target_lang_translations": {
                                "ru": "Вы должны крепко держать <w>поводок</w>, чтобы контролировать собаку."
                            }
                        }
                    ],
                    "synonyms": [
                        "leash",
                        "lead",
                        "tether"
                    ],
                    "common_phrases": [
                        "attach the leadline",
                        "hold the leadline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поводок или ремень, используемый для вождения животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чомбур",
                                "target_lang_sense_clarification": "для лошадей"
                            },
                            {
                                "target_lang_word": "поводок",
                                "target_lang_sense_clarification": "для собак"
                            },
                            {
                                "target_lang_word": "корда",
                                "target_lang_sense_clarification": "длинный повод для тренировки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5555d17d-0aef-3e8f-bf30-9fd35fc916d8",
                    "sense_definition": "A line with a weight at one end used in navigation to determine water depth.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailor lowered the <w>leadline</w> over the side to check the channel's depth.",
                            "target_lang_translations": {
                                "ru": "Матрос опустил <w>лотлинь</w> за борт, чтобы проверить глубину канала."
                            }
                        },
                        {
                            "text": "Before modern sonar, a <w>leadline</w> was essential for avoiding shallow waters.",
                            "target_lang_translations": {
                                "ru": "До появления современных эхолотов <w>лотлинь</w> был необходим, чтобы избегать мелководья."
                            }
                        }
                    ],
                    "synonyms": [
                        "sounding line"
                    ],
                    "common_phrases": [
                        "cast the leadline",
                        "heave the leadline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Веревка с грузом на конце, используемая в навигации для определения глубины воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лотлинь",
                                "target_lang_sense_clarification": "морской термин"
                            },
                            {
                                "target_lang_word": "лот",
                                "target_lang_sense_clarification": "инструмент измерения глубины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe56bcab-bfc6-308f-98b2-75e0f1b2bc36",
                    "sense_definition": "A weighted rope attached to the bottom edge of a fishing net to keep it submerged.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fishing_gear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Weights on the <w>leadline</w> ensure the net sinks quickly to the riverbed.",
                            "target_lang_translations": {
                                "ru": "Грузила на <w>нижней подборе</w> обеспечивают быстрое погружение сети на дно реки."
                            }
                        },
                        {
                            "text": "The fishermen repaired the damaged <w>leadline</w> after it snagged on a rock.",
                            "target_lang_translations": {
                                "ru": "Рыбаки починили поврежденную <w>грузовую подбору</w> после того, как она зацепилась за камень."
                            }
                        }
                    ],
                    "common_phrases": [
                        "net leadline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утяжеленный трос, прикрепленный к нижнему краю рыболовной сети для удержания ее под водой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нижняя подбора",
                                "target_lang_sense_clarification": "рыболовный термин"
                            },
                            {
                                "target_lang_word": "грузовая подбора",
                                "target_lang_sense_clarification": "часть сети с грузилами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
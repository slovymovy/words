                        "plasma screen",
                        "plasma TV",
                        "plasma ball"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "physics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние вещества, представляющее собой частично или полностью ионизированный газ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плазма",
                                "target_lang_sense_clarification": "физика"
                            }

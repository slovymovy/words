                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «head office» (головной офис)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "головной офис",
                                "target_lang_sense_clarification": "бизнес"
                            },
                            {
                                "target_lang_word": "штаб-квартира",
                                "target_lang_sense_clarification": "организация"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "f390564c-0346-36d8-8f14-9b617045ecc3",
                    "sense_definition": "Used to attract attention to something sighted, usually by lookouts at sea.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_call",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lookout shouted \"Land <w>ho</w>!\" as the island appeared on the horizon.",
                            "target_lang_translations": {
                                "ru": "Вперёдсмотрящий закричал: «Земля <w>на горизонте</w>!», когда остров показался вдали."
                            }
                        },
                        {
                            "text": "Sail <w>ho</w>! A ship was spotted to the east.",
                            "target_lang_translations": {
                                "ru": "<w>Вижу</w> парус! Корабль был замечен на востоке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Land ho",
                        "Sail ho"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, используемый моряками, чтобы привлечь внимание к увиденному (например, земле или кораблю)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вижу",
                                "target_lang_sense_clarification": "в составе фраз при обнаружении объекта"
                            },
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "оклик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f33da2e-7b53-3b6e-97f9-2b16af0867da",
                    "sense_definition": "A call used to excite attention, give notice of approach, or express surprise.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "general_call",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ho</w>! Is there anyone inside the castle?",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>! Есть кто-нибудь в замке?"
                            }
                        },
                        {
                            "text": "Westward <w>ho</w>! We travel towards the setting sun.",
                            "target_lang_translations": {
                                "ru": "На запад, <w>вперёд</w>! Мы едем к заходящему солнцу."
                            }
                        }
                    ],
                    "synonyms": [
                        "hey",
                        "halloo"
                    ],
                    "common_phrases": [
                        "Westward ho",
                        "tally-ho"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас для привлечения внимания, предупреждения о приближении или выражения удивления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "оклик"
                            },
                            {
                                "target_lang_word": "гей",
                                "target_lang_sense_clarification": "устаревшее, народно-поэтическое"
                            },
                            {
                                "target_lang_word": "вперёд",
                                "target_lang_sense_clarification": "призыв к движению"
                            }
                        ]
                    }
                }
            ]
        },
        {

                            {
                                "target_lang_word": "вышеуказанное имущество",
                                "target_lang_sense_clarification": "в юридических текстах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8f2701f-8a60-35cf-b45f-a912bfc3a5e6",
                    "sense_definition": "A slang or euphemistic term for the vagina.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

                            "target_lang_translations": {
                                "en": "An experienced stove-maker always has a sensible <w>assistant</w>."
                            }
                        },
                        {
                            "text": "Я был <w>подсобником</w> у электрика и подавал ему инструменты.",
                            "target_lang_translations": {
                                "en": "I was a <w>helper</w> for an electrician and handed him tools."
                            }
                        }
                    ],
                    "synonyms": [
                        "помощник",
                        "подручный",
                        "подмастерье"
                    ],
                    "common_phrases": [

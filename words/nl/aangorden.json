{
    "word_family": [
        "gorden",
        "gordel",
        "omgorden",
        "ontgorden"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "09d78669-63d2-33f2-8fd5-d85ce4dd8e74",
                    "sense_definition": "Iemand of iets stevig vastzetten met riemen, vaak voor de veiligheid.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fastening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zorg ervoor dat de kinderen achterin goed zijn <w>aangegord</w>.",
                            "target_lang_translations": {
                                "en": "Make sure the children in the back are properly <w>strapped in</w>.",
                                "ru": "Убедитесь, что дети сзади надежно <w>пристегнуты</w>."
                            }
                        },
                        {
                            "text": "De bergbeklimmer moest zijn uitrusting veilig <w>aangorden</w>.",
                            "target_lang_translations": {
                                "en": "The mountain climber had to safely <w>strap on</w> his equipment.",
                                "ru": "Альпинист должен был надежно <w>закрепить</w> свое снаряжение."
                            }
                        }
                    ],
                    "synonyms": [
                        "vastgespen",
                        "vastsnoeren",
                        "vastmaken"
                    ],
                    "antonyms": [
                        "losmaken",
                        "losgespen"
                    ],
                    "target_lang_definitions": {
                        "en": "To fasten someone or something securely with straps, often for safety.",
                        "ru": "Надежно закрепить кого-то или что-то ремнями, часто в целях безопасности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strap in",
                                "target_lang_sense_clarification": "to secure with a strap, e.g., in a vehicle"
                            },
                            {
                                "target_lang_word": "buckle up",
                                "target_lang_sense_clarification": "to fasten a seatbelt"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пристегивать",
                                "target_lang_sense_clarification": "ремнем безопасности"
                            },
                            {
                                "target_lang_word": "закреплять",
                                "target_lang_sense_clarification": "фиксировать ремнями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc3c4369-4c58-3954-8bc4-6dc140fca3d3",
                    "sense_definition": "Iets (zoals een wapen of tas) met een riem of band om het lichaam vastbinden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fastening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ridder ging zijn zwaard <w>aangorden</w> voor het toernooi.",
                            "target_lang_translations": {
                                "en": "The knight went to <w>gird on</w> his sword for the tournament.",
                                "ru": "Рыцарь собирался <w>опоясаться</w> мечом перед турниром."
                            }
                        },
                        {
                            "text": "Hij heeft zijn rugzak stevig <w>aangegord</w> voor de lange wandeling.",
                            "target_lang_translations": {
                                "en": "He securely <w>strapped on</w> his backpack for the long hike.",
                                "ru": "Он крепко <w>пристегнул</w> свой рюкзак перед долгой прогулкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "ombinden",
                        "omdoen"
                    ],
                    "antonyms": [
                        "afdoen"
                    ],
                    "common_phrases": [
                        "het zwaard aangorden"
                    ],
                    "target_lang_definitions": {
                        "en": "To bind something (such as a weapon or bag) around the body with a belt or strap.",
                        "ru": "Прикрепить что-то (например, оружие или сумку) к телу с помощью ремня."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gird on",
                                "target_lang_sense_clarification": "archaic or literary; often used with swords"
                            },
                            {
                                "target_lang_word": "strap on",
                                "target_lang_sense_clarification": "to attach something to the body with straps"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "опоясываться",
                                "target_lang_sense_clarification": "мечом или оружием"
                            },
                            {
                                "target_lang_word": "надевать",
                                "target_lang_sense_clarification": "рюкзак или снаряжение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa0c9dbb-546f-3cdd-a562-b73f3f763343",
                    "sense_definition": "Zich geestelijk klaarmaken of wapenen voor een zware taak of strijd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij moeten ons <w>aangorden</w> voor de uitdagingen die komen gaan.",
                            "target_lang_translations": {
                                "en": "We must <w>gird ourselves</w> for the challenges to come.",
                                "ru": "Мы должны <w>приготовиться</w> к грядущим испытаниям."
                            }
                        },
                        {
                            "text": "Zij heeft zich met moed <w>aangegord</w> om het onrecht te bestrijden.",
                            "target_lang_translations": {
                                "en": "She <w>girded herself</w> with courage to fight the injustice.",
                                "ru": "Она <w>вооружилась</w> мужеством, чтобы бороться с несправедливостью."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorbereiden",
                        "wapenen"
                    ],
                    "common_phrases": [
                        "zich ten strijde aangorden"
                    ],
                    "target_lang_definitions": {
                        "en": "To mentally prepare or arm oneself for a difficult task or battle.",
                        "ru": "Морально подготовиться или вооружиться для выполнения тяжелой задачи или борьбы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gird oneself",
                                "target_lang_sense_clarification": "figurative; to prepare for action"
                            },
                            {
                                "target_lang_word": "brace oneself",
                                "target_lang_sense_clarification": "to prepare for something difficult or unpleasant"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приготовиться",
                                "target_lang_sense_clarification": "к испытаниям"
                            },
                            {
                                "target_lang_word": "вооружиться",
                                "target_lang_sense_clarification": "терпением или мужеством"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
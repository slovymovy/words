            "senses": [
                {
                    "sense_id": "92dd8fed-f817-34bc-a874-5640b24c5c16",
                    "sense_definition": "Wat gedaan of gebruikt mag worden omdat er toestemming voor is gegeven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is niet <w>toegestaan</w> om in dit gebouw te roken.",
                            "target_lang_translations": {
                                "en": "It is not <w>allowed</w> to smoke in this building.",
                                "ru": "В этом здании курить не <w>разрешено</w>."
                            }
                        },
                        {
                            "text": "Een hond is een <w>toegestaan</w> huisdier, mits de andere bewoners er geen last van hebben.",
                            "target_lang_translations": {
                                "en": "A dog is a <w>permitted</w> pet, provided the other residents are not bothered by it.",
                                "ru": "Собака — <w>разрешённое</w> домашнее животное, при условии, что это не мешает другим жильцам."
                            }
                        }
                    ],
                    "synonyms": [
                        "legaal",
                        "wettig"
                    ],
                    "antonyms": [
                        "verboden",
                        "illegaal",
                        "onwettig"
                    ],
                    "common_phrases": [
                        "niet toegestaan",
                        "wettelijk toegestaan"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that is permitted or allowed because permission has been granted.",
                        "ru": "То, что разрешено делать или использовать, так как на это было дано разрешение."
                    },

            "senses": [
                {
                    "sense_id": "5045abc2-00c6-33df-af1a-79be8e8e59d8",
                    "sense_definition": "Zamarznięta woda w stanie stałym, twarda i zimna substancja.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_ice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą na jeziorze tworzy się gruby <w>lód</w>.",
                            "target_lang_translations": {
                                "ru": "Зимой на озере образуется толстый <w>лёд</w>."
                            }
                        },
                        {
                            "text": "Poproszę colę z dużą ilością <w>lodu</w>.",
                            "target_lang_translations": {
                                "ru": "Дайте, пожалуйста, колу с большим количеством <w>льда</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "woda",
                        "ukrop",
                        "para"
                    ],
                    "common_phrases": [
                        "kostka lodu",
                        "zimny jak lód",
                        "kruszyć lód",
                        "góra lodowa",
                        "przełamać lody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Замёрзшая вода в твёрдом состоянии, твёрдое и холодное вещество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лёд",
                                "target_lang_sense_clarification": "замёрзшая вода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1069d135-e903-37b6-8e48-d2401752781a",
                    "sense_definition": "Słodki, mrożony deser, zazwyczaj na bazie mleka, śmietany lub owoców; często używane w liczbie mnogiej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W upalne dni dzieci uwielbiają jeść <w>lody</w>.",
                            "target_lang_translations": {
                                "ru": "В жаркие дни дети обожают есть <w>мороженое</w>."
                            }
                        },
                        {
                            "text": "Kupiłem sobie pysznego <w>loda</w> na patyku.",
                            "target_lang_translations": {
                                "ru": "Я купил себе вкусное <w>мороженое</w> на палочке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gałka lodów",
                        "lody waniliowe",
                        "iść na lody",
                        "lody na patyku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкий замороженный десерт, обычно на основе молока, сливок или фруктов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мороженое",
                                "target_lang_sense_clarification": "десерт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6be70ab-3bee-3629-a4e0-2f05b31a7f02",
                    "sense_definition": "Powierzchnia pokryta lodem, specjalnie przygotowana do uprawiania sportów zimowych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_ice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hokeiści wyjechali na <w>lód</w> zaraz po przerwie.",
                            "target_lang_translations": {
                                "ru": "Хоккеисты вышли на <w>лёд</w> сразу после перерыва."
                            }
                        },
                        {
                            "text": "Podczas zawodów łyżwiarze tańczyli na <w>lodzie</w>.",
                            "target_lang_translations": {
                                "ru": "Во время соревнований фигуристы танцевали на <w>льду</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lodowisko",
                        "lodowisko",
                        "lodowisko"
                    ],
                    "common_phrases": [
                        "wyjść na lód",
                        "tańczyć na lodzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поверхность, покрытая льдом, специально подготовленная для зимних видов спорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лёд",
                                "target_lang_sense_clarification": "поверхность катка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80af0863-086f-3da6-8dc2-94a42ac997cb",
                    "sense_definition": "Wulgarnie o seksie oralnym polegającym na stymulacji penisa ustami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_sexual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest bardzo wulgarne określenie na czynność seksualną, potocznie zwaną robieniem <w>loda</w>.",
                            "target_lang_translations": {
                                "ru": "Это очень вульгарное название сексуального действия, в просторечии называемого <w>минетом</w>."
                            }
                        },
                        {
                            "text": "W slangu młodzieżowym słowo <w>lód</w> może mieć obraźliwe znaczenie.",
                            "target_lang_translations": {
                                "ru": "В молодёжном сленге это слово может означать <w>минет</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "robić loda"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное название орального секса, заключающегося в стимуляции пениса ртом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "минет",
                                "target_lang_sense_clarification": "оральный секс"
                            }
                        ]
                    }
                }
            ]
        }
    ]

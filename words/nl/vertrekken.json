                        {
                            "text": "Zij luisterde naar het vonnis zonder een spier te <w>vertrekken</w>.",
                            "target_lang_translations": {
                                "en": "She listened to the verdict without <w>moving</w> a muscle."
                            }
                        }
                    ],
                    "common_phrases": [

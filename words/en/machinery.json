    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bcf7b8b4-e7d9-3d57-a3db-e1317b08ab88",
                    "sense_definition": "Machines or equipment used collectively, especially in a factory or industry.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory installed new <w>machinery</w> to increase the speed of production.",
                            "target_lang_translations": {
                                "ru": "Завод установил новое <w>оборудование</w>, чтобы увеличить скорость производства."
                            }
                        },
                        {
                            "text": "Operating heavy <w>machinery</w> requires special training and safety gear.",
                            "target_lang_translations": {
                                "ru": "Управление тяжелой <w>техникой</w> требует специальной подготовки и защитного снаряжения."
                            }
                        }
                    ],
                    "synonyms": [
                        "equipment",
                        "apparatus",
                        "gear"
                    ],
                    "common_phrases": [
                        "heavy machinery",
                        "industrial machinery",
                        "farm machinery",
                        "agricultural machinery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность машин, механизмов или оборудования, используемых в производстве или промышленности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оборудование",
                                "target_lang_sense_clarification": "совокупность устройств"
                            },
                            {
                                "target_lang_word": "техника",
                                "target_lang_sense_clarification": "машины и механизмы"
                            },
                            {
                                "target_lang_word": "машины",
                                "target_lang_sense_clarification": "станки и агрегаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a030950-e1af-3636-a353-5e065616db54",
                    "sense_definition": "The moving or working parts inside a machine.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The delicate <w>machinery</w> inside the watch must be cleaned by a professional.",
                            "target_lang_translations": {
                                "ru": "Тонкий <w>механизм</w> внутри часов должен чистить профессионал."
                            }
                        },
                        {
                            "text": "Rust had damaged the internal <w>machinery</w>, causing the engine to fail.",
                            "target_lang_translations": {
                                "ru": "Ржавчина повредила внутренний <w>механизм</w>, что привело к отказу двигателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "mechanism",
                        "works",
                        "components"
                    ],
                    "common_phrases": [
                        "complex machinery",
                        "internal machinery",
                        "delicate machinery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движущиеся или рабочие части внутри устройства или машины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "механизм",
                                "target_lang_sense_clarification": "внутреннее устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ca8030a-057c-3757-be23-ce3378d31333",
                    "sense_definition": "The system, structure, or organization that allows a government or institution to function.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "system_organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>machinery</w> of government can be slow to react to sudden social changes.",
                            "target_lang_translations": {
                                "ru": "Государственный <w>механизм</w> может медленно реагировать на внезапные социальные изменения."
                            }
                        },
                        {
                            "text": "We need to improve the administrative <w>machinery</w> to process applications more efficiently.",
                            "target_lang_translations": {
                                "ru": "Нам нужно улучшить административный <w>аппарат</w>, чтобы эффективнее обрабатывать заявки."
                            }
                        }
                    ],
                    "synonyms": [
                        "system",
                        "apparatus",
                        "structure",
                        "organization"
                    ],
                    "common_phrases": [
                        "machinery of government",
                        "political machinery",
                        "administrative machinery",
                        "legal machinery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система, структура или организация, обеспечивающая функционирование правительства или учреждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "механизм",
                                "target_lang_sense_clarification": "система функционирования"
                            },
                            {
                                "target_lang_word": "аппарат",
                                "target_lang_sense_clarification": "совокупность учреждений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5c446eb-8402-3f1d-a6f3-abee83485d74",
                    "sense_definition": "The literary devices or techniques used in a story or play to create a specific effect.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The novel relies on the traditional <w>machinery</w> of gothic horror to scare the reader.",
                            "target_lang_translations": {
                                "ru": "Роман опирается на традиционные <w>приёмы</w> готического хоррора, чтобы напугать читателя."
                            }
                        },
                        {
                            "text": "Critics analyzed the dramatic <w>machinery</w> the author used to resolve the complex plot.",
                            "target_lang_translations": {
                                "ru": "Критики проанализировали драматургические <w>средства</w>, использованные автором для разрешения сложного сюжета."
                            }
                        }
                    ],
                    "synonyms": [
                        "devices",
                        "techniques"
                    ],
                    "common_phrases": [
                        "literary machinery"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "literary context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Литературные приёмы или техники, используемые в произведении для создания определённого эффекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приёмы",

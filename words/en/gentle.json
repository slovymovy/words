    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "53567bf0-0333-3fec-b61b-b47dbf211164",
                    "sense_definition": "Having a kind, tender, and calm nature.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "gentle_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stuart is a <w>gentle</w> man; he would never hurt you.",
                            "target_lang_translations": {
                                "ru": "Стюарт — <w>мягкий</w> человек; он никогда тебя не обидит."
                            }
                        },
                        {
                            "text": "She greeted us with a <w>gentle</w> smile on her face.",
                            "target_lang_translations": {
                                "ru": "Она встретила нас с <w>нежной</w> улыбкой на лице."
                            }
                        }
                    ],
                    "synonyms": [
                        "kind",
                        "friendly"
                    ],
                    "common_phrases": [
                        "gentle smile",
                        "gentle voice",
                        "gentle soul"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий добрым, мягким и спокойным характером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мягкий",
                                "target_lang_sense_clarification": "о характере"
                            },
                            {
                                "target_lang_word": "нежный",
                                "target_lang_sense_clarification": "ласковый"
                            },
                            {
                                "target_lang_word": "ласковый",
                                "target_lang_sense_clarification": "проявляющий любовь"
                            },
                            {
                                "target_lang_word": "кроткий",
                                "target_lang_sense_clarification": "незлобивый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0924e58f-cf88-3418-8d36-7cba9cce1186",
                    "sense_definition": "Not strong, rough, or violent; soft and mild in action or effect.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gentle_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I felt something touch my shoulder; it was <w>gentle</w> and a little slimy.",
                            "target_lang_translations": {
                                "ru": "Я почувствовал, как что-то коснулось моего плеча; прикосновение было <w>легким</w> и немного склизким."
                            }
                        },
                        {
                            "text": "A <w>gentle</w> breeze blew through the open window on a summer day.",
                            "target_lang_translations": {
                                "ru": "В летний день в открытое окно дул <w>легкий</w> ветерок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gentle breeze",
                        "gentle touch",
                        "gentle rain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не грубый и не резкий; легкий или мягкий по воздействию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легкий",
                                "target_lang_sense_clarification": "о ветре или прикосновении"
                            },
                            {
                                "target_lang_word": "нежный",
                                "target_lang_sense_clarification": "приятный на ощупь"
                            },
                            {
                                "target_lang_word": "мягкий",
                                "target_lang_sense_clarification": "не жесткий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e442bc9-bef0-3a74-9271-2651514c77c5",
                    "sense_definition": "Calm and easy to control or handle, usually of animals.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gentle_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The horse is very <w>gentle</w> and safe for children to ride.",
                            "target_lang_translations": {
                                "ru": "Эта лошадь очень <w>смирная</w> и безопасна для катания детей."
                            }
                        },
                        {
                            "text": "We need a <w>gentle</w> dog that will get along with the cat.",
                            "target_lang_translations": {
                                "ru": "Нам нужна <w>спокойная</w> собака, которая поладит с кошкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gentle horse",
                        "gentle dog"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спокойный и послушный (обычно о животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смирный",
                                "target_lang_sense_clarification": "тихий, не буйный"
                            },
                            {
                                "target_lang_word": "послушный",
                                "target_lang_sense_clarification": "легко управляемый"
                            },
                            {
                                "target_lang_word": "спокойный",
                                "target_lang_sense_clarification": "уравновешенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fe94b97-f093-3746-97ba-37f236885e2f",
                    "sense_definition": "Not steep or sudden; gradual.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gentle_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The walks in this area have a <w>gentle</w> incline.",
                            "target_lang_translations": {
                                "ru": "Прогулочные тропы в этом районе имеют <w>пологий</w> уклон."
                            }
                        },
                        {
                            "text": "The road went down a <w>gentle</w> slope towards the river.",
                            "target_lang_translations": {
                                "ru": "Дорога спускалась по <w>пологому</w> склону к реке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gentle slope",
                        "gentle incline",
                        "gentle curve"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не крутой; с постепенным уклоном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пологий",
                                "target_lang_sense_clarification": "о склоне или подъеме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ce8bfc8-8175-3815-930a-da8ab89f84cb",
                    "sense_definition": "Showing politeness and respect.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "gentle_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He gave me a <w>gentle</w> reminder that we had to hurry up.",
                            "target_lang_translations": {
                                "ru": "Он сделал мне <w>деликатное</w> напоминание о том, что нам нужно поторопиться."
                            }
                        },
                        {
                            "text": "The teacher gave the student a <w>gentle</w> warning about talking in class.",
                            "target_lang_translations": {
                                "ru": "Учитель сделал ученику <w>мягкое</w> замечание за разговоры на уроке."
                            }
                        }
                    ],
                    "synonyms": [
                        "polite",
                        "respectful"
                    ],
                    "antonyms": [
                        "rude"
                    ],
                    "common_phrases": [
                        "gentle reminder",
                        "gentle hint"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий вежливость и уважение; тактичный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "деликатный",
                                "target_lang_sense_clarification": "тактичный"
                            },
                            {
                                "target_lang_word": "вежливый",
                                "target_lang_sense_clarification": "учтивый"
                            },
                            {
                                "target_lang_word": "мягкий",
                                "target_lang_sense_clarification": "не строгий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9ae2008-33ab-343d-89c6-d7e3d6ca1f53",
                    "sense_definition": "Belonging to a family of high social standing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "gentle_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are of <w>gentle</w> blood, so you must act with honor.",
                            "target_lang_translations": {
                                "ru": "В тебе течет <w>благородная</w> кровь, поэтому ты должен поступать по чести."
                            }
                        },
                        {
                            "text": "In the past, only men of <w>gentle</w> birth could hold such titles.",
                            "target_lang_translations": {
                                "ru": "В прошлом только люди <w>знатного</w> происхождения могли носить такие титулы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gentle birth",
                        "gentle blood"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий к знатному роду или высокому сословию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благородный",
                                "target_lang_sense_clarification": "аристократический"
                            },
                            {
                                "target_lang_word": "знатный",
                                "target_lang_sense_clarification": "высокого происхождения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c20931dc-8bbd-3f7b-b829-6d8c5bba902e",
                    "sense_definition": "To make someone or something calm or soothing.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gentle_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mother <w>gentled</w> the crying baby with a lullaby.",
                            "target_lang_translations": {
                                "ru": "Мать <w>убаюкала</w> плачущего ребенка колыбельной."
                            }
                        },
                        {
                            "text": "Music can help to <w>gentle</w> the mind after a stressful day.",
                            "target_lang_translations": {
                                "ru": "Музыка помогает <w>успокоить</w> ум после напряженного дня."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Успокаивать или делать кого-то более смирным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "успокаивать",
                                "target_lang_sense_clarification": "делать спокойным"
                            },
                            {
                                "target_lang_word": "смягчать",
                                "target_lang_sense_clarification": "делать менее суровым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cae6d431-b982-32b0-ba6f-05cb4260570b",
                    "sense_definition": "To become softer, calmer, or less severe.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gentle_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His tone <w>gentled</w> when he realized she was afraid.",
                            "target_lang_translations": {
                                "ru": "Его тон <w>смягчился</w>, когда он понял, что она напугана."
                            }
                        },
                        {
                            "text": "The wind <w>gentled</w> as the storm passed over.",
                            "target_lang_translations": {
                                "ru": "Ветер <w>стих</w>, когда шторм прошел."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться мягче, спокойнее или тише."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смягчаться",
                                "target_lang_sense_clarification": "становиться добрее"
                            },
                            {
                                "target_lang_word": "стихать",
                                "target_lang_sense_clarification": "о ветре или буре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54d4c0da-19d7-3ae7-b62d-ff870efd6192",
                    "sense_definition": "To tame or break in an animal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gentle_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It takes a lot of patience to <w>gentle</w> a wild mustang.",
                            "target_lang_translations": {
                                "ru": "Требуется много терпения, чтобы <w>объездить</w> дикого мустанга."
                            }
                        },
                        {
                            "text": "He learned how to <w>gentle</w> animals on his grandfather's farm.",
                            "target_lang_translations": {
                                "ru": "Он научился <w>приручать</w> животных на ферме своего деда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gentle a horse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приручать или объезжать животное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объезжать",
                                "target_lang_sense_clarification": "лошадь"
                            },
                            {
                                "target_lang_word": "приручать",
                                "target_lang_sense_clarification": "делать ручным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25b7bb76-a624-3941-bbbc-dc5a74336711",
                    "sense_definition": "To raise to a higher social rank.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king <w>gentled</w> the commoner for his bravery in battle.",
                            "target_lang_translations": {
                                "ru": "Король <w>возвысил</w> простолюдина за его храбрость в битве."
                            }
                        },
                        {
                            "text": "Marriage to a duke <w>gentled</w> her status in society.",
                            "target_lang_translations": {
                                "ru": "Брак с герцогом <w>облагородил</w> ее статус в обществе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвышать в звании или статусе (устар.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облагораживать",
                                "target_lang_sense_clarification": "делать благородным"
                            },
                            {
                                "target_lang_word": "возвышать",
                                "target_lang_sense_clarification": "повышать статус"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "078cc305-670d-3bc3-adf6-75631592f2d4",
                    "sense_definition": "A person of noble or high social class.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>gentles</w> of the court gathered for the feast.",
                            "target_lang_translations": {
                                "ru": "<w>Дворяне</w> двора собрались на пир."
                            }
                        },
                        {
                            "text": "He addressed the crowd of nobles and <w>gentles</w>.",
                            "target_lang_translations": {
                                "ru": "Он обратился к толпе знати и <w>дворян</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""

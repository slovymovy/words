    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "63b3a12d-3564-3fda-a892-dbe61e9f0852",
                    "sense_definition": "Tijdelijke afzondering van personen of dieren om de verspreiding van een besmettelijke ziekte te voorkomen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "isolation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de positieve test moest het hele gezin tien dagen in <w>quarantaine</w> blijven.",
                            "target_lang_translations": {
                                "ru": "После положительного теста всей семье пришлось десять дней оставаться на <w>карантине</w>."
                            }
                        },
                        {
                            "text": "De reizigers gingen direct na aankomst in <w>quarantaine</w> om geen risico te nemen.",
                            "target_lang_translations": {
                                "ru": "Путешественники сразу после прибытия отправились на <w>карантин</w>, чтобы не рисковать."
                            }
                        }
                    ],
                    "synonyms": [
                        "afzondering",
                        "isolatie"
                    ],
                    "common_phrases": [
                        "in quarantaine gaan",
                        "in quarantaine zitten",
                        "uit quarantaine komen",
                        "verplichte quarantaine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временная изоляция людей или животных для предотвращения распространения инфекционного заболевания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карантин",
                                "target_lang_sense_clarification": "медицинская изоляция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87d02d7f-0f9e-3ba2-9d50-67c5c24fa1cb",
                    "sense_definition": "Een situatie van uitsluiting of isolement, vaak op politiek of sociaal vlak, zonder medische oorzaak.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "isolation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door zijn controversiële uitspraken belandde de politicus in een sociale <w>quarantaine</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за своих противоречивых высказываний политик оказался в социальной <w>изоляции</w>."
                            }
                        },
                        {
                            "text": "Het land werd jarenlang in diplomatische <w>quarantaine</w> gehouden door de buurlanden.",
                            "target_lang_translations": {
                                "ru": "Страна годами удерживалась соседними государствами в дипломатической <w>изоляции</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "isolement",
                        "uitsluiting"
                    ],
                    "common_phrases": [
                        "politieke quarantaine",
                        "diplomatieke quarantaine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация исключения или изоляции, часто в политическом или социальном плане, без медицинской причины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изоляция",
                                "target_lang_sense_clarification": "исключение из общества"
                            },
                            {
                                "target_lang_word": "карантин",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

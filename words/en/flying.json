{
    "word_family": [
        "fly",
        "flight",
        "flyer",
        "flier",
        "overfly",
        "outfly"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "576087f1-4e01-3049-b1bd-a89a40cd6a5e",
                    "sense_definition": "The present participle and gerund of <w>fly</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "form_of_fly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Look at that bird <w>flying</w> high in the sky.",
                            "target_lang_translations": {
                                "ru": "Посмотри на ту птицу, <w>летящую</w> высоко в небе."
                            }
                        },
                        {
                            "text": "We are <w>flying</w> to Paris for our vacation next week.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе мы <w>летим</w> в Париж в отпуск."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола <w>fly</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "летящий",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "лететь",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4cfe2c10-2fd9-32d2-9e16-1ad22d982371",
                    "sense_definition": "Moving through the air or capable of flight.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "capable_of_flight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>flying</w> insects were attracted to the bright light.",
                            "target_lang_translations": {
                                "ru": "<w>Летающие</w> насекомые слетались на яркий свет."
                            }
                        },
                        {
                            "text": "Children love stories about <w>flying</w> carpets and magic.",
                            "target_lang_translations": {
                                "ru": "Дети любят истории про <w>летающие</w> ковры и магию."
                            }
                        }
                    ],
                    "antonyms": [
                        "flightless"
                    ],
                    "common_phrases": [
                        "flying saucer",
                        "flying object",
                        "flying carpet",
                        "flying insect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движущийся по воздуху или способный летать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "летающий",
                                "target_lang_sense_clarification": "способный к полету"
                            },
                            {
                                "target_lang_word": "летучий",
                                "target_lang_sense_clarification": "свойство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "874eee44-19a7-3a78-9c21-dc4166058fab",
                    "sense_definition": "Done very quickly; brief or hurried.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hurried",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She paid a <w>flying</w> visit to her parents while passing through the city.",
                            "target_lang_translations": {
                                "ru": "Проезжая через город, она нанесла родителям <w>мимолетный</w> визит."
                            }
                        },
                        {
                            "text": "We only had time for a <w>flying</w> lunch before the meeting started.",
                            "target_lang_translations": {
                                "ru": "У нас было время только на <w>быстрый</w> обед перед началом встречи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "flying visit",
                        "flying start"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершенный очень быстро; краткий или поспешный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мимолетный",
                                "target_lang_sense_clarification": "краткий"
                            },
                            {
                                "target_lang_word": "быстрый",
                                "target_lang_sense_clarification": "скорый"
                            },
                            {
                                "target_lang_word": "поспешный",
                                "target_lang_sense_clarification": "сделанный в спешке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0635d022-982a-3093-915c-ecbc7370060f",
                    "sense_definition": "The activity or skill of traveling through the air in an aircraft or by using wings.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "activity_of_flight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a fear of <w>flying</w> and prefers to take the train.",
                            "target_lang_translations": {
                                "ru": "Он боится <w>полетов</w> и предпочитает ездить на поезде."
                            }
                        },
                        {
                            "text": "<w>Flying</w> is generally considered the safest mode of transport.",
                            "target_lang_translations": {
                                "ru": "<w>Авиаперелеты</w> обычно считаются самым безопасным видом транспорта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fear of flying",
                        "go flying"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность или навык перемещения по воздуху на летательном аппарате или с помощью крыльев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полеты",
                                "target_lang_sense_clarification": "деятельность"
                            },
                            {
                                "target_lang_word": "пилотирование",
                                "target_lang_sense_clarification": "навык управления"
                            },
                            {
                                "target_lang_word": "авиаперелеты",
                                "target_lang_sense_clarification": "путешествие по воздуху"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
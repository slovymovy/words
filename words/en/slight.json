        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0195dc6a-5cca-33ef-a037-410b416c21dc",
                    "sense_definition": "Small in degree, amount, or intensity; not considerable or serious.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "small_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was a <w>slight</w> change in the schedule, but everything else remained the same.",
                            "target_lang_translations": {
                                "ru": "В расписании произошло <w>незначи́тельное</w> изменение, но всё остальное осталось прежним."
                            }
                        },
                        {
                            "text": "I have a <w>slight</w> headache, so I think I will go to bed early tonight.",
                            "target_lang_translations": {
                                "ru": "У меня <w>лёгкая</w> головная боль, поэтому, думаю, я лягу спать пораньше сегодня."
                            }
                        }
                    ],
                    "synonyms": [
                        "minor",
                        "small",
                        "insignificant",
                        "negligible",
                        "trifling",
                        "trivial",
                        "tiny"
                    ],
                    "antonyms": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6c6ae0c2-67dc-369e-8265-3e86c44ca9b7",
                    "sense_definition": "Городской наземный электрический транспорт, движущийся по рельсам, а также отдельный вагон этого транспорта.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я каждое утро езжу на работу на <w>трамвае</w>.",
                            "target_lang_translations": {
                                "en": "I go to work by <w>tram</w> every morning."
                            }
                        },
                        {
                            "text": "К остановке подошёл новый низкопольный <w>трамвай</w>.",
                            "target_lang_translations": {
                                "en": "A new low-floor <w>tram</w> approached the stop."
                            }
                        },
                        {
                            "text": "Мы долго ждали <w>трамвай</w>, но он так и не пришёл.",
                            "target_lang_translations": {
                                "en": "We waited for the <w>tram</w> for a long time, but it never came."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ехать на трамвае",
                        "ждать трамвай",
                        "водитель трамвая",
                        "остановка трамвая",
                        "сесть в трамвай"
                    ],
                    "target_lang_definitions": {
                        "en": "Urban surface electric transport moving on rails, as well as an individual carriage of this transport."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tram",
                                "target_lang_sense_clarification": "British / General"
                            },
                            {
                                "target_lang_word": "streetcar",
                                "target_lang_sense_clarification": "American"
                            },
                            {
                                "target_lang_word": "trolley",
                                "target_lang_sense_clarification": "American"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1480f912-deca-3ecb-83cd-c06c34153402",
                    "sense_definition": "Вид уличного рельсового общественного транспорта как система; совокупность путей и сооружений.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Власти города решили развивать <w>трамвай</w> как экологичный вид транспорта.",
                            "target_lang_translations": {
                                "en": "The city authorities decided to develop the <w>tram system</w> as an eco-friendly mode of transport."
                            }
                        },
                        {
                            "text": "В конце девятнадцатого века в Нижнем Новгороде появился <w>трамвай</w>.",
                            "target_lang_translations": {
                                "en": "At the end of the nineteenth century, the <w>tram</w> appeared in Nizhny Novgorod."
                            }
                        }
                    ],
                    "common_phrases": [
                        "история трамвая",
                        "развитие трамвая",
                        "скоростной трамвай",
                        "речной трамвай"
                    ],
                    "target_lang_definitions": {
                        "en": "A type of street rail public transport as a system; the aggregate of tracks and facilities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tramway",
                                "target_lang_sense_clarification": "infrastructure / system"
                            },
                            {
                                "target_lang_word": "tram system",
                                "target_lang_sense_clarification": "general"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbe3bfab-f3a0-3843-8e27-f8b84c86a2e0",
                    "sense_definition": "Пригородный электропоезд.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "transport_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш <w>трамвай</w> до дачи отправляется с третьего пути.",
                            "target_lang_translations": {
                                "en": "Our <w>commuter train</w> to the dacha departs from track three."
                            }
                        },
                        {
                            "text": "Машинист проверил приборы и повёл свой <w>трамвай</w> из депо.",
                            "target_lang_translations": {
                                "en": "The train driver checked the instruments and drove his <w>train</w> out of the depot."
                            }
                        }
                    ],

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выездной",
                                "target_lang_sense_clarification": "о спортивном матче"
                            },
                            {
                                "target_lang_word": "гостевой",
                                "target_lang_sense_clarification": "относящийся к команде гостей"
                            }
                        ]
                    }
                }
            ]
        },
        {

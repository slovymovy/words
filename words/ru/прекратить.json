{
    "word_family": [
        "прекращение",
        "прекращаться",
        "прекращать"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "19c3404d-a642-3e0f-8ebc-a9732f8911ae",
                    "sense_definition": "Перестать делать что-либо, прервать выполнение какого-либо действия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "stop_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она внезапно <прекратила> играть на пианино и посмотрела в окно.",
                            "target_lang_translations": {
                                "en": "She suddenly <stopped> playing the piano and looked out the window."
                            }
                        },
                        {
                            "text": "Врач посоветовал пациенту немедленно <прекратить> курить ради здоровья.",
                            "target_lang_translations": {
                                "en": "The doctor advised the patient to immediately <stop> smoking for the sake of his health."
                            }
                        }
                    ],
                    "synonyms": [
                        "перестать"
                    ],
                    "antonyms": [
                        "начать"
                    ],
                    "common_phrases": [
                        "прекратить работу",
                        "прекратить разговор",
                        "прекратить огонь"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop doing something, to interrupt the performance of an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "to no longer continue an action"
                            },
                            {
                                "target_lang_word": "cease",
                                "target_lang_sense_clarification": "formal; to stop doing something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f11cd8b-4809-3784-a9b0-89af39d0b31e",
                    "sense_definition": "Положить конец чему-либо, заставить что-либо завершиться или остановиться.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stop_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция быстро вмешалась, чтобы <прекратить> уличные беспорядки.",
                            "target_lang_translations": {
                                "en": "The police quickly intervened to <stop> the street riots."
                            }
                        },
                        {
                            "text": "Мы вынуждены <прекратить> сотрудничество из-за нарушения условий договора.",
                            "target_lang_translations": {
                                "en": "We are forced to <terminate> cooperation due to a violation of the contract terms."
                            }
                        }
                    ],
                    "synonyms": [
                        "закончить",
                        "покончить"
                    ],
                    "antonyms": [
                        "начать",
                        "положить начало"
                    ],
                    "common_phrases": [
                        "прекратить безобразие",
                        "прекратить отношения",
                        "прекратить существование"
                    ],
                    "target_lang_definitions": {
                        "en": "To put an end to something, to cause something to finish or stop."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "terminate",
                                "target_lang_sense_clarification": "formal; to bring to an end, especially a contract or partnership"
                            },
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "to cause an activity or event to end"
                            },
                            {
                                "target_lang_word": "end",
                                "target_lang_sense_clarification": "to bring to a conclusion"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
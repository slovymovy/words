    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12c721eb-4df4-31ff-9ccb-d972f06a9835",
                    "sense_definition": "An unknown quantity or unknown value, typically used in mathematics.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unknown_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this equation, you need to solve for <w>x</w>.",
                            "target_lang_translations": {
                                "ru": "В этом уравнении нужно найти <w>икс</w>."
                            }
                        },
                        {
                            "text": "Let <w>x</w> represent the total distance traveled by the car.",
                            "target_lang_translations": {
                                "ru": "Пусть <w>икс</w> обозначает общее расстояние, пройденное автомобилем."
                            }
                        }
                    ],
                    "synonyms": [
                        "variable",
                        "unknown"
                    ],
                    "antonyms": [
                        "constant"
                    ],
                    "common_phrases": [
                        "solve for x",
                        "find the value of x",
                        "x axis"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "mathematical symbol"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неизвестная величина или переменная, обычно используемая в математике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "икс",
                                "target_lang_sense_clarification": "математическая переменная"
                            },
                            {
                                "target_lang_word": "неизвестное",
                                "target_lang_sense_clarification": "искомая величина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5707673-660a-3831-a046-cb4853adde35",
                    "sense_definition": "Any mark that looks like the letter X, often used to indicate a choice, an error, or a signature for someone who cannot write.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please put an <w>x</w> in the box next to the correct answer.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поставьте <w>крестик</w> в поле рядом с правильным ответом."
                            }
                        },
                        {
                            "text": "He could not write his name, so he signed the contract with a simple <w>x</w>.",
                            "target_lang_translations": {
                                "ru": "Он не умел писать свое имя, поэтому подписал контракт простым <w>крестиком</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cross",
                        "mark"
                    ],
                    "antonyms": [
                        "tick",
                        "check"
                    ],
                    "common_phrases": [
                        "mark with an x",
                        "sign with an x"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак в виде двух пересекающихся линий, используемый для выбора, обозначения ошибки или вместо подписи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крестик",
                                "target_lang_sense_clarification": "пометка"
                            },
                            {
                                "target_lang_word": "галочка",
                                "target_lang_sense_clarification": "знак выбора (в некоторых контекстах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6030bcc-6c83-34a4-b26b-1209899a0b06",
                    "sense_definition": "Ecstasy, a particular street drug.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drug",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police found several pills of <w>X</w> during the raid.",
                            "target_lang_translations": {
                                "ru": "Полиция обнаружила несколько таблеток <w>экстази</w> во время рейда."
                            }
                        },
                        {
                            "text": "He admitted to taking <w>X</w> at the party last night.",
                            "target_lang_translations": {
                                "ru": "Он признался, что принимал <w>экстази</w> на вечеринке прошлой ночью."
                            }
                        }
                    ],
                    "synonyms": [
                        "ecstasy",
                        "MDMA",
                        "E"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Экстази, уличный наркотик."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экстази",
                                "target_lang_sense_clarification": "наркотик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd256c17-d054-3a4f-9025-8d0fdcf99015",
                    "sense_definition": "Cross or crossing; often used in abbreviations.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "abbreviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sign warned of a deer <w>Xing</w> ahead on the road.",
                            "target_lang_translations": {
                                "ru": "Знак предупреждал, что впереди <w>переход</w> для оленей."
                            }
                        },
                        {
                            "text": "We agreed to meet near Charing <w>X</w> station.",
                            "target_lang_translations": {
                                "ru": "Мы договорились встретиться у станции Чаринг-<w>Кросс</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cross",
                        "crossing"
                    ],
                    "common_phrases": [
                        "pedestrian xing"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение для слов 'крест' (cross) или 'переход' (crossing)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "дорожный знак (Xing)"
                            },
                            {
                                "target_lang_word": "Кросс",
                                "target_lang_sense_clarification": "в названиях мест (Charing Cross)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f9638430-3235-37db-83ea-f997494a2a55",

                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sufficiency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mamy <dość> czasu, żeby spokojnie wypić kawę.",
                            "target_lang_translations": {

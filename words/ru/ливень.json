    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "572e7b08-ed2d-32c2-8063-4cfd4790b4a2",
                    "sense_definition": "Очень сильный, проливной дождь.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weather_rain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы не успели спрятаться и попали под сильный <w>ливень</w>.",
                            "target_lang_translations": {
                                "en": "We didn't have time to take cover and got caught in a heavy <w>downpour</w>."
                            }
                        },
                        {
                            "text": "За окном внезапно начался летний <w>ливень</w> с грозой.",
                            "target_lang_translations": {
                                "en": "A summer <w>downpour</w> with a thunderstorm suddenly started outside the window."
                            }
                        }
                    ],
                    "synonyms": [
                        "проливной дождь"
                    ],
                    "antonyms": [
                        "дождик"
                    ],
                    "common_phrases": [
                        "попасть под ливень",
                        "тропический ливень",
                        "летний ливень",
                        "проливной ливень"
                    ],
                    "target_lang_definitions": {
                        "en": "A very heavy, often sudden rainfall."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "downpour",
                                "target_lang_sense_clarification": "heavy rain"
                            },
                            {
                                "target_lang_word": "heavy rain",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "cloudburst",
                                "target_lang_sense_clarification": "sudden violent rain"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16e28dbf-4634-3b67-b58f-f3a48955bbaa",
                    "sense_definition": "Большое количество, бурный поток чего-либо, сыплющегося или падающего (в переносном смысле).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_abundance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После премьеры на артиста обрушился <w>ливень</w> аплодисментов.",
                            "target_lang_translations": {
                                "en": "After the premiere, a <w>storm</w> of applause crashed down on the artist."
                            }
                        },
                        {
                            "text": "В ответ на атаку врага полетел свинцовый <w>ливень</w> пуль.",
                            "target_lang_translations": {
                                "en": "In response to the enemy attack, a leaden <w>hail</w> of bullets flew out."
                            }
                        }
                    ],
                    "synonyms": [
                        "наплыв"
                    ],
                    "antonyms": [
                        "ручеёк"
                    ],
                    "common_phrases": [
                        "ливень аплодисментов",
                        "ливень пуль",
                        "ливень наград"
                    ],
                    "target_lang_definitions": {
                        "en": "A large quantity of something falling or directed at someone (figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hail",
                                "target_lang_sense_clarification": "figurative, e.g. of bullets"
                            },
                            {
                                "target_lang_word": "storm",
                                "target_lang_sense_clarification": "figurative, e.g. of applause"
                            },
                            {
                                "target_lang_word": "shower",
                                "target_lang_sense_clarification": "figurative, e.g. of sparks or compliments"
                            }
                        ]
                    }
                }
            ]
        }
    ]

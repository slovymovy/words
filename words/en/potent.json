    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4d538954-3800-3a61-95a9-5ee276717bd5",
                    "sense_definition": "Having a very strong effect on the body or mind, often regarding drugs, alcohol, or smells.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_strength",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bartender mixed a <w>potent</w> cocktail that made everyone feel dizzy after just one glass.",
                            "target_lang_translations": {
                                "ru": "Бармен смешал <w>крепкий</w> коктейль, от которого у всех закружилась голова после первого же бокала."
                            }
                        },
                        {
                            "text": "The doctor prescribed a <w>potent</w> medicine to fight the stubborn infection.",
                            "target_lang_translations": {
                                "ru": "Врач выписал <w>сильнодействующее</w> лекарство для борьбы со стойкой инфекцией."
                            }
                        }
                    ],
                    "synonyms": [
                        "strong",
                        "concentrated",
                        "intoxicating"
                    ],
                    "antonyms": [
                        "counterpotent",
                        "impotent"
                    ],
                    "common_phrases": [
                        "potent drug",
                        "potent smell",
                        "potent drink",
                        "potent mixture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывающий сильное воздействие на тело или разум (о лекарствах, алкоголе или запахах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сильнодействующий",
                                "target_lang_sense_clarification": "о лекарствах или веществах"
                            },
                            {
                                "target_lang_word": "крепкий",
                                "target_lang_sense_clarification": "об алкоголе или напитках"
                            },
                            {
                                "target_lang_word": "сильный",
                                "target_lang_sense_clarification": "о запахе или эффекте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fab0029-7b2c-306d-877b-efb40c3f613e",
                    "sense_definition": "Possessing authority or influence; highly persuasive and convincing.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer presented a <w>potent</w> argument that convinced the jury of the defendant's innocence.",
                            "target_lang_translations": {
                                "ru": "Адвокат представил <w>веский</w> аргумент, который убедил присяжных в невиновности подсудимого."
                            }
                        },
                        {
                            "text": "Music can be a <w>potent</w> symbol of cultural identity for many people.",
                            "target_lang_translations": {
                                "ru": "Музыка может быть <w>мощным</w> символом культурной идентичности для многих людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "compelling",
                        "convincing",
                        "persuasive",
                        "influential"
                    ],
                    "common_phrases": [
                        "potent argument",
                        "potent symbol",
                        "potent force",
                        "potent reason"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий авторитетом или влиянием; очень убедительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убедительный",
                                "target_lang_sense_clarification": "об аргументах или доводах"
                            },
                            {
                                "target_lang_word": "веский",
                                "target_lang_sense_clarification": "о причинах или доказательствах"
                            },
                            {
                                "target_lang_word": "мощный",
                                "target_lang_sense_clarification": "о символах или влиянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "486b7976-2ffc-3299-87e2-75ae510c91b9",
                    "sense_definition": "Possessing great power, strength, or effectiveness.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country developed a <w>potent</w> military force to defend its borders.",
                            "target_lang_translations": {
                                "ru": "Страна создала <w>мощную</w> военную силу для защиты своих границ."
                            }
                        },
                        {
                            "text": "She realized that her words had a <w>potent</w> impact on the crowd.",
                            "target_lang_translations": {
                                "ru": "Она поняла, что её слова оказали <w>сильное</w> воздействие на толпу."
                            }
                        }
                    ],
                    "synonyms": [
                        "powerful",
                        "effective",
                        "mighty"
                    ],
                    "antonyms": [
                        "impotent",
                        "counterpotent"
                    ],
                    "common_phrases": [
                        "potent weapon",
                        "potent remedy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий огромной силой, мощью или эффективностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мощный",
                                "target_lang_sense_clarification": "о силе, оружии или средствах"
                            },
                            {
                                "target_lang_word": "могущественный",

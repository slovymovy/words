                {
                    "sense_id": "4f45aa40-84c3-3192-a151-973beab421e8",
                    "sense_definition": "Zwierzę hodowlane z rogami i bródką, dające mleko, mięso i wełnę.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_mammal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta biała <koza> daje bardzo dużo zdrowego mleka.",
                            "target_lang_translations": {
                                "ru": "Эта белая <коза> дает очень много полезного молока."
                            }
                        },
                        {
                            "text": "Na zielonej łące pasła się <koza> ze swoimi młodymi.",
                            "target_lang_translations": {
                                "ru": "На зеленом лугу паслась <коза> со своими козлятами."
                            }
                        }
                    ],
                    "synonyms": [
                        "kózka"
                    ],
                    "common_phrases": [
                        "uparty jak koza",
                        "doić kozę",
                        "skakać jak koza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Домашнее животное с рогами и бородкой, дающее молоко, мясо и шерсть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коза",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]

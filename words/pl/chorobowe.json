    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "11aba948-b55c-3d60-a07f-987e0a8855f0",
                    "sense_definition": "Płatne zwolnienie z pracy udzielane pracownikowi na czas powrotu do zdrowia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sick_leave_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek jest na <w>chorobowym</w> od zeszłego tygodnia z powodu grypy.",
                            "target_lang_translations": {
                                "ru": "Марек на <w>больничном</w> с прошлой недели из-за гриппа."
                            }
                        },
                        {
                            "text": "Muszę iść na <w>chorobowe</w>, ponieważ mam wysoką gorączkę i nie mogę pracować.",
                            "target_lang_translations": {
                                "ru": "Мне нужно уйти на <w>больничный</w>, так как у меня высокая температура, и я не могу работать."
                            }
                        }
                    ],
                    "synonyms": [
                        "L4",
                        "zwolnienie lekarskie"
                    ],
                    "common_phrases": [
                        "iść na chorobowe",
                        "być na chorobowym",
                        "wrócić z chorobowego"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common term for sick leave"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оплачиваемый отпуск, предоставляемый работнику на время восстановления здоровья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "больничный",
                                "target_lang_sense_clarification": "разг. отпуск по болезни"
                            },
                            {
                                "target_lang_word": "больничный лист",
                                "target_lang_sense_clarification": "документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5677cf8-cb47-37f0-a0a0-845626ec2cb1",
                    "sense_definition": "Świadczenie pieniężne wypłacane pracownikowi za okres niezdolności do pracy spowodowanej chorobą.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sick_leave_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym miesiącu dostałem niższe <w>chorobowe</w>, niż się spodziewałem.",
                            "target_lang_translations": {
                                "ru": "В этом месяце я получил <w>больничные</w> меньше, чем ожидал."
                            }
                        },
                        {
                            "text": "Firma wypłaciła mi zaległe <w>chorobowe</w> dopiero wczoraj.",
                            "target_lang_translations": {
                                "ru": "Фирма выплатила мне задержанные <w>больничные</w> только вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "zasiłek chorobowy"
                    ],
                    "common_phrases": [
                        "dostać chorobowe",
                        "wypłacić chorobowe",
                        "wysokość chorobowego"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "refers to the money/benefit"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежное пособие, выплачиваемое работнику за период нетрудоспособности, вызванной болезнью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "больничные",
                                "target_lang_sense_clarification": "разг. выплата"
                            },
                            {
                                "target_lang_word": "пособие по болезни",
                                "target_lang_sense_clarification": "официальный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a3788800-faa3-3f9c-8ae6-f02e96f9c3c4",
                    "sense_definition": "Forma rodzaju nijakiego liczby pojedynczej oraz rodzaju niemęskoosobowego liczby mnogiej przymiotnika <w>chorobowy</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy pracownik musi obowiązkowo płacić ubezpieczenie <w>chorobowe</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый работник обязан платить взносы на <w>страхование на случай болезни</w>."
                            }
                        },
                        {
                            "text": "Lekarz zauważył u pacjenta niepokojące zmiany <w>chorobowe</w> na skórze.",
                            "target_lang_translations": {
                                "ru": "Врач заметил у пациента тревожные <w>патологические</w> изменения на коже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ubezpieczenie chorobowe",
                        "objawy chorobowe",
                        "zmiany chorobowe"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма среднего рода единственного числа и форма множественного числа прилагательного «chorobowy» (связанный с болезнью)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связанный с болезнью",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "патологический",
                                "target_lang_sense_clarification": "об изменениях в организме"
                            }
                        ]
                    }
                }
            ]
        }
    ]

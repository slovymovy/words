                    "synonyms": [
                        "в обмен на"
                    ],
                    "common_phrases": [
                        "спасибо за всё",
                        "купить за деньги",
                        "глаз за глаз"
                    ]
                },
                {
                    "sense_id": "35389e52-f37a-3735-b8ea-c5ef1dc1c651",

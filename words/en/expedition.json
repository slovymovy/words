                    "frequency": "Middle",
                    "semantic_group_id": "journey_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>expedition</w> arrived safely at the base camp before sunset.",
                            "target_lang_translations": {
                                "ru": "<w>Экспедиция</w> благополучно прибыла в базовый лагерь до заката."
                            }

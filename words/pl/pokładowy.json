                {
                    "sense_id": "766e3c55-b6fb-33ce-96c3-ba6f9e2cc393",
                    "sense_definition": "dotyczący pokładu statku, samolotu lub innego pojazdu; znajdujący się lub odbywający się na pokładzie",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wejściem do samolotu każdy pasażer musi okazać swoją kartę <w>pokładową</w>.",
                            "target_lang_translations": {
                                "ru": "Перед посадкой в самолет каждый пассажир должен предъявить свой <w>посадочный</w> талон."
                            }
                        },
                        {
                            "text": "Personel <w>pokładowy</w> poinstruował podróżnych o zasadach bezpieczeństwa.",
                            "target_lang_translations": {
                                "ru": "<w>Бортовой</w> персонал проинструктировал путешественников о правилах безопасности."
                            }
                        }
                    ],
                    "antonyms": [
                        "naziemny"
                    ],
                    "common_phrases": [
                        "karta pokładowa",
                        "personel pokładowy",
                        "komputer pokładowy",
                        "dziennik pokładowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к борту судна, самолета или другого транспортного средства; находящийся или происходящий на борту"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бортовой",
                                "target_lang_sense_clarification": "находящийся на борту (самолета, судна)"
                            },
                            {
                                "target_lang_word": "палубный",
                                "target_lang_sense_clarification": "относящийся к палубе судна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69af0c2d-a93e-30c5-aaf1-b979f2ebbdaa",
                    "sense_definition": "związany z pokładem geologicznym, czyli naturalną warstwą skał lub surowców",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {

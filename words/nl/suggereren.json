{
    "word_family": [
        "suggestie",
        "suggestief"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b0bf694e-1839-37a0-b0f5-4632c178a097",
                    "sense_definition": "Iets voorstellen of als idee naar voren brengen om te overwegen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "propose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als dat niet lukt, <w>suggereer</w> ik om terug te gaan naar de winkel.",
                            "target_lang_translations": {
                                "en": "If that doesn't work, I <w>suggest</w> going back to the store.",
                                "ru": "Если это не сработает, я <w>предлагаю</w> вернуться в магазин."
                            }
                        },
                        {
                            "text": "De leraar <w>suggereerde</w> dat we het hoofdstuk nog eens moesten lezen voor de toets.",
                            "target_lang_translations": {
                                "en": "The teacher <w>suggested</w> that we read the chapter again for the test.",
                                "ru": "Учитель <w>предложил</w>, чтобы мы перечитали главу перед контрольной."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorstellen",
                        "opperen"
                    ],
                    "common_phrases": [
                        "een oplossing suggereren",
                        "voorzichtig suggereren"
                    ],
                    "target_lang_definitions": {
                        "en": "To put forward an idea or plan for consideration.",
                        "ru": "Предлагать что-либо или выдвигать идею для рассмотрения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suggest",
                                "target_lang_sense_clarification": "to mention an idea"
                            },
                            {
                                "target_lang_word": "propose",
                                "target_lang_sense_clarification": "to offer a plan"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "предлагать",
                                "target_lang_sense_clarification": "выдвигать идею или план"
                            },
                            {
                                "target_lang_word": "советовать",
                                "target_lang_sense_clarification": "давать рекомендацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4f3bb52-652b-3f91-98ae-2231bbf5e5f7",
                    "sense_definition": "De indruk wekken dat iets zo is, zonder het letterlijk te zeggen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "imply",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Diverse onderzoeken <w>suggereren</w> dat er meer aan de hand is met het klimaat.",
                            "target_lang_translations": {
                                "en": "Various studies <w>suggest</w> that there is more going on with the climate.",
                                "ru": "Различные исследования <w>указывают</w> на то, что с климатом происходит нечто серьезное."
                            }
                        },
                        {
                            "text": "Zijn houding <w>suggereerde</w> dat hij eigenlijk geen zin had om mee te gaan.",
                            "target_lang_translations": {
                                "en": "His attitude <w>suggested</w> that he actually didn't feel like going along.",
                                "ru": "Его поведение <w>намекало</w> на то, что на самом деле он не хотел идти."
                            }
                        }
                    ],
                    "synonyms": [
                        "influisteren",
                        "opperen",
                        "voorstellen"
                    ],
                    "common_phrases": [
                        "onderzoek suggereert",
                        "de titel suggereert"
                    ],
                    "target_lang_definitions": {
                        "en": "To give the impression that something is true without stating it directly.",
                        "ru": "Создавать впечатление, что что-то является правдой, не говоря об этом прямо; намекать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suggest",
                                "target_lang_sense_clarification": "to indicate or imply"
                            },
                            {
                                "target_lang_word": "imply",
                                "target_lang_sense_clarification": "to indicate indirectly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "намекать",
                                "target_lang_sense_clarification": "косвенно давать понять"
                            },
                            {
                                "target_lang_word": "наводить на мысль",
                                "target_lang_sense_clarification": "создавать впечатление"
                            },
                            {
                                "target_lang_word": "указывать",
                                "target_lang_sense_clarification": "свидетельствовать (об исследованиях, фактах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
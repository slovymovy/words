{
    "word_family": [
        "regen",
        "regenen",
        "beregening"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b386337c-2cdf-3e01-8c4c-9048c307027f",
                    "sense_definition": "Gewassen of grond kunstmatig van water voorzien, alsof het regent.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "irrigation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer moet zijn akkers <w>beregenen</w> omdat het al weken niet heeft geregend.",
                            "target_lang_translations": {
                                "ru": "Фермер должен <w>орошать</w> свои поля, так как дождя не было уже несколько недель."
                            }
                        },
                        {
                            "text": "Voor het <w>beregenen</w> van gewassen wordt vaak water uit de sloot gebruikt.",
                            "target_lang_translations": {
                                "ru": "Для <w>полива</w> сельскохозяйственных культур часто используется вода из канавы."
                            }
                        }
                    ],
                    "synonyms": [
                        "besproeien",
                        "irrigeren"
                    ],
                    "common_phrases": [
                        "gewassen beregenen",
                        "het land beregenen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственно снабжать посевы или почву водой, имитируя дождь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орошать",
                                "target_lang_sense_clarification": "снабжать водой поля и посевы"
                            },
                            {
                                "target_lang_word": "поливать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "дождевать",
                                "target_lang_sense_clarification": "технический термин, полив дождевальными установками"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61fa789c-026a-3661-b0a1-4b97d573c8a4",
                    "sense_definition": "Fruitbomen met water besproeien om de bloesem door een ijslaagje tegen nachtvorst te beschermen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "frost_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fruittelers <w>beregenen</w> de appelbomen om de kwetsbare bloesem te redden.",
                            "target_lang_translations": {
                                "ru": "Садоводы <w>опрыскивают</w> яблони, чтобы спасти нежные цветки."
                            }
                        },
                        {
                            "text": "Bij nachtvorst in het voorjaar moeten we de boomgaard de hele nacht <w>beregenen</w>.",
                            "target_lang_translations": {
                                "ru": "При весенних ночных заморозках нам приходится всю ночь <w>орошать</w> фруктовый сад."
                            }
                        }
                    ],
                    "synonyms": [
                        "besproeien"
                    ],
                    "common_phrases": [
                        "tegen nachtvorst beregenen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опрыскивать фруктовые деревья водой для защиты цветков от ночных заморозков ледяной коркой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орошать",
                                "target_lang_sense_clarification": "для защиты от заморозков"
                            },
                            {
                                "target_lang_word": "опрыскивать",
                                "target_lang_sense_clarification": "покрывать брызгами воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0177b075-e830-3030-9ee2-c9748f6f115b",
                    "sense_definition": "Nat worden door vallende regen of door regen getroffen worden.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het oude standbeeld werd eeuwenlang <w>beregend</w> en is daardoor verweerd.",
                            "target_lang_translations": {
                                "ru": "Старая статуя веками <w>омывалась дождем</w> и из-за этого выветрилась."
                            }
                        },
                        {
                            "text": "De eenzame wandelaar werd tijdens zijn tocht zwaar <w>beregend</w>.",
                            "target_lang_translations": {
                                "ru": "Одинокий путник сильно <w>промок под дождем</w> во время своего похода."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Намокать под дождем или подвергаться воздействию дождя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "омывать",
                                "target_lang_sense_clarification": "о воздействии дождя на объекты (статуи, здания)"
                            },
                            {
                                "target_lang_word": "мочить",
                                "target_lang_sense_clarification": "делать мокрым (о дожде)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
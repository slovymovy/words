                {
                    "sense_id": "c35dba24-c497-3cd0-8943-0baf991671e9",
                    "sense_definition": "Dawne lub gwarowe określenie młodej dziewczyny lub panny (obecnie rzadkie i może być mylone z wulgaryzmem).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "girl_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We wsi mieszkała urodziwa <w>dziwka</w>, o której rękę starali się wszyscy kawalerowie.",
                            "target_lang_translations": {
                                "ru": "В деревне жила красивая <w>девка</w>, руки которой добивались все кавалеры."
                            }
                        },
                        {
                            "text": "W dawnej polszczyźnie słowo <w>dziwka</w> nie było obraźliwe i oznaczało po prostu dziewczynę.",
                            "target_lang_translations": {
                                "ru": "В старопольском языке слово <w>девка</w> не было оскорбительным и означало просто девушку."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziewczyna",
                        "panna",
                        "dziewka"
                    ],
                    "traits": [

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "881e20a3-2fb2-3401-a3be-85ddb0f57cf0",
                    "sense_definition": "Związany ze świętami, dotyczący świąt (najczęściej Bożego Narodzenia lub Wielkanocy).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W grudniu wysyłamy rodzinie kartki <w>świąteczne</w> z życzeniami.",
                            "target_lang_translations": {
                                "ru": "В декабре мы отправляем семье <w>праздничные</w> открытки с пожеланиями."
                            }
                        },
                        {
                            "text": "Na stole pojawiły się tradycyjne potrawy <w>świąteczne</w>.",
                            "target_lang_translations": {
                                "ru": "На столе появились традиционные <w>праздничные</w> блюда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "okres świąteczny",
                        "drzewko świąteczne",
                        "kartka świąteczna",
                        "porządki świąteczne",
                        "stół świąteczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с праздниками, относящийся к праздникам (чаще всего к Рождеству или Пасхе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "праздничный",
                                "target_lang_sense_clarification": "относящийся к празднику"
                            },
                            {
                                "target_lang_word": "рождественский",
                                "target_lang_sense_clarification": "относящийся к Рождеству (частый контекст в польском)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "540dbdbd-ac46-3a72-a234-3402aef8de12",
                    "sense_definition": "Przypadający w dzień będący świętem; wolny od pracy lub szkoły.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W poniedziałek <w>świąteczny</w> większość sklepów będzie zamknięta.",
                            "target_lang_translations": {
                                "ru": "В <w>праздничный</w> понедельник большинство магазинов будет закрыто."
                            }
                        },
                        {
                            "text": "Autobusy kursują dzisiaj według rozkładu <w>świątecznego</w>.",
                            "target_lang_translations": {
                                "ru": "Автобусы ходят сегодня по <w>праздничному</w> расписанию."
                            }
                        }
                    ],
                    "antonyms": [
                        "roboczy",
                        "powszedni"
                    ],
                    "common_phrases": [
                        "dzień świąteczny",
                        "przerwa świąteczna",
                        "rozkład świąteczny",
                        "czas świąteczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпадающий на день праздника; свободный от работы или учебы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "праздничный",
                                "target_lang_sense_clarification": "о дне, расписании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03a73eef-2108-3454-bf9b-ff2fa3974f45",
                    "sense_definition": "Uroczysty, podniosły, wyjątkowy; przeznaczony na specjalne okazje.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "qualitative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki dekoracjom w całym domu zapanował <w>świąteczny</w> nastrój.",
                            "target_lang_translations": {
                                "ru": "Благодаря украшениям во всем доме воцарилось <w>праздничное</w> настроение."
                            }
                        },
                        {
                            "text": "Wszyscy goście założyli elegancki, <w>świąteczny</w> ubiór.",
                            "target_lang_translations": {
                                "ru": "Все гости надели элегантный <w>праздничный</w> наряд."
                            }
                        }
                    ],
                    "synonyms": [
                        "uroczysty",
                        "odświętny"
                    ],
                    "antonyms": [
                        "codzienny",
                        "powszedni"
                    ],
                    "common_phrases": [
                        "świąteczny nastrój",
                        "świąteczna atmosfera",
                        "świąteczny strój",
                        "świąteczny obiad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественный, возвышенный, особенный; предназначенный для особых случаев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "праздничный",
                                "target_lang_sense_clarification": "настроение, атмосфера"
                            },
                            {
                                "target_lang_word": "торжественный",
                                "target_lang_sense_clarification": "возвышенный, официальный"
                            },
                            {
                                "target_lang_word": "нарядный",
                                "target_lang_sense_clarification": "об одежде, внешнем виде"
                            }
                        ]
                    }
                }
            ]

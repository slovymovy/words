    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "625f91cc-e9a7-3afa-8de2-54a39e693f1e",
                    "sense_definition": "Celowe naginanie lub przeinaczanie faktów w celu wpłynięcia na czyjeś poglądy, emocje lub zachowanie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "social_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Długo wierzyła w jego historyjki, aż wreszcie poznała się na jego <manipulacjach>.",
                            "target_lang_translations": {
                                "ru": "Она долго верила его историям, пока наконец не раскусила его <манипуляции>."
                            }
                        },
                        {
                            "text": "Polityk stosował <manipulację> faktami, aby przekonać niepewnych wyborców do swojego programu.",
                            "target_lang_translations": {
                                "ru": "Политик использовал <манипуляцию> фактами, чтобы убедить сомневающихся избирателей в своей программе."
                            }
                        }
                    ],
                    "synonyms": [
                        "krętactwo"
                    ],
                    "common_phrases": [
                        "manipulacja faktami",
                        "manipulacja emocjonalna",
                        "ulec manipulacji",
                        "techniki manipulacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренное искажение фактов с целью влияния на чьи-либо взгляды, эмоции или поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манипуляция",
                                "target_lang_sense_clarification": "психологическое воздействие"
                            },
                            {
                                "target_lang_word": "манипулирование",
                                "target_lang_sense_clarification": "процесс воздействия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e1313be-53ff-3d4f-ab94-00cb0b7cb3b5",
                    "sense_definition": "Celowe i nieuczciwe wykorzystanie sytuacji dla osiągnięcia własnych korzyści, często materialnych.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor został zwolniony za <manipulacje> finansowe na szkodę spółki.",
                            "target_lang_translations": {
                                "ru": "Директор был уволен за финансовые <манипуляции> в ущерб компании."
                            }
                        },
                        {
                            "text": "To była zwykła <manipulacja>, mająca na celu wyłudzenie pieniędzy od nieświadomych klientów.",
                            "target_lang_translations": {
                                "ru": "Это была обычная <манипуляция> с целью выманивания денег у неосведомленных клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "machlojka"
                    ],
                    "common_phrases": [
                        "manipulacja finansowa",
                        "manipulacja giełdowa",
                        "dopuścić się manipulacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренное и нечестное использование ситуации для получения собственной выгоды, часто материальной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "махинация",
                                "target_lang_sense_clarification": "нечестная сделка, жульничество"
                            },
                            {
                                "target_lang_word": "манипуляция",
                                "target_lang_sense_clarification": "финансовая или рыночная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12398fe7-b0df-3dc6-9a07-21a6d09ba11f",
                    "sense_definition": "Ręczne wykonywanie czynności wymagających precyzji, zręczności lub obsługa skomplikowanego urządzenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Janek wykonywał jakieś <manipulacje> przy gniazdku elektrycznym.",
                            "target_lang_translations": {
                                "ru": "Янек проводил какие-то <манипуляции> с электрической розеткой."
                            }
                        },
                        {
                            "text": "Każda <manipulacja> przy tym delikatnym mechanizmie wymaga dużej ostrożności.",
                            "target_lang_translations": {
                                "ru": "Любая <манипуляция> с этим хрупким механизмом требует большой осторожности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "manipulacja genetyczna",
                        "precyzyjna manipulacja",
                        "manipulacja danymi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнение действий вручную, требующих точности, или управление сложным устройством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манипуляция",
                                "target_lang_sense_clarification": "действие с объектом"
                            },
                            {
                                "target_lang_word": "операция",
                                "target_lang_sense_clarification": "техническое действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

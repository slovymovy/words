    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c05be828-7263-3599-b28e-11f7aa2d1185",
                    "sense_definition": "The quality or state of being not needed or required.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sheer <w>unnecessity</w> of the new policy frustrated the employees.",
                            "target_lang_translations": {
                                "ru": "Явная <w>ненужность</w> новой политики разочаровала сотрудников."
                            }
                        },
                        {
                            "text": "He argued about the <w>unnecessity</w> of taking such a long route home.",
                            "target_lang_translations": {
                                "ru": "Он доказывал <w>ненужность</w> того, чтобы идти домой таким длинным путём."
                            }
                        }
                    ],
                    "synonyms": [
                        "needlessness",
                        "superfluity",
                        "uselessness"
                    ],
                    "antonyms": [
                        "necessity",
                        "need",
                        "indispensability"
                    ],
                    "common_phrases": [
                        "sheer unnecessity",
                        "absolute unnecessity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество или состояние, при котором что-либо не является необходимым или требуемым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ненужность",
                                "target_lang_sense_clarification": "отсутствие надобности или необходимости"
                            },
                            {
                                "target_lang_word": "бесполезность",
                                "target_lang_sense_clarification": "отсутствие пользы или смысла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25f0d188-f5d7-3206-a926-880fb2bee17a",
                    "sense_definition": "Something that is not essential or needed.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We removed all <w>unnecessities</w> from our budget to save for the trip.",
                            "target_lang_translations": {
                                "ru": "Мы исключили все <w>излишества</w> из нашего бюджета, чтобы сэкономить на поездку."
                            }
                        },
                        {
                            "text": "The room was filled with expensive <w>unnecessities</w> that served no real purpose.",
                            "target_lang_translations": {
                                "ru": "Комната была наполнена дорогими <w>ненужными вещами</w>, которые не служили никакой реальной цели."
                            }
                        }
                    ],
                    "synonyms": [
                        "luxury",
                        "extra",
                        "nonessential"
                    ],
                    "antonyms": [
                        "necessity",
                        "essential",
                        "requirement"
                    ],
                    "common_phrases": [
                        "expensive unnecessities",
                        "life's unnecessities"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещь или предмет, который не является существенным или необходимым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "излишество",
                                "target_lang_sense_clarification": "то, что превышает необходимое; предмет роскоши"
                            },
                            {
                                "target_lang_word": "ненужная вещь",
                                "target_lang_sense_clarification": "предмет, без которого можно обойтись"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    }
                },
                {
                    "sense_id": "b3d547bb-bc91-3899-85eb-5e11a183f311",
                    "sense_definition": "Excellent, effective, or accomplished with great skill.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother makes a <w>mean</w> chocolate cake.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка печет <w>отменный</w> шоколадный торт."
                            }
                        },
                        {
                            "text": "He plays a <w>mean</w> guitar when he is in the mood.",
                            "target_lang_translations": {
                                "ru": "Он <w>классно</w> играет на гитаре, когда он в настроении."
                            }
                        }
                    ],
                    "synonyms": [
                        "skillful",
                        "deft",
                        "top-notch"
                    ],
                    "common_phrases": [
                        "play a mean",
                        "make a mean"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

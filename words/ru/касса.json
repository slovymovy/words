                            },
                            {
                                "target_lang_word": "till",
                                "target_lang_sense_clarification": "machine (often UK)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6e5fc67-02dd-312d-bb13-be37de42f972",

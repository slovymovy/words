                            {
                                "target_lang_word": "выкл.",
                                "target_lang_sense_clarification": "сокращение на приборах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f749fe2a-233d-3bc8-a444-1774c60e610d",

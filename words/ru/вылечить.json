    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2d7aa791-f244-3172-b3a2-ecc509284f2d",
                    "sense_definition": "Восстановить здоровье, устранить болезнь с помощью лечения.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "medical_cure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач обещал быстро <вылечить> мою простуду.",
                            "target_lang_translations": {
                                "en": "The doctor promised to quickly <cure> my cold."
                            }
                        },
                        {
                            "text": "Современная медицина может <вылечить> многие опасные болезни.",
                            "target_lang_translations": {
                                "en": "Modern medicine can <cure> many dangerous diseases."
                            }
                        }
                    ],
                    "synonyms": [
                        "излечить",
                        "исцелить"
                    ],
                    "common_phrases": [
                        "вылечить простуду",
                        "вылечить зубы",
                        "вылечить пациента",
                        "полностью вылечить"
                    ],
                    "target_lang_definitions": {
                        "en": "To restore health or eliminate a disease through medical treatment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cure",
                                "target_lang_sense_clarification": "to eliminate a disease or illness"
                            },
                            {
                                "target_lang_word": "heal",
                                "target_lang_sense_clarification": "to restore to health"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32a308c3-603a-3827-900d-607e80b056d9",
                    "sense_definition": "Избавить от чего-либо вредного, нежелательного или от дурной привычки (переносное значение).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_correction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Армия быстро <вылечит> его от лени и безответственности.",
                            "target_lang_translations": {
                                "en": "The army will quickly <cure> him of laziness and irresponsibility."
                            }
                        },
                        {
                            "text": "Только время сможет <вылечить> её от этой глубокой печали.",
                            "target_lang_translations": {
                                "en": "Only time will be able to <cure> her of this deep sorrow."
                            }
                        }
                    ],
                    "synonyms": [
                        "излечить"
                    ],
                    "common_phrases": [
                        "вылечить от лени",
                        "вылечить от привычки",
                        "вылечить от страха"
                    ],
                    "target_lang_definitions": {
                        "en": "To free someone from a bad habit, negative trait, or emotional pain."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cure",
                                "target_lang_sense_clarification": "to rid someone of a habit or trait"
                            },
                            {
                                "target_lang_word": "heal",
                                "target_lang_sense_clarification": "to alleviate emotional distress"
                            }
                        ]
                    }
                }
            ]
        }
    ]

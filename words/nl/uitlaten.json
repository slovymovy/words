{
    "word_family": [
        "uitlating",
        "uitlaat"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "22c3258f-cfad-3d18-b6ce-12560c68608a",
                    "sense_definition": "Een huisdier (meestal een hond) mee naar buiten nemen zodat het kan bewegen en zijn behoefte kan doen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "walk_pet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga even de hond <w>uitlaten</w> in het park.",
                            "target_lang_translations": {
                                "en": "I'm going to <w>walk</w> the dog in the park for a bit.",
                                "ru": "Я пойду <w>выгуляю</w> собаку в парке."
                            }
                        },
                        {
                            "text": "Hij <w>laat</w> zijn hond elke ochtend vroeg <w>uit</w>.",
                            "target_lang_translations": {
                                "en": "He <w>walks</w> his dog early every morning.",
                                "ru": "Он <w>выгуливает</w> свою собаку каждое утро рано."
                            }
                        }
                    ],
                    "synonyms": [
                        "wandelen met"
                    ],
                    "common_phrases": [
                        "de hond uitlaten"
                    ],
                    "target_lang_definitions": {
                        "en": "To take a pet (usually a dog) outside so it can exercise and relieve itself.",
                        "ru": "Выводить домашнее животное (обычно собаку) на улицу для прогулки и туалета."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walk",
                                "target_lang_sense_clarification": "a dog or pet"
                            },
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "bring outside"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выгуливать",
                                "target_lang_sense_clarification": "гулять с животным"
                            },
                            {
                                "target_lang_word": "гулять с",
                                "target_lang_sense_clarification": "проводить время на улице с кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e05b2d9-6cd5-38b0-b4c2-e69efd0c2d35",
                    "sense_definition": "Iemand naar de uitgang begeleiden bij vertrek, of iemand of iets naar buiten laten gaan.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "escort_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil jij de gasten even <w>uitlaten</w>?",
                            "target_lang_translations": {
                                "en": "Could you <w>show</w> the guests <w>out</w>?",
                                "ru": "Ты не <w>проводишь</w> гостей?"
                            }
                        },
                        {
                            "text": "De boer <w>liet</w> de koeien na de winter weer <w>uit</w>.",
                            "target_lang_translations": {
                                "en": "The farmer <w>let</w> the cows <w>out</w> again after winter.",
                                "ru": "Фермер снова <w>выпустил</w> коров после зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitgeleiden",
                        "naar buiten laten"
                    ],
                    "antonyms": [
                        "binnenlaten"
                    ],
                    "common_phrases": [
                        "het bezoek uitlaten"
                    ],
                    "target_lang_definitions": {
                        "en": "To guide someone to the exit upon departure, or to allow someone or something to go outside.",
                        "ru": "Провожать кого-то к выходу или выпускать кого-то или что-то наружу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show out",
                                "target_lang_sense_clarification": "guests or visitors"
                            },
                            {
                                "target_lang_word": "see out",
                                "target_lang_sense_clarification": "escort to the door"
                            },
                            {
                                "target_lang_word": "let out",
                                "target_lang_sense_clarification": "animals or people"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "провожать",
                                "target_lang_sense_clarification": "гостей к выходу"
                            },
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "животных на улицу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35c06496-90ef-3ae6-a4f2-5f468dcc9a03",
                    "sense_definition": "Zijn mening of gevoelens over iets of iemand onder woorden brengen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "express_oneself",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De minister wilde zich niet over de kwestie <w>uitlaten</w>.",
                            "target_lang_translations": {
                                "en": "The minister did not want to <w>comment</w> on the matter.",
                                "ru": "Министр не хотел <w>высказываться</w> по этому вопросу."
                            }
                        },
                        {
                            "text": "Hij heeft zich zeer positief <w>uitgelaten</w> over jouw werk.",
                            "target_lang_translations": {
                                "en": "He <w>expressed himself</w> very positively about your work.",
                                "ru": "Он очень положительно <w>отозвался</w> о твоей работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich uiten",
                        "zich uitspreken",
                        "verklaren"
                    ],
                    "antonyms": [
                        "verzwijgen"
                    ],
                    "common_phrases": [
                        "zich uitlaten over",
                        "zich lovend uitlaten",
                        "zich negatief uitlaten"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wederkerend (zich uitlaten)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To put one's opinion or feelings about something or someone into words.",
                        "ru": "Выражать свое мнение или чувства по поводу чего-либо или кого-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "express oneself",
                                "target_lang_sense_clarification": "voice opinions"
                            },
                            {
                                "target_lang_word": "comment",
                                "target_lang_sense_clarification": "give an opinion on"
                            },
                            {
                                "target_lang_word": "speak out",
                                "target_lang_sense_clarification": "state views publicly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "высказываться",
                                "target_lang_sense_clarification": "выражать мнение"
                            },
                            {
                                "target_lang_word": "отзываться",
                                "target_lang_sense_clarification": "давать оценку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad30b276-8dd8-34d6-bf31-4799b4313f83",
                    "sense_definition": "Een kledingstuk ruimer of langer maken door de naden of zomen los te maken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "lengthen_clothes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat de rok te strak zat, liet ze hem <w>uitlaten</w>.",
                            "target_lang_translations": {
                                "en": "Because the skirt was too tight, she had it <w>let out</w>.",
                                "ru": "Так как юбка была слишком узкой, она отдала её <w>расставить</w>."
                            }
                        },
                        {
                            "text": "De kleermaker kan de broek een stukje <w>uitlaten</w>.",
                            "target_lang_translations": {
                                "en": "The tailor can <w>let</w> the trousers <w>out</w> a bit.",
                                "ru": "Портной может немного <w>расставить</w> брюки."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitleggen"
                    ],
                    "antonyms": [
                        "innemen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a garment larger or longer by undoing the seams or hems.",
                        "ru": "Делать одежду шире или длиннее, распуская швы или подгиб."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "let out",
                                "target_lang_sense_clarification": "clothing/tailoring"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "расставлять",
                                "target_lang_sense_clarification": "делать шире"
                            },
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "удлинять или расширять за счет запаса ткани"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "acee7de3-3abe-3ffc-912f-1cf9a52ad519",
                    "sense_definition": "Meervoud van <w>uitlaat</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auto heeft twee verchroomde <w>uitlaten</w>.",
                            "target_lang_translations": {
                                "en": "The car has two chrome <w>exhausts</w>.",
                                "ru": "У машины две хромированные <w>выхлопные трубы</w>."
                            }
                        },
                        {
                            "text": "Door de kapotte <w>uitlaten</w> maakten de motoren veel lawaai.",
                            "target_lang_translations": {
                                "en": "Because of the broken <w>exhaust pipes</w>, the engines made a lot of noise.",
                                "ru": "Из-за сломанных <w>выхлопных труб</w> двигатели сильно шумели."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'uitlaat' (the pipe through which gas escapes from an engine).",
                        "ru": "Множественное число от слова «выхлопная труба»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exhausts",
                                "target_lang_sense_clarification": "car parts"
                            },
                            {
                                "target_lang_word": "exhaust pipes",
                                "target_lang_sense_clarification": "mechanical"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выхлопные трубы",
                                "target_lang_sense_clarification": "часть автомобиля"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f1a5f74-ed41-344a-9919-abd66d6193e2",
                    "sense_definition": "Iets meemaken of ervaren doordat het plaatsvindt, vaak onverwacht of buiten de eigen controle.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "happen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is mij nog nooit <w>overkomen</w>.",
                            "target_lang_translations": {
                                "en": "That has never <w>happened to</w> me."
                            }
                        },
                        {
                            "text": "Het kan iedereen <w>overkomen</w>, dus voel je niet schuldig.",
                            "target_lang_translations": {
                                "en": "It can <w>happen to</w> anyone, so don't feel guilty."
                            }
                        },
                        {
                            "text": "Hem <w>overkwam</w> een vreselijk ongeluk tijdens de vakantie.",
                            "target_lang_translations": {
                                "en": "A terrible accident <w>happened to</w> him during the holiday."
                            }
                        }
                    ],
                    "synonyms": [
                        "gebeuren",
                        "treffen"
                    ],
                    "common_phrases": [
                        "iemand overkomen",
                        "het beste dat me is overkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To occur to someone or be experienced by someone, often unexpectedly or beyond their control."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "happen to",
                                "target_lang_sense_clarification": "to occur to someone"
                            },
                            {
                                "target_lang_word": "befall",
                                "target_lang_sense_clarification": "formal or literary; usually regarding something negative"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc804866-f97d-31ed-be68-87f7eddc5d3e",
                    "sense_definition": "Van een andere plaats naar de spreker toe komen, vaak voor een bezoek.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "visit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn ouders zijn speciaal voor mijn verjaardag <w>overgekomen</w>.",
                            "target_lang_translations": {
                                "en": "My parents <w>came over</w> especially for my birthday."
                            }
                        },
                        {
                            "text": "Kom je vanavond even <w>over</w> om een film te kijken?",
                            "target_lang_translations": {
                                "en": "Are you <w>coming over</w> tonight to watch a movie?"
                            }
                        }
                    ],
                    "synonyms": [
                        "langskomen",
                        "bezoeken"
                    ],
                    "common_phrases": [
                        "laten overkomen",
                        "uit het buitenland overkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To travel from another place to where the speaker is, usually for a visit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come over",
                                "target_lang_sense_clarification": "to visit someone at their location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1bac14b-43b5-33e5-9807-118d71128c6a",
                    "sense_definition": "Een bepaalde indruk wekken op iemand anders.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "seem",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn grapje <w>kwam</w> bij de collega's verkeerd <w>over</w>.",
                            "target_lang_translations": {
                                "en": "His joke <w>came across</w> wrong to his colleagues."
                            }
                        },
                        {
                            "text": "Ze <w>komt</w> heel zelfverzekerd <w>over</w> tijdens presentaties.",
                            "target_lang_translations": {
                                "en": "She <w>comes across</w> as very confident during presentations."
                            }
                        },
                        {
                            "text": "Ik hoop dat mijn bericht niet onbeleefd <w>overkomt</w>.",
                            "target_lang_translations": {
                                "en": "I hope my message doesn't <w>come across</w> as rude."
                            }
                        }
                    ],
                    "synonyms": [
                        "lijken",
                        "schijnen"
                    ],
                    "common_phrases": [
                        "verkeerd overkomen",
                        "goed overkomen",
                        "raar overkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a particular impression on others; to be perceived in a certain way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come across",
                                "target_lang_sense_clarification": "to give a specific impression"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cadeb040-ee7e-3855-aebd-8305d4c56b12",
                    "sense_definition": "De andere kant van iets bereiken, zoals een rivier, grens of obstakel.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cross",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zijn de rivier veilig <w>overgekomen</w> met de boot.",
                            "target_lang_translations": {
                                "en": "We safely <w>crossed</w> the river by boat."
                            }
                        },
                        {
                            "text": "Het zware onweer is de stad inmiddels <w>overgekomen</w>.",
                            "target_lang_translations": {
                                "en": "The heavy thunderstorm has <w>passed over</w> the city by now."
                            }
                        }
                    ],
                    "synonyms": [
                        "oversteken"
                    ],
                    "target_lang_definitions": {
                        "en": "To reach the other side of an obstacle, boundary, or area."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cross",
                                "target_lang_sense_clarification": "to go from one side to the other"
                            },
                            {
                                "target_lang_word": "get across",
                                "target_lang_sense_clarification": "to succeed in reaching the other side"
                            },
                            {
                                "target_lang_word": "pass over",
                                "target_lang_sense_clarification": "to move over an area (e.g., a storm)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "damp",
        "dampig",
        "dompig"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a8f71de4-4a12-3f1b-8650-c0e8f1e0db89",
                    "sense_definition": "Licht bezweet of klam aanvoelend, vaak op een koude of onprettige manier.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "moisture_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij veegde zijn <w>domige</w> voorhoofd af met een zakdoek.",
                            "target_lang_translations": {
                                "en": "He wiped his <w>clammy</w> forehead with a handkerchief.",
                                "ru": "Он вытер свой <w>влажный</w> лоб носовым платком."
                            }
                        },
                        {
                            "text": "Haar handen voelden koud en <w>domig</w> aan door de koorts.",
                            "target_lang_translations": {
                                "en": "Her hands felt cold and <w>clammy</w> due to the fever.",
                                "ru": "Её руки были холодными и <w>липкими</w> на ощупь из-за жара."
                            }
                        }
                    ],
                    "synonyms": [
                        "klam"
                    ],
                    "common_phrases": [
                        "een domig voorhoofd",
                        "domige handen"
                    ],
                    "target_lang_definitions": {
                        "en": "Slightly sweaty or clammy to the touch, often in a cold or unpleasant way.",
                        "ru": "Слегка влажный или липкий на ощупь, часто вызывающий неприятное ощущение холода."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clammy",
                                "target_lang_sense_clarification": "unpleasantly damp and sticky to the touch"
                            },
                            {
                                "target_lang_word": "damp",
                                "target_lang_sense_clarification": "slightly wet"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "влажный",
                                "target_lang_sense_clarification": "о коже, покрытой испариной"
                            },
                            {
                                "target_lang_word": "липкий",
                                "target_lang_sense_clarification": "неприятно влажный на ощупь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "505181d6-d4b7-3114-b7fe-89af2f8601a3",
                    "sense_definition": "Gekenmerkt door nevel of fijne waterdruppeltjes in de lucht; niet helder.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "moisture_weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het uitzicht verdween langzaam in de <w>domige</w> verte.",
                            "target_lang_translations": {
                                "en": "The view slowly disappeared into the <w>misty</w> distance.",
                                "ru": "Вид медленно исчезал в <w>туманной</w> дали."
                            }
                        },
                        {
                            "text": "Door het <w>domige</w> weer konden we de overkant van het meer niet zien.",
                            "target_lang_translations": {
                                "en": "Due to the <w>hazy</w> weather, we couldn't see the other side of the lake.",
                                "ru": "Из-за <w>мглистой</w> погоды мы не могли разглядеть противоположный берег озера."
                            }
                        }
                    ],
                    "synonyms": [
                        "mistig",
                        "nevelig"
                    ],
                    "common_phrases": [
                        "domig weer",
                        "een domige lucht"
                    ],
                    "target_lang_definitions": {
                        "en": "Characterized by mist or fine water droplets in the air; not clear.",
                        "ru": "Наполненный туманом или мелкой водяной взвесью; неясный, затуманенный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "misty",
                                "target_lang_sense_clarification": "full of or covered with mist"
                            },
                            {
                                "target_lang_word": "hazy",
                                "target_lang_sense_clarification": "obscured by atmospheric haze"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "туманный",
                                "target_lang_sense_clarification": "о погоде или воздухе"
                            },
                            {
                                "target_lang_word": "мглистый",
                                "target_lang_sense_clarification": "подернутый дымкой"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "25e8d463-890d-3e71-804f-52ab724f52a3",
                    "sense_definition": "A person who takes or keeps more than their fair share of something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agent_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is such a <w>hogger</w> of the blankets on cold nights.",
                            "target_lang_translations": {
                                "ru": "Он такой <w>жадина</w>, вечно тянет на себя одеяло холодными ночами."
                            }
                        },
                        {
                            "text": "The manager was known as a <w>hogger</w> of credit for his team's work.",
                            "target_lang_translations": {
                                "ru": "Менеджер был известен как <w>любитель присваивать</w> заслуги за работу своей команды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "blanket hogger",
                        "road hogger"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который берет или удерживает больше своей справедливой доли чего-либо; жадина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жадина",
                                "target_lang_sense_clarification": "человек, который не делится"
                            },
                            {
                                "target_lang_word": "единоличник",
                                "target_lang_sense_clarification": "человек, действующий только в своих интересах"
                            },
                            {
                                "target_lang_word": "эгоист",
                                "target_lang_sense_clarification": "себялюбец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d05fe74c-5710-3513-9eb9-99e8b20c10a3",
                    "sense_definition": "The engineer or driver of a railway locomotive.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "railroad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old <w>hogger</w> knew every curve of the track by heart.",
                            "target_lang_translations": {
                                "ru": "Старый <w>машинист</w> знал каждый изгиб пути наизусть."
                            }
                        },
                        {
                            "text": "A skilled <w>hogger</w> can manage the train's speed smoothly.",
                            "target_lang_translations": {
                                "ru": "Опытный <w>машинист</w> может плавно управлять скоростью поезда."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoghead"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "railroad terminology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Инженер или машинист железнодорожного локомотива (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "машинист",
                                "target_lang_sense_clarification": "водитель поезда"
                            },
                            {
                                "target_lang_word": "механик",
                                "target_lang_sense_clarification": "устаревшее или профессиональное название машиниста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "962604cf-9b1b-38b5-ab4c-2db3e74edec6",
                    "sense_definition": "A stone in curling that fails to reach the far hog line and is removed from play.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The skip was disappointed when his first shot ended up as a <w>hogger</w>.",
                            "target_lang_translations": {
                                "ru": "Скип был разочарован, когда его первый бросок оказался <w>хогом</w>."
                            }
                        },
                        {
                            "text": "Because the ice was slow, the stone became a <w>hogger</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за того, что лед был медленным, камень стал <w>хогом</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Камень в керлинге, который не достиг дальней зачетной линии (хог-лайн) и выбывает из игры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хог",
                                "target_lang_sense_clarification": "термин в керлинге"
                            },
                            {
                                "target_lang_word": "недокат",
                                "target_lang_sense_clarification": "недостаточно сильный бросок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4857fa57-efa0-38ba-b2e2-5aff34ce0107",
                    "sense_definition": "A stocking without a foot, historically worn by coal miners.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The miner pulled up his <w>hogger</w> before descending into the pit.",
                            "target_lang_translations": {
                                "ru": "Шахтер подтянул свои <w>гетры</w> перед спуском в шахту."
                            }
                        },
                        {
                            "text": "He wore an old wool <w>hogger</w> to keep his calves warm.",
                            "target_lang_translations": {
                                "ru": "Он носил старые шерстяные <w>гетры</w>, чтобы согреть икры."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK dialects"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical mining context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чулок без ступни, который исторически носили шахтеры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гетры",
                                "target_lang_sense_clarification": "утепленные чулки без ступни"
                            },
                            {
                                "target_lang_word": "ноговицы",
                                "target_lang_sense_clarification": "устаревшее название одежды для ног"
                            }
                        ]

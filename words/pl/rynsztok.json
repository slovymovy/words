    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3667bee4-80c9-3a7f-9d07-49a855464a5d",
                    "sense_definition": "Otwarty kanał biegnący wzdłuż krawędzi jezdni, służący do odprowadzania wody opadowej i nieczystości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_channel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po gwałtownej burzy woda wylewała się z <w>rynsztoka</w> na chodnik.",
                            "target_lang_translations": {
                                "ru": "После сильной бури вода выливалась из <w>сточной канавы</w> на тротуар."
                            }
                        },
                        {
                            "text": "Upuściłem monetę, która potoczyła się prosto do <w>rynsztoka</w>.",
                            "target_lang_translations": {
                                "ru": "Я уронил монету, которая покатилась прямо в <w>сточную канаву</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ściek"
                    ],
                    "common_phrases": [
                        "wpaść do rynsztoka",
                        "płynąć rynsztokiem",
                        "brudny rynsztok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытый канал вдоль края проезжей части для отвода дождевой воды и нечистот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сточная канава",
                                "target_lang_sense_clarification": "уличный желоб для воды"
                            },
                            {
                                "target_lang_word": "канава",
                                "target_lang_sense_clarification": "углубление в земле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f4e2fc0-c9d4-350c-94b6-fff6ddc9d621",
                    "sense_definition": "Środowisko ludzi zdemoralizowanych, margines społeczny lub bardzo niski, wulgarny poziom kultury.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_degradation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego słownictwo to niestety kompletny <w>rynsztok</w>.",
                            "target_lang_translations": {
                                "ru": "Его лексика — это, к сожалению, полная <w>помойка</w>."
                            }
                        },
                        {
                            "text": "Mężczyzna zdołał się podnieść i nie stoczyć się do <w>rynsztoka</w>.",
                            "target_lang_translations": {
                                "ru": "Мужчине удалось подняться и не скатиться на <w>дно</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stoczyć się do rynsztoka",
                        "język rynsztoka",
                        "wyciągnąć z rynsztoka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Среда деморализованных людей, социальное дно или крайне низкий, вульгарный уровень культуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дно",
                                "target_lang_sense_clarification": "социальный упадок"
                            },
                            {
                                "target_lang_word": "помойка",
                                "target_lang_sense_clarification": "вульгарность, грязь"
                            }
                        ]
                    }
                }
            ]
        }
    ]

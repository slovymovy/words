            "senses": [
                {
                    "sense_id": "16c23445-f7bb-3fc5-a6e6-40d4e4828ce7",
                    "sense_definition": "Een veelvoorkomende jongensnaam.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn oom <w>Jan</w> komt morgen bij ons op bezoek.",
                            "target_lang_translations": {
                                "en": "My uncle <w>Jan</w> is coming to visit us tomorrow.",
                                "ru": "Мой дядя <w>Ян</w> завтра придет к нам в гости."
                            }
                        },
                        {
                            "text": "De nieuwe maatregelen van de overheid raken vooral <w>Jan</w> met de pet.",
                            "target_lang_translations": {
                                "en": "The new government measures mainly affect the <w>average Joe</w>.",
                                "ru": "Новые меры правительства затрагивают в основном <w>простых обывателей</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Johannes"
                    ],
                    "common_phrases": [
                        "Jan en alleman",
                        "Jan met de pet",
                        "boven Jan",
                        "Jan Modaal"
                    ],
                    "target_lang_definitions": {
                        "en": "A common boy's name.",
                        "ru": "Распространенное мужское имя."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Jan",
                                "target_lang_sense_clarification": "Dutch male name, equivalent to John"
                            },
                            {
                                "target_lang_word": "John",
                                "target_lang_sense_clarification": "English equivalent of the name"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Ян",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f72e26bc-788c-3477-b2b5-25b2e1d3fd66",
                    "sense_definition": "Het woord dat in het Nederlandse telefoonalfabet wordt gebruikt voor de letter j.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spelling_alphabet",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "Je spelt die naam als <w>Jan</w>, Anton, Nico, Simon, Eduard, Nico.",
                            "target_lang_translations": {
                                "en": "You spell that name as <w>Juliett</w>, Alfa, November, Sierra, Echo, November.",
                                "ru": "Это имя диктуется как <w>Ян</w>, Антон, Нико, Симон, Эдуард, Нико."
                            }
                        },
                        {
                            "text": "Om verwarring te voorkomen, zei hij <w>Jan</w> in plaats van j.",
                            "target_lang_translations": {
                                "en": "To avoid confusion, he said <w>Juliett</w> instead of j.",
                                "ru": "Чтобы избежать путаницы, он сказал <w>Ян</w> вместо \"й\"."
                            }
                        }
                    ],
                    "synonyms": [
                        "Juliett"
                    ],
                    "target_lang_definitions": {
                        "en": "The word used in the Dutch telephone alphabet for the letter j.",
                        "ru": "Слово, используемое в голландском телефонном алфавите для обозначения буквы j."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Juliett",
                                "target_lang_sense_clarification": "English/NATO phonetic alphabet equivalent"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Ян",
                                "target_lang_sense_clarification": "кодовое слово для буквы J"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "34b71650-6dbb-3a71-8f5d-0e1bcee0d7b4",
                    "sense_definition": "Afkorting van de eerste maand van het jaar, <w>januari</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "month_abbr",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De winkel is gesloten van 1 <w>jan</w> tot en met 5 <w>jan</w>.",
                            "target_lang_translations": {
                                "en": "The shop is closed from <w>Jan.</w> 1st to <w>Jan.</w> 5th.",
                                "ru": "Магазин закрыт с 1 <w>янв.</w> по 5 <w>янв.</w>"
                            }
                        },
                        {
                            "text": "Op de uitnodiging staat dat het feest op 15 <w>jan</w> plaatsvindt.",
                            "target_lang_translations": {
                                "en": "The invitation says the party takes place on <w>Jan.</w> 15.",
                                "ru": "В приглашении сказано, что вечеринка состоится 15 <w>янв.</w>"
                            }

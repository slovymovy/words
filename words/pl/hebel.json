                {
                    "sense_id": "21f37d82-cf9f-3d9b-91c7-525a93210236",
                    "sense_definition": "Narzędzie stolarskie służące do wyrównywania i wygładzania powierzchni drewna poprzez skrawanie cienkich warstw.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony stolarz chwycił pewnie <w>hebel</w> i zaczął wygładzać dębową deskę.",
                            "target_lang_translations": {
                                "ru": "Опытный плотник уверенно схватил <w>рубанок</w> и начал выравнивать дубовую доску."
                            }
                        },
                        {
                            "text": "Musisz dobrze naostrzyć swój <w>hebel</w>, jeśli chcesz uzyskać idealnie gładką powierzchnię.",
                            "target_lang_translations": {
                                "ru": "Ты должен хорошо наточить свой <w>рубанок</w>, если хочешь получить идеально гладкую поверхность."
                            }
                        }
                    ],
                    "synonyms": [
                        "strug"
                    ],
                    "common_phrases": [
                        "ostry hebel",
                        "pracować heblem",
                        "ręczny hebel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Столярный инструмент для выравнивания и сглаживания деревянных поверхностей путем снятия тонких слоев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рубанок",

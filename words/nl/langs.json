                {
                    "sense_id": "501e4b42-831c-335e-bab2-723449980a6b",
                    "sense_definition": "Voorbij een bepaald punt bewegend.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_past",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bus reed <langs> de halte zonder te stoppen.",
                            "target_lang_translations": {
                                "en": "The bus drove <past> the stop without stopping.",
                                "ru": "Автобус проехал <w>мимо</w> остановки, не останавливаясь."
                            }
                        },
                        {
                            "text": "Hij liep <langs> mij zonder iets te zeggen.",
                            "target_lang_translations": {
                                "en": "He walked <past> me without saying anything.",
                                "ru": "Он прошел <w>мимо</w> меня, ничего не сказав."

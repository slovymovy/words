                {
                    "sense_id": "4b323cdd-fd0c-3dec-b48a-b789b11e8d60",
                    "sense_definition": "Szereg połączonych ze sobą narządów w organizmie pełniących wspólną funkcję.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Infekcja zaatakowała górne <drogi> oddechowe pacjenta.",
                            "target_lang_translations": {
                                "ru": "Инфекция поразила верхние дыхательные <пути> пациента."
                            }
                        },
                        {
                            "text": "Lekarz zalecił badanie sprawdzające <drogi> żółciowe.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал обследование, проверяющее желчевыводящие <пути>."
                            }
                        }
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d5a26f6a-7b91-3a31-ace1-51b170ea8ccc",
                    "sense_definition": "Iets op een plek leggen waar anderen het niet gemakkelijk kunnen vinden of zien.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_hiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze gingen met z'n tweeën paaseieren <verbergen> in het bos.",
                            "target_lang_translations": {
                                "ru": "Они вдвоем пошли <прятать> пасхальные яйца в лесу."
                            }
                        },
                        {
                            "text": "Hij wilde het cadeau snel in de kast <verbergen> voordat zij binnenkwam.",
                            "target_lang_translations": {
                                "ru": "Он хотел быстро <спрятать> подарок в шкаф, до того как она войдет."
                            }
                        }
                    ],
                    "synonyms": [
                        "verstoppen",
                        "wegstoppen"
                    ],
                    "antonyms": [
                        "tonen",
                        "laten zien",
                        "vinden"
                    ],
                    "common_phrases": [
                        "sporen verbergen",
                        "de buit verbergen",
                        "iets snel verbergen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положить что-то в такое место, где другие не смогут легко это найти или увидеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прятать",
                                "target_lang_sense_clarification": "класть в укромное место"
                            },
                            {
                                "target_lang_word": "скрывать",
                                "target_lang_sense_clarification": "убирать с глаз долой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "992e7bc2-c68b-3b1a-8baa-b99b6f736e7d",
                    "sense_definition": "Zichzelf op een onopvallende plek plaatsen zodat men niet ontdekt wordt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_hiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <verborg> zich snel in het dichte struikgewas toen hij iemand hoorde.",
                            "target_lang_translations": {
                                "ru": "Он быстро <спрятался> в густых зарослях, когда услышал кого-то."
                            }
                        },
                        {
                            "text": "De kinderen gingen zich tijdens het spel achter de bomen <verbergen>.",
                            "target_lang_translations": {
                                "ru": "Дети пошли <прятаться> за деревьями во время игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "verschuilen",
                        "verstoppen"
                    ],
                    "antonyms": [
                        "tevoorschijn komen"
                    ],
                    "common_phrases": [
                        "zich verbergen voor",
                        "zich in het bos verbergen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить себя в незаметное место, чтобы не быть обнаруженным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прятаться",
                                "target_lang_sense_clarification": "скрывать себя"
                            },
                            {
                                "target_lang_word": "скрываться",
                                "target_lang_sense_clarification": "находиться в укрытии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4de67da9-65d5-3abc-8e30-9115d9a3c99b",
                    "sense_definition": "Iets, zoals een gevoel, feit of informatie, geheimhouden of niet laten merken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_hiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze probeerde haar teleurstelling te <verbergen> door breed te glimlachen.",
                            "target_lang_translations": {
                                "ru": "Она пыталась <скрыть> свое разочарование за широкой улыбкой."
                            }
                        },
                        {
                            "text": "Het is soms moeilijk om de waarheid voor je vrienden te <verbergen>.",
                            "target_lang_translations": {
                                "ru": "Иногда трудно <скрывать> правду от друзей."
                            }
                        }
                    ],
                    "synonyms": [
                        "verhullen",
                        "verhelen"
                    ],
                    "antonyms": [
                        "onthullen",
                        "openbaren",
                        "tonen"
                    ],
                    "common_phrases": [
                        "emoties verbergen",
                        "de waarheid verbergen",
                        "verdriet verbergen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Держать что-то в секрете или не показывать, например чувство, факт или информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрывать",
                                "target_lang_sense_clarification": "утаивать информацию или чувства"
                            },
                            {
                                "target_lang_word": "утаивать",
                                "target_lang_sense_clarification": "держать в тайне"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "введение",
        "ввод",
        "вводный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bbfc4f08-f759-3236-b035-7686f8d0d976",
                    "sense_definition": "Записать данные, команды или информацию в электронное устройство или систему.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для доступа к системе необходимо <ввести> правильный пароль.",
                            "target_lang_translations": {
                                "en": "To access the system, it is necessary to <w>enter</w> the correct password."
                            }
                        },
                        {
                            "text": "Оператор попросил меня <ввести> пин-код на терминале.",
                            "target_lang_translations": {
                                "en": "The operator asked me to <w>enter</w> the PIN code on the terminal."
                            }
                        }
                    ],
                    "synonyms": [
                        "разместить"
                    ],
                    "common_phrases": [
                        "ввести пароль",
                        "ввести данные",
                        "ввести код",
                        "ввести запрос"
                    ],
                    "target_lang_definitions": {
                        "en": "To input data, commands, or information into an electronic device or system."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "data or password"
                            },
                            {
                                "target_lang_word": "input",
                                "target_lang_sense_clarification": "data"
                            },
                            {
                                "target_lang_word": "type in",
                                "target_lang_sense_clarification": "using a keyboard"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a9b1fcc-37b7-35e9-8ae6-0ecc11f73ae0",
                    "sense_definition": "Помочь или заставить кого-либо войти внутрь помещения.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хозяин поспешил <ввести> гостей в тёплый дом.",
                            "target_lang_translations": {
                                "en": "The host hurried to <w>bring</w> the guests into the warm house."
                            }
                        },
                        {
                            "text": "Охрана отказалась <ввести> журналистов в зал заседаний.",
                            "target_lang_translations": {
                                "en": "Security refused to <w>lead</w> the journalists into the meeting hall."
                            }
                        }
                    ],
                    "antonyms": [
                        "вывести"
                    ],
                    "common_phrases": [
                        "ввести в дом",
                        "ввести в комнату",
                        "ввести под руки"
                    ],
                    "target_lang_definitions": {
                        "en": "To help, lead, or force someone to go inside a room or building."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring in",
                                "target_lang_sense_clarification": "guests or people"
                            },
                            {
                                "target_lang_word": "lead in",
                                "target_lang_sense_clarification": "guide someone inside"
                            },
                            {
                                "target_lang_word": "usher in",
                                "target_lang_sense_clarification": "formally"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21583d0d-2897-39ce-998f-a8b6702543da",
                    "sense_definition": "Установить и утвердить что-либо новое (закон, правило, порядок) для всеобщего использования.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "implementation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство планирует <ввести> новые правила дорожного движения.",
                            "target_lang_translations": {
                                "en": "The government plans to <w>introduce</w> new traffic rules."
                            }
                        },
                        {
                            "text": "Директор школы решил <ввести> обязательную форму для учеников.",
                            "target_lang_translations": {
                                "en": "The school principal decided to <w>introduce</w> a mandatory uniform for students."
                            }
                        }
                    ],
                    "synonyms": [
                        "внедрить"
                    ],
                    "antonyms": [
                        "вывести"
                    ],
                    "common_phrases": [
                        "ввести закон",
                        "ввести санкции",
                        "ввести налог",
                        "ввести в эксплуатацию"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish and approve something new (law, rule, order) for general use."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "introduce",
                                "target_lang_sense_clarification": "new rules or laws"
                            },
                            {
                                "target_lang_word": "implement",
                                "target_lang_sense_clarification": "a system or measure"
                            },
                            {
                                "target_lang_word": "impose",
                                "target_lang_sense_clarification": "sanctions or taxes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67a39527-62aa-3bb4-b877-da82c6088a68",
                    "sense_definition": "Включить кого-либо в состав группы, организации или органа управления.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "inclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тренер решил <ввести> молодого игрока в основной состав команды.",
                            "target_lang_translations": {
                                "en": "The coach decided to <w>put</w> the young player in the starting lineup."
                            }
                        },
                        {
                            "text": "Акционеры проголосовали за то, чтобы <ввести> эксперта в совет директоров.",
                            "target_lang_translations": {
                                "en": "The shareholders voted to <w>appoint</w> an expert to the board of directors."
                            }
                        }
                    ],
                    "synonyms": [
                        "включить"
                    ],
                    "antonyms": [
                        "исключить",
                        "вывести"
                    ],
                    "common_phrases": [
                        "ввести в состав",
                        "ввести в совет",
                        "ввести в игру"
                    ],
                    "target_lang_definitions": {
                        "en": "To include someone as a member of a group, organization, or governing body."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appoint",
                                "target_lang_sense_clarification": "to a board or position"
                            },
                            {
                                "target_lang_word": "include",
                                "target_lang_sense_clarification": "in a group"
                            },
                            {
                                "target_lang_word": "bring in",
                                "target_lang_sense_clarification": "new members"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79089554-972c-38e2-98f2-6859c6094e1e",
                    "sense_definition": "Впустить лекарство или иное вещество внутрь организма с помощью медицинских инструментов.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Медсестра должна осторожно <ввести> лекарство пациенту.",
                            "target_lang_translations": {
                                "en": "The nurse must carefully <w>inject</w> the medicine into the patient."
                            }
                        },
                        {
                            "text": "Врач принял решение немедленно <ввести> сыворотку.",
                            "target_lang_translations": {
                                "en": "The doctor decided to immediately <w>administer</w> the serum."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ввести внутривенно",
                        "ввести инъекцию",
                        "ввести препарат"
                    ],
                    "target_lang_definitions": {
                        "en": "To put medicine or another substance into the body using medical instruments."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inject",
                                "target_lang_sense_clarification": "medicine"
                            },
                            {
                                "target_lang_word": "administer",
                                "target_lang_sense_clarification": "drug or treatment"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
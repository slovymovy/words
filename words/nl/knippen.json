                    "sense_id": "896630f3-c7bd-3897-989a-25311e7037fb",
                    "sense_definition": "Met een schaar materiaal in stukken verdelen of korter maken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cutting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kapper gaat morgen mijn haar <w>knippen</w>.",
                            "target_lang_translations": {
                                "ru": "Завтра парикмахер будет <w>стричь</w> мне волосы."
                            }
                        },
                        {
                            "text": "Wat ben jij mooie figuren aan het <w>knippen</w>!",
                            "target_lang_translations": {
                                "ru": "Какие красивые фигуры ты <w>вырезаешь</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "haar knippen",
                        "nagels knippen",
                        "in stukjes knippen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрезать материал на части или укорачивать его с помощью ножниц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стричь",
                                "target_lang_sense_clarification": "волосы, ногти, шерсть"
                            },
                            {
                                "target_lang_word": "резать",
                                "target_lang_sense_clarification": "ножницами (бумагу, ткань)"
                            }

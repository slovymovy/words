    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ee99f9ab-82e3-334f-a918-954f336c1147",
                    "sense_definition": "Najwyższa po papieżu godność kościelna w Kościele katolickim oraz duchowny piastujący to stanowisko.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "church_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po śmierci papieża <w>kardynałowie</w> zebrali się na konklawe.",
                            "target_lang_translations": {
                                "ru": "После смерти Папы <w>кардиналы</w> собрались на конклав."
                            }
                        },
                        {
                            "text": "W uroczystej procesji szedł <w>kardynał</w> ubrany w czerwone szaty.",
                            "target_lang_translations": {
                                "ru": "В торжественной процессии шел <w>кардинал</w>, облаченный в красные одежды."
                            }
                        }
                    ],
                    "synonyms": [
                        "purpurat",
                        "kard."
                    ],
                    "common_phrases": [
                        "kolegium kardynałów",
                        "mianować kardynałem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высший после Папы духовный сан в Католической церкви, а также священнослужитель, занимающий этот пост."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кардинал",
                                "target_lang_sense_clarification": "духовный сан"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5f20777-8c8c-3205-b304-0d50c6adbb8d",
                    "sense_definition": "Ptak z rodziny kardynałów o jaskrawym, zazwyczaj czerwonym upierzeniu i charakterystycznym czubie na głowie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Kardynały</w> żyją w Ameryce Północnej.",
                            "target_lang_translations": {
                                "ru": "<w>Кардиналы</w> обитают в Северной Америке."
                            }
                        },
                        {
                            "text": "W gęstych zaroślach dostrzegłem czerwonego <w>kardynała</w>.",
                            "target_lang_translations": {
                                "ru": "В густых зарослях я заметил красного <w>кардинала</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kardynał szkarłatny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Птица из семейства кардиналовых с ярким, обычно красным оперением и характерным хохолком на голове."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кардинал",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                }
            ]
        }
    ]

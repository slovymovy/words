                        "ujawniać talent",
                        "ujawniać braki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо видимым, заметным или обнаруживаемым (например, симптомы, скрытые качества)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выявлять",
                                "target_lang_sense_clarification": "обнаруживать в ходе проверки или анализа"
                            },
                            {
                                "target_lang_word": "обнаруживать",
                                "target_lang_sense_clarification": "находить, замечать"
                            },
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "демонстрировать наличие чего-либо"
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7166e831-06ca-3e69-8d2e-b2d17ae200be",
                    "sense_definition": "Główny kierunek w kulturze i sztuce europejskiej XV i XVI wieku, charakteryzujący się powrotem do wzorców antycznych i humanizmem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "period_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właśnie zakończył się wykład o <w>renesansie</w> w Polsce.",
                            "target_lang_translations": {
                                "ru": "Только что закончилась лекция о <w>Ренессансе</w> в Польше."
                            }
                        },
                        {
                            "text": "Budzi podziw humanizm doby <w>renesansu</w>.",
                            "target_lang_translations": {
                                "ru": "Гуманизм эпохи <w>Возрождения</w> вызывает восхищение."
                            }
                        },
                        {
                            "text": "Sufit zdobią plafony wykonane w stylu wczesnego <w>renesansu</w> włoskiego.",
                            "target_lang_translations": {
                                "ru": "Потолок украшают плафоны, выполненные в стиле раннего итальянского <w>ренессанса</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "odrodzenie"
                    ],
                    "common_phrases": [
                        "człowiek renesansu",
                        "epoka renesansu",
                        "sztuka renesansu",
                        "w stylu renesansu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направление в европейской культуре и искусстве XV и XVI веков, характеризующееся возвращением к античным образцам и гуманизмом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ренессанс",
                                "target_lang_sense_clarification": "стиль и направление"
                            },
                            {
                                "target_lang_word": "Возрождение",
                                "target_lang_sense_clarification": "культурное движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fadfb221-bb44-3dd4-8c7b-e8a89cbb3035",
                    "sense_definition": "Ponowne pojawienie się, ożywienie i rozwój czegoś, co zostało zapomniane lub minęło.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "revival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obecnie swój <w>renesans</w> przeżywają płyty gramofonowe.",
                            "target_lang_translations": {
                                "ru": "В настоящее время грампластинки переживают свой <w>ренессанс</w>."
                            }
                        },
                        {
                            "text": "Tradycyjne rzemiosło przechodzi ostatnio prawdziwy <w>renesans</w>.",
                            "target_lang_translations": {
                                "ru": "Традиционные ремёсла в последнее время переживают настоящий <w>ренессанс</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "odrodzenie",
                        "odnowa",
                        "rozkwit"
                    ],
                    "common_phrases": [
                        "przeżywać renesans",
                        "prawdziwy renesans"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повторное появление, оживление и развитие чего-либо, что было забыто или прошло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ренессанс",
                                "target_lang_sense_clarification": "новый расцвет"
                            },
                            {
                                "target_lang_word": "возрождение",
                                "target_lang_sense_clarification": "обновление"
                            }
                        ]
                    }
                }
            ]
        },
        {

                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or biblical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по получению, приобретению или добыванию чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приобретение",
                                "target_lang_sense_clarification": "процесс получения знаний, опыта или имущества"
                            },
                            {

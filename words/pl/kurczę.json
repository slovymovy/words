                {
                    "sense_id": "3ac786bc-1965-3e32-8630-04daf023a54f",
                    "sense_definition": "Młody ptak z gatunku kura domowa; pisklę kury.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal_young",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wiejskim podwórku biegało małe, żółte <w>kurczę</w> szukające ziarna.",
                            "target_lang_translations": {
                                "ru": "По деревенскому двору бегал маленький жёлтый <w>цыплёнок</w>, ищущий зерно."
                            }
                        },
                        {
                            "text": "Dzieci z wielkim zachwytem oglądały wykluwające się z jajka <w>kurczę</w>.",
                            "target_lang_translations": {
                                "ru": "Дети с большим восторгом наблюдали за вылупляющимся из яйца <w>цыплёнком</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) kurzę, kurziczka, kurzyczka, kurzynka",
                        "gw. (Górny Śląsk) kurzę, kurziczka, kurzyczka, kurzynka",
                        "gw. (Górny Śląsk) kurzę, kurziczka, kurzyczka, kurzynka",
                        "gw. (Górny Śląsk) kurzę, kurziczka, kurzyczka, kurzynka"
                    ],
                    "common_phrases": [
                        "żółte kurczę",
                        "małe kurczę",
                        "wyklute kurczę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодая птица вида домашняя курица; птенец курицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цыплёнок",
                                "target_lang_sense_clarification": "птенец"
                            }
                        ]

                {
                    "sense_id": "20f0e4de-ef60-362a-ab83-381a3f45919f",
                    "sense_definition": "Bekend of vertrouwd zijn met een persoon, zaak of begrip door ervaring of ontmoeting",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "knowing_familiarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>ken</w> mijn buren al heel lang.",
                            "target_lang_translations": {
                                "en": "I have <w>known</w> my neighbors for a very long time.",
                                "ru": "Я <w>знаю</w> своих соседей уже очень давно."
                            }
                        },
                        {
                            "text": "Zij <w>kent</w> de weg in Amsterdam heel goed.",
                            "target_lang_translations": {
                                "en": "She <w>knows</w> her way around Amsterdam very well.",
                                "ru": "Она очень хорошо <w>знает</w> дорогу в Амстердаме."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iemand van gezicht kennen",
                        "de weg kennen",
                        "elkaar leren kennen",
                        "iemand van naam kennen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be familiar or acquainted with a person, thing, or concept through experience or meeting",
                        "ru": "Быть знакомым с человеком, предметом или понятием через опыт или встречу"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "know",
                                "target_lang_sense_clarification": "to be familiar with people, places, or things"
                            },

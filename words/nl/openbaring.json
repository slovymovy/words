                    "sense_definition": "Het laatste boek van de Bijbel, dat visioenen over de toekomst beschrijft.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "work_of_art_name",
                    "examples": [
                        {
                            "text": "In <w>Openbaring</w> wordt het einde van de wereld voorspeld.",
                            "target_lang_translations": {
                                "ru": "В <w>Откровении</w> предсказывается конец света.",
                                "en": "In <w>Revelation</w>, the end of the world is predicted."
                            }
                        },
                        {
                            "text": "De dominee las tijdens de dienst een stuk voor uit <w>Openbaring</w>.",
                            "target_lang_translations": {
                                "ru": "Во время службы пастор зачитал отрывок из <w>Откровения</w>.",
                                "en": "During the service, the pastor read a passage from <w>Revelation</w>."
                            }
                        }
                    ],

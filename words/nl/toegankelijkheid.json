                            },
                            {
                                "target_lang_word": "accessibility",
                                "target_lang_sense_clarification": "state of being accessible"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "доступ",
                                "target_lang_sense_clarification": "права доступа"
                            },
                            {
                                "target_lang_word": "доступность",

                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это <было> очень интересное путешествие для нас.",
                            "target_lang_translations": {
                                "en": "It <was> a very interesting trip for us."
                            }
                        },
                        {
                            "text": "У меня вчера <было> много свободного времени.",
                            "target_lang_translations": {
                                "en": "I <had> a lot of free time yesterday."
                            }
                        }
                    ],

                    "sense_id": "e97da9fb-8b1e-321f-b034-6116fa75d291",
                    "sense_definition": "De oneetbare vliezen die om graankorrels zitten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "chaff",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer scheidde het <w>kaf</w> van het koren na de oogst.",
                            "target_lang_translations": {
                                "en": "The farmer separated the <w>chaff</w> from the wheat after the harvest.",
                                "ru": "Фермер отделил <w>мякину</w> от зерна после сбора урожая."
                            }
                        },
                        {
                            "text": "Door de wind waaide het lichte <w>kaf</w> weg terwijl het graan bleef liggen.",
                            "target_lang_translations": {
                                "en": "The wind blew away the light <w>chaff</w> while the grain remained.",
                                "ru": "Ветер сдул легкую <w>мякину</w>, в то время как зерно осталось лежать."
                            }
                        }
                    ],
                    "antonyms": [
                        "koren",
                        "graan"
                    ],
                    "common_phrases": [
                        "het kaf van het koren scheiden"
                    ],
                    "target_lang_definitions": {
                        "en": "The inedible husks that surround grain kernels.",
                        "ru": "Несъедобная оболочка (шелуха), покрывающая зерна злаков."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chaff",

                    "sense_id": "a23f9bd8-859d-32b7-8a85-bb4766f93066",
                    "sense_definition": "Waarvoor nog geen oplossing is gevonden; wat nog steeds een probleem vormt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>onopgeloste</w> conflict zorgt voor veel onrust in de regio.",
                            "target_lang_translations": {
                                "en": "The <w>unresolved</w> conflict is causing a lot of unrest in the region.",
                                "ru": "<w>Неразрешённый</w> конфликт вызывает много беспокойства в регионе."
                            }
                        },
                        {
                            "text": "Er blijven enkele <w>onopgeloste</w> problemen over na de vergadering.",
                            "target_lang_translations": {
                                "en": "A few <w>unresolved</w> problems remain after the meeting.",
                                "ru": "После собрания осталось несколько <w>нерешённых</w> проблем."
                            }
                        }
                    ],
                    "synonyms": [
                        "openstaand",
                        "onbeslist"
                    ],
                    "antonyms": [

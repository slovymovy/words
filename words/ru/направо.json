    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "fed57f01-2b6b-3da0-8b7d-ac6f57d8dd13",
                    "sense_definition": "В правую сторону; по направлению к правой руке.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На следующем перекрёстке вам нужно повернуть <w>направо</w>.",
                            "target_lang_translations": {
                                "en": "At the next intersection you need to turn <w>right</w>."
                            }
                        },
                        {
                            "text": "Если вы посмотрите <w>направо</w>, то увидите старый парк.",
                            "target_lang_translations": {
                                "en": "If you look <w>to the right</w>, you will see an old park."
                            }
                        }
                    ],
                    "synonyms": [
                        "вправо"
                    ],
                    "antonyms": [
                        "налево"
                    ],
                    "common_phrases": [
                        "повернуть направо",
                        "посмотрите направо",
                        "движение направо"
                    ],
                    "target_lang_definitions": {
                        "en": "To the right side; in the direction of the right hand."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "right",
                                "target_lang_sense_clarification": "direction of movement"
                            },
                            {
                                "target_lang_word": "to the right",
                                "target_lang_sense_clarification": "direction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "111bee34-b94d-3f5b-8c7b-82ef3f04e5a0",
                    "sense_definition": "В правой стороне; там, где находится правая рука.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Направо</w> от входа висит большое зеркало.",
                            "target_lang_translations": {
                                "en": "<w>To the right</w> of the entrance hangs a large mirror."
                            }
                        },
                        {
                            "text": "Моя комната находится <w>направо</w> по коридору.",
                            "target_lang_translations": {
                                "en": "My room is <w>to the right</w> down the hallway."
                            }
                        }
                    ],
                    "synonyms": [
                        "справа"
                    ],
                    "antonyms": [
                        "слева"
                    ],
                    "common_phrases": [
                        "направо от входа",
                        "направо от меня"
                    ],
                    "target_lang_definitions": {
                        "en": "On the right side; where the right hand is located."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to the right",
                                "target_lang_sense_clarification": "relative location"
                            },
                            {
                                "target_lang_word": "on the right",
                                "target_lang_sense_clarification": "location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7db687b3-4356-394c-91ca-44ea693fc860",
                    "sense_definition": "Военная или спортивная команда повернуться на 90 градусов в правую сторону.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Офицер громко скомандовал: «<w>Направо</w>!»",
                            "target_lang_translations": {
                                "en": "The officer commanded loudly: \"<w>Right face</w>!\""
                            }
                        },
                        {
                            "text": "Весь строй солдат одновременно повернулся <w>направо</w>.",
                            "target_lang_translations": {
                                "en": "The whole formation of soldiers simultaneously turned <w>to the right</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "налево"
                    ],
                    "common_phrases": [
                        "направо шагом марш"
                    ],
                    "target_lang_definitions": {
                        "en": "Military or sports command to turn 90 degrees to the right side."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Right face!",
                                "target_lang_sense_clarification": "military command"
                            },
                            {
                                "target_lang_word": "Right turn!",
                                "target_lang_sense_clarification": "command"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "target_lang_definitions": {
                        "ru": "Полностью законченный или завершённый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сделанный",
                                "target_lang_sense_clarification": "выполненный"
                            },
                            {
                                "target_lang_word": "законченный",
                                "target_lang_sense_clarification": "завершённый"
                            },
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "о работе или проекте"
                            }

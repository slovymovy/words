    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "73d08f2c-3b98-3662-be99-728032212eca",
                    "sense_definition": "Zachowujący się w sposób dystyngowany, nieskłonny do żartów; świadczący o skupieniu, rozwadze lub braku wesołości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój ojciec to bardzo <w>poważny</w> człowiek, który rzadko się śmieje.",
                            "target_lang_translations": {
                                "ru": "Мой отец — очень <w>серьёзный</w> человек, который редко смеётся."
                            }
                        },
                        {
                            "text": "Dlaczego masz taką <w>poważną</w> minę, czy stało się coś złego?",
                            "target_lang_translations": {
                                "ru": "Почему у тебя такое <w>серьёзное</w> лицо, случилось что-то плохое?"
                            }
                        }
                    ],
                    "synonyms": [
                        "stateczny",
                        "na serio"
                    ],
                    "antonyms": [
                        "niepoważny"
                    ],
                    "common_phrases": [
                        "poważny człowiek",
                        "poważna mina",
                        "zachować poważną twarz",
                        "być poważnym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ведущий себя с достоинством, не склонный к шуткам; строгий или сосредоточенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серьёзный",
                                "target_lang_sense_clarification": "о человеке или выражении лица"
                            },
                            {
                                "target_lang_word": "строгий",
                                "target_lang_sense_clarification": "суровый, не улыбающийся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab95bce2-612d-3e79-b9f8-2fb62143aeea",
                    "sense_definition": "Mający duże znaczenie, wpływ lub wagę; wymagający uwagi i niebędący błahostką.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest bardzo <w>poważna</w> decyzja, więc musimy ją dobrze przemyśleć.",
                            "target_lang_translations": {
                                "ru": "Это очень <w>серьёзное</w> решение, поэтому мы должны его хорошо обдумать."
                            }
                        },
                        {
                            "text": "Mamy <w>poważny</w> problem z silnikiem samochodu i nie możemy jechać dalej.",
                            "target_lang_translations": {
                                "ru": "У нас <w>серьёзная</w> проблема с двигателем машины, и мы не можем ехать дальше."
                            }
                        }
                    ],
                    "synonyms": [
                        "ważny",
                        "istotny",
                        "doniosły",
                        "kluczowy",
                        "niebagatelny",
                        "zasadniczy"
                    ],
                    "antonyms": [

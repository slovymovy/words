    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8d98d548-7cfe-3037-a586-839010bdb6a9",
                    "sense_definition": "Сильно удивить, изумить или произвести очень глубокое впечатление.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта неожиданная новость <поразила> всех присутствующих в комнате.",
                            "target_lang_translations": {
                                "en": "This unexpected news <astounded> everyone present in the room."
                            }
                        },
                        {
                            "text": "Её красота и элегантность <поразили> меня с первого взгляда.",
                            "target_lang_translations": {
                                "en": "Her beauty and elegance <struck> me at first sight."
                            }
                        }
                    ],
                    "synonyms": [
                        "удивить",
                        "изумить"
                    ],
                    "common_phrases": [
                        "поразить воображение",
                        "приятно поразить",
                        "поразить до глубины души"
                    ],
                    "target_lang_definitions": {
                        "en": "To greatly surprise, amaze, or make a very deep impression."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "amaze",
                                "target_lang_sense_clarification": "to surprise someone greatly"
                            },
                            {
                                "target_lang_word": "astound",
                                "target_lang_sense_clarification": "to shock or surprise very much"
                            },
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "to make a strong impression on someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2902b32-dbf4-3e7e-bd5e-4d77b44eaefd",
                    "sense_definition": "Нанести вред здоровью, повредить орган или организм (о болезни, вирусе или инфекции).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опасный вирус <поразил> компьютерную сеть крупной компании.",
                            "target_lang_translations": {
                                "en": "A dangerous virus <infected> the large company's computer network."
                            }
                        },
                        {
                            "text": "Болезнь быстро <поразила> лёгкие пациента, и ему потребовалось лечение.",
                            "target_lang_translations": {
                                "en": "The disease quickly <affected> the patient's lungs, and he required treatment."
                            }
                        }
                    ],
                    "synonyms": [
                        "изумить"
                    ],
                    "common_phrases": [
                        "поразить органы",
                        "поразить нервную систему",
                        "вирус поразил"
                    ],
                    "target_lang_definitions": {
                        "en": "To harm health, damage an organ or organism (referring to a disease, virus, or infection)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "affect",
                                "target_lang_sense_clarification": "to attack or damage a part of the body"
                            },
                            {
                                "target_lang_word": "infect",
                                "target_lang_sense_clarification": "to contaminate with a virus or disease"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02729ca6-f34f-3045-88e6-b4db489fb82a",
                    "sense_definition": "Попасть в цель при стрельбе или нанести удар оружием.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Снайпер сумел <поразить> удалённую цель с первого выстрела.",
                            "target_lang_translations": {
                                "en": "The sniper managed to <hit> the distant target with the first shot."
                            }
                        },
                        {
                            "text": "Рыцарь стремился <поразить> дракона своим острым мечом.",
                            "target_lang_translations": {
                                "en": "The knight strove to <strike> the dragon with his sharp sword."
                            }
                        }
                    ],
                    "common_phrases": [
                        "поразить цель",
                        "поразить мишень",
                        "поразить копьём"
                    ],
                    "target_lang_definitions": {
                        "en": "To hit a target when shooting or to strike with a weapon."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hit",
                                "target_lang_sense_clarification": "to successfully reach a target"
                            },
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "to hit forcibly with a weapon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "783653cf-1618-3ad3-b640-db85f13598e2",
                    "sense_definition": "Одержать победу над врагом, разгромить противника в бою.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Армия готовилась <поразить> врага в решающем сражении.",
                            "target_lang_translations": {
                                "en": "The army was preparing to <defeat> the enemy in a decisive battle."
                            }
                        },
                        {
                            "text": "Нам необходимо <поразить> неприятеля, чтобы защитить наш город.",
                            "target_lang_translations": {
                                "en": "We need to <defeat> the adversary to protect our city."
                            }
                        }
                    ],
                    "common_phrases": [
                        "поразить врага",
                        "поразить противника"
                    ],
                    "target_lang_definitions": {
                        "en": "To achieve victory over an enemy, to crush an opponent in battle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defeat",
                                "target_lang_sense_clarification": "to win a victory over an enemy"
                            },
                            {
                                "target_lang_word": "vanquish",
                                "target_lang_sense_clarification": "literary: to defeat thoroughly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "przyjmować",
        "przyjęcie",
        "przyjęty"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a8278322-bc93-36bd-a218-b3df2bf897f4",
                    "sense_definition": "Wziąć coś, co ktoś daje, oferuje lub przekazuje; zgodzić się na przyjęcie czegoś",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_acceptance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <w>przyjąć</w> ten prezent, ponieważ został kupiony specjalnie dla ciebie.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>принять</w> этот подарок, так как он был куплен специально для тебя."
                            }
                        },
                        {
                            "text": "On nie chciał <w>przyjąć</w> pieniędzy za swoją pomoc, mimo że nalegaliśmy.",
                            "target_lang_translations": {
                                "ru": "Он не хотел <w>брать</w> деньги за свою помощь, хотя мы настаивали."
                            }
                        }
                    ],
                    "synonyms": [
                        "wchłonąć",
                        "wchłonąć"
                    ],
                    "common_phrases": [
                        "przyjąć prezent",
                        "przyjąć zaproszenie",
                        "przyjąć ofertę",
                        "przyjąć pomoc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять то, что кто-то дает, предлагает или передает; согласиться принять что-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принять",
                                "target_lang_sense_clarification": "подарок или предложение"
                            },
                            {
                                "target_lang_word": "взять",
                                "target_lang_sense_clarification": "деньги или вещь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bcfdbb4-2625-3505-baf0-ea14ebf6dc1f",
                    "sense_definition": "Pozwolić komuś wejść, ugościć kogoś w domu lub zgodzić się na spotkanie w charakterze oficjalnym (np. lekarz, urzędnik)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz może cię <w>przyjąć</w> dopiero w przyszłym tygodniu po południu.",
                            "target_lang_translations": {
                                "ru": "Врач сможет <w>принять</w> тебя только на следующей неделе после обеда."
                            }
                        },
                        {
                            "text": "Gospodarze postanowili <w>przyjąć</w> gości w salonie przy kominku.",
                            "target_lang_translations": {
                                "ru": "Хозяева решили <w>принять</w> гостей в гостиной у камина."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przyjąć gości",
                        "lekarz przyjmuje",
                        "przyjąć pacjenta",
                        "godnie przyjąć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Впустить кого-то, принять гостей дома или согласиться на встречу в официальном порядке (например, врач, чиновник)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принять",
                                "target_lang_sense_clarification": "посетителя или гостя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74668a5d-4a35-3c3a-824a-5e44a1a0f32d",
                    "sense_definition": "Zatrudnić kogoś do pracy lub zaakceptować kogoś jako nowego członka organizacji, szkoły lub grupy",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "employment_membership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma zdecydowała się <w>przyjąć</w> dwóch nowych pracowników do działu marketingu.",
                            "target_lang_translations": {
                                "ru": "Компания решила <w>принять</w> двух новых сотрудников в отдел маркетинга."
                            }
                        },
                        {
                            "text": "Uniwersytet planuje <w>przyjąć</w> w tym roku więcej studentów zagranicznych.",
                            "target_lang_translations": {
                                "ru": "Университет планирует <w>принять</w> в этом году больше иностранных студентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatrudnić się",
                        "zaangażować się"
                    ],
                    "antonyms": [
                        "zwolnić się"
                    ],
                    "common_phrases": [
                        "przyjąć do pracy",
                        "przyjąć do szkoły",
                        "zostać przyjętym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанять кого-то на работу или принять кого-то в качестве нового члена организации, школы или группы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принять",
                                "target_lang_sense_clarification": "на работу или учебу"
                            },
                            {
                                "target_lang_word": "зачислить",
                                "target_lang_sense_clarification": "в учебное заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba662797-32c8-33d9-a244-c06134f76178",
                    "sense_definition": "Zażyć lekarstwo, zjeść posiłek lub wchłonąć jakąś substancję",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "consumption_absorption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent musi <w>przyjąć</w> kolejną dawkę antybiotyku dokładnie za sześć godzin.",
                            "target_lang_translations": {
                                "ru": "Пациент должен <w>принять</w> следующую дозу антибиотика ровно через шесть часов."
                            }
                        },
                        {
                            "text": "Sucha ziemia może szybko <w>przyjąć</w> dużą ilość wody deszczowej.",
                            "target_lang_translations": {
                                "ru": "Сухая земля может быстро <w>впитать</w> большое количество дождевой воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "wchłonąć"
                    ],
                    "common_phrases": [
                        "przyjąć leki",
                        "przyjąć dawkę",
                        "przyjąć płyny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять лекарство, съесть пищу или впитать какое-либо вещество"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принять",
                                "target_lang_sense_clarification": "лекарство"
                            },
                            {
                                "target_lang_word": "впитать",
                                "target_lang_sense_clarification": "жидкость (о поверхности)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64fcf641-65b4-308d-b100-7a9ee5407c94",
                    "sense_definition": "Zareagować na wiadomość lub sytuację w określony sposób; uznać coś za prawdę",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cognitive_reaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój ojciec bardzo spokojnie potrafił <w>przyjąć</w> tę niespodziewaną wiadomość.",
                            "target_lang_translations": {
                                "ru": "Мой отец смог очень спокойно <w>принять</w> эту неожиданную новость."
                            }
                        },
                        {
                            "text": "Musisz <w>przyjąć</w> do wiadomości, że termin oddania projektu już minął.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>принять</w> к сведению, что срок сдачи проекта уже истек."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przyjąć do wiadomości",
                        "źle przyjąć",
                        "przyjąć z ulgą",
                        "przyjąć założenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отреагировать на новость или ситуацию определенным образом; признать что-то истинным"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принять",
                                "target_lang_sense_clarification": "новость или ситуацию"
                            },
                            {
                                "target_lang_word": "воспринять",
                                "target_lang_sense_clarification": "эмоционально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53d40afe-59e1-3738-8b3e-a993c10a1918",
                    "sense_definition": "Wziąć na siebie jakiś obowiązek, zadanie lub odpowiedzialność",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierownik musiał <w>przyjąć</w> pełną odpowiedzialność za błędy swojego zespołu.",
                            "target_lang_translations": {
                                "ru": "Руководитель должен был <w>принять</w> полную ответственность за ошибки своей команды."
                            }
                        },
                        {
                            "text": "Nie chcę <w>przyjąć</w> na siebie kolejnych zadań, bo mam już za dużo pracy.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <w>брать</w> на себя новые задачи, потому что у меня и так слишком много работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przyjąć odpowiedzialność",
                        "przyjąć wyzwanie",
                        "przyjąć obowiązki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять на себя обязанность, задачу или ответственность"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принять",
                                "target_lang_sense_clarification": "ответственность"
                            },
                            {
                                "target_lang_word": "взять на себя",
                                "target_lang_sense_clarification": "обязательства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f547bae1-2358-3e02-9cde-537f02fab397",
                    "sense_definition": "Zakorzenić się (o roślinach) lub stać się popularnym i powszechnym (o modzie, zwyczajach); używane z 'się'",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_rooting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To egzotyczne drzewo może się nie <w>przyjąć</w> w naszym chłodnym klimacie.",
                            "target_lang_translations": {
                                "ru": "Это экзотическое дерево может не <w>прижиться</w> в нашем холодном климате."
                            }
                        },
                        {
                            "text": "Nowa moda na szerokie spodnie szybko zdążyła się <w>przyjąć</w> wśród młodzieży.",
                            "target_lang_translations": {
                                "ru": "Новая мода на широкие брюки быстро успела <w>прижиться</w> среди молодежи."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakorzenić się"
                    ],
                    "common_phrases": [
                        "przyjąć się",
                        "nie przyjąć się",
                        "szybko się przyjąć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Укорениться (о растениях) или стать популярным и распространенным (о моде, обычаях); используется с возвратной частицей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прижиться",
                                "target_lang_sense_clarification": "о растениях или моде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cb961de-edf4-386d-8716-c2acb5802429",
                    "sense_definition": "Zatwierdzić coś oficjalnie, np. ustawę przez głosowanie",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "formal_approval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Parlament ma dzisiaj <w>przyjąć</w> nową ustawę dotyczącą ochrony środowiska.",
                            "target_lang_translations": {
                                "ru": "Парламент должен сегодня <w>принять</w> новый закон об охране окружающей среды."
                            }
                        },
                        {
                            "text": "Komisja postanowiła jednogłośnie <w>przyjąć</w> ten wniosek bez poprawek.",
                            "target_lang_translations": {
                                "ru": "Комиссия решила единогласно <w>принять</w> это предложение без поправок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przyjąć ustawę",
                        "przyjąć uchwałę",
                        "przyjąć wniosek"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "official/legal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально утвердить что-либо, например, закон путем голосования"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принять",
                                "target_lang_sense_clarification": "закон или решение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "94000368-7ce3-3b15-b17a-ad4635458f66",
                    "sense_definition": "Rocznica dnia, w którym ktoś się urodził.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anniversary_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ewelina dostała na <w>urodziny</w> ciekawą książkę od rodziców.",
                            "target_lang_translations": {
                                "ru": "Эвелина получила на <w>день рождения</w> интересную книгу от родителей."
                            }
                        },
                        {
                            "text": "Wszystkiego najlepszego z okazji twoich osiemnastych <w>urodzin</w>!",
                            "target_lang_translations": {
                                "ru": "Всего самого лучшего в твой восемнадцатый <w>день рождения</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) geburstag, gyburstag"
                    ],
                    "common_phrases": [
                        "obchodzić urodziny",
                        "prezent na urodziny",
                        "z okazji urodzin",
                        "mieć urodziny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Годовщина дня, в который кто-то родился."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "день рождения",
                                "target_lang_sense_clarification": "ежегодный праздник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c293bf46-5ad6-3372-b4fe-c430841d217b",
                    "sense_definition": "Przyjęcie towarzyskie organizowane dla uczczenia rocznicy czyichś narodzin.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anniversary_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sobotę idę na <w>urodziny</w> do mojego najlepszego kolegi z pracy.",
                            "target_lang_translations": {
                                "ru": "В субботу я иду на <w>день рождения</w> к моему лучшему коллеге с работы."
                            }
                        },
                        {
                            "text": "Zorganizowaliśmy huczne <w>urodziny</w>, na które zaprosiliśmy całą rodzinę.",
                            "target_lang_translations": {
                                "ru": "Мы организовали шумный <w>день рождения</w>, на который пригласили всю семью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iść na urodziny",
                        "zaprosić na urodziny",
                        "przyjęcie urodzinowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вечеринка или приём, организованные в честь чьего-либо дня рождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "день рождения",
                                "target_lang_sense_clarification": "празднование, вечеринка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "159a151c-692c-3cf8-aabd-7cb185a316f9",
                    "sense_definition": "Fakt przyjścia na świat; moment narodzenia się.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "act_of_birth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Data jego <w>urodzin</w> została błędnie zapisana w dokumentach urzędowych.",
                            "target_lang_translations": {
                                "ru": "Дата его <w>рождения</w> была ошибочно записана в официальных документах."
                            }
                        },
                        {
                            "text": "Od dnia <w>urodzin</w> mieszkał w tym samym małym miasteczku.",
                            "target_lang_translations": {
                                "ru": "Со дня <w>рождения</w> он жил в том же маленьком городке."
                            }
                        }
                    ],
                    "synonyms": [
                        "narodziny"
                    ],
                    "antonyms": [
                        "śmierć"
                    ],
                    "common_phrases": [
                        "dzień urodzin",
                        "miejsce urodzin",
                        "data urodzin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт появления на свет; момент рождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рождение",
                                "target_lang_sense_clarification": "появление на свет"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "60f1f774-e0dd-3b66-9041-ac5f5d98f09a",
                    "sense_definition": "Het eerste gedeelte van een tekst of toespraak dat als voorbereiding dient.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

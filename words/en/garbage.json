            "senses": [
                {
                    "sense_id": "f3bbf285-e5d6-3b27-8ede-5ebfbb335c29",
                    "sense_definition": "Waste material of any kind, such as food waste or discarded objects.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "waste_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please take out the <w>garbage</w> before you go to sleep.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, вынеси <w>мусор</w> перед сном."
                            }
                        },
                        {
                            "text": "The <w>garbage</w> truck comes to our street every Tuesday morning.",
                            "target_lang_translations": {
                                "ru": "<w>Мусоровоз</w> приезжает на нашу улицу каждый вторник утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "trash",
                        "rubbish",
                        "waste",
                        "refuse",
                        "junk"
                    ],
                    "antonyms": [
                        "treasure",
                        "valuable",
                        "asset",
                        "resource",
                        "recyclable"
                    ],
                    "common_phrases": [
                        "garbage truck",
                        "garbage can",
                        "take out the garbage",
                        "garbage bag",
                        "household garbage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отходы любого вида, такие как пищевые отходы или выброшенные предметы."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "059b3ff7-524c-3f9c-a9d9-6d0efad93046",
                    "sense_definition": "An informal word used to say no.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "negation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you want to come to the cinema? <w>Nah</w>, I've already seen that movie.",
                            "target_lang_translations": {
                                "ru": "Хочешь пойти в кино? — <w>Не-а</w>, я уже видел этот фильм."
                            }
                        },
                        {
                            "text": "<w>Nah</w>, I don't think we should go there tonight.",
                            "target_lang_translations": {
                                "ru": "<w>Да нет</w>, не думаю, что нам стоит туда идти сегодня вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "naw"
                    ],
                    "antonyms": [
                        "yeah",
                        "yep"
                    ],
                    "common_phrases": [
                        "nah thanks",
                        "nah I'm good"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное слово для выражения отрицания или несогласия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не-а",
                                "target_lang_sense_clarification": "разговорный вариант"
                            },
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "да нет",
                                "target_lang_sense_clarification": "мягкое отрицание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c037ead-0c5f-3d7f-bb9e-4a118b2b1a19",
                    "sense_definition": "Used when handing something to someone.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Nah</w>, take this money and buy yourself some lunch.",
                            "target_lang_translations": {
                                "ru": "<w>На</w>, возьми деньги и купи себе что-нибудь на обед."
                            }
                        },
                        {
                            "text": "He held out the keys and said, \"<w>Nah</w>, you drive.\"",
                            "target_lang_translations": {
                                "ru": "Он протянул ключи и сказал: «<w>Держи</w>, ты поведешь»."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "used when giving something"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется при передаче предмета другому человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "при протягивании предмета"
                            },
                            {
                                "target_lang_word": "держи",
                                "target_lang_sense_clarification": "побуждение взять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cd51f49-6e56-3c3e-8f38-65a0eb32df1d",
                    "sense_definition": "An acronym for \"no assholes here\", used in online discussions to indicate that no one is at fault.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "internet_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After reading the full story, most users voted <w>NAH</w>.",
                            "target_lang_translations": {
                                "ru": "Прочитав историю целиком, большинство пользователей проголосовало за вариант «<w>никто не виноват</w>»."
                            }
                        },
                        {
                            "text": "The situation was complicated, but the final verdict was <w>NAH</w>.",
                            "target_lang_translations": {
                                "ru": "Ситуация была сложной, но окончательный вердикт был: «<w>никто не виноват</w>»."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от «no assholes here» (здесь нет мудаков), используемая в интернет-обсуждениях, чтобы указать, что в ситуации никто не виноват."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никто не виноват",
                                "target_lang_sense_clarification": "значение аббревиатуры"
                            }
                        ]
                    }
                }
            ]
        },

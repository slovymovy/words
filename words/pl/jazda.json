    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "08061d76-33da-3229-a562-0aef8a77710a",
                    "sense_definition": "Przemieszczanie się z jednego miejsca do drugiego za pomocą środka transportu lub na zwierzęciu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szybka <w>jazda</w> samochodem w deszczu jest bardzo niebezpieczna.",
                            "target_lang_translations": {
                                "ru": "Быстрая <w>езда</w> на машине в дождь очень опасна."
                            }
                        },
                        {
                            "text": "Jutro czeka nas długa <w>jazda</w> pociągiem nad morze.",
                            "target_lang_translations": {
                                "ru": "Завтра нас ждет долгая <w>поездка</w> на поезде к морю."
                            }
                        }
                    ],
                    "synonyms": [
                        "podróż",
                        "droga"
                    ],
                    "common_phrases": [
                        "jazda konna",
                        "jazda samochodem",
                        "jazda próbna",
                        "jazda bez trzymanki",
                        "nauka jazdy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещение из одного места в другое на транспортном средстве или верхом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "езда",
                                "target_lang_sense_clarification": "процесс движения"
                            },
                            {
                                "target_lang_word": "поездка",
                                "target_lang_sense_clarification": "путешествие, перемещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26c155be-b40b-3033-aa19-141a99802538",
                    "sense_definition": "Formacja wojskowa, w której żołnierze walczą, siedząc na koniach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oddział lekkiej <w>jazdy</w> zaatakował wroga z zaskoczenia.",
                            "target_lang_translations": {
                                "ru": "Отряд легкой <w>кавалерии</w> внезапно атаковал врага."
                            }
                        },
                        {
                            "text": "W dawnych bitwach <w>jazda</w> często decydowała o ostatecznym zwycięstwie.",
                            "target_lang_translations": {
                                "ru": "В давних битвах <w>конница</w> часто решала исход победы."
                            }
                        }
                    ],
                    "synonyms": [
                        "kawaleria",
                        "konnica"
                    ],
                    "common_phrases": [
                        "lekka jazda",
                        "ciężka jazda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воинское формирование, в котором солдаты сражаются верхом на лошадях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кавалерия",
                                "target_lang_sense_clarification": "род войск"
                            },
                            {
                                "target_lang_word": "конница",
                                "target_lang_sense_clarification": "конное войско"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2885f156-3f97-3adb-8f24-dde2b87b735b",
                    "sense_definition": "Proces drukowania, szczególnie w odniesieniu do dużego nakładu materiałów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "printing_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mamy trzy palety papieru do <w>jazdy</w> na dzisiejszą nocną zmianę.",
                            "target_lang_translations": {
                                "ru": "У нас три паллеты бумаги для <w>печати</w> на сегодняшнюю ночную смену."
                            }
                        },
                        {
                            "text": "Maszyna drukarska jest gotowa, więc zaczynamy <w>jazdę</w> z tym zleceniem.",
                            "target_lang_translations": {
                                "ru": "Печатный станок готов, так что начинаем <w>печать</w> этого заказа."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "branżowy żargon drukarski"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс печати, особенно в отношении большого тиража материалов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печать",
                                "target_lang_sense_clarification": "процесс изготовления печатной продукции"
                            },
                            {
                                "target_lang_word": "тиражирование",
                                "target_lang_sense_clarification": "размножение материала"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d83f82c4-023a-369f-ae8b-01eda635594a",
                    "sense_definition": "Okrzyk ponaglający kogoś do pośpiechu lub natychmiastowego rozpoczęcia działania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "imperative_command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "No już, <w>jazda</w> do łóżka, bo jest bardzo późno!",
                            "target_lang_translations": {
                                "ru": "Ну всё, <w>марш</w> в кровать, уже очень поздно!"
                            }
                        },
                        {
                            "text": "Wstawać, <w>jazda</w> do szkoły, bo się spóźnicie!",
                            "target_lang_translations": {
                                "ru": "Вставать, <w>бегом</w> в школу, а то опоздаете!"
                            }
                        }
                    ],
                    "synonyms": [
                        "do roboty"
                    ],
                    "common_phrases": [
                        "jazda z tym"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, побуждающий кого-либо к спешке или немедленному началу действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "команда идти"
                            },
                            {
                                "target_lang_word": "бегом",
                                "target_lang_sense_clarification": "призыв к быстрому движению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0451d684-1726-352f-b0fa-a9e21bb8bea3",
                    "sense_definition": "Niegrzeczne żądanie, aby ktoś natychmiast opuścił dane miejsce.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "imperative_command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jazda</w> stąd, nie chcę cię tu więcej widzieć!",
                            "target_lang_translations": {
                                "ru": "<w>Вон</w> отсюда, не хочу тебя больше здесь видеть!"
                            }
                        },
                        {
                            "text": "Jeśli nie masz biletu, to <w>jazda</w> z autobusu!",
                            "target_lang_translations": {
                                "ru": "Если нет билета, то <w>вон</w> из автобуса!"
                            }
                        }
                    ],
                    "synonyms": [
                        "jazda stąd",
                        "precz",
                        "precz stąd"
                    ],
                    "common_phrases": [
                        "jazda mi stąd"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często nacechowane negatywnie lub agresywnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубое требование немедленно покинуть место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вон",
                                "target_lang_sense_clarification": "требование уйти"
                            },
                            {
                                "target_lang_word": "убирайся",
                                "target_lang_sense_clarification": "грубый приказ уйти"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "Vietnamese",
        "Viet"
    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "27e17355-3065-3949-945e-f79a079e1ce1",
                    "sense_definition": "A country in Southeast Asia. Official name: Socialist Republic of Vietnam. Capital: Hanoi.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geo_location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "I am planning to travel to <w>Vietnam</w> for my summer vacation.",
                            "target_lang_translations": {
                                "ru": "Я планирую поехать во <w>Вьетнам</w> на летние каникулы."
                            }
                        },
                        {
                            "text": "Hanoi is the capital city of <w>Vietnam</w> and is known for its history.",
                            "target_lang_translations": {
                                "ru": "Ханой — столица <w>Вьетнама</w>, известная своей историей."
                            }
                        }
                    ],
                    "synonyms": [
                        "Socialist Republic of Vietnam"
                    ],
                    "common_phrases": [
                        "travel to Vietnam",
                        "made in Vietnam",
                        "people of Vietnam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страна в Юго-Восточной Азии. Официальное название: Социалистическая Республика Вьетнам. Столица: Ханой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Вьетнам",
                                "target_lang_sense_clarification": "страна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ae9acce-3c75-3b05-af57-a1eaced6d207",
                    "sense_definition": "The Vietnam War.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "historical_event",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "My grandfather served in <w>Vietnam</w> and often tells stories about his time there.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка служил во <w>Вьетнаме</w> и часто рассказывает истории о том времени."
                            }
                        },
                        {
                            "text": "Many movies have been produced that depict the events of <w>Vietnam</w>.",
                            "target_lang_translations": {
                                "ru": "Было снято много фильмов, изображающих события во <w>Вьетнаме</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Nam"
                    ],
                    "common_phrases": [
                        "served in Vietnam",
                        "Vietnam veteran",
                        "Vietnam era"
                    ],
                    "target_lang_definitions": {
                        "ru": "Война во Вьетнаме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Вьетнам",
                                "target_lang_sense_clarification": "война"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2777a884-1c96-34d0-8b6e-d138f2e1a96b",
                    "sense_definition": "A long and protracted war or conflict in which the dominating foreign occupier is unable to secure a victory.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor_conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government feared that the intervention would turn into another <w>Vietnam</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство опасалось, что интервенция превратится в очередной <w>Вьетнам</w>."
                            }
                        },
                        {
                            "text": "Critics warned that the invasion could become a <w>Vietnam</w> for the occupying forces.",
                            "target_lang_translations": {
                                "ru": "Критики предупреждали, что вторжение может стать <w>Вьетнамом</w> для оккупационных сил."
                            }
                        }
                    ],
                    "common_phrases": [
                        "another Vietnam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длительный и затяжной конфликт или война, в которой невозможно одержать победу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Вьетнам",
                                "target_lang_sense_clarification": "символ затяжной, безнадежной войны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "543ac90f-a55b-393d-8f47-1daca3398e05",
                    "sense_definition": "A situation in which one cannot win.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "metaphor_situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The disastrous project became his personal <w>Vietnam</w> after months of failure.",
                            "target_lang_translations": {
                                "ru": "Этот провальный проект стал его личным <w>Вьетнамом</w> после месяцев неудач."
                            }
                        },
                        {
                            "text": "She felt that the complicated legal battle was turning into her own <w>Vietnam</w>.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала, что сложная судебная тяжба превращается в её собственный <w>Вьетнам</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "personal Vietnam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, из которой невозможно выйти победителем; провальное дело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Вьетнам",
                                "target_lang_sense_clarification": "личный провал, безвыходная ситуация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "949d736e-2f20-3ec6-bcec-0ecd920912dd",
                    "sense_definition": "A place that is very dangerous, especially if afflicted by violence.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "metaphor_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The neighborhood was so violent that the locals referred to it as <w>Vietnam</w>.",
                            "target_lang_translations": {
                                "ru": "Район был настолько жестоким, что местные называли его <w>Вьетнамом</w>."
                            }
                        },
                        {
                            "text": "Police officers described the chaotic district as a <w>Vietnam</w> during the riots.",
                            "target_lang_translations": {
                                "ru": "Во время беспорядков полицейские описывали этот хаотичный район как <w>Вьетнам</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used to describe a violent place"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень опасное место, особенно если там происходит насилие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Вьетнам",
                                "target_lang_sense_clarification": "опасное место, «горячая точка»"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
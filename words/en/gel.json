                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девушка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "девица",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a8e255e9-33c5-322b-bf68-da3c64868cb6",
                    "sense_definition": "To apply a styling substance to the hair to shape it.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "cosmetic_gel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He washed and <w>gelled</w> his hair before going to the party.",
                            "target_lang_translations": {
                                "ru": "Он вымыл и <w>уложил гелем</w> волосы перед тем, как пойти на вечеринку."
                            }
                        },
                        {
                            "text": "She likes to <w>gel</w> her short hair to make it look spiky.",
                            "target_lang_translations": {
                                "ru": "Ей нравится <w>укладывать гелем</w> свои короткие волосы, чтобы они торчали ежиком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gel one's hair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить гель на волосы для придания им формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укладывать гелем",
                                "target_lang_sense_clarification": "волосы"
                            },
                            {
                                "target_lang_word": "наносить гель",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7d15805-2ea6-3745-b380-b3ac4256c741",
                    "sense_definition": "To work well together or develop a good relationship with someone.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_coherence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new players <w>gelled</w> quickly with the rest of the team.",
                            "target_lang_translations": {
                                "ru": "Новые игроки быстро <w>сработались</w> с остальной командой."
                            }
                        },
                        {
                            "text": "It took some time, but the group finally started to <w>gel</w>.",
                            "target_lang_translations": {
                                "ru": "Это заняло некоторое время, но группа наконец начала <w>сплачиваться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bond",
                        "click"
                    ],
                    "antonyms": [
                        "clash"
                    ],
                    "common_phrases": [
                        "gel well",
                        "gel together"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо работать вместе, ладить или налаживать хорошие отношения с кем-либо."

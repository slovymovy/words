                },
                {
                    "sense_id": "75ec63f0-32b9-3a6c-9332-b49140cdf6e5",
                    "sense_definition": "A Niger-Congo language spoken in Togo.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "language_name",
                    "examples": [
                        {

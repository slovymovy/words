    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8befa8ed-2f30-3a23-ad93-8663780a3be6",
                    "sense_definition": "Taśma, pasek lub łańcuszek zapinany na szyi zwierzęcia domowego, służący do jego prowadzenia, identyfikacji lub ozdoby.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz szczeniak już dorasta, więc trzeba kupić mu nową <w>obrożę</w>.",
                            "target_lang_translations": {
                                "ru": "Наш щенок уже подрастает, поэтому нужно купить ему новый <w>ошейник</w>."
                            }
                        },
                        {
                            "text": "Pies zerwał się z <w>obroży</w> i pobiegł za kotem.",
                            "target_lang_translations": {
                                "ru": "Пес сорвался с <w>ошейника</w> и побежал за котом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obroża dla psa",
                        "skórzana obroża",
                        "obroża przeciwpchelna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремешок, лента или цепочка, надеваемая на шею домашнего животного для выгула, идентификации или украшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ошейник",
                                "target_lang_sense_clarification": "для животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "512687fe-936b-33e1-a919-c976ad80d0e5",
                    "sense_definition": "Barwny pas piór lub sierści wokół szyi zwierzęcia, odróżniający się kolorem od reszty ciała.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten gatunek ptaka wyróżnia się czerwoną <w>obrożą</w> na szyi.",
                            "target_lang_translations": {
                                "ru": "Этот вид птиц выделяется красным <w>ошейником</w> на шее."
                            }
                        },
                        {
                            "text": "Samce mają wyraźną czarną <w>obrożę</w>, której brakuje samicom.",
                            "target_lang_translations": {
                                "ru": "У самцов есть отчетливый черный <w>ошейник</w>, который отсутствует у самок."
                            }
                        }
                    ],
                    "synonyms": [
                        "kołnierz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полоса перьев или шерсти вокруг шеи животного, отличающаяся по цвету от остального тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ошейник",
                                "target_lang_sense_clarification": "окрас"
                            },
                            {
                                "target_lang_word": "воротник",
                                "target_lang_sense_clarification": "оперение или шерсть"
                            }
                        ]
                    }
                }
            ]
        }
    ]

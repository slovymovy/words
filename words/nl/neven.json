                                "target_lang_sense_clarification": "sons of siblings"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "двоюродные братья",
                                "target_lang_sense_clarification": "сыновья дяди или тёти"
                            },
                            {
                                "target_lang_word": "племянники",
                                "target_lang_sense_clarification": "сыновья брата или сестры"
                            }

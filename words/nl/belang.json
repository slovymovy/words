                {
                    "sense_id": "3cd7a2bf-1145-3c75-8586-707cdfb08a0c",
                    "sense_definition": "De mate waarin iets of iemand betekenis heeft, invloed uitoefent of aandacht verdient.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is van groot <w>belang</w> dat je op tijd komt voor de vergadering.",
                            "target_lang_translations": {
                                "en": "It is of great <w>importance</w> that you arrive on time for the meeting.",
                                "ru": "Имеет большое <w>значение</w>, чтобы вы пришли на собрание вовремя."
                            }
                        },
                        {
                            "text": "Zijn mening is voor ons van weinig <w>belang</w>.",
                            "target_lang_translations": {
                                "en": "His opinion is of little <w>importance</w> to us.",
                                "ru": "Его мнение имеет для нас малое <w>значение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "belangrijkheid"
                    ],
                    "common_phrases": [
                        "van belang",
                        "van groot belang",
                        "van levensbelang",
                        "een drukte van belang"
                    ],
                    "target_lang_definitions": {
                        "en": "The degree to which something or someone has meaning, exerts influence, or deserves attention.",
                        "ru": "Степень, в которой что-либо или кто-либо имеет значение, оказывает влияние или заслуживает внимания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "importance",
                                "target_lang_sense_clarification": "significance or value"
                            },
                            {
                                "target_lang_word": "significance",
                                "target_lang_sense_clarification": "quality of being worthy of attention"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "значение",
                                "target_lang_sense_clarification": "смысл, важность, влияние"
                            },
                            {
                                "target_lang_word": "важность",
                                "target_lang_sense_clarification": "значимость, весомость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75c728cd-675b-3fc7-a4e1-06b422bd0342",
                    "sense_definition": "Iets wat voordeel oplevert of noodzakelijk is voor het welzijn van een persoon of groep.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De burgemeester nam een beslissing in het <w>belang</w> van de hele stad.",
                            "target_lang_translations": {
                                "en": "The mayor made a decision in the <w>interest</w> of the entire city.",
                                "ru": "Мэр принял решение в <w>интересах</w> всего города."
                            }
                        },
                        {
                            "text": "Hij heeft er <w>belang</w> bij dat het project snel wordt afgerond.",
                            "target_lang_translations": {
                                "en": "He has an <w>interest</w> in the project being completed quickly.",
                                "ru": "В его <w>интересах</w>, чтобы проект был завершен быстро."
                            }
                        },
                        {
                            "text": "De advocaat verdedigt de <w>belangen</w> van zijn cliënt in de rechtbank.",
                            "target_lang_translations": {
                                "en": "The lawyer defends the <w>interests</w> of his client in court.",
                                "ru": "Адвокат защищает <w>интересы</w> своего клиента в суде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in het belang van",
                        "algemeen belang",
                        "eigen belang",
                        "belangen behartigen"
                    ],
                    "target_lang_definitions": {

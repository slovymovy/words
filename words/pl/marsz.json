{
    "word_family": [
        "maszerować",
        "wymarsz",
        "marszowy",
        "marszruta"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d115de67-6b04-3ed9-bd0c-df6b599e0efd",
                    "sense_definition": "Równomierny, energiczny chód, często wykonywany w określonym tempie lub rytmie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dwie godziny szybkim <w>marszem</w> i będziesz w domu.",
                            "target_lang_translations": {
                                "ru": "Два часа быстрой <w>ходьбы</w> — и ты дома."
                            }
                        },
                        {
                            "text": "Lekarz zalecił mu codzienny <w>marsz</w> dla poprawy kondycji.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал ему ежедневную <w>ходьбу</w> для улучшения физической формы."
                            }
                        }
                    ],
                    "synonyms": [
                        "chód"
                    ],
                    "common_phrases": [
                        "szybki marsz",
                        "marszobieg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Равномерная, энергичная ходьба, часто выполняемая в определенном темпе или ритме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходьба",
                                "target_lang_sense_clarification": "способ передвижения"
                            },
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "ритмичное движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d12bb771-e318-3491-962b-272b1670dbc7",
                    "sense_definition": "Utwór muzyczny o wyraźnym, regularnym rytmie, pierwotnie przeznaczony do towarzyszenia pochodom wojskowym lub uroczystościom.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Orkiestra zagrała żałobnego <w>marsza</w> podczas uroczystości pogrzebowych.",
                            "target_lang_translations": {
                                "ru": "Оркестр сыграл похоронный <w>марш</w> во время траурной церемонии."
                            }
                        },
                        {
                            "text": "Para młoda weszła do kościoła, gdy organy zaczęły grać <w>marsz</w> weselny.",
                            "target_lang_translations": {
                                "ru": "Молодожены вошли в церковь, когда орган начал играть свадебный <w>марш</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "marsz weselny",
                        "marsz żałobny",
                        "marsz wojskowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальное произведение с четким, регулярным ритмом, изначально предназначенное для сопровождения военных шествий или торжеств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "музыкальный жанр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7814bcf6-6605-3299-96ba-751166680aaf",
                    "sense_definition": "Zorganizowane przejście dużej grupy ludzi ulicami miasta, mające na celu wyrażenie poparcia dla jakiejś idei lub protestu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W centrum miasta odbył się <w>marsz</w> przeciwko przemocy.",
                            "target_lang_translations": {
                                "ru": "В центре города прошел <w>марш</w> против насилия."
                            }
                        },
                        {
                            "text": "Uczestnicy <w>marszu</w> nieśli flagi i transparenty z hasłami.",
                            "target_lang_translations": {
                                "ru": "Участники <w>марша</w> несли флаги и транспаранты с лозунгами."
                            }
                        }
                    ],
                    "synonyms": [
                        "pochód",
                        "pochód"
                    ],
                    "common_phrases": [
                        "marsz milczenia",
                        "marsz niepodległości",
                        "marsz protestacyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованное шествие большой группы людей по улицам города с целью выражения поддержки какой-либо идеи или протеста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "демонстрация, акция"
                            },
                            {
                                "target_lang_word": "шествие",
                                "target_lang_sense_clarification": "массовое движение людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21f3a8ba-12d7-3466-9566-2dde51fd5855",
                    "sense_definition": "Zorganizowane przemieszczanie się oddziałów wojskowych z jednego miejsca na drugie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hetman zdołał zatrzymać <w>marsz</w> wrogich wojsk już na granicy.",
                            "target_lang_translations": {
                                "ru": "Гетман сумел остановить <w>марш</w> вражеских войск уже на границе."
                            }
                        },
                        {
                            "text": "Po całonocnym <w>marszu</w> żołnierze byli skrajnie wyczerpani.",
                            "target_lang_translations": {
                                "ru": "После ночного <w>марша</w> солдаты были крайне истощены."
                            }
                        }
                    ],
                    "synonyms": [
                        "pochód"
                    ],
                    "common_phrases": [
                        "wymarsz wojsk",
                        "wyruszyć w marsz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованное передвижение воинских подразделений из одного места в другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "передвижение войск"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f963c1e5-1c36-31a4-b184-69b5c03a12d5",
                    "sense_definition": "Nizina nadmorska lub rzeczna o żyznej glebie, często osuszona i chroniona groblami (termin geograficzny).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krajobraz ten charakteryzuje się rozległym <w>marszem</w> i licznymi kanałami.",
                            "target_lang_translations": {
                                "ru": "Этот ландшафт характеризуется обширным <w>маршем</w> и многочисленными каналами."
                            }
                        },
                        {
                            "text": "Rolnicy uprawiający ten <w>marsz</w> muszą dbać o system odwadniający.",
                            "target_lang_translations": {
                                "ru": "Фермеры, возделывающие этот <w>марш</w>, должны следить за дренажной системой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "geografia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приморская или речная низменность с плодородной почвой, часто осушенная и защищенная дамбами (географический термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "тип ландшафта, низменность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d56185bb-6956-32c8-a073-ee9e004d10b5",
                    "sense_definition": "Ostry, bezpośredni nakaz natychmiastowego pójścia w określone miejsce lub wykonania czynności.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Marsz</w> do pokoju, masz szlaban na komputer!",
                            "target_lang_translations": {
                                "ru": "<w>Марш</w> в комнату, ты наказан и остаешься без компьютера!"
                            }
                        },
                        {
                            "text": "Już późno, <w>marsz</w> do łóżka!",
                            "target_lang_translations": {
                                "ru": "Уже поздно, <w>марш</w> в кровать!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "marsz do łóżka",
                        "marsz do domu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane przy wydawaniu poleceń, często do dzieci"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Резкий, прямой приказ немедленно идти в определенное место или выполнить действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "повелительное междометие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02135ebb-1fa1-3404-bc07-e9ca8ead72e4",
                    "sense_definition": "Komenda wojskowa lub gimnastyczna nakazująca rozpoczęcie marszu.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pluton, baczność, na wprost <w>marsz</w>!",
                            "target_lang_translations": {
                                "ru": "Взвод, смирно, шагом <w>марш</w>!"
                            }
                        },
                        {
                            "text": "Cała grupa, biegiem <w>marsz</w>!",
                            "target_lang_translations": {
                                "ru": "Группа, бегом <w>марш</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "naprzód marsz",
                        "biegiem marsz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Военная или гимнастическая команда для начала движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марш",
                                "target_lang_sense_clarification": "команда"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
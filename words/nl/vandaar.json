            "senses": [
                {
                    "sense_id": "b8b64266-dd46-31b4-b4a7-838be945a32b",
                    "sense_definition": "Om die reden; daarom.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik had geen tijd, <w>vandaar</w> dat ik niet kon komen.",
                            "target_lang_translations": {
                                "en": "I didn't have time, <w>that's why</w> I couldn't come.",
                                "ru": "У меня не было времени, <w>вот почему</w> я не смог прийти."
                            }
                        },
                        {
                            "text": "De auto was kapot, <w>vandaar</w> moesten we met de trein.",
                            "target_lang_translations": {
                                "en": "The car was broken, <w>therefore</w> we had to take the train.",
                                "ru": "Машина сломалась, <w>поэтому</w> нам пришлось ехать на поезде."
                            }
                        }
                    ],
                    "synonyms": [
                        "daarom",
                        "dus"
                    ],
                    "common_phrases": [
                        "vandaar dat",
                        "vandaar de vraag",
                        "vandaar ook"
                    ],
                    "target_lang_definitions": {
                        "en": "For that reason; therefore.",
                        "ru": "По этой причине; поэтому."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "that's why",
                                "target_lang_sense_clarification": "indicating the reason for something"
                            },
                            {
                                "target_lang_word": "therefore",
                                "target_lang_sense_clarification": "formal logical consequence"
                            },
                            {
                                "target_lang_word": "hence",
                                "target_lang_sense_clarification": "formal indication of result"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поэтому",
                                "target_lang_sense_clarification": "следствие"
                            },
                            {
                                "target_lang_word": "отсюда",
                                "target_lang_sense_clarification": "логический вывод"
                            },
                            {
                                "target_lang_word": "вот почему",
                                "target_lang_sense_clarification": "объяснение причины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09d60a69-75ab-38a8-bb42-7cf5815133be",
                    "sense_definition": "Vanuit die plaats; vanaf die plek.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We reizen naar Parijs en gaan <w>vandaar</w> verder naar het zuiden.",
                            "target_lang_translations": {
                                "en": "We are traveling to Paris and will continue south <w>from there</w>.",
                                "ru": "Мы едем в Париж и <w>оттуда</w> отправимся дальше на юг."
                            }
                        },
                        {
                            "text": "Hij liep naar de top en had <w>vandaar</w> een prachtig uitzicht.",
                            "target_lang_translations": {
                                "en": "He walked to the summit and had a beautiful view <w>from there</w>.",
                                "ru": "Он поднялся на вершину, и <w>оттуда</w> открывался прекрасный вид."
                            }
                        }
                    ],
                    "synonyms": [
                        "daarvandaan"
                    ],
                    "common_phrases": [
                        "vandaar verder"
                    ],
                    "target_lang_definitions": {

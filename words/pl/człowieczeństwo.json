                {
                    "sense_id": "eb278e26-6889-3bf5-bb12-c04905177c00",
                    "sense_definition": "Natura ludzka, ogół cech właściwych człowiekowi; fakt bycia człowiekiem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W umiłowaniu słabszych przejawiało się jego prawdziwe <w>człowieczeństwo</w>.",
                            "target_lang_translations": {
                                "ru": "В любви к слабым проявлялась его истинная <w>человечность</w>."
                            }
                        },
                        {
                            "text": "Wojna często sprawia, że ludzie tracą swoje <w>człowieczeństwo</w> i stają się okrutni.",
                            "target_lang_translations": {
                                "ru": "Война часто приводит к тому, что люди теряют свою <w>человечность</w> и становятся жестокими."
                            }
                        }
                    ],
                    "synonyms": [
                        "człowieczość",
                        "humanitaryzm",
                        "humanitarność"
                    ],
                    "common_phrases": [

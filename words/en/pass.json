                    "common_phrases": [
                        "pass for",
                        "pass as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть принятым за кого-то или что-то, чем не являешься."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сойти за",
                                "target_lang_sense_clarification": "кого-либо"
                            }
                        ]
                    }
                }
            ]
        },

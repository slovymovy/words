            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ee19ea57-da84-3d2c-a32d-cde431c8feba",
                    "sense_definition": "Shared by a group of people or a community for common use.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "shared",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The students share a large <w>communal</w> kitchen on the first floor of the dormitory.",
                            "target_lang_translations": {
                                "ru": "Студенты пользуются большой <w>общей</w> кухней на первом этаже общежития."
                            }
                        },
                        {
                            "text": "Residents are responsible for keeping the <w>communal</w> garden clean and tidy.",
                            "target_lang_translations": {
                                "ru": "Жильцы несут ответственность за поддержание чистоты и порядка в <w>общем</w> саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "shared",
                        "collective",
                        "public",
                        "joint"
                    ],
                    "common_phrases": [
                        "communal living",
                        "communal garden",
                        "communal kitchen",
                        "communal area"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используемый совместно группой людей или сообществом для общих целей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о́бщий",
                                "target_lang_sense_clarification": "принадлежащий всем, совместный"
                            },
                            {
                                "target_lang_word": "обще́ственный",
                                "target_lang_sense_clarification": "предназначенный для общественного пользования"
                            },
                            {
                                "target_lang_word": "коммуна́льный",
                                "target_lang_sense_clarification": "относящийся к общему хозяйству или жилью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b97af45-5cce-30ff-898b-84d727469c5a",
                    "sense_definition": "Relating to a community or the people living in it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relating_to_community",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The village has a strong sense of <w>communal</w> identity and tradition.",
                            "target_lang_translations": {
                                "ru": "В деревне сильно развито чувство <w>общинной</w> идентичности и традиций."
                            }
                        },
                        {
                            "text": "They organized a <w>communal</w> effort to repair the damage after the storm.",
                            "target_lang_translations": {
                                "ru": "Они организовали <w>коллективную</w> работу по устранению ущерба после шторма."
                            }

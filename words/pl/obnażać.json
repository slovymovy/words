{
    "word_family": [
        "nagi",
        "nagość",
        "obnażenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d79b81eb-50c1-3089-8bd4-a758c4a326fe",
                    "sense_definition": "Ujawniać coś, co było ukryte, np. prawdę, błędy, słabości lub czyjeś prawdziwe intencje.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_reveal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziennikarskie śledztwo <obnażyło> korupcję na najwyższych szczeblach władzy.",
                            "target_lang_translations": {
                                "ru": "Журналистское расследование <w>обнажило</w> коррупцию в высших эшелонах власти."
                            }
                        },
                        {
                            "text": "Kryzys gospodarczy bezlitośnie <obnaża> wszystkie słabości systemu bankowego.",
                            "target_lang_translations": {
                                "ru": "Экономический кризис безжалостно <w>обнажает</w> все слабости банковской системы."
                            }
                        }
                    ],
                    "synonyms": [
                        "ujawniać",
                        "odsłaniać",
                        "rozbierać"
                    ],
                    "antonyms": [
                        "okrywać",
                        "ubierać"
                    ],
                    "common_phrases": [
                        "obnażać prawdę",
                        "obnażać słabości",
                        "obnażać kłamstwa",
                        "obnażać hipokryzję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раскрывать что-то, что было скрыто, например, правду, ошибки, слабости или чьи-то истинные намерения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнажать",
                                "target_lang_sense_clarification": "раскрывать суть, правду"
                            },
                            {
                                "target_lang_word": "разоблачать",
                                "target_lang_sense_clarification": "выводить на чистую воду"
                            },
                            {
                                "target_lang_word": "вскрывать",
                                "target_lang_sense_clarification": "обнаруживать недостатки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee0ceef9-1cf8-3bcc-a2fa-6beebc421ad8",
                    "sense_definition": "Zdejmować ubranie z kogoś lub odsłaniać część ciała, czyniąc ją nagą.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_undress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz poprosił pacjenta, aby ten <obnażył> klatkę piersiową do badania.",
                            "target_lang_translations": {
                                "ru": "Врач попросил пациента <w>обнажить</w> грудную клетку для осмотра."
                            }
                        },
                        {
                            "text": "Silny podmuch wiatru zerwał jej szal i <obnażył> szyję.",
                            "target_lang_translations": {
                                "ru": "Сильный порыв ветра сорвал её шаль и <w>обнажил</w> шею."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozbierać",
                        "odsłaniać",
                        "rozdziewać"
                    ],
                    "antonyms": [
                        "ubierać",
                        "okrywać",
                        "odziewać"
                    ],
                    "common_phrases": [
                        "obnażać ramiona",
                        "obnażać tors",
                        "obnażać ciało",
                        "obnażać głowę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снимать одежду с кого-либо или открывать часть тела, делая её нагой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнажать",
                                "target_lang_sense_clarification": "открывать часть тела"
                            },
                            {
                                "target_lang_word": "оголять",
                                "target_lang_sense_clarification": "делать голым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9c5da72-4fd5-3d9a-b111-c5d4569e9ae2",
                    "sense_definition": "Pokazywać zęby w grymasie (o zwierzętach lub ludziach) lub wyciągać broń z pochwy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "specific_expose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wściekły pies warczał głośno i <obnażał> ostre kły.",
                            "target_lang_translations": {
                                "ru": "Бешеная собака громко рычала и <w>обнажала</w> острые клыки."
                            }
                        },
                        {
                            "text": "Rycerz bez wahania <obnażył> miecz, gotując się do walki.",
                            "target_lang_translations": {
                                "ru": "Рыцарь без колебаний <w>обнажил</w> меч, готовясь к бою."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsłaniać"
                    ],
                    "antonyms": [
                        "okrywać"
                    ],
                    "common_phrases": [
                        "obnażać zęby",
                        "obnażać kły",
                        "obnażać miecz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывать зубы в гримасе (о животных или людях) или вынимать оружие из ножен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнажать",
                                "target_lang_sense_clarification": "оружие или зубы"
                            },
                            {
                                "target_lang_word": "скалить",
                                "target_lang_sense_clarification": "зубы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dee5159-adc6-333a-9334-346dbb3b16c9",
                    "sense_definition": "Zdejmować z siebie ubranie lub pokazywać swoje ciało nago (często w kontekście publicznym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reflexive_undress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna został aresztowany za to, że <obnażał> się w parku.",
                            "target_lang_translations": {
                                "ru": "Мужчину арестовали за то, что он <w>обнажался</w> в парке."
                            }
                        },
                        {
                            "text": "Modelka bez skrępowania <obnażała> się przed obiektywem aparatu.",
                            "target_lang_translations": {
                                "ru": "Модель без стеснения <w>обнажалась</w> перед объективом камеры."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozbierać się",
                        "rozdziewać się"
                    ],
                    "antonyms": [
                        "ubierać się"
                    ],
                    "common_phrases": [
                        "obnażać się publicznie",
                        "obnażać się przed kimś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снимать с себя одежду или показывать своё тело нагим (часто в публичном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнажаться",
                                "target_lang_sense_clarification": "снимать с себя одежду"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
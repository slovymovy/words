    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "82c698ed-41c2-3de9-9712-a6ae6433d55c",
                    "sense_definition": "Раскидать, разбросать в разные стороны силой (ветром, взрывом и т. п.).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scatter_destroy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный ветер <разметал> все бумаги по комнате.",
                            "target_lang_translations": {
                                "en": "The strong wind <w>scattered</w> all the papers around the room."
                            }
                        },
                        {
                            "text": "Взрыв <разметал> осколки стекла на десятки метров.",
                            "target_lang_translations": {
                                "en": "The explosion <w>scattered</w> glass shards for tens of meters."
                            }
                        }
                    ],
                    "synonyms": [
                        "раскидать",
                        "рассеять",
                        "развевать"
                    ],
                    "antonyms": [
                        "собрать"
                    ],
                    "common_phrases": [
                        "ветер разметал листья",
                        "буря разметала стога",
                        "разметал облака"
                    ],
                    "target_lang_definitions": {
                        "en": "To scatter or throw around in different directions by force (wind, explosion, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scatter",
                                "target_lang_sense_clarification": "general sense of throwing in various directions"
                            },
                            {
                                "target_lang_word": "blow about",
                                "target_lang_sense_clarification": "specifically by wind"
                            },
                            {
                                "target_lang_word": "disperse",
                                "target_lang_sense_clarification": "to cause to separate and go in different directions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2faf0bb0-5756-329d-8923-5798c5c1b1eb",
                    "sense_definition": "Широко и небрежно раскинуть в стороны (руки, ноги, волосы).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "body_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок крепко спал, <разметав> руки и ноги по кровати.",
                            "target_lang_translations": {
                                "en": "The child slept soundly, having <w>flung out</w> his arms and legs across the bed."
                            }
                        },
                        {
                            "text": "Она лежала на подушке и <разметала> свои длинные волосы.",
                            "target_lang_translations": {
                                "en": "She lay on the pillow and <w>spread out</w> her long hair."
                            }
                        }
                    ],
                    "synonyms": [
                        "раскинуть"
                    ],
                    "common_phrases": [
                        "разметать волосы",
                        "разметать руки",
                        "разметать во сне"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To spread out arms, legs, or hair widely and carelessly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fling out",
                                "target_lang_sense_clarification": "sudden or careless movement of limbs"
                            },
                            {
                                "target_lang_word": "spread out",
                                "target_lang_sense_clarification": "to extend over a surface"
                            },
                            {
                                "target_lang_word": "toss",
                                "target_lang_sense_clarification": "regarding hair"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19d2c9f1-5c02-3f1e-bb02-1a289ac2460e",
                    "sense_definition": "Полностью разрушить, развалить строение или конструкцию (часто о стихийных бедствиях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "scatter_destroy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ураган полностью <разметал> старый деревянный сарай.",
                            "target_lang_translations": {
                                "en": "The hurricane completely <w>tore apart</w> the old wooden shed."
                            }
                        },
                        {
                            "text": "Нам пришлось <разметать> забор, чтобы проехала техника.",
                            "target_lang_translations": {
                                "en": "We had to <w>dismantle</w> the fence so the machinery could pass."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрушить",
                        "развалить"
                    ],
                    "common_phrases": [
                        "разметать крышу",
                        "разметать хижину",
                        "шторм разметал"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely destroy or pull apart a structure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tear apart",
                                "target_lang_sense_clarification": "violent destruction"
                            },
                            {
                                "target_lang_word": "demolish",
                                "target_lang_sense_clarification": "to pull or knock down a building"
                            },
                            {
                                "target_lang_word": "dismantle",
                                "target_lang_sense_clarification": "to take apart piece by piece"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13753a0d-e080-39f7-b1cd-d7c84cce0adb",
                    "sense_definition": "Очищать пространство, сметая что-либо в стороны (обычно метлой или лопатой).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дворник вышел рано утром <разметать> снег у подъезда.",
                            "target_lang_translations": {
                                "en": "The street cleaner went out early in the morning to <w>sweep away</w> the snow by the entrance."
                            }
                        },
                        {
                            "text": "Нужно <разметать> кучу листьев, чтобы освободить проход.",
                            "target_lang_translations": {
                                "en": "We need to <w>sweep aside</w> the pile of leaves to clear the passage."
                            }
                        }
                    ],
                    "synonyms": [
                        "мести"
                    ],
                    "antonyms": [
                        "заметать"
                    ],
                    "common_phrases": [
                        "разметать снег",
                        "разметать дорожку"
                    ],
                    "target_lang_definitions": {
                        "en": "To clear a space by sweeping material to the sides."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sweep away",
                                "target_lang_sense_clarification": "to remove by sweeping"
                            },
                            {
                                "target_lang_word": "sweep aside",
                                "target_lang_sense_clarification": "to push to the side with a broom"
                            }
                        ]
                    }
                }
            ]
        }
    ]

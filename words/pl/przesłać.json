    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "08572d90-17af-39e1-aacc-77dbd6915bad",
                    "sense_definition": "Wysłać coś (np. list, paczkę, pieniądze) do kogoś za pośrednictwem poczty, kuriera lub internetu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę jutro <przesłać> te ważne dokumenty do biura w Warszawie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно завтра <отправить> эти важные документы в офис в Варшаве."
                            }
                        },
                        {
                            "text": "Czy możesz mi <przesłać> zdjęcia z wakacji na mój adres e-mail?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <переслать> мне фотографии с отпуска на мой электронный адрес?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wysłać"
                    ],
                    "antonyms": [
                        "odebrać"
                    ],
                    "common_phrases": [
                        "przesłać list",
                        "przesłać pieniądze",
                        "przesłać paczkę",
                        "przesłać pocztą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправить что-либо (например, письмо, посылку, деньги) кому-либо по почте, курьером или через интернет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправить",
                                "target_lang_sense_clarification": "послать письмо, посылку или деньги"
                            },
                            {
                                "target_lang_word": "переслать",
                                "target_lang_sense_clarification": "отправить через средство связи"
                            },
                            {
                                "target_lang_word": "выслать",
                                "target_lang_sense_clarification": "отправить из какого-либо места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5fafd84-5d55-3daf-9c37-1d3a0b069758",
                    "sense_definition": "Przekazać otrzymaną wcześniej wiadomość lub przesyłkę dalej do innej osoby.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <przesłać> tę informację dalej do wszystkich pracowników działu.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <перешлите> эту информацию дальше всем сотрудникам отдела."
                            }
                        },
                        {
                            "text": "Kierownik <przesłał> mi twoje zapytanie, abym się nim zajął.",
                            "target_lang_translations": {
                                "ru": "Руководитель <переслал> мне твой запрос, чтобы я им занялся."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przesłać dalej",
                        "przesłać wiadomość",
                        "przesłać maila"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать полученное ранее сообщение или посылку другому лицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переслать",
                                "target_lang_sense_clarification": "отправить полученное дальше (forward)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c05a3542-abf9-38b4-829a-753af0e84256",
                    "sense_definition": "Umieścić plik lub dane cyfrowe na serwerze lub w systemie informatycznym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Udało mi się w końcu <przesłać> duży plik wideo na serwer.",
                            "target_lang_translations": {
                                "ru": "Мне наконец удалось <загрузить> большой видеофайл на сервер."
                            }
                        },
                        {
                            "text": "Aby zakończyć rejestrację, musisz <przesłać> skan swojego dowodu osobistego.",
                            "target_lang_translations": {
                                "ru": "Чтобы завершить регистрацию, вы должны <отправить> скан своего удостоверения личности."
                            }
                        }
                    ],
                    "synonyms": [
                        "wgrać",
                        "załadować"
                    ],
                    "antonyms": [
                        "pobrać",
                        "ściągnąć"
                    ],
                    "common_phrases": [
                        "przesłać plik",
                        "przesłać dane",
                        "przesłać na serwer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Загрузить или передать файл или цифровые данные на сервер или в информационную систему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправить",
                                "target_lang_sense_clarification": "передать данные"
                            },
                            {
                                "target_lang_word": "загрузить",
                                "target_lang_sense_clarification": "поместить на сервер (upload)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8db877a-7948-3bb6-b292-2f66cb70db14",
                    "sense_definition": "Skierować ku komuś gest życzliwości lub uczucia, taki jak uśmiech lub pocałunek, zazwyczaj na odległość.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gestures",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stojąc w drzwiach pociągu, <przesłała> mu ostatni całus na pożegnanie.",
                            "target_lang_translations": {
                                "ru": "Стоя в дверях поезда, она <послала> ему последний воздушный поцелуй на прощание."
                            }
                        },
                        {
                            "text": "Chciałbym <przesłać> serdeczne pozdrowienia dla całej twojej rodziny.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <передать> сердечный привет всей твоей семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "załadować"
                    ],
                    "common_phrases": [
                        "przesłać całusa",
                        "przesłać uśmiech",
                        "przesłać pozdrowienia"

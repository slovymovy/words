                {
                    "sense_id": "03b8130b-d79e-37d0-8024-2af61ea82ef7",
                    "sense_definition": "Właściciel, dyrektor banku lub osoba zawodowo zajmująca się operacjami finansowymi i bankowością.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "finance_pro",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany <w>bankier</w> przewidział nadchodzący kryzys na giełdzie.",
                            "target_lang_translations": {
                                "ru": "Известный <w>банкир</w> предсказал грядущий кризис на бирже."
                            }
                        },
                        {
                            "text": "Mój wujek pracuje jako <w>bankier</w> inwestycyjny w dużej korporacji.",
                            "target_lang_translations": {
                                "ru": "Мой дядя работает инвестиционным <w>банкиром</w> в крупной корпорации."
                            }
                        }
                    ],
                    "synonyms": [
                        "finansista"
                    ],
                    "common_phrases": [
                        "prywatny bankier",
                        "doświadczony bankier",
                        "szwajcarski bankier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Владелец, управляющий банка или специалист, профессионально занимающийся банковскими и финансовыми операциями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банкир",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c63edd03-b785-33e7-b68b-552b5aabcf59",
                    "sense_definition": "Osoba zarządzająca pulą pieniędzy (bankiem) w grach hazardowych lub kasynie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gambling_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej grze karcianej <w>bankier</w> rozdaje karty i przyjmuje zakłady od graczy.",
                            "target_lang_translations": {
                                "ru": "В этой карточной игре <w>банкир</w> раздает карты и принимает ставки от игроков."
                            }
                        },
                        {
                            "text": "Każdy z uczestników po kolei zostawał <w>bankierem</w> na jedną rundę.",
                            "target_lang_translations": {
                                "ru": "Каждый из участников по очереди становился <w>банкиром</w> на один раунд."
                            }
                        }
                    ],
                    "synonyms": [
                        "krupier"
                    ],
                    "common_phrases": [
                        "grać z bankierem",
                        "rozbić bankiera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, управляющий игровым банком (кассой) в азартных играх или казино."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банкир",
                                "target_lang_sense_clarification": "в играх"
                            },
                            {
                                "target_lang_word": "банкомёт",
                                "target_lang_sense_clarification": "в карточных играх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "276dfdbd-d503-368f-ab6e-1370a8963a48",

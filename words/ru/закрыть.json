    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f021a906-6144-3b4f-bc22-99f8b1951117",
                    "sense_definition": "Затворить дверь, крышку или другой объект, чтобы преградить доступ внутрь или наружу.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, плотно <w>закройте</w> за собой входную дверь.",
                            "target_lang_translations": {
                                "en": "Please <w>close</w> the front door tightly behind you."
                            }
                        },
                        {
                            "text": "Я забыл <w>закрыть</w> окно, и в комнате стало очень холодно.",
                            "target_lang_translations": {
                                "en": "I forgot to <w>close</w> the window, and it got very cold in the room."
                            }
                        }
                    ],
                    "antonyms": [
                        "откры́ть"
                    ],
                    "common_phrases": [
                        "закрыть дверь",
                        "закрыть окно",
                        "закрыть на замок"
                    ],
                    "target_lang_definitions": {
                        "en": "To move a door, lid, or other object so that it covers an opening or blocks access."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "close",
                                "target_lang_sense_clarification": "the most general and neutral term for blocking an opening"
                            },
                            {
                                "target_lang_word": "shut",
                                "target_lang_sense_clarification": "often interchangeable with 'close', but can imply more force or finality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9da567f5-d423-3b51-ba68-a73e072cba7d",
                    "sense_definition": "Сблизить края чего-либо или сложить что-либо (глаза, книгу, зонт).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Девушка устало <w>закрыла</w> глаза и сразу уснула.",
                            "target_lang_translations": {
                                "en": "The girl tiredly <w>closed</w> her eyes and fell asleep immediately."
                            }
                        },
                        {
                            "text": "Ученик <w>закрыл</w> книгу и убрал её в рюкзак.",
                            "target_lang_translations": {
                                "en": "The student <w>closed</w> the book and put it away in his backpack."
                            }
                        }
                    ],
                    "antonyms": [
                        "откры́ть"
                    ],
                    "common_phrases": [
                        "закрыть глаза",
                        "закрыть рот",
                        "закрыть книгу"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring the parts of something together so that it is no longer open (e.g., eyes, book)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "close",
                                "target_lang_sense_clarification": "used for eyes, books, umbrellas, etc."
                            },
                            {
                                "target_lang_word": "shut",
                                "target_lang_sense_clarification": "often used for eyes or mouth"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b394a0ae-5b1f-3a8a-8422-d827798a0259",
                    "sense_definition": "Прекратить работу учреждения, предприятия или завершить какое-либо дело.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за финансового кризиса владельцам пришлось <w>закрыть</w> магазин.",
                            "target_lang_translations": {
                                "en": "Due to the financial crisis, the owners had to <w>close</w> the store."
                            }
                        },
                        {
                            "text": "Мы планируем успешно <w>закрыть</w> этот проект до конца месяца.",
                            "target_lang_translations": {
                                "en": "We plan to successfully <w>close</w> this project by the end of the month."
                            }
                        }
                    ],
                    "antonyms": [
                        "откры́ть"
                    ],
                    "common_phrases": [
                        "закрыть счёт",
                        "закрыть тему",
                        "закрыть бизнес"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop the operations of a business permanently or temporarily, or to bring a task to an end."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "close",
                                "target_lang_sense_clarification": "to cease business operations or finish a deal/project"
                            },
                            {
                                "target_lang_word": "shut down",
                                "target_lang_sense_clarification": "to stop a machine, business, or operation completely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d76a7aac-1f2e-3e02-8737-f83b5c8a321d",
                    "sense_definition": "Заслонить, сделать невидимым или защитить, поместив что-то сверху или спереди.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тёмные грозовые тучи скоро <w>закроют</w> солнце.",
                            "target_lang_translations": {
                                "en": "Dark storm clouds will soon <w>cover</w> the sun."
                            }
                        },
                        {
                            "text": "Ребёнок испугался и <w>закрыл</w> лицо руками.",
                            "target_lang_translations": {
                                "en": "The child got scared and <w>covered</w> his face with his hands."
                            }
                        }
                    ],
                    "antonyms": [
                        "откры́ть"
                    ],
                    "common_phrases": [
                        "закрыть собой",
                        "закрыть лицо руками"
                    ],
                    "target_lang_definitions": {
                        "en": "To place something in front of or over something else to hide, protect, or obscure it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cover",
                                "target_lang_sense_clarification": "to place something over an object"
                            },
                            {
                                "target_lang_word": "shield",
                                "target_lang_sense_clarification": "to protect or screen from view"
                            },
                            {
                                "target_lang_word": "block",
                                "target_lang_sense_clarification": "to obstruct a view or light"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10f4708c-4d22-3068-b556-99e6770513ab",
                    "sense_definition": "Законсервировать овощи или фрукты в герметичные банки для хранения.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка каждое лето любит <w>закрыть</w> огурцы на зиму.",
                            "target_lang_translations": {
                                "en": "Every summer, Grandma likes to <w>can</w> cucumbers for the winter."
                            }
                        },
                        {
                            "text": "Нам осталось <w>закрыть</w> ещё пять банок вишнёвого варенья.",
                            "target_lang_translations": {
                                "en": "We still have five jars of cherry jam left to <w>seal</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "откры́ть"
                    ],
                    "common_phrases": [
                        "закрыть банки",
                        "закрыть компот"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in domestic context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To preserve food in sealed jars for storage."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "can",
                                "target_lang_sense_clarification": "to preserve food in jars or cans (US usage)"
                            },
                            {
                                "target_lang_word": "preserve",
                                "target_lang_sense_clarification": "general term for preparing food for long-term storage"
                            },
                            {
                                "target_lang_word": "seal",
                                "target_lang_sense_clarification": "to close a jar tightly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5380dce4-9048-324f-b205-6057cd7e9fac",
                    "sense_definition": "Лишить свободы, поместить под арест или в тюрьму.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция может <w>закрыть</w> его за хулиганство на пятнадцать суток.",
                            "target_lang_translations": {
                                "en": "The police can <w>lock</w> him <w>up</w> for fifteen days for hooliganism."
                            }
                        },
                        {
                            "text": "Опасного преступника <w>закрыли</w> в камере предварительного заключения.",
                            "target_lang_translations": {
                                "en": "The dangerous criminal was <w>locked up</w> in a pre-trial detention cell."
                            }
                        }
                    ],
                    "synonyms": [
                        "посадить",
                        "упечь"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "criminal or police slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To put someone in prison or detention (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lock up",
                                "target_lang_sense_clarification": "common colloquial term for imprisoning someone"
                            },
                            {
                                "target_lang_word": "jail",
                                "target_lang_sense_clarification": "to put in jail"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
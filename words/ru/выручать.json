            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ad70ebab-2a1a-364d-943b-0b416eaefb30",
                    "sense_definition": "Помогать кому-либо выйти из трудного или опасного положения, спасать от неприятностей.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "help_rescue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Настоящий друг всегда готов <w>выручать</w> в трудную минуту.",
                            "target_lang_translations": {
                                "en": "A real friend is always ready to <w>help out</w> in a difficult moment."
                            }
                        },
                        {
                            "text": "Запас высоты нередко <w>выручал</w> опытного лётчика в полёте.",
                            "target_lang_translations": {
                                "en": "A reserve of altitude often <w>saved</w> the experienced pilot in flight."
                            }
                        },
                        {
                            "text": "Соседи привыкли <w>выручать</w> друг друга деньгами до зарплаты.",
                            "target_lang_translations": {
                                "en": "Neighbors were used to <w>helping out</w> each other with money until payday."
                            }
                        }
                    ],
                    "synonyms": [
                        "спасать",
                        "помогать"
                    ],
                    "antonyms": [
                        "подводить"
                    ],
                    "common_phrases": [
                        "выручать из беды",
                        "выручать друга",
                        "всегда выручать"
                    ],
                    "target_lang_definitions": {
                        "en": "To help someone out of a difficult or dangerous situation; to save from trouble."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "help out",
                                "target_lang_sense_clarification": "assist in a time of need"
                            },
                            {
                                "target_lang_word": "rescue",
                                "target_lang_sense_clarification": "save from danger or difficulty"
                            },
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "prevent from harm or failure"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4907eb7-2c6f-348f-ac62-5e3291239865",
                    "sense_definition": "Получать определённую сумму денег от продажи чего-либо.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Фермеры надеялись <w>выручать</w> хорошие деньги за свой урожай.",
                            "target_lang_translations": {
                                "en": "Farmers hoped to <w>make</w> good money for their harvest."
                            }
                        },
                        {
                            "text": "Магазин стал <w>выручать</w> меньше средств после открытия конкурента.",
                            "target_lang_translations": {
                                "en": "The store began to <w>make</w> less money after a competitor opened."
                            }
                        }
                    ],
                    "synonyms": [
                        "зарабатывать",
                        "получать"
                    ],
                    "antonyms": [
                        "тратить"
                    ],
                    "common_phrases": [
                        "выручать деньги",
                        "выручать прибыль",
                        "выручать от продажи"
                    ],
                    "target_lang_definitions": {
                        "en": "To receive a certain amount of money from selling something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "earn money from sales"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "receive money"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0a861f7-6fac-38ea-be2f-268f28ed08f2",
                    "sense_definition": "Возвращать себе что-либо утраченное, захваченное или оставленное в залог.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "retrieve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я поехал на штрафстоянку <w>выручать</w> свою машину.",
                            "target_lang_translations": {
                                "en": "I went to the impound lot to <w>retrieve</w> my car."
                            }
                        },
                        {
                            "text": "Ему пришлось <w>выручать</w> заложенные в ломбард вещи.",
                            "target_lang_translations": {
                                "en": "He had to <w>redeem</w> the items pledged to the pawnshop."
                            }
                        }
                    ],
                    "synonyms": [
                        "вызволять",
                        "возвращать"
                    ],
                    "antonyms": [

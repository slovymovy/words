    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "073ad801-621b-33f6-939b-b3f51ad8b40a",
                    "sense_definition": "Одна из двух равных частей, которые вместе составляют целое.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "part_of_whole",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я уже прочитал <половину> этой интересной книги.",
                            "target_lang_translations": {
                                "en": "I have already read <half> of this interesting book."
                            }
                        },
                        {
                            "text": "На столе осталась только <половина> красного яблока.",
                            "target_lang_translations": {
                                "en": "Only <half> of the red apple remained on the table."
                            }
                        },
                        {
                            "text": "Сейчас <половина> второго, и нам пора обедать.",
                            "target_lang_translations": {
                                "en": "It is <half> past one, and it is time for us to have lunch."
                            }
                        }
                    ],
                    "synonyms": [
                        "одна вторая"
                    ],
                    "antonyms": [
                        "целое"
                    ],
                    "common_phrases": [
                        "первая половина",
                        "вторая половина",
                        "половина дня",
                        "половина пути",
                        "половина часа"
                    ],
                    "target_lang_definitions": {
                        "en": "One of two equal parts that together make up a whole."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "half",
                                "target_lang_sense_clarification": "50% of something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2aa55f84-2965-3f01-815a-5a42f05493af",
                    "sense_definition": "Середина какого-либо промежутка времени или пространства.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "middle_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы планируем поехать в отпуск в <половине> августа.",
                            "target_lang_translations": {
                                "en": "We plan to go on vacation in the <middle> of August."
                            }
                        },
                        {
                            "text": "Солнце стояло уже на <половине> неба, и становилось жарко.",
                            "target_lang_translations": {
                                "en": "The sun was already in the <middle> of the sky, and it was getting hot."
                            }
                        }
                    ],
                    "synonyms": [
                        "середина"
                    ],
                    "common_phrases": [
                        "в половине лета",
                        "на половине дороги"
                    ],
                    "target_lang_definitions": {
                        "en": "The middle of a period of time or space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "middle",
                                "target_lang_sense_clarification": "center point of a period or area"
                            },
                            {
                                "target_lang_word": "mid-point",
                                "target_lang_sense_clarification": "exact middle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e37ecc33-ac74-3bdc-912b-57d3be17af7e",
                    "sense_definition": "Муж или жена (обычно в шутливом или ироничном контексте).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "spouse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя вторая <половина> очень любит готовить итальянские блюда.",
                            "target_lang_translations": {
                                "en": "My <better half> loves cooking Italian dishes very much."
                            }
                        },
                        {
                            "text": "На корпоратив он пришёл один, так как его <половина> заболела.",
                            "target_lang_translations": {
                                "en": "He came to the office party alone, as his <other half> had fallen ill."
                            }
                        }
                    ],
                    "synonyms": [
                        "супруг",
                        "супруга",
                        "муж",
                        "жена"
                    ],
                    "common_phrases": [
                        "вторая половина",
                        "моя половина"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "humorous"
                        }
                    ],
                    "target_lang_definitions": {

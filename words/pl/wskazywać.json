                },
                {
                    "sense_id": "1e0e5269-ee13-3608-9d4a-c78e989d180f",
                    "sense_definition": "Być znakiem czegoś, świadczyć o czymś lub pozwalać wyciągnąć określony wniosek",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

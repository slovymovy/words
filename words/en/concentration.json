{
    "word_family": [
        "concentrate",
        "concentrator"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "43e8967c-564b-30b8-9209-6add6b363375",
                    "sense_definition": "The ability to direct all your attention or thought to a single object or activity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_focus",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loud noise outside made me lose my <w>concentration</w> completely.",
                            "target_lang_translations": {
                                "ru": "Громкий шум на улице заставил меня полностью потерять <w>концентрацию</w>."
                            }
                        },
                        {
                            "text": "Chess is a game that requires a very high level of <w>concentration</w>.",
                            "target_lang_translations": {
                                "ru": "Шахматы — это игра, требующая очень высокого уровня <w>концентрации</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "focus",
                        "attention"
                    ],
                    "antonyms": [
                        "distraction",
                        "inattention"
                    ],
                    "common_phrases": [
                        "lose concentration",
                        "deep concentration",
                        "lack of concentration",
                        "power of concentration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность направить всё внимание или мысли на один объект или деятельность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "концентра́ция",
                                "target_lang_sense_clarification": "состояние внимания"
                            },
                            {
                                "target_lang_word": "сосредото́ченность",
                                "target_lang_sense_clarification": "сфокусированность мыслей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "066f6b51-4287-38c4-a466-7f66ff456df7",
                    "sense_definition": "The relative amount of a particular substance contained within a solution, mixture, or volume.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "amount_density",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>concentration</w> of salt in the Dead Sea is extremely high.",
                            "target_lang_translations": {
                                "ru": "<w>Концентрация</w> соли в Мёртвом море чрезвычайно высока."
                            }
                        },
                        {
                            "text": "Scientists measured the <w>concentration</w> of carbon dioxide in the atmosphere.",
                            "target_lang_translations": {
                                "ru": "Учёные измерили <w>концентрацию</w> углекислого газа в атмосфере."
                            }
                        }
                    ],
                    "synonyms": [
                        "density"
                    ],
                    "common_phrases": [
                        "high concentration",
                        "low concentration",
                        "concentration of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относительное количество определённого вещества, содержащегося в растворе, смеси или объёме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "концентра́ция",
                                "target_lang_sense_clarification": "насыщенность раствора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db42eb7f-6b92-3ac6-b5e6-a36329a2f0ee",
                    "sense_definition": "A large number of people or things grouped together in one area.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a heavy <w>concentration</w> of traffic in the city center during rush hour.",
                            "target_lang_translations": {
                                "ru": "В час пик в центре города наблюдается сильное <w>скопление</w> транспорта."
                            }
                        },
                        {
                            "text": "The greatest <w>concentration</w> of wildlife is found near the river.",
                            "target_lang_translations": {
                                "ru": "Наибольшее <w>скопление</w> диких животных можно найти у реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "gathering",
                        "cluster",
                        "accumulation"
                    ],
                    "antonyms": [
                        "dispersal",
                        "scattering"
                    ],
                    "common_phrases": [
                        "concentration of troops",
                        "concentration of wealth",
                        "concentration of power"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество людей или предметов, собранных вместе в одной области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скопление",
                                "target_lang_sense_clarification": "группа объектов или людей"
                            },
                            {
                                "target_lang_word": "сосредото́чение",
                                "target_lang_sense_clarification": "сбор сил или средств в одном месте"
                            },
                            {
                                "target_lang_word": "концентра́ция",
                                "target_lang_sense_clarification": "плотное размещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88403990-524c-34eb-85a3-23ed65021457",
                    "sense_definition": "A main field of study that a student focuses on at a college or university.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "academic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided that her academic <w>concentration</w> would be in medieval history.",
                            "target_lang_translations": {
                                "ru": "Она решила, что её академической <w>специализацией</w> будет история средневековья."
                            }
                        },
                        {
                            "text": "Students must declare a <w>concentration</w> by the end of their second year.",
                            "target_lang_translations": {
                                "ru": "Студенты должны выбрать <w>специализацию</w> к концу второго курса."
                            }
                        }
                    ],
                    "synonyms": [
                        "major",
                        "specialization"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная область специализации студента в колледже или университете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "специализа́ция",
                                "target_lang_sense_clarification": "учебный профиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa871494-c337-353a-adc6-9f36b92fcbd6",
                    "sense_definition": "A card game in which players turn over two cards at a time trying to find matching pairs.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "game",
                    "name_type": "game_or_sport_name",
                    "examples": [
                        {
                            "text": "We played a game of <w>concentration</w> to test our memory skills.",
                            "target_lang_translations": {
                                "ru": "Мы сыграли в <w>Мемори</w>, чтобы проверить нашу память."
                            }
                        },
                        {
                            "text": "The children sat on the floor playing <w>concentration</w> with a deck of animal cards.",
                            "target_lang_translations": {
                                "ru": "Дети сидели на полу, играя в <w>Мемори</w> с колодой карт с животными."
                            }
                        }
                    ],
                    "synonyms": [
                        "Memory",
                        "Pelmanism"
                    ],
                    "target_lang_definitions": {
                        "ru": "Карточная игра на запоминание, в которой нужно находить парные карты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мемори",
                                "target_lang_sense_clarification": "название игры"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
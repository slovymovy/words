                    "common_phrases": [
                        "niedźwiedź polarny",
                        "niedźwiedź brunatny",
                        "silny jak niedźwiedź",
                        "sen zimowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное хищное млекопитающее с массивным телом и густой шерстью, обитающее в лесах."

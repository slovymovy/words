{
    "word_family": [
        "ripper",
        "ripped"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ea22e931-b106-3d87-87c7-6694cdb7961f",
                    "sense_definition": "To pull apart or separate forcibly into pieces; to tear or be torn.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tear_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to <w>rip</w> the paper when you open the package.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, чтобы не <w>порвать</w> бумагу, когда будешь открывать посылку."
                            }
                        },
                        {
                            "text": "My jeans <w>ripped</w> when I climbed over the fence.",
                            "target_lang_translations": {
                                "ru": "Мои джинсы <w>порвались</w>, когда я перелезал через забор."
                            }
                        }
                    ],
                    "synonyms": [
                        "tear"
                    ],
                    "common_phrases": [
                        "rip apart",
                        "rip up",
                        "rip open"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рвать или разрывать на части с усилием; порваться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рвать",
                                "target_lang_sense_clarification": "несовершенный вид"
                            },
                            {
                                "target_lang_word": "порвать",
                                "target_lang_sense_clarification": "совершенный вид"
                            },
                            {
                                "target_lang_word": "разорвать",
                                "target_lang_sense_clarification": "на части"
                            },
                            {
                                "target_lang_word": "порваться",
                                "target_lang_sense_clarification": "о ткани или материале"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27205adc-1113-3755-867a-0c36ceed8f17",
                    "sense_definition": "To remove something quickly and violently from its place.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "remove_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The strong wind threatened to <w>rip</w> the roof off the old shed.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер грозил <w>сорвать</w> крышу со старого сарая."
                            }
                        },
                        {
                            "text": "She <w>ripped</w> the poster off the wall in anger.",
                            "target_lang_translations": {
                                "ru": "Она в гневе <w>сорвала</w> плакат со стены."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rip off",
                        "rip out",
                        "rip away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро и с силой отрывать или вырывать что-либо с места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срывать",
                                "target_lang_sense_clarification": "удалять рывком"
                            },
                            {
                                "target_lang_word": "вырывать",
                                "target_lang_sense_clarification": "извлекать с силой"
                            },
                            {
                                "target_lang_word": "оторвать",
                                "target_lang_sense_clarification": "отделить часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1705c6f1-099a-3983-b7e8-28b2520f3003",
                    "sense_definition": "To move very quickly and violently, often causing damage.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "move_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A terrible fire <w>ripped</w> through the building last night.",
                            "target_lang_translations": {
                                "ru": "Прошлой ночью страшный пожар <w>пронёсся</w> по зданию."
                            }
                        },
                        {
                            "text": "The explosion <w>ripped</w> through the hull of the ship.",
                            "target_lang_translations": {
                                "ru": "Взрыв <w>разорвал</w> корпус корабля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rip through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться очень быстро и разрушительно (часто об огне или взрыве)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проноситься",
                                "target_lang_sense_clarification": "двигаться стремительно"
                            },
                            {
                                "target_lang_word": "бушевать",
                                "target_lang_sense_clarification": "об огне или стихии"
                            },
                            {
                                "target_lang_word": "рвануть",
                                "target_lang_sense_clarification": "резко устремиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0889235-1934-397c-b924-a04dd4be3d2e",
                    "sense_definition": "To copy audio or video data from a physical disc to a computer file.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "copy_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent the afternoon <w>ripping</w> his CD collection to his laptop.",
                            "target_lang_translations": {
                                "ru": "Он провёл день, <w>оцифровывая</w> свою коллекцию компакт-дисков на ноутбук."
                            }
                        },
                        {
                            "text": "You can <w>rip</w> the movie from the DVD to watch it on your tablet.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>скопировать</w> фильм с DVD, чтобы посмотреть его на планшете."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Копировать аудио или видео данные с физического диска в файл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оцифровывать",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "копировать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "рипать",
                                "target_lang_sense_clarification": "компьютерный сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "154ad86b-380b-3df3-882a-434f90924a6f",
                    "sense_definition": "A long cut or hole in a material made by tearing.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tear_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a large <w>rip</w> in the sleeve of my favorite jacket.",
                            "target_lang_translations": {
                                "ru": "На рукаве моей любимой куртки большая <w>дыра</w>."
                            }
                        },
                        {
                            "text": "She tried to mend the <w>rip</w> in the curtain with a needle and thread.",
                            "target_lang_translations": {
                                "ru": "Она попыталась зашить <w>разрыв</w> на занавеске иголкой с ниткой."
                            }
                        }
                    ],
                    "synonyms": [
                        "tear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный разрез или дыра в материале, сделанная в результате разрыва."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дыра",
                                "target_lang_sense_clarification": "отверстие"
                            },
                            {
                                "target_lang_word": "разрыв",
                                "target_lang_sense_clarification": "место повреждения"
                            },
                            {
                                "target_lang_word": "прореха",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa21c6ed-b1e7-3dc0-9f86-7138005d9257",
                    "sense_definition": "A strong current of water flowing away from the shore, which can be dangerous for swimmers.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "current_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lifeguards warned everyone about the dangerous <w>rip</w> near the pier.",
                            "target_lang_translations": {
                                "ru": "Спасатели предупредили всех об опасном <w>разрывном течении</w> у пирса."
                            }
                        },
                        {
                            "text": "If you get caught in a <w>rip</w>, you should swim parallel to the beach.",
                            "target_lang_translations": {
                                "ru": "Если вы попали в <w>тягун</w>, нужно плыть параллельно берегу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rip current",
                        "rip tide"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное течение, идущее от берега в море, опасное для пловцов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрывное течение",
                                "target_lang_sense_clarification": "термин"
                            },
                            {
                                "target_lang_word": "тягун",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15c7add5-1d1b-349b-a940-ac5e09703bfd",
                    "sense_definition": "A file containing audio or video data that has been copied from a physical disc.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "copy_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The quality of this audio <w>rip</w> is not very good.",
                            "target_lang_translations": {
                                "ru": "Качество этого аудио-<w>рипа</w> не очень хорошее."
                            }
                        },
                        {
                            "text": "He shared a digital <w>rip</w> of the rare album online.",
                            "target_lang_translations": {
                                "ru": "Он поделился цифровой <w>копией</w> редкого альбома в интернете."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Файл, содержащий аудио или видео данные, скопированные с диска."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рип",
                                "target_lang_sense_clarification": "слэнг"
                            },
                            {
                                "target_lang_word": "оцифровка",
                                "target_lang_sense_clarification": "результат копирования"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "0a21941e-5054-3eeb-95ad-78f978b94b4e",
                    "sense_definition": "An abbreviation for 'Rest in Peace', used to express condolences for the dead.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "condolence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>RIP</w> to the victims of the terrible accident.",
                            "target_lang_translations": {
                                "ru": "<w>Покойтесь с миром</w>, жертвы этой ужасной аварии."
                            }
                        },
                        {
                            "text": "We visited the grave and wrote <w>RIP</w> on the memorial card.",
                            "target_lang_translations": {
                                "ru": "Мы посетили могилу и написали \"<w>Покойся с миром</w>\" на поминальной открытке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "acronym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от 'Rest in Peace' (Покойся с миром), используемая для выражения соболезнований."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покойся с миром",
                                "target_lang_sense_clarification": "фраза"
                            },
                            {
                                "target_lang_word": "земля пухом",
                                "target_lang_sense_clarification": "идиома"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bde17568-23af-30ed-8287-7d4d846c1de4",
                    "sense_definition": "Used to express disappointment, sympathy, or embarrassment about a situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "slang_exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I forgot to save my game progress before the power went out, <w>rip</w>.",
                            "target_lang_translations": {
                                "ru": "Я забыл сохранить прогресс в игре перед тем, как отключили свет, <w>капец</w>."
                            }
                        },
                        {
                            "text": "<w>Rip</w> your phone screen; that crack looks really bad.",
                            "target_lang_translations": {
                                "ru": "<w>Хана</w> экрану твоего телефона; трещина выглядит очень плохо."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения разочарования, сочувствия или неловкости (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капец",
                                "target_lang_sense_clarification": "выражение досады"
                            },
                            {
                                "target_lang_word": "хана",
                                "target_lang_sense_clarification": "конец чему-либо"
                            },
                            {
                                "target_lang_word": "жесть",
                                "target_lang_sense_clarification": "эмоциональная оценка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "8a41b7bf-68fb-3904-8369-052dbffdea6e",
                    "sense_definition": "A male given name.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "<w>Rip</w> Van Winkle is a famous character in American literature.",
                            "target_lang_translations": {
                                "ru": "<w>Рип</w> Ван Винкль — известный персонаж американской литературы."
                            }
                        },
                        {
                            "text": "My uncle <w>Rip</w> lives in the countryside.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Рип</w> живёт в деревне."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Рип",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
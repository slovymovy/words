            "senses": [
                {
                    "sense_id": "16c2b996-97c4-3024-879d-ec3068324701",
                    "sense_definition": "Без малого, очень близко к чему-либо; так, что недостает совсем немного.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <w>почти</w> пришли к месту назначения.",
                            "target_lang_translations": {
                                "en": "We have <w>almost</w> arrived at the destination."
                            }
                        },
                        {
                            "text": "Я <w>почти</w> всё сделал, осталось совсем немного.",
                            "target_lang_translations": {
                                "en": "I have done <w>almost</w> everything, very little is left."
                            }
                        },
                        {
                            "text": "В зале собралось <w>почти</w> сто человек.",
                            "target_lang_translations": {
                                "en": "<w>Nearly</w> a hundred people gathered in the hall."
                            }
                        }
                    ],
                    "synonyms": [
                        "почти что",
                        "приблизительно",
                        "около"
                    ],
                    "antonyms": [
                        "точно",
                        "в точности"
                    ],
                    "common_phrases": [
                        "почти всё",
                        "почти всегда",
                        "почти никогда",
                        "почти готово",
                        "почти каждый"
                    ],
                    "target_lang_definitions": {
                        "en": "Very close to something, but not completely; lacking very little."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "almost",
                                "target_lang_sense_clarification": "Very close to, but not quite"
                            },
                            {
                                "target_lang_word": "nearly",
                                "target_lang_sense_clarification": "In close approximation; virtually"
                            }
                        ]
                    }
                }
            ]
        },

        "mugshot"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b0edceab-4106-3de4-9fea-519e0713bcb7",
                    "sense_definition": "A large cup for beverages, usually having a handle and used without a saucer.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cup_container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wrapped her hands around the warm <w>mug</w> of tea.",
                            "target_lang_translations": {
                                "ru": "Она обхватила руками теплую <w>кружку</w> чая."
                            }
                        },
                        {
                            "text": "He placed his empty coffee <w>mug</w> in the dishwasher.",
                            "target_lang_translations": {
                                "ru": "Он поставил свою пустую <w>кружку</w> из-под кофе в посудомоечную машину."
                            }
                        }
                    ],
                    "synonyms": [
                        "cup"
                    ],
                    "common_phrases": [
                        "coffee mug",
                        "beer mug",
                        "travel mug"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая чашка для напитков, обычно с ручкой и используемая без блюдца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружка",
                                "target_lang_sense_clarification": "сосуд для питья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bba3a864-041f-339d-9041-85e1e4673e7b",
                    "sense_definition": "The face.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "face_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He needs to wash his dirty <w>mug</w> before dinner.",
                            "target_lang_translations": {
                                "ru": "Ему нужно умыть свою грязную <w>рожу</w> перед ужином."
                            }
                        },
                        {
                            "text": "I do not want to see his ugly <w>mug</w> around here again.",
                            "target_lang_translations": {
                                "ru": "Я не хочу больше видеть здесь его уродливую <w>морду</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "face",
                        "dial",
                        "mush"
                    ],
                    "common_phrases": [
                        "ugly mug"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо (обычно в грубом или пренебрежительном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рожа",
                                "target_lang_sense_clarification": "грубое название лица"
                            },
                            {
                                "target_lang_word": "морда",
                                "target_lang_sense_clarification": "грубое название лица"
                            },
                            {
                                "target_lang_word": "харя",
                                "target_lang_sense_clarification": "просторечное название лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52d79ee4-ac7d-3da8-b809-ad9252df84d6",
                    "sense_definition": "A gullible or easily cheated person.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fool_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt like a complete <w>mug</w> for believing her lies.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя полным <w>лохом</w>, поверив её лжи."
                            }
                        },
                        {
                            "text": "Gambling is a <w>mug</w>'s game because the house always wins.",
                            "target_lang_translations": {
                                "ru": "Азартные игры — это занятие для <w>дураков</w>, потому что казино всегда выигрывает."
                            }
                        }
                    ],
                    "synonyms": [
                        "sucker",
                        "chump",
                        "dupe",
                        "mark",
                        "fool"
                    ],
                    "common_phrases": [
                        "mug's game"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Доверчивый человек, которого легко обмануть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простак",
                                "target_lang_sense_clarification": "наивный человек"
                            },
                            {
                                "target_lang_word": "лох",
                                "target_lang_sense_clarification": "жаргонное название жертвы обмана"
                            },
                            {
                                "target_lang_word": "простофиля",
                                "target_lang_sense_clarification": "разиня, глуповатый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05ce98d7-02a5-33fb-86e9-7632e0118964",
                    "sense_definition": "A photograph of a person's face taken for official police records.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "face_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective looked through a book of old <w>mugs</w> to find the suspect.",
                            "target_lang_translations": {
                                "ru": "Детектив просматривал альбом со старыми <w>фотографиями преступников</w>, чтобы найти подозреваемого."
                            }
                        },
                        {
                            "text": "They took his <w>mug</w> and fingerprints at the station.",
                            "target_lang_translations": {
                                "ru": "В участке сделали его <w>снимок</w> и сняли отпечатки пальцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "mug shot"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "short for mug shot"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фотография лица преступника для полицейского досье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фотография арестанта",
                                "target_lang_sense_clarification": "официальный термин"
                            },
                            {
                                "target_lang_word": "снимок",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0902c6a0-b8f6-3f1d-93be-f1466068e68a",
                    "sense_definition": "To assault someone in a public place with the intent of robbing them.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "crime_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <w>mugged</w> while walking home through the park at night.",
                            "target_lang_translations": {
                                "ru": "Его <w>ограбили</w>, когда он шел домой через парк ночью."
                            }
                        },
                        {
                            "text": "The gang threatened to <w>mug</w> the tourists for their cameras.",
                            "target_lang_translations": {
                                "ru": "Банда угрожала <w>ограбить</w> туристов ради их камер."
                            }
                        }
                    ],
                    "synonyms": [
                        "rob",
                        "assault",
                        "attack"
                    ],
                    "common_phrases": [
                        "get mugged"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нападать на кого-либо в общественном месте с целью ограбления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грабить",
                                "target_lang_sense_clarification": "отнимать имущество силой"
                            },
                            {
                                "target_lang_word": "ограбить",
                                "target_lang_sense_clarification": "совершенный вид глагола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b13dd2eb-cd2e-393f-98df-0c37a32096eb",
                    "sense_definition": "To make an exaggerated facial expression, often for a photograph or performance.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "face_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The comedian <w>mugged</w> for the audience to get a cheap laugh.",
                            "target_lang_translations": {
                                "ru": "Комик <w>кривлялся</w> перед публикой, чтобы вызвать дешевый смех."
                            }
                        },
                        {
                            "text": "Stop <w>mugging</w> at the camera and just smile naturally.",
                            "target_lang_translations": {
                                "ru": "Перестань <w>корчить рожи</w> на камеру и просто улыбнись естественно."
                            }
                        }
                    ],
                    "synonyms": [
                        "grimace",
                        "pull faces"
                    ],
                    "common_phrases": [
                        "mug for the camera"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать преувеличенное выражение лица, гримасничать (часто для фото или представления)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кривляться",
                                "target_lang_sense_clarification": "делать неестественные движения лицом"
                            },
                            {
                                "target_lang_word": "корчить рожи",
                                "target_lang_sense_clarification": "строить гримасы"
                            },
                            {
                                "target_lang_word": "гримасничать",
                                "target_lang_sense_clarification": "делать гримасы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4ced8c70-43cc-3409-b639-1288fa29ce99",
                    "sense_definition": "Easily fooled or gullible.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fool_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you think I am <w>mug</w> enough to buy that broken car?",
                            "target_lang_translations": {
                                "ru": "Ты думаешь, я достаточно <w>глуп</w>, чтобы купить эту сломанную машину?"
                            }
                        },
                        {
                            "text": "He was <w>mug</w> enough to believe the scammer's story.",
                            "target_lang_translations": {
                                "ru": "Он был достаточно <w>наивен</w>, чтобы поверить в историю мошенника."
                            }
                        }
                    ],
                    "synonyms": [
                        "gullible",
                        "naive"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Легковерный или глупый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глупый",
                                "target_lang_sense_clarification": "неумный"
                            },
                            {
                                "target_lang_word": "наивный",
                                "target_lang_sense_clarification": "излишне доверчивый"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
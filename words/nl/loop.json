                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Loop</w> je mee naar de supermarkt?",
                            "target_lang_translations": {
                                "ru": "Ты <w>идешь</w> со мной в супермаркет?"
                            }
                        },
                        {
                            "text": "Waarom <w>loop</w> je zo snel weg?",
                            "target_lang_translations": {
                                "ru": "Почему ты так быстро <w>уходишь</w>?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inversion"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Второе лицо единственного числа настоящего времени от глагола <w>lopen</w> (при инверсии)."

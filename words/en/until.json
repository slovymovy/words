    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "9897c4b0-5bf8-3e13-931a-053fcd55b4da",
                    "sense_definition": "Up to a specific point in time or a specific event.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shop is open from nine <w>until</w> five o'clock.",
                            "target_lang_translations": {
                                "ru": "Магазин открыт с девяти <w>до</w> пяти часов."
                            }
                        },
                        {
                            "text": "I will be busy working <w>until</w> tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Я буду занят работой <w>до</w> завтрашнего утра."
                            }
                        },
                        {
                            "text": "We have not seen any changes <w>until</w> now.",
                            "target_lang_translations": {
                                "ru": "Мы не видели никаких изменений <w>до</w> сих пор."
                            }
                        }
                    ],
                    "synonyms": [
                        "till",
                        "up to",
                        "'til"
                    ],
                    "antonyms": [
                        "since"
                    ],
                    "common_phrases": [
                        "until now",
                        "until then",
                        "until recently",
                        "until tomorrow",
                        "until further notice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вплоть до определенного момента времени или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "предлог времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4495622e-9bec-3336-a50f-b3324f21b4e1",
                    "sense_definition": "Up to a specific location or place.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Keep walking <w>until</w> the traffic lights and then turn left.",
                            "target_lang_translations": {
                                "ru": "Идите <w>до</w> светофора, а затем поверните налево."
                            }
                        },
                        {
                            "text": "Drive <w>until</w> the end of the road.",
                            "target_lang_translations": {
                                "ru": "Езжайте <w>до</w> конца дороги."
                            }
                        }
                    ],
                    "synonyms": [
                        "up to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вплоть до определенного места или локации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "предлог места"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "86e3c424-a321-35ac-a671-9a62c2d30a17",
                    "sense_definition": "Up to the time that a condition becomes true or an event happens.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please wait here <w>until</w> I come back.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подожди здесь, <w>пока</w> я <w>не</w> вернусь."
                            }
                        },
                        {
                            "text": "They played football outside <w>until</w> it got dark.",
                            "target_lang_translations": {
                                "ru": "Они играли в футбол на улице, <w>пока не</w> стемнело."
                            }
                        },
                        {
                            "text": "I did not understand the problem <w>until</w> he explained it.",
                            "target_lang_translations": {
                                "ru": "Я не понимал проблему, <w>пока</w> он её <w>не</w> объяснил."
                            }
                        }
                    ],
                    "synonyms": [
                        "till",
                        "'til"
                    ],
                    "antonyms": [
                        "since"
                    ],
                    "common_phrases": [
                        "wait until"
                    ],
                    "target_lang_definitions": {
                        "ru": "До того момента, когда условие станет истинным или произойдет событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока ... не",
                                "target_lang_sense_clarification": "союз"
                            },
                            {
                                "target_lang_word": "до тех пор, пока ... не",
                                "target_lang_sense_clarification": "союз (усиленный)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
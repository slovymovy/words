{
    "word_family": [
        "influential",
        "influencer",
        "uninfluenced"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "98ae6d42-9d68-3a82-8ca1-c2dd5aee3541",
                    "sense_definition": "The power or ability to have an effect on people or things, or to change how something develops.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "power_to_affect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have absolutely no <w>influence</w> over his decisions.",
                            "target_lang_translations": {
                                "ru": "У меня нет абсолютно никакого <w>влияния</w> на его решения."
                            }
                        },
                        {
                            "text": "The weather had a significant <w>influence</w> on our travel plans.",
                            "target_lang_translations": {
                                "ru": "Погода оказала значительное <w>влияние</w> на наши планы путешествия."
                            }
                        }
                    ],
                    "synonyms": [
                        "impact",
                        "effect",
                        "control",
                        "sway",
                        "authority"
                    ],
                    "antonyms": [
                        "powerlessness"
                    ],
                    "common_phrases": [
                        "have influence over",
                        "under the influence",
                        "sphere of influence",
                        "exercise influence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сила или способность оказывать воздействие на людей или вещи, либо изменять ход развития чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влияние",
                                "target_lang_sense_clarification": "действие, оказываемое на кого-либо или что-либо"
                            },
                            {
                                "target_lang_word": "воздействие",
                                "target_lang_sense_clarification": "активное влияние, сила действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d7dceb4-f0f7-3836-aa8b-84113c43c6ec",
                    "sense_definition": "A person or thing that affects someone or something else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "agent_of_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My older brother was a good <w>influence</w> on me when I was growing up.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат оказал на меня хорошее <w>влияние</w>, когда я рос."
                            }
                        },
                        {
                            "text": "He has been a great <w>influence</w> on the voters during the recent elections.",
                            "target_lang_translations": {
                                "ru": "Он имел огромное <w>влияние</w> на избирателей во время недавних выборов."
                            }
                        }
                    ],
                    "synonyms": [
                        "inspiration",
                        "example",
                        "model"
                    ],
                    "common_phrases": [
                        "bad influence",
                        "good influence",
                        "major influence",
                        "positive influence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или вещь, оказывающие воздействие на кого-то или что-то другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влияние",
                                "target_lang_sense_clarification": "воздействующий фактор или пример"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d8997b6-aa00-388e-b5d7-fc9e99c000ec",
                    "sense_definition": "To have an effect on the character, development, or behavior of someone or something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "to_affect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician wants to <w>influence</w> the public with his new speech.",
                            "target_lang_translations": {
                                "ru": "Политик хочет <w>повлиять</w> на общественность своей новой речью."
                            }
                        },
                        {
                            "text": "I must admit that this book <w>influenced</w> my outlook on life significantly.",
                            "target_lang_translations": {
                                "ru": "Я должен признать, что эта книга значительно <w>повлияла</w> на мои взгляды на жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "affect",
                        "impact",
                        "sway",
                        "shape",
                        "mold"
                    ],
                    "common_phrases": [
                        "influence a decision",
                        "heavily influenced",
                        "influence the outcome",
                        "easily influenced"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать воздействие на характер, развитие или поведение кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влиять",
                                "target_lang_sense_clarification": "оказывать воздействие (несовершенный вид)"
                            },
                            {
                                "target_lang_word": "повлиять",
                                "target_lang_sense_clarification": "изменить что-либо, произвести эффект (совершенный вид)"
                            },
                            {
                                "target_lang_word": "воздействовать",
                                "target_lang_sense_clarification": "активно управлять или менять состояние"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
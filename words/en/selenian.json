            "senses": [
                {
                    "sense_id": "8953d9f7-e145-3d11-9e1f-af78ee63f307",
                    "sense_definition": "Of or relating to the Moon.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "moon_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The telescope revealed the stark beauty of the <w>selenian</w> landscape.",
                            "target_lang_translations": {
                                "ru": "Телескоп открыл суровую красоту <w>лунного</w> пейзажа."
                            }
                        },
                        {
                            "text": "Early astronomers made detailed maps of the <w>selenian</w> surface.",
                            "target_lang_translations": {
                                "ru": "Ранние астрономы составили подробные карты <w>лунной</w> поверхности."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Луне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лунный",
                                "target_lang_sense_clarification": "относящийся к естественному спутнику Земли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4906ff0-4288-3210-aad0-bd9316f1e199",
                    "sense_definition": "Containing selenium, especially when this replaces another element.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "chemistry_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chemist synthesized a new <w>selenian</w> compound for the experiment.",
                            "target_lang_translations": {
                                "ru": "Химик синтезировал новое <w>селеновое</w> соединение для эксперимента."
                            }
                        },
                        {
                            "text": "They analyzed the <w>selenian</w> structure of the mineral sample.",
                            "target_lang_translations": {
                                "ru": "Они проанализировали <w>селеновую</w> структуру образца минерала."
                            }
                        }
                    ],
                    "synonyms": [
                        "selenic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий селен, особенно когда он замещает другой элемент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "селеновый",
                                "target_lang_sense_clarification": "относящийся к химическому элементу селен"
                            },
                            {
                                "target_lang_word": "селенистый",
                                "target_lang_sense_clarification": "содержащий селен (в химии)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ae5292a-f5d6-3d23-9b2c-2ffad8ca5c4d",
                    "sense_definition": "Of or relating to the goddess Selene in Greek mythology.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mythology_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poem explores <w>selenian</w> myths regarding the night sky.",
                            "target_lang_translations": {
                                "ru": "Поэма исследует мифы о <w>Селене</w>, касающиеся ночного неба."
                            }
                        },
                        {
                            "text": "Ancient texts describe the <w>selenian</w> chariot driving across the heavens.",
                            "target_lang_translations": {
                                "ru": "Древние тексты описывают колесницу <w>Селены</w>, едущую по небесам."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к богине Селене в греческой мифологии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Селены",
                                "target_lang_sense_clarification": "принадлежащий богине Луны (родительный падеж)"
                            },
                            {
                                "target_lang_word": "лунный",
                                "target_lang_sense_clarification": "в контексте мифологии"
                            }
                        ]
                    }
                }
            ]
        },

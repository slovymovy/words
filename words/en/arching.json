            "senses": [
                {
                    "sense_id": "698510fe-d13e-3b56-9f8f-21d09674f028",
                    "sense_definition": "Present participle and gerund of <w>arch</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cat was <w>arching</w> its back and hissing at the dog.",
                            "target_lang_translations": {
                                "ru": "Кошка <w>выгибала</w> спину и шипела на собаку."
                            }
                        },
                        {
                            "text": "She looked at him while <w>arching</w> her eyebrows in surprise.",
                            "target_lang_translations": {
                                "ru": "Она смотрела на него, удивленно <w>приподнимая</w> брови."
                            }
                        }
                    ],
                    "common_phrases": [
                        "arching over",
                        "arching back"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c6fb8cf7-abdb-35c7-9e99-2e2ada870528",
                    "sense_definition": "Present participle and gerund of <w>wonder</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was <w>wondering</w> if you would like to join us for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Я <w>хотел поинтересоваться</w>, не хотели бы вы присоединиться к нам за ужином сегодня вечером."
                            }
                        },
                        {
                            "text": "She is still <w>wondering</w> where she put her car keys.",
                            "target_lang_translations": {
                                "ru": "Она всё ещё <w>гадает</w>, куда положила ключи от машины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "was wondering if",
                        "just wondering",
                        "start wondering"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола wonder (интересоваться, задаваться вопросом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интересоваться",
                                "target_lang_sense_clarification": "проявлять интерес, желать узнать"
                            },
                            {
                                "target_lang_word": "задаваться вопросом",
                                "target_lang_sense_clarification": "спрашивать самого себя, размышлять над вопросом"
                            },
                            {
                                "target_lang_word": "гадать",
                                "target_lang_sense_clarification": "строить предположения, пытаться понять"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6a7ae440-0225-3901-9399-361a92ebdcd9",
                    "sense_definition": "Showing or feeling curiosity, surprise, or doubt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The little boy looked up at the giant statue with <w>wondering</w> eyes.",
                            "target_lang_translations": {
                                "ru": "Маленький мальчик смотрел на гигантскую статую <w>удивлёнными</w> глазами."
                            }
                        },
                        {
                            "text": "She cast a <w>wondering</w> glance at the unexpected package on her desk.",
                            "target_lang_translations": {
                                "ru": "Она бросила <w>вопрошающий</w> взгляд на неожиданный пакет на своём столе."
                            }
                        }
                    ],
                    "synonyms": [
                        "curious",
                        "inquisitive",
                        "questioning"
                    ],
                    "antonyms": [
                        "indifferent",
                        "uninterested",
                        "bored"
                    ],
                    "common_phrases": [
                        "wondering eyes",
                        "wondering look",
                        "wondering silence",
                        "wondering glance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий или испытывающий любопытство, удивление или сомнение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удивлённый",
                                "target_lang_sense_clarification": "выражающий изумление или сюрприз"
                            },
                            {
                                "target_lang_word": "вопрошающий",
                                "target_lang_sense_clarification": "выражающий вопрос, сомнение или ожидание ответа"
                            },
                            {
                                "target_lang_word": "любопытный",
                                "target_lang_sense_clarification": "проявляющий интерес или любознательность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2e7956cb-581e-3da7-afd2-2836ac093fde",
                    "sense_definition": "The mental activity of thinking about something with curiosity or doubt; a thought or query.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His philosophical <w>wonderings</w> often kept him awake late into the night.",
                            "target_lang_translations": {
                                "ru": "Его философские <w>размышления</w> часто не давали ему уснуть до глубокой ночи."
                            }
                        },
                        {
                            "text": "The journal was filled with her private <w>wonderings</w> about the nature of life.",
                            "target_lang_translations": {
                                "ru": "Дневник был полон её личных <w>раздумий</w> о природе жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "musing",
                        "speculation",
                        "reflection",
                        "contemplation"
                    ],
                    "common_phrases": [
                        "idle wonderings",
                        "inner wonderings"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умственная деятельность, связанная с размышлениями о чем-либо с любопытством или сомнением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размышление",
                                "target_lang_sense_clarification": "процесс обдумывания чего-либо"
                            },
                            {
                                "target_lang_word": "раздумье",
                                "target_lang_sense_clarification": "состояние глубокой задумчивости"
                            }
                        ]
                    }
                }
            ]
        }
    ]

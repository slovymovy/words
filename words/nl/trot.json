    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4a9257ef-583b-3371-878c-0487d27986a5",
                    "sense_definition": "Een gang van een paard of ander dier die sneller is dan stap maar langzamer dan galop; draf.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_gait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het paard ging over van stap naar <w>trot</w>.",
                            "target_lang_translations": {
                                "en": "The horse switched from a walk to a <w>trot</w>."
                            }
                        },
                        {
                            "text": "De ruiter liet het dier in een rustige <w>trot</w> lopen.",
                            "target_lang_translations": {
                                "en": "The rider let the animal move in a calm <w>trot</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "draf"
                    ],
                    "antonyms": [
                        "stap",
                        "galop"
                    ],
                    "common_phrases": [
                        "op trot"
                    ],
                    "target_lang_definitions": {
                        "en": "A gait of a horse or other animal that is faster than a walk but slower than a gallop; trot."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trot",
                                "target_lang_sense_clarification": "gait of a horse"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "drink",
        "drinker",
        "drunken",
        "drunkenness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b04e9a17-d776-306c-993f-2ccbe2d92203",
                    "sense_definition": "Intoxicated as a result of excessive alcohol consumption.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "intoxicated_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He got <w>drunk</w> at the party and fell asleep on the sofa.",
                            "target_lang_translations": {
                                "ru": "Он <w>напился</w> на вечеринке и уснул на диване."
                            }
                        },
                        {
                            "text": "It is illegal and dangerous to drive while you are <w>drunk</w>.",
                            "target_lang_translations": {
                                "ru": "Водить машину <w>пьяным</w> незаконно и опасно."
                            }
                        }
                    ],
                    "synonyms": [
                        "intoxicated",
                        "inebriated",
                        "tipsy",
                        "wasted",
                        "hammered",
                        "plastered"
                    ],
                    "antonyms": [
                        "sober",
                        "teetotal"
                    ],
                    "common_phrases": [
                        "get drunk",
                        "blind drunk",
                        "dead drunk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в состоянии алкогольного опьянения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пьяный",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "нетрезвый",
                                "target_lang_sense_clarification": "формальное или вежливое"
                            },
                            {
                                "target_lang_word": "бухой",
                                "target_lang_sense_clarification": "сленг, грубое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dccebae-25c5-34f8-bedd-b4b49af69cf5",
                    "sense_definition": "Elated or emboldened by a strong emotion, such as power or success.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dictator was <w>drunk</w> with power and refused to listen to his advisors.",
                            "target_lang_translations": {
                                "ru": "Диктатор был <w>опьянён</w> властью и отказывался слушать своих советников."
                            }
                        },
                        {
                            "text": "She felt <w>drunk</w> with joy after winning the championship.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала себя <w>пьяной</w> от радости после победы в чемпионате."
                            }
                        }
                    ],
                    "synonyms": [
                        "ecstatic",
                        "euphoric",
                        "elated"
                    ],
                    "common_phrases": [
                        "drunk with power",
                        "drunk with success"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охваченный сильным чувством, восторгом или страстью (обычно властью или успехом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опьянённый",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "пьяный",
                                "target_lang_sense_clarification": "разговорный вариант для переносного смысла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08cabc9b-7550-3f76-a766-ae5d14ed04e1",
                    "sense_definition": "Drenched or saturated with moisture or liquid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "saturated",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fields were <w>drunk</w> with the heavy rain that fell all night.",
                            "target_lang_translations": {
                                "ru": "Поля были <w>напитаны</w> сильным дождём, который шёл всю ночь."
                            }
                        },
                        {
                            "text": "The poet described arrows <w>drunk</w> with blood.",
                            "target_lang_translations": {
                                "ru": "Поэт описывал стрелы, <w>опьянённые</w> кровью."
                            }
                        }
                    ],
                    "synonyms": [
                        "saturated",
                        "drenched",
                        "soaked"
                    ],
                    "antonyms": [
                        "abstemious"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Напитанный влагой; пропитанный чем-либо (поэтическое или устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напитанный",
                                "target_lang_sense_clarification": "насыщенный влагой"
                            },
                            {
                                "target_lang_word": "пропитанный",
                                "target_lang_sense_clarification": "полностью мокрый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d6994d96-6dcc-3482-ab7d-35acadec84cd",
                    "sense_definition": "The past participle of <w>drink</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have <w>drunk</w> three cups of coffee this morning.",
                            "target_lang_translations": {
                                "ru": "Я <w>выпил</w> три чашки кофе сегодня утром."
                            }
                        },
                        {
                            "text": "She had <w>drunk</w> all the water before we arrived.",
                            "target_lang_translations": {
                                "ru": "Она <w>выпила</w> всю воду до того, как мы приехали."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие прошедшего времени от глагола drink (пить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпитый",
                                "target_lang_sense_clarification": "страдательное причастие"
                            },
                            {
                                "target_lang_word": "выпил",
                                "target_lang_sense_clarification": "форма глагола прошедшего времени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e81f18a9-cad0-3aa7-9489-bc026fc06229",
                    "sense_definition": "A habitual drinker, especially one who is frequently intoxicated.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The town <w>drunk</w> was often seen sleeping on the park bench.",
                            "target_lang_translations": {
                                "ru": "Городского <w>пьяницу</w> часто видели спящим на скамейке в парке."
                            }
                        },
                        {
                            "text": "He became a hopeless <w>drunk</w> after he lost his job.",
                            "target_lang_translations": {
                                "ru": "Он стал безнадёжным <w>пьяницей</w> после того, как потерял работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "drunkard",
                        "alcoholic",
                        "sot",
                        "lush",
                        "wino"
                    ],
                    "antonyms": [
                        "teetotal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, злоупотребляющий алкоголем; алкоголик."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пьяница",
                                "target_lang_sense_clarification": "общеупотребительное"
                            },
                            {
                                "target_lang_word": "алкоголик",
                                "target_lang_sense_clarification": "медицинский или официальный термин"
                            },
                            {
                                "target_lang_word": "алкаш",
                                "target_lang_sense_clarification": "просторечное, пренебрежительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "654972fe-d0a1-354a-acf1-6f85a9acb8ce",
                    "sense_definition": "A prolonged period of drinking alcohol; a drinking bout.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He went on a three-day <w>drunk</w> and missed work.",
                            "target_lang_translations": {
                                "ru": "Он ушёл в трёхдневный <w>запой</w> и пропустил работу."
                            }
                        },
                        {
                            "text": "After his long <w>drunk</w>, he decided to get sober.",
                            "target_lang_translations": {
                                "ru": "После своего длительного <w>запоя</w> он решил протрезветь."
                            }
                        }
                    ],
                    "synonyms": [
                        "bender",
                        "binge"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длительный период употребления алкоголя; запой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запой",
                                "target_lang_sense_clarification": "длительное пьянство"
                            },
                            {
                                "target_lang_word": "пьянка",
                                "target_lang_sense_clarification": "разговорное, вечеринка с выпивкой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0884773d-6edb-3f4e-ab1b-f6029a3021ff",
                    "sense_definition": "In a drunken state.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "adverbial_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was arrested for driving <w>drunk</w>.",
                            "target_lang_translations": {
                                "ru": "Его арестовали за вождение <w>в нетрезвом виде</w>."
                            }
                        },
                        {
                            "text": "They acted <w>drunk</w> even though they had only had soda.",
                            "target_lang_translations": {
                                "ru": "Они вели себя как <w>пьяные</w>, хотя пили только газировку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "driving drunk"
                    ],
                    "target_lang_definitions": {
                        "ru": "В нетрезвом состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пьяным",
                                "target_lang_sense_clarification": "творительный падеж прилагательного"
                            },
                            {
                                "target_lang_word": "в нетрезвом виде",
                                "target_lang_sense_clarification": "официальная фраза"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "aa1d157c-6fed-3a21-baa5-e8bc6bf47a93",
                    "sense_definition": "Wskazuje na osobę, rzecz lub zjawisko towarzyszące komuś lub czemuś; oznacza bycie razem, posiadanie czegoś lub wykonywanie czynności wspólnie (łączy się z narzędnikiem).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_instrumental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Idę do kina <w>z</w> moim bratem.",
                            "target_lang_translations": {
                                "ru": "Я иду в кино <w>с</w> моим братом."
                            }
                        },
                        {
                            "text": "Czy pijesz kawę <w>z</w> mlekiem?",
                            "target_lang_translations": {
                                "ru": "Ты пьёшь кофе <w>с</w> молоком?"
                            }
                        },
                        {
                            "text": "Mężczyzna <w>z</w> brodą wszedł do sklepu.",
                            "target_lang_translations": {
                                "ru": "Мужчина <w>с</w> бородой вошёл в магазин."
                            }
                        }
                    ],
                    "synonyms": [
                        "ze",
                        "około"
                    ],
                    "antonyms": [
                        "bez"
                    ],
                    "common_phrases": [
                        "z rodziną",
                        "z przyjaciółmi",
                        "kawa z mlekiem",
                        "razem z"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на лицо, предмет или явление, сопровождающее кого-то или что-то; означает совместность, обладание чем-то или выполнение действия вместе (употребляется с творительным падежом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "указывает на совместность или наличие (с кем? с чем?)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5efe6c39-7f5b-3da3-82ec-8ef24b440776",
                    "sense_definition": "Wskazuje na miejsce pochodzenia, punkt wyjścia ruchu lub źródło, z którego coś się wywodzi (łączy się z dopełniaczem).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_genitive_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy kolega jest <w>z</w> Polski.",
                            "target_lang_translations": {
                                "ru": "Мой новый коллега <w>из</w> Польши."
                            }
                        },
                        {
                            "text": "Właśnie wracam <w>z</w> pracy do domu.",
                            "target_lang_translations": {
                                "ru": "Я как раз возвращаюсь <w>с</w> работы домой."
                            }
                        },
                        {
                            "text": "Ten cytat pochodzi <w>z</w> ulubionej książki mojej mamy.",
                            "target_lang_translations": {
                                "ru": "Эта цитата взята <w>из</w> любимой книги моей мамы."
                            }
                        }
                    ],
                    "synonyms": [
                        "ze",
                        "ze"
                    ],
                    "antonyms": [
                        "bez"
                    ],
                    "common_phrases": [
                        "z domu",
                        "z pracy",
                        "z Polski",
                        "z daleka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на место происхождения, начальную точку движения или источник, из которого что-то происходит (употребляется с родительным падежом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "движение изнутри или происхождение (из страны, из дома)"
                            },
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "движение с поверхности или мероприятия (с работы, с улицы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17537e91-a10b-34c9-be14-15745fd16bb1",
                    "sense_definition": "Wskazuje na materiał, tworzywo lub składnik, z którego coś jest zrobione lub się składa (łączy się z dopełniaczem).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_genitive_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stół jest zrobiony <w>z</w> drewna dębowego.",
                            "target_lang_translations": {
                                "ru": "Этот стол сделан <w>из</w> дуба."
                            }
                        },
                        {
                            "text": "Ugotowałem pyszną zupę <w>z</w> jarzyn.",
                            "target_lang_translations": {
                                "ru": "Я сварил вкусный суп <w>из</w> овощей."
                            }
                        }
                    ],
                    "synonyms": [
                        "ze"
                    ],
                    "common_phrases": [
                        "z drewna",
                        "ze złota",
                        "z plastiku",
                        "jeden z"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на материал, вещество или компонент, из которого что-то сделано или состоит (употребляется с родительным падежом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "указывает на материал изготовления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bcc3964-a113-37ff-b9ef-b65dc5d2eae2",
                    "sense_definition": "Wskazuje na przyczynę lub powód jakiegoś stanu, zachowania lub zdarzenia (łączy się z dopełniaczem).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_genitive_cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczynka płakała <w>z</w> radości, gdy dostała prezent.",
                            "target_lang_translations": {
                                "ru": "Девочка плакала <w>от</w> радости, когда получила подарок."
                            }
                        },
                        {
                            "text": "Zrobiłem to <w>z</w> głupoty i teraz żałuję.",
                            "target_lang_translations": {
                                "ru": "Я сделал это <w>из</w> глупости и теперь жалею."
                            }
                        }
                    ],
                    "synonyms": [
                        "ze"
                    ],
                    "common_phrases": [
                        "z radości",
                        "z nudów",
                        "z powodu",
                        "z zazdrości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на причину или повод какого-либо состояния, поведения или события (употребляется с родительным падежом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "указывает на причину (обычно эмоциональную или физическую)"
                            },
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "указывает на внутренний мотив или побуждение"
                            },
                            {
                                "target_lang_word": "из-за",
                                "target_lang_sense_clarification": "указывает на внешнюю причину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9a36c97-2097-39ed-9fdc-47bf315d89b7",
                    "sense_definition": "Wskazuje na czas powstania czegoś lub okres, z którego coś pochodzi (łączy się z dopełniaczem).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_genitive_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oglądaliśmy wczoraj stary film <w>z</w> lat dziewięćdziesiątych.",
                            "target_lang_translations": {
                                "ru": "Мы вчера смотрели старый фильм <w>из</w> девяностых."
                            }
                        },
                        {
                            "text": "To jest pamiątka <w>z</w> moich wakacji nad morzem.",
                            "target_lang_translations": {
                                "ru": "Это сувенир <w>с</w> моих каникул на море."
                            }
                        }
                    ],
                    "synonyms": [
                        "ze"
                    ],
                    "common_phrases": [
                        "z rana",
                        "z dzieciństwa",
                        "z dnia na dzień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на время возникновения чего-либо или период, к которому что-то относится (употребляется с родительным падежом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "происхождение из определенного периода"
                            },
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "начало отсчета времени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "43b2c87a-7d02-3a79-b92b-b1a92ceff402",
                    "sense_definition": "Słowo stawiane przed liczebnikami lub określeniami czasu, oznaczające przybliżoną ilość lub wartość.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_particle_approx",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czekam tu na ciebie już <w>z</w> godzinę.",
                            "target_lang_translations": {
                                "ru": "Я жду тебя тут уже <w>около</w> часа."
                            }
                        },
                        {
                            "text": "Pożycz mi <w>z</w> dziesięć złotych na zakupy.",
                            "target_lang_translations": {
                                "ru": "Одолжи мне <w>около</w> десяти злотых на покупки."
                            }
                        }
                    ],
                    "synonyms": [
                        "około",
                        "mniej więcej",
                        "ze"
                    ],
                    "common_phrases": [
                        "z godzinę",
                        "z tydzień",
                        "z pięć razy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, которое ставится перед числительными или обозначениями времени, означающее приблизительное количество или значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "приблизительное количество"
                            },
                            {
                                "target_lang_word": "примерно",
                                "target_lang_sense_clarification": "наречие со значением приблизительности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9d04b83-506f-3568-aba5-b15eff76ba35",
                    "sense_definition": "Trzydziesta litera polskiego alfabetu.",
                    "learner_level": "A1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_noun_letter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wyraz \"zebra\" zaczyna się na literę <w>z</w>.",
                            "target_lang_translations": {
                                "ru": "Слово «зебра» начинается на букву <w>з</w>."
                            }
                        },
                        {
                            "text": "W polskim alfabecie po literze y występuje <w>z</w>.",
                            "target_lang_translations": {
                                "ru": "В польском алфавите после буквы y идёт <w>z</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "od a do z"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тридцатая буква польского алфавита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "з",
                                "target_lang_sense_clarification": "название буквы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
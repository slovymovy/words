{
    "word_family": [
        "coordination",
        "coordinator",
        "coordinative",
        "uncoordinated"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7399e76b-2e8a-3a5e-817a-98b9909e823b",
                    "sense_definition": "To organize different people or activities so that they work together efficiently.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>coordinate</w> our schedules to find a time to meet.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>согласовать</w> наши расписания, чтобы найти время для встречи."
                            }
                        },
                        {
                            "text": "It can be difficult to <w>coordinate</w> movement of both legs after an operation.",
                            "target_lang_translations": {
                                "ru": "После операции может быть трудно <w>координировать</w> движения обеих ног."
                            }
                        }
                    ],
                    "synonyms": [
                        "organize",
                        "synchronize",
                        "manage"
                    ],
                    "common_phrases": [
                        "coordinate efforts",
                        "coordinate activities",
                        "coordinate with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организовывать людей или действия для эффективной совместной работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "координировать",
                                "target_lang_sense_clarification": "управлять взаимодействием"
                            },
                            {
                                "target_lang_word": "согласовывать",
                                "target_lang_sense_clarification": "приводить в соответствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea0d25df-40b4-395f-8c21-c0063546bd8c",
                    "sense_definition": "To look good together because colors or styles match.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "matching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The outfit you're wearing doesn't <w>coordinate</w>.",
                            "target_lang_translations": {
                                "ru": "Твой наряд не <w>сочетается</w>."
                            }
                        },
                        {
                            "text": "The curtains <w>coordinate</w> perfectly with the new rug.",
                            "target_lang_translations": {
                                "ru": "Шторы идеально <w>гармонируют</w> с новым ковром."
                            }
                        }
                    ],
                    "synonyms": [
                        "match",
                        "harmonize"
                    ],
                    "common_phrases": [
                        "colors coordinate",
                        "coordinate with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо смотреться вместе благодаря сочетанию цветов или стилей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сочетаться",
                                "target_lang_sense_clarification": "подходить друг к другу"
                            },
                            {
                                "target_lang_word": "гармонировать",
                                "target_lang_sense_clarification": "быть в гармонии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53961a81-a346-3b44-a678-ecfdc2e773bb",
                    "sense_definition": "To place in the same order or rank.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system attempts to <w>coordinate</w> the data streams by priority.",
                            "target_lang_translations": {
                                "ru": "Система пытается <w>упорядочить</w> потоки данных по приоритету."
                            }
                        },
                        {
                            "text": "They aim to <w>coordinate</w> the new department with the existing ones.",
                            "target_lang_translations": {
                                "ru": "Они стремятся <w>уравнять</w> новый отдел с существующими."
                            }
                        }
                    ],
                    "antonyms": [
                        "subordinate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Располагать в определенном порядке или присваивать равный статус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упорядочивать",
                                "target_lang_sense_clarification": "приводить в порядок"
                            },
                            {
                                "target_lang_word": "уравнивать",
                                "target_lang_sense_clarification": "делать равным по статусу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "419f3d9d-0699-354a-a00b-5360ad19c7d8",
                    "sense_definition": "A set of numbers used to identify a specific location on a map, grid, or graph.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Give me your <w>coordinates</w> and we'll come and rescue you.",
                            "target_lang_translations": {
                                "ru": "Дай мне свои <w>координаты</w>, и мы приедем тебя спасать."
                            }
                        },
                        {
                            "text": "The pilot entered the <w>coordinates</w> into the navigation system.",
                            "target_lang_translations": {
                                "ru": "Пилот ввел <w>координаты</w> в навигационную систему."
                            }
                        }
                    ],
                    "synonyms": [
                        "position",
                        "location"
                    ],
                    "common_phrases": [
                        "GPS coordinates",
                        "map coordinates",
                        "enter coordinates"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор чисел, определяющих положение точки на карте, сетке или графике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "координата",
                                "target_lang_sense_clarification": "математический или географический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "796b06e9-6c3a-3b49-93f3-caf96d6af45d",
                    "sense_definition": "Clothing items designed to be worn together.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "matching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a set of silk <w>coordinates</w> for the summer trip.",
                            "target_lang_translations": {
                                "ru": "Она купила шелковый <w>комплект</w> для летней поездки."
                            }
                        },
                        {
                            "text": "The store sells matching <w>coordinates</w> in various floral prints.",
                            "target_lang_translations": {
                                "ru": "Магазин продает сочетающиеся <w>комплекты</w> с различными цветочными принтами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "matching coordinates"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предметы одежды, созданные для ношения в комплекте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комплект",
                                "target_lang_sense_clarification": "набор одежды"
                            },
                            {
                                "target_lang_word": "ансамбль",
                                "target_lang_sense_clarification": "сочетание одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce422c9b-f6fa-3dc5-bbe7-6089b05c8dd6",
                    "sense_definition": "A person or thing of equal rank or importance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These agencies act as <w>coordinates</w> within the federal system.",
                            "target_lang_translations": {
                                "ru": "Эти агентства действуют как <w>равноправные единицы</w> в рамках федеральной системы."
                            }
                        },
                        {
                            "text": "The two managers are <w>coordinates</w> and share decision-making power.",
                            "target_lang_translations": {
                                "ru": "Эти два менеджера — <w>ровня</w>, и они делят полномочия по принятию решений."
                            }
                        }
                    ],
                    "synonyms": [
                        "equal",
                        "peer"
                    ],
                    "antonyms": [
                        "subordinate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет равного ранга или важности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ровня",
                                "target_lang_sense_clarification": "равный по положению человек"
                            },
                            {
                                "target_lang_word": "аналог",
                                "target_lang_sense_clarification": "равнозначный предмет"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f4a9b4a0-2974-33ad-9960-01c1afe27e7d",
                    "sense_definition": "Equal in rank, importance, or quality.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two clauses in the sentence are <w>coordinate</w>.",
                            "target_lang_translations": {
                                "ru": "Два простых предложения в составе сложного являются <w>равноправными</w>."
                            }
                        },
                        {
                            "text": "They act as <w>coordinate</w> partners in the business venture.",
                            "target_lang_translations": {
                                "ru": "Они действуют как <w>равноправные</w> партнеры в этом бизнес-проекте."
                            }
                        }
                    ],
                    "synonyms": [
                        "equal",
                        "equivalent"
                    ],
                    "antonyms": [
                        "subordinate"
                    ],
                    "common_phrases": [
                        "coordinate clause",
                        "coordinate conjunction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Равный по рангу, важности или качеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равноправный",
                                "target_lang_sense_clarification": "имеющий равные права"
                            },
                            {
                                "target_lang_word": "сочинительный",
                                "target_lang_sense_clarification": "грамматический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
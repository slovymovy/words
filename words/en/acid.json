    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12a26e42-0ed4-39a6-a5fc-8ce3164c7d74",
                    "sense_definition": "A chemical substance with a pH of less than 7 that can dissolve metals, neutralize alkalis, and typically has a sour taste.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "chemistry_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The stomach produces strong <w>acid</w> to help digest food.",
                            "target_lang_translations": {
                                "ru": "Желудок вырабатывает сильную <w>кислоту</w>, чтобы помочь переварить пищу."
                            }
                        },
                        {
                            "text": "Be careful not to spill the sulfuric <w>acid</w> on your skin.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, не пролей серную <w>кислоту</w> на кожу."
                            }
                        }
                    ],
                    "antonyms": [
                        "alkali",
                        "base"
                    ],
                    "common_phrases": [
                        "stomach acid",
                        "sulfuric acid",
                        "citric acid",
                        "fatty acid",
                        "amino acid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химическое вещество с pH менее 7, которое может растворять металлы, нейтрализовать щелочи и обычно имеет кислый вкус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кислота",
                                "target_lang_sense_clarification": "химическое вещество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48120d61-7d65-3d02-81cd-ab3441570041",
                    "sense_definition": "A powerful hallucinogenic drug; LSD.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drug_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He claimed to have strange visions after he dropped <w>acid</w>.",
                            "target_lang_translations": {
                                "ru": "Он утверждал, что видел странные видения после того, как закинулся <w>кислотой</w>."
                            }
                        },
                        {
                            "text": "The documentary explored the counterculture movement and the use of <w>acid</w> in the 1960s.",
                            "target_lang_translations": {
                                "ru": "Документальный фильм исследовал контркультурное движение и употребление <w>кислоты</w> в 1960-х годах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drop acid",
                        "on acid"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "drug culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный галлюциногенный наркотик; ЛСД."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кислота",
                                "target_lang_sense_clarification": "сленг: наркотик"
                            },
                            {
                                "target_lang_word": "ЛСД",
                                "target_lang_sense_clarification": "наркотическое вещество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "096c9d77-73a8-338b-a7da-83e05b606f5d",
                    "sense_definition": "A set of properties (atomicity, consistency, isolation, durability) that guarantee database transactions are processed reliably.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computing_acronym",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Modern relational databases are designed to ensure <w>ACID</w> compliance for data integrity.",
                            "target_lang_translations": {
                                "ru": "Современные реляционные базы данных разработаны для обеспечения соответствия требованиям <w>ACID</w> ради целостности данных."
                            }
                        },
                        {
                            "text": "The developer explained how the system maintains <w>ACID</w> properties during a crash.",
                            "target_lang_translations": {
                                "ru": "Разработчик объяснил, как система сохраняет свойства <w>ACID</w> во время сбоя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ACID properties",
                        "ACID compliance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор свойств (атомарность, согласованность, изоляция, долговечность), гарантирующих надежную обработку транзакций в базе данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ACID",
                                "target_lang_sense_clarification": "информатика: требования к транзакциям"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4fffd61f-361d-3711-a7ac-c0fd0425e72f",
                    "sense_definition": "Containing acid or having the chemical properties of an acid.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "chem_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Some plants, like azaleas and blueberries, grow best in <w>acid</w> soil.",
                            "target_lang_translations": {
                                "ru": "Некоторые растения, такие как азалии и черника, лучше всего растут в <w>кислой</w> почве."
                            }
                        },
                        {
                            "text": "Pollution from factories has caused <w>acid</w> rain in the region.",
                            "target_lang_translations": {
                                "ru": "Загрязнение от заводов вызвало <w>кислотные</w> дожди в регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "acidic"
                    ],
                    "antonyms": [
                        "alkaline"
                    ],
                    "common_phrases": [
                        "acid rain",
                        "acid soil"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий кислоту или обладающий химическими свойствами кислоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кислотный",
                                "target_lang_sense_clarification": "содержащий кислоту (напр. дождь)"
                            },
                            {
                                "target_lang_word": "кислый",
                                "target_lang_sense_clarification": "о почве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f91bc51f-d57d-365e-876d-c4ac11adada1",
                    "sense_definition": "Having a sharp, sour taste like vinegar.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "taste_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The unripe fruit had a sharp, <w>acid</w> flavor that made him wince.",
                            "target_lang_translations": {
                                "ru": "У незрелого фрукта был резкий <w>кислый</w> вкус, от которого он поморщился."
                            }
                        },
                        {
                            "text": "This wine has a slightly <w>acid</w> aftertaste.",
                            "target_lang_translations": {
                                "ru": "У этого вина слегка <w>кислый</w> привкус."
                            }
                        }
                    ],
                    "synonyms": [
                        "acidic"
                    ],
                    "common_phrases": [
                        "acid taste"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий резкий, кислый вкус, как у уксуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кислый",
                                "target_lang_sense_clarification": "вкус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc97e7d3-535e-3cad-82a8-03a89d3ae89a",
                    "sense_definition": "Relating to a genre of electronic music characterized by repetitive, hypnotic sounds.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The club became famous for playing <w>acid</w> house music all night long.",
                            "target_lang_translations": {
                                "ru": "Клуб прославился тем, что всю ночь играл музыку <w>эйсид</w>-хаус."
                            }
                        },
                        {
                            "text": "He enjoys the distorted sounds typical of <w>acid</w> rock.",
                            "target_lang_translations": {
                                "ru": "Ему нравятся искаженные звуки, типичные для <w>эйсид</w>-рока."
                            }
                        }
                    ],
                    "common_phrases": [
                        "acid house",
                        "acid jazz",
                        "acid rock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к жанру электронной музыки, характеризующемуся повторяющимися, гипнотическими звуками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эйсид",
                                "target_lang_sense_clarification": "музыкальный жанр"
                            },
                            {
                                "target_lang_word": "кислотный",
                                "target_lang_sense_clarification": "о музыке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "037d443c-9fd5-3a4b-8eb0-6694116d0031",
                    "sense_definition": "Sharp, biting, or critical in a cruel way.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "personality_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is known for her <w>acid</w> wit and sarcastic comments.",
                            "target_lang_translations": {
                                "ru": "Она известна своим <w>едким</w> остроумием и саркастическими комментариями."
                            }
                        },
                        {
                            "text": "His <w>acid</w> remarks about the performance offended the actors.",
                            "target_lang_translations": {
                                "ru": "Его <w>язвительные</w> замечания о спектакле обидели актеров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "acid wit",
                        "acid tongue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резкий, язвительный или критичный в жестокой манере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "едкий",
                                "target_lang_sense_clarification": "о характере или словах"
                            },
                            {
                                "target_lang_word": "язвительный",
                                "target_lang_sense_clarification": "насмешливый"
                            }
                        ]
                    }
                }
            ]
        }
    ]

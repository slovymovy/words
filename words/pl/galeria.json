{
    "word_family": [
        "galerzysta",
        "galeryjny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "38e7156e-a5cf-3132-90ff-f015a6794d6d",
                    "sense_definition": "Duży budynek handlowy mieszczący wiele sklepów, restauracji i punktów usługowych pod jednym dachem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdą sobotę jeździmy do <galerii> na duże zakupy.",
                            "target_lang_translations": {
                                "ru": "Каждую субботу мы ездим в <торговый центр> за большими покупками."
                            }
                        },
                        {
                            "text": "Ta nowa <galeria> ma bardzo duży parking podziemny.",
                            "target_lang_translations": {
                                "ru": "У этого нового <торгового центра> очень большая подземная парковка."
                            }
                        }
                    ],
                    "synonyms": [
                        "centrum handlowe",
                        "galeria handlowa"
                    ],
                    "common_phrases": [
                        "iść do galerii",
                        "zakupy w galerii",
                        "nowa galeria"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common short form for 'galeria handlowa'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое торговое здание, вмещающее множество магазинов, ресторанов и пунктов обслуживания под одной крышей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговый центр",
                                "target_lang_sense_clarification": "shopping mall (most common equivalent)"
                            },
                            {
                                "target_lang_word": "галерея",
                                "target_lang_sense_clarification": "shopping arcade (often in names)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4559ee1-de53-3ee2-b7d4-35821727965b",
                    "sense_definition": "Instytucja lub pomieszczenie przeznaczone do wystawiania, a często także sprzedaży dzieł sztuki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody malarz wystawił swoje obrazy w prestiżowej <galerii>.",
                            "target_lang_translations": {
                                "ru": "Молодой художник выставил свои картины в престижной <галерее>."
                            }
                        },
                        {
                            "text": "Wczoraj odwiedziliśmy miejską <galerię> sztuki współczesnej.",
                            "target_lang_translations": {
                                "ru": "Вчера мы посетили городскую <галерею> современного искусства."
                            }
                        }
                    ],
                    "synonyms": [
                        "paradyz"
                    ],
                    "common_phrases": [
                        "galeria sztuki",
                        "galeria obrazów",
                        "wernisaż w galerii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение или помещение, предназначенное для выставки, а часто и продажи произведений искусства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галерея",
                                "target_lang_sense_clarification": "art exhibition space"
                            },
                            {
                                "target_lang_word": "художественная галерея",
                                "target_lang_sense_clarification": "art gallery"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f9ff504-3f75-38af-a783-77c825c1e149",
                    "sense_definition": "Zbiór zdjęć, przedmiotów lub postaci prezentowany w uporządkowany sposób, często w internecie lub publikacji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na stronie internetowej dostępna jest <galeria> zdjęć z wakacji.",
                            "target_lang_translations": {
                                "ru": "На сайте доступна <галерея> фотографий с отпуска."
                            }
                        },
                        {
                            "text": "Aktor dołączył do <galerii> sław polskiego kina.",
                            "target_lang_translations": {
                                "ru": "Актёр вошёл в <галерею> славы польского кино."
                            }
                        }
                    ],
                    "synonyms": [
                        "paradyz",
                        "raik"
                    ],
                    "common_phrases": [
                        "galeria zdjęć",
                        "galeria sław",
                        "przeglądać galerię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подборка фотографий, предметов или персонажей, представленная упорядоченным образом, часто в интернете или публикации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галерея",
                                "target_lang_sense_clarification": "collection of images/photos"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed8d7f73-2a5d-3565-bbd4-2552f24f521d",
                    "sense_definition": "Długi balkon lub przejście wewnątrz albo na zewnątrz budynku, często łączące jego części.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "arch_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z górnej <galerii> roztacza się piękny widok na dziedziniec.",
                            "target_lang_translations": {
                                "ru": "С верхней <галереи> открывается прекрасный вид на внутренний двор."
                            }
                        },
                        {
                            "text": "Przeszliśmy długą <galerią> do wschodniego skrzydła zamku.",
                            "target_lang_translations": {
                                "ru": "Мы прошли по длинной <галерее> в восточное крыло замка."
                            }
                        }
                    ],
                    "synonyms": [
                        "paradyz",
                        "raik"
                    ],
                    "common_phrases": [
                        "galeria widokowa",
                        "zewnętrzna galeria"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный балкон или проход внутри или снаружи здания, часто соединяющий его части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галерея",
                                "target_lang_sense_clarification": "architectural element"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43255be7-7a91-362e-bd48-075f6dd20daf",
                    "sense_definition": "Najwyższe piętro widowni w teatrze lub sali koncertowej; także publiczność tam siedząca.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "arch_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Studenci kupili tanie bilety na <galerię>, by obejrzeć spektakl.",
                            "target_lang_translations": {
                                "ru": "Студенты купили дешёвые билеты на <галёрку>, чтобы посмотреть спектакль."
                            }
                        },
                        {
                            "text": "Z <galerii> widok na scenę był nieco ograniczony przez filary.",
                            "target_lang_translations": {
                                "ru": "С <галёрки> вид на сцену был немного ограничен колоннами."
                            }
                        }
                    ],
                    "synonyms": [
                        "jaskółka",
                        "paradyz",
                        "raik"
                    ],
                    "common_phrases": [
                        "bilet na galerię",
                        "siedzieć na galerii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый верхний ярус зрительного зала в театре или концертном зале; также публика, сидящая там."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галёрка",
                                "target_lang_sense_clarification": "upper tier (colloquial)"
                            },
                            {
                                "target_lang_word": "галерея",
                                "target_lang_sense_clarification": "upper tier (formal/archaic)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

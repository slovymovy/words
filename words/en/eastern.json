    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "65ea8663-b352-33d5-b6d8-d74534ea0675",
                    "sense_definition": "Of, facing, situated in, or related to the east.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sun rises in the <w>eastern</w> sky every morning.",
                            "target_lang_translations": {
                                "ru": "Каждое утро солнце встает в <w>восточной</w> части неба."
                            }
                        },
                        {
                            "text": "They decided to move to the <w>eastern</w> part of the country for work.",
                            "target_lang_translations": {
                                "ru": "Они решили переехать в <w>восточную</w> часть страны ради работы."
                            }
                        }
                    ],
                    "antonyms": [
                        "western"
                    ],
                    "common_phrases": [
                        "eastern Europe",
                        "eastern coast",
                        "eastern border",
                        "eastern side"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к востоку, расположенный на востоке или обращенный к нему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восточный",
                                "target_lang_sense_clarification": "географический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b59d2eb8-d36d-3c53-be3c-4013ffc4d4be",
                    "sense_definition": "Relating to the regions, countries, or cultures of the East, often Asia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a strong interest in <w>Eastern</w> philosophy and art.",
                            "target_lang_translations": {
                                "ru": "Она живо интересуется <w>восточной</w> философией и искусством."
                            }
                        },
                        {
                            "text": "Many <w>Eastern</w> traditions emphasize the importance of family and community.",
                            "target_lang_translations": {
                                "ru": "Многие <w>восточные</w> традиции подчеркивают важность семьи и общины."
                            }
                        }
                    ],
                    "antonyms": [
                        "Western"
                    ],
                    "common_phrases": [
                        "Eastern culture",
                        "Middle Eastern",
                        "Eastern philosophy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к странам, регионам или культуре Востока (часто Азии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восточный",
                                "target_lang_sense_clarification": "культурный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9fb7e6f-2c4f-3f2a-a067-f314c8945a0d",
                    "sense_definition": "Blowing from the east.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A cold <w>eastern</w> wind made everyone shiver as they waited outside.",
                            "target_lang_translations": {
                                "ru": "Холодный <w>восточный</w> ветер заставил всех дрожать, пока они ждали на улице."
                            }
                        },
                        {
                            "text": "The sailors were glad when the strong <w>eastern</w> breeze finally died down.",
                            "target_lang_translations": {
                                "ru": "Моряки обрадовались, когда сильный <w>восточный</w> ветер наконец стих."
                            }
                        }
                    ],
                    "synonyms": [
                        "eoan",
                        "eoan"
                    ],
                    "antonyms": [
                        "western",
                        "westerly"
                    ],
                    "common_phrases": [
                        "eastern wind",
                        "eastern breeze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дующий с востока (о ветре)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восточный",
                                "target_lang_sense_clarification": "о ветре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67bff02a-62a3-32e6-8c8c-340fa63e91f5",
                    "sense_definition": "Relating to the Christian churches that originated in the Eastern Roman Empire.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Eastern</w> Orthodox Church celebrates Easter on a different date than Western churches.",
                            "target_lang_translations": {
                                "ru": "<w>Восточная</w> православная церковь отмечает Пасху в другую дату, нежели западные церкви."
                            }
                        },
                        {
                            "text": "He studies the history and theology of <w>Eastern</w> Christianity.",
                            "target_lang_translations": {
                                "ru": "Он изучает историю и богословие <w>восточного</w> христианства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Eastern Orthodox",
                        "Eastern Church"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к христианским церквям, возникшим в Восточной Римской империи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восточный",
                                "target_lang_sense_clarification": "религиозный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
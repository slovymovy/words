    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2201374d-eca6-3662-be56-664f1d915d35",
                    "sense_definition": "A large area of land that usually has specific characteristics, such as climate or culture, but does not have fixed boundaries.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geographical_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many rare animals can only be found in the tropical <w>regions</w> of the world.",
                            "target_lang_translations": {
                                "ru": "Многие редкие животные встречаются только в тропических <w>регионах</w> мира."
                            }
                        },
                        {
                            "text": "The northern <w>region</w> of the country is famous for its cold winters and heavy snow.",
                            "target_lang_translations": {
                                "ru": "Северный <w>регион</w> страны славится своими холодными зимами и сильными снегопадами."
                            }
                        }
                    ],
                    "synonyms": [
                        "area",
                        "zone",
                        "territory",
                        "district",
                        "part"
                    ],
                    "common_phrases": [
                        "polar region",
                        "tropical region",
                        "coastal region",
                        "mountainous region",
                        "border region"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая территория суши, которая обычно имеет специфические характеристики, такие как климат или культура, но не имеет четких границ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регион",
                                "target_lang_sense_clarification": "обширная территория с общими признаками"
                            },
                            {
                                "target_lang_word": "область",
                                "target_lang_sense_clarification": "территория, зона"
                            },
                            {
                                "target_lang_word": "район",
                                "target_lang_sense_clarification": "часть территории, местность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75967d00-03ee-3a59-ae2f-a024a1c80547",
                    "sense_definition": "A specific administrative or political division within a country or territory.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "administrative_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government promised to send more aid to the poorest <w>region</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство пообещало направить больше помощи в самый бедный <w>регион</w>."
                            }
                        },
                        {
                            "text": "Voters in this <w>region</w> have elected a new representative to the council.",
                            "target_lang_translations": {
                                "ru": "Избиратели в этом <w>регионе</w> выбрали нового представителя в совет."
                            }
                        }
                    ],
                    "synonyms": [
                        "province",
                        "state",
                        "county",
                        "sector"
                    ],
                    "common_phrases": [
                        "autonomous region",
                        "administrative region"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная административная или политическая единица внутри страны или территории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регион",
                                "target_lang_sense_clarification": "административная единица"
                            },
                            {
                                "target_lang_word": "область",
                                "target_lang_sense_clarification": "административно-территориальная единица"
                            },
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "крупная административная единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14c87595-8789-3eb7-ba22-651a2548eb17",
                    "sense_definition": "An approximate amount, cost, or range.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "approximation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The repairs to the house will cost somewhere in the <w>region</w> of five thousand dollars.",
                            "target_lang_translations": {
                                "ru": "Ремонт дома обойдется где-то в <w>районе</w> пяти тысяч долларов."
                            }
                        },
                        {
                            "text": "We are expecting an audience in the <w>region</w> of three hundred people for the event.",
                            "target_lang_translations": {
                                "ru": "Мы ожидаем аудиторию в <w>районе</w> трехсот человек на этом мероприятии."
                            }
                        }
                    ],
                    "synonyms": [
                        "range",
                        "neighborhood"
                    ],
                    "common_phrases": [
                        "in the region of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приблизительное количество, стоимость или диапазон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "район",
                                "target_lang_sense_clarification": "в значении «около», «приблизительно»"
                            },
                            {
                                "target_lang_word": "пределы",
                                "target_lang_sense_clarification": "диапазон значений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c40de60a-6f70-3e61-89fd-fb41c91291a2",
                    "sense_definition": "A specific part or area of the body.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient complained of a sharp pain in the abdominal <w>region</w>.",
                            "target_lang_translations": {
                                "ru": "Пациент жаловался на острую боль в <w>области</w> живота."
                            }
                        },
                        {
                            "text": "Doctors examined the lumbar <w>region</w> carefully to check for any spinal injuries.",
                            "target_lang_translations": {
                                "ru": "Врачи тщательно осмотрели поясничную <w>область</w>, чтобы проверить наличие травм позвоночника."
                            }
                        }
                    ],
                    "synonyms": [
                        "area"
                    ],
                    "common_phrases": [
                        "abdominal region",
                        "lumbar region"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенная часть или зона тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "область",
                                "target_lang_sense_clarification": "анатомическая зона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "627721cb-1661-3623-b8b9-6a7b01be87b9",
                    "sense_definition": "A particular rank, station, or social standing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt that the king was of too high a <w>region</w> to be approached by a commoner.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, что король принадлежит к слишком высокой <w>сфере</w>, чтобы к нему мог обратиться простолюдин."
                            }
                        },
                        {
                            "text": "Her intellect soared into a <w>region</w> far above her peers.",
                            "target_lang_translations": {
                                "ru": "Её интеллект парил в <w>сферах</w>, недосягаемых для её сверстников."
                            }
                        }
                    ],
                    "synonyms": [
                        "rank",
                        "station",
                        "sphere"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Особый ранг, положение или социальный статус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сфера",
                                "target_lang_sense_clarification": "область деятельности или бытия"
                            },
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "социальная среда"
                            }
                        ]
                    }
                }
            ]
        }
    ]

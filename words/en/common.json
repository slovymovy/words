                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "land",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children played football on the village <w>common</w>.",
                            "target_lang_translations": {
                                "ru": "Дети играли в футбол на деревенской <w>общинной земле</w>."
                            }
                        },
                        {
                            "text": "We went for a long walk across the <w>common</w> on Sunday.",
                            "target_lang_translations": {
                                "ru": "В воскресенье мы отправились на долгую прогулку по <w>пустырю</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "village common",
                        "on the common"
                    ],
                    "target_lang_definitions": {

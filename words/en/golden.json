    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "710eb30e-fdbd-3439-a900-61ffe0c7718f",
                    "sense_definition": "Having a color or other richness suggestive of gold.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fields turned <w>golden</w> in the late afternoon sun.",
                            "target_lang_translations": {
                                "ru": "Поля стали <w>золотистыми</w> в лучах предзакатного солнца."
                            }
                        },
                        {
                            "text": "She has beautiful <w>golden</w> hair that shines in the light.",
                            "target_lang_translations": {
                                "ru": "У неё красивые <w>золотые</w> волосы, которые сияют на свету."
                            }
                        }
                    ],
                    "synonyms": [
                        "goldish § Synonyms"
                    ],
                    "common_phrases": [
                        "golden hair",
                        "golden sun",
                        "golden sand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий цвет или блеск, напоминающий золото."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "золоти́стый",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "золото́й",
                                "target_lang_sense_clarification": "цвет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9535c539-7252-31f1-acd1-7a841b2dbef2",
                    "sense_definition": "Made of, or relating to, gold.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king wore a heavy <w>golden</w> crown on his head.",
                            "target_lang_translations": {
                                "ru": "Король носил тяжелую <w>золотую</w> корону на голове."
                            }
                        },
                        {
                            "text": "He found a small <w>golden</w> ring buried in the garden.",
                            "target_lang_translations": {
                                "ru": "Он нашел маленькое <w>золотое</w> кольцо, зарытое в саду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "golden crown",
                        "golden ring",
                        "golden coin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный из золота или относящийся к нему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "золото́й",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b626a86-0311-3e2d-b4e9-33886158fd53",
                    "sense_definition": "Marked by prosperity, creativity, or excellence.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Renaissance is often considered a <w>golden</w> era for art and science.",
                            "target_lang_translations": {
                                "ru": "Эпоху Возрождения часто считают <w>золотой</w> эрой искусства и науки."
                            }
                        },
                        {
                            "text": "They looked back on their college days as a <w>golden</w> time.",
                            "target_lang_translations": {
                                "ru": "Они вспоминали свои студенческие годы как <w>золотое</w> время."
                            }
                        }
                    ],
                    "common_phrases": [
                        "golden age",
                        "golden era",
                        "golden days"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отмеченный процветанием, расцветом или совершенством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "золото́й",
                                "target_lang_sense_clarification": "метафорически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fabcbe8a-fd19-3d45-9b19-50945e4af02e",
                    "sense_definition": "Advantageous or very favourable.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This job offer is a <w>golden</w> opportunity for her career.",
                            "target_lang_translations": {
                                "ru": "Это предложение о работе — <w>блестящая</w> возможность для её карьеры."
                            }
                        },
                        {
                            "text": "He missed a <w>golden</w> chance to score a goal during the match.",
                            "target_lang_translations": {
                                "ru": "Он упустил <w>прекрасный</w> шанс забить гол во время матча."
                            }
                        }
                    ],
                    "common_phrases": [
                        "golden opportunity",
                        "golden rule",
                        "golden ticket"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выгодный, очень благоприятный или исключительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блестя́щий",
                                "target_lang_sense_clarification": "о возможности"
                            },
                            {
                                "target_lang_word": "золото́й",
                                "target_lang_sense_clarification": "в устойчивых выражениях"
                            },
                            {
                                "target_lang_word": "прекра́сный",
                                "target_lang_sense_clarification": "о шансе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53a1946b-193d-3c8c-908e-b2c6055f251e",
                    "sense_definition": "Relating to a fiftieth anniversary.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anniversary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandparents celebrated their <w>golden</w> wedding anniversary with the whole family.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка отпраздновали свою <w>золотую</w> свадьбу всей семьей."
                            }

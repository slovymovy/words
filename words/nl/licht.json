                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лёгкий",
                                "target_lang_sense_clarification": "нетрудный"
                            },
                            {
                                "target_lang_word": "несложный",
                                "target_lang_sense_clarification": "простой"
                            }
                        ]

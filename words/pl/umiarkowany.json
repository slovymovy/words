            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "913bca18-db21-385d-b240-7fffce9c2b76",
                    "sense_definition": "Taki, który nie jest ani zbyt duży, ani zbyt mały; średni pod względem nasilenia, wielkości lub ceny",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intensity_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Polsce występuje klimat <w>umiarkowany</w>, co oznacza ciepłe lata i niezbyt mroźne zimy.",
                            "target_lang_translations": {
                                "ru": "В Польше <w>умеренный</w> климат, что означает теплое лето и не слишком морозные зимы."
                            }
                        },
                        {
                            "text": "Lekarz zalecił pacjentowi jedynie <w>umiarkowany</w> wysiłek fizyczny po operacji.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал пациенту лишь <w>умеренную</w> физическую нагрузку после операции."
                            }
                        }
                    ],
                    "synonyms": [
                        "średni",
                        "przeciętny",
                        "znośny",
                        "łagodny"
                    ],
                    "antonyms": [
                        "skrajny",
                        "ekstremalny",
                        "gwałtowny",
                        "nadmierny",
                        "silny"
                    ],
                    "common_phrases": [
                        "klimat umiarkowany",
                        "umiarkowany optymizm",
                        "w stopniu umiarkowanym",
                        "umiarkowany ból"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не слишком большой и не слишком маленький; средний по силе, величине или цене"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "умеренный",
                                "target_lang_sense_clarification": "о климате, цене, размере"
                            },
                            {
                                "target_lang_word": "средний",
                                "target_lang_sense_clarification": "промежуточный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f86e6162-6b53-3c7b-a175-f38785bd41f8",
                    "sense_definition": "Taki, który potrafi zachować umiar w jedzeniu, piciu lub zachowaniu; powściągliwy i opanowany",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek był człowiekiem <w>umiarkowanym</w> i nigdy nie wpadał w gniew bez powodu.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был <w>сдержанным</w> человеком и никогда не впадал в гнев без причины."
                            }
                        },
                        {
                            "text": "Staram się być <w>umiarkowany</w> w jedzeniu słodyczy, aby dbać o zdrowie.",
                            "target_lang_translations": {
                                "ru": "Я стараюсь быть <w>умеренным</w> в употреблении сладостей, чтобы заботиться о здоровье."
                            }
                        }
                    ],
                    "synonyms": [
                        "powściągliwy",
                        "wstrzemięźliwy",
                        "opanowany",
                        "rozważny"
                    ],
                    "antonyms": [

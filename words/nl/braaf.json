    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "60bc77d5-4307-3584-8695-8d7f984d1069",
                    "sense_definition": "Bereid om de geldende regels in acht te nemen, gehoorzaam.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "obedient",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was niet altijd de <w>braafste</w> van de klas.",
                            "target_lang_translations": {
                                "en": "He wasn't always the <w>best behaved</w> in the class.",
                                "ru": "Он не всегда был самым <w>послушным</w> в классе."
                            }
                        },
                        {
                            "text": "Meisjes zijn vaak <w>braver</w> dan de ondeugende jongens.",
                            "target_lang_translations": {
                                "en": "Girls are often <w>better behaved</w> than the naughty boys.",
                                "ru": "Девочки часто <w>послушнее</w>, чем озорные мальчики."
                            }
                        }
                    ],
                    "synonyms": [
                        "gehoorzaam"
                    ],
                    "antonyms": [
                        "ondeugend",
                        "stout"
                    ],
                    "common_phrases": [
                        "braaf zijn",
                        "een brave hendrik",
                        "braaf meisje"
                    ],
                    "target_lang_definitions": {
                        "en": "Willing to observe applicable rules, obedient.",
                        "ru": "Готовый соблюдать правила, послушный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "well-behaved",
                                "target_lang_sense_clarification": "general conduct"
                            },
                            {
                                "target_lang_word": "good",
                                "target_lang_sense_clarification": "especially of children or pets"
                            },
                            {
                                "target_lang_word": "obedient",
                                "target_lang_sense_clarification": "strictly following orders"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "послушный",
                                "target_lang_sense_clarification": "подчиняющийся правилам (о детях, животных)"
                            },
                            {
                                "target_lang_word": "примерный",
                                "target_lang_sense_clarification": "образцовый (о поведении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93a301df-00ef-33d3-995b-f2c65bf611d8",
                    "sense_definition": "Eerlijk, fatsoenlijk en deugdzaam van karakter.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "virtuous",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurman is een <w>brave</w> man die niemand kwaad doet.",
                            "target_lang_translations": {
                                "en": "My neighbor is a <w>decent</w> man who wouldn't hurt a fly.",
                                "ru": "Мой сосед — <w>порядочный</w> человек, который никого не обидит."
                            }
                        },
                        {
                            "text": "Ze leidden een <w>braaf</w> en rustig leven in het dorp.",
                            "target_lang_translations": {
                                "en": "They led a <w>virtuous</w> and quiet life in the village.",
                                "ru": "Они вели <w>добропорядочную</w> и спокойную жизнь в деревне."
                            }
                        }
                    ],
                    "synonyms": [
                        "eerzaam",
                        "deugdzaam"
                    ],
                    "common_phrases": [
                        "een brave burger",
                        "een brave huisvader"
                    ],
                    "target_lang_definitions": {

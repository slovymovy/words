                        {
                            "text": "Не стоит <завязать> в мелких деталях, нужно видеть суть.",
                            "target_lang_translations": {
                                "en": "One shouldn't <get stuck> in small details; one needs to see the essence."
                            }
                        }
                    ],
                    "synonyms": [
                        "застревать",
                        "задерживаться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To remain in an unpleasant situation or state for a long time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get bogged down",
                                "target_lang_sense_clarification": "metaphorical; overwhelmed by details"
                            },
                            {
                                "target_lang_word": "get stuck",
                                "target_lang_sense_clarification": "unable to progress"
                            }
                        ]
                    }
                }

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e534e0a-7c3b-3bda-a503-dd160f39e0a0",
                    "sense_definition": "Zbiór elementów, z których zbudowana jest jakaś całość, substancja lub przedmiot; to, co wchodzi w zawartość czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "composition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed zakupem produktu spożywczego warto dokładnie przeczytać jego <w>skład</w>.",
                            "target_lang_translations": {
                                "ru": "Перед покупкой продукта питания стоит внимательно прочитать его <w>состав</w>."
                            }
                        },
                        {
                            "text": "W <w>skład</w> tej komisji wchodzą wybitni eksperci z całego kraju.",
                            "target_lang_translations": {
                                "ru": "В <w>состав</w> этой комиссии входят выдающиеся эксперты со всей страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "układ",
                        "zob. sklep",
                        "magazyn",
                        "okoliczność",
                        "depozyt",
                        "załamka",
                        "umowa",
                        "szacunek",
                        "składowisko",
                        "zespół",
                        "drużyna"
                    ],
                    "common_phrases": [
                        "skład chemiczny",
                        "wchodzić w skład",
                        "skład produktu",
                        "prosty skład"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность элементов, из которых состоит целое, вещество или предмет; содержимое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состав",
                                "target_lang_sense_clarification": "ингредиенты, структура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c157cfb-8f83-331d-8cc8-a81d9fa5c947",
                    "sense_definition": "Grupa osób tworzących zespół, organizację lub biorących udział w jakimś wydarzeniu, na przykład w meczu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trener ogłosił podstawowy <w>skład</w> reprezentacji na najbliższe mistrzostwa.",
                            "target_lang_translations": {
                                "ru": "Тренер объявил основной <w>состав</w> сборной на ближайший чемпионат."
                            }
                        },
                        {
                            "text": "Spotkaliśmy się w pełnym <w>składzie</w>, aby omówić plany na przyszły rok.",
                            "target_lang_translations": {
                                "ru": "Мы встретились в полном <w>составе</w>, чтобы обсудить планы на будущий год."
                            }
                        }
                    ],
                    "synonyms": [
                        "zespół",
                        "drużyna"
                    ],
                    "common_phrases": [
                        "skład sędziowski",
                        "w pełnym składzie",
                        "skład rządu",
                        "podstawowy skład"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, составляющих коллектив, организацию или участвующих в каком-либо событии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состав",
                                "target_lang_sense_clarification": "команда, коллектив"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a4977c6-b2dd-38f9-8093-b00a62938d95",
                    "sense_definition": "Budynek lub miejsce przeznaczone do przechowywania dużej ilości towarów, materiałów lub surowców.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "storage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad prowadzi duży <w>skład</w> materiałów budowlanych na obrzeżach miasta.",
                            "target_lang_translations": {
                                "ru": "Мой сосед держит большой <w>склад</w> строительных материалов на окраине города."
                            }
                        },
                        {
                            "text": "Niestety nie mamy obecnie tego modelu lodówki na <w>składzie</w>.",
                            "target_lang_translations": {
                                "ru": "К сожалению, у нас сейчас нет этой модели холодильника на <w>складе</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "magazyn",
                        "składowisko"
                    ],
                    "common_phrases": [
                        "skład budowlany",
                        "skład opału",
                        "na składzie",
                        "skład amunicji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание или место, предназначенное для хранения большого количества товаров или материалов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "склад",
                                "target_lang_sense_clarification": "хранилище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b753c4b6-40c4-395d-87a7-21e55e4a4cbc",
                    "sense_definition": "Zestaw połączonych ze sobą wagonów kolejowych, zazwyczaj ciągniętych przez lokomotywę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Długi <w>skład</w> towarowy zablokował przejazd kolejowy na ponad dziesięć minut.",
                            "target_lang_translations": {
                                "ru": "Длинный товарный <w>состав</w> заблокировал железнодорожный переезд более чем на десять минут."
                            }
                        },
                        {
                            "text": "Na peron trzeci wjedzie opóźniony <w>skład</w> pospieszny z Krakowa.",
                            "target_lang_translations": {
                                "ru": "На третью платформу прибудет опаздывающий скорый <w>состав</w> из Кракова."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skład pociągu",
                        "skład towarowy",
                        "skład osobowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа сцепленных железнодорожных вагонов, обычно с локомотивом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состав",
                                "target_lang_sense_clarification": "поезд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7662c0ae-c0a1-37d3-9571-f7a1d0f98425",
                    "sense_definition": "Proces przygotowywania tekstu do druku, polegający na odpowiednim rozmieszczeniu czcionek i grafik na stronie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "printing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Grafik komputerowy odpowiada za profesjonalny <w>skład</w> naszej nowej książki.",
                            "target_lang_translations": {
                                "ru": "Компьютерный график отвечает за профессиональную <w>вёрстку</w> нашей новой книги."
                            }
                        },
                        {
                            "text": "Wydawnictwo zleciło <w>skład</w> tekstu zewnętrznej firmie poligraficznej.",
                            "target_lang_translations": {
                                "ru": "Издательство поручило <w>набор</w> текста внешней полиграфической фирме."
                            }
                        }
                    ],
                    "common_phrases": [

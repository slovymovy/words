{
    "word_family": [
        "wantowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "693d1140-f0e0-3725-a27c-1163a7979afa",
                    "sense_definition": "Lina olinowania stałego, która podtrzymuje i usztywnia maszt w płaszczyźnie poprzecznej (na boki).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas silnego wiatru pękła jedna <w>wanta</w>, co zagroziło stabilności całego masztu.",
                            "target_lang_translations": {
                                "ru": "Во время сильного ветра лопнула одна <w>ванта</w>, что поставило под угрозу устойчивость всей мачты."
                            }
                        },
                        {
                            "text": "Kapitan nakazał załodze naciągnąć poluzowane <w>wanty</w> przed wypłynięciem w morze.",
                            "target_lang_translations": {
                                "ru": "Капитан приказал команде натянуть ослабленные <w>ванты</w> перед выходом в море."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naciąg want",
                        "pęknięta wanta",
                        "mocowanie wanty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снасть стоячего такелажа, которая поддерживает и укрепляет мачту с боков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ванта",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4f9d3f9-47ae-3d76-b18a-fdc378ef0434",
                    "sense_definition": "Gruba lina stalowa łącząca pylon z przęsłem w konstrukcji mostu podwieszanego.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "construction_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most Świętokrzyski w Warszawie posiada charakterystyczne stalowe <w>wanty</w>, które nadają mu lekkości.",
                            "target_lang_translations": {
                                "ru": "Свентокшиский мост в Варшаве имеет характерные стальные <w>ванты</w>, которые придают ему легкость."
                            }
                        },
                        {
                            "text": "Inżynierowie muszą regularnie kontrolować każdą <w>wantę</w>, aby zapewnić bezpieczeństwo przeprawy.",
                            "target_lang_translations": {
                                "ru": "Инженеры должны регулярно проверять каждую <w>ванту</w>, чтобы обеспечить безопасность переправы."
                            }
                        }
                    ],
                    "synonyms": [
                        "odciąg"
                    ],
                    "common_phrases": [
                        "most wantowy",
                        "stalowa wanta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Толстый стальной трос, соединяющий пилон с пролетом в конструкции вантового моста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ванта",
                                "target_lang_sense_clarification": "элемент моста"
                            },
                            {
                                "target_lang_word": "оттяжка",
                                "target_lang_sense_clarification": "строительный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c46d346-d4a2-3bfb-87ec-2f198b64fe07",
                    "sense_definition": "Wielki, wolno stojący głaz lub blok skalny (określenie używane głównie w Tatrach).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rock_formation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczeni turyści usiedli pod wielką <w>wantą</w>, aby schronić się przed deszczem.",
                            "target_lang_translations": {
                                "ru": "Уставшие туристы сели под большим <w>валуном</w>, чтобы укрыться от дождя."
                            }
                        },
                        {
                            "text": "Szlak prowadził stromym zboczem pomiędzy ogromnymi, granitowymi <w>wantami</w>.",
                            "target_lang_translations": {
                                "ru": "Тропа вела по крутому склону между огромными гранитными <w>глыбами</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "głaz",
                        "blok skalny"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in Podhale/Tatra mountains dialect"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой отдельно стоящий валун или скальный блок (термин, используемый в Татрах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "валун",
                                "target_lang_sense_clarification": "большой камень"
                            },
                            {
                                "target_lang_word": "глыба",
                                "target_lang_sense_clarification": "скальный обломок"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "5117b938-66ea-3480-91f7-d908cee5977f",
                    "sense_definition": "Niet netjes, geordend of verzorgd (van uiterlijk of omgeving).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn slaapkamer is altijd erg <w>slordig</w> met kleren op de grond.",
                            "target_lang_translations": {
                                "en": "His bedroom is always very <w>messy</w> with clothes on the floor.",
                                "ru": "В его спальне всегда очень <w>неряшливо</w>, а на полу валяется одежда."
                            }
                        },
                        {
                            "text": "Je kunt niet met zulke <w>slordige</w> kleding naar het feest gaan.",
                            "target_lang_translations": {
                                "en": "You cannot go to the party in such <w>sloppy</w> clothes.",
                                "ru": "Ты не можешь пойти на вечеринку в такой <w>неопрятной</w> одежде."
                            }
                        }
                    ],
                    "synonyms": [
                        "rommelig",
                        "ordeloos",
                        "wanordelijk"
                    ],
                    "antonyms": [
                        "netjes",
                        "verzorgd"
                    ],
                    "common_phrases": [
                        "er slordig uitzien",
                        "een slordige kamer",
                        "slordige kleding"
                    ],
                    "target_lang_definitions": {
                        "en": "Not neat, orderly, or well-groomed (of appearance or surroundings).",
                        "ru": "Неаккуратный, неопрятный или беспорядочный (о внешнем виде или обстановке)."
                    },

                    "frequency": "Middle",
                    "semantic_group_id": "sphere_of_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king ruled his vast <w>domain</w> with absolute power and authority.",
                            "target_lang_translations": {

{
    "word_family": [
        "construction",
        "constructive",
        "constructor",
        "reconstruct",
        "deconstruct",
        "constructional"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "29fed28b-88d7-309c-915a-010ba96347c2",
                    "sense_definition": "To build or form something by assembling parts.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "build_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>constructed</w> the radio from spares found in the garage.",
                            "target_lang_translations": {
                                "ru": "Мы <w>собрали</w> радиоприемник из запчастей, найденных в гараже."
                            }
                        },
                        {
                            "text": "The city plans to <w>construct</w> a new bridge over the river next year.",
                            "target_lang_translations": {
                                "ru": "В следующем году город планирует <w>построить</w> новый мост через реку."
                            }
                        }
                    ],
                    "synonyms": [
                        "build",
                        "assemble",
                        "put together",
                        "make",
                        "form",
                        "produce"
                    ],
                    "antonyms": [
                        "destroy",
                        "dismantle",
                        "take apart",
                        "disassemble",
                        "ruin",
                        "wreck"
                    ],
                    "common_phrases": [
                        "construct a building",
                        "construct a bridge",
                        "construct a road"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строить или создавать что-либо, собирая части воедино."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строить",
                                "target_lang_sense_clarification": "сооружать здания или конструкции"
                            },
                            {
                                "target_lang_word": "конструировать",
                                "target_lang_sense_clarification": "создавать технические устройства, проектировать"
                            },
                            {
                                "target_lang_word": "сооружать",
                                "target_lang_sense_clarification": "возводить постройки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f7d1a3b-8dbc-31f8-a868-b1f0c13595a3",
                    "sense_definition": "To create a complex idea, argument, or sentence by arranging words or concepts.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "build_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A sentence may be <w>constructed</w> with a subject, verb, and object.",
                            "target_lang_translations": {
                                "ru": "Предложение может быть <w>составлено</w> из подлежащего, сказуемого и дополнения."
                            }
                        },
                        {
                            "text": "He carefully <w>constructed</w> a logical argument to persuade the jury.",
                            "target_lang_translations": {
                                "ru": "Он тщательно <w>выстроил</w> логический аргумент, чтобы убедить присяжных."
                            }
                        }
                    ],
                    "synonyms": [
                        "form"
                    ],
                    "common_phrases": [
                        "construct an argument",
                        "construct a sentence",
                        "construct a theory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать сложную идею, аргумент или предложение, организуя слова или понятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строить",
                                "target_lang_sense_clarification": "создавать в уме планы, теории, аргументы"
                            },
                            {
                                "target_lang_word": "составлять",
                                "target_lang_sense_clarification": "образовывать целое из частей (например, предложение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdd142fc-a42c-3b62-aeec-a997428579a4",
                    "sense_definition": "To draw a geometric figure by following precise specifications and using tools.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>construct</w> a circle that touches each vertex of the given triangle.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>постройте</w> окружность, касающуюся каждой вершины данного треугольника."
                            }
                        },
                        {
                            "text": "Students learned how to <w>construct</w> a perpendicular line using a compass.",
                            "target_lang_translations": {
                                "ru": "Ученики учились <w>строить</w> перпендикулярную линию с помощью циркуля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "construct a triangle",
                        "construct a circle",
                        "construct a line"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чертить геометрическую фигуру по точным спецификациям с использованием инструментов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строить",
                                "target_lang_sense_clarification": "вычерчивать геометрические фигуры"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "43af2d9f-61a4-3c59-bcc9-d55ea340348f",
                    "sense_definition": "A complex idea, model, or theory formed in the mind.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Time is often described as a human <w>construct</w> rather than a physical reality.",
                            "target_lang_translations": {
                                "ru": "Время часто описывают как человеческий <w>конструкт</w>, а не физическую реальность."
                            }
                        },
                        {
                            "text": "The theoretical <w>construct</w> of the atom has evolved significantly over time.",
                            "target_lang_translations": {
                                "ru": "Теоретическая <w>модель</w> атома значительно эволюционировала со временем."
                            }
                        }
                    ],
                    "synonyms": [
                        "concept",
                        "model",
                        "idea",
                        "notion",
                        "representation"
                    ],
                    "common_phrases": [
                        "social construct",
                        "theoretical construct",
                        "mental construct"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сложная идея, модель или теория, сформированная в уме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "концепция",
                                "target_lang_sense_clarification": "система взглядов, теоретическое построение"
                            },
                            {
                                "target_lang_word": "конструкт",
                                "target_lang_sense_clarification": "субъективное понятие, порождение сознания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0f71c12-9680-3cac-8a0f-8c16530771a8",
                    "sense_definition": "Something complex that is built or formed from different parts.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The modern artwork was a strange <w>construct</w> of wire and plastic tubes.",
                            "target_lang_translations": {
                                "ru": "Это современное произведение искусства представляло собой странную <w>конструкцию</w> из проволоки и пластиковых трубок."
                            }
                        },
                        {
                            "text": "Loops and conditional statements are essential constructs in computer programming.",
                            "target_lang_translations": {
                                "ru": "Циклы и условные операторы — важные <w>конструкции</w> в программировании."
                            }
                        }
                    ],
                    "synonyms": [
                        "something constructed from parts"
                    ],
                    "common_phrases": [
                        "complex construct"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сложный объект, построенный или сформированный из различных частей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конструкция",
                                "target_lang_sense_clarification": "устройство, сооружение или структура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "520135ff-5136-335d-a4c9-9b0bd52de686",
                    "sense_definition": "A segment of nucleic acid created artificially for transplantation into a cell.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientists injected the genetic <w>construct</w> into the target tissue.",
                            "target_lang_translations": {
                                "ru": "Ученые ввели генетическую <w>конструкцию</w> в целевую ткань."
                            }
                        },
                        {
                            "text": "This DNA <w>construct</w> was designed to express a specific protein.",
                            "target_lang_translations": {
                                "ru": "Эта ДНК-<w>конструкция</w> была создана для экспрессии определенного белка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "genetic construct",
                        "DNA construct"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственно созданный сегмент нуклеиновой кислоты для введения в клетку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конструкция",
                                "target_lang_sense_clarification": "генетическая структура"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
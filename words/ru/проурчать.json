{
    "word_family": [
        "урчать",
        "урчание",
        "заурчать",
        "поурчать"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "258b4be3-f9a0-33cc-ae6e-bd72071e6c19",
                    "sense_definition": "Издать характерный низкий, вибрирующий звук (обычно о животных, животе или механизмах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кот ласково <w>проурчал</w>, когда я почесал его за ухом.",
                            "target_lang_translations": {
                                "en": "The cat <w>purred</w> affectionately when I scratched him behind the ear."
                            }
                        },
                        {
                            "text": "От сильного голода у него в животе громко <w>проурчало</w>.",
                            "target_lang_translations": {
                                "en": "His stomach <w>rumbled</w> loudly from severe hunger."
                            }
                        }
                    ],
                    "synonyms": [
                        "заурчать"
                    ],
                    "common_phrases": [
                        "кот проурчал",
                        "живот проурчал",
                        "мотор проурчал"
                    ],
                    "target_lang_definitions": {
                        "en": "To emit a characteristic low, vibrating sound (usually referring to animals, the stomach, or mechanisms)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "purr",
                                "target_lang_sense_clarification": "of a cat or engine"
                            },
                            {
                                "target_lang_word": "rumble",
                                "target_lang_sense_clarification": "of a stomach or mechanism"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "700af80d-97f3-3f06-ab17-7b03d1eb74e1",
                    "sense_definition": "Сказать или произнести что-либо низким, рокочущим голосом.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "speech_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он что-то недовольно <w>проурчал</w> себе под нос и отвернулся.",
                            "target_lang_translations": {
                                "en": "He <w>growled</w> something displeasedly under his breath and turned away."
                            }
                        },
                        {
                            "text": "Старик <w>проурчал</w> короткое приветствие и прошёл мимо нас.",
                            "target_lang_translations": {
                                "en": "The old man <w>rumbled</w> a short greeting and walked past us."
                            }
                        }
                    ],
                    "synonyms": [
                        "пробурчать",
                        "пробормотать"
                    ],
                    "common_phrases": [
                        "что-то проурчать",
                        "недовольно проурчать"
                    ],
                    "target_lang_definitions": {
                        "en": "To say or utter something in a low, rumbling voice."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rumble",
                                "target_lang_sense_clarification": "speak in a deep voice"
                            },
                            {
                                "target_lang_word": "growl",
                                "target_lang_sense_clarification": "speak in a low, rough voice"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d062f223-7ce2-3a5c-8f58-321be11394db",
                    "sense_definition": "An official document containing a certified statement that serves as proof of a fact, such as birth, marriage, or health status.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "official_docs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You will need to show your birth <w>certificate</w> to apply for a passport."
                        },
                        {
                            "text": "The doctor signed a medical <w>certificate</w> stating that she was fit to work."
                        }
                    ],
                    "synonyms": [
                        "document",
                        "credential"
                    ],
                    "common_phrases": [
                        "birth certificate",
                        "marriage certificate",
                        "death certificate",
                        "medical certificate"
                    ]
                },
                {
                    "sense_id": "d8f8d63e-f705-3482-94f7-7f4a641f1a07",
                    "sense_definition": "A document serving as evidence that a person has completed an educational course or passed an exam.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "official_docs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He received a <w>certificate</w> for completing the advanced English course."
                        },
                        {
                            "text": "The school awards a <w>certificate</w> of attendance to every student."
                        }
                    ],
                    "synonyms": [
                        "diploma",
                        "qualification"
                    ],
                    "common_phrases": [
                        "training certificate",
                        "degree certificate",
                        "award a certificate"
                    ]
                },
                {
                    "sense_id": "f7c6e398-7238-3fab-a62a-c6cdbfeba73e",
                    "sense_definition": "A document evidencing ownership of shares or money held in a bank.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_docs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The investor kept the share <w>certificate</w> in a safe deposit box."
                        },
                        {
                            "text": "The bank issued a savings <w>certificate</w> with a fixed interest rate."
                        }
                    ],
                    "common_phrases": [
                        "share certificate",
                        "savings certificate",
                        "certificate of deposit"
                    ]
                },
                {
                    "sense_id": "980cb297-978f-393f-beb1-9590724013f6",
                    "sense_definition": "A digital file used to verify the identity of a website or user for secure communication.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The browser warned that the website's security <w>certificate</w> had expired."
                        },
                        {
                            "text": "You need to install a digital <w>certificate</w> to access the secure network."
                        }
                    ],
                    "common_phrases": [
                        "digital certificate",
                        "security certificate",
                        "SSL certificate"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short for public key certificate"
                        }
                    ]
                },
                {
                    "sense_id": "f588c5d3-ee17-31e6-b8db-53892af4f7da",
                    "sense_definition": "An official classification indicating the age group for which a motion picture is suitable.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "ratings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The film was granted a 15 <w>certificate</w> because of the violent scenes."
                        },
                        {
                            "text": "Parents should check the <w>certificate</w> before letting children watch the movie."
                        }
                    ],
                    "synonyms": [
                        "rating",
                        "classification"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK"
                        }
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "51be833b-73c8-3938-afae-63ee76134a58",
                    "sense_definition": "To supply a person or thing with an official certificate, especially to authorize or license them.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The agency <w>certificates</w> teachers who have passed all the required exams."
                        },
                        {
                            "text": "The aircraft was <w>certificated</w> for flight after the repairs were inspected."
                        }
                    ],
                    "synonyms": [
                        "certify",
                        "license",
                        "accredit"
                    ]
                }
            ]
        }
    ]
}
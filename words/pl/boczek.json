                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Boczek</w> jest naszym sąsiadem od wielu lat i zawsze służy pomocą.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Бочек</w> — наш сосед уже много лет и всегда готов помочь."
                            }
                        },
                        {
                            "text": "Wczoraj rozmawiałem z panią <w>Boczek</w> o nadchodzącym zebraniu wspólnoty.",
                            "target_lang_translations": {
                                "ru": "Вчера я разговаривал с госпожой <w>Бочек</w> о предстоящем собрании жильцов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия."
                    },
                    "translations": {

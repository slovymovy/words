        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3b7a5b1c-be1e-3957-ae75-cd28a831335d",
                    "sense_definition": "*Eerste persoon enkelvoud tegenwoordige tijd van* <w>stralen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>straal</w> van geluk als ik aan mijn aanstaande vakantie denk.",
                            "target_lang_translations": {

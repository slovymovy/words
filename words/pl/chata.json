{
    "word_family": [
        "chatka",
        "chateńka",
        "chatynka",
        "chatar"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f9d761cc-4dd9-3031-93bd-e8ef6b3b2da5",
                    "sense_definition": "Potoczne określenie na dom lub mieszkanie, w którym ktoś mieszka.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "home_colloquial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice wyjeżdżają na weekend, więc będę miał wolną <w>chatę</w> dla znajomych.",
                            "target_lang_translations": {
                                "ru": "Мои родители уезжают на выходные, так что у меня будет свободная <w>хата</w> для друзей."
                            }
                        },
                        {
                            "text": "Zawsze po ciężkiej pracy wracam prosto na <w>chatę</w>, żeby w końcu odpocząć.",
                            "target_lang_translations": {
                                "ru": "После тяжелой работы я всегда возвращаюсь прямо на <w>хату</w>, чтобы наконец отдохнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "mieszkanie",
                        "dom",
                        "chałupa",
                        "chawira (hawira)"
                    ],
                    "common_phrases": [
                        "mieć wolną chatę",
                        "wbić na chatę",
                        "siedzieć na chacie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название дома или квартиры, где кто-то живет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хата",
                                "target_lang_sense_clarification": "разговорное/сленг"
                            },
                            {
                                "target_lang_word": "квартира",
                                "target_lang_sense_clarification": "нейтральное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6612dca1-47a3-3a96-bc44-2ed127fd5091",
                    "sense_definition": "Drewniany dom mieszkalny, typowy dla dawnej wsi lub terenów górskich.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "building_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na górskim szlaku turystycznym mijaliśmy starą, drewnianą <w>chatę</w> pokrytą gontem.",
                            "target_lang_translations": {
                                "ru": "На горном туристическом маршруте мы прошли мимо старой деревянной <w>хаты</w>, крытой гонтом."
                            }
                        },
                        {
                            "text": "W tej zabytkowej <w>chacie</w> można zjeść pyszne, regionalne potrawy przygotowane przez gospodynię.",
                            "target_lang_translations": {
                                "ru": "В этой старинной <w>хате</w> можно отведать вкусные региональные блюда, приготовленные хозяйкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "chałupa",
                        "domek",
                        "chatka"
                    ],
                    "common_phrases": [
                        "góralska chata",
                        "wiejska chata",
                        "drewniana chata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревянный жилой дом, типичный для старой деревни или горной местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хата",
                                "target_lang_sense_clarification": "сельский дом (южнорусский/украинский тип)"
                            },
                            {
                                "target_lang_word": "изба",
                                "target_lang_sense_clarification": "деревянный сруб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1079a73a-18a5-3727-add7-89d33ecae3f8",
                    "sense_definition": "Prosty, prymitywny budynek mieszkalny wykonany z nietrwałych materiałów, takich jak glina czy słoma.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "building_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podróżnicy zatrzymali się w małej <w>chacie</w> zbudowanej z gałęzi i liści palmowych.",
                            "target_lang_translations": {
                                "ru": "Путешественники остановились в маленькой <w>хижине</w>, построенной из веток и пальмовых листьев."
                            }
                        },
                        {
                            "text": "Uboga ludność w tej części świata wciąż buduje proste <w>chaty</w> z gliny.",
                            "target_lang_translations": {
                                "ru": "Бедное население в этой части света все еще строит простые <w>хижины</w> из глины."
                            }
                        }
                    ],
                    "synonyms": [
                        "lepianka",
                        "szałas",
                        "koleba"
                    ],
                    "common_phrases": [
                        "chata z gliny",
                        "słomiana chata",
                        "prymitywna chata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простая, примитивная жилая постройка из недолговечных материалов, таких как глина или солома."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хижина",
                                "target_lang_sense_clarification": "примитивное жилище"
                            },
                            {
                                "target_lang_word": "лачуга",
                                "target_lang_sense_clarification": "бедное/ветхое жилище"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
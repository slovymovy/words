    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "328f02f8-34c3-3b9f-9727-678978fb550f",
                    "sense_definition": "Часть туши убитых животных, используемая как продукт питания.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы купили на рынке свежее <w>мясо</w> для супа.",
                            "target_lang_translations": {
                                "en": "We bought fresh <w>meat</w> for the soup at the market."
                            }
                        },
                        {
                            "text": "Врачи рекомендуют есть нежирное <w>мясо</w> и много овощей.",
                            "target_lang_translations": {
                                "en": "Doctors recommend eating lean <w>meat</w> and plenty of vegetables."
                            }
                        }
                    ],
                    "synonyms": [
                        "мясное"
                    ],
                    "common_phrases": [
                        "жареное мясо",
                        "кусок мяса",
                        "свежее мясо",
                        "есть мясо"
                    ],
                    "target_lang_definitions": {
                        "en": "Part of the carcass of killed animals used as food."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meat",
                                "target_lang_sense_clarification": "food"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "173768ac-3d53-3c11-8f2a-ac4d5b68fa08",
                    "sense_definition": "Мышечная ткань человека или животного, плоть.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рана была глубокой, и было видно живое <w>мясо</w>.",
                            "target_lang_translations": {
                                "en": "The wound was deep, and raw <w>flesh</w> was visible."
                            }
                        },
                        {
                            "text": "Спортсмен долго тренировался, чтобы нарастить <w>мясо</w> на костях.",
                            "target_lang_translations": {
                                "en": "The athlete trained for a long time to put some <w>meat</w> on his bones."
                            }
                        }
                    ],
                    "synonyms": [
                        "плоть",
                        "мышцы",
                        "мускулы",
                        "мышечная ткань"
                    ],
                    "common_phrases": [
                        "до живого мяса",
                        "кости да мясо"
                    ],
                    "target_lang_definitions": {
                        "en": "Muscle tissue of a human or animal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flesh",
                                "target_lang_sense_clarification": "anatomy"
                            },
                            {
                                "target_lang_word": "meat",
                                "target_lang_sense_clarification": "colloquial, muscle bulk"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c17e76f6-95d4-3975-95ee-d7c80e2544a5",
                    "sense_definition": "Солдаты, которых командование безжалостно посылает на верную смерть (обычно в выражении «пушечное мясо»).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Генералы использовали необученных новобранцев как пушечное <w>мясо</w>.",
                            "target_lang_translations": {
                                "en": "The generals used untrained recruits as cannon <w>fodder</w>."
                            }
                        },
                        {
                            "text": "Никто не хотел быть пушечным <w>мясом</w> в этой бессмысленной войне.",
                            "target_lang_translations": {
                                "en": "No one wanted to be cannon <w>fodder</w> in this senseless war."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пушечное мясо"
                    ],
                    "target_lang_definitions": {
                        "en": "Soldiers ruthlessly sent by command to certain death (usually in the phrase \"cannon fodder\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fodder",
                                "target_lang_sense_clarification": "in \"cannon fodder\""
                            },
                            {
                                "target_lang_word": "meat",
                                "target_lang_sense_clarification": "figurative"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54b1d1bc-cd2d-3e47-a8c3-3011a4a9c326",
                    "sense_definition": "Мягкая внутренняя часть плодов и ягод под кожурой.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Арбуз был очень спелым, и его красное <w>мясо</w> таяло во рту.",
                            "target_lang_translations": {
                                "en": "The watermelon was very ripe, and its red <w>flesh</w> melted in the mouth."
                            }
                        },
                        {
                            "text": "Нужно аккуратно отделить <w>мясо</w> персика от косточки.",
                            "target_lang_translations": {
                                "en": "You need to carefully separate the peach <w>flesh</w> from the pit."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мясо арбуза"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standard word is мякоть"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The soft inner part of fruits and berries under the skin."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flesh",
                                "target_lang_sense_clarification": "of fruit"
                            },
                            {
                                "target_lang_word": "pulp",
                                "target_lang_sense_clarification": "soft part of fruit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6154ea6e-243e-3306-bfc5-de41302dea5d",
                    "sense_definition": "Кровавая битва, жестокая драка или беспорядочное скопление людей.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slang_situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В конце фильма началось настоящее <w>мясо</w> с перестрелками.",
                            "target_lang_translations": {
                                "en": "At the end of the movie, a real <w>bloodbath</w> with shootouts began."
                            }

            "senses": [
                {
                    "sense_id": "8200a6ac-1101-3e3e-b771-56882f6bc889",
                    "sense_definition": "Made smaller, fewer, or less in amount, size, or intensity.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>diminished</w> empire struggled to maintain control over its remaining territories.",
                            "target_lang_translations": {
                                "ru": "<w>Ослабленная</w> империя изо всех сил пыталась сохранить контроль над оставшимися территориями."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6cfc0764-4c01-3d98-8630-3c587e4ff89b",
                    "sense_definition": "Dokument urzędowy stwierdzający wyłączne prawo wynalazcy do korzystania z wynalazku przez określony czas.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_docs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma farmaceutyczna uzyskała wczoraj <w>patent</w> na nowy lek przeciwbólowy.",
                            "target_lang_translations": {
                                "ru": "Фармацевтическая компания вчера получила <w>патент</w> на новое обезболивающее."
                            }
                        },
                        {
                            "text": "Urząd patentowy odrzucił ich wniosek o <w>patent</w> z powodu błędów w dokumentacji.",
                            "target_lang_translations": {
                                "ru": "Патентное ведомство отклонило их заявку на <w>патент</w> из-за ошибок в документации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "urząd patentowy",
                        "uzyskać patent",
                        "zgłosić patent",
                        "ochrona patentowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный документ, подтверждающий исключительное право изобретателя на использование изобретения в течение определенного времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "патент",
                                "target_lang_sense_clarification": "документ на изобретение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4a32af2-0ebf-3bb8-978b-9ff813f75bd8",
                    "sense_definition": "Dokument potwierdzający posiadanie uprawnień do prowadzenia jachtów lub łodzi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_docs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat zdał trudny egzamin na <w>patent</w> sternika motorowodnego.",
                            "target_lang_translations": {
                                "ru": "Мой брат сдал сложный экзамен на <w>права</w> рулевого моторной лодки."
                            }
                        },
                        {
                            "text": "Bez ważnego <w>patentu</w> nie możesz legalnie sterować tak dużym jachtem na jeziorze.",
                            "target_lang_translations": {
                                "ru": "Без действующих <w>прав</w> ты не можешь легально управлять такой большой яхтой на озере."
                            }
                        }
                    ],
                    "common_phrases": [
                        "patent żeglarski",
                        "patent sternika",
                        "egzamin na patent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ, подтверждающий право на управление яхтами или лодками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "права",
                                "target_lang_sense_clarification": "лицензия на управление судном"
                            },
                            {
                                "target_lang_word": "удостоверение",
                                "target_lang_sense_clarification": "документ о квалификации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4448d580-7325-34e9-a92a-e7a611ce0901",
                    "sense_definition": "Sprytny sposób lub metoda na zrobienie czegoś, często ułatwiająca rozwiązanie problemu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szukam jakiegoś dobrego <w>patentu</w> na szybkie obieranie dużej ilości ziemniaków.",
                            "target_lang_translations": {
                                "ru": "Я ищу какой-нибудь хороший <w>способ</w> быстро почистить много картошки."
                            }
                        },
                        {
                            "text": "Marek ma swój własny <w>patent</w> na naprawę cieknącego kranu w łazience.",
                            "target_lang_translations": {
                                "ru": "У Марека есть свой собственный <w>метод</w> починки протекающего крана в ванной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć patent na",
                        "dobry patent",
                        "sprzedać patent"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хитрый способ или метод сделать что-то, часто облегчающий решение проблемы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "способ",
                                "target_lang_sense_clarification": "метод решения"
                            },
                            {
                                "target_lang_word": "лайфхак",
                                "target_lang_sense_clarification": "хитрость"
                            },
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "уловка"
                            }
                        ]
                    }
                }
            ]
        }

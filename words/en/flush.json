                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямо",
                                "target_lang_sense_clarification": "точно (об ударе)"
                            },
                            {
                                "target_lang_word": "точно",
                                "target_lang_sense_clarification": "о попадании"
                            }
                        ]
                    }
                }

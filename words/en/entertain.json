{
    "word_family": [
        "entertainment",
        "entertainer",
        "entertaining"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7300877a-84f3-34d1-b99e-faffcf33d438",
                    "sense_definition": "To amuse someone or engage their attention in an agreeable way.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "amusement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The clown <w>entertained</w> the children with magic tricks and balloons.",
                            "target_lang_translations": {
                                "ru": "Клоун <w>развлекал</w> детей фокусами и воздушными шарами."
                            }
                        },
                        {
                            "text": "We need to find a way to <w>entertain</w> the crowd while we fix the microphone.",
                            "target_lang_translations": {
                                "ru": "Нам нужно найти способ <w>развлечь</w> толпу, пока мы чиним микрофон."
                            }
                        }
                    ],
                    "synonyms": [
                        "amuse",
                        "delight",
                        "please"
                    ],
                    "antonyms": [
                        "bore",
                        "annoy"
                    ],
                    "common_phrases": [
                        "entertain the audience",
                        "entertain children",
                        "entertain the crowd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Развлекать кого-либо или занимать чье-то внимание приятным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развлекать",
                                "target_lang_sense_clarification": "занимать внимание, веселить"
                            },
                            {
                                "target_lang_word": "забавлять",
                                "target_lang_sense_clarification": "смешить, доставлять удовольствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5de6e6b1-9235-329f-a481-dded84541603",
                    "sense_definition": "To provide food, drink, and hospitality to guests, usually at one's home.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "hospitality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They love to <w>entertain</w> friends and neighbors on the weekends.",
                            "target_lang_translations": {
                                "ru": "Они любят <w>принимать</w> друзей и соседей по выходным."
                            }
                        },
                        {
                            "text": "She often <w>entertains</w> business clients at expensive restaurants.",
                            "target_lang_translations": {
                                "ru": "Она часто <w>принимает</w> деловых клиентов в дорогих ресторанах."
                            }
                        }
                    ],
                    "synonyms": [
                        "host",
                        "receive",
                        "treat"
                    ],
                    "common_phrases": [
                        "entertain guests",
                        "entertain at home",
                        "entertain visitors"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать гостеприимство, угощать едой и напитками гостей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принимать гостей",
                                "target_lang_sense_clarification": "быть хозяином, оказывать прием"
                            },
                            {
                                "target_lang_word": "угощать",
                                "target_lang_sense_clarification": "предлагать еду и напитки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "131c5a90-b078-3036-a9da-c34d3aaa7e20",
                    "sense_definition": "To think about an idea, proposal, or feeling; to take something into consideration.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "cognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee refused to <w>entertain</w> the proposal for a budget increase.",
                            "target_lang_translations": {
                                "ru": "Комитет отказался <w>рассматривать</w> предложение об увеличении бюджета."
                            }
                        },
                        {
                            "text": "I will not <w>entertain</w> any doubts about his loyalty until I see proof.",
                            "target_lang_translations": {
                                "ru": "Я не буду <w>допускать</w> никаких сомнений в его верности, пока не увижу доказательств."
                            }
                        }
                    ],
                    "synonyms": [
                        "consider",
                        "contemplate",
                        "ponder"
                    ],
                    "antonyms": [
                        "dismiss",
                        "reject"
                    ],
                    "common_phrases": [
                        "entertain an idea",
                        "entertain a proposal",
                        "entertain a possibility",
                        "entertain a doubt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обдумывать идею, предложение или чувство; принимать что-либо во внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассматривать",
                                "target_lang_sense_clarification": "принимать во внимание предложение или идею"
                            },
                            {
                                "target_lang_word": "допускать",
                                "target_lang_sense_clarification": "позволять мысли или сомнению возникнуть"
                            },
                            {
                                "target_lang_word": "обдумывать",
                                "target_lang_sense_clarification": "размышлять над чем-то"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f5eb6617-8232-34fe-8fb6-60da8c88df95",
                    "sense_definition": "Entertainment or pleasure.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "amusement_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king prepared a grand <w>entertain</w> for his loyal subjects.",
                            "target_lang_translations": {
                                "ru": "Король подготовил грандиозное <w>развлечение</w> для своих верных подданных."
                            }
                        },
                        {
                            "text": "They sought only <w>entertain</w> and joy during the festival.",
                            "target_lang_translations": {
                                "ru": "Они искали лишь <w>развлечений</w> и радости во время праздника."
                            }
                        }
                    ],
                    "synonyms": [
                        "entertainment",
                        "pleasure"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern English uses 'entertainment' instead."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Развлечение или удовольствие (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развлечение",
                                "target_lang_sense_clarification": "устаревшая форма существительного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76c144e2-a1e0-3e5b-a01c-385c1d0e8fc2",
                    "sense_definition": "Reception of a guest; welcome.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "hospitality_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The traveler gave thanks for the warm <w>entertain</w> he received at the castle.",
                            "target_lang_translations": {
                                "ru": "Путешественник поблагодарил за теплый <w>прием</w>, оказанный ему в замке."
                            }
                        },
                        {
                            "text": "We expect better <w>entertain</w> than this cold greeting.",
                            "target_lang_translations": {
                                "ru": "Мы ожидаем лучшего <w>приема</w>, чем это холодное приветствие."
                            }
                        }
                    ],
                    "synonyms": [
                        "welcome",
                        "reception"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern English uses 'welcome' or 'hospitality' instead."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прием гостя; приветствие (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прием",
                                "target_lang_sense_clarification": "встреча гостей, гостеприимство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
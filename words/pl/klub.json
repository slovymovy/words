                {
                    "sense_id": "10ce83be-bfbf-312b-97a5-98bd353c9e42",
                    "sense_definition": "Organizacja lub stowarzyszenie zrzeszające ludzi o wspólnych zainteresowaniach, celach lub uprawiających ten sam sport",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "association",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat zapisał się do <w>klubu</w> szachowego, aby rozwijać swoje umiejętności.",
                            "target_lang_translations": {
                                "ru": "Мой брат записался в шахматный <w>клуб</w>, чтобы развивать свои навыки."
                            }
                        },
                        {
                            "text": "Członkowie <w>klubu</w> spotykają się w każdy piątek na wspólnych dyskusjach.",
                            "target_lang_translations": {
                                "ru": "Члены <w>клуба</w> встречаются каждую пятницу для совместных дискуссий."
                            }
                        }
                    ],
                    "synonyms": [
                        "stowarzyszenie",
                        "organizacja"
                    ],
                    "common_phrases": [

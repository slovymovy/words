    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b9af67f5-cb41-3ff3-bc0a-f7dbfcdcc61c",
                    "sense_definition": "Impossible to obtain or acquire.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spare parts for the obsolete machine are now completely <w>unprocurable</w>.",
                            "target_lang_translations": {
                                "ru": "Запчасти для устаревшего станка теперь совершенно <w>недоступны</w>."
                            }
                        },
                        {
                            "text": "During the severe shortage, fresh vegetables were virtually <w>unprocurable</w> in the city.",
                            "target_lang_translations": {
                                "ru": "Во время острой нехватки свежие овощи в городе были практически <w>недоступны</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "unobtainable"
                    ],
                    "antonyms": [
                        "obtainable",
                        "procurable"
                    ],
                    "common_phrases": [
                        "virtually unprocurable",
                        "almost unprocurable",
                        "remains unprocurable"
                    ],
                    "target_lang_definitions": {
                        "ru": "Невозможный для получения или приобретения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недоступный",
                                "target_lang_sense_clarification": "общий термин для того, что нельзя получить"
                            },
                            {
                                "target_lang_word": "невозможный для приобретения",
                                "target_lang_sense_clarification": "формальный вариант"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "75443c54-2960-34f9-9532-bfcb209c3553",
                    "sense_definition": "Something that is impossible to obtain.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager crossed off the <w>unprocurables</w> from the inventory list.",
                            "target_lang_translations": {
                                "ru": "Менеджер вычеркнул <w>недоступные позиции</w> из инвентарного списка."
                            }
                        },
                        {
                            "text": "We must find suitable substitutes for these <w>unprocurables</w> immediately.",
                            "target_lang_translations": {
                                "ru": "Мы должны немедленно найти подходящую замену этим <w>недоступным товарам</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что невозможно получить или приобрести (обычно о товарах или ресурсах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недоступный товар",
                                "target_lang_sense_clarification": "предмет, который нельзя купить или достать"
                            },
                            {
                                "target_lang_word": "дефицит",
                                "target_lang_sense_clarification": "товары, которых не хватает"
                            }
                        ]
                    }
                }
            ]
        }
    ]

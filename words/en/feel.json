{
    "word_family": [
        "feeling",
        "feeler",
        "unfeeling"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cfdb6ea2-231d-3312-95ab-24209d6f0625",
                    "sense_definition": "To become aware of something through the skin or physical contact.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>feel</w> the warmth of the sun on your face.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>почувствовать</w> тепло солнца на своём лице."
                            }
                        },
                        {
                            "text": "She <w>felt</w> the soft texture of the blanket.",
                            "target_lang_translations": {
                                "ru": "Она <w>ощутила</w> мягкую текстуру одеяла."
                            }
                        }
                    ],
                    "synonyms": [
                        "touch"
                    ],
                    "common_phrases": [
                        "feel pain",
                        "feel the heat",
                        "feel a pulse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воспринимать что-либо через кожу или физический контакт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чу́вствовать",
                                "target_lang_sense_clarification": "воспринимать органами чувств"
                            },
                            {
                                "target_lang_word": "ощуща́ть",
                                "target_lang_sense_clarification": "воспринимать тактильно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2545bae-89d9-3fa9-8265-ad2a68394e37",
                    "sense_definition": "To experience an emotion or physical sensation.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "internal_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>feel</w> happy when I am with my friends.",
                            "target_lang_translations": {
                                "ru": "Я <w>чувствую себя</w> счастливым, когда я с друзьями."
                            }
                        },
                        {
                            "text": "He <w>felt</w> tired after working all day.",
                            "target_lang_translations": {
                                "ru": "Он <w>чувствовал</w> усталость после работы весь день."
                            }
                        }
                    ],
                    "synonyms": [
                        "handle",
                        "touch"
                    ],
                    "common_phrases": [
                        "feel happy",
                        "feel sad",
                        "feel sick",
                        "feel better"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать эмоцию или физическое ощущение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чу́вствовать",
                                "target_lang_sense_clarification": "испытывать состояние или эмоцию"
                            },
                            {
                                "target_lang_word": "ощуща́ть",
                                "target_lang_sense_clarification": "переживать чувство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a62e52c-7e4f-3c42-8ac2-ab1b759fae7f",
                    "sense_definition": "To give a particular sensation or impression; to seem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "impression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The water <w>feels</w> cold this morning.",
                            "target_lang_translations": {
                                "ru": "Вода <w>ощущается</w> холодной сегодня утром."
                            }
                        },
                        {
                            "text": "It <w>feels</w> strange to be back in my hometown.",
                            "target_lang_translations": {
                                "ru": "<w>Кажется</w> странным снова быть в родном городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "touch"
                    ],
                    "common_phrases": [
                        "feel like",
                        "feel good",
                        "feel strange"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить определённое впечатление или ощущение; казаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каза́ться",
                                "target_lang_sense_clarification": "производить впечатление"
                            },
                            {
                                "target_lang_word": "ощуща́ться",
                                "target_lang_sense_clarification": "восприниматься чувствами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8e651f6-bccc-31e0-9537-428e37a0cb03",
                    "sense_definition": "To have a belief, opinion, or impression about something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>feel</w> that we should leave before it gets dark.",
                            "target_lang_translations": {
                                "ru": "Я <w>считаю</w>, что нам следует уйти до наступления темноты."
                            }
                        },
                        {
                            "text": "She <w>felt</w> it was important to tell the truth.",
                            "target_lang_translations": {
                                "ru": "Она <w>считала</w> важным сказать правду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "feel that",
                        "feel strongly about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь убеждение, мнение или впечатление о чём-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "счита́ть",
                                "target_lang_sense_clarification": "иметь мнение"
                            },
                            {
                                "target_lang_word": "полага́ть",
                                "target_lang_sense_clarification": "думать, допускать"
                            },
                            {
                                "target_lang_word": "ду́мать",
                                "target_lang_sense_clarification": "мыслить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3d00347-06ee-32d5-8613-7038db14c28e",
                    "sense_definition": "To search for something using the hands, especially when it is not visible.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>felt</w> for the light switch in the dark room.",
                            "target_lang_translations": {
                                "ru": "Он <w>искал на ощупь</w> выключатель в тёмной комнате."
                            }
                        },
                        {
                            "text": "She <w>felt</w> around in her bag to find her keys.",
                            "target_lang_translations": {
                                "ru": "Она <w>шарила</w> в сумке, чтобы найти ключи."
                            }
                        }
                    ],
                    "synonyms": [
                        "grope"
                    ],
                    "common_phrases": [
                        "feel for",
                        "feel around"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать что-либо руками, особенно когда предмет не виден."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иска́ть на о́щупь",
                                "target_lang_sense_clarification": "искать руками"
                            },
                            {
                                "target_lang_word": "ша́рить",
                                "target_lang_sense_clarification": "искать вслепую"
                            },
                            {
                                "target_lang_word": "нащу́пывать",
                                "target_lang_sense_clarification": "находить касанием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "576df22c-8afc-359b-bdb9-09cabec61268",
                    "sense_definition": "To have sympathy or compassion for someone.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "internal_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I really <w>feel</w> for you during this difficult time.",
                            "target_lang_translations": {
                                "ru": "Я искренне <w>сочувствую</w> тебе в это трудное время."
                            }
                        },
                        {
                            "text": "We <w>feel</w> for the families who lost their homes.",
                            "target_lang_translations": {
                                "ru": "Мы <w>сочувствуем</w> семьям, потерявшим свои дома."
                            }
                        }
                    ],
                    "common_phrases": [
                        "feel for someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать сочувствие или сострадание к кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сочу́вствовать",
                                "target_lang_sense_clarification": "проявлять сострадание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c06df938-1d73-3e1b-a485-a691ac28f838",
                    "sense_definition": "The quality of something perceived by touching it; the sensation produced by touching.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I like the soft <w>feel</w> of this cotton shirt.",
                            "target_lang_translations": {
                                "ru": "Мне нравится, какая эта хлопковая рубашка <w>на ощупь</w>."
                            }
                        },
                        {
                            "text": "The wood has a rough <w>feel</w> to it.",
                            "target_lang_translations": {
                                "ru": "Дерево шероховатое <w>на ощупь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "touch"
                    ],
                    "common_phrases": [
                        "soft feel",
                        "rough feel",
                        "smooth feel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество предмета, воспринимаемое при прикосновении; ощущение от прикосновения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ощуще́ние",
                                "target_lang_sense_clarification": "чувственное восприятие"
                            },
                            {
                                "target_lang_word": "на о́щупь",
                                "target_lang_sense_clarification": "при касании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13fc19ee-0c0f-3e2b-9a14-f553aab2b683",
                    "sense_definition": "A general impression or atmosphere of a place or situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "impression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The restaurant has a very cozy and welcoming <w>feel</w>.",
                            "target_lang_translations": {
                                "ru": "В ресторане очень уютная и гостеприимная <w>атмосфера</w>."
                            }
                        },
                        {
                            "text": "You should visit the city to get a <w>feel</w> for the local culture.",
                            "target_lang_translations": {
                                "ru": "Тебе стоит посетить город, чтобы прочувствовать местную <w>атмосферу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get a feel for",
                        "have a feel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общее впечатление или атмосфера места или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атмосфе́ра",
                                "target_lang_sense_clarification": "общий тон, настроение"
                            },
                            {
                                "target_lang_word": "дух",
                                "target_lang_sense_clarification": "внутренняя сущность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2eaff4a2-3bc7-33e2-862d-82fb68b1e95b",
                    "sense_definition": "An intuitive understanding or natural ability.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a natural <w>feel</w> for playing the piano.",
                            "target_lang_translations": {
                                "ru": "У неё природное <w>чутьё</w> к игре на пианино."
                            }
                        },
                        {
                            "text": "It takes time to develop a <w>feel</w> for the language.",
                            "target_lang_translations": {
                                "ru": "Требуется время, чтобы развить <w>чутьё</w> языка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "feel for music",
                        "feel for language"
                    ],
                    "target_lang_definitions": {
                        "ru": "Интуитивное понимание или природная способность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чутьё",
                                "target_lang_sense_clarification": "интуиция"
                            },
                            {
                                "target_lang_word": "чу́вство",
                                "target_lang_sense_clarification": "способность ощущать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
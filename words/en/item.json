                            {
                                "target_lang_word": "вещь",
                                "target_lang_sense_clarification": "бытовой предмет или покупка"
                            },
                            {
                                "target_lang_word": "штука",
                                "target_lang_sense_clarification": "разговорное; отдельный экземпляр"
                            }
                        ]

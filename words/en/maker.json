            "senses": [
                {
                    "sense_id": "c04054ed-ddf5-3ff8-8441-8a98d1b5d023",
                    "sense_definition": "A person or thing that makes or produces something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to buy a new coffee <w>maker</w> for the office kitchen.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить новую <w>кофеварку</w> для офисной кухни."
                            }
                        },
                        {
                            "text": "She is a key decision <w>maker</w> in the company and guides its future strategy.",
                            "target_lang_translations": {
                                "ru": "Она является ключевым <w>лицом, принимающим решения</w>, в компании и определяет её будущую стратегию."
                            }
                        }
                    ],
                    "synonyms": [
                        "creator",
                        "producer",
                        "manufacturer",
                        "builder",
                        "constructor"
                    ],
                    "antonyms": [
                        "destroyer",
                        "consumer"
                    ],
                    "common_phrases": [
                        "decision maker",
                        "coffee maker",
                        "film maker",
                        "trouble maker",
                        "peace maker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или устройство, которое что-либо создает или производит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "создатель",
                                "target_lang_sense_clarification": "тот, кто творит или создает что-либо"
                            },
                            {
                                "target_lang_word": "производитель",
                                "target_lang_sense_clarification": "изготовитель товаров или услуг"
                            },
                            {
                                "target_lang_word": "изготовитель",
                                "target_lang_sense_clarification": "тот, кто делает вещи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9bf8791-69f2-3f42-9bab-6ff20932ed9c",
                    "sense_definition": "Someone who signs a promissory note, thereby becoming responsible for payment.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>maker</w> of the note is legally obligated to pay the specified amount by the due date.",
                            "target_lang_translations": {
                                "ru": "<w>Векселедатель</w> юридически обязан выплатить указанную сумму к установленному сроку."
                            }
                        },
                        {
                            "text": "If the <w>maker</w> defaults on the loan, the bank will take legal action to recover the funds.",
                            "target_lang_translations": {
                                "ru": "Если <w>векселедатель</w> не выполнит обязательства по займу, банк предпримет юридические действия для возврата средств."
                            }
                        }
                    ],
                    "synonyms": [
                        "signatory"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "legal or financial context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, выдающее вексель и обязующееся произвести по нему платеж."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "векселедатель",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1101b7a2-fdb9-3673-a094-44608f078ee6",

                            {
                                "target_lang_word": "штрафная площадь",
                                "target_lang_sense_clarification": "футбольный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e10314c2-c8ff-3094-9e3a-3ad5344a0442",
                    "sense_definition": "Een groep bestaande uit zestien personen of eenheden.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>zestien</w> werden geselecteerd uit een grote groep kandidaten.",
                            "target_lang_translations": {
                                "ru": "Эти <w>шестнадцать</w> были отобраны из большой группы кандидатов."
                            }
                        },
                        {
                            "text": "Slechts vier van de <w>zestien</w> wisten de moeilijke opdracht te voltooien.",
                            "target_lang_translations": {
                                "ru": "Лишь четверо из <w>шестнадцати</w> смогли выполнить это сложное задание."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа, состоящая из шестнадцати человек или единиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шестнадцать",

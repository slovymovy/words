    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c4dbd53-99e4-3d8d-b88d-8fa4f89dcc7f",
                    "sense_definition": "A business or establishment where alcoholic drinks and sometimes food are served to customers.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "establishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to meet at a local <w>bar</w> for a drink after work.",
                            "target_lang_translations": {
                                "ru": "Мы решили встретиться в местном <w>баре</w>, чтобы выпить после работы."
                            }
                        },
                        {
                            "text": "The hotel has a nice <w>bar</w> on the roof with a view of the city.",
                            "target_lang_translations": {
                                "ru": "В отеле есть хороший <w>бар</w> на крыше с видом на город."
                            }
                        }
                    ],
                    "synonyms": [
                        "pub",
                        "tavern",
                        "saloon"
                    ],
                    "common_phrases": [
                        "go to a bar",
                        "meet at the bar",
                        "hotel bar",
                        "sports bar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заведение или предприятие, где посетителям подают алкогольные напитки и иногда еду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бар",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "паб",
                                "target_lang_sense_clarification": "пивное заведение в британском стиле"
                            },
                            {
                                "target_lang_word": "кабак",
                                "target_lang_sense_clarification": "разговорное, пренебрежительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47bd507c-fbf1-3ee8-9a95-5aba294552a9",
                    "sense_definition": "A block of a solid substance, usually rectangular in shape.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a <w>bar</w> of chocolate to share with her friends.",
                            "target_lang_translations": {
                                "ru": "Она купила <w>плитку</w> шоколада, чтобы поделиться с друзьями."
                            }
                        },
                        {
                            "text": "Please wash your hands with this <w>bar</w> of soap.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, помой руки этим <w>куском</w> мыла."
                            }
                        }
                    ],
                    "synonyms": [
                        "block",
                        "slab",
                        "cake"
                    ],
                    "common_phrases": [
                        "bar of chocolate",
                        "bar of soap",
                        "candy bar",
                        "gold bar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок твердого вещества, обычно прямоугольной формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плитка",
                                "target_lang_sense_clarification": "шоколада"
                            },
                            {
                                "target_lang_word": "брусок",
                                "target_lang_sense_clarification": "мыла, золота"
                            },
                            {
                                "target_lang_word": "кусок",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a76c0a6-2ce5-300e-b215-7a69d8464593",
                    "sense_definition": "A long, straight, rigid piece of metal or wood.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The thief tried to pry the window open with a metal <w>bar</w>.",
                            "target_lang_translations": {
                                "ru": "Вор попытался вскрыть окно металлическим <w>прутом</w>."
                            }
                        },
                        {
                            "text": "The prisoner looked out through the iron <w>bars</w> of his cell.",
                            "target_lang_translations": {
                                "ru": "Заключенный смотрел наружу сквозь железные <w>прутья</w> своей камеры."
                            }
                        }
                    ],
                    "synonyms": [
                        "rod",
                        "pole",
                        "shaft"
                    ],
                    "common_phrases": [
                        "iron bar",
                        "metal bar",
                        "steel bar",
                        "behind bars"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный, прямой, жесткий кусок металла или дерева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прут",
                                "target_lang_sense_clarification": "металлический"
                            },
                            {
                                "target_lang_word": "решетка",
                                "target_lang_sense_clarification": "на окне, в тюрьме (обычно мн.ч.)"
                            },
                            {
                                "target_lang_word": "брус",
                                "target_lang_sense_clarification": "деревянный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d1bf857-e13d-3cb1-b019-5a52586e107a",
                    "sense_definition": "A long, high counter in a business or home where drinks or food are served.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "establishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We sat at the <w>bar</w> and ordered some coffee.",
                            "target_lang_translations": {
                                "ru": "Мы сели за <w>барную стойку</w> и заказали кофе."
                            }
                        },
                        {
                            "text": "The bartender wiped the <w>bar</w> with a clean cloth.",
                            "target_lang_translations": {
                                "ru": "Бармен протер <w>стойку</w> чистой тряпкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "counter"
                    ],
                    "common_phrases": [
                        "sit at the bar",
                        "behind the bar",
                        "breakfast bar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий прилавок в заведении или дома, где подают напитки или еду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "барная стойка",
                                "target_lang_sense_clarification": "полное название"
                            },
                            {
                                "target_lang_word": "стойка",
                                "target_lang_sense_clarification": "краткая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca4e74f0-ca08-3af2-a45c-44a5f8ab4135",
                    "sense_definition": "A visual symbol, usually a small rectangle, indicating the strength of a wireless signal or battery life.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot make a call because I have no <w>bars</w> on my phone.",
                            "target_lang_translations": {
                                "ru": "Я не могу позвонить, потому что на моем телефоне нет <w>полосок</w> сигнала."
                            }
                        },
                        {
                            "text": "The signal is weak here, showing only one <w>bar</w>.",
                            "target_lang_translations": {
                                "ru": "Сигнал здесь слабый, показывает только одну <w>полоску</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "full bars",
                        "signal bars"
                    ],
                    "target_lang_definitions": {
                        "ru": "Визуальный символ, обычно прямоугольник, указывающий уровень беспроводного сигнала или заряда батареи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полоска",
                                "target_lang_sense_clarification": "индикатор на экране"
                            },
                            {
                                "target_lang_word": "деление",
                                "target_lang_sense_clarification": "шкалы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e0f4585-fa52-33a0-9a95-1050eee85db9",
                    "sense_definition": "A standard of quality or performance that is expected or used for comparison.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new athlete really raised the <w>bar</w> for everyone else in the competition.",
                            "target_lang_translations": {
                                "ru": "Новый спортсмен действительно поднял <w>планку</w> для всех остальных участников соревнований."
                            }
                        },
                        {
                            "text": "We need to set the <w>bar</w> high if we want to achieve excellence.",
                            "target_lang_translations": {
                                "ru": "Нам нужно установить <w>планку</w> высоко, если мы хотим достичь совершенства."
                            }
                        }
                    ],
                    "synonyms": [
                        "standard",
                        "level",
                        "benchmark"
                    ],
                    "common_phrases": [
                        "raise the bar",
                        "set the bar",
                        "lower the bar"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "idiomatic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандарт качества или производительности, ожидаемый или используемый для сравнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "планка",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31757218-b7a0-3021-93b0-fba33bc37fe6",
                    "sense_definition": "Something that blocks movement or prevents an action; an obstacle.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "obstruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lack of money should not be a <w>bar</w> to getting a good education.",
                            "target_lang_translations": {
                                "ru": "Нехватка денег не должна быть <w>преградой</w> для получения хорошего образования."
                            }
                        },
                        {
                            "text": "The fallen tree acted as a <w>bar</w> to any vehicles trying to pass.",
                            "target_lang_translations": {
                                "ru": "Упавшее дерево послужило <w>препятствием</w> для любых транспортных средств, пытающихся проехать."
                            }
                        }
                    ],
                    "synonyms": [
                        "obstacle",
                        "barrier",
                        "hindrance",
                        "obstruction"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что блокирует движение или предотвращает действие; препятствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преграда",
                                "target_lang_sense_clarification": "то, что преграждает путь"
                            },
                            {
                                "target_lang_word": "препятствие",
                                "target_lang_sense_clarification": "помеха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f305d684-636d-34fb-a4e3-36a102109873",
                    "sense_definition": "A specific segment of time in music, defined by a given number of beats; a measure.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The drummer missed a beat in the final <w>bar</w> of the song.",
                            "target_lang_translations": {
                                "ru": "Барабанщик сбился с ритма в последнем <w>такте</w> песни."
                            }
                        },
                        {
                            "text": "Please start playing from the third <w>bar</w> on page two.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, начните играть с третьего <w>такта</w> на второй странице."
                            }
                        }
                    ],
                    "synonyms": [
                        "measure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный отрезок времени в музыке, заданный количеством ударов; такт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "такт",
                                "target_lang_sense_clarification": "музыкальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f39e266-b2c3-38a1-859c-2794593da7da",
                    "sense_definition": "The legal profession, or the group of lawyers qualified to practice law.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After years of study, she was finally called to the <w>bar</w>.",
                            "target_lang_translations": {
                                "ru": "После многих лет учебы она была наконец допущена к <w>адвокатуре</w>."
                            }
                        },
                        {
                            "text": "Members of the <w>bar</w> gathered for their annual conference.",
                            "target_lang_translations": {
                                "ru": "Члены <w>коллегии адвокатов</w> собрались на свою ежегодную конференцию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "called to the bar",
                        "the bar exam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридическая профессия или группа юристов, имеющих право заниматься адвокатской практикой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адвокатура",
                                "target_lang_sense_clarification": "профессия"
                            },
                            {
                                "target_lang_word": "коллегия адвокатов",
                                "target_lang_sense_clarification": "организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26e9b4dc-7d19-3c13-8867-c7e45e27e586",
                    "sense_definition": "A ridge of sand or sediment formed by water currents, often near a shore or river mouth.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boat could not enter the harbor because of a shallow sand <w>bar</w>.",
                            "target_lang_translations": {
                                "ru": "Лодка не могла войти в гавань из-за мелкой песчаной <w>отмели</w>."
                            }
                        },
                        {
                            "text": "We walked out onto the <w>bar</w> at low tide to collect shells.",
                            "target_lang_translations": {
                                "ru": "Мы вышли на <w>косу</w> во время отлива, чтобы собрать ракушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "sandbar",
                        "shoal",
                        "bank"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гряда песка или осадочных пород, образованная течением воды, часто у берега или устья реки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отмель",
                                "target_lang_sense_clarification": "мелководье"
                            },
                            {
                                "target_lang_word": "коса",
                                "target_lang_sense_clarification": "полоса суши"
                            },
                            {
                                "target_lang_word": "банка",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67cb7fdd-2e89-3310-9cc6-1cbf614ed99d",
                    "sense_definition": "Lyrics in hip-hop or rap music, especially those that are skillful or clever.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That rapper is famous for having incredible <w>bars</w> and flow.",
                            "target_lang_translations": {
                                "ru": "Этот рэпер известен своими невероятными <w>строчками</w> и флоу."
                            }
                        },
                        {
                            "text": "He spent all night writing <w>bars</w> for his new track.",
                            "target_lang_translations": {
                                "ru": "Он провел всю ночь, сочиняя <w>текст</w> для своего нового трека."
                            }
                        }
                    ],
                    "synonyms": [
                        "lyrics",
                        "lines",
                        "verses"
                    ],
                    "common_phrases": [
                        "drop bars",
                        "spit bars"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "hip-hop culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Текст в хип-хопе или рэпе, особенно искусный или умный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строчки",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "панчи",
                                "target_lang_sense_clarification": "сленг, ударные строки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "264a1904-198e-3ecc-b882-a5711bc312ae",
                    "sense_definition": "A metric unit of pressure equal to 100,000 pascals.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tire pressure should be maintained at around 2.5 <w>bar</w>.",
                            "target_lang_translations": {
                                "ru": "Давление в шинах должно поддерживаться на уровне около 2,5 <w>бар</w>."
                            }
                        },
                        {
                            "text": "Atmospheric pressure at sea level is approximately one <w>bar</w>.",
                            "target_lang_translations": {
                                "ru": "Атмосферное давление на уровне моря составляет примерно один <w>бар</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Метрическая единица давления, равная 100 000 паскалей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бар",
                                "target_lang_sense_clarification": "единица измерения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6f116b2c-4072-3aa4-8786-1b9487a065ad",
                    "sense_definition": "To officially prevent someone from entering a place or doing something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "prohibit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The unruly customer was <w>barred</w> from the club for fighting.",
                            "target_lang_translations": {
                                "ru": "Буйному посетителю <w>запретили вход</w> в клуб за драку."
                            }
                        },
                        {
                            "text": "Strict laws <w>bar</w> companies from dumping waste in the river.",
                            "target_lang_translations": {
                                "ru": "Строгие законы <w>запрещают</w> компаниям сбрасывать отходы в реку."
                            }
                        }
                    ],
                    "synonyms": [
                        "ban",
                        "prohibit",
                        "forbid",
                        "exclude"
                    ],
                    "antonyms": [
                        "allow",
                        "permit",
                        "admit"
                    ],
                    "common_phrases": [
                        "bar from entry",
                        "permanently barred"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально запрещать кому-либо входить в место или делать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запрещать",
                                "target_lang_sense_clarification": "не разрешать"
                            },
                            {
                                "target_lang_word": "не допускать",
                                "target_lang_sense_clarification": "внутрь"
                            },
                            {
                                "target_lang_word": "отстранять",
                                "target_lang_sense_clarification": "от участия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdc0fb5f-6523-3a61-a5c0-35f723058289",
                    "sense_definition": "To block a path or road so that nothing can pass.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "obstruct",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A fallen tree <w>barred</w> our way, so we had to turn back.",
                            "target_lang_translations": {
                                "ru": "Упавшее дерево <w>преградило</w> нам путь, поэтому нам пришлось повернуть назад."
                            }
                        },
                        {
                            "text": "Police officers <w>barred</w> the exit to catch the suspect.",
                            "target_lang_translations": {
                                "ru": "Полицейские <w>перекрыли</w> выход, чтобы поймать подозреваемого."
                            }
                        }
                    ],
                    "synonyms": [
                        "block",
                        "obstruct"
                    ],
                    "antonyms": [
                        "clear",
                        "open"
                    ],
                    "common_phrases": [
                        "bar the way",
                        "bar the exit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перекрывать путь или дорогу так, чтобы ничто не могло пройти."

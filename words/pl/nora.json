    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e679eda-a47a-33fc-8849-3505d5cc55a0",
                    "sense_definition": "Podziemne schronienie wykopane przez zwierzęta, służące im jako legowisko i miejsce ukrycia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod starym dębem zauważyliśmy głęboką lisią <w>norę</w>.",
                            "target_lang_translations": {
                                "ru": "Под старым дубом мы заметили глубокую лисью <w>нору</w>."
                            }
                        },
                        {
                            "text": "Zimorodki gniazdują w <w>norze</w> wygrzebanej w piaszczystym urwisku.",
                            "target_lang_translations": {
                                "ru": "Зимородки гнездятся в <w>норе</w>, вырытой в песчаном обрыве."
                            }
                        }
                    ],
                    "synonyms": [
                        "jama"
                    ],
                    "common_phrases": [
                        "lisia nora",
                        "wejście do nory",
                        "ukryć się w norze",
                        "wyjść z nory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подземное убежище, вырытое животными, служащее им логовом и укрытием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нора",
                                "target_lang_sense_clarification": "жилище животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc0f24b3-043e-3aa4-8f11-316ac72dcc2e",
                    "sense_definition": "Ciemne, brudne, zaniedbane lub bardzo ciasne pomieszczenie mieszkalne.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdzie ja mam zapraszać gości, do tej ciemnej <w>nory</w>?",
                            "target_lang_translations": {
                                "ru": "Куда мне приглашать гостей, в эту тёмную <w>нору</w>?"
                            }
                        },
                        {
                            "text": "Studenci wynajęli tanią <w>norę</w> na poddaszu starej kamienicy.",
                            "target_lang_translations": {
                                "ru": "Студенты сняли дешёвую <w>нору</w> на чердаке старого дома."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ciemna nora",
                        "śmierdząca nora",
                        "mieszkać w norze"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тёмное, грязное, запущенное или очень тесное жилое помещение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нора",
                                "target_lang_sense_clarification": "убогое жилище"
                            },
                            {
                                "target_lang_word": "дыра",
                                "target_lang_sense_clarification": "разг. плохое место"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a1023f23-5776-3fde-939c-ac7c9f2eb81c",
                    "sense_definition": "Imię żeńskie.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Z upartą cierpliwością krążył dokoła <w>Nory</w>, czekając na jej przyzwolenie.",
                            "target_lang_translations": {
                                "ru": "С упрямым терпением он кружил вокруг <w>Норы</w>, ожидая её позволения."
                            }
                        },
                        {
                            "text": "Moja koleżanka <w>Nora</w> uwielbia dalekie podróże i wspinaczkę.",
                            "target_lang_translations": {
                                "ru": "Моя подруга <w>Нора</w> обожает дальние путешествия и скалолазание."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Нора",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
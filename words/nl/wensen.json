    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6f90be7b-96ff-3e8a-8cc5-30a8daf02871",
                    "sense_definition": "De hoop uitspreken dat iemand iets, meestal iets goeds, zal ervaren of ontvangen",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "wish_someone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>wens</w> je een heel fijne verjaardag en veel plezier.",
                            "target_lang_translations": {
                                "en": "I <w>wish</w> you a very happy birthday and lots of fun.",
                                "ru": "Я <w>желаю</w> тебе чудесного дня рождения и хорошо повеселиться."
                            }
                        },
                        {
                            "text": "Wij <w>wensen</w> jullie veel succes met de nieuwe baan.",
                            "target_lang_translations": {
                                "en": "We <w>wish</w> you good luck with the new job.",
                                "ru": "Мы <w>желаем</w> вам больших успехов на новой работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "toewensen"
                    ],
                    "common_phrases": [
                        "iemand geluk wensen",
                        "iemand het beste wensen",
                        "beterschap wensen"
                    ],
                    "target_lang_definitions": {
                        "en": "To express the hope that someone will experience or receive something, usually something good",
                        "ru": "Выражать надежду на то, что кто-то испытает или получит что-то, обычно хорошее"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wish",
                                "target_lang_sense_clarification": "to hope someone receives something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "выражать добрые пожелания кому-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5987e37a-c597-3ee4-b292-1440c8ad422d",
                    "sense_definition": "Graag willen dat iets gebeurt of dat men iets krijgt; verlangen naar iets",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "desire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directie <w>wenst</w> niet gestoord te worden tijdens de vergadering.",
                            "target_lang_translations": {
                                "en": "The management does not <w>wish</w> to be disturbed during the meeting.",
                                "ru": "Руководство <w>желает</w>, чтобы его не беспокоили во время собрания."
                            }
                        },
                        {
                            "text": "De kwaliteit van het eten liet helaas te <w>wensen</w> over.",
                            "target_lang_translations": {
                                "en": "Unfortunately, the quality of the food left something to be <w>desired</w>.",
                                "ru": "Качество еды, к сожалению, оставляло <w>желать</w> лучшего."
                            }
                        }
                    ],
                    "synonyms": [
                        "verlangen",
                        "willen",
                        "begeren",
                        "hopen"
                    ],
                    "common_phrases": [
                        "te wensen overlaten"
                    ],
                    "target_lang_definitions": {
                        "en": "To want something to happen or to receive something; to long for something",
                        "ru": "Хотеть, чтобы что-то произошло, или стремиться получить что-либо"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wish",
                                "target_lang_sense_clarification": "formal; to want"
                            },
                            {
                                "target_lang_word": "desire",
                                "target_lang_sense_clarification": "to want earnestly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "хотеть, стремиться (формально)"
                            },
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "иметь намерение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

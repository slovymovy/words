{
    "word_family": [
        "jawoord",
        "jaknikker"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "18313130-dea2-3f63-8840-b74a11a27eb9",
                    "sense_definition": "Wordt gebruikt om een vraag bevestigend te beantwoorden of om instemming en toestemming uit te drukken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil je met ons meegaan? — <w>Ja</w>, graag!",
                            "target_lang_translations": {
                                "en": "Do you want to come along with us? — <w>Yes</w>, please!",
                                "ru": "Хочешь пойти с нами? — <w>Да</w>, с удовольствием!"
                            }
                        },
                        {
                            "text": "Heeft hij dat echt gezegd? <w>Ja</w>.",
                            "target_lang_translations": {
                                "en": "Did he really say that? <w>Yes</w>.",
                                "ru": "Он правда это сказал? <w>Да</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jep"
                    ],
                    "antonyms": [
                        "nee",
                        "neen"
                    ],
                    "common_phrases": [
                        "ja natuurlijk",
                        "ja graag",
                        "ja hoor",
                        "ja en amen"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to answer a question affirmatively or to express agreement and permission.",
                        "ru": "Используется для утвердительного ответа на вопрос или для выражения согласия и разрешения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "yes",
                                "target_lang_sense_clarification": "standard affirmation"
                            },
                            {
                                "target_lang_word": "yeah",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "yep",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "утверждение"
                            },
                            {
                                "target_lang_word": "ага",
                                "target_lang_sense_clarification": "разговорное согласие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "a5123940-9966-3392-9b77-658ada9d9a1f",
                    "sense_definition": "Een uitroep van vreugde, enthousiasme of overwinning.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ja</w>! We hebben de wedstrijd gewonnen!",
                            "target_lang_translations": {
                                "en": "<w>Yes</w>! We won the match!",
                                "ru": "<w>Да</w>! Мы выиграли матч!"
                            }
                        },
                        {
                            "text": "<w>Ja</w>! Het is eindelijk gelukt.",
                            "target_lang_translations": {
                                "en": "<w>Yes</w>! It finally worked.",
                                "ru": "<w>Да</w>! Наконец-то получилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "yes"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation of joy, enthusiasm, or victory.",
                        "ru": "Возглас радости, энтузиазма или победы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "yes",
                                "target_lang_sense_clarification": "exclamation of joy"
                            },
                            {
                                "target_lang_word": "yeah",
                                "target_lang_sense_clarification": "exclamation of joy"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "возглас радости"
                            },
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "выражение успеха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04932ddf-60a1-32f9-a0a4-38ad47933924",
                    "sense_definition": "Wordt gebruikt als stopwoord om een gedachte af te sluiten, berusting uit te drukken of een correctie in te leiden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "discourse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was niet perfect, maar <w>ja</w>, wat doe je eraan?",
                            "target_lang_translations": {
                                "en": "It wasn't perfect, but <w>yeah</w>, what can you do?",
                                "ru": "Это было не идеально, но <w>да</w>, что поделаешь?"
                            }
                        },
                        {
                            "text": "Nou <w>ja</w>, laten we het maar vergeten.",
                            "target_lang_translations": {
                                "en": "<w>Well</w>, let's just forget about it.",
                                "ru": "Ну <w>да</w>, давай просто забудем об этом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nou ja",
                        "ach ja",
                        "maar ja"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used as a filler to close a thought, express resignation, or introduce a correction.",
                        "ru": "Используется как слово-паразит для завершения мысли, выражения смирения или введения поправки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "resignation or transition"
                            },
                            {
                                "target_lang_word": "yeah",
                                "target_lang_sense_clarification": "resignation or filler"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ну да",
                                "target_lang_sense_clarification": "выражение смирения"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "слово-паразит"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87b18842-94e4-30e1-819c-29bf10a83978",
                    "sense_definition": "Wordt vragend gebruikt wanneer men iets niet goed verstaan of begrepen heeft.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "query",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ja</w>? Wat zei je net?",
                            "target_lang_translations": {
                                "en": "<w>Sorry</w>? What did you just say?",
                                "ru": "<w>Да</w>? Что ты только что сказал?"
                            }
                        },
                        {
                            "text": "Ik verstond je niet goed, <w>ja</w>?",
                            "target_lang_translations": {
                                "en": "I didn't hear you well, <w>okay</w>?",
                                "ru": "Я тебя плохо расслышал, <w>да</w>?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used interrogatively when one has not heard or understood something well.",
                        "ru": "Используется вопросительно, когда человек что-то не расслышал или не понял."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sorry",
                                "target_lang_sense_clarification": "request for repetition"
                            },
                            {
                                "target_lang_word": "pardon",
                                "target_lang_sense_clarification": "request for repetition"
                            },
                            {
                                "target_lang_word": "yes",
                                "target_lang_sense_clarification": "interrogative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "просьба повторить"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "вопросительный отклик"
                            },
                            {
                                "target_lang_word": "простите",
                                "target_lang_sense_clarification": "вежливый переспрос"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "62eb9c42-2c85-3cd1-bbc8-9dd82d416d6d",
                    "sense_definition": "Een bevestigend antwoord of een stem vóór iets.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nee heb je al, <w>ja</w> kan je krijgen.",
                            "target_lang_translations": {
                                "en": "You already have a no, you can get a <w>yes</w>.",
                                "ru": "«Нет» у тебя уже есть, а «<w>да</w>» ты можешь получить."
                            }
                        },
                        {
                            "text": "Hij antwoordde met een volmondig <w>ja</w>.",
                            "target_lang_translations": {
                                "en": "He answered with a wholehearted <w>yes</w>.",
                                "ru": "Он ответил категоричным <w>да</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "jawoord"
                    ],
                    "antonyms": [
                        "nee"
                    ],
                    "common_phrases": [
                        "het jawoord"
                    ],
                    "target_lang_definitions": {
                        "en": "An affirmative answer or a vote in favor of something.",
                        "ru": "Утвердительный ответ или голос «за»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "yes",
                                "target_lang_sense_clarification": "noun"
                            },
                            {
                                "target_lang_word": "aye",
                                "target_lang_sense_clarification": "affirmative vote"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "8d845249-e136-3e6c-a2e1-97436b915fc3",
                    "sense_definition": "Een verkorte vorm van de naam van God (Jahweh) zoals gebruikt in de Bijbel.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "deity_name",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Halleluja betekent letterlijk: prijs <w>Ja</w>.",
                            "target_lang_translations": {
                                "en": "Hallelujah literally means: praise <w>Jah</w>.",
                                "ru": "Аллилуйя буквально означает: хвалите <w>Ях</w>."
                            }
                        },
                        {
                            "text": "In oude psalmen wordt de naam <w>Ja</w> soms gebruikt.",
                            "target_lang_translations": {
                                "en": "In old psalms the name <w>Jah</w> is sometimes used.",
                                "ru": "В старых псалмах иногда используется имя <w>Ях</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A shortened form of the name of God (Yahweh) as used in the Bible.",
                        "ru": "Сокращенная форма имени Бога (Яхве), используемая в Библии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Jah",
                                "target_lang_sense_clarification": "religious name"
                            },
                            {
                                "target_lang_word": "Yah",
                                "target_lang_sense_clarification": "religious name"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Ях",
                                "target_lang_sense_clarification": "религиозное имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]

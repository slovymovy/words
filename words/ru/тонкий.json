    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "987b3161-bd8b-3ee5-ba6c-b00a22a0c332",
                    "sense_definition": "Имеющий малую толщину, небольшое расстояние между двумя поверхностями; противоположный толстому.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_dimension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На озере ещё очень <w>тонкий</w> лёд, поэтому ходить по нему опасно.",
                            "target_lang_translations": {
                                "en": "The ice on the lake is still very <w>thin</w>, so it is dangerous to walk on it."
                            }
                        },
                        {
                            "text": "Она порезала сыр на <w>тонкие</w> кусочки для бутербродов.",
                            "target_lang_translations": {
                                "en": "She cut the cheese into <w>thin</w> slices for sandwiches."
                            }
                        }
                    ],
                    "antonyms": [
                        "толстый"
                    ],
                    "common_phrases": [
                        "тонкий лёд",
                        "тонкий слой",
                        "тонкая книга",
                        "тонкая нить"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a small distance between opposite surfaces; not thick."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thin",
                                "target_lang_sense_clarification": "General term for having little thickness"
                            },
                            {
                                "target_lang_word": "fine",
                                "target_lang_sense_clarification": "Very thin, often used for threads, layers, or powders"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f289aa5-88a9-3643-8638-2aa8de6aba71",
                    "sense_definition": "Стройный, худощавый, не имеющий лишнего веса (о человеке или частях тела).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "body_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У балерины была очень <w>тонкая</w> талия и изящная фигура.",
                            "target_lang_translations": {
                                "en": "The ballerina had a very <w>slender</w> waist and a graceful figure."
                            }
                        },
                        {
                            "text": "Его <w>тонкие</w> пальцы быстро бегали по клавишам пианино.",
                            "target_lang_translations": {
                                "en": "His <w>slender</w> fingers ran quickly over the piano keys."
                            }
                        }
                    ],
                    "synonyms": [
                        "худой",
                        "тощий"
                    ],
                    "antonyms": [
                        "толстый",
                        "полный",
                        "жирный"
                    ],
                    "common_phrases": [
                        "тонкая талия",
                        "тонкие пальцы",
                        "тонкие ноги",
                        "тонкая шея"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a slim physique with little body fat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slender",
                                "target_lang_sense_clarification": "Gracefully thin"
                            },
                            {
                                "target_lang_word": "thin",
                                "target_lang_sense_clarification": "Having little flesh or fat"
                            },
                            {
                                "target_lang_word": "slim",
                                "target_lang_sense_clarification": "Attractively thin"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4664f74-4f33-3bcf-9ea9-f85bb2747ef4",
                    "sense_definition": "Изысканный, нежный, приятный и благородный (о запахе, вкусе или манерах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "refined_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В комнате чувствовался <w>тонкий</w> аромат дорогих духов.",
                            "target_lang_translations": {
                                "en": "A <w>subtle</w> fragrance of expensive perfume could be felt in the room."
                            }
                        },
                        {
                            "text": "У этого вина очень <w>тонкий</w> вкус, который оценят только знатоки.",
                            "target_lang_translations": {
                                "en": "This wine has a very <w>delicate</w> taste that only connoisseurs will appreciate."
                            }
                        }
                    ],
                    "synonyms": [
                        "утончённый",
                        "деликатный"
                    ],
                    "antonyms": [
                        "грубый"
                    ],
                    "common_phrases": [
                        "тонкий аромат",
                        "тонкий вкус",
                        "тонкий запах"
                    ],
                    "target_lang_definitions": {
                        "en": "Pleasingly understated, intricate, or gentle (of smells, tastes, or manners)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "subtle",
                                "target_lang_sense_clarification": "Not obvious; understated and refined"
                            },
                            {
                                "target_lang_word": "delicate",
                                "target_lang_sense_clarification": "Pleasingly soft, faint, or fragile"
                            },
                            {
                                "target_lang_word": "refined",
                                "target_lang_sense_clarification": "Showing sophistication and good taste"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44725075-17ff-338e-bb83-f370ea07ee04",
                    "sense_definition": "Высокий по звучанию, иногда пронзительный (о голосе или звуке).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из детской комнаты доносился <w>тонкий</w> голосок моей сестры.",
                            "target_lang_translations": {
                                "en": "My sister's <w>thin</w> little voice came from the nursery."
                            }
                        },
                        {
                            "text": "В тишине раздался неприятный <w>тонкий</w> писк комара.",
                            "target_lang_translations": {
                                "en": "The unpleasant <w>high-pitched</w> squeak of a mosquito rang out in the silence."
                            }
                        }
                    ],
                    "synonyms": [
                        "высокий"
                    ],
                    "antonyms": [
                        "низкий",
                        "грубый"
                    ],
                    "common_phrases": [
                        "тонкий голос",
                        "тонкий звук",
                        "тонкий писк"
                    ],
                    "target_lang_definitions": {
                        "en": "High in frequency or pitch; lacking resonance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "high-pitched",
                                "target_lang_sense_clarification": "High in frequency"
                            },
                            {
                                "target_lang_word": "thin",
                                "target_lang_sense_clarification": "Lacking volume or resonance"
                            },
                            {
                                "target_lang_word": "high",
                                "target_lang_sense_clarification": "High in pitch"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c71430b4-dbb5-3a9c-bd38-a2079677d9f9",
                    "sense_definition": "Умный, проницательный, скрытый или требующий особого понимания (о намёке, юморе, уме).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это был очень <w>тонкий</w> намёк, и я не сразу понял его смысл.",
                            "target_lang_translations": {
                                "en": "It was a very <w>subtle</w> hint, and I didn't immediately understand its meaning."
                            }
                        },
                        {
                            "text": "Автор книги использует <w>тонкий</w> юмор, чтобы высмеять пороки общества.",
                            "target_lang_translations": {
                                "en": "The author uses <w>subtle</w> humor to ridicule the vices of society."
                            }
                        }
                    ],
                    "antonyms": [
                        "грубый",
                        "топорный"
                    ],
                    "common_phrases": [
                        "тонкий намёк",
                        "тонкий юмор",
                        "тонкий ум",
                        "тонкая ирония"
                    ],
                    "target_lang_definitions": {
                        "en": "Clever, indirect, and not immediately obvious."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "subtle",
                                "target_lang_sense_clarification": "Difficult to perceive; clever and indirect"
                            },
                            {
                                "target_lang_word": "fine",
                                "target_lang_sense_clarification": "Involving very small distinctions or details"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72f872db-7236-32e5-9468-9f4ca8fe48c2",
                    "sense_definition": "Очень чувствительный, способный различать малейшие детали (о слухе, зрении, интуиции).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "senses_keen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Музыкант должен иметь <w>тонкий</w> слух, чтобы слышать фальшивые ноты.",
                            "target_lang_translations": {
                                "en": "A musician must have a <w>fine</w> ear to hear false notes."
                            }
                        },
                        {
                            "text": "Благодаря <w>тонкому</w> чутью детектив быстро нашёл преступника.",
                            "target_lang_translations": {
                                "en": "Thanks to <w>keen</w> intuition, the detective quickly found the criminal."
                            }
                        }
                    ],
                    "common_phrases": [
                        "тонкий слух",
                        "тонкое чутьё",
                        "тонкое наблюдение"
                    ],
                    "target_lang_definitions": {
                        "en": "Highly sensitive and able to perceive small details."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keen",
                                "target_lang_sense_clarification": "Sharp and highly sensitive (especially of senses)"
                            },
                            {
                                "target_lang_word": "fine",
                                "target_lang_sense_clarification": "Able to distinguish minute details"
                            },
                            {
                                "target_lang_word": "delicate",
                                "target_lang_sense_clarification": "Highly sensitive to small changes"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
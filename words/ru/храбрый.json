    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a4043528-9e7f-3f7e-a552-46b043189b21",
                    "sense_definition": "Действующий решительно, не проявляя страха, не боясь опасности.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш <w>храбрый</w> солдат защищал родину до последнего дня.",
                            "target_lang_translations": {
                                "en": "Our <w>brave</w> soldier defended the homeland until the last day."
                            }
                        },
                        {
                            "text": "Только по-настоящему <w>храбрый</w> человек не боится говорить правду.",
                            "target_lang_translations": {
                                "en": "Only a truly <w>brave</w> person is not afraid to speak the truth."
                            }
                        }
                    ],
                    "synonyms": [
                        "смелый",
                        "отважный",
                        "бесстрашный",
                        "мужественный"
                    ],
                    "antonyms": [
                        "трусливый",
                        "робкий",
                        "нерешительный"
                    ],
                    "common_phrases": [
                        "храбрый воин",
                        "храбрый человек",
                        "самый храбрый",
                        "будь храбрым"
                    ],
                    "target_lang_definitions": {
                        "en": "Acting decisively without showing fear; not afraid of danger."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brave",
                                "target_lang_sense_clarification": "willing to face and endure danger or pain"
                            },
                            {
                                "target_lang_word": "courageous",
                                "target_lang_sense_clarification": "showing mental or moral strength"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b84ca4c1-39eb-3349-8ede-60803c6e920b",
                    "sense_definition": "Свидетельствующий о бесстрашии, выражающий отвагу.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это был честный и <w>храбрый</w> поступок, которым можно гордиться.",
                            "target_lang_translations": {
                                "en": "It was an honest and <w>brave</w> act to be proud of."
                            }
                        },
                        {
                            "text": "У него доброе и <w>храброе</w> сердце, готовое помочь каждому.",
                            "target_lang_translations": {
                                "en": "He has a kind and <w>brave</w> heart, ready to help everyone."
                            }
                        }
                    ],
                    "synonyms": [
                        "смелый",
                        "решительный"
                    ],
                    "antonyms": [
                        "трусливый"
                    ],
                    "common_phrases": [
                        "храброе сердце",
                        "храбрый поступок",
                        "храбрый вид"
                    ],
                    "target_lang_definitions": {
                        "en": "Demonstrating fearlessness or expressing courage (of actions or qualities)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brave",
                                "target_lang_sense_clarification": "characterized by courage"
                            },
                            {
                                "target_lang_word": "courageous",
                                "target_lang_sense_clarification": "of an action or quality that requires courage"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a3efd4b1-bb70-3a7b-a0ac-4284a63037e2",
                    "sense_definition": "Действие по сбору сведений о противнике, местности или выяснение чего-либо ранее неизвестного.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам нужно произвести <разведку> и узнать, что они замышляют.",
                            "target_lang_translations": {
                                "en": "We need to conduct <reconnaissance> and find out what they are plotting."
                            }
                        },
                        {
                            "text": "Командир отправил группу в <разведку>, чтобы выяснить расположение вражеских сил.",
                            "target_lang_translations": {
                                "en": "The commander sent a group on <reconnaissance> to determine the location of enemy forces."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пойти в разведку",
                        "разведка боем",
                        "вести разведку",
                        "воздушная разведка"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of gathering information about an enemy or an area, typically for military purposes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reconnaissance",
                                "target_lang_sense_clarification": "military observation"
                            },
                            {
                                "target_lang_word": "scouting",
                                "target_lang_sense_clarification": "gathering information"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ea3b758-95de-3886-a063-f3957282dac4",
                    "sense_definition": "Военное подразделение или государственная служба, занимающаяся сбором секретной информации.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он — фронтовик, служил в <разведке>, был командиром взвода.",
                            "target_lang_translations": {
                                "en": "He is a veteran, served in <intelligence>, and was a platoon commander."
                            }
                        },
                        {
                            "text": "Иностранная <разведка> пыталась получить доступ к закрытым государственным архивам.",
                            "target_lang_translations": {
                                "en": "Foreign <intelligence> tried to gain access to classified state archives."
                            }
                        }
                    ],
                    "common_phrases": [
                        "служить в разведке",
                        "военная разведка",
                        "внешняя разведка",
                        "офицер разведки"
                    ],
                    "target_lang_definitions": {
                        "en": "A government organization or military unit responsible for collecting secret information about enemies or other countries."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "intelligence",
                                "target_lang_sense_clarification": "agency or service"
                            },
                            {
                                "target_lang_word": "intelligence service",
                                "target_lang_sense_clarification": "organization"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f214d7ed-1bb3-3ac2-b279-01d6dbcabadb",
                    "sense_definition": "Поиск и обследование месторождений полезных ископаемых или природных ресурсов.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Геологическая <разведка> подтвердила наличие больших запасов нефти в этом регионе.",
                            "target_lang_translations": {
                                "en": "Geological <exploration> confirmed the presence of large oil reserves in this region."
                            }
                        },
                        {
                            "text": "Компания выделила значительные средства на <разведку> новых золотых месторождений.",
                            "target_lang_translations": {
                                "en": "The company allocated significant funds for the <exploration> of new gold deposits."
                            }
                        }
                    ],
                    "common_phrases": [
                        "геологическая разведка",
                        "разведка недр",
                        "разведка месторождения"
                    ],
                    "target_lang_definitions": {
                        "en": "The activity of searching an area for natural resources, such as minerals or oil."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exploration",
                                "target_lang_sense_clarification": "geological search"
                            },
                            {
                                "target_lang_word": "prospecting",
                                "target_lang_sense_clarification": "search for minerals"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
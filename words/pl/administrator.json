            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc39f594-273e-3910-b680-f2a19a224c45",
                    "sense_definition": "Osoba zarządzająca czymś, na przykład budynkiem, majątkiem, instytucją lub zbiorem danych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "management",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy <w>administrator</w> budynku zadbał o szybką naprawę zepsutej windy.",
                            "target_lang_translations": {
                                "ru": "Новый <w>управляющий</w> зданием позаботился о быстром ремонте сломанного лифта."
                            }
                        },
                        {
                            "text": "Jako <w>administrator</w> danych osobowych, firma musi przestrzegać surowych przepisów prawnych.",
                            "target_lang_translations": {
                                "ru": "Как <w>администратор</w> персональных данных, компания должна соблюдать строгие правовые нормы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zarządca"
                    ],
                    "common_phrases": [
                        "administrator budynku",
                        "administrator danych",
                        "administrator nieruchomości",
                        "administrator masy-upadłościowej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, управляющее чем-либо, например зданием, имуществом, учреждением или массивом данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "администратор",
                                "target_lang_sense_clarification": "должностное лицо"
                            },
                            {
                                "target_lang_word": "управляющий",
                                "target_lang_sense_clarification": "зданием или имуществом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "544c38fd-e5c5-34e0-8b9f-63a274ad9710",
                    "sense_definition": "Osoba nadzorująca system komputerowy, sieć lub serwis internetowy, odpowiedzialna za ich sprawne działanie i uprawnienia użytkowników.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "it",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Główny <w>administrator</w> sieci zablokował dostęp do podejrzanych stron internetowych.",
                            "target_lang_translations": {
                                "ru": "Главный <w>администратор</w> сети заблокировал доступ к подозрительным веб-сайтам."
                            }
                        },
                        {
                            "text": "Jeśli zapomniałeś hasła, musisz skontaktować się z <w>administratorem</w> systemu.",
                            "target_lang_translations": {
                                "ru": "Если вы забыли пароль, нужно связаться с системным <w>администратором</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "administrator sieci",
                        "administrator systemu",
                        "prawa administratora",
                        "konto administratora"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист, контролирующий компьютерную систему, сеть или веб-сайт, отвечающий за их исправную работу и права пользователей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "администратор",
                                "target_lang_sense_clarification": "IT-специалист"
                            },
                            {
                                "target_lang_word": "системный администратор",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c5fd70e-9782-365a-ad0c-31ee2db2cbac",
                    "sense_definition": "Dawne określenie osoby wykonującej wyroki sądowe, w tym karę śmierci.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych kronikach zapisano, że <w>administrator</w> wykonał wyrok na głównym placu miasta.",
                            "target_lang_translations": {
                                "ru": "В старинных хрониках записано, что <w>палач</w> привел приговор в исполнение на главной площади города."
                            }
                        },
                        {
                            "text": "Skazaniec ze strachem spoglądał na zbliżającego się <w>administratora</w>.",
                            "target_lang_translations": {
                                "ru": "Осужденный со страхом смотрел на приближающегося <w>палача</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kat"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historyczne znaczenie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название лица, приводящего в исполнение судебные приговоры, в том числе смертную казнь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "палач",
                                "target_lang_sense_clarification": "историческое значение"
                            },
                            {
                                "target_lang_word": "исполнитель приговора",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "rower",
        "rowerowy",
        "rowerzysta",
        "rowerzystka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b9046b2f-87cb-3e46-8345-08a4abd112ca",
                    "sense_definition": "Zdrobnienie od słowa <w>rower</w>; mały rower przeznaczony dla dzieci.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko jechało na <rowerku>, a matka szła tuż za nim.",
                            "target_lang_translations": {
                                "ru": "Ребёнок ехал на <велосипедике>, а мама шла прямо за ним."
                            }
                        },
                        {
                            "text": "Mój syn dostał na urodziny swój pierwszy <rowerek> biegowy.",
                            "target_lang_translations": {
                                "ru": "Мой сын получил на день рождения свой первый <беговел>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rowerek biegowy",
                        "rowerek trójkołowy",
                        "jeździć na rowerku"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова «велосипед»; маленький велосипед, предназначенный для детей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велосипедик",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            },
                            {
                                "target_lang_word": "детский велосипед",
                                "target_lang_sense_clarification": "велосипед для детей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89773b57-0394-3642-8b23-ee964009cf97",
                    "sense_definition": "Urządzenie treningowe wyglądające jak rower, które służy do ćwiczeń w miejscu, a nie do przemieszczania się.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą zamiast biegać na zewnątrz, wolę pojeździć na <rowerku> w domu.",
                            "target_lang_translations": {
                                "ru": "Зимой вместо бега на улице я предпочитаю покататься на <велотренажёре> дома."
                            }
                        },
                        {
                            "text": "Na siłowni wszystkie <rowerki> były zajęte przez ćwiczących.",
                            "target_lang_translations": {
                                "ru": "В спортзале все <велотренажёры> были заняты тренирующимися."
                            }
                        }
                    ],
                    "synonyms": [
                        "rower stacjonarny",
                        "ergometr rowerowy"
                    ],
                    "common_phrases": [
                        "rowerek stacjonarny",
                        "ćwiczyć na rowerku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тренировочное устройство, похожее на велосипед, которое используется для упражнений на месте, а не для передвижения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велотренажёр",
                                "target_lang_sense_clarification": "спортивный снаряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ce300f7-228c-366a-a468-9539aad34727",
                    "sense_definition": "Ćwiczenie gimnastyczne wykonywane w leżeniu na plecach, polegające na naprzemiennym poruszaniu nogami w sposób imitujący pedałowanie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "exercise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Instruktor kazał nam robić <rowerek> przez dwie minuty bez przerwy.",
                            "target_lang_translations": {
                                "ru": "Инструктор велел нам делать <велосипед> в течение двух минут без перерыва."
                            }
                        },
                        {
                            "text": "Regularne wykonywanie <rowerka> pomaga wzmocnić mięśnie brzucha.",
                            "target_lang_translations": {
                                "ru": "Регулярное выполнение <велосипеда> помогает укрепить мышцы живота."
                            }
                        }
                    ],
                    "common_phrases": [
                        "robić rowerek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гимнастическое упражнение, выполняемое лёжа на спине, заключающееся в попеременном движении ногами, имитирующем вращение педалей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велосипед",
                                "target_lang_sense_clarification": "упражнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3baeb9d3-7e4a-3b1f-9d0c-888d7814b2f4",
                    "sense_definition": "W żargonie tenisowym: wynik meczu, w którym jeden z zawodników wygrywa dwa sety do zera (6:0, 6:0).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sport_score",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mecz zakończył się upokarzającym <rowerkiem> dla młodego zawodnika.",
                            "target_lang_translations": {
                                "ru": "Матч закончился унизительной <двойной баранкой> для молодого игрока."
                            }
                        },
                        {
                            "text": "Nikt nie spodziewał się, że faworyt turnieju dostanie <rowerek> w finale.",
                            "target_lang_translations": {
                                "ru": "Никто не ожидал, что фаворит турнира получит <двойную баранку> в финале."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dostać rowerek"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "sport (tennis)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В теннисном жаргоне: результат матча, в котором один из игроков выигрывает два сета всухую (6:0, 6:0)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двойная баранка",
                                "target_lang_sense_clarification": "теннисный сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
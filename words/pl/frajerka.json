    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "15463d6e-510f-3023-8119-6fb6b9064947",
                    "sense_definition": "Kobieta naiwna, łatwowierna, niezaradna życiowo, którą łatwo oszukać lub wykorzystać.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "naive_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź taką <w>frajerką</w> i nie pożyczaj pieniędzy nieznajomym.",
                            "target_lang_translations": {
                                "ru": "Не будь такой <w>лохушкой</w> и не одалживай деньги незнакомцам."
                            }
                        },
                        {
                            "text": "Myślał, że trafiła mu się łatwa <w>frajerka</w>, ale szybko wyprowadziła go z błędu.",
                            "target_lang_translations": {
                                "ru": "Он думал, что ему попалась легкая добыча (<w>лохушка</w>), но она быстро вывела его из заблуждения."
                            }
                        }
                    ],
                    "synonyms": [
                        "naiwniara"
                    ],
                    "antonyms": [
                        "cwaniara"
                    ],
                    "common_phrases": [
                        "wyjść na frajerkę",
                        "robić z siebie frajerkę",
                        "szukać frajerki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potoczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наивная, доверчивая женщина, не приспособленная к жизни, которую легко обмануть или использовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лохушка",
                                "target_lang_sense_clarification": "разг., о наивной женщине, которую легко обмануть"
                            },
                            {
                                "target_lang_word": "простофиля",
                                "target_lang_sense_clarification": "о простодушном, несообразительном человеке"
                            },
                            {
                                "target_lang_word": "дурочка",
                                "target_lang_sense_clarification": "наивная, глупая женщина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "693536fe-90ad-385c-875b-304b043cb9b7",
                    "sense_definition": "Kobieta pozostająca z kimś w nieformalnym związku uczuciowym.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "partner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego wierna <w>frajerka</w> czekała na niego pod bramą więzienia przez wiele lat.",
                            "target_lang_translations": {
                                "ru": "Его верная <w>маруха</w> ждала его у ворот тюрьмы много лет."
                            }
                        },
                        {
                            "text": "Harnasia zdradziła własna <w>frajerka</w> i wydała go w ręce wrogów.",
                            "target_lang_translations": {
                                "ru": "Харнася предала собственная <w>любовница</w> и выдала его в руки врагов."
                            }
                        }
                    ],
                    "synonyms": [
                        "naiwniara"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gwara więzienna lub środowiskowa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, состоящая с кем-либо в неформальных любовных отношениях (часто в криминальном жаргоне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маруха",
                                "target_lang_sense_clarification": "крим. жаргон, любовница преступника"
                            },
                            {
                                "target_lang_word": "любовница",
                                "target_lang_sense_clarification": "женщина в неформальной связи"
                            },
                            {
                                "target_lang_word": "подруга",
                                "target_lang_sense_clarification": "разг., девушка, с которой встречаются"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портал",
                                "target_lang_sense_clarification": "спорт"
                            },
                            {
                                "target_lang_word": "трансферный портал",
                                "target_lang_sense_clarification": "база данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00268fe9-d0bb-33f1-9bd8-704ecfc12b78",
                    "sense_definition": "A short vein that carries blood into the liver.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>portal</w> vein is responsible for directing blood from the digestive tract to the liver.",
                            "target_lang_translations": {
                                "ru": "<w>Воротная</w> вена отвечает за направление крови от пищеварительного тракта к печени."
                            }
                        },
                        {
                            "text": "Doctors monitored the pressure in the patient's <w>portal</w> system carefully.",
                            "target_lang_translations": {
                                "ru": "Врачи тщательно следили за давлением в <w>воротной</w> системе пациента."
                            }
                        }
                    ],
                    "common_phrases": [
                        "portal vein"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "anatomy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая вена, несущая кровь в печень."

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "239b66b5-9246-3ed9-b82c-7d32aed74694",
                    "sense_definition": "Een hard omhulsel van gipsverband dat om een gebroken lichaamsdeel wordt aangebracht om het te laten genezen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "medical_cast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer zit zes weken in het <w>gips</w>.",
                            "target_lang_translations": {
                                "en": "My brother is in a <w>cast</w> for six weeks.",
                                "ru": "Мой брат шесть недель в <w>гипсе</w>."
                            }
                        },
                        {
                            "text": "De dokter zal morgen het <w>gips</w> van mijn been verwijderen.",
                            "target_lang_translations": {
                                "en": "The doctor will remove the <w>cast</w> from my leg tomorrow.",
                                "ru": "Завтра врач снимет <w>гипс</w> с моей ноги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in het gips",
                        "gips krijgen"
                    ],
                    "target_lang_definitions": {
                        "en": "A hard casing made of plaster bandage applied around a broken body part to allow it to heal.",
                        "ru": "Твердая повязка из гипсового бинта, которая накладывается на сломанную часть тела для фиксации и заживления."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cast",
                                "target_lang_sense_clarification": "medical casing for broken bones"
                            },
                            {
                                "target_lang_word": "plaster cast",
                                "target_lang_sense_clarification": "full term for medical casing"
                            },
                            {
                                "target_lang_word": "plaster",
                                "target_lang_sense_clarification": "British English common usage"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гипс",
                                "target_lang_sense_clarification": "медицинская повязка"
                            },
                            {
                                "target_lang_word": "гипсовая повязка",
                                "target_lang_sense_clarification": "формальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57a09f8b-bddc-3a2b-bfbe-fd4bc73b6640",
                    "sense_definition": "Een wit poeder of mineraal dat hard wordt na menging met water, veel gebruikt als bouwmateriaal of voor het maken van beelden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kunstenaar maakte een prachtig beeld van <w>gips</w>.",

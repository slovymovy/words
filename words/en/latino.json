            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e3300fbf-7589-342c-b889-a245f3f711d1",
                    "sense_definition": "A person, especially a man, who is from or has ancestors from Latin America.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a proud <w>Latino</w> who values his cultural heritage.",
                            "target_lang_translations": {
                                "ru": "Он гордый <w>латиноамериканец</w>, который ценит свое культурное наследие."
                            }
                        },
                        {
                            "text": "Many <w>Latinos</w> have made significant contributions to the local economy.",
                            "target_lang_translations": {
                                "ru": "Многие <w>латиноамериканцы</w> внесли значительный вклад в местную экономику."
                            }
                        }
                    ],
                    "synonyms": [
                        "Hispanic"
                    ],
                    "common_phrases": [
                        "proud Latino",
                        "young Latino"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "ba1e053d-51ff-36be-a5d3-903327d32fbb",
                    "sense_definition": "Spójnik łączący wyrazy lub zdania, wyrażający alternatywę; wskazuje, że z wymienionych możliwości zachodzi lub jest wybierana jedna (często wykluczająca inne).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Możesz wybrać na deser lody <w>albo</w> ciasto czekoladowe.",
                            "target_lang_translations": {
                                "ru": "Ты можешь выбрать на десерт мороженое <w>или</w> шоколадный торт."
                            }
                        },
                        {
                            "text": "W ten weekend pójdziemy do kina <w>albo</w> zostaniemy w domu.",
                            "target_lang_translations": {
                                "ru": "В эти выходные мы пойдем в кино <w>или</w> останемся дома."
                            }
                        },
                        {
                            "text": "Musisz się zdecydować: <w>albo</w> wóz, <w>albo</w> przewóz.",
                            "target_lang_translations": {
                                "ru": "Ты должен решить: <w>либо</w> пан, <w>либо</w> пропал."
                            }
                        }
                    ],
                    "synonyms": [
                        "lub",
                        "bądź",
                        "czy"
                    ],
                    "antonyms": [
                        "i",
                        "oraz",
                        "ani"
                    ],
                    "common_phrases": [
                        "albo-albo",
                        "albo wóz-albo przewóz",
                        "tak albo siak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, соединяющий слова или предложения, выражающий альтернативу; указывает на то, что из перечисленных возможностей происходит или выбирается одна (часто исключающая другие)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "или",
                                "target_lang_sense_clarification": "союз, выражающий выбор"
                            },
                            {
                                "target_lang_word": "либо",
                                "target_lang_sense_clarification": "вариант, часто взаимоисключающий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "6c97881a-14db-3e3d-9d19-14fb589085bc",
                    "sense_definition": "Partykuła wprowadzająca pytanie, często retoryczne, wyrażająca zdziwienie, oburzenie, powątpiewanie lub ironię.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Albo</w> ja wiem, czy to dobry pomysł?",
                            "target_lang_translations": {
                                "ru": "<w>Разве</w> я знаю, хорошая ли это идея?"
                            }
                        },
                        {
                            "text": "<w>Albo</w> to ładnie tak oszukiwać swoich przyjaciół?",
                            "target_lang_translations": {
                                "ru": "<w>Разве</w> это красиво так обманывать своих друзей?"
                            }
                        }
                    ],
                    "synonyms": [
                        "czy",
                        "czyż",
                        "alboż"
                    ],
                    "common_phrases": [

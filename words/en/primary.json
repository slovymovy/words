                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основной",
                                "target_lang_sense_clarification": "наиболее важный"
                            },
                            {
                                "target_lang_word": "главный",
                                "target_lang_sense_clarification": "центральный, ведущий"
                            },
                            {
                                "target_lang_word": "первостепенный",
                                "target_lang_sense_clarification": "имеющий высший приоритет"
                            }
                        ]

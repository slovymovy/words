    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac1ffb27-25d4-3176-a7f7-d384b67c19b4",
                    "sense_definition": "Cecha przedmiotu wynikająca z tego, w jaki sposób odbija on światło; barwa.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niebo ma dzisiaj piękny, niebieski <w>kolor</w>.",
                            "target_lang_translations": {
                                "ru": "У неба сегодня красивый голубой <w>цвет</w>."
                            }
                        },
                        {
                            "text": "Jaki jest twój ulubiony <w>kolor</w>?",
                            "target_lang_translations": {
                                "ru": "Какой твой любимый <w>цвет</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "barwa",
                        "odcień",
                        "zabarwienie"
                    ],
                    "common_phrases": [
                        "ulubiony kolor",
                        "jasny kolor",
                        "ciemny kolor",
                        "żywy kolor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство предмета, зависящее от того, как он отражает свет; окраска."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвет",
                                "target_lang_sense_clarification": "окраска, тон"
                            },
                            {
                                "target_lang_word": "краска",
                                "target_lang_sense_clarification": "оттенок, тон (часто во мн. ч.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a92f1b6-342d-3032-8a7e-18ce21961c0f",
                    "sense_definition": "Jedna z czterech kategorii kart w talii (kier, karo, trefl, pik), oznaczona specyficznym symbolem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cards_suit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W brydżu piki są najcenniejszym <w>kolorem</w>.",
                            "target_lang_translations": {
                                "ru": "В бридже пики — самая ценная <w>масть</w>."
                            }
                        },
                        {
                            "text": "Musisz dołożyć kartę w tym samym <w>kolorze</w>, jeśli ją posiadasz.",
                            "target_lang_translations": {
                                "ru": "Ты должен положить карту той же <w>масти</w>, если она у тебя есть."
                            }
                        }
                    ],
                    "antonyms": [
                        "atu"
                    ],
                    "common_phrases": [
                        "dołożyć do koloru",
                        "grać w kolor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из четырех категорий карт в колоде (черви, бубны, трефы, пики), обозначаемая специальным символом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "масть",
                                "target_lang_sense_clarification": "категория карт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3af0432b-d45e-3cf4-8091-d1b7dfb1d6e4",
                    "sense_definition": "Naturalne zaczerwienienie twarzy pojawiające się pod wpływem emocji, wysiłku lub świadczące o zdrowiu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "complexion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zosia na myśl o spotkaniu od razu nabrała <w>kolorów</w>.",
                            "target_lang_translations": {
                                "ru": "При мысли о встрече Зося сразу залилась <w>румянцем</w>."
                            }
                        },
                        {
                            "text": "Długi spacer sprawił, że wróciły jej <w>kolory</w> na policzki.",
                            "target_lang_translations": {
                                "ru": "Длительная прогулка вернула <w>румянец</w> на её щеки."
                            }
                        }
                    ],
                    "synonyms": [
                        "rumieniec",
                        "wypieki"
                    ],
                    "common_phrases": [
                        "nabrać kolorów",
                        "stracić kolory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественное покраснение лица, появляющееся под влиянием эмоций, усилий или свидетельствующее о здоровье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "румянец",
                                "target_lang_sense_clarification": "здоровый цвет лица"
                            },
                            {
                                "target_lang_word": "краска",
                                "target_lang_sense_clarification": "покраснение от смущения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f72ba29-c180-3f4e-a442-560a0430452b",
                    "sense_definition": "Silny układ kart w pokerze, składający się z pięciu kart tego samego rodzaju (np. same kiery).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "poker_hand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>kolor</w> okazał się silniejszy od jego strita.",
                            "target_lang_translations": {
                                "ru": "Мой <w>флеш</w> оказался сильнее его стрита."
                            }
                        },
                        {
                            "text": "Wylosowanie <w>koloru</w> z rozdania zdarza się dość rzadko.",
                            "target_lang_translations": {
                                "ru": "Получение <w>флеша</w> с раздачи случается довольно редко."
                            }
                        }
                    ],
                    "common_phrases": [
                        "królewski kolor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильная комбинация карт в покере, состоящая из пяти карт одной масти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "флеш",
                                "target_lang_sense_clarification": "комбинация в покере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ccf1b59-31bb-3b2b-a92d-c0c548dd8f56",
                    "sense_definition": "Liczba kwantowa w fizyce cząstek elementarnych, służąca do rozróżniania kwarków i gluonów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physics_charge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proton i neutron składają się z trzech kwarków o różnych <w>kolorach</w>.",
                            "target_lang_translations": {
                                "ru": "Протон и нейтрон состоят из трех кварков разных <w>цветов</w>."
                            }
                        },
                        {
                            "text": "Oddziaływania silne powodują zmianę <w>koloru</w> kwarków.",
                            "target_lang_translations": {
                                "ru": "Сильные взаимодействия вызывают изменение <w>цвета</w> кварков."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "fizyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Квантовое число в физике элементарных частиц, служащее для различения кварков и глюонов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цвет",
                                "target_lang_sense_clarification": "квантовое число"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ff8c754-1594-352c-b28b-4a66b22185af",
                    "sense_definition": "W heraldyce: jedna z ciemnych barw tarczy herbowej (czerwień, błękit, zieleń, czerń), w odróżnieniu od metali.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heraldry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zasada alternacji zabrania kładzenia <w>koloru</w> bezpośrednio na kolor.",
                            "target_lang_translations": {
                                "ru": "Правило тинктур запрещает накладывать <w>эмаль</w> непосредственно на эмаль."
                            }
                        },
                        {
                            "text": "W tym herbie dominuje <w>kolor</w> czerwony na srebrnym tle.",
                            "target_lang_translations": {
                                "ru": "В этом гербе доминирует красный <w>цвет</w> на серебряном фоне."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "heraldyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В геральдике: один из темных тонов гербового щита (червлень, лазурь, зелень, чернь) в отличие от металлов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эмаль",
                                "target_lang_sense_clarification": "геральдический термин"
                            },
                            {
                                "target_lang_word": "финифть",
                                "target_lang_sense_clarification": "устаревший геральдический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f7a14f37-686d-3414-9efb-75d1290f7291",
                    "sense_definition": "Wyrażenie żalu, smutku lub rozczarowania z powodu jakiejś sytuacji.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "regret_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jaka <szkoda>, że nie możesz być dzisiaj z nami na przyjęciu.",
                            "target_lang_translations": {
                                "ru": "Как <жаль>, что ты не можешь быть сегодня с нами на вечеринке."
                            }
                        },
                        {
                            "text": "Strasznie mi <szkoda> tego dziecka, które musi tyle cierpieć.",
                            "target_lang_translations": {
                                "ru": "Мне ужасно <жаль> этого ребенка, которому приходится так много страдать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jaka szkoda",
                        "wielka szkoda",
                        "szkoda gadać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение сожаления, грусти или разочарования по поводу какой-либо ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаль",
                                "target_lang_sense_clarification": "выражение сожаления"
                            },
                            {
                                "target_lang_word": "как жаль",
                                "target_lang_sense_clarification": "усиленное выражение сожаления"
                            },
                            {
                                "target_lang_word": "обидно",
                                "target_lang_sense_clarification": "чувство досады или огорчения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f28886ed-f4e0-3e3c-abb5-9e82fee690db",
                    "sense_definition": "Stwierdzenie, że nie warto czegoś robić lub że coś jest marnowane (np. czas, wysiłek).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "waste_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wstań już z łóżka, <szkoda> dnia na spanie!",
                            "target_lang_translations": {
                                "ru": "Вставай уже с постели, <жаль> тратить день на сон!"
                            }
                        },
                        {
                            "text": "<Szkoda> czasu na kłótnie, lepiej weźmy się do pracy.",
                            "target_lang_translations": {
                                "ru": "<Жаль> времени на ссоры, лучше возьмемся за работу."
                            }
                        },
                        {
                            "text": "Żadnych zmian się nie doczekamy, <szkoda> marzyć.",
                            "target_lang_translations": {
                                "ru": "Никаких перемен мы не дождемся, <бесполезно> и мечтать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szkoda czasu",
                        "szkoda słów",
                        "szkoda zachodu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утверждение, что не стоит чего-то делать или что ресурсы (время, усилия) тратятся впустую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаль",
                                "target_lang_sense_clarification": "о напрасной трате времени или сил"
                            },
                            {
                                "target_lang_word": "жалко",
                                "target_lang_sense_clarification": "разговорный вариант"
                            },
                            {
                                "target_lang_word": "не стоит",
                                "target_lang_sense_clarification": "отсутствие смысла в действии"
                            }
                        ]
                    }
                }
            ]
        },
        {

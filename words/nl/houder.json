    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f4d4a526-3544-3143-8a83-877e1aed2b85",
                    "sense_definition": "Een voorwerp dat bedoeld is om iets anders vast te houden, te ondersteunen of te bevatten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "object_holder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zette zijn koffiebeker voorzichtig in de <w>houder</w> naast het stuur.",
                            "target_lang_translations": {
                                "en": "He carefully placed his coffee cup in the <w>holder</w> next to the steering wheel.",
                                "ru": "Он осторожно поставил свою чашку с кофе в <w>держатель</w> рядом с рулем."
                            }
                        },
                        {
                            "text": "Deze <w>houder</w> is speciaal ontworpen om je telefoon stevig op zijn plaats te houden.",
                            "target_lang_translations": {
                                "en": "This <w>holder</w> is specially designed to keep your phone firmly in place.",
                                "ru": "Этот <w>держатель</w> специально разработан, чтобы надежно удерживать телефон на месте."
                            }
                        }
                    ],
                    "synonyms": [
                        "standaard",
                        "steun",
                        "bakje"
                    ],
                    "common_phrases": [
                        "in de houder",
                        "uit de houder"
                    ],
                    "target_lang_definitions": {
                        "en": "An object intended to hold, support, or contain something else.",
                        "ru": "Предмет, предназначенный для удержания, поддержки или хранения чего-либо другого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "holder",
                                "target_lang_sense_clarification": "device or container"
                            },
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "support base"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "держатель",
                                "target_lang_sense_clarification": "устройство для фиксации"
                            },
                            {
                                "target_lang_word": "подставка",
                                "target_lang_sense_clarification": "опора для предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80c7a47e-afad-364c-9263-91381f85096f",
                    "sense_definition": "Iemand die iets officieel in bezit heeft of beheert, zoals een document, een titel, aandelen of dieren.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_holder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>houder</w> van het paspoort moet het document persoonlijk ondertekenen.",
                            "target_lang_translations": {
                                "en": "The passport <w>holder</w> must sign the document in person.",
                                "ru": "<w>Владелец</w> паспорта должен подписать документ лично."
                            }
                        },
                        {
                            "text": "Als <w>houder</w> van het wereldrecord werd de atleet door iedereen toegejuicht.",
                            "target_lang_translations": {
                                "en": "As the world record <w>holder</w>, the athlete was cheered on by everyone.",
                                "ru": "Как <w>обладателя</w> мирового рекорда, спортсмена приветствовали все."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezitter",
                        "eigenaar",
                        "drager"
                    ],
                    "common_phrases": [
                        "houder van een vergunning",
                        "houder van aandelen"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who officially possesses or manages something, such as a document, title, shares, or animals.",
                        "ru": "Лицо, официально владеющее чем-либо или управляющее чем-либо, например документом, титулом, акциями или животными."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "holder",
                                "target_lang_sense_clarification": "person with legal possession"
                            },
                            {
                                "target_lang_word": "bearer",
                                "target_lang_sense_clarification": "of a document or title"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "владелец",
                                "target_lang_sense_clarification": "собственник"
                            },
                            {
                                "target_lang_word": "обладатель",
                                "target_lang_sense_clarification": "имеющий титул или награду"
                            },
                            {
                                "target_lang_word": "держатель",
                                "target_lang_sense_clarification": "владелец документов или акций"
                            }
                        ]
                    }
                }
            ]
        }
    ]

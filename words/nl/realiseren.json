{
    "word_family": [
        "realisatie",
        "realisme",
        "realist",
        "realistisch"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5c216877-d554-3434-900f-53a054f00d29",
                    "sense_definition": "Zich bewust worden van iets; begrijpen wat de situatie is of wat er gebeurt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "awareness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>realiseerde</w> me ineens dat ik mijn sleutels thuis had laten liggen.",
                            "target_lang_translations": {
                                "en": "I suddenly <w>realized</w> that I had left my keys at home.",
                                "ru": "Я вдруг <w>осознал</w>, что оставил ключи дома."
                            }
                        },
                        {
                            "text": "Hij <w>realiseert</w> zich niet hoeveel tijd dit project eigenlijk kost.",
                            "target_lang_translations": {
                                "en": "He does not <w>realize</w> how much time this project actually takes.",
                                "ru": "Он не <w>осознает</w>, сколько времени на самом деле требует этот проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "beseffen"
                    ],
                    "common_phrases": [
                        "zich iets realiseren",
                        "zich niet realiseren",
                        "zich terdege realiseren"
                    ],
                    "target_lang_definitions": {
                        "en": "To become aware of something; to understand a situation or fact clearly.",
                        "ru": "Осознавать что-либо; понимать, какова ситуация или что происходит."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "realize",
                                "target_lang_sense_clarification": "to become aware"
                            },
                            {
                                "target_lang_word": "become aware of",
                                "target_lang_sense_clarification": "to notice"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "осознавать",
                                "target_lang_sense_clarification": "понимать, отдавать себе отчет"
                            },
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "постигать смысл ситуации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "346e7d5d-6e07-3bdf-9286-f3e5f8b0faaf",
                    "sense_definition": "Iets wat bedacht of gepland is daadwerkelijk uitvoeren of maken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "achievement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De architect wil een modern en duurzaam gebouw <w>realiseren</w>.",
                            "target_lang_translations": {
                                "en": "The architect wants to <w>realize</w> a modern and sustainable building.",
                                "ru": "Архитектор хочет <w>реализовать</w> современное и экологичное здание."
                            }
                        },
                        {
                            "text": "Na jaren hard werken heeft zij eindelijk haar droom <w>gerealiseerd</w>.",
                            "target_lang_translations": {
                                "en": "After years of hard work, she finally <w>realized</w> her dream.",
                                "ru": "После долгих лет упорной работы она наконец <w>осуществила</w> свою мечту."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwezenlijken"
                    ],
                    "common_phrases": [
                        "een droom realiseren",
                        "een plan realiseren",
                        "doelen realiseren"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something that was planned or imagined happen in reality.",
                        "ru": "Осуществлять или выполнять то, что было задумано или запланировано."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "realize",
                                "target_lang_sense_clarification": "to make real"
                            },
                            {
                                "target_lang_word": "achieve",
                                "target_lang_sense_clarification": "a goal or plan"
                            },
                            {
                                "target_lang_word": "fulfill",
                                "target_lang_sense_clarification": "a dream or ambition"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "реализовать",
                                "target_lang_sense_clarification": "выполнить план или проект"
                            },
                            {
                                "target_lang_word": "осуществить",
                                "target_lang_sense_clarification": "претворить в жизнь"
                            },
                            {
                                "target_lang_word": "воплотить",
                                "target_lang_sense_clarification": "сделать реальным (мечту, идею)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e21815de-11f8-348f-a9b4-6b9fd4ab6445",
                    "sense_definition": "Muzieknoten ten gehore brengen precies zoals ze in het notenschrift staan.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is voor de pianist een grote uitdaging om dit complexe stuk te <w>realiseren</w>.",
                            "target_lang_translations": {
                                "en": "It is a great challenge for the pianist to <w>realize</w> this complex piece.",
                                "ru": "Для пианиста большой вызов — <w>исполнить</w> это сложное произведение."
                            }
                        },
                        {
                            "text": "Wat de componist heeft geschreven, is op dit oude instrument nauwelijks te <w>realiseren</w>.",
                            "target_lang_translations": {
                                "en": "What the composer wrote is hardly possible to <w>realize</w> on this old instrument.",
                                "ru": "То, что написал композитор, на этом старом инструменте вряд ли можно <w>реализовать</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To perform or render musical notes exactly as they are written.",
                        "ru": "Исполнять музыкальные ноты именно так, как они записаны в партитуре."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "realize",
                                "target_lang_sense_clarification": "musical realization"
                            },
                            {
                                "target_lang_word": "perform",
                                "target_lang_sense_clarification": "to play music"
                            },
                            {
                                "target_lang_word": "execute",
                                "target_lang_sense_clarification": "technical performance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "исполнить",
                                "target_lang_sense_clarification": "сыграть музыкальное произведение"
                            },
                            {
                                "target_lang_word": "реализовать",
                                "target_lang_sense_clarification": "технически воспроизвести написанное"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "29de2f46-ca0e-3266-a810-a7e058b9720f",
                    "sense_definition": "Ceramiczny element budowlany w kształcie prostopadłościanu, używany do wznoszenia murów i ścian.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten wysoki mur jest zbudowany z czerwonych <w>cegieł</w>.",
                            "target_lang_translations": {
                                "ru": "Эта высокая стена построена из красного <w>кирпича</w>."
                            }
                        },
                        {
                            "text": "Mój dziadek postawił ten dom z solidnej <w>cegły</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка построил этот дом из добротного <w>кирпича</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cegłówka"
                    ],
                    "common_phrases": [
                        "czerwona cegła",
                        "mur z cegły",
                        "dom z cegły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Керамический строительный элемент в форме параллелепипеда, используемый для возведения стен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кирпич",
                                "target_lang_sense_clarification": "строительный материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "defab8b1-c96f-3395-bdd6-6a7622f30f8c",
                    "sense_definition": "Bardzo gruba, obszerna i często nudna książka.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "thick_book",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Profesor zalecił studentom przeczytanie tej naukowej <w>cegły</w> na egzamin.",
                            "target_lang_translations": {
                                "ru": "Профессор посоветовал студентам прочитать этот научный <w>кирпич</w> к экзамену."
                            }
                        },
                        {
                            "text": "Nie zabieraj tej <w>cegły</w> na wakacje, bo zajmie ci pół walizki.",
                            "target_lang_translations": {
                                "ru": "Не бери этот <w>кирпич</w> в отпуск, он займет полчемодана."
                            }
                        }
                    ],
                    "synonyms": [

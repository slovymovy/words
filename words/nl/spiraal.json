            "senses": [
                {
                    "sense_id": "ebe0fad3-0813-3577-ba71-1e7760c360a3",
                    "sense_definition": "Een kromme lijn die zich vanuit een middelpunt steeds verder naar buiten draait, of een voorwerp met deze vorm.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shape_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rook kringelde in een <w>spiraal</w> omhoog naar het plafond.",
                            "target_lang_translations": {
                                "en": "The smoke curled upwards in a <w>spiral</w> towards the ceiling."
                            }
                        },
                        {
                            "text": "Hij tekende een perfecte <w>spiraal</w> op het papier.",
                            "target_lang_translations": {
                                "en": "He drew a perfect <w>spiral</w> on the paper."
                            }
                        }
                    ],
                    "synonyms": [
                        "helix"
                    ],
                    "target_lang_definitions": {
                        "en": "A curved line that winds outward from a center, or an object with this shape."

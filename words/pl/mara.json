    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5a2673e4-cb79-3c90-945c-e80a9893c8af",
                    "sense_definition": "Nierzeczywisty obraz, widziadło powstające w wyobraźni lub we śnie; często kojarzone z czymś nieuchwytnym, tajemniczym lub budzącym lęk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To była tylko senna <w>mara</w>, która zniknęła zaraz po przebudzeniu.",
                            "target_lang_translations": {
                                "ru": "Это было лишь сонное <w>видение</w>, которое исчезло сразу после пробуждения."
                            }
                        },
                        {
                            "text": "W gęstej mgle drzewa wyglądały jak przerażające <w>mary</w>.",
                            "target_lang_translations": {
                                "ru": "В густом тумане деревья выглядели как пугающие <w>призраки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "widziadło",
                        "zjawa",
                        "majak",
                        "przywidzenie",
                        "zwid",
                        "fantom",
                        "widmo",
                        "halucynacja"
                    ],
                    "common_phrases": [
                        "senna mara",
                        "nocna mara"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нереальный образ, видение, возникающее в воображении или во сне; часто ассоциируется с чем-то неуловимым, таинственным или пугающим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "призрак",
                                "target_lang_sense_clarification": "фантом, дух"
                            },
                            {
                                "target_lang_word": "видение",
                                "target_lang_sense_clarification": "образ в воображении или сне"
                            },
                            {
                                "target_lang_word": "марево",
                                "target_lang_sense_clarification": "иллюзия, туманный образ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37b8acc9-3378-3171-ae15-ea007fac9044",
                    "sense_definition": "W wierzeniach ludowych: zły duch lub demon, który nocą dusi śpiących ludzi i zwierzęta.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starzy ludzie wierzyli, że <w>mara</w> siada na piersi śpiącego i odbiera mu oddech.",
                            "target_lang_translations": {
                                "ru": "Старики верили, что <w>мара</w> садится на грудь спящего и отнимает у него дыхание."
                            }
                        },
                        {
                            "text": "Aby odstraszyć <w>marę</w>, wieszano w stajniach specjalne amulety.",
                            "target_lang_translations": {
                                "ru": "Чтобы отпугнуть <w>мару</w>, в конюшнях вешали специальные амулеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "zjawa",
                        "widmo",
                        "widziadło"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy dawnych wierzeń"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В народных поверьях: злой дух или демон, который по ночам душит спящих людей и животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мара",
                                "target_lang_sense_clarification": "мифологический персонаж"
                            },
                            {
                                "target_lang_word": "злой дух",
                                "target_lang_sense_clarification": "демон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dad4004-1ccb-3bf6-a64f-c0ddd5cd27ce",
                    "sense_definition": "Ssak z rzędu gryzoni o długich nogach, zamieszkujący stepy Ameryki Południowej (mara patagońska).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ogrodzie zoologicznym widzieliśmy wybieg, na którym biegała <w>mara</w>.",
                            "target_lang_translations": {
                                "ru": "В зоопарке мы видели вольер, по которому бегала <w>мара</w>."
                            }
                        },
                        {
                            "text": "<w>Mara</w> patagońska przypomina wyglądem zająca, ale jest gryzoniem.",
                            "target_lang_translations": {
                                "ru": "Патагонская <w>мара</w> внешне напоминает зайца, но является грызуном."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mara patagońska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Млекопитающее из отряда грызунов с длинными ногами, обитающее в степях Южной Америки (патагонская мара)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мара",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c181bd7c-dd13-30a8-9f12-e8737f5a6465",
                    "sense_definition": "Imię żeńskie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja koleżanka <w>Mara</w> pochodzi z Włoch.",
                            "target_lang_translations": {
                                "ru": "Моя подруга <w>Мара</w> родом из Италии."
                            }
                        },
                        {
                            "text": "Na liście gości widnieje imię <w>Mara</w>.",
                            "target_lang_translations": {
                                "ru": "В списке гостей значится имя <w>Мара</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мара",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bad95a3-e2a3-389a-9cc2-cea57c4a3788",
                    "sense_definition": "Nazwa geograficzna odnosząca się do rzek (m.in. w Afryce i Rumunii) oraz regionów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Rzeka <w>Mara</w> przepływa przez rezerwat przyrody w Afryce.",
                            "target_lang_translations": {
                                "ru": "Река <w>Мара</w> протекает через природный заповедник в Африке."
                            }
                        },
                        {
                            "text": "Podczas safari przekroczyliśmy rzekę o nazwie <w>Mara</w>.",
                            "target_lang_translations": {
                                "ru": "Во время сафари мы пересекли реку под названием <w>Мара</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Географическое название, относящееся к рекам (в частности, в Африке и Румынии) и регионам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мара",
                                "target_lang_sense_clarification": "река или регион"
                            }
                        ]
                    }
                }
            ]
        },
        {

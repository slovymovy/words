                            },
                            {
                                "target_lang_word": "удаваться",
                                "target_lang_sense_clarification": "проходить успешно"
                            },
                            {
                                "target_lang_word": "сработать",
                                "target_lang_sense_clarification": "о плане или замысле"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "to function correctly or produce the desired result"
                            },
                            {
                                "target_lang_word": "succeed",
                                "target_lang_sense_clarification": "to have a favorable outcome"
                            },
                            {
                                "target_lang_word": "turn out well",
                                "target_lang_sense_clarification": "regarding the quality of a creation (e.g., food, photos)"
                            }
                        ]

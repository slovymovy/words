{
    "word_family": [
        "ввод",
        "введение",
        "вводный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "230abe14-d120-3570-975f-18817415e2f6",
                    "sense_definition": "Провожать, сопровождать или заставлять кого-либо войти внутрь помещения.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Секретарь вежливо <вводит> посетителя в кабинет директора.",
                            "target_lang_translations": {
                                "en": "The secretary politely <w>shows</w> the visitor into the director's office."
                            }
                        },
                        {
                            "text": "Охрана <вводит> подозреваемого в здание через заднюю дверь.",
                            "target_lang_translations": {
                                "en": "Security <w>brings</w> the suspect into the building through the back door."
                            }
                        }
                    ],
                    "antonyms": [
                        "выводить"
                    ],
                    "common_phrases": [
                        "вводить в дом",
                        "вводить гостя",
                        "вводить в зал",
                        "вводить в комнату"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring, lead, or escort someone into a room or building."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring in",
                                "target_lang_sense_clarification": "physical movement"
                            },
                            {
                                "target_lang_word": "show in",
                                "target_lang_sense_clarification": "guests or visitors"
                            },
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "guide someone"
                            },
                            {
                                "target_lang_word": "escort",
                                "target_lang_sense_clarification": "accompany someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24dc7f36-d5e5-36f8-a6a4-d39084792da5",
                    "sense_definition": "Записывать информацию, символы или команды в электронное устройство или систему.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вы должны правильно <вводить> пароль для доступа к сайту.",
                            "target_lang_translations": {
                                "en": "You must correctly <w>enter</w> the password to access the site."
                            }
                        },
                        {
                            "text": "Бухгалтер внимательно <вводит> цифры в таблицу отчёта.",
                            "target_lang_translations": {
                                "en": "The accountant carefully <w>enters</w> the figures into the report table."
                            }
                        }
                    ],
                    "antonyms": [
                        "выводить"
                    ],
                    "common_phrases": [
                        "вводить пароль",
                        "вводить данные",
                        "вводить код",
                        "вводить текст"
                    ],
                    "target_lang_definitions": {
                        "en": "To type or record information into an electronic device or system."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "computing"
                            },
                            {
                                "target_lang_word": "input",
                                "target_lang_sense_clarification": "data processing"
                            },
                            {
                                "target_lang_word": "type in",
                                "target_lang_sense_clarification": "manual entry"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16c37a91-e5de-3e5b-9ce5-1771641a87ab",
                    "sense_definition": "Устанавливать и начинать применять что-то новое, например закон, правило или порядок.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство <вводит> новые налоги на импортные товары.",
                            "target_lang_translations": {
                                "en": "The government <w>is introducing</w> new taxes on imported goods."
                            }
                        },
                        {
                            "text": "В нашей школе с сентября <вводят> обязательную форму.",
                            "target_lang_translations": {
                                "en": "In our school, they <w>are introducing</w> a mandatory uniform from September."
                            }
                        }
                    ],
                    "synonyms": [
                        "внедрять",
                        "устанавливать"
                    ],
                    "antonyms": [
                        "выводить",
                        "выводить"
                    ],
                    "common_phrases": [
                        "вводить правила",
                        "вводить закон",
                        "вводить санкции",
                        "вводить моду"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish and start applying something new, such as a law, rule, or system."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "introduce",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "impose",
                                "target_lang_sense_clarification": "taxes or sanctions"
                            },
                            {
                                "target_lang_word": "implement",
                                "target_lang_sense_clarification": "put into effect"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "130ce246-dee9-360f-b5c0-48fcf7d399bc",
                    "sense_definition": "Помещать лекарственное вещество внутрь организма с помощью шприца или другого инструмента.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Медсестра аккуратно <вводит> обезболивающее лекарство пациенту.",
                            "target_lang_translations": {
                                "en": "The nurse carefully <w>administers</w> the painkiller to the patient."
                            }
                        },
                        {
                            "text": "Врач решил <вводить> препарат внутривенно для быстрого эффекта.",
                            "target_lang_translations": {
                                "en": "The doctor decided to <w>administer</w> the drug intravenously for a quick effect."
                            }
                        }
                    ],
                    "antonyms": [
                        "выводить"
                    ],
                    "common_phrases": [
                        "вводить лекарство",
                        "вводить внутривенно",
                        "вводить вакцину",
                        "вводить инъекцию"
                    ],
                    "target_lang_definitions": {
                        "en": "To put a medicinal substance into the body, usually by injection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "administer",
                                "target_lang_sense_clarification": "medical context"
                            },
                            {
                                "target_lang_word": "inject",
                                "target_lang_sense_clarification": "with a syringe"
                            },
                            {
                                "target_lang_word": "introduce",
                                "target_lang_sense_clarification": "technical"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d389b57-9398-3921-8c71-886444d1e97d",
                    "sense_definition": "Включать кого-либо в состав группы, коллектива или списка действующих лиц.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тренер постепенно <вводит> молодого игрока в основной состав команды.",
                            "target_lang_translations": {
                                "en": "The coach is gradually <w>bringing</w> the young player into the main squad."
                            }
                        },
                        {
                            "text": "Режиссёр <вводит> в спектакль новых актёров на главные роли.",
                            "target_lang_translations": {
                                "en": "The director <w>is introducing</w> new actors into the play for the lead roles."
                            }
                        }
                    ],
                    "synonyms": [
                        "включать"
                    ],
                    "antonyms": [
                        "выводить"
                    ],
                    "common_phrases": [
                        "вводить в состав",
                        "вводить в игру",
                        "вводить в совет"
                    ],
                    "target_lang_definitions": {
                        "en": "To include someone in a group, team, or cast."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring in",
                                "target_lang_sense_clarification": "into a group"
                            },
                            {
                                "target_lang_word": "introduce",
                                "target_lang_sense_clarification": "new members"
                            },
                            {
                                "target_lang_word": "integrate",
                                "target_lang_sense_clarification": "make part of a whole"
                            }
                        ]
                    }
                }
            ]
        }

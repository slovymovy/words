                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделяться",
                                "target_lang_sense_clarification": "быть заметным на фоне"
                            },
                            {
                                "target_lang_word": "обособляться",
                                "target_lang_sense_clarification": "становиться самостоятельным"

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оказываться",
                                "target_lang_sense_clarification": "попадать в ситуацию"
                            },
                            {
                                "target_lang_word": "находиться",
                                "target_lang_sense_clarification": "пребывать в состоянии"
                            }
                        ]
                    }
                }
            ]
        }

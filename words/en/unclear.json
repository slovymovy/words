    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0587b687-6e19-36f8-986b-2ba1dd6ca8d0",
                    "sense_definition": "Difficult to understand or having more than one possible meaning.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "meaning_ambiguity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The instructions were <w>unclear</w>, so we made a mistake.",
                            "target_lang_translations": {
                                "ru": "Инструкции были <w>непонятными</w>, поэтому мы совершили ошибку."
                            }
                        },
                        {
                            "text": "It remains <w>unclear</w> why he decided to leave the job so suddenly.",
                            "target_lang_translations": {
                                "ru": "Остается <w>неясным</w>, почему он решил так внезапно уйти с работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "ambiguous",
                        "vague",
                        "confusing",
                        "obscure"
                    ],
                    "antonyms": [
                        "clear",
                        "obvious",
                        "explicit",
                        "definite"
                    ],
                    "common_phrases": [
                        "remains unclear",
                        "unclear reason",
                        "unclear instructions",
                        "unclear meaning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудный для понимания или имеющий более одного возможного значения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непонятный",
                                "target_lang_sense_clarification": "сложный для осмысления"
                            },
                            {
                                "target_lang_word": "неясный",
                                "target_lang_sense_clarification": "туманный, неопределенный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dac27a9f-8e7c-3832-a935-753c48fba69f",
                    "sense_definition": "Not easy to see, hear, or read.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old photograph was <w>unclear</w> and faded with age.",
                            "target_lang_translations": {
                                "ru": "Старая фотография была <w>нечёткой</w> и выцветшей от времени."
                            }
                        },
                        {
                            "text": "His voice sounded <w>unclear</w> over the bad telephone connection.",
                            "target_lang_translations": {
                                "ru": "Его голос звучал <w>неразборчиво</w> из-за плохой телефонной связи."
                            }
                        }
                    ],
                    "synonyms": [
                        "blurry",
                        "indistinct",
                        "faint",
                        "illegible"
                    ],
                    "antonyms": [
                        "clear",
                        "visible",
                        "legible",
                        "distinct"
                    ],
                    "common_phrases": [
                        "unclear image",
                        "unclear picture",
                        "unclear sound"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плохо различимый на вид, слух или при чтении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нечёткий",
                                "target_lang_sense_clarification": "размытый, плохо видимый"
                            },
                            {
                                "target_lang_word": "неразборчивый",
                                "target_lang_sense_clarification": "трудный для прочтения или восприятия на слух"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a511164-0eb6-30e4-b312-21788428194d",
                    "sense_definition": "Not fully understanding or knowing something; uncertain.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am still <w>unclear</w> about what she meant by that comment.",
                            "target_lang_translations": {
                                "ru": "Я до сих пор <w>не уверен</w> в том, что она имела в виду под этим комментарием."
                            }
                        },
                        {
                            "text": "The students were <w>unclear</w> on the rules of the new game.",
                            "target_lang_translations": {
                                "ru": "Студенты <w>плохо понимали</w> правила новой игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "uncertain",
                        "unsure",
                        "confused"
                    ],
                    "antonyms": [
                        "certain",
                        "sure",
                        "clear"
                    ],
                    "common_phrases": [
                        "unclear about",
                        "unclear on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не до конца понимающий или знающий что-либо; неуверенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не уверенный",
                                "target_lang_sense_clarification": "испытывающий сомнения"
                            },
                            {
                                "target_lang_word": "не понимающий",
                                "target_lang_sense_clarification": "не осознающий полностью"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2ae3ecfd-35c0-355c-9009-b0d8acf37be4",
                    "sense_definition": "To undo the process of clearing something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "reversal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software allows <w>unclearing</w> the selection if you click the button again.",
                            "target_lang_translations": {
                                "ru": "Программа позволяет <w>отменить очистку</w> выделения, если вы нажмете кнопку снова."
                            }
                        },
                        {
                            "text": "It is difficult to <w>unclear</w> the data once the system has purged it.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>восстановить</w> данные после того, как система их удалила."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отменить процесс очистки чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отменить очистку",
                                "target_lang_sense_clarification": "вернуть удаленное или очищенное"
                            },
                            {
                                "target_lang_word": "восстановить",
                                "target_lang_sense_clarification": "вернуть в исходное состояние"
                            }
                        ]
                    }
                }
            ]
        }
    ]

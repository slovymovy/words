    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3348f373-6954-3ba4-9d9e-156c3195281a",
                    "sense_definition": "Iemand die afkomstig is uit een ander land of gebied.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "foreigner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>vreemdeling</w> moest zijn paspoort laten zien aan de grens.",
                            "target_lang_translations": {
                                "en": "The <w>foreigner</w> had to show his passport at the border.",
                                "ru": "<w>Иностранец</w> должен был предъявить паспорт на границе."
                            }
                        },
                        {
                            "text": "Zij voelde zich geen <w>vreemdeling</w> meer in haar nieuwe woonplaats.",
                            "target_lang_translations": {
                                "en": "She no longer felt like a <w>stranger</w> in her new place of residence.",
                                "ru": "Она больше не чувствовала себя <w>чужой</w> на новом месте жительства."
                            }
                        }
                    ],
                    "synonyms": [
                        "buitenlander",
                        "allochtoon"
                    ],
                    "antonyms": [
                        "autochtoon"
                    ],
                    "common_phrases": [
                        "vreemdeling in eigen land",
                        "illegale vreemdeling"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who comes from another country or area.",
                        "ru": "Человек, приехавший из другой страны или региона."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "foreigner",
                                "target_lang_sense_clarification": "person from another country"
                            },
                            {
                                "target_lang_word": "stranger",
                                "target_lang_sense_clarification": "outsider or newcomer in a place"
                            },
                            {
                                "target_lang_word": "alien",
                                "target_lang_sense_clarification": "legal term for a non-citizen"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "иностранец",
                                "target_lang_sense_clarification": "гражданин другой страны"
                            },
                            {
                                "target_lang_word": "чужеземец",
                                "target_lang_sense_clarification": "человек из чужой земли (книжное)"
                            },
                            {
                                "target_lang_word": "приезжий",
                                "target_lang_sense_clarification": "человек, прибывший из другой местности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec87bd20-3469-3e3f-b95e-13790fd9c04d",
                    "sense_definition": "Iemand die men niet kent; een onbekend persoon.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "stranger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn ouders waarschuwden me om nooit met een <w>vreemdeling</w> mee te gaan.",
                            "target_lang_translations": {
                                "en": "My parents warned me never to go with a <w>stranger</w>.",
                                "ru": "Родители предупреждали меня никогда не уходить с <w>незнакомцем</w>."
                            }
                        },
                        {
                            "text": "Hij keek me aan alsof ik een volslagen <w>vreemdeling</w> was.",
                            "target_lang_translations": {
                                "en": "He looked at me as if I were a complete <w>stranger</w>.",
                                "ru": "Он смотрел на меня так, словно я был полным <w>незнакомцем</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "buitenlander"
                    ],
                    "common_phrases": [
                        "volslagen vreemdeling"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone one does not know; an unknown person.",
                        "ru": "Человек, которого не знают; неизвестная личность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stranger",
                                "target_lang_sense_clarification": "person one does not know"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "незнакомец",
                                "target_lang_sense_clarification": "неизвестный человек"
                            },
                            {
                                "target_lang_word": "чужой",
                                "target_lang_sense_clarification": "посторонний, не свой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c98acd58-61e6-3137-8364-37423271f7fc",
                    "sense_definition": "Iemand die nog niet veel weet van een bepaald onderwerp of vakgebied.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "novice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als <w>vreemdeling</w> in de financiële wereld begreep hij de termen niet.",
                            "target_lang_translations": {
                                "en": "As a <w>stranger</w> to the financial world, he did not understand the terms.",
                                "ru": "Будучи <w>новичком</w> в финансовом мире, он не понимал терминов."
                            }
                        },
                        {
                            "text": "Ik ben een <w>vreemdeling</w> op het gebied van kwantummechanica.",
                            "target_lang_translations": {
                                "en": "I am a <w>stranger</w> to the field of quantum mechanics.",
                                "ru": "Я <w>профан</w> в области квантовой механики."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vreemdeling in Jeruzalem"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who does not know much about a specific subject or field.",
                        "ru": "Человек, который мало знает об определенном предмете или области."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stranger",
                                "target_lang_sense_clarification": "someone unfamiliar with something"
                            },
                            {
                                "target_lang_word": "outsider",
                                "target_lang_sense_clarification": "someone not belonging to a specific group or field"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "профан",
                                "target_lang_sense_clarification": "несведущий в чем-либо человек"
                            },
                            {
                                "target_lang_word": "новичок",
                                "target_lang_sense_clarification": "начинающий, неопытный"
                            },
                            {
                                "target_lang_word": "дилетант",
                                "target_lang_sense_clarification": "любитель, не специалист"
                            }
                        ]
                    }
                }
            ]
        }
    ]

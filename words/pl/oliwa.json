            "senses": [
                {
                    "sense_id": "d4046cc4-16fc-3c84-aa4c-fbb38d740d6c",
                    "sense_definition": "Ciekły tłuszcz roślinny wytłaczany z owoców oliwki, używany głównie w kuchni.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_oil",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do sałatki greckiej niezbędna jest dobrej jakości <w>oliwa</w> z oliwek.",
                            "target_lang_translations": {
                                "ru": "Для греческого салата необходимо качественное <w>оливковое масло</w>."
                            }
                        },
                        {
                            "text": "W kuchni śródziemnomorskiej <w>oliwa</w> stanowi podstawę wielu potraw.",
                            "target_lang_translations": {
                                "ru": "В средиземноморской кухне <w>оливковое масло</w> составляет основу многих блюд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oliwa z oliwek",
                        "dolać oliwy do ognia",
                        "oliwa sprawiedliwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкий растительный жир, получаемый из плодов оливы, используемый в основном в кулинарии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оливковое масло",
                                "target_lang_sense_clarification": "пищевой продукт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fb60009-ec0f-3aa8-8034-4f6893bcc677",
                    "sense_definition": "Olej techniczny służący do smarowania mechanizmów i maszyn.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "lubricant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary zegar przestał chodzić, bo zaschła w nim <w>oliwa</w>.",
                            "target_lang_translations": {
                                "ru": "Старые часы перестали ходить, потому что в них засохло <w>масло</w>."
                            }
                        },
                        {
                            "text": "Mechanik sprawdził, czy <w>oliwa</w> w maszynie wymaga uzupełnienia.",
                            "target_lang_translations": {
                                "ru": "Механик проверил, требует ли доливки <w>масло</w> в машине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oliwa do maszyn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Техническое масло, используемое для смазки механизмов и машин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "машинное масло",

{
    "word_family": [
        "chłopak",
        "chłopiec",
        "chłopka",
        "chłopski",
        "chłopstwo",
        "chłopięcy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b6c3221d-dda3-3f66-a3e9-e5d9150d2d54",
                    "sense_definition": "Dorosły mężczyzna, często silny i postawny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "man_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <chłop> pomógł mi wnieść ciężkie zakupy na trzecie piętro.",
                            "target_lang_translations": {
                                "ru": "Этот <мужик> помог мне занести тяжелые покупки на третий этаж."
                            }
                        },
                        {
                            "text": "To jest swój <chłop>, zawsze można na nim polegać w trudnej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Это свой <мужик>, на него всегда можно положиться в трудной ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "facet"
                    ],
                    "antonyms": [
                        "baba",
                        "babka"
                    ],
                    "common_phrases": [
                        "kawał chłopa",
                        "swój chłop",
                        "równy chłop"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрослый мужчина, часто крепкий и статный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мужик",
                                "target_lang_sense_clarification": "разг. мужчина"
                            },
                            {
                                "target_lang_word": "мужчина",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41908882-7676-3d24-808d-01d35a603a99",
                    "sense_definition": "Mąż lub stały partner życiowy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "man_relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <chłop> naprawił wczoraj kran w kuchni, więc nie muszę dzwonić po hydraulika.",
                            "target_lang_translations": {
                                "ru": "Мой <мужик> вчера починил кран на кухне, так что мне не нужно вызывать сантехника."
                            }
                        },
                        {
                            "text": "Kaśka i jej <chłop> wyjechali na tydzień w góry.",
                            "target_lang_translations": {
                                "ru": "Каська и её <мужик> уехали на неделю в горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "mąż"
                    ],
                    "antonyms": [
                        "baba"
                    ],
                    "common_phrases": [
                        "mój chłop",
                        "szukać chłopa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used by women to refer to their partners"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Муж или постоянный спутник жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мужик",
                                "target_lang_sense_clarification": "разг. муж, сожитель"
                            },
                            {
                                "target_lang_word": "муж",
                                "target_lang_sense_clarification": "супруг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c9ded08-f148-318c-a35a-fb9b28d0b838",
                    "sense_definition": "Członek warstwy społecznej zamieszkującej wieś i zajmującej się rolnictwem (głównie w kontekście historycznym).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "peasant_class",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach <chłop> musiał pracować na polu swojego pana.",
                            "target_lang_translations": {
                                "ru": "В давние времена <крестьянин> должен был работать на поле своего господина."
                            }
                        },
                        {
                            "text": "Powieść opisuje ciężką dolę <chłopów> w dziewiętnastym wieku.",
                            "target_lang_translations": {
                                "ru": "Роман описывает тяжелую долю <крестьян> в девятнадцатом веке."
                            }
                        }
                    ],
                    "synonyms": [
                        "włościanin",
                        "wieśniak"
                    ],
                    "common_phrases": [
                        "dola chłopa",
                        "bunt chłopów",
                        "syn chłopa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель социального слоя, живущего в деревне и занимающегося сельским хозяйством (преимущественно в историческом контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крестьянин",
                                "target_lang_sense_clarification": "историческое сословие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbb9e31e-0e8e-3808-a77d-7c0a0cd5471b",
                    "sense_definition": "Osoba prowadząca gospodarstwo rolne.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "farmer_occupation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy doświadczony <chłop> wie, kiedy najlepiej rozpocząć żniwa.",
                            "target_lang_translations": {
                                "ru": "Каждый опытный <крестьянин> знает, когда лучше всего начинать жатву."
                            }
                        },
                        {
                            "text": "Miejscowi <chłopi> protestowali przeciwko niskim cenom skupu zboża.",
                            "target_lang_translations": {
                                "ru": "Местные <крестьяне> протестовали против низких закупочных цен на зерно."
                            }
                        }
                    ],
                    "synonyms": [
                        "rolnik"
                    ],
                    "common_phrases": [
                        "zamożny chłop",
                        "praca chłopa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, ведущий сельское хозяйство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крестьянин",
                                "target_lang_sense_clarification": "сельский житель"
                            },
                            {
                                "target_lang_word": "земледелец",
                                "target_lang_sense_clarification": "обрабатывающий землю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d23227ab-7477-3a69-af48-3de4ed63160f",
                    "sense_definition": "Mężczyzna zachowujący się w sposób prymitywny, niekulturalny lub prosty.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "simpleton",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie zachowuj się jak <chłop>, bądź uprzejmy dla gości.",
                            "target_lang_translations": {
                                "ru": "Не веди себя как <деревенщина>, будь вежлив с гостями."
                            }
                        },
                        {
                            "text": "To zwykły <chłop>, który nie potrafi docenić sztuki ani literatury.",
                            "target_lang_translations": {
                                "ru": "Это обычный <мужик>, который не умеет ценить ни искусство, ни литературу."
                            }
                        }
                    ],
                    "synonyms": [
                        "prostak",
                        "cham"
                    ],
                    "common_phrases": [
                        "wyjść na chłopa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, ведущий себя примитивно, некультурно или простовато."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "деревенщина",
                                "target_lang_sense_clarification": "неотесанный человек"
                            },
                            {
                                "target_lang_word": "мужик",
                                "target_lang_sense_clarification": "грубый, невоспитанный человек"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
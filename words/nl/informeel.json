                {
                    "sense_id": "c9825134-f21a-3c86-8fdc-305359e53535",
                    "sense_definition": "Niet officieel; zonder strakke regels of ceremonies.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was een <w>informeel</w> gesprek tussen de collega's.",
                            "target_lang_translations": {
                                "en": "It was an <w>informal</w> conversation between the colleagues.",
                                "ru": "Это был <w>неформальный</w> разговор между коллегами."
                            }
                        },
                        {
                            "text": "In de <w>informele</w> sfeer van de bijeenkomst kwamen ze snel tot een akkoord.",
                            "target_lang_translations": {
                                "en": "In the <w>informal</w> atmosphere of the meeting, they quickly reached an agreement.",
                                "ru": "В <w>неформальной</w> обстановке встречи они быстро пришли к соглашению."
                            }
                        }
                    ],
                    "synonyms": [

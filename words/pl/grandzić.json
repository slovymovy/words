{
    "word_family": [
        "granda",
        "grandziarz"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "38ea7c62-1145-332d-b0db-cd8d2c7fd10b",
                    "sense_definition": "Zachowywać się nieodpowiednio, hałaśliwie, często wszczynając kłótnie lub bójki.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiedzi wezwali policję, ponieważ grupa młodzieży zaczęła <w>grandzić</w> pod blokiem.",
                            "target_lang_translations": {
                                "ru": "Соседи вызвали полицию, так как группа молодежи начала <w>хулиганить</w> под домом."
                            }
                        },
                        {
                            "text": "Nie możesz tak ciągle <w>grandzić</w>, bo w końcu narobisz sobie poważnych kłopotów.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь так постоянно <w>буянить</w>, иначе в конце концов наживешь себе серьезных проблем."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozrabiać",
                        "awanturować się",
                        "chuliganić",
                        "dokazywać"
                    ],
                    "antonyms": [
                        "uspokajać się",
                        "być grzecznym"
                    ],
                    "common_phrases": [
                        "przestać grandzić",
                        "zacząć grandzić"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя неподобающе, шумно, часто затевая ссоры или драки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буянить",
                                "target_lang_sense_clarification": "вести себя шумно и агрессивно"
                            },
                            {
                                "target_lang_word": "хулиганить",
                                "target_lang_sense_clarification": "нарушать общественный порядок"
                            },
                            {
                                "target_lang_word": "скандалить",
                                "target_lang_sense_clarification": "устраивать ссоры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "085e0ba0-b3d3-398d-b914-b82755eaffe6",
                    "sense_definition": "Zabierać cudzą własność bez pozwolenia; kraść.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "crime",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach chłopak musiał <w>grandzić</w> na targu, żeby zdobyć jedzenie.",
                            "target_lang_translations": {
                                "ru": "В прежние времена парню приходилось <w>воровать</w> на рынке, чтобы добыть еду."
                            }
                        },
                        {
                            "text": "Złapali go na gorącym uczynku, gdy próbował <w>grandzić</w> węgiel z wagonów.",
                            "target_lang_translations": {
                                "ru": "Его поймали с поличным, когда он пытался <w>тащить</w> уголь из вагонов."
                            }
                        }
                    ],
                    "synonyms": [
                        "kraść",
                        "zwędzić",
                        "buchnąć",
                        "podkradać"
                    ],
                    "antonyms": [
                        "kupować",
                        "oddawać"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "słowo z gwary przestępczej lub środowiskowej"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать чужую собственность без разрешения; воровать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воровать",
                                "target_lang_sense_clarification": "красть"
                            },
                            {
                                "target_lang_word": "тырить",
                                "target_lang_sense_clarification": "красть (разг.)"
                            },
                            {
                                "target_lang_word": "тащить",
                                "target_lang_sense_clarification": "красть, уносить (разг.)"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "вытянуть",
        "затянуть",
        "натянуть",
        "обтянуть",
        "оттянуть",
        "перетянуть",
        "подтянуть",
        "притянуть",
        "протянуть",
        "растянуть",
        "стянуть",
        "утянуть",
        "тяга",
        "тяготение",
        "тяжёлый",
        "тяжесть",
        "тягучий"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f6540173-32e0-33eb-ab9e-70af2ba56397",
                    "sense_definition": "С усилием перемещать предмет, приближая его к себе.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нужно <тянуть> дверь на себя, чтобы открыть её.",
                            "target_lang_translations": {
                                "en": "You need to <pull> the door towards yourself to open it."
                            }
                        },
                        {
                            "text": "Лошадь устала <тянуть> тяжёлую телегу в гору.",
                            "target_lang_translations": {
                                "en": "The horse was tired of <pulling> the heavy cart uphill."
                            }
                        }
                    ],
                    "synonyms": [
                        "тащить"
                    ],
                    "antonyms": [
                        "толкать"
                    ],
                    "common_phrases": [
                        "тянуть на себя",
                        "тянуть руку",
                        "тянуть канат"
                    ],
                    "target_lang_definitions": {
                        "en": "To move an object by using force to bring it closer to oneself."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pull",
                                "target_lang_sense_clarification": "to apply force to move something towards oneself"
                            },
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "to pull or drag something (often more formal or specific)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69c805f4-4478-33db-b211-43a8c3e75881",
                    "sense_definition": "Медлить, не торопиться с выполнением чего-либо; намеренно затягивать время.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_delay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хватит <тянуть> время, давай начнём работать.",
                            "target_lang_translations": {
                                "en": "Stop <stalling> for time, let's start working."
                            }
                        },
                        {
                            "text": "Он не знал правильного ответа и пытался <тянуть> с решением.",
                            "target_lang_translations": {
                                "en": "He didn't know the right answer and tried to <delay> the decision."
                            }
                        }
                    ],
                    "synonyms": [
                        "медлить",
                        "мешкать"
                    ],
                    "common_phrases": [
                        "тянуть время",
                        "тянуть с ответом",
                        "не тяни"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To delay or hesitate in doing something; to intentionally prolong time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stall",
                                "target_lang_sense_clarification": "to delay taking action to gain time"
                            },
                            {
                                "target_lang_word": "delay",
                                "target_lang_sense_clarification": "to make something happen later than expected"
                            },
                            {
                                "target_lang_word": "drag out",
                                "target_lang_sense_clarification": "to make something last longer than necessary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99b299d2-a066-3bc9-a072-09a489dfc211",
                    "sense_definition": "Испытывать сильное желание или влечение к кому-либо или чему-либо (обычно используется в безличных конструкциях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгой зимы меня <тянет> на море.",
                            "target_lang_translations": {
                                "en": "After a long winter, I <am drawn> to the sea."
                            }
                        },
                        {
                            "text": "Его всегда <тянуло> к искусству и музыке.",
                            "target_lang_translations": {
                                "en": "He was always <drawn> to art and music."
                            }
                        }
                    ],
                    "synonyms": [
                        "влечь",
                        "манить",
                        "хотеться"
                    ],
                    "common_phrases": [
                        "тянет домой",
                        "тянет в сон",
                        "душа тянет"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel a strong desire or attraction towards someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be drawn to",
                                "target_lang_sense_clarification": "to feel attracted or pulled towards something"
                            },
                            {
                                "target_lang_word": "crave",
                                "target_lang_sense_clarification": "to feel a powerful desire for something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "456e7bc7-8188-30c0-9470-2d91de175f65",
                    "sense_definition": "Иметь определённый вес.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот огромный арбуз <тянет> на десять килограммов.",
                            "target_lang_translations": {
                                "en": "This huge watermelon <weighs> ten kilograms."
                            }
                        },
                        {
                            "text": "Твой чемодан <тянет> слишком много, придётся доплатить.",
                            "target_lang_translations": {
                                "en": "Your suitcase <weighs> too much, you'll have to pay extra."
                            }
                        }
                    ],
                    "synonyms": [
                        "весить"
                    ],
                    "common_phrases": [
                        "тянуть на килограмм",
                        "сколько тянет"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To have a specific weight."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weigh",
                                "target_lang_sense_clarification": "to have a certain weight"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a1d580a-08cb-303e-8d07-3aa024c02e99",
                    "sense_definition": "Быть тесным, вызывать ощущение давления или неудобства (об одежде или обуви).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новые ботинки немного <тянут>, но скоро разносятся.",
                            "target_lang_translations": {
                                "en": "The new boots <are> a bit <tight>, but they will stretch soon."
                            }
                        },
                        {
                            "text": "Пиджак мне мал и <тянет> в плечах.",
                            "target_lang_translations": {
                                "en": "The jacket is too small for me and <is tight> in the shoulders."
                            }
                        }
                    ],
                    "common_phrases": [
                        "рубашка тянет",
                        "тянет в плечах"
                    ],
                    "target_lang_definitions": {
                        "en": "To be too tight or cause a feeling of pressure (of clothing or footwear)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be tight",
                                "target_lang_sense_clarification": "to fit too closely, causing discomfort"
                            },
                            {
                                "target_lang_word": "pinch",
                                "target_lang_sense_clarification": "to squeeze painfully (of shoes)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "806dabcb-267b-321a-b7d5-7cb3521c6cdf",
                    "sense_definition": "Прокладывать, протягивать что-либо длинное (провод, трубу, дорогу).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие начали <тянуть> интернет-кабель в наш дом.",
                            "target_lang_translations": {
                                "en": "The workers started to <run> an internet cable into our house."
                            }
                        },
                        {
                            "text": "Через лес решили <тянуть> новую дорогу.",
                            "target_lang_translations": {
                                "en": "They decided to <lay> a new road through the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "прокладывать",
                        "проводить"
                    ],
                    "common_phrases": [
                        "тянуть кабель",
                        "тянуть провод",
                        "тянуть трубу"
                    ],
                    "target_lang_definitions": {
                        "en": "To lay or extend something long (like a wire, pipe, or road)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "to install or extend a line, cable, or pipe"
                            },
                            {
                                "target_lang_word": "lay",
                                "target_lang_sense_clarification": "to put down or construct (a road, track, etc.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45861cf3-a12e-3729-987d-d416bb2d1851",
                    "sense_definition": "Медленно пить, всасывать жидкость или вдыхать воздух.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Приятно сидеть в кафе и медленно <тянуть> горячий кофе.",
                            "target_lang_translations": {
                                "en": "It's nice to sit in a cafe and slowly <sip> hot coffee."
                            }
                        },
                        {
                            "text": "Насос перестал <тянуть> воду из колодца.",
                            "target_lang_translations": {
                                "en": "The pump stopped <drawing> water from the well."
                            }
                        }
                    ],
                    "synonyms": [
                        "всасывать",
                        "сосать"
                    ],
                    "common_phrases": [
                        "тянуть сок",
                        "тянуть коктейль",
                        "тянуть воздух"
                    ],
                    "target_lang_definitions": {
                        "en": "To drink slowly, suck in liquid, or inhale air."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sip",
                                "target_lang_sense_clarification": "to drink slowly in small amounts"
                            },
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "to suck or pull liquid/air (e.g., a pump or breath)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58ea7a19-30c3-3a92-a56e-bd183a440593",
                    "sense_definition": "Ощущаться (о запахе, холоде) или создавать поток воздуха (о сквозняке, дымоходе).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sensation_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Закрой окно, оттуда сильно <тянет> холодом.",
                            "target_lang_translations": {
                                "en": "Close the window, there is a strong cold <draft> coming from there."
                            }
                        },
                        {
                            "text": "С кухни вкусно <тянуло> свежими пирогами.",
                            "target_lang_translations": {
                                "en": "A delicious smell of fresh pies <wafted> from the kitchen."
                            }
                        }
                    ],
                    "synonyms": [
                        "веять"
                    ],
                    "common_phrases": [
                        "тянет холодом",
                        "тянет сыростью",
                        "печка не тянет"
                    ],
                    "target_lang_definitions": {
                        "en": "To be felt (of a smell or cold) or to create an air current."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "draft",
                                "target_lang_sense_clarification": "to create a current of air (also 'draught')"
                            },
                            {
                                "target_lang_word": "waft",
                                "target_lang_sense_clarification": "to pass gently through the air (of a smell)"
                            },
                            {
                                "target_lang_word": "come from",
                                "target_lang_sense_clarification": "to originate or emanate from (of cold/smell)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5a89642-f34d-3e6e-b3d7-a704ba7bb08c",
                    "sense_definition": "Медленно и протяжно произносить слова или звуки.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Певец начал <тянуть> грустную мелодию.",
                            "target_lang_translations": {
                                "en": "The singer began to <draw out> a sad melody."
                            }
                        },
                        {
                            "text": "Не нужно так <тянуть> слова, говори быстрее.",
                            "target_lang_translations": {
                                "en": "You don't need to <drawl> your words so much, speak faster."
                            }
                        }
                    ],
                    "common_phrases": [
                        "тянуть слова",
                        "тянуть песню",
                        "тянуть гласные"
                    ],
                    "target_lang_definitions": {
                        "en": "To pronounce words or sounds slowly and in a prolonged manner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drawl",
                                "target_lang_sense_clarification": "to speak slowly with prolonged vowel sounds"
                            },
                            {
                                "target_lang_word": "draw out",
                                "target_lang_sense_clarification": "to prolong a sound or note"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68d3423b-926b-3f74-a070-f1aee8c0cf20",
                    "sense_definition": "Справляться с работой, действовать эффективно; работать исправно (о механизмах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "capability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старый двигатель уже не <тянет> машину в крутую гору.",
                            "target_lang_translations": {
                                "en": "The old engine can no longer <pull> the car up a steep hill."
                            }
                        },
                        {
                            "text": "Новый сотрудник пока не <тянет> такую сложную работу.",
                            "target_lang_translations": {
                                "en": "The new employee can't <handle> such complex work yet."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мотор не тянет",
                        "не тянуть нагрузку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cope with work effectively or function properly (of mechanisms)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pull",
                                "target_lang_sense_clarification": "to exert enough force to move a load (of an engine)"
                            },
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "to manage or cope with a task"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
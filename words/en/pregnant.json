                },
                {
                    "sense_id": "a998b2cc-5da3-3aa9-9b58-8e9ddf824ee3",
                    "sense_definition": "Used to describe a couple who are expecting a baby together.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "biological_state",
                    "name_type": "no",
                    "examples": [
                        {

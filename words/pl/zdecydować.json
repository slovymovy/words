    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a620305a-23b9-3f74-ba8e-635cd5bf7c2e",
                    "sense_definition": "Podjąć świadomą decyzję w jakiejś sprawie lub dokonać wyboru po namyśle.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy wspólnie <zdecydować>, co kupimy mamie na urodziny.",
                            "target_lang_translations": {
                                "ru": "Мы должны вместе <решить>, что купим маме на день рождения."
                            }
                        },
                        {
                            "text": "Sąd musi <zdecydować>, kto jest winny w tej skomplikowanej sprawie.",
                            "target_lang_translations": {
                                "ru": "Суд должен <решить>, кто виновен в этом сложном деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "postanowić",
                        "rozstrzygnąć"
                    ],
                    "common_phrases": [
                        "zdecydować o czymś",
                        "zdecydować wspólnie",
                        "musisz zdecydować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять осознанное решение по какому-либо вопросу или сделать выбор после размышления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решить",
                                "target_lang_sense_clarification": "принять решение"
                            },
                            {
                                "target_lang_word": "принять решение",
                                "target_lang_sense_clarification": "устойчивое выражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63afccc2-abc4-392d-b001-4554e28de027",
                    "sense_definition": "Dokonać wyboru i postanowić coś zrobić (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Długo nie mogłam się <zdecydować> na zmianę pracy, ale teraz jestem zadowolona.",
                            "target_lang_translations": {
                                "ru": "Я долго не могла <решиться> на смену работы, но теперь я довольна."
                            }
                        },
                        {
                            "text": "Czy w końcu udało ci się <zdecydować>, gdzie spędzisz urlop?",
                            "target_lang_translations": {
                                "ru": "Тебе наконец удалось <решить>, где ты проведешь отпуск?"
                            }
                        }
                    ],
                    "synonyms": [
                        "postanowić",
                        "namyślić się"
                    ],
                    "common_phrases": [
                        "zdecydować się na coś",
                        "zdecydować się wyjechać",
                        "ostatecznie się zdecydować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать выбор и намереваться что-то сделать (используется с возвратным местоимением \"się\")."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решиться",
                                "target_lang_sense_clarification": "осмелиться, принять окончательное решение"
                            },
                            {
                                "target_lang_word": "определиться",
                                "target_lang_sense_clarification": "сделать выбор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06f53f04-f2e5-38cc-bb70-5558b380f8e3",
                    "sense_definition": "Mieć decydujący wpływ na wynik lub przebieg jakiegoś zdarzenia; przesądzić o czymś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeden błąd bramkarza może <zdecydować> o wyniku całego meczu.",
                            "target_lang_translations": {
                                "ru": "Одна ошибка вратаря может <решить> исход всего матча."
                            }
                        },
                        {
                            "text": "Głosy wyborców z tego regionu mogą <zdecydować> o ostatecznym zwycięstwie kandydata.",
                            "target_lang_translations": {
                                "ru": "Голоса избирателей из этого региона могут <решить> судьбу окончательной победы кандидата."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesądzić"
                    ],
                    "common_phrases": [
                        "zdecydować o wyniku",
                        "zdecydować o losie",
                        "zdecydować o sukcesie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказать решающее влияние на результат или ход события; предопределить что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решить",
                                "target_lang_sense_clarification": "определить исход"
                            },
                            {
                                "target_lang_word": "предопределить",
                                "target_lang_sense_clarification": "заранее определить результат"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2bbf8b07-58be-3bba-abc9-8555fca68ef7",
                    "sense_definition": "Zabawka w kształcie postaci ludzkiej, często z ruchomymi kończynami poruszanymi za pomocą sznurka.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "toy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko dostało w prezencie kolorowego, drewnianego <w>pajaca</w>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок получил в подарок цветного деревянного <w>паяца</w>."
                            }
                        },
                        {
                            "text": "Pociągnij za sznurek, a <w>pajac</w> zacznie ruszać rękami i nogami.",
                            "target_lang_translations": {
                                "ru": "Потяни за шнурок, и <w>паяц</w> начнет двигать руками и ногами."
                            }
                        }
                    ],
                    "synonyms": [
                        "marionetka",
                        "kukiełka"
                    ],
                    "common_phrases": [
                        "drewniany pajac",
                        "pajac na sznurku",
                        "kolorowy pajac"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрушка в виде человеческой фигурки, часто с подвижными конечностями, которые приводятся в движение с помощью шнурка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дергунчик",
                                "target_lang_sense_clarification": "игрушка с подвижными конечностями"
                            },
                            {
                                "target_lang_word": "паяц",
                                "target_lang_sense_clarification": "игрушка"
                            },
                            {
                                "target_lang_word": "марионетка",
                                "target_lang_sense_clarification": "кукла на нитках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c01190d4-d346-3a79-995f-ac5623fa7efc",
                    "sense_definition": "Osoba zachowująca się niepoważnie, próbująca na siłę rozśmieszyć innych lub postępująca niemądrze; określenie obraźliwe.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestań robić z siebie <w>pajaca</w> i zacznij zachowywać się poważnie.",
                            "target_lang_translations": {
                                "ru": "Перестань строить из себя <w>шута</w> и начни вести себя серьёзно."
                            }
                        },
                        {
                            "text": "Nie słuchaj go, to zwykły <w>pajac</w>, który nic nie wie.",
                            "target_lang_translations": {
                                "ru": "Не слушай его, это обычный <w>клоун</w>, который ничего не знает."
                            }
                        }
                    ],
                    "synonyms": [
                        "błazen",
                        "głupek",
                        "idiota",
                        "dureń",
                        "kretyn",
                        "klaun"
                    ],
                    "common_phrases": [
                        "robić z siebie pajaca",
                        "niezły pajac",
                        "zachowywać się jak pajac"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который ведет себя несерьезно, паясничает, пытаясь рассмешить других, или поступает глупо; оскорбительное слово."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клоун",
                                "target_lang_sense_clarification": "перен. несерьезный человек"
                            },
                            {
                                "target_lang_word": "шут",
                                "target_lang_sense_clarification": "перен. тот, кто паясничает"
                            },
                            {
                                "target_lang_word": "паяц",
                                "target_lang_sense_clarification": "перен. кривляка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f923debe-e240-3914-92fe-c2fea6ddea82",
                    "sense_definition": "Komiczna postać w cyrku lub w dawnych sztukach teatralnych, której zadaniem jest rozśmieszanie publiczności.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "performer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cyrkowy <w>pajac</w> zabawiał dzieci swoimi sztuczkami i żartami.",
                            "target_lang_translations": {
                                "ru": "Цирковой <w>паяц</w> развлекал детей своими трюками и шутками."
                            }
                        },
                        {
                            "text": "Na scenę wyszedł smutny <w>pajac</w> z pomalowaną twarzą.",
                            "target_lang_translations": {
                                "ru": "На сцену вышел грустный <w>паяц</w> с разрисованным лицом."
                            }
                        }
                    ],

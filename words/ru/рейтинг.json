            "senses": [
                {
                    "sense_id": "a578a0e6-a2cc-34a5-8bde-d2c2a2e0a373",
                    "sense_definition": "Числовой или порядковый показатель, отражающий уровень качества, значимости или успеха кого-либо или чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "indicator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Узнав, что <w>рейтинг</w> его ресторана упал, шеф-повар очень расстроился.",
                            "target_lang_translations": {
                                "en": "Having learned that his restaurant's <w>rating</w> had dropped, the chef became very upset."
                            }
                        },
                        {
                            "text": "Зачисление в старшие классы школы будет проходить по учебному <w>рейтингу</w>.",
                            "target_lang_translations": {
                                "en": "Enrollment in high school will be based on academic <w>ranking</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "показатель",
                        "индекс"
                    ],
                    "common_phrases": [
                        "высокий рейтинг",
                        "низкий рейтинг",
                        "рейтинг продаж"
                    ],
                    "target_lang_definitions": {
                        "en": "A numerical or ordinal score reflecting the quality, significance, or success of someone or something."
                    },
                    "translations": {
                        "en": [
                            {

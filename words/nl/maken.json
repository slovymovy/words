                            {
                                "target_lang_word": "сделать",
                                "target_lang_sense_clarification": "привести в состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e17994c1-ef4d-3d01-9dd8-3f5e75b96cf9",
                    "sense_definition": "Iets dat kapot is weer heel of werkend krijgen.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "repair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader kan alles in huis zelf <w>maken</w>.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "19a91c53-a492-36f0-a611-b3faffdd7d51",
                    "sense_definition": "The arrival or onset of something; the time when something first comes into being or appears.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "group_arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>advent</w> of the internet has revolutionized how people communicate globally.",
                            "target_lang_translations": {
                                "ru": "<w>Появление</w> интернета произвело революцию в том, как люди общаются во всем мире."
                            }
                        },
                        {
                            "text": "Many traditional jobs disappeared with the <w>advent</w> of automated machinery.",
                            "target_lang_translations": {
                                "ru": "Многие традиционные профессии исчезли с <w>появлением</w> автоматизированного оборудования."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrival",
                        "onset",
                        "approach",
                        "oncome"
                    ],
                    "common_phrases": [
                        "advent of",
                        "with the advent of",
                        "since the advent of"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибытие, появление или начало чего-либо; момент, когда что-то впервые возникает или становится известным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "появление",
                                "target_lang_sense_clarification": "возникновение, начало существования"
                            },
                            {
                                "target_lang_word": "наступление",
                                "target_lang_sense_clarification": "начало периода или события"
                            },
                            {
                                "target_lang_word": "приход",
                                "target_lang_sense_clarification": "прибытие, начало (часто метафорически)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "8622ffa9-0429-35ff-b9b1-ef5935f2e833",
                    "sense_definition": "The period or season of the Christian church year leading up to Christmas, beginning on Advent Sunday.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_religious",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "Children often count down the days to Christmas using an <w>Advent</w> calendar.",
                            "target_lang_translations": {
                                "ru": "Дети часто отсчитывают дни до Рождества, используя календарь <w>Адвента</w>."
                            }
                        },
                        {
                            "text": "The church was decorated with purple candles during <w>Advent</w>.",
                            "target_lang_translations": {
                                "ru": "Во время <w>Адвента</w> церковь была украшена фиолетовыми свечами."
                            }
                        }
                    ],
                    "synonyms": [
                        "Christmas season",
                        "Christmastide"
                    ],
                    "common_phrases": [
                        "Advent calendar",
                        "Advent Sunday",
                        "Advent wreath"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период церковного года, предшествующий Рождеству; начинается в первое воскресенье Адвента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Адвент",
                                "target_lang_sense_clarification": "предрождественский период в западной христианской традиции"
                            },
                            {
                                "target_lang_word": "Рождественский пост",
                                "target_lang_sense_clarification": "аналог в православной традиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75b8f854-9d3a-3866-aca2-777b188171f1",
                    "sense_definition": "The first or second coming of Christ in Christian theology.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_religious",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "The sermon focused on the prophecies regarding the Second <w>Advent</w>.",
                            "target_lang_translations": {
                                "ru": "Проповедь была посвящена пророчествам о Втором <w>пришествии</w>."
                            }
                        },
                        {
                            "text": "Believers prepared their hearts for the <w>Advent</w> of the Messiah.",
                            "target_lang_translations": {
                                "ru": "Верующие готовили свои сердца к <w>пришествию</w> Мессии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Second Advent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое или второе пришествие Христа в христианском богословии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пришествие",
                                "target_lang_sense_clarification": "явление Христа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b25ffa19-8d00-3207-b2a8-905a4eba4ecb",
                    "sense_definition": "A civil parish near Bodmin Moor in Cornwall, England.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We drove through the small parish of <w>Advent</w> during our trip to Cornwall.",
                            "target_lang_translations": {
                                "ru": "Мы проехали через небольшой приход <w>Адвент</w> во время нашей поездки в Корнуолл."
                            }
                        },
                        {
                            "text": "The map indicated a remote area known as <w>Advent</w>.",
                            "target_lang_translations": {
                                "ru": "На карте была обозначена отдаленная местность, известная как <w>Адвент</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гражданский приход недалеко от Бодмин-Мур в Корнуолле, Англия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Адвент",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "74c75126-efa5-3ce6-a49a-a4faa846f4b9",
                    "sense_definition": "To arrive or begin, especially regarding the first appearance of something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A new era of peace <w>advented</w> after the long war.",
                            "target_lang_translations": {
                                "ru": "После долгой войны <w>наступила</w> новая эра мира."
                            }
                        },
                        {
                            "text": "The winter season <w>advented</w> with a sudden drop in temperature.",
                            "target_lang_translations": {
                                "ru": "Зимний сезон <w>наступил</w> с внезапным понижением температуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "approach"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rarely used in modern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибывать или начинаться, особенно в отношении первого появления чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наступать",
                                "target_lang_sense_clarification": "о времени, событии или эре"
                            },
                            {
                                "target_lang_word": "приходить",
                                "target_lang_sense_clarification": "появляться, наставать"
                            }
                        ]
                    }
                }
            ]
        }

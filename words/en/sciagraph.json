                    "sense_id": "5cad6813-7149-3323-9b2b-e4e436b7a4e2",
                    "sense_definition": "A vertical section of a building showing its internal structure.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "architecture_section",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The architect drew a detailed <w>sciagraph</w> to illustrate the interior height of the nave.",
                            "target_lang_translations": {
                                "ru": "Архитектор начертил детальный <w>разрез</w>, чтобы проиллюстрировать внутреннюю высоту нефа."
                            }
                        },
                        {
                            "text": "Students examined the <w>sciagraph</w> to understand how the dome was supported.",
                            "target_lang_translations": {
                                "ru": "Студенты изучали <w>разрез</w>, чтобы понять, как поддерживался купол."
                            }
                        }
                    ],
                    "synonyms": [
                        "section",
                        "profile",
                        "cross-section"
                    ],
                    "common_phrases": [
                        "architectural sciagraph",
                        "building sciagraph"
                    ],
                    "target_lang_definitions": {

                            {
                                "target_lang_word": "день рожде́ния",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2803021-14e0-3250-b153-2bc14f08ca7d",
                    "sense_definition": "The anniversary of the day on which something is created.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "anniversary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city celebrated its three hundredth <w>birthday</w> with fireworks.",

{
    "word_family": [
        "nawała",
        "nawalnica"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "41d9c24f-91ca-347b-8eac-ea99249bf991",
                    "sense_definition": "O deszczu lub zjawiskach atmosferycznych: bardzo gwałtowny, intensywny i obfity.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagły deszcz <w>nawalny</w> zalał ulice w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Внезапный <w>ливневый</w> дождь затопил улицы в центре города."
                            }
                        },
                        {
                            "text": "Systemy kanalizacji nie wytrzymały naporu wody podczas opadu <w>nawalnego</w>.",
                            "target_lang_translations": {
                                "ru": "Канализационные системы не выдержали напора воды во время <w>ливневых</w> осадков."
                            }
                        }
                    ],
                    "synonyms": [
                        "ulewny",
                        "gwałtowny",
                        "rzęsisty"
                    ],
                    "antonyms": [
                        "mżący",
                        "lekki",
                        "drobny"
                    ],
                    "common_phrases": [
                        "deszcz nawalny",
                        "opad nawalny"
                    ],
                    "target_lang_definitions": {
                        "ru": "О дожде или атмосферных явлениях: очень бурный, интенсивный и обильный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ливневый",
                                "target_lang_sense_clarification": "о дожде: проливной, очень сильный"
                            },
                            {
                                "target_lang_word": "проливной",
                                "target_lang_sense_clarification": "очень сильный (о дожде)"
                            },
                            {
                                "target_lang_word": "шквальный",
                                "target_lang_sense_clarification": "бурный, сопровождаемый сильным ветром"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75de6e34-5718-3c04-a232-55420aa4d8df",
                    "sense_definition": "Występujący w wielkiej ilości lub z dużym natężeniem, często w kontekście ataku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artyleria przeciwnika rozpoczęła ogień <w>nawalny</w> na nasze pozycje.",
                            "target_lang_translations": {
                                "ru": "Артиллерия противника открыла <w>шквальный</w> огонь по нашим позициям."
                            }
                        },
                        {
                            "text": "Żołnierze musieli ukryć się w okopach przed atakiem <w>nawalnym</w>.",
                            "target_lang_translations": {
                                "ru": "Солдатам пришлось укрыться в окопах от <w>массированной</w> атаки."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmasowany",
                        "masowy"
                    ],
                    "antonyms": [
                        "pojedynczy",
                        "sporadyczny"
                    ],
                    "common_phrases": [
                        "ogień nawalny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий в большом количестве или с большой интенсивностью, часто в контексте атаки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шквальный",
                                "target_lang_sense_clarification": "об огне или обстреле: очень интенсивный"
                            },
                            {
                                "target_lang_word": "массированный",
                                "target_lang_sense_clarification": "сосредоточенный, массовый (об атаке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67c0c933-9bfd-3e5f-8f72-c46152759209",
                    "sense_definition": "Cechujący się uporem, natręctwem lub gwałtownym napieraniem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>nawalny</w> styl bycia zniechęcał do niego wielu ludzi.",
                            "target_lang_translations": {
                                "ru": "Его <w>напористый</w> стиль поведения отталкивал от него многих людей."
                            }
                        },
                        {
                            "text": "Musieliśmy stawić czoła <w>nawalnym</w> prośbom o zmianę decyzji.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось столкнуться с <w>настойчивыми</w> просьбами об изменении решения."
                            }
                        }
                    ],
                    "synonyms": [
                        "natrętny",
                        "nieustępliwy",
                        "nachalny"
                    ],
                    "antonyms": [
                        "ustępliwy",
                        "dyskretny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся упорством, навязчивостью или резким напором."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напористый",
                                "target_lang_sense_clarification": "энергичный, пробивной"
                            },
                            {
                                "target_lang_word": "настойчивый",
                                "target_lang_sense_clarification": "упорный, не отступающий"
                            },
                            {
                                "target_lang_word": "навязчивый",
                                "target_lang_sense_clarification": "надоедливый"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
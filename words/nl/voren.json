            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ad903d7e-d153-3904-9a93-0f130337c345",
                    "sense_definition": "In voorwaartse richting.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_side",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij liep <w>naar voren</w> toen zijn naam genoemd werd.",
                            "target_lang_translations": {
                                "en": "He walked <w>forward</w> when his name was called.",
                                "ru": "Он вышел <w>вперёд</w>, когда назвали его имя."
                            }
                        },
                        {
                            "text": "Wil je alsjeblieft een stap <w>naar voren</w> doen?",
                            "target_lang_translations": {
                                "en": "Could you please take a step <w>forward</w>?",
                                "ru": "Не могли бы вы сделать шаг <w>вперёд</w>?"
                            }
                        }
                    ],
                    "antonyms": [
                        "achteren"
                    ],
                    "common_phrases": [
                        "naar voren",
                        "naar voren komen",
                        "naar voren brengen"
                    ],
                    "target_lang_definitions": {
                        "en": "In a forward direction.",
                        "ru": "В направлении вперёд."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forward",
                                "target_lang_sense_clarification": "direction of movement"
                            },
                            {
                                "target_lang_word": "to the front",
                                "target_lang_sense_clarification": "movement towards the front part"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вперёд",
                                "target_lang_sense_clarification": "направление движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58b7fa65-12b9-3a41-b92b-cf517963d1b0",
                    "sense_definition": "Aan of van de voorzijde.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "direction_side",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auto is <w>van voren</w> zwaar beschadigd na het ongeluk.",
                            "target_lang_translations": {
                                "en": "The car is heavily damaged <w>at the front</w> after the accident.",
                                "ru": "Машина сильно повреждена <w>спереди</w> после аварии."
                            }
                        },
                        {
                            "text": "We moeten weer helemaal <w>van voren</w> af aan beginnen.",
                            "target_lang_translations": {
                                "en": "We have to start all over <w>from the beginning</w>.",
                                "ru": "Нам придётся начать всё <w>сначала</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "achteren"
                    ],
                    "common_phrases": [
                        "van voren",
                        "van voren af aan"
                    ],
                    "target_lang_definitions": {
                        "en": "At or from the front side.",
                        "ru": "С передней стороны или у передней части."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from the front",
                                "target_lang_sense_clarification": "origin or perspective"
                            },
                            {
                                "target_lang_word": "at the front",
                                "target_lang_sense_clarification": "location"
                            },
                            {
                                "target_lang_word": "from the beginning",
                                "target_lang_sense_clarification": "idiomatic: starting over"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спереди",
                                "target_lang_sense_clarification": "местоположение или сторона"
                            },
                            {
                                "target_lang_word": "сначала",
                                "target_lang_sense_clarification": "идиоматическое: с самого начала"
                            }
                        ]
                    }
                }
            ]
        },
        {

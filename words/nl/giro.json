                },
                {
                    "sense_id": "82ff0170-8f58-35bc-a04b-2bc375b33693",
                    "sense_definition": "Een betaalsysteem waarmee geld tussen rekeningen wordt overgemaakt zonder contant geld, vaak geassocieerd met de voormalige postbank.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb het geld gisteren via de <w>giro</w> naar je overgemaakt.",
                            "target_lang_translations": {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b7328635-4a20-3b4a-a774-f51075175ce9",
                    "sense_definition": "De periode van de dag tussen het middaguur en de avond, van 12.00 tot 18.00 uur.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_of_day",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben in de <w>namiddag</w> een afspraak bij de tandarts.",
                            "target_lang_translations": {
                                "en": "We have an appointment at the dentist in the <w>afternoon</w>.",
                                "ru": "У нас назначена встреча у стоматолога во <w>второй половине дня</w>."
                            }
                        },
                        {
                            "text": "De kinderen spelen de hele <w>namiddag</w> buiten in de tuin.",
                            "target_lang_translations": {
                                "en": "The children play outside in the garden the whole <w>afternoon</w>.",
                                "ru": "Дети играют в саду всю <w>вторую половину дня</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "middag"
                    ],
                    "antonyms": [
                        "voormiddag"
                    ],
                    "common_phrases": [
                        "in de namiddag",
                        "hele namiddag"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "België"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The period of the day between noon and evening, from 12:00 PM to 6:00 PM.",
                        "ru": "Период дня между полуднем и вечером, с 12:00 до 18:00."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "afternoon",
                                "target_lang_sense_clarification": "period between noon and evening"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вторая половина дня",
                                "target_lang_sense_clarification": "время с полудня до вечера"
                            },
                            {
                                "target_lang_word": "послеобеденное время",
                                "target_lang_sense_clarification": "время после обеда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8dbbeefa-32c4-3d56-b243-937cd5be862e",
                    "sense_definition": "Het laatste deel van de middag, ongeveer tussen 16.00 en 18.00 uur.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_of_day",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de late <w>namiddag</w> begint de zon langzaam te zakken.",
                            "target_lang_translations": {
                                "en": "In the late <w>afternoon</w>, the sun starts to set slowly.",
                                "ru": "Поздним <w>днем</w> солнце начинает медленно садиться."
                            }
                        },
                        {
                            "text": "Op een koude <w>namiddag</w> dronken we warme chocolademelk.",
                            "target_lang_translations": {
                                "en": "On a cold <w>afternoon</w>, we drank hot chocolate.",
                                "ru": "В один холодный <w>день</w> мы пили горячий шоколад."
                            }
                        }
                    ],
                    "synonyms": [
                        "achtermiddag"
                    ],
                    "common_phrases": [
                        "late namiddag"
                    ],
                    "target_lang_definitions": {

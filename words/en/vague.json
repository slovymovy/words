                        {
                            "text": "In ancient times, <w>vague</w> tribes roamed these lands.",
                            "target_lang_translations": {
                                "ru": "В древние времена <w>кочевые</w> племена бродили по этим землям."
                            }
                        }
                    ],
                    "synonyms": [
                        "wandering",
                        "vagrant",
                        "roaming",
                        "vagabond"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

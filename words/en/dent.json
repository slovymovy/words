{
    "word_family": [
        "indent",
        "indentation",
        "undented"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2c8a04ba-2bee-3757-9863-617ecc0f4d52",
                    "sense_definition": "A shallow hollow or depression in the surface of an object, usually caused by a blow or pressure.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_indentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accident left a large <w>dent</w> in the passenger door of his car.",
                            "target_lang_translations": {
                                "ru": "После аварии на пассажирской двери его машины осталась большая <w>вмятина</w>."
                            }
                        },
                        {
                            "text": "She tried to hammer out the <w>dent</w> in the metal sheet.",
                            "target_lang_translations": {
                                "ru": "Она пыталась выправить <w>вмятину</w> на металлическом листе молотком."
                            }
                        }
                    ],
                    "synonyms": [
                        "indentation",
                        "hollow",
                        "depression",
                        "ding",
                        "nick"
                    ],
                    "antonyms": [
                        "bulge",
                        "bump",
                        "protuberance"
                    ],
                    "common_phrases": [
                        "dent in the car",
                        "huge dent",
                        "fix a dent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неглубокое углубление или впадина на поверхности предмета, обычно возникшее от удара или давления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вмятина",
                                "target_lang_sense_clarification": "на поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13f38fb5-5f7b-3266-b93d-2c3ba42a9708",
                    "sense_definition": "A reduction in amount or a negative effect on something, such as one's finances or progress.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_indentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The expensive vacation put a significant <w>dent</w> in our savings.",
                            "target_lang_translations": {
                                "ru": "Дорогой отпуск пробил значительную <w>брешь</w> в наших сбережениях."
                            }
                        },
                        {
                            "text": "We have been working all day, but we have hardly made a <w>dent</w> in the pile of paperwork.",
                            "target_lang_translations": {
                                "ru": "Мы работали весь день, но едва <w>уменьшили</w> эту кипу бумаг."
                            }
                        }
                    ],
                    "synonyms": [
                        "reduction",
                        "diminution",
                        "dip"
                    ],
                    "antonyms": [
                        "boost",
                        "increase"
                    ],
                    "common_phrases": [
                        "make a dent",
                        "put a dent in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшение количества или негативное воздействие на что-либо, например, на финансы или прогресс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брешь",
                                "target_lang_sense_clarification": "в бюджете или защите"
                            },
                            {
                                "target_lang_word": "ущерб",
                                "target_lang_sense_clarification": "урон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7745257-cd8b-3f5c-bf15-c013650fc476",
                    "sense_definition": "A variety of maize (corn) with kernels that have a small indentation on the top.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_maize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Farmers in this region primarily grow yellow <w>dent</w> corn for livestock feed.",
                            "target_lang_translations": {
                                "ru": "Фермеры в этом регионе выращивают в основном желтую <w>зубовидную</w> кукурузу на корм скоту."
                            }
                        },
                        {
                            "text": "The <w>dent</w> variety is distinguished by the small depression on each kernel.",
                            "target_lang_translations": {
                                "ru": "<w>Зубовидный</w> сорт отличается небольшим углублением на каждом зерне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dent corn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность кукурузы (маиса), зерна которой имеют небольшое углубление на верхушке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зубовидная кукуруза",
                                "target_lang_sense_clarification": "сорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "330978bb-e0ec-3541-b6dd-1c3a6efe6fae",
                    "sense_definition": "A tooth-like projection, such as on a gear wheel or a carding machine.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_tooth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "One <w>dent</w> on the old gear was chipped, causing the machine to jam.",
                            "target_lang_translations": {
                                "ru": "Один <w>зубец</w> на старой шестерне откололся, из-за чего машину заклинило."
                            }
                        },
                        {
                            "text": "The mechanic carefully inspected every <w>dent</w> of the wheel.",
                            "target_lang_translations": {
                                "ru": "Механик внимательно осмотрел каждый <w>зуб</w> колеса."
                            }
                        }
                    ],
                    "synonyms": [
                        "tooth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зубцевидный выступ, например, на зубчатом колесе или чесальной машине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зубец",
                                "target_lang_sense_clarification": "шестерни"
                            },
                            {
                                "target_lang_word": "зуб",
                                "target_lang_sense_clarification": "механизма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5e11b3a2-2cbd-3541-b57c-088a5fcb2111",
                    "sense_definition": "To make a hollow area in a surface by hitting or pressing it, or to become shaped this way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_indentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to <w>dent</w> the table when you move the heavy furniture.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны, чтобы не <w>помять</w> стол, когда будете двигать тяжелую мебель."
                            }
                        },
                        {
                            "text": "Aluminum is a soft metal that <w>dents</w> quite easily.",
                            "target_lang_translations": {
                                "ru": "Алюминий — мягкий металл, который довольно легко <w>мнется</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "indent",
                        "ding"
                    ],
                    "common_phrases": [
                        "dent the surface",
                        "dent easily"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать углубление на поверхности ударом или нажатием, или деформироваться таким образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помять",
                                "target_lang_sense_clarification": "сделать вмятину"
                            },
                            {
                                "target_lang_word": "оставить вмятину",
                                "target_lang_sense_clarification": "повредить поверхность"
                            },
                            {
                                "target_lang_word": "мяться",
                                "target_lang_sense_clarification": "деформироваться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a229291e-27ab-3b5c-8c3a-24f60fa904e7",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Dent</w> has been working at the company for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Дент</w> работает в компании уже более двадцати лет."
                            }
                        },
                        {
                            "text": "We invited the <w>Dent</w> family to the neighborhood gathering.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили семью <w>Дент</w> на соседскую вечеринку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дент",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15e75dcf-87cc-3ae9-a0b9-1e2876ff7096",
                    "sense_definition": "A village and civil parish in Cumbria, England.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_names",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They planned a hiking trip to the hills surrounding <w>Dent</w>.",
                            "target_lang_translations": {
                                "ru": "Они запланировали поход по холмам в окрестностях <w>Дента</w>."
                            }
                        },
                        {
                            "text": "The cobbled streets of <w>Dent</w> are popular with tourists.",
                            "target_lang_translations": {
                                "ru": "Мощеные улицы <w>Дента</w> популярны среди туристов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревня и гражданский приход в Камбрии, Англия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дент",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "pusher",
        "pushy",
        "pushover",
        "pushchair",
        "pushup",
        "push-button"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e4cb934f-06c4-31bf-88b9-67dec7578d15",
                    "sense_definition": "To apply force to an object such that it moves away from the person or thing applying the force.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>push</w> the door hard to open it.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сильно <w>толкнуть</w> дверь, чтобы открыть её."
                            }
                        },
                        {
                            "text": "He <w>pushed</w> his bike up the steep hill.",
                            "target_lang_translations": {
                                "ru": "Он <w>толкал</w> свой велосипед вверх по крутому склону."
                            }
                        }
                    ],
                    "synonyms": [
                        "shove",
                        "press",
                        "thrust"
                    ],
                    "antonyms": [
                        "pull",
                        "draw",
                        "tug"
                    ],
                    "common_phrases": [
                        "push the button",
                        "push the door",
                        "push a cart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Применять силу к объекту, чтобы отодвинуть его от человека или предмета, прилагающего силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толкать",
                                "target_lang_sense_clarification": "применять физическую силу (несов. вид)"
                            },
                            {
                                "target_lang_word": "толкнуть",
                                "target_lang_sense_clarification": "применить физическую силу (сов. вид)"
                            },
                            {
                                "target_lang_word": "пихать",
                                "target_lang_sense_clarification": "грубо толкать (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7a7b833-94ed-3c5a-bf66-8dacfd607fdf",
                    "sense_definition": "To continually attempt to persuade a person into a particular course of action.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My parents always <w>push</w> me to study harder.",
                            "target_lang_translations": {
                                "ru": "Мои родители всегда <w>подталкивают</w> меня учиться усерднее."
                            }
                        },
                        {
                            "text": "We are being <w>pushed</w> for an answer by the committee.",
                            "target_lang_translations": {
                                "ru": "Комитет <w>торопит</w> нас с ответом."
                            }
                        }
                    ],
                    "synonyms": [
                        "urge",
                        "pressure",
                        "force"
                    ],
                    "common_phrases": [
                        "push someone to do something",
                        "push for an answer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянно пытаться убедить человека совершить определенное действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подталкивать",
                                "target_lang_sense_clarification": "побуждать к действию"
                            },
                            {
                                "target_lang_word": "давить",
                                "target_lang_sense_clarification": "оказывать психологическое давление"
                            },
                            {
                                "target_lang_word": "настаивать",
                                "target_lang_sense_clarification": "требовать выполнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a593b69-b65a-3c14-b402-a592f53057bd",
                    "sense_definition": "To continually exert oneself in order to achieve a goal.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>push</w> yourself if you want to succeed.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>заставлять себя</w>, если хочешь добиться успеха."
                            }
                        },
                        {
                            "text": "Don't <w>push</w> too hard or you might get injured.",
                            "target_lang_translations": {
                                "ru": "Не <w>усердствуй</w> слишком сильно, иначе можешь получить травму."
                            }
                        }
                    ],
                    "synonyms": [
                        "strive",
                        "struggle"
                    ],
                    "common_phrases": [
                        "push oneself",
                        "push the limits"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянно прилагать усилия, заставлять себя работать на пределе для достижения цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заставлять себя",
                                "target_lang_sense_clarification": "принуждать себя к действию"
                            },
                            {
                                "target_lang_word": "напрягаться",
                                "target_lang_sense_clarification": "прилагать усилия"
                            },
                            {
                                "target_lang_word": "усердствовать",
                                "target_lang_sense_clarification": "проявлять рвение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8356d089-4d00-33cc-a194-eeeef8b73b4d",
                    "sense_definition": "To continually promote a point of view or a product for sale.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is <w>pushing</w> its new perfume aggressively.",
                            "target_lang_translations": {
                                "ru": "Компания агрессивно <w>продвигает</w> свои новые духи."
                            }
                        },
                        {
                            "text": "Stop <w>pushing</w> the issue because I am not interested.",
                            "target_lang_translations": {
                                "ru": "Перестань <w>педалировать</w> эту тему, потому что мне это не интересно."
                            }
                        }
                    ],
                    "synonyms": [
                        "promote",
                        "advertise"
                    ],
                    "common_phrases": [
                        "push a product",
                        "push an idea"
                    ],
                    "target_lang_definitions": {
                        "ru": "Активно продвигать точку зрения или товар на продажу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продвигать",
                                "target_lang_sense_clarification": "способствовать распространению"
                            },
                            {
                                "target_lang_word": "рекламировать",
                                "target_lang_sense_clarification": "объявлять о товаре"
                            },
                            {
                                "target_lang_word": "проталкивать",
                                "target_lang_sense_clarification": "настойчиво продвигать (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5dd882b5-8f14-3f66-aadd-eb28f12f812a",
                    "sense_definition": "To approach or come close to a specific age, number, or limit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "approach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My old car is <w>pushing</w> 200,000 miles.",
                            "target_lang_translations": {
                                "ru": "Пробег моей старой машины <w>приближается к</w> 200 000 миль."
                            }
                        },
                        {
                            "text": "He is <w>pushing</w> sixty but still looks very young.",
                            "target_lang_translations": {
                                "ru": "Ему <w>под</w> шестьдесят, но он все еще выглядит очень молодо."
                            }
                        }
                    ],
                    "synonyms": [
                        "approach",
                        "near"
                    ],
                    "common_phrases": [
                        "pushing forty",
                        "pushing sixty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приближаться к определенному возрасту, числу или пределу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приближаться к",
                                "target_lang_sense_clarification": "подходить к значению"
                            },
                            {
                                "target_lang_word": "подходить к",
                                "target_lang_sense_clarification": "быть близким к"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a789d159-ea93-30d4-bb2c-ef011fe31adc",
                    "sense_definition": "To add a data item to the top of a stack.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The function <w>pushes</w> the value onto the stack.",
                            "target_lang_translations": {
                                "ru": "Функция <w>помещает</w> значение в стек."
                            }
                        },
                        {
                            "text": "When you <w>push</w> data, it is stored for later retrieval.",
                            "target_lang_translations": {
                                "ru": "Когда вы <w>помещаете</w> данные, они сохраняются для последующего извлечения."
                            }
                        }
                    ],
                    "antonyms": [
                        "pop"
                    ],
                    "common_phrases": [
                        "push onto the stack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавлять элемент данных на вершину стека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помещать",
                                "target_lang_sense_clarification": "класть в стек"
                            },
                            {
                                "target_lang_word": "записывать",
                                "target_lang_sense_clarification": "сохранять данные"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6cf57d0b-4b41-31fb-8eaf-43b222df5909",
                    "sense_definition": "A short, directed application of force; an act of pushing.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Give the door a hard <w>push</w> if it sticks.",
                            "target_lang_translations": {
                                "ru": "Сильно <w>толкни</w> дверь, если она заедает."
                            }
                        },
                        {
                            "text": "With one strong <w>push</w>, the car started moving.",
                            "target_lang_translations": {
                                "ru": "От одного сильного <w>толчка</w> машина начала двигаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "shove",
                        "thrust"
                    ],
                    "antonyms": [
                        "pull",
                        "tug"
                    ],
                    "common_phrases": [
                        "give a push",
                        "a gentle push"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткое направленное применение силы; акт толкания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толчок",
                                "target_lang_sense_clarification": "резкое движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96745013-ff44-3b13-afa6-555ed00f497b",
                    "sense_definition": "A great or determined effort to do something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need one last <w>push</w> to finish the project on time.",
                            "target_lang_translations": {
                                "ru": "Нам нужен один последний <w>рывок</w>, чтобы закончить проект вовремя."
                            }
                        },
                        {
                            "text": "The army made a major <w>push</w> to retake the city.",
                            "target_lang_translations": {
                                "ru": "Армия предприняла масштабное <w>наступление</w>, чтобы вернуть город."
                            }
                        }
                    ],
                    "synonyms": [
                        "drive",
                        "campaign",
                        "effort"
                    ],
                    "common_phrases": [
                        "the final push",
                        "a big push"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое или решительное усилие сделать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рывок",
                                "target_lang_sense_clarification": "резкое усиление действий"
                            },
                            {
                                "target_lang_word": "усилие",
                                "target_lang_sense_clarification": "напряжение сил"
                            },
                            {
                                "target_lang_word": "наступление",
                                "target_lang_sense_clarification": "активные действия (воен.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "276dbc35-53b7-37dc-938b-07f0f3ff2211",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Push</w> was listed in the directory.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Пуш</w> был указан в справочнике."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>Push</w> yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера я встретил женщину по имени Сара <w>Пуш</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",

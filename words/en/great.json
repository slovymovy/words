{
    "word_family": [
        "greatly",
        "greatness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "880a2ba6-f834-3350-837f-651a4ee5e2e3",
                    "sense_definition": "Very good, wonderful, or fantastic.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quality_positive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The food at the party was absolutely <w>great</w>.",
                            "target_lang_translations": {
                                "ru": "Еда на вечеринке была просто <w>отличной</w>."
                            }
                        },
                        {
                            "text": "We had a <w>great</w> time visiting the museum yesterday.",
                            "target_lang_translations": {
                                "ru": "Мы <w>отлично</w> провели время в музее вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "gr8"
                    ],
                    "common_phrases": [
                        "great job",
                        "look great",
                        "feel great",
                        "great idea"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень хороший, замечательный или фантастический."
                    },
                    "translations": {
                        "ru": [
                            {

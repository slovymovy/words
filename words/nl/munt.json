    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a0e34b27-dc18-3ef4-9263-ee36559d5f01",
                    "sense_definition": "Een plat, meestal rond stuk metaal dat een bepaalde waarde vertegenwoordigt en als geld wordt gebruikt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "money_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb een <w>munt</w> van twee euro in mijn zak.",
                            "target_lang_translations": {
                                "en": "I have a two-euro <w>coin</w> in my pocket.",
                                "ru": "У меня в кармане <w>монета</w> в два евро."
                            }
                        },
                        {
                            "text": "Hij gooide een <w>munt</w> in de automaat om te betalen.",
                            "target_lang_translations": {
                                "en": "He threw a <w>coin</w> into the machine to pay.",
                                "ru": "Он бросил <w>монету</w> в автомат, чтобы заплатить."
                            }
                        }
                    ],
                    "synonyms": [
                        "muntstuk"
                    ],
                    "common_phrases": [
                        "een munt slaan",
                        "valse munt"
                    ],
                    "target_lang_definitions": {
                        "en": "A flat, usually round piece of metal that represents a certain value and is used as money.",
                        "ru": "Плоский, обычно круглый кусок металла, который представляет определенную ценность и используется в качестве денег."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coin",
                                "target_lang_sense_clarification": "piece of metal money"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "монета",
                                "target_lang_sense_clarification": "денежный знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1353abb-37d3-342f-91c0-b22734db2532",
                    "sense_definition": "Een plant met sterk geurende bladeren die vaak wordt gebruikt om thee van te trekken of als smaakmaker in gerechten.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik drink graag thee met verse <w>munt</w> en honing.",
                            "target_lang_translations": {
                                "en": "I like drinking tea with fresh <w>mint</w> and honey.",
                                "ru": "Я люблю пить чай со свежей <w>мятой</w> и медом."
                            }
                        },
                        {
                            "text": "In onze tuin groeit de <w>munt</w> erg snel.",
                            "target_lang_translations": {
                                "en": "The <w>mint</w> grows very fast in our garden.",
                                "ru": "В нашем саду <w>мята</w> растет очень быстро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "verse munt",
                        "thee met munt"
                    ],
                    "target_lang_definitions": {
                        "en": "A plant with strongly scented leaves that is often used to make tea or as a flavoring in dishes.",
                        "ru": "Растение с сильно пахнущими листьями, которое часто используется для заваривания чая или как приправа в блюдах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mint",
                                "target_lang_sense_clarification": "herb/plant"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мята",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53876d40-a36e-366f-85ad-284398494537",
                    "sense_definition": "Het officiële geldstelsel dat in een bepaald land of gebied als betaalmiddel geldt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De euro is een sterke <w>munt</w> in Europa.",
                            "target_lang_translations": {
                                "en": "The euro is a strong <w>currency</w> in Europe.",
                                "ru": "Евро — сильная <w>валюта</w> в Европе."
                            }
                        },
                        {
                            "text": "In welk land wordt deze vreemde <w>munt</w> gebruikt?",
                            "target_lang_translations": {
                                "en": "In which country is this foreign <w>currency</w> used?",
                                "ru": "В какой стране используется эта иностранная <w>валюта</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "munteenheid"
                    ],
                    "common_phrases": [
                        "vreemde munt",
                        "stabiele munt"
                    ],
                    "target_lang_definitions": {
                        "en": "The official monetary system that serves as a means of payment in a specific country or area.",
                        "ru": "Официальная денежная система, действующая в определенной стране или регионе как средство платежа."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "currency",
                                "target_lang_sense_clarification": "monetary system"

                        {
                            "text": "The project is finally <w>done</w> after months of effort.",
                            "target_lang_translations": {
                                "ru": "Проект наконец <w>закончен</w> после месяцев усилий."
                            }
                        }
                    ],
                    "synonyms": [
                        "finished",
                        "completed",
                        "concluded"
                    ],
                    "antonyms": [
                        "unfinished",
                        "incomplete"
                    ],
                    "common_phrases": [
                        "well done",
                        "half done",
                        "easier said than done"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f0c38906-3fc4-357d-870d-606468a53986",
                    "sense_definition": "Een uitrit die leidt van een grotere weg (zoals een snelweg) naar een kleinere weg of andere bestemming.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten de volgende <w>afslag</w> nemen om bij het hotel te komen.",
                            "target_lang_translations": {
                                "en": "We have to take the next <w>exit</w> to get to the hotel.",
                                "ru": "Нам нужно свернуть на следующем <w>съезде</w>, чтобы добраться до отеля."
                            }
                        },
                        {
                            "text": "Hij miste de <w>afslag</w> naar Utrecht en moest een stuk omrijden.",
                            "target_lang_translations": {
                                "en": "He missed the <w>exit</w> to Utrecht and had to make a detour.",
                                "ru": "Он пропустил <w>съезд</w> на Утрехт и был вынужден сделать крюк."
                            }
                        }
                    ],
                    "synonyms": [
                        "afrit"
                    ],
                    "common_phrases": [
                        "de afslag nemen",
                        "de afslag missen",
                        "de eerste afslag"
                    ],
                    "target_lang_definitions": {
                        "en": "An exit leading from a major road (such as a highway) to a smaller road or other destination.",
                        "ru": "Съезд с большой дороги (например, шоссе) на меньшую дорогу или к другому пункту назначения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exit",
                                "target_lang_sense_clarification": "from a highway"
                            },
                            {
                                "target_lang_word": "turnoff",
                                "target_lang_sense_clarification": "road branching off"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "съезд",
                                "target_lang_sense_clarification": "с дороги"
                            },
                            {
                                "target_lang_word": "поворот",
                                "target_lang_sense_clarification": "на дороге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4087e24-85dd-3490-83bd-12dd7b519c6b",
                    "sense_definition": "Een openbare verkoping (vaak van vis) waarbij de prijs begint bij een hoog bedrag en zakt totdat iemand koopt.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vissers brengen hun vangst direct naar de <w>afslag</w> in de haven.",
                            "target_lang_translations": {
                                "en": "The fishermen bring their catch directly to the <w>auction</w> in the harbor.",
                                "ru": "Рыбаки привозят свой улов прямо на <w>аукцион</w> в порту."
                            }
                        },
                        {
                            "text": "Bij verkoop bij <w>afslag</w> moet de koper snel beslissen voordat iemand anders toeslaat.",
                            "target_lang_translations": {
                                "en": "When selling by <w>Dutch auction</w>, the buyer must decide quickly before someone else strikes.",
                                "ru": "При продаже на <w>аукционе на понижение</w> покупатель должен решать быстро, пока не перехватил кто-то другой."
                            }
                        }
                    ],
                    "antonyms": [
                        "opbod"
                    ],
                    "common_phrases": [
                        "vis aan de afslag",
                        "bij afslag verkopen"
                    ],
                    "target_lang_definitions": {
                        "en": "A public sale (often of fish) where the price starts at a high amount and drops until someone buys.",
                        "ru": "Публичная распродажа (часто рыбы), при которой цена начинается с высокой и снижается, пока кто-нибудь не купит."
                    },

                {
                    "sense_id": "2bd7d945-46f4-3bad-9b5e-4fb41931e6f1",
                    "sense_definition": "Trzecia osoba liczby mnogiej (rodzaj niemęskoosobowy) czasu przeszłego od czasownika <w>wściec</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiadki <w>wściekły</w> się na hałas dobiegający z ulicy.",
                            "target_lang_translations": {
                                "ru": "Соседки <w>взбесились</w> из-за шума, доносившегося с улицы."
                            }
                        },
                        {
                            "text": "Kiedy zabrakło prądu, wszystkie maszyny w fabryce się <w>wściekły</w>.",
                            "target_lang_translations": {
                                "ru": "Когда отключили электричество, все машины на фабрике <w>взбесились</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени глагола 'wściec się' (разозлиться) для женского и среднего рода во множественном числе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взбесились",
                                "target_lang_sense_clarification": "пришли в бешенство"
                            },
                            {
                                "target_lang_word": "разозлились",
                                "target_lang_sense_clarification": "стали злыми"
                            }
                        ]
                    }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f743e065-8726-3380-9a9c-b0596cc95631",
                    "sense_definition": "Rozłożyć całość na mniejsze części lub rozdać coś pomiędzy kilka osób",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "division_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Matka sprawiedliwie <podzieliła> jabłko na cztery równe kawałki.",
                            "target_lang_translations": {
                                "ru": "Мать справедливо <разделила> яблоко на четыре равных куска."
                            }
                        },
                        {
                            "text": "Musimy <podzielić> zadania w zespole, aby zdążyć z projektem na czas.",
                            "target_lang_translations": {
                                "ru": "Мы должны <распределить> задачи в команде, чтобы успеть с проектом вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozdzielić się"
                    ],
                    "antonyms": [
                        "połączyć",
                        "scalić"
                    ],
                    "common_phrases": [
                        "podzielić na pół",
                        "podzielić na części",
                        "podzielić między ludzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разложить целое на более мелкие части или раздать что-то нескольким людям"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разделить",
                                "target_lang_sense_clarification": "на части"
                            },
                            {
                                "target_lang_word": "поделить",
                                "target_lang_sense_clarification": "распределить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0550b7e5-fbb4-3007-a5d0-a53f519dbac8",
                    "sense_definition": "Dać komuś część tego, co się posiada, lub pozwolić komuś korzystać ze swojej własności (używane z zaimkiem „się”)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sharing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek <podzielił> się kanapką z głodnym kolegą ze szkoły.",
                            "target_lang_translations": {
                                "ru": "Марек <поделился> бутербродом с голодным школьным товарищем."
                            }
                        },
                        {
                            "text": "Chciałbym <podzielić> się z tobą dobrą wiadomością, którą właśnie usłyszałem.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <поделиться> с тобой хорошей новостью, которую только что услышал."
                            }
                        }
                    ],
                    "antonyms": [
                        "zachować"
                    ],
                    "common_phrases": [
                        "podzielić się jedzeniem",
                        "podzielić się opłatkiem",
                        "podzielić się wrażeniami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать кому-то часть того, что имеешь, или позволить пользоваться своей собственностью (используется с возвратным местоимением)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поделиться",
                                "target_lang_sense_clarification": "чем-то с кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb51dc69-2477-3355-9afe-775d05ee53a0",
                    "sense_definition": "Wykonać działanie matematyczne polegające na obliczeniu, ile razy jedna liczba mieści się w drugiej",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "division_math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił ucznia, aby <podzielił> dwadzieścia przez cztery.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика, чтобы тот <разделил> двадцать на четыре."
                            }
                        },
                        {
                            "text": "Trudno jest <podzielić> tak dużą kwotę w pamięci bez kalkulatora.",
                            "target_lang_translations": {
                                "ru": "Трудно <разделить> такую большую сумму в уме без калькулятора."
                            }
                        }
                    ],
                    "antonyms": [
                        "pomnożyć"
                    ],
                    "common_phrases": [
                        "podzielić przez dwa",
                        "podzielić liczby",
                        "podzielić w pamięci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнить математическое действие, вычисляя, сколько раз одно число содержится в другом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разделить",
                                "target_lang_sense_clarification": "математически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30b4cd1c-5152-37a0-876d-bcdaf3df7370",
                    "sense_definition": "Sprawić, że ludzie przestają być zgodni i zaczynają się kłócić lub mieć odmienne zdania",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta kontrowersyjna decyzja polityczna głęboko <podzieliła> społeczeństwo.",
                            "target_lang_translations": {
                                "ru": "Это спорное политическое решение глубоко <разделило> общество."
                            }
                        },
                        {
                            "text": "Spór o spadek niestety trwale <podzielił> rodzeństwo.",
                            "target_lang_translations": {
                                "ru": "Спор о наследстве, к сожалению, навсегда <разделил> братьев и сестер."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozdzielić się"
                    ],
                    "antonyms": [
                        "zjednoczyć",
                        "pogodzić"
                    ],
                    "common_phrases": [
                        "podzielić naród",
                        "podzielić rodzinę",
                        "kwestia podzieliła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать причиной разногласий, ссор или отсутствия единства между людьми"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разделить",
                                "target_lang_sense_clarification": "общество, людей"
                            },
                            {
                                "target_lang_word": "разобщить",
                                "target_lang_sense_clarification": "внести разлад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81f9be8f-5bd2-322c-b7d5-dc7590f51bd0",
                    "sense_definition": "Zgodzić się z czyimś poglądem lub doświadczyć tego samego losu co inna osoba",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sharing_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że komisja <podzieli> moje zdanie w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Я надеюсь, что комиссия <разделит> мое мнение по этому вопросу."
                            }
                        },
                        {
                            "text": "Wielu żołnierzy <podzieliło> tragiczny los swojego dowódcy na polu bitwy.",
                            "target_lang_translations": {
                                "ru": "Многие солдаты <разделили> трагическую участь своего командира на поле битвы."
                            }
                        }
                    ],
                    "antonyms": [
                        "zakwestionować",
                        "odrzucić"
                    ],
                    "common_phrases": [
                        "podzielić los",
                        "podzielić zdanie",
                        "podzielić opinię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Согласиться с чьим-то мнением или пережить ту же участь, что и другой человек"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разделить",
                                "target_lang_sense_clarification": "мнение, судьбу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
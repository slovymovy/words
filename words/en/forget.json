{
    "word_family": [
        "forgetful",
        "forgetfulness",
        "unforgettable",
        "unforgotten"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8a4ffff9-3369-3fe7-b8ad-7df4b2164ebd",
                    "sense_definition": "To be unable to remember a fact, information, or past event.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cognitive_memory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have completely <w>forgotten</w> his name even though we met yesterday.",
                            "target_lang_translations": {
                                "ru": "Я совершенно <w>забыл</w> его имя, хотя мы встретились вчера."
                            }
                        },
                        {
                            "text": "She will never <w>forget</w> the wonderful time she had in Paris.",
                            "target_lang_translations": {
                                "ru": "Она никогда не <w>забудет</w> то чудесное время, которое она провела в Париже."
                            }
                        }
                    ],
                    "synonyms": [
                        "disremember"
                    ],
                    "antonyms": [
                        "recall",
                        "recollect",
                        "remember",
                        "reminisce"
                    ],
                    "common_phrases": [
                        "forget the past",
                        "never forget",
                        "completely forget"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть не в состоянии вспомнить факт, информацию или прошлое событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "не помнить"
                            },
                            {
                                "target_lang_word": "забывать",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc700143-632d-3be4-904b-380d603fdbe4",
                    "sense_definition": "To fail to perform a planned or necessary action because of not remembering.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>forget</w> to lock the door when you leave the house.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>забудь</w> запереть дверь, когда будешь уходить из дома."
                            }
                        },
                        {
                            "text": "I <w>forgot</w> to buy milk on my way home from work.",
                            "target_lang_translations": {
                                "ru": "Я <w>забыл</w> купить молоко по дороге домой с работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "overlook",
                        "pass over",
                        "forget"
                    ],
                    "antonyms": [
                        "remember"
                    ],
                    "common_phrases": [
                        "forget to ask",
                        "forget to mention",
                        "forget to call"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не выполнить запланированное или необходимое действие из-за того, что оно вылетело из головы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "сделать что-то"
                            },
                            {
                                "target_lang_word": "запамятовать",
                                "target_lang_sense_clarification": "формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d189e3a-0b22-3db5-8d5d-b520dac01e25",
                    "sense_definition": "To unintentionally leave something in a place.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>forgot</w> my keys on the kitchen table this morning.",
                            "target_lang_translations": {
                                "ru": "Я <w>забыл</w> свои ключи на кухонном столе сегодня утром."
                            }
                        },
                        {
                            "text": "He always <w>forgets</w> his umbrella on the bus when it stops raining.",
                            "target_lang_translations": {
                                "ru": "Он всегда <w>забывает</w> свой зонт в автобусе, когда прекращается дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "forget",
                        "forget"
                    ],
                    "antonyms": [
                        "remember"
                    ],
                    "common_phrases": [
                        "forget my keys",
                        "forget at home",
                        "forget my wallet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случайно оставить вещь в каком-либо месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "оставить вещь"
                            },
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "не взять с собой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "343ffb37-1a6a-34f4-8abf-2c046987b108",
                    "sense_definition": "To intentionally stop thinking about something or put it out of one's mind.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cognitive_memory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let's just <w>forget</w> this argument ever happened and be friends again.",
                            "target_lang_translations": {
                                "ru": "Давай просто <w>забудем</w>, что этот спор вообще был, и снова будем друзьями."
                            }
                        },
                        {
                            "text": "You need to <w>forget</w> about the mistakes you made and move forward.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>забыть</w> об ошибках, которые ты совершил, и двигаться дальше."
                            }
                        }
                    ],
                    "synonyms": [
                        "obliviate"
                    ],
                    "common_phrases": [
                        "forget it",
                        "forgive and forget",
                        "forget about it"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренно перестать думать о чём-либо или выбросить это из головы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "перестать думать"
                            },
                            {
                                "target_lang_word": "выкинуть из головы",
                                "target_lang_sense_clarification": "фразеологизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "204f571d-1163-3c10-9344-1c8b54f7fd91",
                    "sense_definition": "To fail to consider or realize a specific fact or situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cognitive_realization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "People often <w>forget</w> how much hard work goes into running a business.",
                            "target_lang_translations": {
                                "ru": "Люди часто <w>забывают</w>, сколько тяжелого труда уходит на ведение бизнеса."
                            }
                        },
                        {
                            "text": "He <w>forgot</w> that she was a vegetarian and cooked a meat dish.",
                            "target_lang_translations": {
                                "ru": "Он <w>забыл</w>, что она вегетарианка, и приготовил мясное блюдо."
                            }
                        }
                    ],
                    "synonyms": [
                        "overlook"
                    ],
                    "antonyms": [
                        "mind"
                    ],
                    "common_phrases": [
                        "forget that",
                        "people often forget"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упустить из виду или не осознать определенный факт или ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забывать",
                                "target_lang_sense_clarification": "упускать из виду"
                            },
                            {
                                "target_lang_word": "не учитывать",
                                "target_lang_sense_clarification": "игнорировать факт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecbfed68-dceb-3181-ad89-353040f56419",
                    "sense_definition": "Used as a mild expression of annoyance or dismissal, replacing a stronger expletive.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Forget</w> you, I am not going to help you after what you said!",
                            "target_lang_translations": {
                                "ru": "<w>Да ну</w> тебя, я не собираюсь помогать тебе после того, что ты сказал!"
                            }
                        },
                        {
                            "text": "If that is how you feel, then <w>forget</w> this whole plan.",
                            "target_lang_translations": {
                                "ru": "Если ты так считаешь, тогда <w>забудь</w> про весь этот план."
                            }
                        }
                    ],
                    "common_phrases": [
                        "forget you",
                        "forget this"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "euphemism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как мягкое выражение раздражения или отказа, заменяя более грубые ругательства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забудь",
                                "target_lang_sense_clarification": "отказ"
                            },
                            {
                                "target_lang_word": "да ну",
                                "target_lang_sense_clarification": "раздражение"
                            },
                            {
                                "target_lang_word": "к чёрту",
                                "target_lang_sense_clarification": "эмоционально"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "3b1ba43d-5f1b-3cc9-b312-3ccd754990cb",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Forget</w> has lived in this neighborhood for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Форгет</w> живет в этом районе уже более двадцати лет."
                            }
                        },
                        {
                            "text": "We are looking for a file on an employee named <w>Forget</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ищем досье на сотрудника по фамилии <w>Форгет</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

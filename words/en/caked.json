                            },
                            {
                                "target_lang_word": "облеплять",
                                "target_lang_sense_clarification": "покрывать поверхность толстым слоем"
                            },
                            {
                                "target_lang_word": "спекаться",
                                "target_lang_sense_clarification": "слёживаться в твердую массу"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e443995b-6854-33ce-a063-735105c6ef4a",
                    "sense_definition": "Ответная реплика на приветствие, поздравление или комплимент, означающая «и вам того же» или «мне тоже».",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Очень рад с вами познакомиться! — <Взаимно>!",
                            "target_lang_translations": {
                                "en": "— Very nice to meet you! — <Likewise>!"
                            }
                        },
                        {
                            "text": "— Желаю отличных выходных! — Спасибо, <взаимно>.",
                            "target_lang_translations": {
                                "en": "— Have a great weekend! — Thanks, <same to you>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A response to a greeting, congratulation, or compliment, meaning \"the same to you\" or \"me too\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "likewise",
                                "target_lang_sense_clarification": "common response to \"nice to meet you\""
                            },
                            {
                                "target_lang_word": "same to you",
                                "target_lang_sense_clarification": "used to return good wishes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15117096-5d29-397f-91e8-a6d9633f37bb",
                    "sense_definition": "Совершаясь с обеих сторон, в равной или сопоставимой мере по отношению друг к другу.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эти два утверждения <взаимно> исключают друг друга.",
                            "target_lang_translations": {
                                "en": "These two statements <mutually> exclude each other."
                            }
                        },
                        {
                            "text": "Сотрудничество между нашими фирмами было <взаимно> выгодным.",
                            "target_lang_translations": {
                                "en": "The cooperation between our firms was <mutually> beneficial."
                            }
                        }
                    ],
                    "synonyms": [
                        "обоюдно",
                        "двусторонне"
                    ],
                    "antonyms": [
                        "односторонне"
                    ],
                    "common_phrases": [
                        "взаимно выгодный",
                        "взаимно исключать",
                        "взаимно вежливы"
                    ],
                    "target_lang_definitions": {
                        "en": "Occurring on both sides, equally or comparably in relation to each other."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mutually",
                                "target_lang_sense_clarification": "in a shared manner"
                            },
                            {
                                "target_lang_word": "reciprocally",
                                "target_lang_sense_clarification": "in a way that involves exchange"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b779393e-ee2b-3caa-8d31-ef5c8e765690",
                    "sense_definition": "Краткая форма среднего рода прилагательного <w>взаимный</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это чувство было глубоко и <взаимно>.",
                            "target_lang_translations": {
                                "en": "This feeling was deep and <mutual>."
                            }
                        },
                        {
                            "text": "Влияние культур друг на друга всегда <взаимно>.",
                            "target_lang_translations": {
                                "en": "The influence of cultures on each other is always <mutual>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "чувство взаимно",
                        "всё взаимно"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short neuter singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short neuter form of the adjective \"mutual\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mutual",
                                "target_lang_sense_clarification": "shared by two or more parties"
                            }
                        ]
                    }
                }
            ]
        }
    ]

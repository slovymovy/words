{
    "word_family": [
        "dribbler",
        "dribbling"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0f5544cb-820a-3071-a90d-e1cbe8a97372",
                    "sense_definition": "To move a ball forward in a game by repeatedly kicking or bouncing it.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sports_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He can <w>dribble</w> the ball past the defenders with ease.",
                            "target_lang_translations": {
                                "ru": "Он может с легкостью <w>вести</w> мяч мимо защитников."
                            }
                        },
                        {
                            "text": "The player <w>dribbled</w> down the field to score a goal.",
                            "target_lang_translations": {
                                "ru": "Игрок <w>вел</w> мяч по полю, чтобы забить гол."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dribble the ball",
                        "dribble past",
                        "dribble down-field"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать мяч вперед в игре, ударяя его ногой или отбивая от пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести мяч",
                                "target_lang_sense_clarification": "спорт"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "300f686d-1447-3bec-ad44-9b72de6cd356",
                    "sense_definition": "To flow or fall slowly in small drops or a thin, unsteady stream.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "liquid_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Water began to <w>dribble</w> from the old rusty pipe.",
                            "target_lang_translations": {
                                "ru": "Вода начала <w>капать</w> из старой ржавой трубы."
                            }
                        },
                        {
                            "text": "Blood <w>dribbled</w> down his chin after the cut.",
                            "target_lang_translations": {
                                "ru": "Кровь <w>стекала тонкой струйкой</w> по его подбородку после пореза."
                            }
                        }
                    ],
                    "synonyms": [
                        "trickle",
                        "drip",
                        "leak"
                    ],
                    "antonyms": [
                        "gush",
                        "pour",
                        "stream"
                    ],
                    "common_phrases": [
                        "dribble down",
                        "dribble out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Течь или падать медленно мелкими каплями или тонкой, неровной струйкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капать",
                                "target_lang_sense_clarification": "падать каплями"
                            },
                            {
                                "target_lang_word": "течь тонкой струйкой",
                                "target_lang_sense_clarification": "о жидкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3d43423-09a2-36ab-be26-8ff27b36be60",
                    "sense_definition": "To let saliva flow uncontrollably from the mouth.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "saliva",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baby <w>dribbled</w> all over his clean bib.",
                            "target_lang_translations": {
                                "ru": "Ребенок <w>напускал слюней</w> на свой чистый слюнявчик."
                            }
                        },
                        {
                            "text": "My dog tends to <w>dribble</w> when he is sleeping.",
                            "target_lang_translations": {
                                "ru": "Моя собака имеет обыкновение <w>пускать слюни</w>, когда спит."
                            }
                        }
                    ],
                    "synonyms": [
                        "drool",
                        "slobber"
                    ],
                    "common_phrases": [
                        "dribble on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неконтролируемо пускать слюну изо рта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пускать слюни",
                                "target_lang_sense_clarification": "физиологический процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b038dfe-75b9-3188-875e-4cc49bb8b377",
                    "sense_definition": "To pour a liquid slowly in a thin stream or small drops.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "liquid_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chef <w>dribbled</w> olive oil over the salad for flavor.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар <w>сбрызнул</w> салат оливковым маслом для вкуса."
                            }
                        },
                        {
                            "text": "She carefully <w>dribbled</w> the sauce onto the plate.",
                            "target_lang_translations": {
                                "ru": "Она осторожно <w>полила</w> соус на тарелку тонкой струйкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "drizzle",
                        "sprinkle"
                    ],
                    "common_phrases": [
                        "dribble oil",
                        "dribble sauce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медленно лить жидкость тонкой струйкой или каплями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбрызгивать",
                                "target_lang_sense_clarification": "кулинария"
                            },
                            {
                                "target_lang_word": "поливать тонкой струйкой",
                                "target_lang_sense_clarification": "о жидкости"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d1e79c82-4d62-3449-9b19-b7cc3e05dcd1",
                    "sense_definition": "The act of moving a ball by kicking or bouncing it.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sports_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His quick <w>dribble</w> confused the opposing team.",
                            "target_lang_translations": {
                                "ru": "Его быстрый <w>дриблинг</w> сбил с толку команду противника."
                            }
                        },
                        {
                            "text": "She practiced her <w>dribble</w> every day after school.",
                            "target_lang_translations": {
                                "ru": "Она тренировала <w>ведение мяча</w> каждый день после школы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the dribble"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по перемещению мяча ударами ноги или отбиванием от пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дриблинг",
                                "target_lang_sense_clarification": "спорт"
                            },
                            {
                                "target_lang_word": "ведение мяча",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e7abc9b-0af5-36b0-b8ea-832886b21a77",
                    "sense_definition": "A very small, weak stream or amount of liquid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "liquid_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Only a <w>dribble</w> of coffee remained in the pot.",
                            "target_lang_translations": {
                                "ru": "В кофейнике осталась лишь <w>капля</w> кофе."
                            }
                        },
                        {
                            "text": "A <w>dribble</w> of water ran down the window pane.",
                            "target_lang_translations": {
                                "ru": "<w>Струйка</w> воды стекала по оконному стеклу."
                            }
                        }
                    ],
                    "synonyms": [
                        "trickle",
                        "drop"
                    ],
                    "antonyms": [
                        "flood",
                        "torrent"
                    ],
                    "common_phrases": [
                        "a dribble of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень маленький, слабый поток или количество жидкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "струйка",
                                "target_lang_sense_clarification": "жидкости"
                            },
                            {
                                "target_lang_word": "капля",
                                "target_lang_sense_clarification": "малое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc196979-0162-30ab-89d4-557d2d2db5ff",
                    "sense_definition": "Saliva that has flowed from the mouth.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "saliva_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wiped a bit of <w>dribble</w> from the infant's chin.",
                            "target_lang_translations": {
                                "ru": "Она вытерла немного <w>слюны</w> с подбородка младенца."
                            }
                        },
                        {
                            "text": "There was <w>dribble</w> on the pillow where he had slept.",
                            "target_lang_translations": {
                                "ru": "На подушке, где он спал, были <w>слюни</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "drool",
                        "slobber",
                        "spit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слюна, вытекшая изо рта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слюна",
                                "target_lang_sense_clarification": "жидкость"
                            },
                            {
                                "target_lang_word": "слюни",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ae7e2cb-01dd-3c2a-9edc-6ae9fab2da23",
                    "sense_definition": "Nonsense or worthless talk.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nonsense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not want to listen to this <w>dribble</w> anymore.",
                            "target_lang_translations": {
                                "ru": "Я больше не хочу слушать этот <w>бред</w>."
                            }
                        },
                        {
                            "text": "The report was full of bureaucratic <w>dribble</w>.",
                            "target_lang_translations": {
                                "ru": "Отчет был полон бюрократической <w>чуши</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nonsense",
                        "rubbish",
                        "drivel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бессмыслица или пустая болтовня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чушь",
                                "target_lang_sense_clarification": "абсурд"
                            },
                            {
                                "target_lang_word": "бред",
                                "target_lang_sense_clarification": "бессмыслица"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
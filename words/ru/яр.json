                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Мы проезжали через село <w>Яр</w> по пути в областной центр.",
                            "target_lang_translations": {
                                "en": "We drove through the village of <w>Yar</w> on the way to the regional center."
                            }
                        },
                        {
                            "text": "Моя бабушка родилась в небольшом посёлке <w>Яр</w> на севере страны.",
                            "target_lang_translations": {
                                "en": "My grandmother was born in the small settlement of <w>Yar</w> in the north of the country."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Name of a settlement in Russia."
                    },

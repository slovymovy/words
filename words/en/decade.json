    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c78510d1-5a15-36f1-a4e4-8c9647e89982",
                    "sense_definition": "A period of ten years.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Technology has changed rapidly in the last <w>decade</w>.",
                            "target_lang_translations": {
                                "ru": "За последнее <w>десятилетие</w> технологии стремительно изменились."
                            }
                        },
                        {
                            "text": "She has lived in this house for more than a <w>decade</w>.",
                            "target_lang_translations": {
                                "ru": "Она живет в этом доме уже более <w>десятилетия</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "decennium",
                        "decennary"
                    ],
                    "common_phrases": [
                        "past decade",
                        "next decade",
                        "last decade",
                        "over a decade",
                        "recent decades"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени продолжительностью в десять лет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "десятилетие",
                                "target_lang_sense_clarification": "период в 10 лет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20dfc0e7-485d-3abb-9879-3d15ec99b2cd",
                    "sense_definition": "A series of prayers in the rosary, consisting of one Our Father and ten Hail Marys.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family gathered to pray a <w>decade</w> of the rosary.",
                            "target_lang_translations": {
                                "ru": "Семья собралась, чтобы прочитать <w>десяток</w> розария."
                            }
                        },
                        {
                            "text": "He silently recited the first <w>decade</w> while walking to church.",
                            "target_lang_translations": {
                                "ru": "По дороге в церковь он молча читал первый <w>десяток</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rosary decade"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серия молитв в розарии, состоящая из одного «Отче наш» и десяти «Богородице Дево, радуйся»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "десяток",
                                "target_lang_sense_clarification": "часть розария"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87bd001f-d066-3941-896d-78743816ff86",
                    "sense_definition": "The interval between any two quantities having a ratio of 10 to 1.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The frequency response is flat over several <w>decades</w>.",
                            "target_lang_translations": {
                                "ru": "Частотная характеристика остается плоской на протяжении нескольких <w>декад</w>."
                            }
                        },
                        {
                            "text": "There are <w>decades</w> between these two values on the logarithmic scale.",
                            "target_lang_translations": {
                                "ru": "На логарифмической шкале между этими двумя значениями лежат <w>декады</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Интервал между двумя величинами, отношение которых равно 10 к 1."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "декада",
                                "target_lang_sense_clarification": "логарифмический интервал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33243f46-3a0e-3745-acd6-356ec5988f4e",
                    "sense_definition": "A group or set of ten items or people.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The commander led a <w>decade</w> of soldiers into the valley.",
                            "target_lang_translations": {
                                "ru": "Командир вел <w>десяток</w> солдат в долину."
                            }
                        },
                        {
                            "text": "A <w>decade</w> of wagons moved slowly across the plain.",
                            "target_lang_translations": {
                                "ru": "<w>Десяток</w> повозок медленно двигался по равнине."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used to refer to a group of ten soldiers or items"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа или набор из десяти предметов или людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "десяток",
                                "target_lang_sense_clarification": "группа из десяти"
                            }
                        ]
                    }
                }
            ]
        }
    ]

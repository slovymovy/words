        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0abf6c7d-9453-31ab-a4a1-e76638150042",
                    "sense_definition": "Kept safe from harm, injury, or damage; defended.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "defended_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This forest is a <w>protected</w> area, so hunting is strictly forbidden here.",
                            "target_lang_translations": {
                                "ru": "Этот лес является <w>заповедной</w> зоной, поэтому охота здесь строго запрещена."
                            }
                        },
                        {
                            "text": "The witness was moved to a <w>protected</w> location to ensure her safety before the trial.",
                            "target_lang_translations": {
                                "ru": "Свидетеля перевезли в <w>охраняемое</w> место, чтобы обеспечить её безопасность до суда."
                            }
                        }
                    ],
                    "synonyms": [
                        "defended",
                        "guarded",
                        "safe",
                        "secure"
                    ],
                    "antonyms": [
                        "unprotected"
                    ],
                    "common_phrases": [
                        "protected area",
                        "protected species",
                        "protected status",
                        "protected by law"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся под охраной или защитой; защищённый от вреда или повреждений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защищённый",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "охраняемый",
                                "target_lang_sense_clarification": "находящийся под наблюдением"
                            },
                            {
                                "target_lang_word": "заповедный",
                                "target_lang_sense_clarification": "о природных территориях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb9146f9-aa41-3cdd-bc08-dce2d2ff8c65",
                    "sense_definition": "Relating to a program element in object-oriented programming that is accessible only within its own class and by derived subclasses.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should declare the variable as <w>protected</w> if you want subclasses to access it.",
                            "target_lang_translations": {
                                "ru": "Вам следует объявить переменную <w>защищённой</w>, если вы хотите, чтобы подклассы имели к ней доступ."
                            }
                        },
                        {
                            "text": "The <w>protected</w> method cannot be called from outside the class hierarchy.",
                            "target_lang_translations": {
                                "ru": "<w>Защищённый</w> метод нельзя вызвать извне иерархии классов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "protected member",
                        "protected method",
                        "protected access"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к элементу программы в ООП, доступному только внутри своего класса и производных подклассов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защищённый",
                                "target_lang_sense_clarification": "модификатор доступа"

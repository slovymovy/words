    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "57d1e17b-ba42-3670-891d-c95e6b6c1ac3",
                    "sense_definition": "Happening before a more important action or event to prepare for it; introductory.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctors have completed a <w>preliminary</w> examination of the patient.",
                            "target_lang_translations": {
                                "ru": "Врачи завершили <w>предварительный</w> осмотр пациента."
                            }
                        },
                        {
                            "text": "We need to do some <w>preliminary</w> research before starting the project.",
                            "target_lang_translations": {
                                "ru": "Нам нужно провести <w>предварительное</w> исследование перед началом проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "initial",
                        "introductory",
                        "preparatory"
                    ],
                    "antonyms": [
                        "final",
                        "definitive"
                    ],
                    "common_phrases": [
                        "preliminary results",
                        "preliminary hearing",
                        "preliminary stage",
                        "preliminary investigation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий перед более важным действием или событием в качестве подготовки к нему; вводный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предварительный",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "подготовительный",
                                "target_lang_sense_clarification": "служащий для подготовки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad02ee75-2e22-3325-8497-ac4c30c27471",
                    "sense_definition": "A minor or preparatory action or event that comes before the main one.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They dispensed with the <w>preliminaries</w> and got straight to business.",
                            "target_lang_translations": {
                                "ru": "Они обошлись без <w>предисловий</w> и сразу перешли к делу."
                            }
                        },
                        {
                            "text": "The contract signing was just a <w>preliminary</w> to the actual merger.",
                            "target_lang_translations": {
                                "ru": "Подписание контракта было лишь <w>вступлением</w> к фактическому слиянию."
                            }
                        }
                    ],
                    "synonyms": [
                        "introductory",
                        "preparatory"
                    ],
                    "antonyms": [
                        "final",
                        "definitive"
                    ],
                    "common_phrases": [
                        "skip the preliminaries",
                        "necessary preliminaries"
                    ],
                    "target_lang_definitions": {
                        "ru": "Незначительное или подготовительное действие или событие, предшествующее основному."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предисловие",
                                "target_lang_sense_clarification": "вводная часть, часто во мн. ч."
                            },
                            {
                                "target_lang_word": "подготовка",
                                "target_lang_sense_clarification": "процесс подготовки"
                            }
                        ]
                    }
                },
                {

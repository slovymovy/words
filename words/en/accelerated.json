    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cb8ababe-e267-3c08-9eb6-c4077a186cca",
                    "sense_definition": "Simple past and past participle of <w>accelerate</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car <w>accelerated</w> rapidly to merge onto the busy highway.",
                            "target_lang_translations": {
                                "ru": "Машина быстро <w>разогналась</w>, чтобы влиться в поток на оживленном шоссе."
                            }
                        },
                        {
                            "text": "Economic growth <w>accelerated</w> sharply after the new policies were implemented.",
                            "target_lang_translations": {
                                "ru": "Экономический рост резко <w>ускорился</w> после внедрения новой политики."
                            }
                        }
                    ],
                    "synonyms": [
                        "sped up",
                        "hastened",
                        "quickened"
                    ],
                    "antonyms": [
                        "decelerated",
                        "slowed down"
                    ],
                    "common_phrases": [
                        "accelerated rapidly",
                        "accelerated smoothly",
                        "accelerated sharply"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола accelerate."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ускорился",
                                "target_lang_sense_clarification": "стал происходить или двигаться быстрее"
                            },
                            {
                                "target_lang_word": "разогнался",
                                "target_lang_sense_clarification": "набрал скорость (обычно о транспортном средстве)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9d6a1cd9-ca24-3997-80b6-ef28df32bf8f",
                    "sense_definition": "Moving or progressing faster than is usual.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "speed_timing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to take an <w>accelerated</w> course to finish her degree in three years.",
                            "target_lang_translations": {
                                "ru": "Она решила пройти <w>ускоренный</w> курс, чтобы получить диплом за три года."
                            }
                        },
                        {
                            "text": "The company experienced <w>accelerated</w> growth due to the high demand for its products.",
                            "target_lang_translations": {
                                "ru": "Компания пережила <w>ускоренный</w> рост благодаря высокому спросу на её продукцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "fast-tracked",
                        "quickened",
                        "expedited"
                    ],
                    "antonyms": [
                        "gradual",
                        "slow"
                    ],
                    "common_phrases": [
                        "accelerated course",
                        "accelerated growth",
                        "accelerated pace",
                        "accelerated learning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движущийся или развивающийся быстрее, чем обычно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ускоренный",
                                "target_lang_sense_clarification": "происходящий в более быстром темпе (о курсах, росте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18049813-4f6a-3aae-8f88-d6c6ed06999d",
                    "sense_definition": "Happening sooner than expected.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "speed_timing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The project team struggled to meet the <w>accelerated</w> deadline set by the management.",
                            "target_lang_translations": {
                                "ru": "Команда проекта с трудом пыталась уложиться в <w>сжатые</w> сроки, установленные руководством."
                            }
                        },
                        {
                            "text": "Due to the emergency, we are working on an <w>accelerated</w> schedule this week.",
                            "target_lang_translations": {
                                "ru": "Из-за чрезвычайной ситуации на этой неделе мы работаем по <w>ускоренному</w> графику."
                            }
                        }
                    ],
                    "synonyms": [
                        "early",
                        "premature",
                        "advanced"
                    ],
                    "antonyms": [
                        "delayed",
                        "postponed",
                        "late"
                    ],
                    "common_phrases": [
                        "accelerated deadline",
                        "accelerated schedule",
                        "accelerated timeline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий раньше, чем ожидалось, или в более сжатые сроки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сжатый",
                                "target_lang_sense_clarification": "сокращённый по времени (о сроках)"
                            },
                            {
                                "target_lang_word": "ускоренный",
                                "target_lang_sense_clarification": "выполняемый быстрее плана (о графике)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "scarecrow",
        "crowbar",
        "crow's nest"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7ced763c-c4c8-3adb-864a-62bb6765ecde",
                    "sense_definition": "A large bird with shiny black feathers and a harsh cry.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "bird_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A black <w>crow</w> landed on the fence.",
                            "target_lang_translations": {
                                "ru": "Черная <w>ворона</w> села на забор."
                            }
                        },
                        {
                            "text": "The farmer put up a scarecrow to frighten every <w>crow</w> away.",
                            "target_lang_translations": {
                                "ru": "Фермер поставил пугало, чтобы отпугнуть всех <w>ворон</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "as the crow flies",
                        "white crow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная птица с блестящим черным оперением и резким криком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ворона",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "083d7727-c633-363b-80a4-191c88c25e0c",
                    "sense_definition": "A member of a Native American tribe originally from Montana.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "tribe_group",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "The <w>Crow</w> people have a rich cultural history.",
                            "target_lang_translations": {
                                "ru": "Народ <w>кроу</w> имеет богатую культурную историю."
                            }
                        },
                        {
                            "text": "He is studying the traditions of the <w>Crow</w> tribe.",
                            "target_lang_translations": {
                                "ru": "Он изучает традиции племени <w>кроу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Absaroki",
                        "Crow Indian"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель индейского племени, изначально проживавшего в Монтане."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроу",
                                "target_lang_sense_clarification": "племя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc37ccc4-a3f9-369e-ac52-95602c854a95",
                    "sense_definition": "The loud cry made by a rooster, especially in the morning.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We woke up to the loud <w>crow</w> of the rooster.",
                            "target_lang_translations": {
                                "ru": "Мы проснулись от громкого <w>кукареканья</w> петуха."
                            }
                        },
                        {
                            "text": "The <w>crow</w> signaled the start of a new day.",
                            "target_lang_translations": {
                                "ru": "<w>Кукареканье</w> возвестило о начале нового дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "cock-a-doodle-doo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громкий крик петуха, особенно по утрам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кукареканье",
                                "target_lang_sense_clarification": "звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dcc43525-4d28-3e42-8817-8f80403de11e",
                    "sense_definition": "An unpleasant, ill-tempered, or ugly woman.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was rude and called his neighbor an old <w>crow</w>.",
                            "target_lang_translations": {
                                "ru": "Он был груб и назвал соседку старой <w>каргой</w>."
                            }
                        },
                        {
                            "text": "Don't listen to that bitter old <w>crow</w>.",
                            "target_lang_translations": {
                                "ru": "Не слушай эту злобную старую <w>каргу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "harridan",
                        "shrew",
                        "harpy"
                    ],
                    "common_phrases": [
                        "old crow"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "offensive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятная, сварливая или безобразная женщина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карга",
                                "target_lang_sense_clarification": "бранное"
                            },
                            {
                                "target_lang_word": "грымза",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29f224e7-a509-3913-978b-9c9eb336ed6c",
                    "sense_definition": "A bar of iron used as a lever.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The worker used a <w>crow</w> to pry the heavy lid open.",
                            "target_lang_translations": {
                                "ru": "Рабочий использовал <w>лом</w>, чтобы поддеть тяжелую крышку."
                            }
                        },
                        {
                            "text": "He grabbed a <w>crow</w> to break the lock.",
                            "target_lang_translations": {
                                "ru": "Он схватил <w>лом</w>, чтобы сломать замок."
                            }
                        }
                    ],
                    "synonyms": [
                        "crowbar"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "short for crowbar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Железный прут, используемый в качестве рычага."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лом",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aac8ece0-dd28-37f6-9e46-69e7d6ca7cd1",
                    "sense_definition": "To make the loud, shrill sound characteristic of a rooster; or to make a happy sound like a baby.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rooster began to <w>crow</w> as soon as the sun came up.",
                            "target_lang_translations": {
                                "ru": "Петух начал <w>кукарекать</w>, как только взошло солнце."
                            }
                        },
                        {
                            "text": "The baby started to <w>crow</w> with delight when she saw her mother.",
                            "target_lang_translations": {
                                "ru": "Ребенок начал радостно <w>гулить</w>, когда увидел маму."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать громкий, пронзительный звук, характерный для петуха; или издавать радостные звуки (о младенце)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кукарекать",
                                "target_lang_sense_clarification": "о петухе"
                            },
                            {
                                "target_lang_word": "гулить",
                                "target_lang_sense_clarification": "о младенце"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "779aa752-5032-35da-aaf8-340c9618c047",
                    "sense_definition": "To talk with too much pride about something you have achieved.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He continued to <w>crow</w> about his victory for weeks.",
                            "target_lang_translations": {
                                "ru": "Он продолжал <w>бахвалиться</w> своей победой неделями."
                            }
                        },
                        {
                            "text": "It is not polite to <w>crow</w> over your opponent's defeat.",
                            "target_lang_translations": {
                                "ru": "Невежливо <w>злорадствовать</w> над поражением противника."
                            }
                        }
                    ],
                    "synonyms": [
                        "brim",
                        "brimstone"
                    ],
                    "common_phrases": [
                        "crow over"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить с чрезмерной гордостью о своих достижениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бахвалиться",
                                "target_lang_sense_clarification": "хвастаться"
                            },
                            {
                                "target_lang_word": "кичиться",
                                "target_lang_sense_clarification": "гордиться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a6f7b1fe-43f8-34db-b598-314ba946eab2",
                    "sense_definition": "A surname.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Crow</w> lives down the street.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Кроу</w> живет в конце улицы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кроу",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca79b940-234f-32af-b593-500c24fd952f",
                    "sense_definition": "The Siouan language spoken by the Crow tribe.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tribe_group",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "She is learning to speak <w>Crow</w>.",
                            "target_lang_translations": {
                                "ru": "Она учится говорить на <w>языке кроу</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Язык сиуанской семьи, на котором говорит племя кроу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык кроу",
                                "target_lang_sense_clarification": "язык"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f72d979b-14d0-34c5-8b35-350d75a33474",
                    "sense_definition": "Very dark black in color.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>crow</w> hair shone in the light.",
                            "target_lang_translations": {
                                "ru": "Ее <w>иссиня-черные</w> волосы сияли на свету."
                            }
                        },
                        {
                            "text": "The night was <w>crow</w> black.",
                            "target_lang_translations": {
                                "ru": "Ночь была <w>черной как смоль</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень темный, черный цвет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иссиня-черный",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "вороной",
                                "target_lang_sense_clarification": "масть"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
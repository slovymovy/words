                        {
                            "text": "She was named <w>Joan</w> after her grandmother.",
                            "target_lang_translations": {
                                "ru": "Ее назвали <w>Джоан</w> в честь бабушки."
                            }
                        }
                    ],
                    "common_phrases": [

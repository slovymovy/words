    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "05aca75b-ba26-3f01-be09-e99af0d63900",
                    "sense_definition": "Идти или ехать непосредственно за кем-либо или чем-либо",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_behind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, <следуйте> за мной в мой кабинет.",
                            "target_lang_translations": {
                                "en": "Please <follow> me to my office."
                            }
                        },
                        {
                            "text": "Собака привыкла повсюду <следовать> за своим хозяином.",
                            "target_lang_translations": {
                                "en": "The dog was used to <following> its master everywhere."
                            }
                        }
                    ],
                    "synonyms": [
                        "идти́ сле́дом за",
                        "идти́ вслед за"
                    ],
                    "common_phrases": [
                        "следовать за мной",
                        "следовать за ним",
                        "следовать по пятам"
                    ],
                    "target_lang_definitions": {
                        "en": "To go or travel directly behind someone or something"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "move behind someone"
                            },
                            {
                                "target_lang_word": "go after",
                                "target_lang_sense_clarification": "pursue or move behind"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27895be7-077f-3828-80c5-fc96370b4c28",
                    "sense_definition": "Быть нужным, должным или правильным в данной ситуации (обычно используется в безличных конструкциях)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вам <следует> больше отдыхать и меньше волноваться.",
                            "target_lang_translations": {
                                "en": "You <should> rest more and worry less."
                            }
                        },
                        {
                            "text": "Этого <следовало> ожидать с самого начала.",
                            "target_lang_translations": {
                                "en": "This <should> have been expected from the very beginning."
                            }
                        }
                    ],
                    "synonyms": [
                        "на́до",
                        "ну́жно",
                        "сто́ить"
                    ],
                    "common_phrases": [
                        "как следует",
                        "не следует забывать",
                        "следует отметить",
                        "вам следует"
                    ],
                    "target_lang_definitions": {
                        "en": "To be necessary, proper, or expected in a given situation"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "should",
                                "target_lang_sense_clarification": "modal verb indicating obligation or advice"
                            },
                            {
                                "target_lang_word": "ought to",
                                "target_lang_sense_clarification": "indicating duty or correctness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6226c57e-70ec-351c-8c59-cf4bb713ffc2",
                    "sense_definition": "Двигаться по определённому маршруту (о транспортных средствах)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скорый поезд <следует> до станции Москва без остановок.",
                            "target_lang_translations": {
                                "en": "The express train <runs> to Moscow station without stops."
                            }
                        },
                        {
                            "text": "Автобус <следует> по расписанию, несмотря на погоду.",
                            "target_lang_translations": {
                                "en": "The bus <runs> on schedule, despite the weather."
                            }
                        }
                    ],
                    "common_phrases": [
                        "поезд следует",
                        "автобус следует",
                        "следовать по маршруту"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "official transport announcements"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To move along a specific route (regarding vehicles)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "operate on a route or schedule"
                            },
                            {
                                "target_lang_word": "proceed",
                                "target_lang_sense_clarification": "move forward or travel (formal)"
                            },
                            {
                                "target_lang_word": "travel",
                                "target_lang_sense_clarification": "go from one place to another"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4764ffaf-d74c-3180-a4fd-c9aa3bb56f51",
                    "sense_definition": "Наступать, появляться или располагаться сразу после чего-либо во времени или пространстве",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_behind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За холодной зимой всегда <следует> тёплая весна.",
                            "target_lang_translations": {
                                "en": "A warm spring always <follows> a cold winter."
                            }
                        },
                        {
                            "text": "В книге главы <следуют> одна за другой в строгом порядке.",
                            "target_lang_translations": {
                                "en": "In the book, the chapters <follow> one another in strict order."
                            }
                        }
                    ],
                    "common_phrases": [
                        "продолжение следует",
                        "следовать за другом",
                        "следовать примеру"
                    ],
                    "target_lang_definitions": {
                        "en": "To happen, appear, or be located immediately after something in time or space"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "come after in sequence"
                            },
                            {
                                "target_lang_word": "come after",
                                "target_lang_sense_clarification": "occur subsequently"
                            },
                            {
                                "target_lang_word": "ensue",
                                "target_lang_sense_clarification": "happen as a result or afterwards"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cedc785f-c0ed-394b-bbee-ef35fce9e0af",
                    "sense_definition": "Поступать в соответствии с чем-либо, руководствоваться чем-либо (правилами, советами, примером)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <следовать> совету нашего опытного учителя.",
                            "target_lang_translations": {
                                "en": "We decided to <follow> our experienced teacher's advice."
                            }
                        },
                        {
                            "text": "Все водители обязаны строго <следовать> правилам дорожного движения.",
                            "target_lang_translations": {
                                "en": "All drivers are required to strictly <follow> traffic rules."
                            }
                        }
                    ],
                    "synonyms": [
                        "руково́дствоваться"
                    ],
                    "common_phrases": [
                        "следовать совету",
                        "следовать правилам",
                        "следовать инструкции",
                        "следовать моде"
                    ],
                    "target_lang_definitions": {
                        "en": "To act in accordance with rules, advice, or examples"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "act according to instructions or advice"
                            },
                            {
                                "target_lang_word": "adhere to",
                                "target_lang_sense_clarification": "stick strictly to rules"
                            },
                            {
                                "target_lang_word": "abide by",
                                "target_lang_sense_clarification": "accept and act in accordance with"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cf00ae4-f033-3938-8c68-55b8771f978d",
                    "sense_definition": "Быть логическим выводом или результатом чего-либо",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из ваших слов <следует>, что вы не виноваты в случившемся.",
                            "target_lang_translations": {
                                "en": "It <follows> from your words that you are not to blame for what happened."
                            }
                        },
                        {
                            "text": "Отсюда <следует> единственный возможный и правильный вывод.",
                            "target_lang_translations": {
                                "en": "The only possible and correct conclusion <follows> from this."
                            }
                        }
                    ],
                    "synonyms": [
                        "вытека́ть"
                    ],
                    "common_phrases": [
                        "отсюда следует",
                        "из этого следует",
                        "как следует из"
                    ],
                    "target_lang_definitions": {
                        "en": "To be a logical conclusion or result of something"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "be a logical consequence"
                            },
                            {
                                "target_lang_word": "result",
                                "target_lang_sense_clarification": "occur as an outcome"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "600a56eb-3821-3214-8c2f-c303a19c35ab",
                    "sense_definition": "Причитаться к уплате или получению",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скажите, пожалуйста, сколько с меня <следует> за проезд?",
                            "target_lang_translations": {
                                "en": "Tell me, please, how much do I <owe> for the fare?"
                            }
                        },
                        {
                            "text": "Ему <следовало> получить большую премию за эту работу.",
                            "target_lang_translations": {
                                "en": "He was <due> to receive a large bonus for this work."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сколько с меня следует",
                        "с вас следует"
                    ],
                    "target_lang_definitions": {
                        "en": "To be required to be paid or received"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be due",
                                "target_lang_sense_clarification": "expected to be paid or received"
                            },
                            {
                                "target_lang_word": "owe",
                                "target_lang_sense_clarification": "be under obligation to pay"
                            }
                        ]
                    }
                }
            ]
        }
    ]

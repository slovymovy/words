{
    "word_family": [
        "wejście",
        "wchodzić"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "faf13fe5-1e8c-325c-88c0-76224bbac58f",
                    "sense_definition": "Przejść z zewnątrz do środka jakiegoś pomieszczenia, budynku lub pojazdu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <wejść> do środka i zamknąć za sobą drzwi.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <войдите> внутрь и закройте за собой дверь."
                            }
                        },
                        {
                            "text": "Musimy szybko <wejść> do domu, bo zaczyna padać deszcz.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро <войти> в дом, так как начинается дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "wleźć",
                        "wnijść",
                        "wniść"
                    ],
                    "common_phrases": [
                        "wejść do domu",
                        "wejść do środka",
                        "wejść przez okno",
                        "wejść do pokoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместиться снаружи внутрь помещения, здания или транспортного средства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "войти",
                                "target_lang_sense_clarification": "переместиться внутрь"
                            },
                            {
                                "target_lang_word": "зайти",
                                "target_lang_sense_clarification": "посетить или проникнуть внутрь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53946d55-8dfd-33dd-b79a-355e9ce6198c",
                    "sense_definition": "Otworzyć stronę internetową lub uzyskać dostęp do systemu komputerowego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <wejść> na stronę banku, aby zrobić przelew.",
                            "target_lang_translations": {
                                "ru": "Ты должен <зайти> на сайт банка, чтобы сделать перевод."
                            }
                        },
                        {
                            "text": "Próbuję <wejść> na pocztę, ale zapomniałem hasła.",
                            "target_lang_translations": {
                                "ru": "Я пытаюсь <войти> в почту, но забыл пароль."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wejść na stronę",
                        "wejść na link",
                        "wejść na Facebooka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открыть веб-сайт или получить доступ к компьютерной системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зайти",
                                "target_lang_sense_clarification": "на сайт"
                            },
                            {
                                "target_lang_word": "войти",
                                "target_lang_sense_clarification": "в систему или аккаунт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76cd2bd3-1aeb-378d-be94-cde81fee2f9a",
                    "sense_definition": "Przemieścić się na górę czegoś lub wspiąć się na wyższą pozycję.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kot nie potrafił sam <wejść> na wysokie drzewo.",
                            "target_lang_translations": {
                                "ru": "Кот не мог сам <залезть> на высокое дерево."
                            }
                        },
                        {
                            "text": "Musimy <wejść> na czwarte piętro, ponieważ winda nie działa.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <подняться> на четвертый этаж, так как лифт не работает."
                            }
                        }
                    ],
                    "synonyms": [
                        "wleźć"
                    ],
                    "antonyms": [
                        "zleźć"
                    ],
                    "common_phrases": [
                        "wejść na górę",
                        "wejść na szczyt",
                        "wejść po schodach",
                        "wejść na drabinę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подняться наверх или взобраться на более высокую позицию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подняться",
                                "target_lang_sense_clarification": "наверх"
                            },
                            {
                                "target_lang_word": "взобраться",
                                "target_lang_sense_clarification": "с усилием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be128cc1-cc81-3e48-9d20-6020d190353d",
                    "sense_definition": "Stać się członkiem jakiejś organizacji, grupy lub zespołu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy pracownik bardzo szybko <wszedł> do naszego zespołu.",
                            "target_lang_translations": {
                                "ru": "Новый сотрудник очень быстро <вошёл> в нашу команду."
                            }
                        },
                        {
                            "text": "Polska <weszła> do Unii Europejskiej w 2004 roku.",
                            "target_lang_translations": {
                                "ru": "Польша <вступила> в Европейский союз в 2004 году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wejść do zarządu",
                        "wejść do gry",
                        "wejść w skład"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать членом организации, группы или команды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "войти",
                                "target_lang_sense_clarification": "в состав группы"
                            },
                            {
                                "target_lang_word": "вступить",
                                "target_lang_sense_clarification": "в организацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cc5eecd-ad64-3376-9247-e6fbe4f09b91",
                    "sense_definition": "Zmieścić się w czymś; pasować rozmiarem do wnętrza czegoś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te buty są za małe, nie mogę w nie <wejść>.",
                            "target_lang_translations": {
                                "ru": "Эти туфли слишком малы, я не могу в них <влезть>."
                            }
                        },
                        {
                            "text": "Wszystkie ubrania powinny bez problemu <wejść> do tej walizki.",
                            "target_lang_translations": {
                                "ru": "Вся одежда должна без проблем <поместиться> в этот чемодан."
                            }
                        }
                    ],
                    "synonyms": [
                        "wleźć"
                    ],
                    "common_phrases": [
                        "wejść w spodnie",
                        "wejść do walizki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместиться внутри чего-либо; подойти по размеру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влезть",
                                "target_lang_sense_clarification": "в одежду или обувь"
                            },
                            {
                                "target_lang_word": "поместиться",
                                "target_lang_sense_clarification": "в емкость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "334a4c16-d7ac-3230-a7c3-4cf4cc670292",
                    "sense_definition": "Postawić nogę w czymś, zazwyczaj mokrym lub brudnym.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj, żeby nie <wejść> w tę głęboką kałużę.",
                            "target_lang_translations": {
                                "ru": "Осторожно, чтобы не <наступить> в эту глубокую лужу."
                            }
                        },
                        {
                            "text": "Przez przypadek <wszedłem> w błoto i mam teraz brudne buty.",
                            "target_lang_translations": {
                                "ru": "Я случайно <наступил> в грязь, и у меня теперь грязные ботинки."
                            }
                        }
                    ],
                    "synonyms": [
                        "wleźć"
                    ],
                    "common_phrases": [
                        "wejść w kałużę",
                        "wejść w błoto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наступить во что-то, обычно мокрое или грязное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наступить",
                                "target_lang_sense_clarification": "ногой во что-то"
                            },
                            {
                                "target_lang_word": "влезть",
                                "target_lang_sense_clarification": "разг. в грязь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d594d434-34f2-3862-9448-8d43fa975253",
                    "sense_definition": "Rozpocząć obowiązywanie (o prawie) lub stać się trwałym elementem zachowania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowe przepisy drogowe <wejdą> w życie od przyszłego miesiąca.",
                            "target_lang_translations": {
                                "ru": "Новые правила дорожного движения <вступят> в силу со следующего месяца."
                            }
                        },
                        {
                            "text": "Codzienne bieganie szybko <weszło> mu w krew.",
                            "target_lang_translations": {
                                "ru": "Ежедневный бег быстро <вошёл> у него в привычку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wejść w życie",
                        "wejść w nawyk",
                        "wejść w krew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступить в силу (о законе) или стать устойчивой привычкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вступить",
                                "target_lang_sense_clarification": "в силу"
                            },
                            {
                                "target_lang_word": "войти",
                                "target_lang_sense_clarification": "в привычку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9805988b-7303-3a75-9556-39d13ddeb0b5",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>wejście</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym budynku nie ma żadnych bocznych <wejść>.",
                            "target_lang_translations": {
                                "ru": "В этом здании нет никаких боковых <входов>."
                            }
                        },
                        {
                            "text": "Szukaliśmy bezpiecznych <wejść> do jaskini przez godzinę.",
                            "target_lang_translations": {
                                "ru": "Мы целый час искали безопасные <входы> в пещеру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brak wejść",
                        "dużo wejść"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного «wejście» (вход)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "входов",
                                "target_lang_sense_clarification": "родительный падеж мн. ч."
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
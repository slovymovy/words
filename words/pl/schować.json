{
    "word_family": [
        "chować",
        "schowek",
        "przechować",
        "zachować",
        "wychować",
        "pochować",
        "uchować",
        "odchować"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c9f590cd-514b-3fa5-afe7-6e8af0cbfb9c",
                    "sense_definition": "Umieścić przedmiot w przeznaczonym do tego miejscu (np. w kieszeni, torbie, szafie), aby go zabezpieczyć lub zrobić porządek.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "przechowywanie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <schować> telefon do kieszeni i skupić się na lekcji.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <уберите> телефон в карман и сосредоточьтесь на уроке."
                            }
                        },
                        {
                            "text": "Muszę <schować> czyste naczynia do szafki kuchennej.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <убрать> чистую посуду в кухонный шкаф."
                            }
                        }
                    ],
                    "synonyms": [
                        "odłożyć",
                        "włożyć"
                    ],
                    "antonyms": [
                        "wyciągnąć",
                        "wyjąć"
                    ],
                    "common_phrases": [
                        "schować do kieszeni",
                        "schować do plecaka",
                        "schować na miejsce",
                        "schować do szuflady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положить предмет в предназначенное для него место (например, в карман, сумку, шкаф), чтобы сохранить его или навести порядок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убрать",
                                "target_lang_sense_clarification": "положить на место, навести порядок"
                            },
                            {
                                "target_lang_word": "спрятать",
                                "target_lang_sense_clarification": "положить внутрь чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73ed3179-77a0-3315-adb3-e2401a3024f7",
                    "sense_definition": "Ukryć coś w miejscu niewidocznym dla innych, aby nikt tego nie znalazł lub nie zobaczył.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ukrywanie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej próbował szybko <schować> skradziony portfel pod kurtką.",
                            "target_lang_translations": {
                                "ru": "Вор пытался быстро <спрятать> украденный кошелек под курткой."
                            }
                        },
                        {
                            "text": "Rodzice postanowili <schować> prezenty świąteczne przed dziećmi.",
                            "target_lang_translations": {
                                "ru": "Родители решили <спрятать> рождественские подарки от детей."
                            }
                        }
                    ],
                    "synonyms": [
                        "ukryć",
                        "zataić"
                    ],
                    "antonyms": [
                        "pokazać",
                        "ujawnić",
                        "odsłonić"
                    ],
                    "common_phrases": [
                        "schować przed dziećmi",
                        "schować głęboko",
                        "schować skarb",
                        "schować dowody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скрыть что-то в невидимом для других месте, чтобы никто этого не нашел или не увидел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спрятать",
                                "target_lang_sense_clarification": "скрыть от чужих глаз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84c80c4b-3df0-302f-8366-2b67692033f3",
                    "sense_definition": "Sprawić, że jest się niewidocznym dla innych, wchodząc do kryjówki lub za przesłonę (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ukrywanie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas zabawy w chowanego Tomek zdążył <schować> się za dużym drzewem.",
                            "target_lang_translations": {
                                "ru": "Во время игры в прятки Томек успел <спрятаться> за большим деревом."
                            }
                        },
                        {
                            "text": "Kot lubi <schować> się pod łóżkiem, gdy przychodzą goście.",
                            "target_lang_translations": {
                                "ru": "Кот любит <прятаться> под кроватью, когда приходят гости."
                            }
                        }
                    ],
                    "synonyms": [
                        "ukryć się"
                    ],
                    "antonyms": [
                        "pokazać się",
                        "wyjść"
                    ],
                    "common_phrases": [
                        "schować się za rogiem",
                        "schować się pod stół",
                        "schować się przed deszczem",
                        "schować się w cieniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать себя невидимым для других, укрывшись где-либо (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спрятаться",
                                "target_lang_sense_clarification": "укрыться самому"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
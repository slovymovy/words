                {
                    "sense_id": "d599f7c8-8595-3ca5-9aa5-2589a7afe10e",
                    "sense_definition": "Abbreviation of <w>carbon copy</w>; a copy of a note or email sent to an addressee other than the main addressee.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "email_copy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I noticed that my manager was in the <w>cc</w> of your last email.",
                            "target_lang_translations": {
                                "ru": "Я заметил, что мой менеджер был в <w>копии</w> твоего последнего письма."
                            }
                        },
                        {
                            "text": "Please include the finance department in the <w>cc</w> when you send the invoice.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поставь финансовый отдел в <w>копию</w>, когда будешь отправлять счет."
                            }
                        }
                    ],
                    "synonyms": [
                        "copy"
                    ],
                    "common_phrases": [
                        "in the cc",
                        "blind cc"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Копия письма или сообщения, отправленная дополнительному адресату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копия",
                                "target_lang_sense_clarification": "электронного письма"
                            }
                        ]
                    }
                },
                {

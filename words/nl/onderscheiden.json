                    ],
                    "synonyms": [
                        "decoreren",
                        "eren",
                        "lauweren"
                    ],
                    "common_phrases": [
                        "koninklijk onderscheiden",
                        "onderscheiden worden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вручать кому-либо официальную награду, например медаль или орден, за особые достижения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "награждать",
                                "target_lang_sense_clarification": "давать награду"

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "63bd4921-fac9-3143-91d6-11f0b0bdbc76",
                    "sense_definition": "Podwieźć kogoś lub dostarczyć coś w określone miejsce przy okazji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy mógłbyś mnie <podrzucić> do centrum handlowego?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты <подбросить> меня до торгового центра?"
                            }
                        },
                        {
                            "text": "Jutro rano <podrzucę> ci te dokumenty do biura.",
                            "target_lang_translations": {
                                "ru": "Завтра утром я <заброшу> тебе эти документы в офис."
                            }
                        }
                    ],
                    "synonyms": [
                        "podwieźć",
                        "zawieźć",
                        "dostarczyć"
                    ],
                    "antonyms": [
                        "odebrać"
                    ],
                    "common_phrases": [
                        "podrzucić do domu",
                        "podrzucić do pracy",
                        "podrzucić kogoś autem",
                        "podrzucić paczkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвезти кого-то или доставить что-то в определенное место по пути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвезти",
                                "target_lang_sense_clarification": "дать возможность проехать"
                            },
                            {
                                "target_lang_word": "подбросить",
                                "target_lang_sense_clarification": "подвезти, разг."
                            },
                            {
                                "target_lang_word": "забросить",
                                "target_lang_sense_clarification": "доставить по пути, разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "468db6c0-8cfa-34e7-83bb-088591ab97c7",
                    "sense_definition": "Rzucić czymś w górę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędzia <podrzucił> monetę, aby zdecydować, kto zacznie mecz.",
                            "target_lang_translations": {
                                "ru": "Судья <подбросил> монету, чтобы решить, кто начнет матч."
                            }
                        },
                        {
                            "text": "Dziecko radośnie <podrzuciło> piłkę wysoko w powietrze.",
                            "target_lang_translations": {
                                "ru": "Ребенок радостно <подкинул> мяч высоко в воздух."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzucić"
                    ],
                    "antonyms": [
                        "złapać",
                        "chwycić"
                    ],
                    "common_phrases": [
                        "podrzucić piłkę",
                        "podrzucić monetę",
                        "podrzucić do góry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бросить что-то вверх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подбросить",
                                "target_lang_sense_clarification": "кинуть вверх"
                            },
                            {
                                "target_lang_word": "подкинуть",
                                "target_lang_sense_clarification": "кинуть вверх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c58cd0c8-4aaa-3b81-bbe7-afd8887d9b42",
                    "sense_definition": "Potajemnie umieścić coś w czyimś otoczeniu, często w celu obciążenia winą lub zrobienia niespodzianki.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej <podrzucił> skradziony portfel do plecaka niewinnego przechodnia.",
                            "target_lang_translations": {
                                "ru": "Вор <подбросил> украденный кошелек в рюкзак невинного прохожего."
                            }
                        },
                        {
                            "text": "Ktoś <podrzucił> nam anonimowy list pod drzwi wejściowe.",
                            "target_lang_translations": {
                                "ru": "Кто-то <подкинул> нам анонимное письмо под входную дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "podłożyć"
                    ],
                    "common_phrases": [
                        "podrzucić dowody",
                        "podrzucić kukułcze jajo",
                        "podrzucić świnię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тайно поместить что-то, часто с целью обвинить или сделать сюрприз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подбросить",
                                "target_lang_sense_clarification": "тайно подложить"
                            },
                            {
                                "target_lang_word": "подкинуть",
                                "target_lang_sense_clarification": "тайно подложить"
                            }
                        ]
                    }
                }

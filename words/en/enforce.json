    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5c7007ab-5cd0-32b4-97e8-ffe59793c185",
                    "sense_definition": "To make sure that people obey a particular law, rule, or regulation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local police department works hard to <w>enforce</w> traffic laws on the highway.",
                            "target_lang_translations": {
                                "ru": "Местное отделение полиции усердно работает над тем, чтобы <w>обеспечить соблюдение</w> правил дорожного движения на шоссе."
                            }
                        },
                        {
                            "text": "It is difficult to <w>enforce</w> a rule if nobody understands the reason behind it.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>обеспечить соблюдение</w> правила, если никто не понимает стоящую за ним причину."
                            }
                        },
                        {
                            "text": "The school administration decided to strictly <w>enforce</w> the ban on mobile phones.",
                            "target_lang_translations": {
                                "ru": "Администрация школы решила строго <w>следить за соблюдением</w> запрета на мобильные телефоны."
                            }
                        }
                    ],
                    "synonyms": [
                        "implement",
                        "apply",
                        "impose",
                        "administer"
                    ],
                    "antonyms": [
                        "disregard",
                        "ignore",
                        "waive",
                        "neglect"
                    ],
                    "common_phrases": [
                        "enforce the law",
                        "strictly enforce",
                        "enforce a rule",
                        "enforce regulations"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечивать соблюдение закона, правила или предписания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обеспечивать соблюдение",
                                "target_lang_sense_clarification": "законов или правил"
                            },
                            {
                                "target_lang_word": "следить за исполнением",
                                "target_lang_sense_clarification": "контролировать выполнение"
                            },
                            {
                                "target_lang_word": "приводить в исполнение",
                                "target_lang_sense_clarification": "официально реализовывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4daa0453-de61-3436-a07e-6202379beb0b",
                    "sense_definition": "To make something happen or to force someone to do something because it is necessary or unavoidable.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden economic crisis <w>enforced</w> strict spending cuts across the entire company.",
                            "target_lang_translations": {
                                "ru": "Внезапный экономический кризис <w>вынудил</w> пойти на жесткое сокращение расходов во всей компании."
                            }
                        },
                        {
                            "text": "Bad weather conditions <w>enforced</w> a delay in the construction of the new bridge.",
                            "target_lang_translations": {
                                "ru": "Плохие погодные условия <w>сделали неизбежной</w> задержку строительства нового моста."
                            }
                        }
                    ],
                    "synonyms": [
                        "compel",
                        "force",
                        "necessitate",
                        "dictate"
                    ],
                    "common_phrases": [
                        "enforce silence",
                        "enforce compliance",
                        "enforce discipline"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять что-то произойти или принуждать кого-то к действию в силу необходимости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навязывать",
                                "target_lang_sense_clarification": "волю или мнение"
                            },
                            {
                                "target_lang_word": "принуждать",
                                "target_lang_sense_clarification": "заставлять действовать"
                            },
                            {
                                "target_lang_word": "вынуждать",
                                "target_lang_sense_clarification": "создавать необходимость"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
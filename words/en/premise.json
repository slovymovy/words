                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Утверждать или предполагать что-либо в качестве основы для дальнейшего обсуждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предварять",
                                "target_lang_sense_clarification": "сказать что-то в качестве вступления"
                            },
                            {
                                "target_lang_word": "основывать",
                                "target_lang_sense_clarification": "базировать рассуждение на чем-либо"
                            }
                        ]
                    }
                }
            ]
        }

                {
                    "sense_id": "ba61c36e-11a6-3cc3-91e0-9737314e8a7b",
                    "sense_definition": "Niet eten of drinken gedurende een bepaalde tijd, vaak uit religieuze overweging of voor medisch onderzoek.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moslims <w>vasten</w> tijdens de ramadan van zonsopgang tot zonsondergang.",
                            "target_lang_translations": {
                                "en": "Muslims <w>fast</w> during Ramadan from sunrise to sunset.",
                                "ru": "Мусульмане <w>постятся</w> во время Рамадана от восхода до заката."
                            }
                        },
                        {
                            "text": "Voor de operatie moest de patiënt twaalf uur lang <w>vasten</w>.",
                            "target_lang_translations": {
                                "en": "Before the surgery, the patient had to <w>fast</w> for twelve hours.",
                                "ru": "Перед операцией пациент должен был двенадцать часов <w>голодать</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "eten"
                    ],
                    "common_phrases": [
                        "streng vasten",
                        "moeten vasten"
                    ],
                    "target_lang_definitions": {
                        "en": "To not eat or drink for a certain period, often for religious reasons or for a medical examination.",
                        "ru": "Воздерживаться от еды или питья в течение определенного времени, часто по религиозным соображениям или для медицинского обследования."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fast",
                                "target_lang_sense_clarification": "verb: to abstain from food"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поститься",
                                "target_lang_sense_clarification": "соблюдать религиозный пост"
                            },
                            {
                                "target_lang_word": "голодать",
                                "target_lang_sense_clarification": "воздерживаться от пищи (медицинский контекст)"
                            }
                        ]
                    }
                }
            ]
        },
        {

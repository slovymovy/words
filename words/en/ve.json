                {
                    "sense_id": "ae4637f2-2037-3ac3-9772-73a6393788f5",
                    "sense_definition": "Initialism of Victory in Europe; used especially in the term VE Day.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "The whole country celebrated <w>VE</w> Day with street parties and music.",
                            "target_lang_translations": {
                                "ru": "Вся страна праздновала День <w>Победы в Европе</w> уличными вечеринками и музыкой."
                            }
                        },
                        {
                            "text": "My grandfather often shares stories about his experiences on <w>VE</w> Day.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка часто делится историями о том, что он пережил в День <w>Победы в Европе</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "VE Day"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от «Victory in Europe» (Победа в Европе); используется преимущественно в названии праздника «VE Day»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Победа в Европе",
                                "target_lang_sense_clarification": "в контексте «VE Day»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "03e8f149-3739-3346-99c0-f6bee0c5cf50",
                    "sense_definition": "Pronunciation spelling of <w>we</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ve</w> vill go to the market together early tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "<w>Мы</w> пойдем на рынок вместе завтра рано утром."
                            }
                        },
                        {
                            "text": "I believe <w>ve</w> have enough supplies to last through the winter.",
                            "target_lang_translations": {
                                "ru": "Я полагаю, у <w>нас</w> достаточно припасов, чтобы продержаться всю зиму."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "representing a German accent"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вариант написания местоимения «we» (мы), передающий особенности произношения (обычно немецкий акцент)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мы",
                                "target_lang_sense_clarification": "перевод смысла (оригинал имитирует акцент)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e474cc02-53c9-337a-b717-d3825ee2fc80",
                    "sense_definition": "Gender-neutral third-person singular subject pronoun, equivalent to singular <w>they</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ve</w> decided to walk to the park instead of taking the bus.",
                            "target_lang_translations": {
                                "ru": "<w>Они</w> решили прогуляться до парка пешком вместо того, чтобы ехать на автобусе."
                            }
                        },
                        {
                            "text": "I asked my friend if <w>ve</w> wanted to join us for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Я спросил своего друга, хотят ли <w>они</w> присоединиться к нам за ужином сегодня."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гендерно-нейтральное личное местоимение третьего лица единственного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "они",
                                "target_lang_sense_clarification": "как гендерно-нейтральное местоимение ед. числа"
                            },
                            {
                                "target_lang_word": "ви",
                                "target_lang_sense_clarification": "неологизм, транслитерация"
                            }
                        ]
                    }
                }
            ]
        },
        {

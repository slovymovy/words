{
    "word_family": [
        "masztowy",
        "masztowiec"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0dc5f463-bef3-3428-b49a-7803b4be1ccf",
                    "sense_definition": "Pionowy słup na statku żaglowym, służący do utrzymywania żagli.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "nautical_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas silnego sztormu główny <maszt> statku został poważnie uszkodzony.",
                            "target_lang_translations": {
                                "ru": "Во время сильного шторма главная <мачта> судна была серьезно повреждена."
                            }
                        },
                        {
                            "text": "Marynarz musiał szybko wejść na <maszt>, aby poprawić liny.",
                            "target_lang_translations": {
                                "ru": "Моряку пришлось быстро подняться на <мачту>, чтобы поправить тросы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "główny maszt",
                        "złamany maszt",
                        "wejść na maszt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальный столб на парусном судне, служащий для удержания парусов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мачта",
                                "target_lang_sense_clarification": "на судне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa42383a-4b59-3546-b027-b5648c47d93e",
                    "sense_definition": "Wysoka konstrukcja lub słup służący do zawieszania flag, anten, oświetlenia lub kamer.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkańcy protestowali przeciwko budowie nowego <masztu> telefonii komórkowej w ich okolicy.",
                            "target_lang_translations": {
                                "ru": "Жители протестовали против строительства новой <мачты> сотовой связи в их районе."
                            }
                        },
                        {
                            "text": "Przed budynkiem urzędu stoi wysoki <maszt>, na którym powiewa flaga.",
                            "target_lang_translations": {
                                "ru": "Перед зданием администрации стоит высокая <мачта>, на которой развевается флаг."
                            }
                        }
                    ],
                    "synonyms": [
                        "słup"
                    ],
                    "common_phrases": [
                        "maszt flagowy",
                        "maszt antenowy",
                        "maszt oświetleniowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокая конструкция или столб для размещения флагов, антенн, освещения или камер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мачта",
                                "target_lang_sense_clarification": "сооружение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
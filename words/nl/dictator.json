                    "semantic_group_id": "historical_magistrate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Julius Caesar werd uiteindelijk benoemd tot <w>dictator</w> voor het leven.",
                            "target_lang_translations": {
                                "en": "Julius Caesar was eventually appointed <w>dictator</w> for life."

                },
                {
                    "sense_id": "17549787-d56d-338b-b295-cc32f20c887c",
                    "sense_definition": "Een morele verplichting of taak die men moet vervullen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "duty_obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vervulde zijn <w>plecht</w> zonder enige aarzeling.",
                            "target_lang_translations": {
                                "en": "He fulfilled his <w>duty</w> without any hesitation.",
                                "ru": "Он исполнил свой <w>долг</w> без малейших колебаний."
                            }
                        },
                        {
                            "text": "Het was zijn dure <w>plecht</w> om voor zijn familie te zorgen.",
                            "target_lang_translations": {
                                "en": "It was his solemn <w>duty</w> to care for his family.",
                                "ru": "Заботиться о семье было его священным <w>долгом</w>."

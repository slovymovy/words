            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b394a35b-31c1-3ad7-99c4-5b19a0034a1f",
                    "sense_definition": "Pierwiastek chemiczny o symbolu Zn i liczbie atomowej 30; niebieskawobiały, kruchy metal stosowany w przemyśle i medycynie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił mi suplementację, ponieważ badania wykazały niski poziom <w>cynku</w>.",
                            "target_lang_translations": {
                                "ru": "Врач порекомендовал мне добавки, так как анализы показали низкий уровень <w>цинка</w>."
                            }
                        },
                        {
                            "text": "Do produkcji mosiądzu wykorzystuje się miedź oraz <w>cynk</w>.",
                            "target_lang_translations": {
                                "ru": "Для производства латуни используют медь и <w>цинк</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Zn"
                    ],
                    "common_phrases": [
                        "tlenek cynku",
                        "maść z cynkiem",
                        "niedobór cynku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химический элемент с символом Zn и атомным номером 30; голубовато-белый хрупкий металл, используемый в промышленности и медицине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цинк",
                                "target_lang_sense_clarification": "химический элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dc237fd-d75e-3ba3-9248-296587ab849e",
                    "sense_definition": "Poufna informacja, ostrzeżenie lub wiadomość przekazana komuś w tajemnicy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kolega dał mi <w>cynk</w>, że szef planuje jutro niezapowiedzianą kontrolę.",
                            "target_lang_translations": {
                                "ru": "Коллега дал мне <w>наводку</w>, что начальник планирует завтра внезапную проверку."
                            }
                        },
                        {
                            "text": "Policja dostała <w>cynk</w> o planowanym napadzie na bank w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Полиция получила <w>наводку</w> о планируемом ограблении банка в центре города."
                            }
                        }
                    ],
                    "synonyms": [
                        "sygnał"
                    ],
                    "common_phrases": [
                        "dać cynk",
                        "dostać cynk",
                        "mieć cynk"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конфиденциальная информация, предупреждение или сообщение, переданное кому-либо по секрету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наводка",
                                "target_lang_sense_clarification": "секретная информация, подсказка"
                            },
                            {
                                "target_lang_word": "сигнал",
                                "target_lang_sense_clarification": "предупреждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3050822-96a1-3201-ab53-960de91b5df3",
                    "sense_definition": "Dawny dęty instrument muzyczny z ustnikiem, wykonany z drewna obciągniętego skórą, popularny w renesansie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum instrumentów dawnych oglądaliśmy dobrze zachowany renesansowy <w>cynk</w>.",
                            "target_lang_translations": {
                                "ru": "В музее старинных инструментов мы рассматривали хорошо сохранившийся ренессансный <w>цинк</w>."
                            }
                        },
                        {
                            "text": "Muzyk wydobył z instrumentu czysty dźwięk, dmuchając w drewniany <w>cynk</w>.",
                            "target_lang_translations": {
                                "ru": "Музыкант извлек из инструмента чистый звук, дуя в деревянный <w>цинк</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kornet"
                    ],
                    "common_phrases": [
                        "grać na cynku"
                    ],
                    "traits": [
                        {

                {
                    "sense_id": "b95c3676-26fa-3847-ab98-47dc47605ca3",
                    "sense_definition": "Miejsce stałego zamieszkania, do którego się wraca i z którym jest się emocjonalnie związanym.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "home_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem bardzo zmęczony i chcę już wrócić do <w>domu</w>.",
                            "target_lang_translations": {
                                "ru": "Я очень устал и хочу уже вернуться <w>домой</w>."
                            }
                        },
                        {
                            "text": "Wszędzie dobrze, ale w <w>domu</w> najlepiej.",
                            "target_lang_translations": {
                                "ru": "В гостях хорошо, а <w>дома</w> лучше."
                            }
                        }
                    ],
                    "synonyms": [
                        "mieszkanie",
                        "dach nad głową",
                        "własny kąt",
                        "ognisko domowe",
                        "pielesze",
                        "gniazdo rodzinne",
                        "ojcowska strzecha"
                    ],
                    "common_phrases": [
                        "wracać do domu",
                        "czuć się jak w domu",
                        "nie ma to jak w domu",
                        "praca w domu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место постоянного проживания, куда человек возвращается и с которым эмоционально связан."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дом",
                                "target_lang_sense_clarification": "жилище, домашний очаг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecb89da9-35aa-3f15-affc-b6758924ab84",
                    "sense_definition": "Budynek przeznaczony do celów mieszkalnych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {

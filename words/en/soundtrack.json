            "senses": [
                {
                    "sense_id": "cbb36c23-1562-3bf4-b639-b7d86155a68e",
                    "sense_definition": "The music and recorded sound that accompany a movie, video game, or other production, often available as an album.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "music_component",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>soundtrack</w> of the film features several hit songs from the eighties.",
                            "target_lang_translations": {
                                "ru": "В <w>саундтрек</w> фильма вошло несколько хитов восьмидесятых."
                            }
                        },
                        {
                            "text": "We listened to the movie <w>soundtrack</w> while driving to the beach.",
                            "target_lang_translations": {
                                "ru": "Мы слушали <w>саундтрек</w> к фильму, пока ехали на пляж."
                            }
                        }
                    ],
                    "synonyms": [
                        "score",
                        "background music"
                    ],
                    "common_phrases": [
                        "original soundtrack",
                        "movie soundtrack",
                        "film soundtrack",
                        "soundtrack album"
                    ],
                    "target_lang_definitions": {

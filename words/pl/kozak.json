    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "50e8d00b-b683-343f-a697-005d83e2ce24",
                    "sense_definition": "Wysoki but zimowy, zazwyczaj sięgający do kolan, noszony głównie przez kobiety.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "footwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą zawsze noszę ciepłe <kozaki>, żeby nie zmarznąć.",
                            "target_lang_translations": {
                                "ru": "Зимой я всегда ношу теплые <сапоги>, чтобы не замерзнуть."
                            }
                        },
                        {
                            "text": "Jej nowe skórzane <kozaki> pasują do każdego płaszcza.",
                            "target_lang_translations": {
                                "ru": "Ее новые кожаные <сапоги> подходят к любому пальто."
                            }
                        }
                    ],
                    "synonyms": [
                        "botek"
                    ],
                    "common_phrases": [
                        "skórzane kozaki",
                        "zamszowe kozaki",
                        "kozaki na obcasie",
                        "zimowe kozaki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокие зимние сапоги, обычно до колен, которые носят преимущественно женщины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сапог",
                                "target_lang_sense_clarification": "обувь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac676b88-4f21-36d5-a599-75100c1486ba",
                    "sense_definition": "Osoba bardzo odważna, pewna siebie, brawurowa lub robiąca coś imponującego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek to prawdziwy <kozak>, skoczył do wody z najwyższego klifu.",
                            "target_lang_translations": {
                                "ru": "Марек — настоящий <смельчак>, он прыгнул в воду с самого высокого утеса."
                            }
                        },
                        {
                            "text": "Nie zgrywaj takiego <kozaka>, bo możesz sobie zrobić krzywdę.",
                            "target_lang_translations": {
                                "ru": "Не строй из себя такого <крутого>, потому что можешь навредить себе."
                            }
                        }
                    ],
                    "synonyms": [
                        "chwat",
                        "zuch",
                        "śmiałek",
                        "chojrak",
                        "gieroj",
                        "junak"
                    ],
                    "common_phrases": [
                        "niezły kozak",
                        "zgrywać kozaka",
                        "ale kozak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень смелый, уверенный в себе человек, иногда дерзкий или совершающий впечатляющие поступки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смельчак",
                                "target_lang_sense_clarification": "храбрый человек"
                            },
                            {
                                "target_lang_word": "крутой",
                                "target_lang_sense_clarification": "разг. уверенный в себе, дерзкий"
                            },
                            {
                                "target_lang_word": "удалец",
                                "target_lang_sense_clarification": "разг. бойкий, удалой человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97cfeb13-ebe8-3a99-bbe4-8fdc675f3ce2",
                    "sense_definition": "Członek historycznej społeczności wojskowej zamieszkującej stepy na pograniczu dawnej Rzeczpospolitej i Rosji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "historical_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach <Kozacy> słynęli z doskonałej jazdy konnej.",
                            "target_lang_translations": {
                                "ru": "В давние времена <казаки> славились отличной верховой ездой."
                            }
                        },
                        {
                            "text": "Powieść opisuje liczne bitwy toczone przez <Kozaków> na stepach.",
                            "target_lang_translations": {
                                "ru": "Роман описывает многочисленные битвы, которые вели <казаки> в степях."
                            }
                        }
                    ],
                    "synonyms": [
                        "mołojec"
                    ],
                    "common_phrases": [
                        "Kozacy zaporoscy",
                        "ataman Kozaków",
                        "powstanie Kozaków"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "historical context related to Ukraine/Russia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель исторического военного сообщества, населявшего степи на пограничье бывшей Речи Посполитой и России."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "казак",
                                "target_lang_sense_clarification": "историческое сословие"

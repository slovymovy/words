            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "063fed52-070b-318e-a9cf-6ee105137e7b",
                    "sense_definition": "Third-person singular simple present indicative of <w>premise</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>premises</w> his theory on the idea that all humans are rational.",
                            "target_lang_translations": {
                                "ru": "Он <w>базирует</w> свою теорию на идее, что все люди рациональны."
                            }

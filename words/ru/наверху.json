    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "252c0b64-3f33-3ef4-874e-22eeb53a186e",
                    "sense_definition": "На верхнем этаже здания или помещения.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "loc_floors",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя спальня находится <наверху>, а кухня внизу.",
                            "target_lang_translations": {
                                "en": "My bedroom is <w>upstairs</w>, and the kitchen is downstairs."
                            }
                        },
                        {
                            "text": "Соседи <наверху> снова громко слушают музыку.",
                            "target_lang_translations": {
                                "en": "The neighbors <w>upstairs</w> are listening to loud music again."
                            }
                        }
                    ],
                    "synonyms": [
                        "вверху"
                    ],
                    "antonyms": [
                        "внизу"
                    ],
                    "common_phrases": [
                        "соседи наверху",
                        "комната наверху",
                        "ждать наверху"
                    ],
                    "target_lang_definitions": {
                        "en": "On the upper floor of a building."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "upstairs",
                                "target_lang_sense_clarification": "on a higher floor"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd3dc776-643b-3f47-98b5-67aff6b56722",
                    "sense_definition": "В верхней части чего-либо, на высоте; в более высоком месте.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "loc_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Посмотри, что там <наверху> на полке.",
                            "target_lang_translations": {
                                "en": "Look at what is <w>up there</w> on the shelf."
                            }
                        },
                        {
                            "text": "Мы забрались на гору, и <наверху> был сильный ветер.",
                            "target_lang_translations": {
                                "en": "We climbed the mountain, and there was a strong wind <w>at the top</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "вверху",
                        "выше",
                        "сверху"
                    ],
                    "antonyms": [
                        "внизу"
                    ],
                    "common_phrases": [
                        "там наверху",
                        "где-то наверху",
                        "самом наверху"
                    ],
                    "target_lang_definitions": {
                        "en": "In a higher place or position; at a height."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "above",
                                "target_lang_sense_clarification": "general location overhead"
                            },
                            {
                                "target_lang_word": "up there",
                                "target_lang_sense_clarification": "indicating a specific high location"
                            },
                            {
                                "target_lang_word": "at the top",
                                "target_lang_sense_clarification": "at the highest point"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68e2bef7-9449-3ca8-b312-9a04a234a096",
                    "sense_definition": "В руководящих органах, среди начальства или властей.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это важное решение было принято <наверху>.",
                            "target_lang_translations": {
                                "en": "This important decision was made <w>at the top</w>."
                            }
                        },
                        {
                            "text": "У него есть связи <наверху>, поэтому он не волнуется.",
                            "target_lang_translations": {
                                "en": "He has connections <w>at the top</w>, so he isn't worried."
                            }
                        }
                    ],
                    "synonyms": [
                        "в верхах"
                    ],
                    "common_phrases": [
                        "решить наверху",
                        "связи наверху",
                        "доложить наверху"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Among the leadership, bosses, or authorities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at the top",
                                "target_lang_sense_clarification": "in the hierarchy or management"
                            },
                            {
                                "target_lang_word": "high up",
                                "target_lang_sense_clarification": "in a position of power"
                            }
                        ]
                    }
                }
            ]
        },
        {

                {
                    "sense_id": "444b98d0-a203-3615-9676-1be671e09cfd",
                    "sense_definition": "Zbiór przedmiotów tego samego rodzaju związanych razem lub trzymanych blisko siebie",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dozorca otworzył ciężką bramę, wyciągając z kieszeni duży <w>pęk</w> kluczy.",
                            "target_lang_translations": {
                                "ru": "Смотритель открыл тяжелые ворота, вытащив из кармана большую <w>связку</w> ключей."
                            }
                        },
                        {
                            "text": "Na stole w salonie stał świeży <w>pęk</w> kwiatów zerwanych w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "На столе в гостиной стоял свежий <w>букет</w> цветов, сорванных в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "wiązka"
                    ],
                    "common_phrases": [
                        "pęk kluczy",
                        "pęk kwiatów",
                        "pęk rzodkiewek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собрание предметов одного вида, связанных вместе или находящихся близко друг к другу"

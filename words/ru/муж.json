            "senses": [
                {
                    "sense_id": "c3e7ecff-32d4-30ba-b7af-11172ed746f6",
                    "sense_definition": "Мужчина по отношению к женщине, с которой он состоит в браке.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "family_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её <муж> работает врачом в большой городской больнице.",
                            "target_lang_translations": {
                                "en": "Her <w>husband</w> works as a doctor in a big city hospital."
                            }
                        },
                        {
                            "text": "На празднике они выглядели как счастливые <муж> и жена.",
                            "target_lang_translations": {
                                "en": "At the celebration, they looked like a happy <w>husband</w> and wife."
                            }
                        },
                        {
                            "text": "Она очень скучала по своему <мужу>, когда он уехал в командировку.",
                            "target_lang_translations": {
                                "en": "She missed her <w>husband</w> very much when he went on a business trip."
                            }
                        }
                    ],
                    "synonyms": [
                        "супру́г"
                    ],
                    "antonyms": [
                        "жена́",
                        "супру́га"
                    ],
                    "common_phrases": [
                        "выйти замуж",
                        "бывший муж",
                        "гражданский муж",
                        "муж и жена"
                    ],
                    "target_lang_definitions": {
                        "en": "A man who is married to a woman; a male spouse."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "husband",
                                "target_lang_sense_clarification": "male spouse"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "410d92d6-b286-3bd0-9067-582ed855c7ad",
                    "sense_definition": "Деятель на каком-либо общественном, научном или государственном поприще (обычно в высоком стиле).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "public_figure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом зале собрались лучшие учёные <мужи> страны.",
                            "target_lang_translations": {
                                "en": "The country's best learned <w>men</w> gathered in this hall."
                            }
                        },
                        {
                            "text": "Как опытный государственный <муж>, он всегда думал о благе народа.",
                            "target_lang_translations": {
                                "en": "As an experienced <w>statesman</w>, he always thought about the welfare of the people."
                            }
                        }
                    ],
                    "common_phrases": [
                        "государственный муж",
                        "учёный муж"
                    ],
                    "target_lang_definitions": {
                        "en": "A prominent person active in public, scientific, or state affairs (usually high style)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "man",
                                "target_lang_sense_clarification": "distinguished person (e.g., learned men)"
                            },
                            {
                                "target_lang_word": "statesman",
                                "target_lang_sense_clarification": "political figure"
                            },
                            {
                                "target_lang_word": "figure",
                                "target_lang_sense_clarification": "public figure"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "599750d4-9621-3d44-9fa6-39959c56d5d3",
                    "sense_definition": "Взрослый человек мужского пола, мужчина (обычно в торжественной или поэтической речи).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "man_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Седой <муж> медленно подошёл к трону и поклонился королю.",
                            "target_lang_translations": {
                                "en": "The gray-haired <w>man</w> slowly approached the throne and bowed to the king."
                            }
                        },
                        {
                            "text": "Теперь я слышу речь не мальчика, но <мужа>.",
                            "target_lang_translations": {
                                "en": "Now I hear the speech not of a boy, but of a <w>man</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "мужчина"
                    ],
                    "traits": [

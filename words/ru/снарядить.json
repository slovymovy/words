    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fef1cf81-340b-3e90-a635-001a57055258",
                    "sense_definition": "Подготовить кого-либо или что-либо к отправке, походу или работе, снабдив всем необходимым (одеждой, припасами, оборудованием).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "equip_prepare",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец помог <снарядить> сына в долгую дорогу, собрав ему рюкзак с едой и тёплыми вещами.",
                            "target_lang_translations": {
                                "en": "The father helped <outfit> his son for the long journey by packing a backpack with food and warm clothes."
                            }
                        },
                        {
                            "text": "Командование решило <снарядить> специальный отряд для разведки местности.",
                            "target_lang_translations": {
                                "en": "The command decided to <equip> a special squad to scout the area."
                            }
                        }
                    ],
                    "synonyms": [
                        "экипировать",
                        "оснастить",
                        "снабдить",
                        "собрать"
                    ],
                    "common_phrases": [
                        "снарядить экспедицию",
                        "снарядить в поход",
                        "снарядить корабль",
                        "снарядить в дорогу"
                    ],
                    "target_lang_definitions": {
                        "en": "To prepare someone or something for departure, a campaign, or work by supplying them with everything necessary (clothing, supplies, equipment)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "equip",
                                "target_lang_sense_clarification": "to supply with necessary items for a particular purpose"
                            },
                            {
                                "target_lang_word": "outfit",
                                "target_lang_sense_clarification": "to provide with equipment or clothes, especially for a journey"
                            },
                            {
                                "target_lang_word": "fit out",
                                "target_lang_sense_clarification": "to supply a ship or expedition with necessaries"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d14ea52-a05b-3218-a3d6-6e34676a848b",
                    "sense_definition": "Отправить кого-либо куда-либо с каким-то поручением (обычно подразумевается предварительная организация этой отправки).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "send_dispatch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <снарядить> самого младшего в магазин за хлебом.",
                            "target_lang_translations": {
                                "en": "We decided to <send off> the youngest one to the store for bread."
                            }
                        },
                        {
                            "text": "Директор приказал срочно <снарядить> курьера с документами в главный офис.",
                            "target_lang_translations": {
                                "en": "The director ordered to urgently <dispatch> a courier with documents to the head office."
                            }
                        }
                    ],
                    "synonyms": [
                        "отправить",
                        "послать",
                        "командировать"
                    ],
                    "common_phrases": [
                        "снарядить гонца",
                        "снарядить посыльного"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies organizing the sending"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To send someone somewhere with a specific task or errand (usually implying preliminary organization)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dispatch",
                                "target_lang_sense_clarification": "to send off to a destination or for a purpose"
                            },
                            {
                                "target_lang_word": "send off",
                                "target_lang_sense_clarification": "to cause someone to go to a place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03f563c1-61ac-3643-bcaa-855345d7a883",
                    "sense_definition": "Красиво одеть, нарядить кого-либо.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dress_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мать старалась <снарядить> дочку как куклу на праздник.",
                            "target_lang_translations": {
                                "en": "The mother tried to <dress up> her daughter like a doll for the holiday."
                            }
                        },
                        {
                            "text": "Бабушка любила <снарядить> внуков во всё новое перед походом в церковь.",
                            "target_lang_translations": {
                                "en": "Grandmother loved to <deck out> the grandchildren in all new clothes before going to church."
                            }
                        }
                    ],
                    "synonyms": [

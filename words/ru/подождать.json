    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bc8dddac-02da-3281-a258-f24836a7fe4e",
                    "sense_definition": "Провести некоторое время в ожидании кого-либо или чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <подождать> автобус на остановке.",
                            "target_lang_translations": {
                                "en": "We decided to <wait> for the bus at the bus stop."
                            }
                        },
                        {
                            "text": "Пожалуйста, <подождите> меня у входа в здание.",
                            "target_lang_translations": {
                                "en": "Please <wait> for me at the entrance to the building."
                            }
                        }
                    ],
                    "common_phrases": [
                        "подождать немного",
                        "подождать ответа",
                        "подождать минуту",
                        "подождать на улице"
                    ],
                    "target_lang_definitions": {
                        "en": "To stay in a place or delay action until something happens or someone arrives."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wait",
                                "target_lang_sense_clarification": "general meaning"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f3038f6-506a-3a91-aeed-0d86239eaa11",
                    "sense_definition": "Не начинать действовать сразу, отложить действие на некоторое время.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "С принятием окончательного решения лучше <подождать>.",
                            "target_lang_translations": {
                                "en": "It is better to <wait> with the final decision."
                            }
                        },
                        {
                            "text": "Не торопись отправлять письмо, стоит <подождать> до утра.",
                            "target_lang_translations": {
                                "en": "Don't rush to send the letter, it is worth <waiting> until morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "повременить",
                        "погодить"
                    ],
                    "antonyms": [
                        "поспешить",
                        "поторопиться"
                    ],
                    "common_phrases": [
                        "подождать с решением",
                        "подождать с выводами",
                        "лучше подождать"
                    ],
                    "target_lang_definitions": {
                        "en": "To delay doing something; to not act immediately."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wait",
                                "target_lang_sense_clarification": "to delay action"
                            },
                            {
                                "target_lang_word": "hold off",
                                "target_lang_sense_clarification": "to postpone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "053a1c59-1da8-3884-a934-c36b1cd398cc",
                    "sense_definition": "Выражение удивления или несогласия в разговорной речи (обычно в повелительном наклонении).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Подожди>, ты действительно хочешь туда пойти?",
                            "target_lang_translations": {
                                "en": "<Wait>, do you really want to go there?"
                            }
                        },
                        {
                            "text": "<Подождите>, я совсем не это имел в виду!",
                            "target_lang_translations": {
                                "en": "<Wait>, that is not what I meant at all!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "подожди-ка",
                        "подожди минутку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to interrupt, express surprise, or ask for a pause in conversation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wait",
                                "target_lang_sense_clarification": "interjection"
                            },
                            {
                                "target_lang_word": "hold on",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "hang on",
                                "target_lang_sense_clarification": "informal"
                            }
                        ]
                    }
                }
            ]

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8e726792-f949-3840-861a-0ff04a31bb7a",
                    "sense_definition": "Mieć określone samopoczucie fizyczne lub psychiczne (używane z zaimkiem zwrotnym \"się\")",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziś rano <w>czuję</w> się bardzo dobrze i mam dużo energii.",
                            "target_lang_translations": {
                                "ru": "Сегодня утром я <w>чувствую</w> себя очень хорошо, и у меня много энергии."
                            }
                        },
                        {
                            "text": "Jak się <w>czujesz</w> po wczorajszym treningu na siłowni?",
                            "target_lang_translations": {
                                "ru": "Как ты себя <w>чувствуешь</w> после вчерашней тренировки в спортзале?"
                            }
                        },
                        {
                            "text": "Mimo choroby pacjent <w>czuł</w> się już znacznie lepiej.",
                            "target_lang_translations": {
                                "ru": "Несмотря на болезнь, пациент <w>чувствовал</w> себя уже значительно лучше."
                            }
                        }
                    ],
                    "synonyms": [
                        "mieć",
                        "uważać się"
                    ],
                    "common_phrases": [
                        "czuć się dobrze",
                        "czuć się źle",
                        "czuć się pewnie",
                        "czuć się jak u siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в определенном физическом или душевном состоянии (используется с возвратным местоимением)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствовать себя",
                                "target_lang_sense_clarification": "состояние здоровья или духа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bc580d2-962f-3484-926f-6c622cab2d7f",
                    "sense_definition": "Odbierać wrażenia zmysłowe, rozpoznawać coś za pomocą węchu, smaku lub dotyku",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Czuję</w> zapach świeżo parzonej kawy w całej kuchni.",
                            "target_lang_translations": {
                                "ru": "Я <w>чувствую</w> запах свежесваренного кофе во всей кухне."
                            }
                        },
                        {
                            "text": "Kiedy dotknął kaloryfera, <w>poczuł</w> przyjemne ciepło.",
                            "target_lang_translations": {
                                "ru": "Когда он дотронулся до батареи, то <w>почувствовал</w> приятное тепло."
                            }
                        },
                        {
                            "text": "Czy ty też <w>czujesz</w> ten dziwny smak w zupie?",
                            "target_lang_translations": {
                                "ru": "Ты тоже <w>чувствуешь</w> этот странный вкус в супе?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wąchać",
                        "odczuwać"
                    ],
                    "common_phrases": [
                        "czuć zapach",
                        "czuć smak",
                        "czuć ból",
                        "czuć głód"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воспринимать впечатления органами чувств: обонянием, вкусом или осязанием"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствовать",
                                "target_lang_sense_clarification": "воспринимать органами чувств"
                            },
                            {
                                "target_lang_word": "ощущать",
                                "target_lang_sense_clarification": "физическое восприятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "555485d4-a2cc-3211-83ef-996aff40a541",
                    "sense_definition": "Doznawać silnych emocji lub uczuć względem kogoś lub czegoś",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek <w>czuje</w> wielką radość z powodu twojego sukcesu.",
                            "target_lang_translations": {

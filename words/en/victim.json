{
    "word_family": [
        "victimize",
        "victimization",
        "victimizer",
        "victimless",
        "victimhood"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "acc08137-3455-374b-8af9-3a2412365e0b",
                    "sense_definition": "A person who is harmed, injured, or killed as a result of a crime, accident, or other event or action.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_harmed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>victim</w> of the robbery gave a detailed statement to the police.",
                            "target_lang_translations": {
                                "ru": "<w>Жертва</w> ограбления дала подробные показания полиции."
                            }
                        },
                        {
                            "text": "She saw herself as a <w>victim</w> of circumstance rather than her own choices.",
                            "target_lang_translations": {
                                "ru": "Она считала себя <w>жертвой</w> обстоятельств, а не собственного выбора."
                            }
                        }
                    ],
                    "synonyms": [
                        "injured party"
                    ],
                    "antonyms": [
                        "culprit",
                        "criminal",
                        "assailant",
                        "aggressor",
                        "offender"
                    ],
                    "common_phrases": [
                        "victim of crime",
                        "play the victim",
                        "fall victim",
                        "innocent victim",
                        "victim of abuse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, пострадавший или погибший в результате преступления, несчастного случая или другого события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жертва",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "потерпевший",
                                "target_lang_sense_clarification": "юридический термин"
                            },
                            {
                                "target_lang_word": "пострадавший",
                                "target_lang_sense_clarification": "в результате несчастного случая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86b2bbeb-40ca-3855-b245-dd8a75f99fed",
                    "sense_definition": "A living being that is killed and offered as a sacrifice, usually in a religious rite.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sacrifice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient tribe offered a <w>victim</w> to appease the gods.",
                            "target_lang_translations": {
                                "ru": "Древнее племя принесло <w>жертву</w>, чтобы умилостивить богов."
                            }
                        },
                        {
                            "text": "A pure white bull was selected as the <w>victim</w> for the ceremony.",
                            "target_lang_translations": {
                                "ru": "Чисто-белый бык был выбран в качестве <w>жертвы</w> для церемонии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sacrificial victim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живое существо, которое убивают и приносят в дар, обычно в религиозном обряде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жертва",
                                "target_lang_sense_clarification": "религиозное приношение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "356cd0e3-e781-3eb7-9ab4-532722124df6",
                    "sense_definition": "To make someone a victim; to treat someone unfairly or cause them harm.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt <w>victimed</w> by the constant criticism from his peers.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя <w>жертвой</w> постоянной критики со стороны сверстников."
                            }
                        },
                        {
                            "text": "The corrupt system <w>victims</w> those who cannot defend themselves.",
                            "target_lang_translations": {
                                "ru": "Коррумпированная система <w>делает жертвами</w> тех, кто не может защитить себя."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Превращать кого-либо в жертву; обращаться с кем-либо несправедливо или причинять вред."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать жертвой",
                                "target_lang_sense_clarification": "превращать в жертву"
                            },
                            {
                                "target_lang_word": "преследовать",
                                "target_lang_sense_clarification": "подвергать гонениям"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
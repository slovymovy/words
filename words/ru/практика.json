            "pos": "noun",
            "senses": [
                {
                    "sense_id": "abab1aad-5dac-36bf-b796-efeaf7d6718b",
                    "sense_definition": "Часть учебного процесса, работа по специальности для получения опыта и закрепления теоретических знаний.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "educational_training",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студенты проходят летнюю <практику> на крупном заводе.",
                            "target_lang_translations": {
                                "en": "Students are doing a summer <internship> at a large factory."
                            }
                        },
                        {
                            "text": "После успешной <практики> меня пригласили на работу.",
                            "target_lang_translations": {
                                "en": "After a successful <internship>, I was offered a job."
                            }
                        }
                    ],
                    "synonyms": [
                        "опыт"
                    ],
                    "common_phrases": [
                        "проходить практику",
                        "производственная практика",
                        "летняя практика",
                        "преддипломная практика"
                    ],
                    "target_lang_definitions": {
                        "en": "Part of the educational process involving work in a specialty to gain experience and consolidate theoretical knowledge."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "internship",
                                "target_lang_sense_clarification": "period of work experience"
                            },
                            {
                                "target_lang_word": "practicum",
                                "target_lang_sense_clarification": "academic course with practical work"
                            },
                            {
                                "target_lang_word": "work placement",
                                "target_lang_sense_clarification": "temporary job for experience"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3135bb4d-b4d2-3b29-bbaf-888a977b6bd0",
                    "sense_definition": "Реальная деятельность, применение знаний в деле (в отличие от теории).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Теория без <практики> бесполезна для специалиста.",
                            "target_lang_translations": {
                                "en": "Theory without <practice> is useless for a specialist."
                            }
                        },
                        {
                            "text": "Мы решили проверить этот новый метод на <практике>.",
                            "target_lang_translations": {
                                "en": "We decided to test this new method in <practice>."
                            }
                        }
                    ],
                    "antonyms": [
                        "теория"
                    ],
                    "common_phrases": [
                        "на практике",
                        "теория и практика",
                        "применить на практике",
                        "оторванный от практики"
                    ],
                    "target_lang_definitions": {
                        "en": "Real activity and application of knowledge, as opposed to theory."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "practice",
                                "target_lang_sense_clarification": "application vs theory"
                            }
                        ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e34833ca-5e95-30d8-892d-5b0c35e9b538",
                    "sense_definition": "Overtuigd zijn dat iets waar is of dat iets of iemand bestaat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "belief_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>geloofde</w> dat de aarde door vliegende schotels bezocht werd.",
                            "target_lang_translations": {
                                "ru": "Он <w>верил</w>, что Землю посещали летающие тарелки."
                            }
                        },
                        {
                            "text": "Ik kan bijna niet <w>geloven</w> dat we de wedstrijd echt gewonnen hebben.",
                            "target_lang_translations": {
                                "ru": "Я почти не могу <w>поверить</w>, что мы действительно выиграли матч."
                            }
                        }
                    ],
                    "synonyms": [
                        "aannemen",
                        "denken"
                    ],
                    "antonyms": [
                        "twijfelen",
                        "ontkennen"
                    ],
                    "common_phrases": [
                        "niet te geloven",
                        "in God geloven",
                        "heilig geloven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть убеждённым в том, что что-то является правдой, или в существовании кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верить",
                                "target_lang_sense_clarification": "иметь убеждение в истинности или существовании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2c9a8e6-a48a-393b-b327-9ace76c91139",
                    "sense_definition": "Vertrouwen dat iemand de waarheid spreekt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "belief_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>geloofde</w> de oplichter en deze wist hem veel geld af te troggelen.",
                            "target_lang_translations": {
                                "ru": "Он <w>поверил</w> мошеннику, и тот сумел выманить у него много денег."
                            }
                        },
                        {
                            "text": "Je moet me op mijn woord <w>geloven</w> als ik zeg dat het veilig is.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>поверить</w> мне на слово, если я говорю, что это безопасно."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertrouwen"
                    ],
                    "antonyms": [
                        "wantrouwen"
                    ],
                    "common_phrases": [
                        "iemand op zijn woord geloven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доверять тому, что кто-то говорит правду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верить",
                                "target_lang_sense_clarification": "принимать чьи-то слова за истину"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a24ea143-4601-3ad5-8929-e658d9a951f9",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>geloof</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze grote stad leven mensen van verschillende <w>geloven</w> vreedzaam samen.",
                            "target_lang_translations": {
                                "ru": "В этом большом городе мирно живут люди разных <w>вероисповеданий</w>."
                            }
                        },
                        {
                            "text": "De dialoog tussen de diverse <w>geloven</w> is belangrijk voor wederzijds begrip.",
                            "target_lang_translations": {
                                "ru": "Диалог между различными <w>вероисповеданиями</w> важен для взаимопонимания."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от существительного «geloof» (вера, религия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вероисповедания",
                                "target_lang_sense_clarification": "религиозные конфессии"
                            },
                            {
                                "target_lang_word": "верования",
                                "target_lang_sense_clarification": "религиозные убеждения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
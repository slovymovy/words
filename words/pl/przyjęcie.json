{
    "word_family": [
        "przyjmować",
        "przyjemność",
        "przyjemny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "afcaca52-64c2-3348-ad0a-ee220b794215",
                    "sense_definition": "Uroczyste spotkanie towarzyskie, często połączone z posiłkiem dla zaproszonych gości",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "social_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wieczorem zorganizowaliśmy małe <w>przyjęcie</w> dla najbliższych przyjaciół.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы организовали небольшую <w>вечеринку</w> для самых близких друзей."
                            }
                        },
                        {
                            "text": "To było bardzo eleganckie <w>przyjęcie</w> weselne z mnóstwem pysznego jedzenia.",
                            "target_lang_translations": {
                                "ru": "Это был очень элегантный свадебный <w>приём</w> с множеством вкусной еды."
                            }
                        }
                    ],
                    "synonyms": [
                        "impreza",
                        "balanga",
                        "biba",
                        "jubel"
                    ],
                    "common_phrases": [
                        "przyjęcie urodzinowe",
                        "przyjęcie weselne",
                        "wydać przyjęcie",
                        "zaproszenie na przyjęcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественное общественное собрание, часто сопровождаемое угощением для приглашенных гостей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "официальное мероприятие"
                            },
                            {
                                "target_lang_word": "вечеринка",
                                "target_lang_sense_clarification": "менее формальное мероприятие"
                            },
                            {
                                "target_lang_word": "банкет",
                                "target_lang_sense_clarification": "торжественный обед или ужин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6f45a19-3162-3336-a6da-7c5f7f099aee",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>przyjąć</w>: akt włączenia kogoś do grupy, zatrudnienia w pracy lub zarejestrowania pacjenta w szpitalu",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_of_admitting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz dyżurny skierował chorego pacjenta natychmiast na izbę <w>przyjęć</w>.",
                            "target_lang_translations": {
                                "ru": "Дежурный врач немедленно направил больного пациента в <w>приёмный</w> покой."
                            }
                        },
                        {
                            "text": "Jego <w>przyjęcie</w> do nowego zespołu w firmie przebiegło bardzo sprawnie.",
                            "target_lang_translations": {
                                "ru": "Его <w>приём</w> в новый коллектив компании прошел очень гладко."
                            }
                        }
                    ],
                    "synonyms": [
                        "akceptacja"
                    ],
                    "common_phrases": [
                        "izba przyjęć",
                        "godziny przyjęć",
                        "przyjęcie do pracy",
                        "przyjęcie do szpitala"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное от «принять»: акт включения кого-либо в группу, найма на работу или регистрации пациента в больнице"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "на работу, в больницу или к врачу"
                            },
                            {
                                "target_lang_word": "зачисление",
                                "target_lang_sense_clarification": "в состав группы или учебное заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2009206c-6e0c-333d-9a45-b20153d124b1",
                    "sense_definition": "Sposób, w jaki ktoś reaguje na nową osobę, dzieło lub wiadomość; reakcja otoczenia",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa książka tego autora spotkała się z bardzo ciepłym <w>przyjęciem</w> krytyków.",
                            "target_lang_translations": {
                                "ru": "Новая книга этого автора встретила очень тёплый <w>приём</w> у критиков."
                            }
                        },
                        {
                            "text": "Jej odważna propozycja nie znalazła dobrego <w>przyjęcia</w> wśród starszych pracowników.",
                            "target_lang_translations": {
                                "ru": "Её смелое предложение не встретило хорошего <w>приёма</w> среди старших сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "odbiór"
                    ],
                    "common_phrases": [
                        "ciepłe przyjęcie",
                        "chłodne przyjęcie",
                        "spotkać się z przyjęciem"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, как кто-то реагирует на нового человека, произведение или новость; реакция окружающих"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "встреча, оказанная кому-либо или чему-либо"
                            },
                            {
                                "target_lang_word": "встреча",
                                "target_lang_sense_clarification": "реакция на появление чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e05626ce-20ef-33ae-9c1e-fe001cd24011",
                    "sense_definition": "Zgoda na zapłatę określonej sumy pieniężnej, na przykład weksla lub czeku",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "financial_acceptance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bank potwierdził <w>przyjęcie</w> czeku do realizacji w trybie natychmiastowym.",
                            "target_lang_translations": {
                                "ru": "Банк подтвердил <w>принятие</w> чека к исполнению в немедленном порядке."
                            }
                        },
                        {
                            "text": "Warunkiem finalizacji tej transakcji jest <w>przyjęcie</w> weksla przez dłużnika.",
                            "target_lang_translations": {
                                "ru": "Условием завершения этой сделки является <w>принятие</w> векселя должником."
                            }
                        }
                    ],
                    "synonyms": [
                        "akcept"
                    ],
                    "common_phrases": [
                        "przyjęcie weksla",
                        "przyjęcie czeku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Согласие на оплату определенной денежной суммы, например, векселя или чека"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принятие",
                                "target_lang_sense_clarification": "к оплате или исполнению"
                            },
                            {
                                "target_lang_word": "акцепт",
                                "target_lang_sense_clarification": "банковский термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

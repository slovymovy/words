                        "bicz wodny"
                    ],
                    "common_phrases": [
                        "korzystać z hydromasażu",
                        "zabieg hydromasażu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лечебная или расслабляющая процедура, заключающаяся в массаже тела струями воды."
                    },
                    "translations": {

                    "sense_id": "a871642a-6b64-3427-a048-ecbfc59acd02",
                    "sense_definition": "Odpadki, resztki jedzenia lub zużyte przedmioty przeznaczone do wyrzucenia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "trash_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę pamiętać, żeby rano wynieść <w>śmieci</w> do kontenera.",
                            "target_lang_translations": {
                                "ru": "Мне нужно помнить, чтобы утром вынести <w>мусор</w> в контейнер."
                            }
                        },
                        {
                            "text": "W naszym domu segregujemy <w>śmieci</w> na szkło, papier i plastik.",
                            "target_lang_translations": {
                                "ru": "У нас дома мы сортируем <w>мусор</w> на стекло, бумагу и пластик."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpad"
                    ],
                    "common_phrases": [
                        "wynosić śmieci",
                        "kosz na śmieci",
                        "segregacja śmieci",
                        "kupa śmieci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отходы, остатки еды или использованные предметы, предназначенные для выбрасывания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мусор",
                                "target_lang_sense_clarification": "бытовые отходы"

{
    "word_family": [
        "echt",
        "echtheid"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b208af34-6c1a-32cf-affc-6dd70df7e21b",
                    "sense_definition": "Drukt een tegenstelling of beperking uit ten opzichte van het voorgaande.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij wilde graag komen, hij had <w>echter</w> geen tijd.",
                            "target_lang_translations": {
                                "en": "He wanted to come; <w>however</w>, he didn't have time.",
                                "ru": "Он очень хотел прийти, <w>однако</w> у него не было времени."
                            }
                        },
                        {
                            "text": "Het plan klinkt goed; er zijn <w>echter</w> enkele risico's verbonden aan de uitvoering.",
                            "target_lang_translations": {
                                "en": "The plan sounds good; <w>however</w>, there are some risks attached to the execution.",
                                "ru": "План звучит хорошо; с его выполнением, <w>однако</w>, связаны некоторые риски."
                            }
                        }
                    ],
                    "synonyms": [
                        "evenwel"
                    ],
                    "common_phrases": [
                        "dit is echter",
                        "er zijn echter"
                    ],
                    "target_lang_definitions": {
                        "en": "Expresses a contrast or limitation relative to the preceding statement.",
                        "ru": "Выражает противопоставление или ограничение по отношению к сказанному ранее."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "however",
                                "target_lang_sense_clarification": "used to introduce a statement that contrasts with or contradicts something that has been said previously"
                            },
                            {
                                "target_lang_word": "nevertheless",
                                "target_lang_sense_clarification": "formal; in spite of that; notwithstanding"
                            },
                            {
                                "target_lang_word": "though",
                                "target_lang_sense_clarification": "used to introduce a contrasting fact, often less formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "однако",
                                "target_lang_sense_clarification": "противопоставление"
                            },
                            {
                                "target_lang_word": "тем не менее",
                                "target_lang_sense_clarification": "вводное слово"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e0c4a848-f817-3346-9579-a5685de17d4d",

{
    "word_family": [
        "agency"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1aee2f0b-c584-3fd1-b6a3-abe672fd9a72",
                    "sense_definition": "A person who acts on behalf of another person or organization in business, legal, or financial matters.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "representative_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We contacted a real estate <w>agent</w> to help us sell our house.",
                            "target_lang_translations": {
                                "ru": "Мы связались с <w>агентом</w> по недвижимости, чтобы он помог нам продать дом."
                            }
                        },
                        {
                            "text": "The insurance <w>agent</w> explained the details of the policy coverage.",
                            "target_lang_translations": {
                                "ru": "Страховой <w>агент</w> объяснил детали страхового покрытия."
                            }
                        }
                    ],
                    "synonyms": [
                        "representative",
                        "rep"
                    ],
                    "common_phrases": [
                        "real estate agent",
                        "travel agent",
                        "insurance agent",
                        "estate agent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, действующее от имени другого лица или организации в деловых, юридических или финансовых вопросах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "представитель, посредник"
                            },
                            {
                                "target_lang_word": "представитель",
                                "target_lang_sense_clarification": "уполномоченное лицо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf307fb4-80be-3f16-9327-872b97887c17",
                    "sense_definition": "A person employed by a government department or intelligence organization to gather information or enforce laws.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The secret <w>agent</w> traveled undercover to gather intelligence on the group.",
                            "target_lang_translations": {
                                "ru": "Секретный <w>агент</w> путешествовал под прикрытием, чтобы собрать разведданные о группе."
                            }
                        },
                        {
                            "text": "Two federal <w>agents</w> arrived at the scene to investigate the crime.",
                            "target_lang_translations": {
                                "ru": "Два федеральных <w>агента</w> прибыли на место происшествия для расследования преступления."
                            }
                        }
                    ],
                    "common_phrases": [
                        "secret agent",
                        "federal agent",
                        "special agent",
                        "police agent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, нанятое правительственным ведомством или разведывательной организацией для сбора информации или обеспечения соблюдения законов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "сотрудник спецслужб"
                            },
                            {
                                "target_lang_word": "оперативник",
                                "target_lang_sense_clarification": "сотрудник правоохранительных органов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62751dd2-7127-31bf-8daf-5078f4868fa4",
                    "sense_definition": "A person who finds work for actors, musicians, athletes, or writers and manages their business affairs.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "representative_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>agent</w> negotiated a much higher salary for the new movie role.",
                            "target_lang_translations": {
                                "ru": "Её <w>агент</w> договорился о гораздо более высокой зарплате за роль в новом фильме."
                            }
                        },
                        {
                            "text": "The football player hired a new <w>agent</w> to handle his contract talks.",
                            "target_lang_translations": {
                                "ru": "Футболист нанял нового <w>агента</w> для ведения переговоров по контракту."
                            }
                        }
                    ],
                    "synonyms": [
                        "manager",
                        "representative"
                    ],
                    "common_phrases": [
                        "sports agent",
                        "literary agent",
                        "talent agent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который находит работу для актеров, музыкантов, спортсменов или писателей и управляет их деловыми вопросами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "менеджер артиста или спортсмена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e613ce9-efa5-3b9c-afc0-e667f6cb6a02",
                    "sense_definition": "A substance or force that produces a specific chemical, biological, or physical effect.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "active_force_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lemon juice acts as a natural cleaning <w>agent</w> for removing stains.",
                            "target_lang_translations": {
                                "ru": "Лимонный сок действует как натуральное чистящее <w>средство</w> для удаления пятен."
                            }
                        },
                        {
                            "text": "The factory was fined for releasing a toxic chemical <w>agent</w> into the river.",
                            "target_lang_translations": {
                                "ru": "Завод оштрафовали за сброс токсичного химического <w>вещества</w> в реку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cleaning agent",
                        "chemical agent",
                        "biological agent",
                        "oxidizing agent"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical or scientific"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество или сила, вызывающая определенный химический, биологический или физический эффект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средство",
                                "target_lang_sense_clarification": "вещество для определенной цели"
                            },
                            {
                                "target_lang_word": "вещество",
                                "target_lang_sense_clarification": "химическое или биологическое"
                            },
                            {
                                "target_lang_word": "реагент",
                                "target_lang_sense_clarification": "химический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2611f4a-67d1-3c3f-9fa5-5cbb4e9f7bab",
                    "sense_definition": "Someone or something that takes an active role, exerts power, or produces an effect.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "active_force_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Education is a powerful <w>agent</w> of social change in developing countries.",
                            "target_lang_translations": {
                                "ru": "Образование — это мощный <w>фактор</w> социальных перемен в развивающихся странах."
                            }
                        },
                        {
                            "text": "Humans are moral <w>agents</w> who are responsible for their own choices.",
                            "target_lang_translations": {
                                "ru": "Люди — это моральные <w>субъекты</w>, которые несут ответственность за свой собственный выбор."
                            }
                        }
                    ],
                    "synonyms": [
                        "actor",
                        "performer"
                    ],
                    "common_phrases": [
                        "agent of change",
                        "free agent",
                        "moral agent"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or philosophical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кто-то или что-то, что играет активную роль, проявляет силу или производит эффект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фактор",
                                "target_lang_sense_clarification": "движущая сила"
                            },
                            {
                                "target_lang_word": "проводник",
                                "target_lang_sense_clarification": "носитель идеи или силы"
                            },
                            {
                                "target_lang_word": "субъект",
                                "target_lang_sense_clarification": "философское понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0991f853-acda-3d36-8085-09b3c4ca1e76",
                    "sense_definition": "A computer program that performs a specific task in the background or on behalf of a user.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software <w>agent</w> automatically filters spam emails from your inbox.",
                            "target_lang_translations": {
                                "ru": "Программный <w>агент</w> автоматически фильтрует спам во входящих сообщениях."
                            }
                        },
                        {
                            "text": "Web browsers act as a user <w>agent</w> to retrieve content from servers.",
                            "target_lang_translations": {
                                "ru": "Веб-браузеры действуют как пользовательский <w>агент</w> (user agent) для получения контента с серверов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "user agent",
                        "intelligent agent",
                        "software agent"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа, которая выполняет определенную задачу в фоновом режиме или от имени пользователя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "компьютерная программа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e028b69-8328-32ca-baf1-04dd71b9e9d4",
                    "sense_definition": "The person or thing that performs the action expressed by the verb in a sentence.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the sentence 'John kicked the ball', John is the <w>agent</w> of the action.",
                            "target_lang_translations": {
                                "ru": "В предложении «Джон ударил по мячу» Джон является <w>агенсом</w> действия."
                            }
                        },
                        {
                            "text": "Passive sentences often omit the <w>agent</w> if it is unknown or unimportant.",
                            "target_lang_translations": {
                                "ru": "Пассивные предложения часто опускают <w>агенс</w>, если он неизвестен или неважен."
                            }
                        }
                    ],
                    "antonyms": [
                        "patient",
                        "recipient"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "linguistics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или предмет, выполняющий действие, выраженное глаголом в предложении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агенс",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "субъект",
                                "target_lang_sense_clarification": "производитель действия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e13fcf3c-5d93-3694-bf12-f0cf4181bba5",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We met Mr. <w>Agent</w> at the conference last week.",
                            "target_lang_translations": {
                                "ru": "Мы встретили мистера <w>Агента</w> на конференции на прошлой неделе."
                            }
                        },
                        {
                            "text": "The <w>Agent</w> family has lived in this town for generations.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Агент</w> живет в этом городе уже несколько поколений."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Агент",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
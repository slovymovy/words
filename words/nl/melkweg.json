    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c4d3cf3d-85fc-3c12-83eb-5cca325e85a0",
                    "sense_definition": "Het sterrenstelsel waarin ons zonnestelsel zich bevindt, zichtbaar als een lichtende band aan de nachtelijke hemel.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Op een heldere nacht kun je de <w>Melkweg</w> goed zien.",
                            "target_lang_translations": {
                                "ru": "В ясную ночь можно хорошо разглядеть <w>Млечный Путь</w>."
                            }
                        },
                        {
                            "text": "Onze zon is slechts een klein onderdeel van de <w>Melkweg</w>.",
                            "target_lang_translations": {
                                "ru": "Наше Солнце — лишь малая часть <w>Млечного Пути</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de Melkweg zien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Галактика, в которой находится наша Солнечная система, видимая как светящаяся полоса на ночном небе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Млечный Путь",
                                "target_lang_sense_clarification": "наша Галактика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0878ec0d-e719-3591-af97-11deebfd2979",
                    "sense_definition": "Een grote verzameling sterren die door de eigen zwaartekracht bij elkaar gehouden wordt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Astronomen hebben een nieuwe <w>melkweg</w> ontdekt.",
                            "target_lang_translations": {
                                "ru": "Астрономы открыли новую <w>галактику</w>."
                            }
                        },
                        {
                            "text": "In die verre <w>melkweg</w> zijn veel jonge sterren te vinden.",
                            "target_lang_translations": {
                                "ru": "В той далекой <w>галактике</w> можно найти много молодых звезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "sterrenstelsel",
                        "melkwegstelsel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое скопление звезд, удерживаемое вместе собственной гравитацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галактика",
                                "target_lang_sense_clarification": "звездная система"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        "target_lang_code": "de",
                        "target_sense_ids": [],
                        "cross_references": [
                            {
                                "target_word": "Fehlverhalten",
                                "target_lang_code": "de",
                                "target_senses": [
                                    {
                                        "sense_id": "4832a7be-4289-39fe-8594-ce7d34fa960b",
                                        "entry_id": "7040279a-7b31-3afc-bdbd-23de233801d9",
                                        "glosses": [
                                            "falsches, unangebrachtes Verhalten"
                                        ],
                                        "tags": [],
                                        "examples": [
                                            {
                                                "example_id": "de3e0bf4-236c-4e9a-8248-44f056077bf4",
                                                "sense_id": "4832a7be-4289-39fe-8594-ce7d34fa960b",
                                                "text": "Ein solches Fehlverhalten ist unakzeptabel."
                                            },
                                            {
                                                "example_id": "f0e0d1ab-1eab-4bf3-93b3-f6377e62da07",
                                                "sense_id": "4832a7be-4289-39fe-8594-ce7d34fa960b",
                                                "text": "„Das Oberlandesgericht Wien kam zu dem Schluss, dass dieses Fehlverhalten aufgrund der Begleitumstände die fristlose Entlassung des Arbeitnehmers nicht rechtfertigt.“"
                                            },
                                            {
                                                "example_id": "26085098-5ae6-412d-80bc-5cd3a141f1dc",
                                                "sense_id": "4832a7be-4289-39fe-8594-ce7d34fa960b",
                                                "text": "„Die Briten versuchten sich einmal mehr aus ihrer historischen Verantwortung zu stehlen und leugneten ihr koloniales Fehlverhalten.“"
                                            }
                                        ],
                                        "sense_index_json": "\"1\""
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "translation_id": "4cf7eda5-c308-4547-a88d-76e4017d5af1",

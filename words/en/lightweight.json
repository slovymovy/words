                },
                {
                    "sense_id": "6a0dfcaa-2dfc-3b68-bd3c-cec37353ebe8",
                    "sense_definition": "Lacking deep meaning, seriousness, or influence.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie was entertaining but a bit <w>lightweight</w> in its plot.",

            "senses": [
                {
                    "sense_id": "8825b8a1-3933-38c5-8fa9-71f03af67d53",
                    "sense_definition": "Dostarczać jedzenia, dawać pożywienie ludziom lub zwierzętom.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci poszły do parku, aby <karmić> kaczki chlebem.",
                            "target_lang_translations": {
                                "ru": "Дети пошли в парк, чтобы <w>кормить</w> уток хлебом."
                            }
                        },
                        {
                            "text": "Mój brat codziennie rano <karmi> naszego psa.",
                            "target_lang_translations": {
                                "ru": "Мой брат каждое утро <w>кормит</w> нашу собаку."
                            }
                        }
                    ],
                    "synonyms": [
                        "odżywiać",
                        "odżywiać"
                    ],
                    "antonyms": [
                        "głodzić"
                    ],
                    "common_phrases": [
                        "karmić psa",
                        "karmić gołębie",
                        "karmić łyżeczką"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать еду, пищу людям или животным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кормить",
                                "target_lang_sense_clarification": "давать пищу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e605910a-a0d7-3988-ba74-df36bb255030",
                    "sense_definition": "Żywić potomstwo własnym mlekiem (o matkach i samicach ssaków).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele matek decyduje się <karmić> piersią przez pierwszy rok życia dziecka.",
                            "target_lang_translations": {
                                "ru": "Многие матери решают <w>кормить</w> грудью в течение первого года жизни ребенка."
                            }
                        },
                        {
                            "text": "Ona <karmi> swoje niemowlę na żądanie, kiedy tylko jest głodne.",
                            "target_lang_translations": {
                                "ru": "Она <w>кормит</w> своего младенца по требованию, как только он голоден."
                            }
                        }
                    ],
                    "common_phrases": [
                        "karmić piersią",
                        "karmić butelką",
                        "karmić dziecko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вскармливать потомство своим молоком (о матерях и самках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кормить",
                                "target_lang_sense_clarification": "грудью, молоком"
                            }
                        ]
                    }
                }
            ]
        }

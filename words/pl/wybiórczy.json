    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0b15848c-773d-3aba-ad35-bf284ff2a9ab",
                    "sense_definition": "Obejmujący tylko wybrane elementy z całości, a nie wszystko; nieszczegółowy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>wybiórcza</w> pamięć sprawia, że pamięta tylko to, co jest dla niego wygodne.",
                            "target_lang_translations": {
                                "ru": "Его <w>избирательная</w> память приводит к тому, что он помнит только то, что ему удобно."
                            }
                        },
                        {
                            "text": "Zastosowaliśmy podejście <w>wybiórcze</w>, koncentrując się wyłącznie na najważniejszych aspektach projektu.",
                            "target_lang_translations": {
                                "ru": "Мы применили <w>избирательный</w> подход, сосредоточившись исключительно на самых важных аспектах проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "selektywny"
                    ],
                    "antonyms": [
                        "całościowy",
                        "kompleksowy"
                    ],
                    "common_phrases": [
                        "pamięć wybiórcza",
                        "wybiórcze podejście",
                        "wybiórcza uwaga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охватывающий только отдельные элементы целого, а не всё; неполный, фрагментарный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избирательный",
                                "target_lang_sense_clarification": "касающийся выбора, неполный"
                            },
                            {
                                "target_lang_word": "выборочный",
                                "target_lang_sense_clarification": "производимый путем отбора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a400c4c-a3fa-3079-a061-34f2dbc6968d",
                    "sense_definition": "Taki, który dokonuje wyboru w sposób surowy, stawiając wysokie wymagania; trudno go zadowolić.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój kot jest bardzo <w>wybiórczy</w> jeśli chodzi o jedzenie i akceptuje tylko jedną markę karmy.",
                            "target_lang_translations": {
                                "ru": "Мой кот очень <w>привередлив</w> в еде и признает корм только одной марки."
                            }
                        },
                        {
                            "text": "Musisz być bardziej <w>wybiórczy</w> w doborze współpracowników, aby uniknąć problemów w przyszłości.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть более <w>разборчивым</w> в выборе сотрудников, чтобы избежать проблем в будущем."
                            }
                        }
                    ],
                    "synonyms": [
                        "wybredny"
                    ],
                    "common_phrases": [
                        "być wybiórczym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строгий в выборе, предъявляющий высокие требования; тот, кому трудно угодить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разборчивый",
                                "target_lang_sense_clarification": "требовательный к качеству"
                            },
                            {
                                "target_lang_word": "привередливый",
                                "target_lang_sense_clarification": "капризный, слишком требовательный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

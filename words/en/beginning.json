        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "55b3a629-3ecb-3ab0-ae0f-67dbab4a2a93",
                    "sense_definition": "The first part or section of something that continues for a period of time or space.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author describes the main character’s youth at the <w>beginning</w> of the story.",
                            "target_lang_translations": {
                                "ru": "Автор описывает юность главного героя в <w>начале</w> рассказа."
                            }
                        },
                        {
                            "text": "Please sign your name at the <w>beginning</w> of the document.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поставьте свою подпись в <w>начале</w> документа."
                            }
                        }
                    ],
                    "synonyms": [
                        "start",
                        "head"
                    ],
                    "antonyms": [
                        "end",
                        "conclusion"
                    ],
                    "common_phrases": [
                        "at the beginning",
                        "from the beginning",
                        "in the beginning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первая часть или раздел чего-либо, продолжающийся во времени или пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начало",
                                "target_lang_sense_clarification": "первая часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b584d44b-429e-3121-81e6-731b96440399",
                    "sense_definition": "The act or process of starting an action or event.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This meeting marks the <w>beginning</w> of our new project.",
                            "target_lang_translations": {
                                "ru": "Эта встреча знаменует <w>начало</w> нашего нового проекта."
                            }
                        },
                        {
                            "text": "We are looking forward to the <w>beginning</w> of the school year.",
                            "target_lang_translations": {
                                "ru": "Мы с нетерпением ждем <w>начала</w> учебного года."

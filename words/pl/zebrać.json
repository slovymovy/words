                },
                {
                    "sense_id": "003be6d7-54f7-3838-b348-c4308f494388",
                    "sense_definition": "Spotkać się w większej grupie osób w określonym miejscu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gromadzenie",
                    "name_type": "no",
                    "examples": [

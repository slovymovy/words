    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7309f7df-98e5-3cac-b38b-f9b20de73e00",
                    "sense_definition": "Taki, który czuje miłość do innej osoby; będący pod wpływem silnego uczucia do kogoś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_love",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest <zakochany> po uszy w swojej nowej koleżance z pracy.",
                            "target_lang_translations": {
                                "ru": "Мой брат по уши <w>влюблён</w> в свою новую коллегу по работе."
                            }
                        },
                        {
                            "text": "Ona wygląda na bardzo szczęśliwą i szczerze <zakochaną>.",
                            "target_lang_translations": {
                                "ru": "Она выглядит очень счастливой и искренне <w>влюблённой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zadurzony",
                        "rozkochany"
                    ],
                    "common_phrases": [
                        "zakochany po uszy",
                        "zakochany bez pamięci",
                        "zakochany do szaleństwa",
                        "zakochany od pierwszego wejrzenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто испытывает любовь к другому человеку; находящийся под влиянием сильного чувства к кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влюблённый",
                                "target_lang_sense_clarification": "испытывающий чувство любви"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3f1704f6-3767-35e2-92e9-a4712d9992c9",
                    "sense_definition": "Osoba, która darzy kogoś uczuciem miłości.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_love",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W parku na ławce siedzieli przytuleni do siebie <zakochani>.",
                            "target_lang_translations": {
                                "ru": "В парке на скамейке сидели, прижавшись друг к другу, <w>влюблённые</w>."
                            }
                        },
                        {
                            "text": "Walentynki to wyjątkowy dzień dla wszystkich <zakochanych>.",
                            "target_lang_translations": {
                                "ru": "День святого Валентина — особенный день для всех <w>влюблённых</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzień zakochanych",
                        "święto zakochanych",
                        "para zakochanych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который испытывает к кому-то чувство любви."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влюблённый",
                                "target_lang_sense_clarification": "субстантивированное прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "25303aa9-3e05-36d9-9b87-e4be9e42499d",
                    "sense_definition": "Imiesłów przymiotnikowy od czasownika: <zakochać> się.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_love",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem w tobie <zakochany> od chwili, gdy cię pierwszy raz zobaczyłem.",
                            "target_lang_translations": {
                                "ru": "Я <w>влюблён</w> в тебя с того момента, как впервые тебя увидел."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie byłem tak bardzo <zakochany> jak teraz.",
                            "target_lang_translations": {
                                "ru": "Никогда раньше я не был так сильно <w>влюблён</w>, как сейчас."
                            }
                        }
                    ],

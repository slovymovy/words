                    ],
                    "synonyms": [
                        "relate",
                        "correspond"
                    ],
                    "common_phrases": [
                        "bear no relation",
                        "bear on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь логическую связь или отношение к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь отношение",
                                "target_lang_sense_clarification": "быть связанным с чем-то"
                            }
                        ]
                    }
                }
            ]
        },
        {

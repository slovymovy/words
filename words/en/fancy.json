    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ad2e0477-4724-3074-8d76-ecbc887482f3",
                    "sense_definition": "Decorative or featuring intricate details; not plain.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "adj_decorative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a <w>fancy</w> dress to the wedding party.",
                            "target_lang_translations": {
                                "ru": "Она надела <w>нарядное</w> платье на свадебную вечеринку."
                            }
                        },
                        {
                            "text": "I do not need a <w>fancy</w> phone, just one that works.",
                            "target_lang_translations": {
                                "ru": "Мне не нужен <w>навороченный</w> телефон, просто такой, который работает."
                            }
                        }
                    ],
                    "synonyms": [
                        "decorative",
                        "ornate"
                    ],
                    "antonyms": [
                        "plain",
                        "simple"
                    ],
                    "common_phrases": [
                        "fancy dress",
                        "fancy restaurant",
                        "look fancy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный или с замысловатыми деталями; не простой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарядный",
                                "target_lang_sense_clarification": "одежда, внешний вид"
                            },
                            {
                                "target_lang_word": "причудливый",
                                "target_lang_sense_clarification": "необычный, с украшениями"
                            },
                            {
                                "target_lang_word": "навороченный",
                                "target_lang_sense_clarification": "разг. технически сложный, модный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83330fd3-6862-33e9-a630-07047a653854",
                    "sense_definition": "Expensive, luxurious, or of superior quality.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They stayed at a <w>fancy</w> hotel during their vacation in Paris.",
                            "target_lang_translations": {
                                "ru": "Они остановились в <w>шикарном</w> отеле во время отпуска в Париже."
                            }
                        },
                        {
                            "text": "We cannot afford to pay such <w>fancy</w> prices for dinner.",
                            "target_lang_translations": {
                                "ru": "Мы не можем позволить себе платить такие <w>заоблачные</w> цены за ужин."
                            }
                        }
                    ],
                    "synonyms": [
                        "high-end"
                    ],
                    "antonyms": [
                        "simple"
                    ],
                    "common_phrases": [
                        "fancy car",
                        "fancy hotel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дорогой, роскошный или высшего качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шикарный",
                                "target_lang_sense_clarification": "роскошный, дорогой"
                            },
                            {
                                "target_lang_word": "роскошный",
                                "target_lang_sense_clarification": "богатый, великолепный"
                            },
                            {
                                "target_lang_word": "модный",
                                "target_lang_sense_clarification": "престижный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9253767c-36be-3ee4-a72d-85aef9414d8e",
                    "sense_definition": "Executed with great skill or complexity.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "adj_skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The football player showed off some <w>fancy</w> footwork to avoid the defender.",
                            "target_lang_translations": {
                                "ru": "Футболист продемонстрировал <w>виртуозную</w> работу ног, чтобы уйти от защитника."
                            }
                        },
                        {
                            "text": "He tried some <w>fancy</w> driving maneuvers on the ice.",
                            "target_lang_translations": {
                                "ru": "Он попытался выполнить несколько <w>хитрых</w> маневров вождения на льду."
                            }
                        }
                    ],
                    "antonyms": [
                        "simple"
                    ],
                    "common_phrases": [
                        "fancy footwork",
                        "fancy moves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполненный с большим мастерством или сложностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виртуозный",
                                "target_lang_sense_clarification": "о мастерстве"
                            },
                            {
                                "target_lang_word": "хитрый",
                                "target_lang_sense_clarification": "сложный, искусный"
                            },
                            {
                                "target_lang_word": "искусный",
                                "target_lang_sense_clarification": "умелый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06ba3a6b-f8ea-3ac7-a632-fad60c6730a5",
                    "sense_definition": "Unnecessarily complicated or elaborate.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adj_complicated",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am not interested in his <w>fancy</w> ideas about how to run the company.",
                            "target_lang_translations": {
                                "ru": "Меня не интересуют его <w>мудрёные</w> идеи о том, как управлять компанией."
                            }
                        },
                        {
                            "text": "Just give me the facts without any <w>fancy</w> words.",
                            "target_lang_translations": {
                                "ru": "Просто изложи мне факты без всяких <w>красивых</w> слов."
                            }
                        }
                    ],
                    "synonyms": [
                        "highfalutin"
                    ],
                    "antonyms": [
                        "simple"
                    ],
                    "target_lang_definitions": {
                        "ru": "Излишне сложный или вычурный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мудрёный",
                                "target_lang_sense_clarification": "разг. непонятный, запутанный"
                            },
                            {
                                "target_lang_word": "вычурный",
                                "target_lang_sense_clarification": "излишне украшенный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dcf4ff75-6cea-3e9f-95f6-6eac4662b9a3",
                    "sense_definition": "To want to have or do something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_desire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you <w>fancy</w> going to the cinema tonight?",
                            "target_lang_translations": {
                                "ru": "Ты <w>хочешь</w> пойти в кино сегодня вечером?"
                            }
                        },
                        {
                            "text": "I really <w>fancy</w> a cold drink right now.",
                            "target_lang_translations": {
                                "ru": "Я очень <w>хочу</w> холодный напиток прямо сейчас."
                            }
                        }
                    ],
                    "synonyms": [
                        "feel like"
                    ],
                    "common_phrases": [
                        "fancy a drink",
                        "fancy a go"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хотеть иметь что-то или сделать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "иметь желание"
                            },
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "стремиться к чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a62763d1-b6da-35b9-b821-f1ed737b5fe3",
                    "sense_definition": "To be sexually or romantically attracted to someone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_attraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I think he <w>fancies</w> the girl who works at the coffee shop.",
                            "target_lang_translations": {
                                "ru": "Думаю, ему <w>нравится</w> девушка, которая работает в кофейне."
                            }
                        },
                        {
                            "text": "She has <w>fancied</w> him since they were in school together.",
                            "target_lang_translations": {
                                "ru": "Он ей <w>нравился</w> с тех пор, как они вместе учились в школе."
                            }
                        }
                    ],
                    "synonyms": [
                        "like"
                    ],
                    "common_phrases": [
                        "fancy someone rotten"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British English"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать сексуальное или романтическое влечение к кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нравиться",
                                "target_lang_sense_clarification": "вызывать симпатию"
                            },
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "испытывать физическое влечение"

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb26fd7b-be78-3ef3-8683-e9b43273acb8",
                    "sense_definition": "Een gereedschap dat bestaat uit twee scharnierende delen, gebruikt om voorwerpen vast te grijpen, te buigen of door te knippen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zou ik jouw <w>tang</w> even mogen lenen?",
                            "target_lang_translations": {
                                "en": "Could I borrow your <w>pliers</w> for a moment?"
                            }
                        },
                        {
                            "text": "De loodgieter gebruikte een <w>tang</w> om de lekkende buis stevig vast te draaien.",
                            "target_lang_translations": {
                                "en": "The plumber used a <w>pair of pliers</w> to firmly tighten the leaking pipe."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een tang gebruiken",
                        "met een tang"
                    ],
                    "target_lang_definitions": {
                        "en": "A tool consisting of two hinged parts, used to grip, bend, or cut objects."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pliers",
                                "target_lang_sense_clarification": "tool for gripping, bending, or cutting"
                            },
                            {
                                "target_lang_word": "tongs",
                                "target_lang_sense_clarification": "tool for picking up objects"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7361e737-9c92-3615-a36d-e0074164d977",
                    "sense_definition": "Een onaardige, vaak oudere vrouw die zich vervelend, bazig of kwaadaardig gedraagt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat een ouwe <w>tang</w> is zij!",
                            "target_lang_translations": {
                                "en": "What an old <w>hag</w> she is!"
                            }
                        },
                        {
                            "text": "Zijn buurvrouw gedraagt zich als een echte <w>tang</w> door altijd tegen de kinderen te schreeuwen.",
                            "target_lang_translations": {
                                "en": "His neighbor behaves like a real <w>shrew</w> by always screaming at the children."
                            }
                        }
                    ],
                    "synonyms": [
                        "feeks",
                        "helleveeg",
                        "kreng"
                    ],
                    "common_phrases": [
                        "een oude tang",
                        "wat een tang"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An unkind, often older woman who behaves in an annoying, bossy, or malicious manner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hag",
                                "target_lang_sense_clarification": "unpleasant older woman"
                            },
                            {
                                "target_lang_word": "shrew",
                                "target_lang_sense_clarification": "ill-tempered woman"
                            },
                            {
                                "target_lang_word": "vixen",
                                "target_lang_sense_clarification": "malicious or quarrelsome woman"
                            }
                        ]
                    }
                }
            ]
        }

                            "target_lang_translations": {
                                "ru": "Мистер <w>Бамп</w> — персонаж известной серии детских книг."
                            }
                        },
                        {
                            "text": "We met a family with the last name <w>Bump</w> at the event.",
                            "target_lang_translations": {
                                "ru": "Мы встретили семью с фамилией <w>Бамп</w> на мероприятии."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бамп",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "fbe7b28a-8eb0-3243-b956-46036b14480b",
                    "sense_definition": "Getallen in de juiste volgorde opnoemen (1, 2, 3, enzovoort)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "numbers",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik doe mijn ogen dicht en <w>tel</w> langzaam tot tien.",
                            "target_lang_translations": {
                                "ru": "Я закрываю глаза и медленно <w>считаю</w> до десяти.",
                                "en": "I close my eyes and slowly <w>count</w> to ten."
                            }
                        },
                        {
                            "text": "Mijn kleine broertje leert net om tot twintig te <w>tellen</w>.",
                            "target_lang_translations": {
                                "ru": "Мой младший брат только учится <w>считать</w> до двадцати.",
                                "en": "My little brother is just learning to <w>count</w> to twenty."
                            }
                        }
                    ],
                    "synonyms": [
                        "opnoemen"
                    ],
                    "common_phrases": [
                        "tot tien tellen",
                        "hardop tellen",
                        "op je vingers tellen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Называть числа по порядку (1, 2, 3 и т. д.)",
                        "en": "To recite numbers in sequence (1, 2, 3, etc.)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "называть числа"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "count",
                                "target_lang_sense_clarification": "recite numbers"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "debaa32e-8f8a-3392-89ed-b4393d0774d4",
                    "sense_definition": "Het aantal van personen of dingen bepalen",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "numbers",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>tel</w> hoeveel geld ik nog in mijn portemonnee heb.",
                            "target_lang_translations": {
                                "ru": "Я <w>считаю</w>, сколько денег у меня осталось в кошельке.",
                                "en": "I <w>count</w> how much money I still have in my wallet."
                            }
                        },
                        {
                            "text": "De boer <w>telt</w> elke avond zijn koeien in de wei.",
                            "target_lang_translations": {
                                "ru": "Фермер каждый вечер <w>считает</w> своих коров на лугу.",
                                "en": "The farmer <w>counts</w> his cows in the meadow every evening."
                            }
                        }
                    ],
                    "common_phrases": [
                        "je zegeningen tellen",
                        "de dagen tellen",
                        "geld tellen"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c79f0cb5-42fa-37b8-9d03-f3f96c5b77d1",
                    "sense_definition": "Хорошо или плохо отмываться, становиться чистым в процессе стирки (об одежде или ткани).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "washing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта льняная скатерть отлично <w>стирается</w> даже в холодной воде.",
                            "target_lang_translations": {
                                "en": "This linen tablecloth <w>washes</w> perfectly even in cold water."
                            }
                        },
                        {
                            "text": "Пятна от чернил плохо <w>стираются</w> с белой рубашки.",
                            "target_lang_translations": {
                                "en": "Ink stains do not <w>wash out</w> well from a white shirt."
                            }
                        }
                    ],
                    "common_phrases": [
                        "хорошо стираться",
                        "легко стираться",
                        "плохо стираться"
                    ],
                    "target_lang_definitions": {
                        "en": "To wash well or poorly, to become clean during the washing process (about clothes or fabric)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wash",
                                "target_lang_sense_clarification": "to withstand washing or become clean"
                            },
                            {
                                "target_lang_word": "clean",
                                "target_lang_sense_clarification": "to become free of dirt"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "154776dd-49c5-3389-9605-1212f6049315",
                    "sense_definition": "Исчезать, удаляться или портиться от трения или длительного использования.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "disappearance_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Подошва на моих старых ботинках почти полностью <w>стёрлась</w>.",
                            "target_lang_translations": {
                                "en": "The sole on my old boots has almost completely <w>worn out</w>."
                            }
                        },
                        {
                            "text": "Надпись на монете со временем <w>стирается</w> и становится нечитаемой.",
                            "target_lang_translations": {
                                "en": "The inscription on the coin <w>wears off</w> over time and becomes unreadable."
                            }
                        }
                    ],
                    "synonyms": [
                        "вытираться",
                        "истираться"
                    ],
                    "common_phrases": [
                        "стираться до дыр",
                        "краска стирается"
                    ],
                    "target_lang_definitions": {
                        "en": "To disappear, be removed, or deteriorate from friction or prolonged use."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wear off",
                                "target_lang_sense_clarification": "to disappear gradually from a surface"
                            },
                            {
                                "target_lang_word": "wear out",
                                "target_lang_sense_clarification": "to become unusable through use"
                            },
                            {
                                "target_lang_word": "rub off",
                                "target_lang_sense_clarification": "to be removed by friction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4693b228-67d5-36eb-a79d-3541cea2067b",
                    "sense_definition": "Становиться менее заметным, забываться или полностью исчезать (о воспоминаниях, чувствах, границах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disappearance_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "С годами плохие воспоминания <w>стираются</w> из памяти.",
                            "target_lang_translations": {
                                "en": "Over the years, bad memories <w>fade</w> from memory."
                            }
                        },
                        {
                            "text": "Границы между городом и деревней постепенно <w>стираются</w>.",
                            "target_lang_translations": {
                                "en": "The boundaries between the city and the countryside are gradually <w>blurring</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "стираться из памяти",
                        "границы стираются",
                        "различия стираются"
                    ],
                    "target_lang_definitions": {
                        "en": "To become less noticeable, be forgotten, or disappear completely (about memories, feelings, boundaries)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fade",
                                "target_lang_sense_clarification": "to lose strength or distinctness"
                            },
                            {
                                "target_lang_word": "blur",
                                "target_lang_sense_clarification": "to become indistinct (boundaries, differences)"
                            },
                            {
                                "target_lang_word": "disappear",
                                "target_lang_sense_clarification": "to cease to be visible or perceived"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4c2fc5a-7203-3596-b984-0dbb38517405",
                    "sense_definition": "Заниматься стиркой своего белья или одежды.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "washing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обычно <w>стираюсь</w> по выходным, когда у меня больше свободного времени.",
                            "target_lang_translations": {
                                "en": "I usually <w>do laundry</w> on weekends when I have more free time."
                            }
                        },
                        {
                            "text": "Студенты в общежитии часто <w>стираются</w> в общей прачечной.",
                            "target_lang_translations": {
                                "en": "Students in the dorm often <w>do their laundry</w> in the shared laundry room."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To wash one's own linen or clothes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "do laundry",
                                "target_lang_sense_clarification": "to wash clothes"
                            },
                            {
                                "target_lang_word": "do one's washing",
                                "target_lang_sense_clarification": "British English variation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "65fafc1f-40c4-3460-8a57-afd8d05ae2be",
                    "sense_definition": "Grzyb kapeluszowy z rodzaju Paxillus, zazwyczaj trujący lub niejadalny, występujący w lasach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "grzyby",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie znaleźliśmy trującego <w>krowiaka</w> podwiniętego.",
                            "target_lang_translations": {
                                "ru": "В лесу мы нашли ядовитую <w>свинушку</w> тонкую."
                            }
                        },
                        {
                            "text": "Atlas grzybów ostrzega przed zbieraniem <w>krowiaków</w>.",
                            "target_lang_translations": {
                                "ru": "Атлас грибов предостерегает от сбора <w>свинушек</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "krowiak podwinięty",
                        "krowiak aksamitny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Шляпочный гриб рода Paxillus, обычно ядовитый или несъедобный, встречающийся в лесах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свинушка",
                                "target_lang_sense_clarification": "гриб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b89e85b-6d7f-3c9a-83df-89415624a8b7",
                    "sense_definition": "Odchody krowie, często wykorzystywane w rolnictwie jako naturalny nawóz.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nawóz",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik rozrzucił <w>krowiak</w> na polu, aby użyźnić glebę.",
                            "target_lang_translations": {
                                "ru": "Фермер разбросал <w>коровий навоз</w> на поле, чтобы удобрить почву."
                            }
                        },
                        {
                            "text": "Wysuszony <w>krowiak</w> był dawniej używany jako opał.",
                            "target_lang_translations": {
                                "ru": "Высушенный <w>кизяк</w> раньше использовался как топливо."
                            }
                        }
                    ],
                    "synonyms": [
                        "krowieniec"
                    ],
                    "common_phrases": [
                        "suszony krowiak",
                        "świeży krowiak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коровий помет, часто используемый в сельском хозяйстве как натуральное удобрение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коровий навоз",
                                "target_lang_sense_clarification": "удобрение"
                            },
                            {
                                "target_lang_word": "кизяк",
                                "target_lang_sense_clarification": "сушеный, топливо"
                            },
                            {
                                "target_lang_word": "коровяк",
                                "target_lang_sense_clarification": "жидкое удобрение"
                            }
                        ]
                    }
                }
            ]
        },
        {

                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cause deep grief, to sadden greatly, or to distress."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grieve",
                                "target_lang_sense_clarification": "to cause great sorrow to someone"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "used in the phrase 'break one's heart'"
                            },
                            {
                                "target_lang_word": "distress",
                                "target_lang_sense_clarification": "to cause emotional pain"
                            }
                        ]
                    }
                }
            ]
        }
    ]

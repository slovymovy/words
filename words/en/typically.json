                },
                {
                    "sense_id": "9ac8294e-e18a-3565-ba2a-ccf66de6e46c",
                    "sense_definition": "In a way that shows the usual qualities or features of a particular person or thing.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He answered the difficult question with a <w>typically</w> witty remark.",
                            "target_lang_translations": {
                                "ru": "Он ответил на сложный вопрос <w>типично</w> остроумным замечанием."
                            }
                        },
                        {
                            "text": "The weather was <w>typically</w> unpredictable for this time of year.",
                            "target_lang_translations": {
                                "ru": "Погода была <w>типично</w> непредсказуемой для этого времени года."
                            }
                        }
                    ],
                    "antonyms": [
                        "atypically"
                    ],
                    "common_phrases": [

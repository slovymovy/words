            "senses": [
                {
                    "sense_id": "a80f6f89-a424-3e01-8588-e855f6cf4f3e",
                    "sense_definition": "Een lange, smalle ruimte in een gebouw die verschillende kamers met elkaar verbindt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen rennen door de lange <w>gang</w> naar buiten.",
                            "target_lang_translations": {
                                "en": "The children run through the long <w>hallway</w> to the outside.",
                                "ru": "Дети бегут по длинному <w>коридору</w> на улицу."
                            }
                        },
                        {
                            "text": "Aan het einde van de <w>gang</w> is de deur naar de keuken.",
                            "target_lang_translations": {
                                "en": "At the end of the <w>hallway</w> is the door to the kitchen.",
                                "ru": "В конце <w>коридора</w> находится дверь на кухню."
                            }
                        }
                    ],
                    "synonyms": [
                        "galerij",
                        "tunnel"
                    ],
                    "common_phrases": [
                        "in de gang",
                        "op de gang",
                        "lange gang"
                    ],
                    "target_lang_definitions": {
                        "en": "A long, narrow passage in a building that connects different rooms.",
                        "ru": "Длинное узкое пространство в здании, соединяющее разные комнаты."
                    },
                    "translations": {
                        "en": [

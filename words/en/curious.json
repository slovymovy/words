                {
                    "sense_id": "877dac70-cb47-3113-9494-f228a368084b",
                    "sense_definition": "Eager to learn or know something; tending to ask questions and investigate.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inquisitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Children are naturally <w>curious</w> about the world around them.",
                            "target_lang_translations": {
                                "ru": "Дети от природы <w>любознательны</w> по отношению к окружающему миру."
                            }
                        },
                        {
                            "text": "I was just <w>curious</w> to know why he decided to leave so early.",
                            "target_lang_translations": {
                                "ru": "Мне было просто <w>любопытно</w> узнать, почему он решил уйти так рано."
                            }
                        }
                    ],
                    "synonyms": [
                        "inquisitive",
                        "inquiring",
                        "investigative"
                    ],
                    "antonyms": [
                        "incurious",
                        "uncurious"
                    ],
                    "common_phrases": [
                        "curious about",
                        "just curious",
                        "naturally curious"
                    ],
                    "target_lang_definitions": {
                        "ru": "Желающий узнать или научиться чему-то; склонный задавать вопросы и исследовать."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3e093ef7-7c7e-35ec-82d1-70559de94423",
                    "sense_definition": "Добиться принятия решения или желаемого результата с помощью давления, уговоров или влияния.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор смог <продавить> этот проект, несмотря на возражения коллег.",
                            "target_lang_translations": {
                                "en": "The director managed to <w>push through</w> this project despite his colleagues' objections."
                            }
                        },
                        {
                            "text": "Ему удалось <продавить> свою идею на совете директоров.",
                            "target_lang_translations": {
                                "en": "He managed to <w>force through</w> his idea at the board of directors."
                            }
                        }
                    ],
                    "synonyms": [
                        "протаранить"
                    ],
                    "common_phrases": [
                        "продавить решение",
                        "продавить закон",
                        "продавить инициативу",
                        "продавить свою позицию"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To succeed in getting a decision, law, or idea accepted, often by using pressure or influence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "push through",
                                "target_lang_sense_clarification": "a decision or project"
                            },
                            {
                                "target_lang_word": "force through",
                                "target_lang_sense_clarification": "against resistance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2311e4b4-65da-3fcc-a6fb-bcf58043e15d",
                    "sense_definition": "Надавливая, сделать углубление, вмятину или прогнуть внутрь.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_deformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тяжёлый шкаф <продавил> линолеум в коридоре.",
                            "target_lang_translations": {
                                "en": "The heavy wardrobe <w>dented</w> the linoleum in the hallway."
                            }
                        },
                        {
                            "text": "Он случайно сел на картонную коробку и <продавил> её крышку.",
                            "target_lang_translations": {
                                "en": "He accidentally sat on the cardboard box and <w>caved in</w> its lid."
                            }
                        }
                    ],
                    "synonyms": [
                        "вдавить",
                        "надавить"
                    ],
                    "antonyms": [
                        "выпрямить"
                    ],
                    "common_phrases": [
                        "продавить диван",
                        "продавить пол",
                        "продавить кресло"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a hollow or dent in a surface by pressing on it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dent",
                                "target_lang_sense_clarification": "make a depression in a surface"
                            },
                            {
                                "target_lang_word": "cave in",
                                "target_lang_sense_clarification": "cause to collapse inward"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a68279d-be76-3304-a5b0-aeaaa99da5fc",
                    "sense_definition": "Надавливая с силой, проломить или сделать сквозное отверстие.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_deformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Упавшая с дерева ветка <продавила> крышу старого сарая.",
                            "target_lang_translations": {
                                "en": "A branch falling from the tree <w>broke through</w> the roof of the old shed."
                            }
                        },
                        {
                            "text": "Она так сильно нажимала на карандаш, что <продавила> бумагу.",
                            "target_lang_translations": {
                                "en": "She pressed so hard on the pencil that she <w>punched through</w> the paper."
                            }
                        }
                    ],
                    "synonyms": [
                        "проломить",
                        "просадить"
                    ],
                    "common_phrases": [
                        "продавить стекло",
                        "продавить лёд",
                        "продавить насквозь"
                    ],
                    "target_lang_definitions": {
                        "en": "To break or make a hole through a material by pressing with force."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break through",
                                "target_lang_sense_clarification": "a surface or barrier"
                            },
                            {
                                "target_lang_word": "punch through",
                                "target_lang_sense_clarification": "paper or thin material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92ec399b-68fe-3da6-af07-b2b83ebfee1c",
                    "sense_definition": "С усилием пропустить или протолкнуть что-либо сквозь узкое отверстие или преграду.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Повар решил <продавить> варёные овощи через сито для пюре.",
                            "target_lang_translations": {
                                "en": "The cook decided to <w>press</w> the boiled vegetables through a puree sieve."
                            }
                        },
                        {
                            "text": "Мощный насос должен <продавить> воду через сложную систему труб.",
                            "target_lang_translations": {
                                "en": "A powerful pump must <w>force</w> water through the complex system of pipes."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдавить"
                    ],
                    "common_phrases": [
                        "продавить через сито",
                        "продавить в отверстие"
                    ],
                    "target_lang_definitions": {
                        "en": "To force a substance or object through a narrow opening or barrier."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "force",
                                "target_lang_sense_clarification": "through a barrier or pipe"
                            },
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "through a sieve or filter"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
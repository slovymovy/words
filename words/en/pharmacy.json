                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фармацевтика",
                                "target_lang_sense_clarification": "профессиональная деятельность"
                            },
                            {
                                "target_lang_word": "фармация",
                                "target_lang_sense_clarification": "сфера деятельности"

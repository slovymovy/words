    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f3fd044-f143-32a7-b933-3c14fe202147",
                    "sense_definition": "Przestrzeń fizyczna rozciągająca się poza atmosferą Ziemi, w której znajdują się ciała niebieskie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pierwszy lot w <w>kosmos</w> był przełomowym momentem w historii ludzkości.",
                            "target_lang_translations": {
                                "ru": "Первый полет в <w>космос</w> стал переломным моментом в истории человечества."
                            }
                        },
                        {
                            "text": "Ziemia widziana z <w>kosmosu</w> wygląda jak błękitna kula.",
                            "target_lang_translations": {
                                "ru": "Земля из <w>космоса</w> выглядит как голубой шар."
                            }
                        }
                    ],
                    "synonyms": [
                        "przestrzeń kosmiczna"
                    ],
                    "common_phrases": [
                        "lot w kosmos",
                        "podbój kosmosu",
                        "z kosmosu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое пространство, простирающееся за пределами атмосферы Земли, в котором находятся небесные тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "космос",
                                "target_lang_sense_clarification": "пространство вне Земли"
                            },
                            {
                                "target_lang_word": "космическое пространство",
                                "target_lang_sense_clarification": "научный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06e2b913-8810-3cff-943c-e58913b608bf",
                    "sense_definition": "Cały świat fizyczny, czasoprzestrzeń oraz wszelka materia i energia; wszechświat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naukowcy wciąż próbują odkryć, jak powstał <w>kosmos</w>.",
                            "target_lang_translations": {
                                "ru": "Ученые все еще пытаются выяснить, как возникла <w>вселенная</w>."
                            }
                        },
                        {
                            "text": "W całym <w>kosmosie</w> istnieje niezliczona ilość galaktyk.",
                            "target_lang_translations": {
                                "ru": "Во всей <w>вселенной</w> существует бесчисленное количество галактик."
                            }
                        }
                    ],
                    "synonyms": [
                        "wszechświat",
                        "uniwersum"
                    ],
                    "common_phrases": [
                        "tajemnice kosmosu",
                        "bezkres kosmosu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Весь физический мир, пространство-время, а также вся материя и энергия; вселенная."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вселенная",
                                "target_lang_sense_clarification": "весь мир"
                            },
                            {
                                "target_lang_word": "космос",
                                "target_lang_sense_clarification": "мироздание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f403ed60-cf89-3721-940f-ea603b96d4c4",
                    "sense_definition": "Coś niesamowitego, absurdalnego, trudnego do zrozumienia lub bardzo zaskakującego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ceny w tej restauracji są z <w>kosmosu</w>, nikogo na to nie stać.",
                            "target_lang_translations": {
                                "ru": "Цены в этом ресторане — просто <w>космос</w>, никому не по карману."
                            }
                        },
                        {
                            "text": "To jest dla mnie totalny <w>kosmos</w>, że on zdał ten egzamin bez nauki.",
                            "target_lang_translations": {
                                "ru": "Для меня это полный <w>космос</w>, что он сдал этот экзамен без подготовки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ceny z kosmosu",
                        "to jest kosmos",
                        "totalny kosmos"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то невероятное, абсурдное, трудное для понимания или очень удивительное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "космос",
                                "target_lang_sense_clarification": "что-то невероятное (разг.)"
                            },
                            {
                                "target_lang_word": "фантастика",
                                "target_lang_sense_clarification": "что-то удивительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "568efa79-3e6c-3e94-af0b-adb8e6a8ff8f",
                    "sense_definition": "Roślina ozdobna z rodziny astrowatych o delikatnych, pierzastych liściach i kolorowych kwiatach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wiejskim ogrodzie mojej babci zawsze kwitł różowy <w>kosmos</w>.",
                            "target_lang_translations": {
                                "ru": "В деревенском саду моей бабушки всегда цвела розовая <w>космея</w>."
                            }
                        },
                        {
                            "text": "Wysialiśmy <w>kosmosy</w> wzdłuż płotu, żeby latem było kolorowo.",
                            "target_lang_translations": {
                                "ru": "Мы посеяли <w>космеи</w> вдоль забора, чтобы летом было красочно."
                            }
                        }
                    ],
                    "synonyms": [
                        "onętek",
                        "warszawianka",
                        "ponętka",
                        "kosmos podwójnie pierzasty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративное растение семейства астровых с нежными перистыми листьями и разноцветными цветами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "космея",
                                "target_lang_sense_clarification": "цветок"
                            }
                        ]
                    }
                }
            ]
        }
    ]

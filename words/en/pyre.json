{
    "word_family": [],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3623a6ac-4e23-39b1-a84b-9be6e3000a5d",
                    "sense_definition": "A pile of wood or other material used for burning a dead body during a funeral ceremony.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fire_pile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers built a funeral <w>pyre</w> to honor their dead.",
                            "target_lang_translations": {
                                "ru": "Жители деревни соорудили <w>погребальный костёр</w>, чтобы почтить память умерших."
                            }
                        },
                        {
                            "text": "They placed the warrior's body on the <w>pyre</w> before lighting the fire.",
                            "target_lang_translations": {
                                "ru": "Они положили тело воина на <w>костёр</w>, прежде чем зажечь огонь."
                            }
                        }
                    ],
                    "synonyms": [
                        "funeral pile"
                    ],
                    "common_phrases": [
                        "funeral pyre",
                        "build a pyre"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сооружение из дров или другого горючего материала для сжигания тела умершего во время похоронного обряда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погребальный костёр",
                                "target_lang_sense_clarification": "ритуальный костёр для кремации"
                            },
                            {
                                "target_lang_word": "костёр",
                                "target_lang_sense_clarification": "в контексте похорон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3000cafc-3601-3016-a851-c4add0f38026",
                    "sense_definition": "A large pile of combustible material, especially one that is burning or intended to be burned.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fire_pile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Flames rose high from the <w>pyre</w> in the night sky.",
                            "target_lang_translations": {
                                "ru": "Пламя высоко поднималось от <w>костра</w> в ночное небо."
                            }
                        },
                        {
                            "text": "The huge <w>pyre</w> of dry branches burned for several hours.",
                            "target_lang_translations": {
                                "ru": "Огромный <w>костёр</w> из сухих веток горел несколько часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "bonfire",
                        "heap"
                    ],
                    "common_phrases": [
                        "blazing pyre"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая куча горючих материалов, сложенная для сжигания, или большой огонь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "костёр",
                                "target_lang_sense_clarification": "большой огонь"
                            },
                            {
                                "target_lang_word": "груда",
                                "target_lang_sense_clarification": "материала для сжигания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
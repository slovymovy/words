                            {
                                "target_lang_word": "выдворять",
                                "target_lang_sense_clarification": "из страны"
                            },
                            {
                                "target_lang_word": "депортировать",
                                "target_lang_sense_clarification": "официально"
                            }
                        ]
                    }
                },
                {

{
    "word_family": [
        "vergeetachtig",
        "vergetelheid",
        "vergeetput"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "123e2106-eb2c-3c2b-96e7-82fc8a3f5ac1",
                    "sense_definition": "Niet meer weten; uit het geheugen verdwenen zijn.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "memory_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben helemaal <w>vergeten</w> hoe hij heet.",
                            "target_lang_translations": {
                                "en": "I have completely <w>forgotten</w> what his name is.",
                                "ru": "Я совершенно <w>забыл</w>, как его зовут."
                            }
                        },
                        {
                            "text": "We moeten niet <w>vergeten</w> dat vrijheid heel belangrijk is.",
                            "target_lang_translations": {
                                "en": "We must not <w>forget</w> that freedom is very important.",
                                "ru": "Мы не должны <w>забывать</w>, что свобода очень важна."
                            }
                        }
                    ],
                    "antonyms": [
                        "onthouden",
                        "herinneren"
                    ],
                    "common_phrases": [
                        "niet vergeten",
                        "snel vergeten",
                        "vergeten en vergeven"
                    ],
                    "target_lang_definitions": {
                        "en": "To no longer know or remember something; to have disappeared from memory.",
                        "ru": "Перестать помнить; исчезнуть из памяти."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forget",
                                "target_lang_sense_clarification": "to lose remembrance of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "перестать помнить информацию"
                            },
                            {
                                "target_lang_word": "забывать",
                                "target_lang_sense_clarification": "процесс утраты памяти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fe08479-227d-3fc3-b217-a73907e43d88",
                    "sense_definition": "Nalaten iets te doen omdat men er niet aan gedacht heeft.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "omission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was <w>vergeten</w> om melk te halen bij de supermarkt.",
                            "target_lang_translations": {
                                "en": "He had <w>forgotten</w> to get milk at the supermarket.",
                                "ru": "Он <w>забыл</w> купить молоко в супермаркете."
                            }
                        },
                        {
                            "text": "Ik <w>vergat</w> gisteren de deur op slot te doen.",
                            "target_lang_translations": {
                                "en": "I <w>forgot</w> to lock the door yesterday.",
                                "ru": "Вчера я <w>забыл</w> запереть дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "verzuimen",
                        "nalaten"
                    ],
                    "antonyms": [
                        "denken aan"
                    ],
                    "common_phrases": [
                        "vergeten te doen",
                        "helemaal vergeten"
                    ],
                    "target_lang_definitions": {
                        "en": "To fail to do something because one did not think of it.",
                        "ru": "Не сделать что-то из-за того, что не подумал об этом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forget",
                                "target_lang_sense_clarification": "to neglect to do something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "не сделать что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f105533-a428-3e49-a6c4-c55e1657406b",
                    "sense_definition": "Per ongeluk ergens achterlaten en niet meenemen.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "leaving_behind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb mijn sleutels thuis op tafel <w>vergeten</w>.",
                            "target_lang_translations": {
                                "en": "I <w>left</w> my keys on the table at home.",
                                "ru": "Я <w>забыл</w> свои ключи дома на столе."
                            }
                        },
                        {
                            "text": "Ben je je tas niet <w>vergeten</w> in de trein?",
                            "target_lang_translations": {
                                "en": "Didn't you <w>leave</w> your bag on the train?",
                                "ru": "Ты не <w>забыл</w> свою сумку в поезде?"
                            }
                        }
                    ],
                    "synonyms": [
                        "achterlaten",
                        "laten liggen"
                    ],
                    "antonyms": [
                        "meenemen"
                    ],
                    "common_phrases": [
                        "thuis vergeten",
                        "ergens vergeten"
                    ],
                    "target_lang_definitions": {
                        "en": "To accidentally leave something behind and not take it with you.",
                        "ru": "Случайно оставить где-то и не взять с собой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "to leave behind (often used with a location)"
                            },
                            {
                                "target_lang_word": "forget",
                                "target_lang_sense_clarification": "to unintentionally leave behind"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "забыть",
                                "target_lang_sense_clarification": "оставить вещь"
                            },
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "не взять с собой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b7b8f476-4ee4-3363-a4db-b7440b29004d",
                    "sense_definition": "Niet meer bekend; waar niet meer aan gedacht wordt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "memory_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pastinaak is een bekend voorbeeld van de zogenoemde <w>vergeten</w> groenten.",
                            "target_lang_translations": {
                                "en": "Parsnip is a well-known example of the so-called <w>forgotten</w> vegetables.",
                                "ru": "Пастернак — известный пример так называемых <w>забытых</w> овощей."
                            }
                        },
                        {
                            "text": "Hij vond een <w>vergeten</w> brief in de oude lade van het bureau.",
                            "target_lang_translations": {
                                "en": "He found a <w>forgotten</w> letter in the old desk drawer.",
                                "ru": "Он нашел <w>забытое</w> письмо в старом ящике письменного стола."
                            }
                        }
                    ],
                    "synonyms": [
                        "onbekend"
                    ],
                    "antonyms": [
                        "bekend",
                        "beroemd"
                    ],
                    "common_phrases": [
                        "vergeten groenten",
                        "een vergeten schrijver"
                    ],
                    "target_lang_definitions": {
                        "en": "No longer known; no longer thought about.",
                        "ru": "Больше не известный; о котором больше не думают."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forgotten",
                                "target_lang_sense_clarification": "no longer remembered"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "забытый",
                                "target_lang_sense_clarification": "утраченный, старый"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
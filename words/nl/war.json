    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c066564-67af-304d-8c36-0129755eb0c1",
                    "sense_definition": "Een toestand van verwarring of chaos, vrijwel altijd gebruikt in de vaste verbinding 'in de war'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "confusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sorry, ik ben even helemaal in de <w>war</w> door jouw moeilijke vraag.",
                            "target_lang_translations": {
                                "ru": "Извини, я совершенно <w>запутался</w> из-за твоего сложного вопроса."
                            }
                        },
                        {
                            "text": "De vele wijzigingen in het rooster brachten de studenten behoorlijk in de <w>war</w>.",
                            "target_lang_translations": {
                                "ru": "Множество изменений в расписании сильно <w>сбили с толку</w> студентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwarring",
                        "wanorde"
                    ],
                    "common_phrases": [
                        "in de war",
                        "in de war zijn",
                        "in de war brengen",
                        "in de war raken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние замешательства или хаоса, почти всегда используется в устойчивом выражении 'in de war'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путаница",
                                "target_lang_sense_clarification": "беспорядок, неразбериха"
                            },
                            {
                                "target_lang_word": "замешательство",
                                "target_lang_sense_clarification": "психологическое состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8eb526b8-ccbe-3dd2-a1e7-8d757691a2a0",
                    "sense_definition": "Een kluwen van door elkaar geraakte draden, haren of andere lijnen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tangle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het duurt altijd lang om mijn oortjes uit de <w>war</w> te halen.",
                            "target_lang_translations": {
                                "ru": "У меня всегда уходит много времени на то, чтобы <w>распутать</w> наушники."
                            }
                        },
                        {
                            "text": "Na het fietsen in de harde wind zat haar haar flink in de <w>war</w>.",
                            "target_lang_translations": {
                                "ru": "После езды на велосипеде при сильном ветре её волосы сильно <w>растрепались</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uit de war halen",
                        "in de war zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спутанный комок ниток, волос или других линий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путаница",
                                "target_lang_sense_clarification": "спутанные нитки или провода"
                            },
                            {
                                "target_lang_word": "колтун",
                                "target_lang_sense_clarification": "сбившийся ком волос или шерсти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56315aed-a542-31fa-a10d-1298b75ae532",
                    "sense_definition": "Een harde, donkere plek in hout waar een tak heeft gezeten.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "wood_defect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De timmerman moest voorzichtig om de harde <w>war</w> heen zagen.",
                            "target_lang_translations": {
                                "ru": "Плотнику пришлось осторожно пилить вокруг твердого <w>сучка</w>."
                            }
                        },
                        {
                            "text": "Dit stuk hout is onbruikbaar voor de kast omdat er een grote <w>war</w> in zit.",
                            "target_lang_translations": {
                                "ru": "Этот кусок дерева непригоден для шкафа, потому что в нем есть большой <w>сучок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwarring",
                        "wanorde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердое темное место в древесине, где раньше росла ветка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сучок",
                                "target_lang_sense_clarification": "дефект древесины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2fe2dab-c783-3660-a10a-7b9a68726d61",
                    "sense_definition": "Een verhoging op de waterbodem of een constructie die vroeger werd gebruikt om vis te vangen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fishing_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op oude kaarten van de rivier staat nog een <w>war</w> aangegeven.",
                            "target_lang_translations": {
                                "ru": "На старых картах реки всё ещё обозначена <w>запруда</w>."
                            }
                        },
                        {
                            "text": "De vissers bouwden vroeger een <w>war</w> om de paling te kunnen vangen.",
                            "target_lang_translations": {
                                "ru": "Раньше рыбаки строили <w>ловушку</w>, чтобы ловить угрей."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical fishing term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвышение на дне водоема или конструкция, которая раньше использовалась для ловли рыбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запруда",
                                "target_lang_sense_clarification": "сооружение на воде"
                            },
                            {
                                "target_lang_word": "рыболовная ловушка",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                }
            ]
        },

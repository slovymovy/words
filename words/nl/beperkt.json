{
    "word_family": [
        "beperken",
        "beperking"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5faa96a5-ed93-3c76-857b-e01ecdf75dfb",
                    "sense_definition": "Gebonden aan grenzen; niet groot, ruim of volledig.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het apparaat is goed inzetbaar in <w>beperkte</w> ruimtes.",
                            "target_lang_translations": {
                                "en": "The device is highly usable in <w>confined</w> spaces.",
                                "ru": "Устройство хорошо подходит для использования в <w>ограниченных</w> пространствах."
                            }
                        },
                        {
                            "text": "Er is slechts een <w>beperkt</w> aantal kaarten beschikbaar voor het concert.",
                            "target_lang_translations": {
                                "en": "There is only a <w>limited</w> number of tickets available for the concert.",
                                "ru": "Для концерта доступно лишь <w>ограниченное</w> количество билетов."
                            }
                        }
                    ],
                    "synonyms": [
                        "begrensd",
                        "beknopt",
                        "eindig"
                    ],
                    "antonyms": [
                        "onbeperkt",
                        "uitgebreid",
                        "uitvoerig"
                    ],
                    "common_phrases": [
                        "beperkt aantal",
                        "beperkte toegang",
                        "in beperkte mate",
                        "beperkt houdbaar"
                    ],
                    "target_lang_definitions": {
                        "en": "Bound by limits; not large, spacious, or complete.",
                        "ru": "Имеющий границы; небольшой, не просторный или не полный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "limited",
                                "target_lang_sense_clarification": "small in amount, number, or range"
                            },
                            {
                                "target_lang_word": "restricted",
                                "target_lang_sense_clarification": "kept within certain limits or rules"
                            },
                            {
                                "target_lang_word": "confined",
                                "target_lang_sense_clarification": "limited in space or area"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ограниченный",
                                "target_lang_sense_clarification": "имеющий пределы или рамки"
                            },
                            {
                                "target_lang_word": "стеснённый",
                                "target_lang_sense_clarification": "ограниченный в пространстве или средствах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "69a60d87-0976-36f8-a4a8-c7d601645e87",
                    "sense_definition": "Vervoeging van <w>beperken</w>: tweede en derde persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>beperkt</w> zijn uitgaven om geld te besparen.",
                            "target_lang_translations": {
                                "en": "He <w>limits</w> his expenses to save money.",
                                "ru": "Он <w>ограничивает</w> свои расходы, чтобы сэкономить деньги."
                            }
                        },
                        {
                            "text": "De overlast <w>beperkt</w> zich gelukkig tot de directe omgeving.",
                            "target_lang_translations": {
                                "en": "Fortunately, the nuisance <w>is limited</w> to the immediate vicinity.",
                                "ru": "К счастью, неудобство <w>ограничивается</w> непосредственной близостью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beperkt zich tot"
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugation of <w>limit</w>: second and third person singular present tense.",
                        "ru": "Форма глагола beperken: 2-е и 3-е лицо единственного числа настоящего времени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "limits",
                                "target_lang_sense_clarification": "sets a boundary or maximum"
                            },
                            {
                                "target_lang_word": "restricts",
                                "target_lang_sense_clarification": "keeps under control or within limits"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ограничивает",
                                "target_lang_sense_clarification": "устанавливает предел или границу"
                            },
                            {
                                "target_lang_word": "сдерживает",
                                "target_lang_sense_clarification": "держит в рамках, не дает распространиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95f8ba8f-5ceb-39af-a5f3-db39366abbe8",
                    "sense_definition": "Voltooid deelwoord van <w>beperken</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brandweer heeft de schade aan het gebouw <w>beperkt</w>.",
                            "target_lang_translations": {
                                "en": "The fire department has <w>limited</w> the damage to the building.",
                                "ru": "Пожарные <w>ограничили</w> ущерб зданию."
                            }
                        },
                        {
                            "text": "We hebben de risico's zo veel mogelijk <w>beperkt</w>.",
                            "target_lang_translations": {
                                "en": "We have <w>limited</w> the risks as much as possible.",
                                "ru": "Мы максимально <w>ограничили</w> риски."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past participle of <w>limit</w>.",
                        "ru": "Причастие прошедшего времени от глагола beperken."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "limited",
                                "target_lang_sense_clarification": "reduced or kept within bounds"
                            },
                            {
                                "target_lang_word": "restricted",
                                "target_lang_sense_clarification": "placed limits on"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ограничил",
                                "target_lang_sense_clarification": "совершил действие по установлению границ (прошедшее время)"
                            },
                            {
                                "target_lang_word": "сократил",
                                "target_lang_sense_clarification": "уменьшил количество или объем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a090e24-437f-31a5-a874-f0d51cc4f76f",
                    "sense_definition": "Vervoeging van <w>beperken</w>: gebiedende wijs meervoud.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Beperkt</w> uw snelheid in de bebouwde kom.",
                            "target_lang_translations": {
                                "en": "<w>Limit</w> your speed in built-up areas.",
                                "ru": "<w>Ограничьте</w> скорость в населенных пунктах."
                            }
                        },
                        {
                            "text": "<w>Beperkt</w> u tot de essentie van het verhaal.",
                            "target_lang_translations": {
                                "en": "<w>Limit</w> yourself to the essence of the story.",
                                "ru": "<w>Ограничьтесь</w> сутью рассказа."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",

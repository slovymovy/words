                    "frequency": "High",
                    "semantic_group_id": "state_of_calm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Оставьте меня в <w>покое</w>, я хочу побыть один.",
                            "target_lang_translations": {
                                "en": "Leave me in <w>peace</w>, I want to be alone."
                            }
                        },
                        {
                            "text": "В лесу царил удивительный <w>покой</w> и тишина.",
                            "target_lang_translations": {
                                "en": "Amazing <w>peace</w> and silence reigned in the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "спокойствие",
                        "тишина",
                        "безмятежность",
                        "умиротворённость",
                        "бесстрастность"

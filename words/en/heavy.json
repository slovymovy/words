    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2af4ece8-c558-3ca2-bb19-5749d0237132",
                    "sense_definition": "Having great weight; difficult to lift or move.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_weight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you help me carry this box because it is too <w>heavy</w>?",
                            "target_lang_translations": {
                                "ru": "Ты можешь помочь мне нести эту коробку, потому что она слишком <w>тяжёлая</w>?"
                            }
                        },
                        {
                            "text": "The suitcase was so <w>heavy</w> that I had to drag it across the floor.",
                            "target_lang_translations": {
                                "ru": "Чемодан был таким <w>тяжёлым</w>, что мне пришлось тащить его по полу."
                            }
                        }
                    ],
                    "antonyms": [
                        "light"
                    ],
                    "common_phrases": [
                        "heavy load",
                        "heavy object",
                        "heavy lifting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большой вес; такой, который трудно поднять или сдвинуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тяжёлый",
                                "target_lang_sense_clarification": "общий термин для большого веса"
                            },
                            {
                                "target_lang_word": "увесистый",
                                "target_lang_sense_clarification": "значительный по весу, ощутимый"
                            },
                            {
                                "target_lang_word": "неподъёмный",
                                "target_lang_sense_clarification": "слишком тяжёлый, чтобы поднять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf23c1bb-a0d5-3c53-9982-abcf7bcd30d3",
                    "sense_definition": "Great in amount, degree, or intensity.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We arrived late to the meeting due to the <w>heavy</w> traffic.",
                            "target_lang_translations": {
                                "ru": "Мы опоздали на встречу из-за <w>плотного</w> движения."
                            }
                        },
                        {
                            "text": "There was <w>heavy</w> rain throughout the night, causing some flooding.",
                            "target_lang_translations": {
                                "ru": "Всю ночь шёл <w>сильный</w> дождь, вызвавший наводнение."
                            }
                        }
                    ],
                    "synonyms": [
                        "swear"
                    ],
                    "antonyms": [
                        "light"
                    ],
                    "common_phrases": [
                        "heavy rain",
                        "heavy traffic",
                        "heavy snow",
                        "heavy bleeding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой по количеству, степени или интенсивности."

                    "sense_id": "1f35eb64-70ad-3c19-8828-3693c3a92158",
                    "sense_definition": "Iemand die een beroep of activiteit zeer vakkundig uitoefent.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "professional_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft die lastige kwestie als een echte <w>pro</w> opgelost.",
                            "target_lang_translations": {
                                "ru": "Она решила этот сложный вопрос как настоящий <w>профи</w>."
                            }
                        },
                        {
                            "text": "Als je wilt leren golfen, kun je het beste les nemen van een <w>pro</w>.",
                            "target_lang_translations": {
                                "ru": "Если хочешь научиться играть в гольф, лучше всего брать уроки у <w>профи</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "professional"
                    ],
                    "common_phrases": [
                        "een echte pro"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "shortening of professional"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто занимается какой-либо деятельностью профессионально и очень качественно."

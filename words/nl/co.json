                {
                    "sense_id": "e52c55d1-0b37-3382-ab42-f3d8c05af6a0",
                    "sense_definition": "Een verkleinvorm van de vrouwelijke voornaam <w>Jacoba</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Tante <w>Co</w> komt morgen bij ons op de koffie.",
                            "target_lang_translations": {
                                "ru": "Тетя <w>Ко</w> завтра придет к нам на кофе."
                            }
                        },
                        {
                            "text": "Zij wordt door iedereen <w>Co</w> genoemd.",
                            "target_lang_translations": {

                    "target_lang_definitions": {
                        "ru": "Вмешиваться или лезть к кому-либо или чему-либо, часто вызывая неприятности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связываться",
                                "target_lang_sense_clarification": "иметь дело с кем-то опасным или неприятным"
                            },
                            {
                                "target_lang_word": "лезть",
                                "target_lang_sense_clarification": "вмешиваться не в свое дело"
                            },
                            {

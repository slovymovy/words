    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "df4809fb-c0c6-33c4-ae9d-0bef2212b300",
                    "sense_definition": "The height of an object or place above sea level or the ground.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "height_measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The plane reached a cruising <w>altitude</w> of thirty thousand feet shortly after takeoff.",
                            "target_lang_translations": {
                                "ru": "Вскоре после взлета самолет набрал крейсерскую <w>высоту</w> в тридцать тысяч футов."
                            }
                        },
                        {
                            "text": "Many athletes train at high <w>altitude</w> to improve their physical endurance.",
                            "target_lang_translations": {
                                "ru": "Многие спортсмены тренируются на большой <w>высоте</w>, чтобы улучшить свою физическую выносливость."
                            }
                        }
                    ],
                    "synonyms": [
                        "elevation",
                        "height"
                    ],
                    "antonyms": [
                        "depth"
                    ],
                    "common_phrases": [
                        "high altitude",
                        "cruising altitude",
                        "low altitude",
                        "lose altitude",
                        "at an altitude"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высота объекта или места над уровнем моря или землей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "расстояние от земли или уровня моря"
                            },
                            {
                                "target_lang_word": "высота над уровнем моря",
                                "target_lang_sense_clarification": "абсолютная высота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d8c543c-5969-3f9d-93f0-975a8d4bfa93",
                    "sense_definition": "The perpendicular distance from a vertex to the opposite side of a geometric figure.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "height_measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To find the area of the triangle, you must multiply the base by the <w>altitude</w> and divide by two.",
                            "target_lang_translations": {
                                "ru": "Чтобы найти площадь треугольника, нужно умножить основание на <w>высоту</w> и разделить на два."
                            }
                        },
                        {
                            "text": "The teacher asked the students to draw a line representing the <w>altitude</w> of the pyramid.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников начертить линию, обозначающую <w>высоту</w> пирамиды."
                            }
                        }
                    ],
                    "synonyms": [
                        "height"
                    ],
                    "common_phrases": [
                        "altitude of a triangle"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "geometry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перпендикулярное расстояние от вершины до противоположной стороны геометрической фигуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "геометрический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a7edbc1-6795-3b05-9a2d-dc3885a36dea",
                    "sense_definition": "The angular distance of a celestial body above the horizon.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The navigator used a sextant to measure the <w>altitude</w> of the sun at noon.",
                            "target_lang_translations": {
                                "ru": "Штурман использовал секстант, чтобы измерить <w>высоту</w> солнца в полдень."
                            }
                        },
                        {
                            "text": "We observed the star at a low <w>altitude</w> just before it set below the horizon.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали звезду на низкой <w>высоте</w> прямо перед тем, как она зашла за горизонт."
                            }
                        }
                    ],
                    "synonyms": [
                        "elevation"
                    ],
                    "common_phrases": [
                        "solar altitude"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "astronomy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Угловое расстояние небесного тела над горизонтом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "астрономический термин"
                            },
                            {
                                "target_lang_word": "угол возвышения",
                                "target_lang_sense_clarification": "угол над горизонтом"
                            }
                        ]
                    }
                }
            ]
        }

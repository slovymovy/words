                },
                {
                    "sense_id": "72f38e56-51ee-3a5c-ab13-785539cb6140",
                    "sense_definition": "An exclamation of triumph, surprise, or discovery.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "triumph_exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ha</w>! I knew you were hiding behind the door.",
                            "target_lang_translations": {
                                "ru": "<w>Ха</w>! Я знал, что ты прячешься за дверью."
                            }
                        },
                        {
                            "text": "<w>Ha</w>! Checkmate! I have won the game.",
                            "target_lang_translations": {
                                "ru": "<w>Ха</w>! Шах и мат! Я выиграл."
                            }
                        }
                    ],
                    "synonyms": [
                        "aha",
                        "gotcha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас торжества, удивления или внезапного открытия."
                    },
                    "translations": {

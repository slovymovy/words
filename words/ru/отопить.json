            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5472ffc3-c551-305f-84ee-4d03b8af3d6f",
                    "sense_definition": "Нагреть помещение, сделав его тёплым с помощью печи, батарей или системы отопления.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой очень трудно <w>отопить</w> такой большой и старый дом.",
                            "target_lang_translations": {
                                "en": "In winter, it is very difficult to <w>heat</w> such a big and old house."
                            }
                        },
                        {
                            "text": "Мы решили <w>отопить</w> дачу дровами, чтобы быстро согреться.",
                            "target_lang_translations": {
                                "en": "We decided to <w>heat</w> the dacha with firewood to get warm quickly."
                            }
                        }
                    ],
                    "synonyms": [
                        "обогреть",
                        "нагреть"
                    ],
                    "antonyms": [
                        "выстудить"
                    ],
                    "common_phrases": [
                        "отопить дом",
                        "отопить квартиру",
                        "отопить помещение",
                        "отопить здание"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a room or building warm using a stove, radiators, or a heating system."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heat",
                                "target_lang_sense_clarification": "to make a building or room warm"
                            },
                            {
                                "target_lang_word": "heat up",
                                "target_lang_sense_clarification": "to make warm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e5654d3-8151-3790-aef9-f204f2897f7d",
                    "sense_definition": "Поставить реи на корабле наклонно по отношению к горизонту (обычно в знак траура).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В знак глубокого траура все корабли в гавани <w>отопили</w> реи.",
                            "target_lang_translations": {
                                "en": "As a sign of deep mourning, all ships in the harbor <w>peaked</w> their yards."
                            }
                        },
                        {
                            "text": "Капитан отдал приказ <w>отопить</w> реи и приспустить флаг.",
                            "target_lang_translations": {
                                "en": "The captain gave the order to <w>peak</w> the yards and lower the flag."
                            }
                        }
                    ],
                    "synonyms": [
                        "искосить"
                    ],
                    "antonyms": [
                        "выровнять"
                    ],
                    "common_phrases": [
                        "отопить реи"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To set the yards of a ship at an angle to the horizon (usually as a sign of mourning)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "peak",
                                "target_lang_sense_clarification": "nautical: to raise the point of a gaff or yard"
                            },
                            {
                                "target_lang_word": "cockbill",
                                "target_lang_sense_clarification": "nautical: to set yards at an angle as a sign of mourning"
                            }
                        ]
                    }
                }
            ]
        }

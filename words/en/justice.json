    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ed21e469-24ba-3170-9d11-dd1f3872386f",
                    "sense_definition": "The quality of being fair, impartial, and reasonable.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fairness_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must fight for peace and <w>justice</w> in our society.",
                            "target_lang_translations": {
                                "ru": "Мы должны бороться за мир и <w>справедливость</w> в нашем обществе."
                            }
                        },
                        {
                            "text": "He has a strong sense of <w>justice</w> and always tries to do the right thing.",
                            "target_lang_translations": {
                                "ru": "У него обостренное чувство <w>справедливости</w>, и он всегда старается поступать правильно."
                            }
                        }
                    ],
                    "antonyms": [
                        "injustice"
                    ],
                    "common_phrases": [
                        "sense of justice",
                        "social justice",
                        "do justice",
                        "poetic justice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство быть справедливым, беспристрастным и разумным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "справедливость",
                                "target_lang_sense_clarification": "моральное качество, честность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4a02079-4364-3554-a155-8cb20a5fc144",
                    "sense_definition": "The legal system or process of judging crimes and punishing criminals.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The criminal was finally brought to <w>justice</w> after many years.",
                            "target_lang_translations": {
                                "ru": "Преступник был наконец предан <w>правосудию</w> спустя много лет."
                            }
                        },
                        {
                            "text": "Many people believe the <w>justice</w> system needs significant reform.",
                            "target_lang_translations": {
                                "ru": "Многие считают, что система <w>правосудия</w> нуждается в значительной реформе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bring to justice",
                        "justice system",
                        "criminal justice",
                        "miscarriage of justice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правовая система или процесс судебного разбирательства и наказания преступников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правосудие",
                                "target_lang_sense_clarification": "судебная деятельность"
                            },
                            {
                                "target_lang_word": "юстиция",
                                "target_lang_sense_clarification": "система судебных учреждений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e20e1f61-b240-3dde-87d6-d50e4581db3a",
                    "sense_definition": "A judge in a court of law, especially in higher courts.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "official_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>justice</w> listened carefully to the arguments from both lawyers.",
                            "target_lang_translations": {
                                "ru": "<w>Судья</w> внимательно выслушал доводы обоих адвокатов."
                            }
                        },
                        {
                            "text": "She was appointed as a <w>justice</w> of the Supreme Court last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году она была назначена <w>судьей</w> Верховного суда."
                            }
                        }
                    ],
                    "synonyms": [
                        "judge"
                    ],
                    "common_phrases": [
                        "chief justice",
                        "associate justice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Судья в суде, особенно в высших судебных инстанциях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судья",
                                "target_lang_sense_clarification": "должностное лицо"
                            }
                        ]
                    }
                }
            ]
        },

                        "druk polecony",
                        "druk urzędowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный бланк с полями для заполнения или официальный документ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бланк",
                                "target_lang_sense_clarification": "документ для заполнения"
                            },
                            {
                                "target_lang_word": "формуляр",
                                "target_lang_sense_clarification": "форма документа"

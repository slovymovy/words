                {
                    "sense_id": "133bcd12-ef0c-3aca-9183-99ab4cf3e047",
                    "sense_definition": "Een schematisch overzicht of weergave van een situatie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "overview",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De medaille<w>spiegel</w> laat zien welk land de meeste prijzen heeft gewonnen.",
                            "target_lang_translations": {
                                "en": "The medal <w>table</w> shows which country has won the most prizes.",
                                "ru": "Медальный <w>зачет</w> показывает, какая страна выиграла больше всего наград."
                            }
                        },
                        {
                            "text": "Dit rapport vormt een <w>spiegel</w> van de huidige economische situatie.",
                            "target_lang_translations": {

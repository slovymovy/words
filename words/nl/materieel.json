                        "verouderd materieel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортные средства, предназначенные для рельсовых перевозок, такие как поезда, трамваи и метро."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвижной состав",
                                "target_lang_sense_clarification": "железнодорожный транспорт"
                            }
                        ]
                    }
                }
            ]

                {
                    "sense_id": "7ba2dd35-6afb-3c06-ba66-117d55667990",
                    "sense_definition": "A programmable electronic device that performs mathematical calculations and logical operations, used to process, store, and retrieve data.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do all my homework on my personal <w>computer</w>.",
                            "target_lang_translations": {
                                "ru": "Я делаю всё домашнее задание на своём персональном <w>компьютере</w>."
                            }
                        },
                        {
                            "text": "The <w>computer</w> crashed and I lost all my important files.",
                            "target_lang_translations": {
                                "ru": "<w>Компьютер</w> завис, и я потерял все свои важные файлы."
                            }
                        }
                    ],
                    "synonyms": [
                        "machine",
                        "processor",
                        "electronic computer"
                    ],
                    "common_phrases": [
                        "personal computer",
                        "computer science",
                        "computer program",
                        "computer system",
                        "desktop computer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Программируемое электронное устройство, выполняющее математические вычисления и логические операции, используемое для обработки, хранения и поиска данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компьютер",
                                "target_lang_sense_clarification": "электронное устройство"
                            },
                            {
                                "target_lang_word": "ЭВМ",
                                "target_lang_sense_clarification": "формальный или устаревший термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c298dbd9-9731-3e1c-a7d6-79ddcc7f4b08",
                    "sense_definition": "A person employed to perform mathematical computations.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Before electronic devices, a <w>computer</w> was a human clerk who did math.",
                            "target_lang_translations": {
                                "ru": "До появления электронных устройств <w>вычислителем</w> называли служащего, который занимался математикой."
                            }
                        },
                        {
                            "text": "She worked as a <w>computer</w> calculating flight paths for the space program.",
                            "target_lang_translations": {
                                "ru": "Она работала <w>вычислителем</w>, рассчитывая траектории полётов для космической программы."
                            }
                        }
                    ],
                    "synonyms": [

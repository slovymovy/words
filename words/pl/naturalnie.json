    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "4a0d2a8d-f04b-3a69-9fe6-e33a355da7a1",
                    "sense_definition": "Słowo wyrażające potwierdzenie, zgodę lub przekonanie o oczywistości czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "– Czy mogę pożyczyć twój długopis? – <w>Naturalnie</w>, proszę bardzo.",
                            "target_lang_translations": {
                                "ru": "— Могу я одолжить твою ручку? — <w>Естественно</w>, пожалуйста."
                            }
                        },
                        {
                            "text": "<w>Naturalnie</w>, że pamiętam o naszym spotkaniu w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "<w>Естественно</w>, я помню о нашей встрече на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "oczywiście",
                        "jasne",
                        "pewnie",
                        "jak najbardziej",
                        "rzecz jasna"
                    ],
                    "common_phrases": [
                        "ależ naturalnie",
                        "naturalnie że tak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, выражающее подтверждение, согласие или уверенность в очевидности чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "естественно",
                                "target_lang_sense_clarification": "выражение согласия или очевидности"
                            },
                            {
                                "target_lang_word": "конечно",
                                "target_lang_sense_clarification": "общеупотребительное выражение согласия"
                            },
                            {
                                "target_lang_word": "разумеется",
                                "target_lang_sense_clarification": "само собой разумеется"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9cdc6ab4-ac11-3195-ba0e-da598198d637",
                    "sense_definition": "W sposób zgodny z naturą, swobodny, niewymuszony lub pozbawiony sztuczności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo stresu, aktorka zachowywała się na scenie bardzo <w>naturalnie</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на стресс, актриса вела себя на сцене очень <w>естественно</w>."
                            }
                        },
                        {
                            "text": "Ten sok owocowy smakuje zupełnie <w>naturalnie</w>, bez chemicznych dodatków.",
                            "target_lang_translations": {
                                "ru": "Этот фруктовый сок на вкус совершенно <w>натуральный</w>, без химических добавок."
                            }
                        }
                    ],
                    "antonyms": [
                        "sztucznie",
                        "nienaturalnie",
                        "syntetycznie"
                    ],
                    "common_phrases": [
                        "zachowywać się naturalnie",
                        "wyglądać naturalnie",
                        "występować naturalnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественным образом, свободно, непринужденно или без искусственности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "естественно",
                                "target_lang_sense_clarification": "непринужденно, без напряжения"
                            },
                            {
                                "target_lang_word": "натурально",
                                "target_lang_sense_clarification": "правдоподобно, в соответствии с натурой"
                            },
                            {
                                "target_lang_word": "непринужденно",
                                "target_lang_sense_clarification": "свободно, раскованно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf9dd9e9-0238-3d35-8cce-9e5de156c300",
                    "sense_definition": "W sposób oczywisty, wynikający z logicznego następstwa faktów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako rodzic, <w>naturalnie</w> martwię się o bezpieczeństwo moich dzieci.",
                            "target_lang_translations": {
                                "ru": "Как родитель, я, <w>естественно</w>, беспокоюсь о безопасности своих детей."
                            }
                        },
                        {
                            "text": "Wybór padł <w>naturalnie</w> na najbardziej doświadczonego pracownika w zespole.",
                            "target_lang_translations": {
                                "ru": "Выбор <w>естественно</w> пал на самого опытного сотрудника в команде."
                            }
                        }
                    ],
                    "synonyms": [
                        "oczywiście",
                        "rzecz jasna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очевидным образом, как логическое следствие фактов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "естественно",
                                "target_lang_sense_clarification": "как следствие"
                            },
                            {
                                "target_lang_word": "закономерно",
                                "target_lang_sense_clarification": "обоснованно логикой событий"
                            }
                        ]
                    }
                }
            ]
        }
    ]

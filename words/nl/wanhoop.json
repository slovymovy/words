    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "59615a51-206a-3e8b-b032-61bda58a48b5",
                    "sense_definition": "Een ellendige toestand waarin men geen uitkomst meer ziet.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik hoorde de <w>wanhoop</w> in zijn stem toen hij belde.",
                            "target_lang_translations": {
                                "en": "I heard the <w>despair</w> in his voice when he called."
                            }
                        },
                        {
                            "text": "Uit pure <w>wanhoop</w> zocht hij naar een andere oplossing voor het probleem.",
                            "target_lang_translations": {
                                "en": "Out of pure <w>desperation</w> he looked for another solution to the problem."
                            }
                        }
                    ],
                    "synonyms": [
                        "radeloosheid"
                    ],
                    "antonyms": [
                        "hoop"
                    ],
                    "common_phrases": [
                        "tot wanhoop drijven",
                        "uit wanhoop",
                        "de wanhoop nabij"
                    ],
                    "target_lang_definitions": {
                        "en": "A miserable state in which one sees no way out."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "despair",
                                "target_lang_sense_clarification": "complete loss of hope"
                            },
                            {
                                "target_lang_word": "desperation",
                                "target_lang_sense_clarification": "state of hopelessness leading to rash action"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cd11d55b-69ee-3716-b9b5-ce139b8b9d65",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>wanhopen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>wanhoop</w> niet, want ik weet dat er nog kansen zijn.",
                            "target_lang_translations": {
                                "en": "I do not <w>despair</w>, because I know there are still chances."
                            }
                        },
                        {
                            "text": "Zolang ik nog mogelijkheden zie, <w>wanhoop</w> ik niet.",
                            "target_lang_translations": {
                                "en": "As long as I still see possibilities, I do not <w>despair</w>."
                            }
                        }
                    ],

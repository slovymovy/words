            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d9ae4ea-e694-3036-99c0-a2f8818f8b85",
                    "sense_definition": "Niet slagen in een opzet, een fout maken of tekortschieten in een verplichting.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude geschriften waarschuwen dat zelfs de wijsten kunnen <w>feilen</w>.",
                            "target_lang_translations": {
                                "en": "The old scriptures warn that even the wisest can <w>err</w>.",
                                "ru": "Древние писания предупреждают, что даже мудрейшие могут <w>ошибаться</w>."
                            }
                        },
                        {
                            "text": "Zijn moed <w>feilde</w> niet, zelfs niet in het zicht van het gevaar.",
                            "target_lang_translations": {
                                "en": "His courage did not <w>falter</w>, even in the face of danger.",
                                "ru": "Его мужество не <w>дрогнуло</w>, даже перед лицом опасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "falen",
                        "mislukken",
                        "zich vergissen"
                    ],
                    "antonyms": [
                        "slagen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "In modern Nederlands wordt meestal 'falen' of 'zich vergissen' gebruikt."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To fail in an attempt, make a mistake, or fall short in an obligation.",
                        "ru": "Потерпеть неудачу в попытке, совершить ошибку или не выполнить обязательство."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to fail",
                                "target_lang_sense_clarification": "general failure to succeed or perform a duty"
                            },
                            {
                                "target_lang_word": "to err",
                                "target_lang_sense_clarification": "to make a mistake (often literary or archaic)"
                            },
                            {
                                "target_lang_word": "to falter",
                                "target_lang_sense_clarification": "to lose strength or momentum (e.g., courage)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ошибаться",
                                "target_lang_sense_clarification": "совершать ошибку (часто в литературном контексте)"
                            },
                            {
                                "target_lang_word": "терпеть неудачу",
                                "target_lang_sense_clarification": "не достигать желаемого результата"
                            },
                            {
                                "target_lang_word": "оступаться",
                                "target_lang_sense_clarification": "совершать ошибку (образно)"
                            }
                        ]
                    }
                }
            ]
        },
        {

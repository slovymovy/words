    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2307731d-6177-3791-82ef-59d894ac8bea",
                    "sense_definition": "Het beoefenen van zwarte magie of tovenarij, vaak in verband gebracht met de duivel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "magic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dorpelingen waren bang voor <w>duivelarij</w> en meden het bos 's nachts.",
                            "target_lang_translations": {
                                "ru": "Жители деревни боялись <w>чертовщины</w> и избегали леса по ночам."
                            }
                        },
                        {
                            "text": "In oude verhalen wordt <w>duivelarij</w> vaak bestraft door de kerk.",
                            "target_lang_translations": {
                                "ru": "В старых преданиях <w>дьявольщина</w> часто карается церковью."
                            }
                        }
                    ],
                    "synonyms": [
                        "satanisme"
                    ],
                    "common_phrases": [
                        "van duivelarij beschuldigen",
                        "pure duivelarij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Практика черной магии или колдовства, часто ассоциируемая с дьяволом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чертовщина",
                                "target_lang_sense_clarification": "нечистая сила, колдовство"
                            },
                            {
                                "target_lang_word": "дьявольщина",
                                "target_lang_sense_clarification": "связь с дьяволом, злые чары"
                            },
                            {
                                "target_lang_word": "бесовщина",
                                "target_lang_sense_clarification": "действия злых духов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2132a0bf-f9a2-3e55-acd0-60fbb7e78322",
                    "sense_definition": "Vervelend gedoe, aanhoudend gezeur of een lastige situatie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nuisance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb geen zin in al die administratieve <w>duivelarij</w>.",
                            "target_lang_translations": {
                                "ru": "У меня нет желания заниматься всей этой административной <w>морокой</w>."
                            }
                        },
                        {
                            "text": "Wat een <w>duivelarij</w> is het om deze oude machine te repareren.",
                            "target_lang_translations": {
                                "ru": "Какое же это <w>мучение</w> — чинить эту старую машину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wat een duivelarij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятные хлопоты, нудная работа или сложная ситуация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морока",
                                "target_lang_sense_clarification": "хлопотное дело"
                            },
                            {
                                "target_lang_word": "мучение",
                                "target_lang_sense_clarification": "неприятная, тяжелая задача"
                            },
                            {
                                "target_lang_word": "наказание",
                                "target_lang_sense_clarification": "в значении 'неприятность, обуза'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
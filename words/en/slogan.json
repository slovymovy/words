            "senses": [
                {
                    "sense_id": "68afb5ef-e4e3-3ec6-94fa-7d5ac3d1b815",
                    "sense_definition": "A short and memorable phrase used in advertising to promote a product or brand.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "catchphrase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The marketing team needs a catchy <w>slogan</w> for the new product launch.",
                            "target_lang_translations": {
                                "ru": "Маркетинговой команде нужен броский <w>слоган</w> для запуска нового продукта."
                            }
                        },
                        {
                            "text": "A good <w>slogan</w> helps customers remember the brand name easily.",
                            "target_lang_translations": {
                                "ru": "Хороший <w>слоган</w> помогает покупателям легко запомнить название бренда."
                            }
                        }
                    ],
                    "synonyms": [
                        "tagline",
                        "strapline"
                    ],
                    "common_phrases": [
                        "catchy slogan",
                        "advertising slogan",
                        "marketing slogan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткая и запоминающаяся фраза, используемая в рекламе для продвижения товара или бренда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слоган",
                                "target_lang_sense_clarification": "рекламный"
                            },
                            {
                                "target_lang_word": "девиз",
                                "target_lang_sense_clarification": "бренда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2866a20-b0b8-35c7-a64b-36d7e9d21542",
                    "sense_definition": "A distinctive phrase used by a group or political party to express a position or goal.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "catchphrase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The candidate repeated his political <w>slogan</w> at every rally to inspire voters.",
                            "target_lang_translations": {
                                "ru": "Кандидат повторял свой политический <w>лозунг</w> на каждом митинге, чтобы воодушевить избирателей."
                            }
                        },
                        {
                            "text": "Protesters painted a <w>slogan</w> on the wall to demand immediate change.",
                            "target_lang_translations": {
                                "ru": "Протестующие написали на стене <w>лозунг</w> с требованием немедленных перемен."
                            }
                        }
                    ],
                    "synonyms": [

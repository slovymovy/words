        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2e0f9407-b362-3f34-b631-717baa287b27",
                    "sense_definition": "To welcome someone in a friendly manner, either in person or through other means such as writing.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "welcome",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She went to the door to <w>greet</w> her guests with a warm smile.",
                            "target_lang_translations": {
                                "ru": "Она пошла к двери, чтобы <w>встретить</w> гостей теплой улыбкой."
                            }
                        },
                        {
                            "text": "The host <w>greeted</w> us at the entrance and showed us to our table.",
                            "target_lang_translations": {
                                "ru": "Хозяин <w>поприветствовал</w> нас у входа и проводил к нашему столику."
                            }
                        }
                    ],
                    "synonyms": [
                        "hail"
                    ],
                    "common_phrases": [
                        "greet a friend",
                        "greet with a smile",
                        "greet warmly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приветствовать кого-либо дружелюбно, лично или иным способом (например, письменно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приветствовать",
                                "target_lang_sense_clarification": "обращаться с приветствием"
                            },
                            {
                                "target_lang_word": "здороваться",
                                "target_lang_sense_clarification": "говорить «здравствуйте» или жать руку"
                            },
                            {
                                "target_lang_word": "встречать",
                                "target_lang_sense_clarification": "принимать гостей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bfde528-8682-33ea-8f9b-93e1a75a6913",
                    "sense_definition": "To be noticed by or present itself to someone's sight, hearing, or other senses.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "perceive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A spectacular view of the ocean <w>greeted</w> us when we reached the summit.",
                            "target_lang_translations": {
                                "ru": "Захватывающий вид на океан <w>встретил</w> нас, когда мы достигли вершины."
                            }
                        },
                        {
                            "text": "The smell of fresh coffee <w>greeted</w> him as he walked into the kitchen.",
                            "target_lang_translations": {
                                "ru": "Запах свежего кофе <w>встретил</w> его, когда он вошел в кухню."
                            }
                        }
                    ],
                    "common_phrases": [
                        "greet the eye",
                        "greet the ear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представать перед чьим-либо взором, слухом или другими чувствами; быть замеченным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречать",
                                "target_lang_sense_clarification": "попадаться на глаза или навстречу"
                            },
                            {
                                "target_lang_word": "приветствовать",
                                "target_lang_sense_clarification": "метафорически: о звуках, запахах или видах"
                            }
                        ]

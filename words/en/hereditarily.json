    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2d6c01be-ee34-3c16-8a09-50f7e29c2ec4",
                    "sense_definition": "In a way that is passed from parents to offspring, usually referring to genetic characteristics or biological traits.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor explained that the condition is <w>hereditarily</w> transmitted through the mother's side.",
                            "target_lang_translations": {
                                "ru": "Врач объяснил, что это заболевание передается <w>по наследству</w> по материнской линии."
                            }
                        },
                        {
                            "text": "Some scientists believe that intelligence is partly <w>hereditarily</w> determined.",
                            "target_lang_translations": {
                                "ru": "Некоторые ученые полагают, что интеллект частично предопределен <w>наследственно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "genetically",
                        "congenitally",
                        "innately"
                    ],
                    "antonyms": [
                        "environmentally"
                    ],
                    "common_phrases": [
                        "hereditarily predisposed",
                        "hereditarily transmitted",
                        "hereditarily determined"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способом, при котором признаки передаются от родителей к потомству; обычно относится к генетическим характеристикам или биологическим чертам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наследственно",
                                "target_lang_sense_clarification": "генетически"
                            },
                            {
                                "target_lang_word": "по наследству",
                                "target_lang_sense_clarification": "от родителей к детям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f64b836-486a-3b74-acd5-8f2cb760991d",
                    "sense_definition": "In a manner related to the legal succession of title, rank, or property from one generation to the next.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the past, political power was often held <w>hereditarily</w> by a single family.",
                            "target_lang_translations": {
                                "ru": "В прошлом политическая власть часто удерживалась одной семьей <w>по праву наследования</w>."
                            }
                        },
                        {
                            "text": "The duke claimed he was <w>hereditarily</w> entitled to the estate.",
                            "target_lang_translations": {
                                "ru": "Герцог заявил, что имеет право на поместье <w>по наследству</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hereditarily entitled",
                        "hereditarily acquired"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способом, связанным с законным переходом титула, звания или имущества от одного поколения к другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по праву наследования",
                                "target_lang_sense_clarification": "юридический аспект"
                            },
                            {
                                "target_lang_word": "по наследству",
                                "target_lang_sense_clarification": "передача имущества или титула"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
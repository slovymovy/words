    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8c4785e7-34e3-3206-97ec-928d6966841f",
                    "sense_definition": "Something that exists apart from other things, having its own independent existence; often refers to a business, organization, or political unit.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "independent_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The corporation is a separate legal <w>entity</w> from its owners.",
                            "target_lang_translations": {
                                "ru": "Корпорация — это отдельное <w>юридическое лицо</w>, независимое от своих владельцев."
                            }
                        },
                        {
                            "text": "After the merger, the two companies became a single business <w>entity</w>.",
                            "target_lang_translations": {
                                "ru": "После слияния две компании стали единой бизнес-<w>единицей</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "entity",
                        "entity",
                        "entity",
                        "entity"
                    ],
                    "common_phrases": [
                        "legal entity",
                        "corporate entity",
                        "business entity",
                        "separate entity",
                        "single entity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организация, учреждение или политическая единица, существующая независимо от других; часто подразумевает правовой или экономический статус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юридическое лицо",
                                "target_lang_sense_clarification": "правовой статус организации"
                            },
                            {
                                "target_lang_word": "субъект",
                                "target_lang_sense_clarification": "экономическая или правовая единица"
                            },
                            {
                                "target_lang_word": "организация",
                                "target_lang_sense_clarification": "учреждение или компания"
                            },
                            {
                                "target_lang_word": "самостоятельная единица",
                                "target_lang_sense_clarification": "отдельный элемент структуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7d436e6-b11c-35ce-bbf8-42f0a05d3d4b",
                    "sense_definition": "Something that has a real existence; a thing with distinct and independent existence, often distinguished from its qualities or relations.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "independent_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Philosophy often examines whether the mind is a separate <w>entity</w> from the body.",
                            "target_lang_translations": {
                                "ru": "Философия часто рассматривает вопрос, является ли разум отдельной <w>сущностью</w>, отличной от тела."
                            }
                        },
                        {
                            "text": "The artist viewed the painting not just as paint on canvas, but as a living <w>entity</w>.",
                            "target_lang_translations": {
                                "ru": "Художник рассматривал картину не просто как краску на холсте, а как живое <w>существо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "entity",
                        "entity",
                        "entity"
                    ],
                    "common_phrases": [
                        "distinct entity",
                        "independent entity",
                        "living entity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечто реально существующее; объект с независимым бытием, отличный от своих свойств и отношений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сущность",
                                "target_lang_sense_clarification": "философское понятие, нечто существующее"
                            },
                            {
                                "target_lang_word": "нечто",
                                "target_lang_sense_clarification": "неопределенный объект"
                            },
                            {
                                "target_lang_word": "организм",
                                "target_lang_sense_clarification": "в контексте живых существ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d26933bb-1642-3610-8e79-2c72a81384b1",
                    "sense_definition": "A distinct object or item, such as a person, place, or thing, about which data is stored in a computer database.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The database administrator defined a new <w>entity</w> to store customer information.",
                            "target_lang_translations": {
                                "ru": "Администратор базы данных определил новую <w>сущность</w> для хранения информации о клиентах."
                            }
                        },
                        {
                            "text": "In this model, every product is treated as a unique data <w>entity</w>.",
                            "target_lang_translations": {
                                "ru": "В этой модели каждый продукт рассматривается как уникальная информационная <w>сущность</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "data entity",
                        "entity relationship"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный объект или элемент данных, информация о котором хранится и обрабатывается в компьютерной системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сущность",
                                "target_lang_sense_clarification": "элемент модели данных"
                            },
                            {
                                "target_lang_word": "объект",
                                "target_lang_sense_clarification": "в программировании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f47572f8-563d-369f-9a89-51b5e6af9e1b",
                    "sense_definition": "A mysterious being or spirit, often one without a physical body.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ghost hunters claimed a supernatural <w>entity</w> was haunting the old mansion.",
                            "target_lang_translations": {
                                "ru": "Охотники за привидениями утверждали, что в старом особняке обитает сверхъестественная <w>сущность</w>."

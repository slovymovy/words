    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c8eb906-dff1-3c88-878b-67513e2e672b",
                    "sense_definition": "Liczba mnoga od słowa <w>korzonek</w>, oznaczająca małe, drobne części podziemne rośliny.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "botanical_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta młoda roślina ma jeszcze bardzo słabe <w>korzonki</w>.",
                            "target_lang_translations": {
                                "ru": "У этого молодого растения еще очень слабые <w>корешки</w>."
                            }
                        },
                        {
                            "text": "Do wywaru warzywnego warto dodać umyte <w>korzonki</w> pietruszki.",
                            "target_lang_translations": {
                                "ru": "В овощной бульон стоит добавить мытые <w>корешки</w> петрушки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "suszone korzonki",
                        "delikatne korzonki"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "liczba mnoga"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова \"korzonek\", обозначающее маленькие, мелкие подземные части растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корешки",
                                "target_lang_sense_clarification": "маленькие корни растений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0792e227-bb22-355a-88c3-4b90f2a29228",
                    "sense_definition": "Potoczne określenie korzeni nerwowych wychodzących z rdzenia kręgowego, często używane w kontekście bólu pleców spowodowanego ich zapaleniem.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_colloquial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek narzeka na <w>korzonki</w> i musi leżeć w łóżku.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка жалуется на <w>радикулит</w> и должен лежать в постели."
                            }
                        },
                        {
                            "text": "Lekarz przepisał mi maść rozgrzewającą na chore <w>korzonki</w>.",
                            "target_lang_translations": {
                                "ru": "Врач прописал мне согревающую мазь от <w>радикулита</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "korzenie nerwowe"
                    ],
                    "common_phrases": [
                        "mieć korzonki",
                        "chore korzonki",
                        "zapalenie korzonków",
                        "przewiać korzonki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w kontekście dolegliwości zdrowotnych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название нервных корешков, отходящих от спинного мозга, часто используется в контексте боли в спине, вызванной их воспалением (радикулита)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радикулит",
                                "target_lang_sense_clarification": "заболевание (разговорное)"
                            },
                            {
                                "target_lang_word": "нервные корешки",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

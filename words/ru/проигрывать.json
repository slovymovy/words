    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "998d405f-3044-3ee1-8fb2-1b123f41d6eb",
                    "sense_definition": "Терпеть поражение в борьбе, соревновании, споре или игре; уступать сопернику.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша команда часто <w>проигрывает</w> в выездных матчах из-за плохой подготовки.",
                            "target_lang_translations": {
                                "en": "Our team often <w>loses</w> away matches due to poor preparation."
                            }
                        },
                        {
                            "text": "Никто не любит <w>проигрывать</w> со счётом, который кажется разгромным.",
                            "target_lang_translations": {
                                "en": "No one likes to <w>lose</w> with a score that seems crushing."
                            }
                        }
                    ],
                    "antonyms": [
                        "выи́грывать"
                    ],
                    "common_phrases": [
                        "проигрывать войну",
                        "проигрывать матч",
                        "проигрывать спор",
                        "проигрывать со счётом"
                    ],
                    "target_lang_definitions": {
                        "en": "To be defeated in a game, competition, or argument; to fail to win."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose",
                                "target_lang_sense_clarification": "be defeated"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37243c65-1a0a-33e0-87b1-aa9424a79bde",
                    "sense_definition": "Лишаться денег или имущества в результате неудачи в азартной игре.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он боялся <w>проигрывать</w> крупные суммы, поэтому редко ходил в казино.",
                            "target_lang_translations": {
                                "en": "He was afraid to <w>lose</w> large sums, so he rarely went to the casino."
                            }
                        },
                        {
                            "text": "Не стоит <w>проигрывать</w> деньги, которые отложены на важные покупки.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>lose</w> money that is set aside for important purchases."
                            }
                        }
                    ],
                    "antonyms": [
                        "выи́грывать"
                    ],
                    "common_phrases": [
                        "проигрывать деньги",
                        "проигрывать в карты",
                        "проигрывать состояние",
                        "проигрывать ставку"
                    ],
                    "target_lang_definitions": {
                        "en": "To lose money or property through gambling or betting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose",
                                "target_lang_sense_clarification": "money in gambling"
                            },
                            {
                                "target_lang_word": "gamble away",
                                "target_lang_sense_clarification": "lose by betting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ecc7e31-4292-360e-bcfd-1acbb25580b1",
                    "sense_definition": "Воспроизводить звуковую или видеозапись с помощью технического устройства.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "playback",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот старый магнитофон всё ещё хорошо <w>проигрывает</w> кассеты.",
                            "target_lang_translations": {
                                "en": "This old tape recorder still <w>plays</w> cassettes well."
                            }
                        },
                        {
                            "text": "Программа отказалась <w>проигрывать</w> файл из-за неизвестного формата.",
                            "target_lang_translations": {
                                "en": "The program refused to <w>play</w> the file due to an unknown format."
                            }
                        }
                    ],
                    "common_phrases": [
                        "проигрывать пластинку",
                        "проигрывать диск",
                        "проигрывать мелодию",
                        "проигрывать файл"
                    ],
                    "target_lang_definitions": {
                        "en": "To reproduce audio or video recordings using a device."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "play",
                                "target_lang_sense_clarification": "media"
                            },
                            {
                                "target_lang_word": "play back",
                                "target_lang_sense_clarification": "recording"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c71b356-6245-3ff0-9a2b-f2ea0803d46d",
                    "sense_definition": "Исполнять музыкальное произведение на инструменте, часто с целью упражнения или проверки.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "playback",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пианист начал <w>проигрывать</w> сложный пассаж перед началом концерта.",
                            "target_lang_translations": {
                                "en": "The pianist began to <w>play through</w> a difficult passage before the start of the concert."
                            }
                        },
                        {
                            "text": "Преподаватель посоветовал ученику медленно <w>проигрывать</w> гаммы каждый день.",
                            "target_lang_translations": {
                                "en": "The teacher advised the student to slowly <w>play</w> scales every day."
                            }
                        }
                    ],
                    "common_phrases": [
                        "проигрывать гаммы",
                        "проигрывать пьесу",
                        "проигрывать этюд",
                        "проигрывать на рояле"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform a musical piece on an instrument, often for practice or rehearsal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "play",
                                "target_lang_sense_clarification": "music"
                            },
                            {
                                "target_lang_word": "run through",
                                "target_lang_sense_clarification": "practice a piece"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "calculation_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgens mijn <w>rekening</w> moeten we er over tien minuten zijn.",
                            "target_lang_translations": {

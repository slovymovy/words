    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b658ba1-902e-353e-ac9e-f604429c5e85",
                    "sense_definition": "Uraz fizyczny jakiejś części ciała, często odniesiony podczas uprawiania sportu, który powoduje ból oraz ogranicza lub uniemożliwia sprawność ruchową.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Najlepszy zawodnik naszej drużyny uległ wczoraj poważnej <w>kontuzji</w>.",
                            "target_lang_translations": {
                                "ru": "Лучший игрок нашей команды вчера получил серьезную <w>травму</w>."
                            }
                        },
                        {
                            "text": "Przez bolesną <w>kontuzję</w> kolana musiała zrezygnować z udziału w maratonie.",
                            "target_lang_translations": {
                                "ru": "Из-за болезненной <w>травмы</w> колена ей пришлось отказаться от участия в марафоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "uraz"
                    ],
                    "common_phrases": [
                        "doznać kontuzji",
                        "leczyć kontuzję",
                        "poważna kontuzja",
                        "kontuzja kolana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая травма какой-либо части тела, часто полученная во время занятий спортом, вызывающая боль и ограничивающая подвижность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "травма",
                                "target_lang_sense_clarification": "телесное повреждение, часто в спорте"
                            },
                            {
                                "target_lang_word": "повреждение",
                                "target_lang_sense_clarification": "нарушение целостности тканей или органов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a750c04-3680-3ccd-a794-41eeabd7c2ea",
                    "sense_definition": "Stłuczenie tkanek miękkich powstałe na skutek uderzenia, zwykle niewiążące się z przerwaniem skóry.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz stwierdził bolesną <w>kontuzję</w> mięśnia po upadku ze schodów.",
                            "target_lang_translations": {
                                "ru": "Врач диагностировал болезненный <w>ушиб</w> мышцы после падения с лестницы."
                            }
                        },
                        {
                            "text": "To tylko lekka <w>kontuzja</w>, która powinna zagoić się w ciągu kilku dni.",
                            "target_lang_translations": {
                                "ru": "Это всего лишь легкий <w>ушиб</w>, который должен зажить за несколько дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "stłuczenie"
                    ],
                    "common_phrases": [
                        "lekka kontuzja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повреждение мягких тканей вследствие удара, обычно без разрыва кожи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ушиб",
                                "target_lang_sense_clarification": "повреждение тканей без раны"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "teveel",
        "tekort",
        "tegelijk",
        "terecht",
        "tenzij"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f996d418-1748-380d-91f9-bf69270c9286",
                    "sense_definition": "In een hogere mate dan wenselijk, nodig of goed is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water is <w>te</w> koud om in te zwemmen.",
                            "target_lang_translations": {
                                "en": "The water is <w>too</w> cold to swim in.",
                                "ru": "Вода <w>слишком</w> холодная, чтобы в ней плавать."
                            }
                        },
                        {
                            "text": "Je hebt <w>te</w> veel suiker in de koffie gedaan.",
                            "target_lang_translations": {
                                "en": "You put <w>too</w> much sugar in the coffee.",
                                "ru": "Ты положил <w>слишком</w> много сахара в кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "overmatig"
                    ],
                    "common_phrases": [
                        "te veel",
                        "te laat",
                        "te gek",
                        "te klein"
                    ],
                    "target_lang_definitions": {
                        "en": "In a higher degree than is desirable, necessary, or good.",
                        "ru": "В большей степени, чем желательно, необходимо или хорошо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "too",
                                "target_lang_sense_clarification": "indicating excess"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "слишком",
                                "target_lang_sense_clarification": "указывает на избыток"
                            },
                            {
                                "target_lang_word": "чересчур",
                                "target_lang_sense_clarification": "чрезмерно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "ad3dd999-3b1e-39d0-84f0-8dda8a54413f",
                    "sense_definition": "Een grammaticaal woord dat voor het hele werkwoord (de infinitief) staat, vaak om een doel of handeling aan te geven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "grammar_marker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is leuk om je weer <w>te</w> zien.",
                            "target_lang_translations": {
                                "en": "It is nice <w>to</w> see you again.",
                                "ru": "Приятно снова тебя видеть."
                            }
                        },
                        {
                            "text": "Ik probeer het probleem <w>te</w> begrijpen.",
                            "target_lang_translations": {
                                "en": "I am trying <w>to</w> understand the problem.",
                                "ru": "Я пытаюсь понять проблему."
                            }
                        }
                    ],
                    "common_phrases": [
                        "om te",
                        "hoeven te",
                        "beginnen te",
                        "te maken hebben met"
                    ],
                    "target_lang_definitions": {
                        "en": "A grammatical word placed before the infinitive verb, often to indicate a purpose or action.",
                        "ru": "Грамматическая частица, стоящая перед инфинитивом глагола, часто указывающая на цель или действие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "infinitive marker"
                            }
                        ],
                        "ru": []
                    }
                },
                {
                    "sense_id": "68ae9563-bef4-3794-8ac4-01b951c3c6ef",
                    "sense_definition": "Per; geeft de manier of het middel aan waarmee men zich verplaatst of iets doet.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "preposition_mode",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat de bus niet kwam, gingen we <w>te</w> voet naar huis.",
                            "target_lang_translations": {
                                "en": "Because the bus didn't come, we went home <w>on</w> foot.",
                                "ru": "Так как автобус не пришел, мы пошли домой пешком."
                            }
                        },
                        {
                            "text": "De politie patrouilleert vaak <w>te</w> paard in het centrum.",
                            "target_lang_translations": {
                                "en": "The police often patrol <w>on</w> horseback in the center.",
                                "ru": "Полиция часто патрулирует центр верхом."
                            }
                        }
                    ],
                    "synonyms": [
                        "per"
                    ],
                    "common_phrases": [
                        "te voet",
                        "te paard",
                        "te water"
                    ],
                    "target_lang_definitions": {
                        "en": "By; indicates the manner or means by which one moves or does something.",
                        "ru": "Указывает на способ или средство передвижения или выполнения действия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "used with foot or horseback"
                            },
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "indicating means of transport"
                            }
                        ],
                        "ru": []
                    }
                },
                {
                    "sense_id": "0878bcc4-0fc3-3e13-b4cb-cd1471a8c2e0",
                    "sense_definition": "In of bij; wordt gebruikt om een plaats aan te duiden, voornamelijk in formele taal of bij stadsnamen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "preposition_loc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het hoofdkantoor van het bedrijf is gevestigd <w>te</w> Rotterdam.",
                            "target_lang_translations": {
                                "en": "The company's headquarters are established <w>in</w> Rotterdam.",
                                "ru": "Головной офис компании находится <w>в</w> Роттердаме."
                            }
                        },
                        {
                            "text": "De koning werd geboren <w>te</w> Utrecht.",
                            "target_lang_translations": {
                                "en": "The king was born <w>in</w> Utrecht.",
                                "ru": "Король родился <w>в</w> Утрехте."
                            }
                        }
                    ],
                    "synonyms": [
                        "in"
                    ],
                    "common_phrases": [
                        "gevestigd te",
                        "wonende te"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "In or at; used to indicate a location, mainly in formal language or with city names.",
                        "ru": "В или на; используется для обозначения местоположения, в основном в официальном языке или с названиями городов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "location (cities/towns)"
                            },
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "specific location"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "местоположение (города)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "7ebe00b9-b8e1-3a97-bb5b-122530acb628",
                    "sense_definition": "Een oude vorm van het lidwoord die alleen nog in specifieke vaste uitdrukkingen voorkomt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Loop naar <w>te</w> drommel met je onzin!",
                            "target_lang_translations": {
                                "en": "Go to <w>the</w> devil with your nonsense!",
                                "ru": "Иди к черту со своей ерундой!"
                            }
                        },
                        {
                            "text": "Dat is niet <w>te</w> geloven, wat een verhaal.",
                            "target_lang_translations": {
                                "en": "That is not <w>to</w> be believed, what a story.",
                                "ru": "В это невозможно поверить, что за история."
                            }
                        }
                    ],
                    "common_phrases": [
                        "te drommel"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "idiomatic"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An old form of the article that only occurs in specific fixed expressions.",
                        "ru": "Старая форма артикля, которая встречается только в определенных устойчивых выражениях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the",
                                "target_lang_sense_clarification": "archaic article in idioms"
                            }
                        ],
                        "ru": []
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d6a681d8-0bbc-31ba-bf51-a77bf969fe98",
                    "sense_definition": "Een uiting van spot, hoon of een beledigende opmerking.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politicus moest veel <w>schimp</w> verdragen na het schandaal.",
                            "target_lang_translations": {
                                "en": "The politician had to endure much <w>scorn</w> after the scandal.",
                                "ru": "Политику пришлось вынести много <w>оскорблений</w> после скандала."
                            }
                        },
                        {
                            "text": "Zijn toespraak zat vol <w>schimp</w> en hoon voor zijn tegenstanders.",
                            "target_lang_translations": {
                                "en": "His speech was full of <w>abuse</w> and mockery for his opponents.",
                                "ru": "Его речь была полна <w>насмешек</w> и презрения к противникам."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoon",
                        "spot",
                        "belediging",
                        "smaad"
                    ],
                    "antonyms": [
                        "lof",
                        "eerbetoon"
                    ],
                    "common_phrases": [
                        "schimp en hoon"
                    ],
                    "target_lang_definitions": {
                        "en": "An expression of mockery, scorn, or an insulting remark.",
                        "ru": "Выражение насмешки, презрения или оскорбительное замечание."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scorn",
                                "target_lang_sense_clarification": "contempt or disdain felt toward a person or object considered despicable"
                            },
                            {
                                "target_lang_word": "mockery",
                                "target_lang_sense_clarification": "teasing and contemptuous language or behavior directed at a particular person or thing"
                            },
                            {
                                "target_lang_word": "abuse",
                                "target_lang_sense_clarification": "insulting and offensive language"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "насмешка",
                                "target_lang_sense_clarification": "обидная шутка, издевка"
                            },
                            {
                                "target_lang_word": "оскорбление",
                                "target_lang_sense_clarification": "унижение чести и достоинства"
                            },
                            {
                                "target_lang_word": "поношение",
                                "target_lang_sense_clarification": "резкая критика, брань (книжн.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "326650f8-8129-34cf-8978-e90b03b64223",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>schimpen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>schimp</w> op de onrechtvaardige behandeling die wij hebben gekregen.",
                            "target_lang_translations": {
                                "en": "I <w>rail</w> against the unjust treatment we received.",
                                "ru": "Я <w>ругаюсь</w> на несправедливое отношение, с которым мы столкнулись."
                            }
                        },
                        {
                            "text": "Soms <w>schimp</w> ik uit frustratie, maar ik bedoel het niet kwaad.",
                            "target_lang_translations": {
                                "en": "Sometimes I <w>scold</w> out of frustration, but I don't mean any harm.",
                                "ru": "Иногда я <w>ругаюсь</w> от разочарования, но я не желаю зла."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'schimpen' (to rail, to scold).",
                        "ru": "Форма первого лица единственного числа настоящего времени глагола <w>schimpen</w> (ругаться, поносить)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rail",
                                "target_lang_sense_clarification": "to complain or protest strongly and persistently about"
                            },
                            {
                                "target_lang_word": "scold",
                                "target_lang_sense_clarification": "to remonstrate with or rebuke (someone) angrily"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ругаюсь",
                                "target_lang_sense_clarification": "выражаю недовольство бранными словами"
                            },
                            {
                                "target_lang_word": "поношу",
                                "target_lang_sense_clarification": "сильно ругаю, оскорбляю (книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "210cccbd-c9f7-3ee5-819d-fdecbedefdc9",
                    "sense_definition": "Gebiedende wijs van <w>schimpen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Schimp</w> niet zo op mensen die hun best doen.",
                            "target_lang_translations": {
                                "en": "Do not <w>rail</w> at people who are doing their best like that.",
                                "ru": "Не <w>ругайся</w> так на людей, которые стараются изо всех сил."
                            }
                        },
                        {
                            "text": "<w>Schimp</w> en scheld zoveel je wilt, het verandert niets aan de feiten.",
                            "target_lang_translations": {
                                "en": "<w>Rail</w> and curse all you want, it doesn't change the facts.",
                                "ru": "<w>Ругайся</w> и бранись сколько хочешь, это не изменит фактов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative mood of 'schimpen' (to rail, to scold).",
                        "ru": "Повелительное наклонение глагола <w>schimpen</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rail",
                                "target_lang_sense_clarification": "command to complain or protest strongly"
                            },
                            {
                                "target_lang_word": "scold",
                                "target_lang_sense_clarification": "command to rebuke angrily"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ругайся",
                                "target_lang_sense_clarification": "призыв выражать недовольство"
                            },
                            {
                                "target_lang_word": "злословь",
                                "target_lang_sense_clarification": "говори дурное о ком-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2eda1690-7614-3db7-820d-3c1c0390dbec",
                    "sense_definition": "(bij inversie) Tweede persoon enkelvoud tegenwoordige tijd van <w>schimpen</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waarom <w>schimp</w> je zo op die arme jongen?",
                            "target_lang_translations": {
                                "en": "Why do you <w>scold</w> that poor boy so much?",
                                "ru": "Почему ты так <w>ругаешься</w> на этого бедного мальчика?"
                            }
                        },
                        {
                            "text": "<w>Schimp</w> je nu alweer op het slechte weer?",
                            "target_lang_translations": {
                                "en": "Are you <w>railing</w> against the bad weather again?",
                                "ru": "Ты опять <w>ругаешься</w> на плохую погоду?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {

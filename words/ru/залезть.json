    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "059795a7-38e0-3210-88cd-50669b737548",
                    "sense_definition": "Взобраться, подняться наверх (на дерево, на крышу, на гору).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "climb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кот испугался собаки и быстро <залез> на высокое дерево.",
                            "target_lang_translations": {
                                "en": "The cat got scared of the dog and quickly <climbed> a tall tree."
                            }
                        },
                        {
                            "text": "Мальчик <залез> на крышу гаража, чтобы достать свой мяч.",
                            "target_lang_translations": {
                                "en": "The boy <climbed> onto the garage roof to get his ball."
                            }
                        }
                    ],
                    "synonyms": [
                        "взобраться",
                        "подняться"
                    ],
                    "antonyms": [
                        "слезть",
                        "спуститься"
                    ],
                    "common_phrases": [
                        "залезть на дерево",
                        "залезть на крышу",
                        "залезть на стол"
                    ],
                    "target_lang_definitions": {
                        "en": "To move upward to a higher position, often using hands and feet (e.g., a tree, a roof)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "climb",
                                "target_lang_sense_clarification": "general upward movement"
                            },
                            {
                                "target_lang_word": "climb up",
                                "target_lang_sense_clarification": "emphasizes the direction"
                            },
                            {
                                "target_lang_word": "get up",
                                "target_lang_sense_clarification": "colloquial, to ascend"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2a465f3-485a-3308-a417-6af02ca915af",
                    "sense_definition": "Проникнуть внутрь чего-либо, поместиться в каком-либо пространстве (часто с усилием или в тесное место).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "enter_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети играли в прятки, и один из них <залез> под кровать.",
                            "target_lang_translations": {
                                "en": "The children were playing hide-and-seek, and one of them <crawled> under the bed."
                            }
                        },
                        {
                            "text": "Вода в озере была тёплой, и мы с удовольствием <залезли> купаться.",
                            "target_lang_translations": {
                                "en": "The water in the lake was warm, and we happily <got in> to swim."
                            }
                        }
                    ],
                    "synonyms": [
                        "забраться",
                        "проникнуть"
                    ],
                    "antonyms": [
                        "вылезть"
                    ],
                    "common_phrases": [
                        "залезть под стол",
                        "залезть в воду",
                        "залезть в машину",
                        "залезть под одеяло"
                    ],
                    "target_lang_definitions": {
                        "en": "To enter inside something or fit into a space, often requiring effort or bending down."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get in",
                                "target_lang_sense_clarification": "general entry into a space or vehicle"
                            },
                            {
                                "target_lang_word": "crawl",
                                "target_lang_sense_clarification": "to move into a low or tight space"
                            },
                            {
                                "target_lang_word": "climb in",
                                "target_lang_sense_clarification": "entry requiring physical effort"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5062edfe-dde4-3498-9c40-5fceb006bcde",
                    "sense_definition": "Опустить руку во что-либо (в карман, в сумку), чтобы найти или взять что-то.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reach_into",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <залез> в карман брюк и достал оттуда ключи от квартиры.",
                            "target_lang_translations": {
                                "en": "He <reached> into his trouser pocket and took out the apartment keys."
                            }
                        },
                        {
                            "text": "Я <залезла> в сумочку, чтобы найти телефон, но его там не было.",
                            "target_lang_translations": {
                                "en": "I <reached> into my purse to find my phone, but it wasn't there."
                            }
                        }
                    ],
                    "common_phrases": [
                        "залезть в карман",
                        "залезть в сумку",
                        "залезть рукой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To put one's hand inside something (like a pocket or bag) to retrieve an object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reach into",
                                "target_lang_sense_clarification": "to extend hand inside"
                            },
                            {
                                "target_lang_word": "dig into",
                                "target_lang_sense_clarification": "to search inside with hand"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "159b2f17-e700-3011-8ed2-f9deb574b4ac",
                    "sense_definition": "Проникнуть в помещение тайно, незаконно или с целью кражи.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "illegal_entry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пока хозяев не было дома, воры <залезли> в квартиру через окно.",
                            "target_lang_translations": {
                                "en": "While the owners were not home, thieves <broke> into the apartment through the window."
                            }
                        },
                        {
                            "text": "Кто-то <залез> в наш огород ночью и украл всю клубнику.",
                            "target_lang_translations": {
                                "en": "Someone <snuck> into our garden at night and stole all the strawberries."
                            }
                        }
                    ],
                    "synonyms": [
                        "пробраться",
                        "проникнуть"
                    ],
                    "common_phrases": [
                        "залезть в дом",
                        "залезть в окно",
                        "залезть в огород"
                    ],
                    "target_lang_definitions": {
                        "en": "To enter a building or area secretly or illegally."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break into",
                                "target_lang_sense_clarification": "illegal entry"
                            },
                            {
                                "target_lang_word": "sneak into",
                                "target_lang_sense_clarification": "secret entry"
                            },
                            {
                                "target_lang_word": "climb into",
                                "target_lang_sense_clarification": "physical entry, often via window"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce8508bf-7a68-3779-9c87-76ae253d763e",
                    "sense_definition": "Оказаться в неприятной ситуации или обременительном положении (переносное значение).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_trouble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Чтобы купить новую машину, он <залез> в большие долги.",
                            "target_lang_translations": {
                                "en": "To buy a new car, he <got> into heavy debt."
                            }
                        },
                        {
                            "text": "Фирма <залезла> в кредиты и теперь не может расплатиться с банком.",
                            "target_lang_translations": {
                                "en": "The firm <got> into loans and now cannot pay the bank back."
                            }
                        }
                    ],
                    "synonyms": [
                        "попасть"
                    ],
                    "common_phrases": [
                        "залезть в долги",
                        "залезть в кредиты"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become involved in a difficult situation or financial burden."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get into",
                                "target_lang_sense_clarification": "debt or trouble"
                            },
                            {
                                "target_lang_word": "run up",
                                "target_lang_sense_clarification": "specifically for debts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a74c376-7c5e-3d2b-8df6-caf7850b5e38",
                    "sense_definition": "Вмешаться в чужие дела, разговоры или сферу деятельности без спроса.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "meddle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он случайно <залез> в настройки компьютера и сбил все программы.",
                            "target_lang_translations": {
                                "en": "He accidentally <got> into the computer settings and messed up all the programs."
                            }
                        },
                        {
                            "text": "Не стоит <залезть> в душу человеку, если он сам не хочет рассказывать о проблемах.",
                            "target_lang_translations": {
                                "en": "You shouldn't <pry> into a person's soul if they don't want to talk about their problems themselves."
                            }
                        }
                    ],
                    "synonyms": [
                        "вмешаться"
                    ],
                    "common_phrases": [
                        "залезть в душу",
                        "залезть в настройки",
                        "залезть в телефон"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To interfere with something or intrude into privacy without permission."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mess with",
                                "target_lang_sense_clarification": "settings or devices"
                            },
                            {
                                "target_lang_word": "pry into",
                                "target_lang_sense_clarification": "personal matters or soul"
                            },
                            {
                                "target_lang_word": "get into",
                                "target_lang_sense_clarification": "accessing settings or systems"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0754cfb2-e234-344e-be64-467830ad2586",
                    "sense_definition": "A term of endearment or friendly form of address, short for <w>honey</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "endearment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you pass me the sugar, <w>hon</w>?",
                            "target_lang_translations": {
                                "ru": "Можешь передать мне сахар, <w>дорогуша</w>?"
                            }
                        },
                        {
                            "text": "Don't worry about it, <w>hon</w>, everything will be fine.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся об этом, <w>милая</w>, всё будет хорошо."
                            }
                        }
                    ],
                    "synonyms": [
                        "honey",
                        "sweetheart",
                        "dear",
                        "love",
                        "darling"
                    ],
                    "common_phrases": [
                        "thanks hon",
                        "hi hon",
                        "listen hon"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the US, particularly in the South and Baltimore"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое или дружеское обращение, сокращение от «honey» (милый/милая)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дорогуша",
                                "target_lang_sense_clarification": "разговорное, фамильярное"

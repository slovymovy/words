    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "23eb9399-cfcb-3b87-89c7-ca95daf7746d",
                    "sense_definition": "Substancja o gęstej, półpłynnej i kleistej konsystencji, używana do czyszczenia, pielęgnacji lub konserwacji różnych powierzchni.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_consistency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano i wieczorem nakładam <pastę> na szczoteczkę, aby umyć zęby.",
                            "target_lang_translations": {
                                "ru": "Каждое утро и каждый вечер я наношу <пасту> на щетку, чтобы почистить зубы."
                            }
                        },
                        {
                            "text": "Muszę kupić czarną <pastę> do butów, ponieważ moje eleganckie półbuty straciły blask.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить черный <крем> для обуви, так как мои элегантные туфли потеряли блеск."
                            }
                        }
                    ],
                    "synonyms": [
                        "maź"
                    ],
                    "common_phrases": [
                        "pasta do zębów",
                        "pasta do butów",
                        "pasta polerska",
                        "pasta czyszcząca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество густой, полужидкой и клейкой консистенции, используемое для чистки, ухода или защиты различных поверхностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паста",
                                "target_lang_sense_clarification": "для чистки или зубов"
                            },
                            {
                                "target_lang_word": "крем",
                                "target_lang_sense_clarification": "для обуви"
                            },
                            {
                                "target_lang_word": "мазь",
                                "target_lang_sense_clarification": "густая субстанция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55a55a61-1278-3e9b-89e4-079a45477714",
                    "sense_definition": "Gęsta masa spożywcza zrobiona z rozdrobnionych składników, przeznaczona zazwyczaj do smarowania pieczywa.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_consistency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na śniadanie przygotowałem pyszną <pastę> jajeczną ze szczypiorkiem.",
                            "target_lang_translations": {
                                "ru": "На завтрак я приготовил вкусную яичную <пасту> с зеленым луком."
                            }
                        },
                        {
                            "text": "Kanapki z <pastą> rybną są zdrową i pożywną przekąską do pracy.",
                            "target_lang_translations": {
                                "ru": "Бутерброды с рыбной <пастой> — это здоровая и питательная закуска на работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pasta jajeczna",
                        "pasta rybna",
                        "pasta twarogowa",
                        "kanapka z pastą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Густая пищевая масса из измельченных ингредиентов, обычно предназначенная для намазывания на хлеб."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паста",
                                "target_lang_sense_clarification": "пищевая"
                            },
                            {
                                "target_lang_word": "паштет",
                                "target_lang_sense_clarification": "мясной или рыбный"
                            },
                            {
                                "target_lang_word": "намазка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "553ce472-25ea-3883-b049-3277aaf6a0de",
                    "sense_definition": "Danie kuchni włoskiej przygotowane z makaronu i sosu; określenie stosowane także wobec samego makaronu w stylu włoskim.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej włoskiej restauracji serwują wyśmienitą <pastę> z owocami morza.",
                            "target_lang_translations": {
                                "ru": "В этом итальянском ресторане подают отменную <пасту> с морепродуктами."
                            }
                        },
                        {
                            "text": "Na dzisiejszy obiad ugotowałem <pastę> z sosem pomidorowym i świeżą bazylią.",
                            "target_lang_translations": {
                                "ru": "На сегодняшний обед я приготовил <пасту> с томатным соусом и свежим базиликом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "włoska pasta",
                        "świeża pasta",
                        "pasta z sosem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Блюдо итальянской кухни из макаронных изделий с соусом; также название самих макарон в итальянском стиле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паста",
                                "target_lang_sense_clarification": "блюдо"
                            },
                            {
                                "target_lang_word": "макароны",
                                "target_lang_sense_clarification": "изделия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e117bf50-02ce-3d72-8461-64ca3ccd1628",
                    "sense_definition": "Zmyślona, często absurdalna lub humorystyczna historia, która jest wielokrotnie kopiowana i wklejana na forach internetowych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Użytkownicy forum internetowego ciągle wklejają tę samą <pastę> pod każdym nowym postem.",
                            "target_lang_translations": {
                                "ru": "Пользователи интернет-форума постоянно вставляют одну и ту же <пасту> под каждым новым постом."
                            }
                        },
                        {
                            "text": "Przeczytałem wczoraj zabawną <pastę> o wizycie w sklepie budowlanym, która stała się wiralem.",
                            "target_lang_translations": {
                                "ru": "Вчера я прочитал забавную <пасту> о визите в строительный магазин, которая стала вирусной."
                            }
                        }
                    ],
                    "synonyms": [

                    "synonyms": [
                        "nuclear"
                    ],
                    "common_phrases": [
                        "atomic bomb",
                        "atomic energy",
                        "atomic power",
                        "atomic age"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использующий ядерную энергию или относящийся к оружию, основанному на расщеплении атома."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атомный",
                                "target_lang_sense_clarification": "ядерный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48feb163-b1d2-30ea-8f4f-8655f2843935",
                    "sense_definition": "Considered as a single unit that cannot be divided or split into smaller parts.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "indivisible",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this logic system, a simple statement is treated as an <w>atomic</w> fact.",
                            "target_lang_translations": {
                                "ru": "В этой логической системе простое утверждение рассматривается как <w>атомарный</w> факт."
                            }
                        },
                        {
                            "text": "A bit is an <w>atomic</w> item of data that cannot be broken down further.",
                            "target_lang_translations": {
                                "ru": "Бит — это <w>атомарная</w> единица данных, которую нельзя разбить на части."
                            }
                        }
                    ],
                    "synonyms": [
                        "indivisible"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассматриваемый как единое целое, которое невозможно разделить на более мелкие части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атомарный",
                                "target_lang_sense_clarification": "неделимый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d843636-c46a-3d4b-8d46-2000c7523a99",
                    "sense_definition": "Guaranteed to complete fully without interruption or not happen at all, preventing inconsistent states in computing.",

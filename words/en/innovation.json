    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1defeefa-d970-3e78-aeb3-f427413ccb63",
                    "sense_definition": "The act or process of introducing new ideas, devices, or methods.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company encourages creativity and <w>innovation</w> in all its departments.",
                            "target_lang_translations": {
                                "ru": "Компания поощряет творчество и <w>новаторство</w> во всех своих отделах."
                            }
                        },
                        {
                            "text": "Technological <w>innovation</w> has rapidly changed how we communicate with each other.",
                            "target_lang_translations": {
                                "ru": "Технологические <w>инновации</w> стремительно изменили то, как мы общаемся друг с другом."
                            }
                        }
                    ],
                    "synonyms": [
                        "introduction",
                        "modernization",
                        "development"
                    ],
                    "antonyms": [
                        "exnovation"
                    ],
                    "common_phrases": [
                        "technological innovation",
                        "foster innovation",
                        "drive innovation",
                        "constant innovation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс внедрения новых идей, устройств или методов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инновация",
                                "target_lang_sense_clarification": "процесс создания и внедрения новшеств"
                            },
                            {
                                "target_lang_word": "новаторство",
                                "target_lang_sense_clarification": "деятельность по внедрению нового"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb3f3455-73ca-34e9-b76f-7ac840ae3f4d",
                    "sense_definition": "A new idea, method, or device that has been created or introduced.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The smartphone was a major <w>innovation</w> that revolutionized the industry.",
                            "target_lang_translations": {
                                "ru": "Смартфон стал крупной <w>инновацией</w>, совершившей революцию в отрасли."
                            }
                        },
                        {
                            "text": "We are looking for practical <w>innovations</w> to improve energy efficiency in our homes.",
                            "target_lang_translations": {
                                "ru": "Мы ищем практические <w>нововведения</w> для повышения энергоэффективности в наших домах."
                            }
                        }
                    ],
                    "synonyms": [
                        "invention",
                        "novelty",
                        "improvement"
                    ],
                    "common_phrases": [
                        "latest innovation",
                        "major innovation",
                        "product innovation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Новая идея, метод или устройство, которое было создано или внедрено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нововведение",
                                "target_lang_sense_clarification": "вновь введенное правило, метод или устройство"
                            },
                            {
                                "target_lang_word": "новшество",
                                "target_lang_sense_clarification": "что-либо новое, недавно созданное"
                            },
                            {
                                "target_lang_word": "инновация",
                                "target_lang_sense_clarification": "новый продукт или технология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "113f429b-517f-3de6-9cea-3fa36f54c1e5",
                    "sense_definition": "A newly formed shoot on a plant, especially the annual growth on the stem of a moss.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The botanist carefully examined the green <w>innovation</w> growing from the old stem.",
                            "target_lang_translations": {
                                "ru": "Ботаник внимательно изучил зеленый <w>побег</w>, растущий от старого стебля."
                            }
                        },
                        {
                            "text": "Each spring, a fresh <w>innovation</w> appears, adding to the height of the plant.",
                            "target_lang_translations": {
                                "ru": "Каждую весну появляется новый <w>побег</w>, прибавляющий растению высоты."
                            }
                        }
                    ],
                    "synonyms": [
                        "shoot",
                        "growth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Недавно образовавшийся побег на растении, особенно годичный прирост на стебле мха."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]
}
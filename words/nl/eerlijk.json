{
    "word_family": [
        "eer",
        "eerlijkheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5d099ff4-3fa6-3b78-b9bc-b9e21825982c",
                    "sense_definition": "Vrij van leugen en bedrog; de waarheid sprekend.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "honest_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wees <w>eerlijk</w> en vertel de waarheid!",
                            "target_lang_translations": {
                                "en": "Be <w>honest</w> and tell the truth!",
                                "ru": "Будь <w>честным</w> и скажи правду!"
                            }
                        },
                        {
                            "text": "Hij gaf een <w>eerlijk</w> antwoord op de moeilijke vraag.",
                            "target_lang_translations": {
                                "en": "He gave an <w>honest</w> answer to the difficult question.",
                                "ru": "Он дал <w>честный</w> ответ на трудный вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "oprecht",
                        "waarheidsgetrouw"
                    ],
                    "antonyms": [
                        "oneerlijk",
                        "vals"
                    ],
                    "common_phrases": [
                        "eerlijk is eerlijk",
                        "eerlijk duurt het langst"
                    ],
                    "target_lang_definitions": {
                        "en": "Free from lies and deceit; telling the truth.",
                        "ru": "Свободный от лжи и обмана; говорящий правду."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "honest",
                                "target_lang_sense_clarification": "telling the truth"
                            },
                            {
                                "target_lang_word": "truthful",
                                "target_lang_sense_clarification": "corresponding to reality"
                            },
                            {
                                "target_lang_word": "sincere",
                                "target_lang_sense_clarification": "genuine, not feigned"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "честный",
                                "target_lang_sense_clarification": "правдивый, не лгущий"
                            },
                            {
                                "target_lang_word": "искренний",
                                "target_lang_sense_clarification": "выражающий подлинные чувства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7e05e5e-274c-32a5-a568-647fe96f2fc2",
                    "sense_definition": "Rechtvaardig; volgens de regels en zonder iemand te benadelen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fair_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De scheidsrechter nam een <w>eerlijke</w> beslissing tijdens de wedstrijd.",
                            "target_lang_translations": {
                                "en": "The referee made a <w>fair</w> decision during the match.",
                                "ru": "Судья принял <w>справедливое</w> решение во время матча."
                            }
                        },
                        {
                            "text": "Het is niet <w>eerlijk</w> dat zij meer snoep krijgt dan ik.",
                            "target_lang_translations": {
                                "en": "It is not <w>fair</w> that she gets more candy than I do.",
                                "ru": "Это не <w>справедливо</w>, что она получает больше конфет, чем я."
                            }
                        }
                    ],
                    "antonyms": [
                        "oneerlijk"
                    ],
                    "common_phrases": [
                        "niet eerlijk"
                    ],
                    "target_lang_definitions": {
                        "en": "Just; according to rules and without disadvantaging anyone.",
                        "ru": "Справедливый; действующий по правилам и без ущерба для других."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fair",
                                "target_lang_sense_clarification": "just and impartial"
                            },
                            {
                                "target_lang_word": "just",
                                "target_lang_sense_clarification": "morally right"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "справедливый",
                                "target_lang_sense_clarification": "беспристрастный, правильный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7705aba4-9f1c-38af-975f-968517549b2e",
                    "sense_definition": "Op een oprechte of rechtvaardige manier.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "honest_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Eerlijk</w> gezegd kwam het nieuws niet als een verrassing.",
                            "target_lang_translations": {
                                "en": "<w>Honestly</w>, the news did not come as a surprise.",
                                "ru": "<w>Честно</w> говоря, эта новость не стала сюрпризом."
                            }
                        },
                        {
                            "text": "We zullen de taart <w>eerlijk</w> verdelen onder alle kinderen.",
                            "target_lang_translations": {
                                "en": "We will divide the cake <w>fairly</w> among all the children.",
                                "ru": "Мы <w>справедливо</w> разделим торт между всеми детьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "oprecht"
                    ],
                    "antonyms": [
                        "oneerlijk"
                    ],
                    "common_phrases": [
                        "eerlijk gezegd",
                        "eerlijk delen"
                    ],
                    "target_lang_definitions": {
                        "en": "In a sincere or just manner.",
                        "ru": "Искренне или справедливо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "honestly",
                                "target_lang_sense_clarification": "sincerely, really"
                            },
                            {
                                "target_lang_word": "fairly",
                                "target_lang_sense_clarification": "equitably, in a just way"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "честно",
                                "target_lang_sense_clarification": "правдиво, без обмана"
                            },
                            {
                                "target_lang_word": "откровенно",
                                "target_lang_sense_clarification": "прямо, не скрывая мыслей"
                            },
                            {
                                "target_lang_word": "справедливо",
                                "target_lang_sense_clarification": "по правилам, поровну"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            },
                            {
                                "target_lang_word": "глубоководный аппарат",
                                "target_lang_sense_clarification": "для исследований глубин"
                            },
                            {
                                "target_lang_word": "батискаф",
                                "target_lang_sense_clarification": "тип глубоководного аппарата"
                            }
                        ]
                    }
                }
            ]
        }

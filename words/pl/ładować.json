            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c8eedb25-a9d8-3a0d-a7b3-38cfa66dac41",
                    "sense_definition": "Dostarczać energię elektryczną do baterii lub akumulatora urządzenia, aby mogło ono działać.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "electricity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę codziennie <ładować> mój telefon, bo bateria jest słaba.",
                            "target_lang_translations": {
                                "ru": "Мне нужно каждый день <заряжать> телефон, потому что батарея слабая."
                            }
                        },
                        {
                            "text": "Gdzie mogę <ładować> laptopa na tym lotnisku?",
                            "target_lang_translations": {
                                "ru": "Где я могу <зарядить> ноутбук в этом аэропорту?"
                            }
                        }
                    ],
                    "antonyms": [
                        "rozładowywać"
                    ],
                    "common_phrases": [
                        "ładować telefon",
                        "ładować baterię",
                        "ładować akumulator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать электричеством батарею или аккумулятор устройства для его работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заряжать",
                                "target_lang_sense_clarification": "аккумулятор, устройство"

                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape_geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The flare <w>arced</w> across the night sky, illuminating the forest below.",
                            "target_lang_translations": {

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1e949d09-2fad-3316-855b-26e86855df97",
                    "sense_definition": "Having or showing a sharp, perceptive, and shrewd mind.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor was known for his <w>argute</w> analysis of ancient texts.",
                            "target_lang_translations": {
                                "ru": "Профессор был известен своим <w>проницательным</w> анализом древних текстов."
                            }
                        },
                        {
                            "text": "She showed an <w>argute</w> judgment when selecting the best candidate for the job.",
                            "target_lang_translations": {
                                "ru": "Она продемонстрировала <w>острое</w> суждение при выборе лучшего кандидата на эту должность."
                            }
                        }
                    ],
                    "synonyms": [
                        "astute",
                        "shrewd",
                        "perceptive",
                        "acute"
                    ],
                    "antonyms": [
                        "obtuse",
                        "dull"
                    ],
                    "common_phrases": [
                        "argute judgment",
                        "argute observation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий острым, проницательным и сообразительным умом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проницательный",
                                "target_lang_sense_clarification": "способный быстро и точно понимать суть вещей"
                            },
                            {
                                "target_lang_word": "острый",
                                "target_lang_sense_clarification": "об уме или восприятии"
                            },
                            {
                                "target_lang_word": "сообразительный",
                                "target_lang_sense_clarification": "быстро схватывающий суть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "360c1d8a-ce23-3419-aeab-6d93111b0c11",
                    "sense_definition": "Describing a sound that is shrill, sharp, or piercing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>argute</w> cry of the eagle echoed across the valley.",
                            "target_lang_translations": {
                                "ru": "<w>Пронзительный</w> крик орла эхом разнёсся по долине."
                            }
                        },
                        {
                            "text": "Suddenly, an <w>argute</w> voice pierced the silence of the library.",
                            "target_lang_translations": {
                                "ru": "Внезапно <w>резкий</w> голос пронзил тишину библиотеки."
                            }
                        }
                    ],
                    "synonyms": [
                        "shrill",
                        "piercing",
                        "sharp"
                    ],
                    "antonyms": [
                        "deep",
                        "muffled"
                    ],
                    "common_phrases": [
                        "argute voice",
                        "argute cry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывает звук: пронзительный, резкий или визгливый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пронзительный",
                                "target_lang_sense_clarification": "о звуке или крике"
                            },
                            {
                                "target_lang_word": "резкий",
                                "target_lang_sense_clarification": "о голосе или звуке"
                            }
                        ]
                    }
                }
            ]
        }

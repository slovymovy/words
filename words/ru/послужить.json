    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a1fe1b66-9a27-3bcf-b45a-21b3fa4b20ec",
                    "sense_definition": "Стать чем-либо, выполнить роль или функцию чего-либо (причины, основания, примера).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его смелый поступок должен <w>послужить</w> примером для всех нас.",
                            "target_lang_translations": {
                                "en": "His brave act should <w>serve as</w> an example for all of us."
                            }
                        },
                        {
                            "text": "Это небольшое недоразумение <w>послужило</w> поводом для серьёзной ссоры.",
                            "target_lang_translations": {
                                "en": "This slight misunderstanding <w>served as</w> a pretext for a serious quarrel."
                            }
                        }
                    ],
                    "synonyms": [
                        "стать",
                        "явиться",
                        "оказаться"
                    ],
                    "common_phrases": [
                        "послужить причиной",
                        "послужить примером",
                        "послужить поводом",
                        "послужить основой",
                        "послужить уроком"
                    ],
                    "target_lang_definitions": {
                        "en": "To become something, to fulfill the role or function of something (such as a cause, basis, or example)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve as",
                                "target_lang_sense_clarification": "to function or act in a specific capacity (e.g., as an example or cause)"
                            },
                            {
                                "target_lang_word": "act as",
                                "target_lang_sense_clarification": "to fulfill a specific function"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8ca3cc6-92e4-3c59-a53d-63f6ef6f4334",
                    "sense_definition": "Провести некоторое время, выполняя обязанности по службе, работая где-либо или будучи пригодным для использования.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он хотел <w>послужить</w> своей стране и записался в армию добровольцем.",
                            "target_lang_translations": {
                                "en": "He wanted to <w>serve</w> his country and enlisted in the army as a volunteer."
                            }
                        },
                        {
                            "text": "Это старое пальто ещё может <w>послужить</w> тебе на даче.",
                            "target_lang_translations": {
                                "en": "This old coat can still <w>serve</w> you at the dacha."
                            }
                        }
                    ],
                    "synonyms": [
                        "прослужить",
                        "поработать"
                    ],
                    "common_phrases": [
                        "послужить в армии",
                        "послужить отечеству",
                        "послужить людям"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend some time performing duties, working, or being of use."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "to perform duties or be of use for a period of time"
                            },
                            {
                                "target_lang_word": "be of service",
                                "target_lang_sense_clarification": "to be helpful or useful"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
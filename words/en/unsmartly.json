            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e55d5cad-f553-31fd-91f4-5f2825eacd1e",
                    "sense_definition": "In a way that looks untidy, shoddy, or unfashionable.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The guests were surprised to see him <w>unsmartly</w> dressed for such a formal gala.",
                            "target_lang_translations": {
                                "ru": "Гости были удивлены, увидев его <w>неопрятно</w> одетым для такого официального торжества."
                            }
                        },
                        {
                            "text": "She felt <w>unsmartly</w> attired in her casual clothes among the business executives.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала, что одета <w>немодно</w> в своей повседневной одежде среди бизнесменов."
                            }
                        }
                    ],
                    "synonyms": [
                        "shabbily",
                        "dowdily",
                        "sloppily",
                        "untidily"
                    ],
                    "antonyms": [
                        "smartly",
                        "stylishly",
                        "neatly",
                        "fashionably"
                    ],
                    "common_phrases": [
                        "unsmartly dressed",
                        "unsmartly attired"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопрятно, немодно или безвкусно (обычно о внешнем виде или одежде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неопрятно",
                                "target_lang_sense_clarification": "неаккуратно, неряшливо"
                            },
                            {
                                "target_lang_word": "немодно",
                                "target_lang_sense_clarification": "не стильно, устарело"
                            },
                            {
                                "target_lang_word": "безвкусно",
                                "target_lang_sense_clarification": "без чувства стиля"
                            },
                            {
                                "target_lang_word": "неряшливо",
                                "target_lang_sense_clarification": "небрежно в одежде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7b5348c-2b1c-3e82-8680-7d526f3b5cc7",
                    "sense_definition": "In a way that shows a lack of intelligence or good judgment.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "intelligence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>unsmartly</w> decided to argue with the police officer instead of staying calm.",
                            "target_lang_translations": {
                                "ru": "Он <w>неразумно</w> решил поспорить с полицейским вместо того, чтобы сохранять спокойствие."
                            }
                        },
                        {
                            "text": "The team played <w>unsmartly</w> in the final minutes and lost the game.",
                            "target_lang_translations": {
                                "ru": "Команда сыграла <w>бестолково</w> на последних минутах и проиграла матч."
                            }
                        }
                    ],
                    "synonyms": [
                        "foolishly",
                        "stupidly",
                        "unwisely",
                        "unintelligently"
                    ],
                    "antonyms": [
                        "smartly",
                        "intelligently",
                        "wisely",
                        "cleverly"
                    ],
                    "common_phrases": [
                        "acted unsmartly",
                        "played unsmartly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неразумно, глупо или опрометчиво; демонстрируя отсутствие сообразительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неразумно",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "45c0fefa-161f-3714-a864-9c51edc2272f",
                    "sense_definition": "A plant or part of a plant used as food, such as a cabbage, potato, carrot, or bean.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Eating fresh <w>vegetables</w> is essential for a healthy diet.",
                            "target_lang_translations": {
                                "ru": "Употребление в пищу свежих <w>овощей</w> необходимо для здорового питания."
                            }
                        },
                        {
                            "text": "We grow our own <w>vegetables</w> in the garden every summer.",
                            "target_lang_translations": {
                                "ru": "Каждое лето мы выращиваем свои собственные <w>овощи</w> в огороде."
                            }
                        }
                    ],
                    "synonyms": [
                        "veg",
                        "veggie"
                    ],
                    "common_phrases": [
                        "fresh vegetables",
                        "root vegetables",
                        "green vegetables",
                        "fruit and vegetables"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение или часть растения, употребляемые в пищу, например, капуста, картофель, морковь или фасоль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о́вощ",
                                "target_lang_sense_clarification": "съедобное растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40df23ca-a46c-3017-b810-a85c8a52c8a3",
                    "sense_definition": "Any living organism belonging to the plant kingdom, as distinct from an animal or mineral.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>vegetable</w> kingdom includes trees, flowers, and ferns.",
                            "target_lang_translations": {
                                "ru": "<w>Растительное</w> царство включает в себя деревья, цветы и папоротники."
                            }
                        },
                        {
                            "text": "Decaying <w>vegetable</w> matter helps to enrich the soil.",
                            "target_lang_translations": {
                                "ru": "Гниющие <w>растительные</w> остатки помогают обогатить почву."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vegetable kingdom",
                        "vegetable matter"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "scientific or formal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой живой организм, относящийся к царству растений, в отличие от животного или минерала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расте́ние",
                                "target_lang_sense_clarification": "организм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9dca448-2db5-3cf8-b28a-de4eff03b296",
                    "sense_definition": "A person who is unable to think, speak, or move due to severe brain damage.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tragic accident left the driver a <w>vegetable</w> for the rest of his life.",
                            "target_lang_translations": {
                                "ru": "Трагическая авария превратила водителя в <w>овощ</w> на всю оставшуюся жизнь."
                            }
                        },
                        {
                            "text": "He is terrified of the possibility of becoming a <w>vegetable</w> on life support.",
                            "target_lang_translations": {
                                "ru": "Он до смерти боится возможности стать <w>овощем</w> на аппарате жизнеобеспечения."
                            }
                        }
                    ],
                    "synonyms": [
                        "cabbage"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often offensive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который не может думать, говорить или двигаться из-за тяжелого повреждения мозга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о́вощ",
                                "target_lang_sense_clarification": "перен., разг."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "693bd47c-9e3e-3317-bac5-b5c5917d52fd",

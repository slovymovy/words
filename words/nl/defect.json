{
    "word_family": [
        "defectueus"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "267aff63-d6db-3064-9673-616fa05ba0db",
                    "sense_definition": "Niet meer werkend; kapot (meestal gezegd van apparaten of machines).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>defecte</w> apparaat kon niet meer gerepareerd worden.",
                            "target_lang_translations": {
                                "ru": "<w>Неисправный</w> прибор уже нельзя было починить."
                            }
                        },
                        {
                            "text": "De lift is tijdelijk <w>defect</w>, dus we moeten de trap nemen.",
                            "target_lang_translations": {
                                "ru": "Лифт временно <w>неисправен</w>, поэтому нам придется идти по лестнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapot",
                        "stuk",
                        "kaduuk"
                    ],
                    "antonyms": [
                        "heel",
                        "werkend"
                    ],
                    "common_phrases": [
                        "defect raken",
                        "tijdelijk defect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не работающий; сломанный (обычно говорится об устройствах или машинах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неисправный",
                                "target_lang_sense_clarification": "имеющий повреждения, не работающий"
                            },
                            {
                                "target_lang_word": "сломанный",
                                "target_lang_sense_clarification": "вышедший из строя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d424abd2-e174-3974-9ce3-8e596b692526",
                    "sense_definition": "Een storing, mankement of beschadiging waardoor iets niet goed functioneert.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>defecten</w> werden provisorisch verholpen door de technische dienst.",
                            "target_lang_translations": {
                                "ru": "<w>Неисправности</w> были временно устранены технической службой."
                            }
                        },
                        {
                            "text": "De monteur zocht lang naar het <w>defect</w> in de motor.",
                            "target_lang_translations": {
                                "ru": "Механик долго искал <w>поломку</w> в двигателе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een defect verhelpen",
                        "een technisch defect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбой, изъян или повреждение, из-за которого что-то не функционирует должным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неисправность",
                                "target_lang_sense_clarification": "нарушение в работе механизма"
                            },
                            {
                                "target_lang_word": "дефект",
                                "target_lang_sense_clarification": "изъян, недостаток"
                            },
                            {
                                "target_lang_word": "поломка",
                                "target_lang_sense_clarification": "повреждение, приведшее к отказу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
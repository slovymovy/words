    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1ec744a-abc4-3386-9930-4e9af9036927",
                    "sense_definition": "Starszy mężczyzna, często postrzegany jako zrzędliwy, niechlujny lub antypatyczny; określenie potoczne i lekceważące.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "person_desc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary <dziad> znowu krzyczy na dzieci bawiące się pod blokiem.",
                            "target_lang_translations": {
                                "ru": "Этот старый <дед> снова кричит на детей, играющих под домом."
                            }
                        },
                        {
                            "text": "Nie bądź takim <dziadem>, pożycz mi trochę pieniędzy na bilet.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <жмотом>, одолжи мне немного денег на билет."
                            }
                        }
                    ],
                    "synonyms": [
                        "grzyb"
                    ],
                    "common_phrases": [
                        "stary dziad",
                        "zrzędliwy dziad"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "often used as an insult"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пожилой мужчина, часто воспринимаемый как ворчливый, неопрятный или неприятный; разговорное и пренебрежительное определение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дед",
                                "target_lang_sense_clarification": "разговорное, часто пренебрежительное"
                            },
                            {
                                "target_lang_word": "старикашка",
                                "target_lang_sense_clarification": "уничижительное"
                            },
                            {
                                "target_lang_word": "старый хрыч",
                                "target_lang_sense_clarification": "бранное, идиоматическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0102c8b-d949-37ad-a941-d0013c3ef884",
                    "sense_definition": "Przodek, członek rodu z dawnych pokoleń; słowo używane głównie w kontekście historycznym lub w utartych zwrotach.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "family_ancestry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To gospodarstwo należy do naszej rodziny z <dziada> pradziada.",
                            "target_lang_translations": {
                                "ru": "Это хозяйство принадлежит нашей семье от <деда>-прадеда."
                            }
                        },
                        {
                            "text": "Nasi <dziadowie> walczyli o wolność tego kraju w wielu powstaniach.",
                            "target_lang_translations": {
                                "ru": "Наши <деды> сражались за свободу этой страны во многих восстаниях."
                            }
                        }
                    ],
                    "synonyms": [
                        "przodek",
                        "praojciec",
                        "antenat",
                        "praszczur"
                    ],
                    "antonyms": [
                        "wnuk"
                    ],
                    "common_phrases": [
                        "z dziada pradziada",
                        "nasi dziadowie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предок, член рода из прошлых поколений; слово используется в основном в историческом контексте или в устойчивых выражениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предок",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "дед",
                                "target_lang_sense_clarification": "в значении предок, обычно во мн. ч."
                            },
                            {
                                "target_lang_word": "пращур",
                                "target_lang_sense_clarification": "книжное, устаревшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf691a00-d689-3da7-8e30-f62204a1ba7f",
                    "sense_definition": "Człowiek bardzo ubogi, żyjący z jałmużny, często wędrowny.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod kościołem siedział stary <dziad> i prosił przechodniów o drobne.",
                            "target_lang_translations": {
                                "ru": "У церкви сидел старый <нищий> и просил у прохожих мелочь."
                            }
                        },
                        {
                            "text": "W dawnych czasach wędrowny <dziad> przynosił nowiny ze świata do wsi.",
                            "target_lang_translations": {
                                "ru": "В давние времена странствующий <нищий> приносил новости из мира в деревню."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzep",
                        "czep"
                    ],
                    "common_phrases": [
                        "dziad proszalny",
                        "torba dziada"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly historical or literary context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень бедный человек, живущий подаянием, часто странствующий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нищий",
                                "target_lang_sense_clarification": "человек, живущий подаянием"
                            },
                            {
                                "target_lang_word": "попрошайка",
                                "target_lang_sense_clarification": "разговорное, назойливый проситель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c62d4b5-cec3-3da3-987e-f0bc5ad25ae0",
                    "sense_definition": "Dawny ludowy obrzęd poświęcony duchom zmarłych przodków, polegający na ich przywoływaniu i ugaszczaniu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawni Słowianie obchodzili <dziady>, przynosząc jedzenie na groby zmarłych.",
                            "target_lang_translations": {
                                "ru": "Древние славяне отмечали <Дзяды>, принося еду на могилы умерших."
                            }
                        },
                        {
                            "text": "W szkole omawialiśmy drugą część Dziadów, która opisuje ten pogański obrzęd.",
                            "target_lang_translations": {
                                "ru": "В школе мы обсуждали вторую часть «<Дзядов>», которая описывает этот языческий обряд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obrzęd dziadów",
                        "noc dziadów"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "refers to historical customs"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Древний народный обряд, посвященный духам умерших предков, заключающийся в их призывании и угощении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дзяды",
                                "target_lang_sense_clarification": "название обряда и произведения Мицкевича"
                            },
                            {
                                "target_lang_word": "Деды",
                                "target_lang_sense_clarification": "этнографическое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "187d7a8d-67fa-30fc-ac5b-b1cbd72dcad3",
                    "sense_definition": "Owocostan niektórych roślin (np. łopianu) wyposażony w haczyki, który łatwo przyczepia się do ubrania lub sierści zwierząt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój pies wrócił ze spaceru po łące cały oblepiony kłującymi <dziadami>.",
                            "target_lang_translations": {
                                "ru": "Моя собака вернулась с прогулки по лугу, вся облепленная колючими <репьями>."
                            }
                        },
                        {
                            "text": "Musiałem długo wyciągać te małe <dziady> z wełnianego swetra.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось долго вытаскивать эти маленькие <колючки> из шерстяного свитера."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzep",
                        "czep"
                    ],
                    "common_phrases": [
                        "czepiać się jak dziad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соплодие некоторых растений (например, лопуха) с крючками, которое легко цепляется к одежде или шерсти животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репей",
                                "target_lang_sense_clarification": "ботаническое/общее"
                            },
                            {
                                "target_lang_word": "колючка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43f3dd7f-0355-38b6-8683-1f2240305a9c",
                    "sense_definition": "Ojciec ojca lub matki (forma obecnie rzadka, zastąpiona słowem dziadek).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "family_ancestry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <dziad> zawsze powtarzał, że uczciwa praca jest najważniejsza.",
                            "target_lang_translations": {
                                "ru": "Мой <дед> всегда повторял, что честный труд важнее всего."
                            }
                        },
                        {
                            "text": "Jako mały chłopiec często słuchałem opowieści, które snuł mój stary <dziad>.",
                            "target_lang_translations": {
                                "ru": "Маленьким мальчиком я часто слушал истории, которые рассказывал мой старый <дед>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziadek"
                    ],
                    "antonyms": [
                        "wnuk"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "standard term is dziadek"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "used in some regions"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отец отца или матери (форма, ныне редкая, замененная словом dziadek)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дед",
                                "target_lang_sense_clarification": "родственник"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
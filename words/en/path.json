{
    "word_family": [
        "pathway",
        "footpath",
        "bypath",
        "towpath",
        "pathless",
        "pathfinder"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8d63ea5-f6b2-39ef-a5cd-148229ec7d2f",
                    "sense_definition": "A trail or track laid down for walking or made by continual treading.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_route",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We walked along the narrow <w>path</w> through the dense forest.",
                            "target_lang_translations": {
                                "ru": "Мы шли по узкой <w>тропинке</w> через густой лес."
                            }
                        },
                        {
                            "text": "The garden <w>path</w> leads directly to the back door of the house.",
                            "target_lang_translations": {
                                "ru": "Садовая <w>дорожка</w> ведет прямо к задней двери дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "trail",
                        "track",
                        "way"
                    ],
                    "common_phrases": [
                        "garden path",
                        "mountain path",
                        "narrow path",
                        "follow the path",
                        "walking path"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тропа или дорожка, проложенная для ходьбы или образовавшаяся от постоянного хождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тропинка",
                                "target_lang_sense_clarification": "узкая пешеходная дорожка"
                            },
                            {
                                "target_lang_word": "дорожка",
                                "target_lang_sense_clarification": "специально устроенный путь в саду или парке"
                            },
                            {
                                "target_lang_word": "тропа",
                                "target_lang_sense_clarification": "протоптанная дорога в лесу или горах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ccde40f8-c6ad-37c8-892f-c9cd52be048a",
                    "sense_definition": "A course of action, plan, or way of achieving a particular result or goal.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_route",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is trying to decide on the best career <w>path</w> for her future.",
                            "target_lang_translations": {
                                "ru": "Она пытается выбрать лучший карьерный <w>путь</w> для своего будущего."
                            }
                        },
                        {
                            "text": "Hard work and dedication are the surest <w>path</w> to success.",
                            "target_lang_translations": {
                                "ru": "Упорный труд и самоотдача — самый верный <w>путь</w> к успеху."
                            }
                        }
                    ],
                    "synonyms": [
                        "way",
                        "track",
                        "trail"
                    ],
                    "common_phrases": [
                        "career path",
                        "path to success",
                        "path of least resistance",
                        "on the right path",
                        "chosen path"
                    ],
                    "target_lang_definitions": {
                        "ru": "План действий или способ достижения определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "направление развития или способ достижения чего-либо"
                            },
                            {
                                "target_lang_word": "стезя",
                                "target_lang_sense_clarification": "жизненный путь, сфера деятельности (высокий стиль)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6800005a-06ef-31e7-b4ca-72eb1a4a639b",
                    "sense_definition": "The direction or line along which a person or object moves.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_route",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hurricane changed its <w>path</w> and headed out towards the ocean.",
                            "target_lang_translations": {
                                "ru": "Ураган изменил свой <w>путь</w> и направился в сторону океана."
                            }
                        },
                        {
                            "text": "Our <w>paths</w> crossed briefly when we were both living in London.",
                            "target_lang_translations": {
                                "ru": "Наши <w>пути</w> ненадолго пересеклись, когда мы оба жили в Лондоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "track",
                        "trail",
                        "way"
                    ],
                    "common_phrases": [
                        "flight path",
                        "path of the storm",
                        "cross paths",
                        "intercept a path"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направление или линия, вдоль которой движется человек или предмет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "линия движения"
                            },
                            {
                                "target_lang_word": "траектория",
                                "target_lang_sense_clarification": "линия полета или перемещения объекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a786501-f171-32e7-85b3-0d2fd6e94b80",
                    "sense_definition": "A string of characters used to identify a specific location within a directory structure in computing.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to specify the full file <w>path</w> to access the document.",
                            "target_lang_translations": {
                                "ru": "Вам нужно указать полный <w>путь</w> к файлу, чтобы получить доступ к документу."
                            }
                        },
                        {
                            "text": "The system could not find the network <w>path</w> requested by the user.",
                            "target_lang_translations": {
                                "ru": "Система не смогла найти сетевой <w>путь</w>, запрошенный пользователем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "file path",
                        "absolute path",
                        "relative path",
                        "network path"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Строка символов, указывающая расположение файла или каталога в файловой системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "адрес файла или каталога в системе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dab26fa2-1c03-318b-96b0-f8a8c766773d",
                    "sense_definition": "To navigate through a file system directory tree to a desired file or folder.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computing_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>path</w> to the correct folder before running the script.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>указать путь</w> к правильной папке перед запуском скрипта."
                            }
                        },
                        {
                            "text": "The software automatically <w>paths</w> to the default installation directory.",
                            "target_lang_translations": {
                                "ru": "Программа автоматически <w>прокладывает путь</w> к каталогу установки по умолчанию."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться по файловой системе к нужному файлу или папке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокладывать путь",
                                "target_lang_sense_clarification": "определять маршрут к объекту"
                            },
                            {
                                "target_lang_word": "указывать путь",
                                "target_lang_sense_clarification": "задавать адрес расположения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "13080b69-1011-307c-9028-dc14f446210f",
                    "sense_definition": "Pierwiastek chemiczny o symbolu Ag i liczbie atomowej 47",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enough",
                                "target_lang_sense_clarification": "used to express a desire for something to stop"
                            },
                            {
                                "target_lang_word": "come on",
                                "target_lang_sense_clarification": "used to encourage someone to stop doing something (colloquial)"
                            }
                        ]
                    }
                }
            ]
        }

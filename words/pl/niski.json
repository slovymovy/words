                },
                {
                    "sense_id": "8a203aa4-8867-3c06-8c78-9aaf3081fcf4",
                    "sense_definition": "Oceniany negatywnie pod względem moralnym; niegodziwy lub podły",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "moral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowały nim <niskie> pobudki, kiedy postanowił oszukać swojego przyjaciela.",
                            "target_lang_translations": {
                                "ru": "Им двигали <низкие> побуждения, когда он решил обмануть своего друга."
                            }
                        },
                        {
                            "text": "To było zachowanie na bardzo <niskim> poziomie kultury osobistej.",
                            "target_lang_translations": {
                                "ru": "Это было поведение на очень <низком> уровне личной культуры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niskie pobudki",
                        "niskie instynkty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оцениваемый отрицательно с моральной точки зрения; подлый или недостойный"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "низкий",
                                "target_lang_sense_clarification": "о поступках, морали"
                            },
                            {
                                "target_lang_word": "подлый",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]

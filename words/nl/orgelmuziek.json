    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e73cf17-c4d9-3d82-97e5-ef3a3ead4a4d",
                    "sense_definition": "Muziek die speciaal gecomponeerd is om op een orgel gespeeld te worden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Johann Sebastian Bach heeft veel bekende <w>orgelmuziek</w> geschreven.",
                            "target_lang_translations": {
                                "ru": "Иоганн Себастьян Бах написал много известной <w>органной музыки</w>."
                            }
                        },
                        {
                            "text": "Tijdens de kerkdienst luisterden we naar prachtige klassieke <w>orgelmuziek</w>.",
                            "target_lang_translations": {
                                "ru": "Во время церковной службы мы слушали прекрасную классическую <w>органную музыку</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "klassieke orgelmuziek",
                        "religieuze orgelmuziek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыка, специально написанная для исполнения на органе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "органная музыка",
                                "target_lang_sense_clarification": "музыкальный жанр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0bcbb51-1f7e-3f24-b5cc-6cc5fdc707b9",
                    "sense_definition": "Het geluid of de klanken die door een orgel worden voortgebracht.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De luide <w>orgelmuziek</w> vulde de hele kathedraal tijdens de ceremonie.",
                            "target_lang_translations": {
                                "ru": "Громкая <w>органная музыка</w> наполнила весь собор во время церемонии."
                            }
                        },
                        {
                            "text": "Op de markt hoorden we de vrolijke <w>orgelmuziek</w> van een draaiorgel.",
                            "target_lang_translations": {
                                "ru": "На рынке мы услышали веселую <w>органную музыку</w> шарманки."
                            }
                        }
                    ],
                    "synonyms": [
                        "orgelspel",
                        "orgelklank"
                    ],
                    "common_phrases": [
                        "klinkende orgelmuziek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук или мелодии, издаваемые органом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "органная музыка",
                                "target_lang_sense_clarification": "звучание инструмента"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1565a72e-2184-3780-bae2-70d50133e376",
                    "sense_definition": "Superior in quality, value, or price compared to others of the same type.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quality_val",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Customers are often willing to pay more for <w>premium</w> products that last longer.",
                            "target_lang_translations": {
                                "ru": "Клиенты часто готовы платить больше за <w>премиальные</w> продукты, которые служат дольше."
                            }
                        },
                        {
                            "text": "The hotel offers <w>premium</w> services including a private concierge and spa access.",
                            "target_lang_translations": {
                                "ru": "Отель предлагает услуги <w>премиум-класса</w>, включая личного консьержа и доступ в спа."
                            }
                        }
                    ],
                    "synonyms": [
                        "superior",
                        "high-end",
                        "deluxe",
                        "top-quality",
                        "exclusive"
                    ],
                    "antonyms": [
                        "discount",
                        "discount",
                        "discount"
                    ],
                    "common_phrases": [
                        "premium quality",
                        "premium brand",
                        "premium content",
                        "premium price"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превосходящий по качеству, ценности или цене другие аналоги того же типа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "премиальный",
                                "target_lang_sense_clarification": "о качестве или бренде"
                            },
                            {
                                "target_lang_word": "премиум-класса",
                                "target_lang_sense_clarification": "о категории товара"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fb74370f-80d2-3aa1-9c87-92ee0e60ff06",
                    "sense_definition": "The amount of money that must be paid regularly for an insurance policy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "insurance_pay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My car insurance <w>premiums</w> have increased significantly this year.",
                            "target_lang_translations": {
                                "ru": "Мои <w>страховые взносы</w> на автомобиль в этом году значительно выросли."
                            }
                        },
                        {
                            "text": "You must pay the annual <w>premium</w> on time to ensure you remain covered.",
                            "target_lang_translations": {
                                "ru": "Вы должны вовремя оплачивать ежегодный <w>страховой взнос</w>, чтобы страховка продолжала действовать."
                            }
                        }
                    ],
                    "synonyms": [
                        "payment",
                        "fee",
                        "installment"
                    ],
                    "common_phrases": [
                        "insurance premium",
                        "monthly premium",
                        "annual premium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная сумма, которую необходимо регулярно выплачивать по страховому полису."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страховой взнос",
                                "target_lang_sense_clarification": "регулярный платеж"
                            },
                            {
                                "target_lang_word": "страховая премия",
                                "target_lang_sense_clarification": "финансовый термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f19a866e-f6f7-3324-be4a-5ce6d0c30ddd",
                    "sense_definition": "An extra sum of money added to a standard price or charge.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "extra_cost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Buyers are paying a <w>premium</w> for homes in this popular neighborhood.",
                            "target_lang_translations": {
                                "ru": "Покупатели платят <w>надбавку</w> за дома в этом популярном районе."
                            }
                        },
                        {
                            "text": "Organic vegetables often sell at a <w>premium</w> compared to regular produce.",
                            "target_lang_translations": {
                                "ru": "Органические овощи часто продаются с <w>наценкой</w> по сравнению с обычными продуктами."
                            }
                        }
                    ],
                    "synonyms": [
                        "surcharge",
                        "bonus",
                        "extra"
                    ],
                    "antonyms": [
                        "discount"
                    ],
                    "common_phrases": [
                        "at a premium",
                        "pay a premium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дополнительная сумма, добавляемая к стандартной цене или тарифу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надбавка",
                                "target_lang_sense_clarification": "к цене"
                            },
                            {
                                "target_lang_word": "наценка",
                                "target_lang_sense_clarification": "увеличение стоимости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c61bd9e-6158-3fc4-94a1-dc91db38f08d",
                    "sense_definition": "A high value, importance, or scarcity assigned to something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "high_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Space is at a <w>premium</w> in the crowded city center.",
                            "target_lang_translations": {
                                "ru": "Свободное место в переполненном центре города — <w>на вес золота</w>."
                            }
                        },
                        {
                            "text": "The company puts a <w>premium</w> on honesty and integrity in the workplace.",
                            "target_lang_translations": {
                                "ru": "Компания придает <w>особое значение</w> честности и порядочности на рабочем месте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "put a premium on",
                        "place a premium on",
                        "at a premium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокая ценность, важность или редкость, приписываемая чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "особая ценность",
                                "target_lang_sense_clarification": "значимость"
                            },
                            {
                                "target_lang_word": "дефицит",
                                "target_lang_sense_clarification": "редкость"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "voorganger",
        "voorgaand"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dc23cb15-e96e-35d9-9856-8d21f2df8330",
                    "sense_definition": "Voor iemand uit lopen of als eerste gaan, vaak om de weg te wijzen of uit beleefdheid.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gids zal de groep <w>voorgaan</w> tijdens de wandeling door het bos.",
                            "target_lang_translations": {
                                "en": "The guide will <w>lead the way</w> for the group during the walk through the forest."
                            }
                        },
                        {
                            "text": "U mag bij de deur wel <w>voorgaan</w>.",
                            "target_lang_translations": {
                                "en": "You may <w>go first</w> at the door."
                            }
                        }
                    ],
                    "synonyms": [
                        "vooroplopen",
                        "voorafgaan"
                    ],
                    "antonyms": [
                        "volgen",
                        "achterblijven"
                    ],
                    "common_phrases": [
                        "iemand voorgaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To walk ahead of someone or go first, often to show the way or out of politeness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go first",
                                "target_lang_sense_clarification": "general movement or politeness"
                            },
                            {
                                "target_lang_word": "lead the way",
                                "target_lang_sense_clarification": "to guide others"
                            },
                            {
                                "target_lang_word": "precede",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "356f856d-f565-3470-a324-bdbc47695627",
                    "sense_definition": "Belangrijker gevonden worden dan iets of iemand anders; prioriteit krijgen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "priority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veiligheid moet altijd <w>voorgaan</w> boven snelheid.",
                            "target_lang_translations": {
                                "en": "Safety must always <w>come first</w> before speed."
                            }
                        },
                        {
                            "text": "In noodsituaties laten we hulpverleners <w>voorgaan</w>.",
                            "target_lang_translations": {
                                "en": "In emergency situations, we let aid workers <w>go first</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "prevaleren",
                        "voorrang hebben"
                    ],
                    "antonyms": [
                        "wijken",
                        "achterstellen"
                    ],
                    "common_phrases": [
                        "veiligheid gaat voor",
                        "werk gaat voor"
                    ],
                    "target_lang_definitions": {
                        "en": "To be considered more important than something or someone else; to have priority."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come first",
                                "target_lang_sense_clarification": "to be the most important"
                            },
                            {
                                "target_lang_word": "take precedence",
                                "target_lang_sense_clarification": "formal priority"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83a0ebd7-19f2-38c4-bae7-572cf0467954",
                    "sense_definition": "Een tijdstip aanwijzen dat later is dan de werkelijke tijd (gezegd van een klok of horloge).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik was te vroeg omdat mijn horloge vijf minuten bleek te <w>voorgaan</w>.",
                            "target_lang_translations": {
                                "en": "I was too early because my watch turned out to <w>be fast</w> by five minutes."
                            }
                        },
                        {
                            "text": "Die oude klok in de gang <w>gaat voor</w>.",
                            "target_lang_translations": {
                                "en": "That old clock in the hallway <w>is fast</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorlopen"
                    ],
                    "antonyms": [
                        "achterlopen"
                    ],
                    "common_phrases": [
                        "het horloge gaat voor"
                    ],
                    "target_lang_definitions": {
                        "en": "To indicate a time later than the actual time (said of a clock or watch)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be fast",
                                "target_lang_sense_clarification": "regarding timekeeping devices"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bed3b1a9-8e16-36df-90d2-04f5d3ed9158",
                    "sense_definition": "De leiding hebben en spreken tijdens een religieuze dienst of gebed.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe dominee zal aanstaande zondag voor het eerst <w>voorgaan</w>.",
                            "target_lang_translations": {
                                "en": "The new minister will <w>lead the service</w> for the first time this Sunday."
                            }
                        },
                        {
                            "text": "Hij vroeg zijn vader om te <w>voorgaan</w> in gebed voor de maaltijd.",
                            "target_lang_translations": {
                                "en": "He asked his father to <w>lead</w> in prayer before the meal."
                            }
                        }
                    ],
                    "synonyms": [
                        "leiden",
                        "celebreren"
                    ],
                    "common_phrases": [
                        "voorgaan in gebed",
                        "voorgaan in de dienst"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in charge and speak during a religious service or prayer."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "in a religious service or prayer"
                            },
                            {
                                "target_lang_word": "officiate",
                                "target_lang_sense_clarification": "formal religious context"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
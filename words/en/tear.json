        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2b807590-294c-35a2-b5bd-0b5b89194a00",
                    "sense_definition": "To pull something apart or into pieces with force.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "destruction_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to <w>tear</w> the paper when you open the package.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны, чтобы не <w>порвать</w> бумагу, когда открываете посылку."
                            }
                        },
                        {
                            "text": "He <w>tore</w> his coat on a sharp nail.",
                            "target_lang_translations": {
                                "ru": "Он <w>порвал</w> свое пальто об острый гвоздь."
                            }
                        }
                    ],
                    "synonyms": [
                        "rend",
                        "rip"
                    ],
                    "common_phrases": [
                        "tear apart",
                        "tear in two",
                        "tear to pieces"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разделять что-либо на части с применением силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рвать",
                                "target_lang_sense_clarification": "разделять на части"
                            },
                            {
                                "target_lang_word": "порвать",
                                "target_lang_sense_clarification": "нарушить целостность"

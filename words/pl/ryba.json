            "senses": [
                {
                    "sense_id": "d433364a-1b73-3bf4-98b7-49039f6a2b08",
                    "sense_definition": "Zwierzę wodne z kręgosłupem, które oddycha skrzelami i porusza się za pomocą płetw.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "zwierzę",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym czystym jeziorze pływa niejedna duża <w>ryba</w>.",
                            "target_lang_translations": {
                                "ru": "В этом чистом озере плавает не одна большая <w>рыба</w>."
                            }
                        },
                        {
                            "text": "Większość <w>ryb</w> składa ikrę, z której wylęgają się młode.",
                            "target_lang_translations": {
                                "ru": "Большинство <w>рыб</w> мечет икру, из которой вылупляются мальки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "łowić ryby",
                        "słodkowodna ryba",
                        "pływać jak ryba",
                        "czuć się jak ryba w wodzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Водное позвоночное животное, которое дышит жабрами и передвигается с помощью плавников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыба",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a73c1b1f-73f2-379a-8197-180b99dd6d39",
                    "sense_definition": "Mięso ryby przygotowane jako posiłek lub produkt spożywczy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "jedzenie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na dzisiejszy obiad przygotowałem smażoną <w>rybę</w> z frytkami.",
                            "target_lang_translations": {
                                "ru": "На сегодняшний обед я приготовил жареную <w>рыбу</w> с картошкой фри."
                            }
                        },
                        {
                            "text": "Lekarze zalecają, aby jeść chudą <w>rybę</w> przynajmniej raz w tygodniu.",
                            "target_lang_translations": {
                                "ru": "Врачи рекомендуют есть нежирную <w>рыбу</w> по крайней мере раз в неделю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ryba po grecku",
                        "wędzona ryba",
                        "świeża ryba",
                        "filet z ryby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясо рыбы, приготовленное как блюдо или продукт питания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыба",
                                "target_lang_sense_clarification": "еда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e35849b-f2b9-36ac-957d-59b055e20bae",
                    "sense_definition": "Osoba urodzona pod znakiem zodiaku Ryb (między 19 lutego a 20 marca).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "zodiak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra jest zodiakalną <w>Rybą</w>, więc jest bardzo wrażliwa i artystyczna.",
                            "target_lang_translations": {
                                "ru": "Моя сестра — <w>Рыба</w> по гороскопу, поэтому она очень чувствительная и артистичная."
                            }
                        },
                        {
                            "text": "Mówi się, że każda <w>Ryba</w> ma doskonałą intuicję.",
                            "target_lang_translations": {
                                "ru": "Говорят, что у каждой <w>Рыбы</w> отличная интуиция."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zodiakalna Ryba",
                        "znak Ryb",
                        "typowa Ryba"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, рожденный под знаком зодиака Рыбы (между 19 февраля и 20 марта)."

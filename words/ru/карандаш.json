    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "685748e3-2beb-3be1-96a0-521ce8d180bd",
                    "sense_definition": "Инструмент для письма, рисования или черчения, обычно состоящий из графитового стержня в деревянной или пластиковой оболочке.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У тебя есть запасной <w>карандаш</w>?",
                            "target_lang_translations": {
                                "en": "Do you have a spare <w>pencil</w>?"
                            }
                        },
                        {
                            "text": "Художник взял <w>карандаш</w> и начал делать быстрый набросок в блокноте.",
                            "target_lang_translations": {
                                "en": "The artist took a <w>pencil</w> and started making a quick sketch in the notebook."
                            }
                        }
                    ],
                    "common_phrases": [
                        "простой карандаш",
                        "цветной карандаш",
                        "точить карандаш",
                        "писать карандашом"
                    ],
                    "target_lang_definitions": {
                        "en": "An instrument for writing, drawing, or drafting, usually consisting of a graphite core in a wooden or plastic casing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pencil",
                                "target_lang_sense_clarification": "writing instrument"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77889bcb-a5cd-3131-800f-2f80351a38a5",
                    "sense_definition": "Тонкий цилиндрический стержень из какого-либо вещества, используемый в косметических, медицинских или бытовых целях.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для уроков труда ребёнку нужно купить клеящий <w>карандаш</w>.",
                            "target_lang_translations": {
                                "en": "For craft lessons, the child needs to buy a glue <w>stick</w>."
                            }
                        },
                        {
                            "text": "Она использовала чёрный <w>карандаш</w> для глаз, чтобы подчеркнуть взгляд.",
                            "target_lang_translations": {
                                "en": "She used a black eye <w>pencil</w> to emphasize her look."
                            }
                        }
                    ],
                    "synonyms": [
                        "палочка"
                    ],
                    "common_phrases": [
                        "клеящий карандаш",
                        "карандаш для губ",
                        "карандаш для глаз",
                        "ляписный карандаш"
                    ],
                    "target_lang_definitions": {
                        "en": "A thin cylindrical rod of a substance used for cosmetic, medical, or household purposes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stick",
                                "target_lang_sense_clarification": "e.g. glue stick"
                            },
                            {
                                "target_lang_word": "pencil",
                                "target_lang_sense_clarification": "cosmetic, e.g. eye pencil"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dc8ba16-457c-39c1-91ff-aa0bd0a24c2d",
                    "sense_definition": "След, оставленный на бумаге пишущим инструментом, или рисунок, выполненный в такой технике.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хорошая резинка легко удаляет <w>карандаш</w> с плотной бумаги.",
                            "target_lang_translations": {
                                "en": "A good eraser easily removes <w>pencil</w> from thick paper."
                            }
                        },
                        {
                            "text": "Весь альбом был заполнен пейзажами, выполненными в <w>карандаше</w>.",
                            "target_lang_translations": {
                                "en": "The whole album was filled with landscapes done in <w>pencil</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "стереть карандаш",
                        "портрет в карандаше"
                    ],
                    "target_lang_definitions": {
                        "en": "A mark left on paper by a writing instrument, or a drawing executed in such a technique."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pencil",
                                "target_lang_sense_clarification": "mark or artistic medium"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90e0ac1d-eeb1-3465-8b02-043901818b14",
                    "sense_definition": "Световой указатель на мачте железнодорожного светофора в виде вертикальной стрелки.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Машинист заметил горящий <w>карандаш</w> на светофоре и продолжил движение.",
                            "target_lang_translations": {
                                "en": "The train driver noticed the lit <w>vertical arrow</w> on the signal and continued moving."
                            }
                        },
                        {
                            "text": "Этот <w>карандаш</w> сигнализирует о недостаточной длине следующего блок-участка.",
                            "target_lang_translations": {
                                "en": "This <w>vertical arrow</w> signals the insufficient length of the next block section."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "железнодорожный жаргон"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A light indicator on a railway signal mast in the form of a vertical arrow."
                    },

                {
                    "sense_id": "8ea15ec9-1db5-3d2e-b201-37600c712e39",
                    "sense_definition": "De optelsom van alle behaalde resultaten in een competitie of toernooi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "ranking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij staat na drie overwinningen bovenaan in de <w>totaalstand</w>.",
                            "target_lang_translations": {
                                "ru": "После трёх побед он лидирует в <w>общем зачёте</w>.",
                                "en": "After three victories, he is at the top of the <w>overall standings</w>."
                            }
                        },
                        {
                            "text": "De punten van deze wedstrijd tellen mee voor de <w>totaalstand</w> van het seizoen.",
                            "target_lang_translations": {
                                "ru": "Очки за этот матч идут в <w>общий зачёт</w> сезона.",
                                "en": "The points from this match count towards the season's <w>overall standings</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "algemeen klassement",
                        "eindklassement"
                    ],
                    "common_phrases": [
                        "leiden in de totaalstand",
                        "tweede in de totaalstand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумма всех результатов, достигнутых в соревновании или турнире.",
                        "en": "The sum of all results achieved in a competition or tournament."

                },
                {
                    "sense_id": "3bc93997-54f1-308c-a19a-e0a9b19b01f9",
                    "sense_definition": "Iemand die de toekomst voorspelt",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je hoeft geen <w>profeet</w> te zijn om te zien dat dit plan zal mislukken.",
                            "target_lang_translations": {
                                "en": "You don't have to be a <w>prophet</w> to see that this plan will fail."
                            }
                        },
                        {
                            "text": "Nostradamus was een bekende <w>profeet</w> die vele gebeurtenissen zou hebben voorzien.",
                            "target_lang_translations": {
                                "en": "Nostradamus was a famous <w>prophet</w> who is said to have foreseen many events."

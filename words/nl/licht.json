                    ],
                    "common_phrases": [
                        "het licht zien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное озарение или понимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свет",
                                "target_lang_sense_clarification": "метафорически: истина"
                            },
                            {
                                "target_lang_word": "озарение",
                                "target_lang_sense_clarification": "внезапное понимание"
                            }
                        ]
                    }
                }
            ]
        },

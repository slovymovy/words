    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "355828d2-99a7-36a4-87b9-12fb53809f44",
                    "sense_definition": "Dawne narzędzie kary i przymusu w postaci drewnianych kłód z otworami, w które zamykano ręce, nogi lub szyję skazańca, wystawianego na widok publiczny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strażnicy miejscy zakuli złodzieja w <w>dyby</w> na środku rynku.",
                            "target_lang_translations": {
                                "ru": "Городские стражники заковали вора в <w>колодки</w> посреди рыночной площади."
                            }
                        },
                        {
                            "text": "Skazaniec spędził w <w>dybach</w> całą noc, znosząc drwiny przechodniów.",
                            "target_lang_translations": {
                                "ru": "Осужденный провел в <w>колодках</w> всю ночь, терпя насмешки прохожих."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zakuć w dyby",
                        "siedzieć w dybach"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historyczne narzędzie kar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное орудие наказания в виде деревянных колод с отверстиями, в которые запирали руки, ноги или шею осужденного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колодки",
                                "target_lang_sense_clarification": "орудие наказания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "22952b8f-ad23-3b3c-93ad-a32461e0971e",
                    "sense_definition": "Regionalna, gwarowa forma spójnika <w>gdyby</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Dyby</w> on tu był, na pewno by nam pomógł.",
                            "target_lang_translations": {
                                "ru": "<w>Если бы</w> он был здесь, он бы точно нам помог."
                            }
                        },
                        {
                            "text": "Nie wiedziałem, co bym zrobił, <w>dyby</w> nie twoja pomoc.",
                            "target_lang_translations": {
                                "ru": "Я не знал, что бы я делал, <w>если бы</w> не твоя помощь."
                            }
                        }
                    ],
                    "synonyms": [

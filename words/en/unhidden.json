    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "02e70891-9554-3c9d-86b9-1b0645249539",
                    "sense_definition": "Not hidden or concealed; clearly visible.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "visibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The secret door remained <w>unhidden</w> to those who knew where to look.",
                            "target_lang_translations": {
                                "ru": "Потайная дверь оставалась <w>нескрытой</w> для тех, кто знал, куда смотреть."
                            }
                        },
                        {
                            "text": "Her excitement was <w>unhidden</w> as she opened the gift.",
                            "target_lang_translations": {
                                "ru": "Её волнение было <w>нескрытым</w>, когда она открывала подарок."
                            }
                        }
                    ],
                    "synonyms": [
                        "visible",
                        "exposed",
                        "unconcealed",
                        "revealed"
                    ],
                    "antonyms": [
                        "hidden",
                        "concealed",
                        "obscured"
                    ],
                    "common_phrases": [
                        "remain unhidden",
                        "completely unhidden",
                        "unhidden truth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не скрытый или не спрятанный; ясно видимый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нескрытый",
                                "target_lang_sense_clarification": "не спрятанный, видимый"
                            },
                            {
                                "target_lang_word": "явный",
                                "target_lang_sense_clarification": "очевидный (об эмоциях или намерениях)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9aa39cc6-f795-3250-8575-3cde8389882b",
                    "sense_definition": "Past participle of <w>unhide</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_reveal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have <w>unhidden</w> the columns in the spreadsheet so we can see the data.",
                            "target_lang_translations": {
                                "ru": "Я <w>отобразил</w> столбцы в таблице, чтобы мы могли видеть данные."
                            }
                        },
                        {
                            "text": "Once the files were <w>unhidden</w>, the system update could proceed.",
                            "target_lang_translations": {
                                "ru": "Как только файлы были <w>отображены</w>, обновление системы могло продолжиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "revealed",
                        "shown",
                        "exposed"
                    ],
                    "antonyms": [
                        "hidden",
                        "concealed"
                    ],
                    "common_phrases": [
                        "unhidden files",
                        "unhidden rows"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие прошедшего времени от глагола unhide (показывать скрытое)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отображенный",
                                "target_lang_sense_clarification": "сделанный видимым (в компьютерных интерфейсах)"
                            },
                            {
                                "target_lang_word": "раскрытый",
                                "target_lang_sense_clarification": "обнаруженный, выявленный"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "f8b64f84-d11c-3278-ba5b-6eda0d7c42f9",
                    "sense_definition": "A public house where alcoholic beverages are sold and consumed, often serving food.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "drinking_establishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to meet at the <w>pub</w> for a quick drink after work.",
                            "target_lang_translations": {
                                "ru": "Мы решили встретиться в <w>пабе</w>, чтобы пропустить по стаканчику после работы."
                            }
                        },
                        {
                            "text": "The village <w>pub</w> serves excellent fish and chips on Fridays.",
                            "target_lang_translations": {
                                "ru": "В деревенском <w>пабе</w> по пятницам подают отличную рыбу с картошкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "public house",
                        "tavern",
                        "boozer",
                        "local",
                        "watering hole",
                        "alehouse",
                        "saloon"
                    ],
                    "common_phrases": [
                        "go to the pub",
                        "local pub",
                        "pub lunch",
                        "pub quiz",
                        "at the pub"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заведение, где продаются и употребляются алкогольные напитки, часто подается еда; традиционный британский бар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паб",
                                "target_lang_sense_clarification": "питейное заведение в британском или ирландском стиле"
                            },
                            {
                                "target_lang_word": "бар",
                                "target_lang_sense_clarification": "общее название заведений с алкоголем"
                            },
                            {
                                "target_lang_word": "пивная",
                                "target_lang_sense_clarification": "заведение, специализирующееся на пиве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1e1a630-dd73-3c37-be18-d06c696bd791",
                    "sense_definition": "A short form of the word <w>publication</w>.",

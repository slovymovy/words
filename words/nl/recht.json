                    "sense_id": "240dce2c-cdda-3ff9-a9e8-8754f291c94f",
                    "sense_definition": "Zonder bochten of krommingen; in een lineaire lijn.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De weg naar het dorp is lang en <w>recht</w>.",
                            "target_lang_translations": {
                                "ru": "Дорога в деревню длинная и <w>прямая</w>."
                            }
                        },
                        {
                            "text": "Kun je een <w>rechte</w> lijn tekenen zonder liniaal?",
                            "target_lang_translations": {
                                "ru": "Ты можешь нарисовать <w>прямую</w> линию без линейки?"
                            }
                        }
                    ],
                    "antonyms": [
                        "krom"
                    ],
                    "common_phrases": [
                        "recht vooruit",
                        "kaarsrecht",
                        "in een rechte lijn"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4bafb6f1-d02b-30b3-9f2f-0024911a9492",
                    "sense_definition": "The sediment or dregs left at the bottom of a container of beer or cider.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "brewing_residue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The barrel was nearly finished, containing mostly cloudy <w>emptins</w> at the bottom.",
                            "target_lang_translations": {
                                "ru": "Бочка почти закончилась, на дне оставался в основном мутный <w>осадок</w>."
                            }
                        },
                        {
                            "text": "He carefully poured the cider to avoid disturbing the bitter <w>emptins</w> settled below.",
                            "target_lang_translations": {
                                "ru": "Он осторожно разливал сидр, стараясь не потревожить горькую <w>гущу</w>, осевшую внизу."
                            }
                        }
                    ],
                    "synonyms": [
                        "dregs",
                        "lees",
                        "sediment",
                        "grounds"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Осадок или гуща, остающиеся на дне емкости с пивом или сидром."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осадок",
                                "target_lang_sense_clarification": "твердые частицы, оседающие в жидкости"
                            },
                            {
                                "target_lang_word": "гуща",
                                "target_lang_sense_clarification": "густой осадок на дне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9bb50d0-f37b-3153-b532-661b8fcda971",
                    "sense_definition": "A type of liquid yeast or barm obtained from brewing remains, historically used to make bread rise.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "brewing_residue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baker used a cup of <w>emptins</w> to ensure the dough would rise properly.",
                            "target_lang_translations": {
                                "ru": "Пекарь использовал чашку <w>закваски</w>, чтобы тесто гарантированно поднялось."
                            }
                        },
                        {
                            "text": "Without fresh <w>emptins</w> from the brewery, the bread turned out heavy and flat.",
                            "target_lang_translations": {
                                "ru": "Без свежих <w>жидких дрожжей</w> с пивоварни хлеб получался тяжелым и плоским."
                            }
                        }
                    ],
                    "synonyms": [
                        "barm",
                        "yeast",
                        "leaven"
                    ],
                    "common_phrases": [
                        "emptins bread"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность жидких дрожжей или закваски, получаемая из пивного осадка и исторически использовавшаяся для поднятия теста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закваска",
                                "target_lang_sense_clarification": "состав, вызывающий брожение"
                            },
                            {
                                "target_lang_word": "жидкие дрожжи",
                                "target_lang_sense_clarification": "дрожжевая культура в жидком виде"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
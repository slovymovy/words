            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a4b548fb-7c2b-3942-8036-1c9fadd40176",
                    "sense_definition": "Coś, co jest niezgodne z rzeczywistością lub faktami; przeciwieństwo prawdy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "falsehood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To, co on mówi, to kompletna <w>nieprawda</w>.",
                            "target_lang_translations": {
                                "ru": "То, что он говорит, — полная <w>неправда</w>."
                            }
                        },
                        {
                            "text": "Oskarżono go o to, że publicznie głosił <w>nieprawdę</w>.",
                            "target_lang_translations": {
                                "ru": "Его обвинили в том, что он публично говорил <w>неправду</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kłamstwo",
                        "fałsz",
                        "fikcja",
                        "bujda"
                    ],
                    "antonyms": [
                        "prawda"
                    ],
                    "common_phrases": [
                        "to nieprawda",
                        "mówić nieprawdę",
                        "pisać nieprawdę"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что не соответствует действительности или фактам; противоположность правде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неправда",
                                "target_lang_sense_clarification": "отсутствие истины"
                            },
                            {
                                "target_lang_word": "ложь",
                                "target_lang_sense_clarification": "намеренный обман"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "85dca1f7-a28d-301f-81a7-6114c9b95aff",
                    "sense_definition": "Okrzyk wyrażający gwałtowne i stanowcze zaprzeczenie temu, co ktoś powiedział.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "denial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Nieprawda</w>! Nigdy nie byłem w tym miejscu.",
                            "target_lang_translations": {
                                "ru": "<w>Неправда</w>! Я никогда не был в этом месте."
                            }
                        },
                        {
                            "text": "Twierdzisz, że to zbiłem, ale to <w>nieprawda</w>!",
                            "target_lang_translations": {
                                "ru": "Ты утверждаешь, что это разбил я, но это <w>неправда</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "absolutnie nie",
                        "wcale nie",
                        "skądże znowu",
                        "nic podobnego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, выражающий резкое и решительное отрицание сказанного кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неправда",
                                "target_lang_sense_clarification": "возглас несогласия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37b4a19b-14bb-306a-8ded-f432a8cb6fa2",
                    "sense_definition": "Słowo używane na końcu zdania w celu uzyskania potwierdzenia od rozmówcy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "tag_question",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mamy dzisiaj bardzo ładną pogodę, <w>nieprawda</w>?",
                            "target_lang_translations": {
                                "ru": "Сегодня очень хорошая погода, <w>не правда ли</w>?"
                            }
                        },
                        {
                            "text": "Pan chciał ze mną porozmawiać, <w>nieprawda</w>?",
                            "target_lang_translations": {
                                "ru": "Вы хотели со мной поговорить, <w>не правда ли</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "nieprawdaż?",
                        "czyż nie?"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово или фраза, используемые в конце предложения для получения подтверждения от собеседника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не правда ли",
                                "target_lang_sense_clarification": "вопрос для подтверждения"
                            },
                            {
                                "target_lang_word": "не так ли",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]

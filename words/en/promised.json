    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d04c9751-bc9c-3420-8f8b-22fb95721714",
                    "sense_definition": "Simple past and past participle of <w>promise</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>promised</w> to call me as soon as she arrived home.",
                            "target_lang_translations": {
                                "ru": "Она <w>пообещала</w> позвонить мне, как только придет домой."
                            }
                        },
                        {
                            "text": "They have <w>promised</w> to finish the construction work by next month.",
                            "target_lang_translations": {
                                "ru": "Они <w>пообещали</w> закончить строительные работы к следующему месяцу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "promised to help",
                        "promised a reward"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола promise (обещать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пообещал",
                                "target_lang_sense_clarification": "прошедшее время, совершенный вид"
                            },
                            {
                                "target_lang_word": "обещал",
                                "target_lang_sense_clarification": "прошедшее время, несовершенный вид"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9a0fa089-781d-33a5-bd29-b6e5c1502688",
                    "sense_definition": "Assured or guaranteed to happen or appear because of a previous vow or agreement.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pledged_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He finally received the <w>promised</w> payment after weeks of waiting.",
                            "target_lang_translations": {
                                "ru": "Он наконец получил <w>обещанную</w> выплату после недель ожидания."
                            }
                        },
                        {
                            "text": "The children were excited to visit the <w>promised</w> amusement park.",
                            "target_lang_translations": {
                                "ru": "Дети были в восторге от поездки в <w>обещанный</w> парк развлечений."
                            }
                        }
                    ],
                    "synonyms": [
                        "pledged",
                        "vowed",
                        "guaranteed"
                    ],
                    "common_phrases": [
                        "the promised land",
                        "promised money",
                        "promised support"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что было гарантировано или в чем заверили."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обещанный",
                                "target_lang_sense_clarification": "прилагательное/причастие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28aaf598-74d8-350f-9d5d-b21392c996fa",
                    "sense_definition": "Predicted, expected, or anticipated to occur.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pledged_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>promised</w> storm never arrived, leaving the afternoon calm and sunny.",
                            "target_lang_translations": {
                                "ru": "<w>Обещанный</w> шторм так и не пришел, и день остался тихим и солнечным."
                            }
                        },
                        {
                            "text": "Voters are still waiting for the <w>promised</w> economic recovery to begin.",
                            "target_lang_translations": {
                                "ru": "Избиратели все еще ждут начала <w>обещанного</w> восстановления экономики."
                            }
                        }
                    ],
                    "synonyms": [
                        "expected",
                        "anticipated",
                        "foretold",
                        "forecast"
                    ],
                    "antonyms": [
                        "unexpected"
                    ],
                    "common_phrases": [
                        "promised change",
                        "promised weather"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предсказанный или ожидаемый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обещанный",
                                "target_lang_sense_clarification": "о прогнозах или ожиданиях"
                            },
                            {
                                "target_lang_word": "предсказанный",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c70de7d4-73e6-32a7-ac5d-17bd2c15c206",
                    "sense_definition": "Under an obligation to a future commitment, specifically formally pledged to be married.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She cannot accept your proposal because she is already <w>promised</w> to another.",
                            "target_lang_translations": {
                                "ru": "Она не может принять ваше предложение, потому что уже <w>обещана</w> другому."
                            }
                        },
                        {
                            "text": "In the story, the young prince was <w>promised</w> to the daughter of a neighboring king.",
                            "target_lang_translations": {
                                "ru": "В рассказе юный принц был <w>обещан</w> дочери соседнего короля."
                            }
                        }
                    ],
                    "synonyms": [
                        "betrothed",
                        "engaged",
                        "affianced"
                    ],
                    "antonyms": [
                        "unattached",
                        "single"
                    ],
                    "common_phrases": [
                        "promised to another"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "common in historical or literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный обязательством вступить в брак (обычно формально)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрученный",
                                "target_lang_sense_clarification": "о помолвке"
                            },
                            {
                                "target_lang_word": "обещанный",
                                "target_lang_sense_clarification": "устаревшее или литературное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

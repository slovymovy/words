{
    "word_family": [
        "wond",
        "verwonden",
        "gewonde"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ee1d9b8e-856d-30eb-9869-4432adac8698",
                    "sense_definition": "Meervoud van <w>wond</w>: beschadigingen aan het lichaam waarbij de huid of het weefsel open is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verpleegkundige verzorgde de diepe wonden aan zijn been na het ongeluk.",
                            "target_lang_translations": {
                                "ru": "Медсестра обработала глубокие <w>раны</w> на его ноге после аварии."
                            }
                        },
                        {
                            "text": "Tijd heelt alle wonden, maar de littekens blijven vaak zichtbaar.",
                            "target_lang_translations": {
                                "ru": "Время лечит все <w>раны</w>, но шрамы часто остаются видны."
                            }
                        }
                    ],
                    "synonyms": [
                        "letsels",
                        "kwetsuren",
                        "verwondingen"
                    ],
                    "common_phrases": [
                        "zout in de wonden strooien",
                        "zijn wonden likken",
                        "open wonden",
                        "oude wonden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "meervoud"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от <w>wond</w>: повреждения тела, при которых нарушена целостность кожи или тканей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раны",
                                "target_lang_sense_clarification": "физические повреждения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ce447a5e-d40e-37d5-94a9-15c5b13e70cc",
                    "sense_definition": "Verleden tijd meervoud van <w>winden</w>: iets ergens omheen draaien of wikkelen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "wrapping_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De artsen wonden voorzichtig het verband om de gebroken arm van de patiënt.",
                            "target_lang_translations": {
                                "ru": "Врачи осторожно <w>обматывали</w> бинт вокруг сломанной руки пациента."
                            }
                        },
                        {
                            "text": "Zij wonden er geen doekjes om en vertelden direct de harde waarheid.",
                            "target_lang_translations": {
                                "ru": "Они не <w>ходили</w> вокруг да около и сразу высказали горькую правду."
                            }
                        }
                    ],
                    "synonyms": [
                        "wikkelden",
                        "draaiden"
                    ],
                    "common_phrases": [
                        "wonden er geen doekjes om",
                        "wonden zich op"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verleden tijd meervoud"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время (мн. ч.) от <w>winden</w>: накручивать или обматывать что-то вокруг чего-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наматывали",
                                "target_lang_sense_clarification": "накручивали"
                            },
                            {
                                "target_lang_word": "обматывали",
                                "target_lang_sense_clarification": "оборачивали"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5e6ce938-d576-325b-9ede-56f38f03452d",
                    "sense_definition": "Iemand of iets een <w>wond</w> toebrengen; lichamelijk beschadigen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "inflict_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De scherpe doorns van de struik wondden zijn handen tijdens het snoeien.",
                            "target_lang_translations": {
                                "ru": "Острые шипы кустарника <w>поранили</w> ему руки во время обрезки."
                            }
                        },
                        {
                            "text": "De jagers wondden het dier, maar het wist toch te ontsnappen.",
                            "target_lang_translations": {
                                "ru": "Охотники <w>ранили</w> животное, но ему всё же удалось скрыться."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwonden"
                    ],
                    "antonyms": [
                        "genezen",
                        "helen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "meestal vervangen door 'verwonden'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанести кому-то или чему-то <w>рану</w>; повредить физически."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранить",
                                "target_lang_sense_clarification": "наносить повреждения"
                            },
                            {
                                "target_lang_word": "поранить",
                                "target_lang_sense_clarification": "нанести травму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8827d0ae-855c-3e7b-a8b7-bbf0d2aed60d",
                    "sense_definition": "Iemand emotioneel pijn doen of diep <w>kwetsen</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "emotional_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn harde en ondoordachte woorden wondden haar diep in haar ziel.",
                            "target_lang_translations": {
                                "ru": "Его резкие и необдуманные слова глубоко <w>ранили</w> её в душу."
                            }
                        },
                        {
                            "text": "Het verraad van zijn beste vriend wondde hem meer dan hij wilde toegeven.",
                            "target_lang_translations": {
                                "ru": "Предательство лучшего друга <w>ранило</w> его сильнее, чем он хотел признать."
                            }
                        }
                    ],
                    "synonyms": [
                        "kwetsen",
                        "krenken",
                        "grieven"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literair"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинить кому-то эмоциональную боль или глубоко <w>обидеть</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранить",
                                "target_lang_sense_clarification": "причинять душевную боль"
                            },
                            {
                                "target_lang_word": "уязвлять",
                                "target_lang_sense_clarification": "задевать чувства"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
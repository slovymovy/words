                {
                    "sense_id": "d7502856-c2ea-3f49-8104-887e10443987",
                    "sense_definition": "Een stevige, rechthoekige doos voor opslag of vervoer van spullen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij bewaart al zijn oude speelgoed in een houten <kist> op zolder.",
                            "target_lang_translations": {
                                "en": "He keeps all his old toys in a wooden <w>chest</w> in the attic.",
                                "ru": "Он хранит все свои старые игрушки в деревянном <w>сундуке</w> на чердаке."
                            }
                        },
                        {
                            "text": "De piraten begroeven de zware <kist> met goud op het onbewoonde eiland.",
                            "target_lang_translations": {
                                "en": "The pirates buried the heavy <w>chest</w> of gold on the uninhabited island.",
                                "ru": "Пираты закопали тяжелый <w>сундук</w> с золотом на необитаемом острове."
                            }
                        }
                    ],
                    "common_phrases": [
                        "houten kist",
                        "zware kist",
                        "kist met geld"
                    ],
                    "target_lang_definitions": {

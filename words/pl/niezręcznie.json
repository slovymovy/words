    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ca5a4b32-57f0-3286-9a25-edc21fadfa45",
                    "sense_definition": "W sposób wywołujący uczucie zakłopotania, wstydu lub skrępowania; nietaktownie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_awkwardness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czułem się bardzo <w>niezręcznie</w>, gdy zapadła długa cisza.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал себя очень <w>неловко</w>, когда повисла долгая тишина."
                            }
                        },
                        {
                            "text": "Mężczyzna <w>niezręcznie</w> zażartował z szefa podczas spotkania.",
                            "target_lang_translations": {
                                "ru": "Мужчина <w>неловко</w> пошутил над начальником во время встречи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czuć się niezręcznie",
                        "zrobiło się niezręcznie",
                        "wypaść niezręcznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывая чувство смущения, стыда или неловкости; бестактно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неловко",
                                "target_lang_sense_clarification": "чувство смущения"
                            },
                            {
                                "target_lang_word": "неудобно",
                                "target_lang_sense_clarification": "психологический дискомфорт"
                            },
                            {
                                "target_lang_word": "бестактно",
                                "target_lang_sense_clarification": "нарушение норм приличия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41304ffe-d948-3844-ae4b-06c2185a9410",
                    "sense_definition": "W sposób pozbawiony zręczności fizycznej, mało sprawnie; niezdarnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_clumsiness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner <w>niezręcznie</w> postawił tacę i wylał napoje.",
                            "target_lang_translations": {
                                "ru": "Официант <w>неуклюже</w> поставил поднос и разлил напитки."
                            }
                        },
                        {
                            "text": "Dziecko <w>niezręcznie</w> próbowało zawiązać sznurowadła.",
                            "target_lang_translations": {
                                "ru": "Ребенок <w>неумело</w> пытался завязать шнурки."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezdarnie",
                        "niezgrabnie"
                    ],
                    "antonyms": [
                        "zręcznie"
                    ],
                    "common_phrases": [
                        "poruszać się niezręcznie",
                        "chwycić niezręcznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без физической сноровки, неумело; неуклюже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неуклюже",
                                "target_lang_sense_clarification": "отсутствие грации"
                            },
                            {
                                "target_lang_word": "неловко",
                                "target_lang_sense_clarification": "физическая неловкость"
                            },
                            {
                                "target_lang_word": "неумело",
                                "target_lang_sense_clarification": "отсутствие навыка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7766a127-8b52-30e0-8f3c-4db7b37a119e",
                    "sense_definition": "W sposób niewygodny, utrudniający wykonywanie czynności lub dostęp do czegoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "inconvenience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szafka została zamontowana <w>niezręcznie</w> wysoko.",
                            "target_lang_translations": {
                                "ru": "Шкафчик повесили <w>неудобно</w> высоко."
                            }
                        },
                        {
                            "text": "To duże pudło niesie się bardzo <w>niezręcznie</w>.",
                            "target_lang_translations": {
                                "ru": "Эту большую коробку нести очень <w>неудобно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "niewygodnie",
                        "nieporęcznie"
                    ],
                    "common_phrases": [

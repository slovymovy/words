                },
                {
                    "sense_id": "6e422330-ad55-37d6-942a-b26ddcb71cea",
                    "sense_definition": "Nazwa gajówki w Polsce, w województwie łódzkim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Podczas wycieczki rowerowej przejeżdżaliśmy obok gajówki <w>Cygan</w>.",
                            "target_lang_translations": {
                                "ru": "Во время велосипедной прогулки мы проезжали мимо лесничества <w>Цыган</w>."
                            }
                        },
                        {
                            "text": "Na starej mapie zaznaczono miejscowość <w>Cygan</w> w gminie Lubochnia.",
                            "target_lang_translations": {
                                "ru": "На старой карте отмечена местность <w>Цыган</w> в гмине Любохня."
                            }

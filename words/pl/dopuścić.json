{
    "word_family": [
        "dopuszczać",
        "dopuszczenie",
        "dopuszczalny",
        "niedopuszczalny",
        "dopuszczalność"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e6bd230d-b189-339b-9d0f-c0985d85eedc",
                    "sense_definition": "Pozwolić na to, aby coś się wydarzyło, lub wyrazić zgodę na czyjś udział w czymś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "allow_permit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Komisja lekarska <dopuściła> zawodnika do udziału w igrzyskach.",
                            "target_lang_translations": {
                                "ru": "Медицинская комиссия <допустила> спортсмена к участию в играх."
                            }
                        },
                        {
                            "text": "Nie możemy <dopuścić>, aby w naszym zespole panował taki chaos.",
                            "target_lang_translations": {
                                "ru": "Мы не можем <допустить>, чтобы в нашей команде царил такой хаос."
                            }
                        }
                    ],
                    "synonyms": [
                        "popełnić",
                        "popełnić"
                    ],
                    "antonyms": [
                        "zabronić",
                        "zakazać",
                        "uniemożliwić"
                    ],
                    "common_phrases": [
                        "dopuścić do głosu",
                        "dopuścić do użytku",
                        "dopuścić możliwość",
                        "dopuścić do ruchu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позволить чему-либо случиться или разрешить кому-либо участвовать в чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "допустить",
                                "target_lang_sense_clarification": "разрешить, дать согласие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "226b5ccc-8523-30ec-9895-18b91a743482",
                    "sense_definition": "Dokonać czynu nagannego, niewłaściwego lub przestępczego (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "commit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oskarżony przyznał, że <dopuścił> się kradzieży pieniędzy z kasy.",
                            "target_lang_translations": {
                                "ru": "Обвиняемый признал, что <совершил> кражу денег из кассы."
                            }
                        },
                        {
                            "text": "Nigdy nie <dopuściłbym> się takiego kłamstwa wobec przyjaciela.",
                            "target_lang_translations": {
                                "ru": "Я бы никогда не <позволил> себе такой лжи по отношению к другу."
                            }
                        }
                    ],
                    "synonyms": [
                        "popełnić"
                    ],
                    "common_phrases": [
                        "dopuścić się zdrady",
                        "dopuścić się zbrodni",
                        "dopuścić się wykroczenia",
                        "dopuścić się oszustwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершить предосудительный или преступный поступок (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совершить",
                                "target_lang_sense_clarification": "сделать что-то плохое (преступление, проступок)"
                            },
                            {
                                "target_lang_word": "позволить себе",
                                "target_lang_sense_clarification": "решиться на плохой поступок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32c5b28c-b1f4-3408-ac5c-79922d0ffe75",
                    "sense_definition": "Pozwolić komuś lub czemuś zbliżyć się fizycznie lub emocjonalnie; dać dostęp.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "let_near",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ochrona nie <dopuściła> tłumu fanów do sceny po koncercie.",
                            "target_lang_translations": {
                                "ru": "Охрана не <подпустила> толпу фанатов к сцене после концерта."
                            }
                        },
                        {
                            "text": "Przez długi czas nie <dopuszczała> do siebie myśli o porażce.",
                            "target_lang_translations": {
                                "ru": "Долгое время она не <допускала> мысли о поражении."
                            }
                        }
                    ],
                    "synonyms": [
                        "popełnić"
                    ],
                    "antonyms": [
                        "odsunąć",
                        "izolować"
                    ],
                    "common_phrases": [
                        "dopuścić do siebie",
                        "dopuścić kogoś do tajemnicy",
                        "nie dopuścić do siebie myśli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позволить кому-либо или чему-либо приблизиться физически или эмоционально; дать доступ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подпустить",
                                "target_lang_sense_clarification": "дать приблизиться"
                            },
                            {
                                "target_lang_word": "допустить",
                                "target_lang_sense_clarification": "дать доступ (к кому-то/чему-то)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
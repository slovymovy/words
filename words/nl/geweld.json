                    "sense_id": "0e10877c-57ae-352f-9a5c-7cedac3be9fd",
                    "sense_definition": "Gebruik van fysieke kracht om personen te verwonden of schade aan te richten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "violence_human",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie moest <w>geweld</w> gebruiken om de verdachte te arresteren.",
                            "target_lang_translations": {
                                "en": "The police had to use <w>force</w> to arrest the suspect.",
                                "ru": "Полиции пришлось применить <w>силу</w>, чтобы арестовать подозреваемого."
                            }
                        },
                        {
                            "text": "Zij is helaas slachtoffer geworden van huiselijk <w>geweld</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, she has become a victim of domestic <w>violence</w>.",
                                "ru": "К сожалению, она стала жертвой домашнего <w>насилия</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "agressie",
                        "dwang"
                    ],
                    "antonyms": [
                        "vrede",
                        "geweldloosheid"
                    ],
                    "common_phrases": [
                        "huiselijk geweld",
                        "zinloos geweld",
                        "geweld gebruiken",
                        "fysiek geweld"
                    ],
                    "target_lang_definitions": {
                        "en": "The use of physical force to injure people or cause damage.",
                        "ru": "Использование физической силы для нанесения травм или причинения ущерба."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "violence",

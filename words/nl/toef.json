    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cb2e0b45-4192-3ae1-a5a7-29d85d327f5f",
                    "sense_definition": "Een hoeveelheid zachte substantie die ergens op is gespoten of gelegd, vaak in een sierlijke, puntige vorm.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_dollop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik een warme chocolademelk met een grote <w>toef</w> slagroom?",
                            "target_lang_translations": {
                                "ru": "Можно мне горячий шоколад с большой <w>шапкой</w> взбитых сливок?"
                            }
                        },
                        {
                            "text": "De bakker spoot een mooie <w>toef</w> crème bovenop het gebakje.",
                            "target_lang_translations": {
                                "ru": "Пекарь выдавил красивый <w>завиток</w> крема на пирожное."
                            }
                        }
                    ],
                    "synonyms": [
                        "dot",
                        "klodder"
                    ],
                    "common_phrases": [
                        "toef slagroom",
                        "toefje mayonaise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество мягкой субстанции, выдавленной или положенной куда-либо, часто в декоративной, заостренной форме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шапка",
                                "target_lang_sense_clarification": "обильный слой взбитых сливок или пены"
                            },
                            {
                                "target_lang_word": "завиток",
                                "target_lang_sense_clarification": "декоративная форма крема"
                            },
                            {
                                "target_lang_word": "горка",
                                "target_lang_sense_clarification": "небольшая кучка мягкой массы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7a124b1-1b4c-3fdb-945d-f39a20fa5607",
                    "sense_definition": "Een opstaand bosje of plukje dat ergens uitsteekt, bijvoorbeeld van haar, gras of kruiden.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_tuft",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de harde wind stond er een rare <w>toef</w> haar recht overeind.",
                            "target_lang_translations": {
                                "ru": "Из-за сильного ветра странный <w>хохолок</w> волос стоял торчком."
                            }
                        },
                        {
                            "text": "De kok garneerde de soep met een klein <w>toefje</w> verse peterselie.",
                            "target_lang_translations": {
                                "ru": "Повар украсил суп маленьким <w>пучком</w> свежей петрушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "pluk",
                        "bosje",
                        "tuiltje"
                    ],
                    "common_phrases": [
                        "toef haar",
                        "toefje peterselie",
                        "toefje groen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торчащий пучок или клочок чего-либо, например волос, травы или зелени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пучок",
                                "target_lang_sense_clarification": "зелени, травы или волос"
                            },
                            {
                                "target_lang_word": "хохолок",
                                "target_lang_sense_clarification": "торчащая прядь волос"
                            },
                            {
                                "target_lang_word": "клок",
                                "target_lang_sense_clarification": "клочок шерсти или волос"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7993229b-be0f-3c58-b19a-c85d63a5c01b",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>toeven</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>toef</w> graag in deze prachtige tuin om tot rust te komen.",
                            "target_lang_translations": {
                                "ru": "Я с удовольствием <w>пребываю</w> в этом прекрасном саду, чтобы отдохнуть."
                            }
                        },
                        {
                            "text": "Zolang ik in dit vreemde land <w>toef</w>, zal ik mij aanpassen.",
                            "target_lang_translations": {
                                "ru": "Пока я <w>нахожусь</w> в этой чужой стране, я буду приспосабливаться."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "conjugated form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола \"toeven\" (пребывать, находиться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пребываю",

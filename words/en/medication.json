    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "817e7048-4620-339e-b78e-f3a63a33b513",
                    "sense_definition": "A medicine or a set of medicines regularly taken by a patient to treat an illness or condition.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must remember to take your <w>medication</w> at the same time every morning.",
                            "target_lang_translations": {
                                "ru": "Вы должны не забывать принимать <w>лекарство</w> в одно и то же время каждое утро."
                            }
                        },
                        {
                            "text": "The doctor decided to put him on <w>medication</w> to help lower his high blood pressure.",
                            "target_lang_translations": {
                                "ru": "Врач решил назначить ему <w>лекарства</w>, чтобы помочь снизить высокое кровяное давление."
                            }
                        }
                    ],
                    "synonyms": [
                        "medicine",
                        "drug",
                        "remedy",
                        "pharmaceutical",
                        "treatment"
                    ],
                    "common_phrases": [
                        "take medication",
                        "on medication",
                        "prescribed medication",
                        "medication for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лекарство или набор лекарственных средств, регулярно принимаемых пациентом для лечения болезни или состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лекарство",
                                "target_lang_sense_clarification": "лечебное средство"
                            },
                            {
                                "target_lang_word": "медикаменты",
                                "target_lang_sense_clarification": "лекарственные препараты"
                            },
                            {
                                "target_lang_word": "препарат",
                                "target_lang_sense_clarification": "лекарственное вещество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3baa5829-bee7-38b9-bb0b-eb48bb96a8de",
                    "sense_definition": "The act or process of administering medicine to a patient.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The aggressive behavior of the patient was severe enough to necessitate <w>medication</w> and restraint.",
                            "target_lang_translations": {
                                "ru": "Агрессивное поведение пациента было достаточно серьезным, чтобы потребовать <w>медикаментозного лечения</w> и усмирения."
                            }
                        },
                        {
                            "text": "Proper diagnosis of the illness must always precede any form of <w>medication</w>.",
                            "target_lang_translations": {
                                "ru": "Правильная диагностика заболевания всегда должна предшествовать любой форме <w>медикаментозного лечения</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "administration",
                        "treatment"
                    ],
                    "common_phrases": [
                        "necessitate medication",
                        "require medication"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс назначения или введения лекарства пациенту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медикаментозное лечение",
                                "target_lang_sense_clarification": "процесс лечения лекарствами"
                            },
                            {
                                "target_lang_word": "назначение лекарств",
                                "target_lang_sense_clarification": "прописывание препаратов"
                            }
                        ]
                    }
                }
            ]

                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religious_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ojciec <w>minister</w> zwołał wszystkich braci na wieczorną modlitwę w kaplicy.",
                            "target_lang_translations": {
                                "ru": "Отец <w>министр</w> созвал всех братьев на вечернюю молитву в часовне."
                            }
                        },
                        {
                            "text": "W klasztorze franciszkanów wybrano nowego <w>ministra</w> prowincjalnego.",
                            "target_lang_translations": {
                                "ru": "В монастыре францисканцев выбрали нового провинциального <w>министра</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "opat",
                        "przeor"
                    ],
                    "common_phrases": [
                        "minister generalny",
                        "minister prowincjalny"
                    ],
                    "target_lang_definitions": {
                        "ru": "настоятель или высшее должностное лицо в некоторых католических орденах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "министр",
                                "target_lang_sense_clarification": "титул в ордене (напр. францисканцев)"
                            }
                        ]
                    }
                }
            ]
        }

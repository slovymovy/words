    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f8180960-51bc-3f78-8485-2d83b6580ada",
                    "sense_definition": "Występujący lub zdarzający się bardzo rzadko; niespotykany często",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "frequency_rarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To bardzo <w>rzadki</w> okaz, którego nie znajdziesz w zwykłym sklepie.",
                            "target_lang_translations": {
                                "ru": "Это очень <w>редкий</w> экземпляр, который не найдешь в обычном магазине."
                            }
                        },
                        {
                            "text": "Zaćmienie słońca to <w>rzadkie</w> zjawisko, które fascynuje ludzi na całym świecie.",
                            "target_lang_translations": {
                                "ru": "Солнечное затмение — это <w>редкое</w> явление, которое восхищает людей во всем мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieczęsty",
                        "sporadyczny",
                        "unikalny",
                        "wyjątkowy"
                    ],
                    "antonyms": [
                        "częsty",
                        "pospolity"
                    ],
                    "common_phrases": [
                        "rzadki okaz",
                        "rzadkie zjawisko",
                        "rzadki przypadek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встречающийся или происходящий очень редко; необычный"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "редкий",
                                "target_lang_sense_clarification": "встречающийся нечасто"
                            },
                            {
                                "target_lang_word": "нечастый",
                                "target_lang_sense_clarification": "происходящий редко"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1622553-37dc-3465-ba6b-1bb7a1a06127",
                    "sense_definition": "Składający się z elementów rozmieszczonych w dużych odstępach od siebie; niegęsty",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "density_spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>rzadkie</w> włosy były starannie zaczesane do tyłu.",
                            "target_lang_translations": {
                                "ru": "Его <w>редкие</w> волосы были аккуратно зачесаны назад."
                            }
                        },
                        {
                            "text": "Przez <w>rzadki</w> las łatwo było dostrzec drogę biegnącą w oddali.",
                            "target_lang_translations": {
                                "ru": "Сквозь <w>редкий</w> лес было легко разглядеть дорогу, бегущую вдалеке."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozproszony"
                    ],
                    "antonyms": [
                        "gęsty"
                    ],
                    "common_phrases": [
                        "rzadkie włosy",
                        "rzadki las",
                        "rzadkie zęby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий из элементов, расположенных на большом расстоянии друг от друга; негустой"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "редкий",
                                "target_lang_sense_clarification": "негустой (о лесе, волосах, зубах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1cc097a-5461-3f42-a7cb-b063d93b8fee",
                    "sense_definition": "Mający płynną konsystencję, zawierający dużo wody w stosunku do innych składników",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "density_consistency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta zupa jest zbyt <w>rzadka</w>, powinniśmy dodać do niej więcej warzyw.",
                            "target_lang_translations": {
                                "ru": "Этот суп слишком <w>жидкий</w>, нам следует добавить в него больше овощей."
                            }
                        },
                        {
                            "text": "Wolę gęsty sos do makaronu niż taki <w>rzadki</w> i wodnisty.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю густой соус к макаронам, а не такой <w>жидкий</w> и водянистый."
                            }
                        }
                    ],
                    "antonyms": [
                        "gęsty"
                    ],
                    "common_phrases": [
                        "rzadka zupa",
                        "rzadki sos",
                        "rzadkie ciasto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий текучую консистенцию, содержащий много воды по сравнению с другими ингредиентами"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жидкий",
                                "target_lang_sense_clarification": "водянистый, текучий"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
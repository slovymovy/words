{
    "word_family": [
        "being",
        "maybe",
        "wannabe",
        "has-been"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5c88fae7-1c8a-3859-af7c-64f2ebd97f01",
                    "sense_definition": "The third-person singular simple present indicative form of the verb <w>be</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "copula_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>is</w> a very talented musician.",
                            "target_lang_translations": {
                                "ru": "Она <w>является</w> очень талантливым музыкантом."
                            }
                        },
                        {
                            "text": "It <w>is</w> important to drink enough water every day.",
                            "target_lang_translations": {
                                "ru": "<w>Важно</w> пить достаточно воды каждый день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "it is",
                        "he is",
                        "she is",
                        "is it",
                        "that is"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма третьего лица единственного числа настоящего времени глагола <w>be</w> (быть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "является",
                                "target_lang_sense_clarification": "глагол-связка (формально)"
                            },
                            {
                                "target_lang_word": "находится",
                                "target_lang_sense_clarification": "о местоположении"
                            },
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "существует / имеется"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc4f7008-bd77-3800-8e76-409d0644b6ae",
                    "sense_definition": "Used with existential 'there', 'here', or 'where' when the subject is plural, instead of the standard 'are'.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "existential_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There <w>is</w> three people waiting for you outside.",
                            "target_lang_translations": {
                                "ru": "Там <w>есть</w> три человека, которые ждут тебя снаружи."
                            }
                        },
                        {
                            "text": "Here <w>is</w> the documents you asked for yesterday.",
                            "target_lang_translations": {
                                "ru": "Вот <w>лежат</w> документы, которые ты просил вчера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "there is",
                        "here is",
                        "where is"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standard grammar requires 'are' for plural subjects"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется с 'there', 'here' или 'where', когда подлежащее стоит во множественном числе (вместо стандартного 'are')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "разговорное использование"
                            },
                            {
                                "target_lang_word": "находится",
                                "target_lang_sense_clarification": "о местоположении"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cef967f8-0730-3e93-98f5-464742d80fe0",
                    "sense_definition": "Initialism of the Islamic State, a militant organization.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "organization_acronym",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "The government announced new sanctions against <w>IS</w> leadership.",
                            "target_lang_translations": {
                                "ru": "Правительство объявило о новых санкциях против руководства <w>ИГ</w>."
                            }
                        },
                        {
                            "text": "Reports indicate that <w>IS</w> has lost control of the territory.",
                            "target_lang_translations": {
                                "ru": "Сообщения указывают, что <w>ИГ</w> потеряло контроль над территорией."
                            }
                        }
                    ],
                    "synonyms": [
                        "Daesh",
                        "ISIL",
                        "ISIS"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от 'Islamic State' (Исламское государство), военизированной организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ИГ",
                                "target_lang_sense_clarification": "организация"
                            },
                            {
                                "target_lang_word": "ИГИЛ",
                                "target_lang_sense_clarification": "организация"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b3c79f4-2d2d-3a85-b241-e6e79a63a27b",
                    "sense_definition": "Initialism of information systems, a field dealing with computer systems and data.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "acronym_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She works in the <w>IS</w> department at a large bank.",
                            "target_lang_translations": {
                                "ru": "Она работает в отделе <w>ИС</w> в крупном банке."
                            }
                        },
                        {
                            "text": "The company is upgrading its <w>IS</w> infrastructure this year.",
                            "target_lang_translations": {
                                "ru": "В этом году компания модернизирует свою инфраструктуру <w>ИС</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "IS department",
                        "IS manager"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от 'information systems' (информационные системы), области, связанной с компьютерными системами и данными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ИС",
                                "target_lang_sense_clarification": "информационные системы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42082be9-b15c-388b-8db9-85b359ca05a3",
                    "sense_definition": "Abbreviation of Islands, often used in geographical names on maps.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "abbreviation_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are planning a trip to the Virgin <w>Is</w>. next summer.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку на Виргинские <w>о-ва</w> следующим летом."
                            }
                        },
                        {
                            "text": "The map clearly shows the location of the Cayman <w>Is</w>.",
                            "target_lang_translations": {
                                "ru": "На карте четко показано расположение Каймановых <w>о-вов</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от 'Islands' (острова), часто используется в географических названиях на картах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о-ва",
                                "target_lang_sense_clarification": "география"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "381e4bec-d2f1-355f-bc71-63580b8d39ee",
                    "sense_definition": "Een grote, samengebonden hoeveelheid goederen of materiaal, vaak verpakt in een zak of doek.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer tilde met moeite een zware <w>baal</w> hooi op de wagen.",
                            "target_lang_translations": {
                                "ru": "Фермер с трудом поднял тяжелый <w>тюк</w> сена на повозку.",
                                "en": "The farmer struggled to lift a heavy <w>bale</w> of hay onto the wagon."
                            }
                        },
                        {
                            "text": "In het pakhuis lag een grote <w>baal</w> katoen klaar voor verzending.",
                            "target_lang_translations": {
                                "ru": "На складе лежала большая <w>кипа</w> хлопка, готовая к отправке.",
                                "en": "In the warehouse, a large <w>bale</w> of cotton lay ready for shipment."
                            }
                        }
                    ],
                    "synonyms": [
                        "bundel",
                        "pak",
                        "zak"
                    ],
                    "common_phrases": [

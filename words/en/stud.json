        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "29fe6409-1dff-36a8-aca1-82a5a05c1b23",
                    "sense_definition": "A small metal or plastic object that sticks out from a surface, often used for decoration, grip on shoes, or as a fastener.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "protrusion_objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The football player checked his <w>studs</w> before running onto the muddy field.",
                            "target_lang_translations": {
                                "ru": "Футболист проверил свои <w>шипы</w> перед тем, как выбежать на грязное поле."
                            }
                        },
                        {
                            "text": "Her leather jacket was covered in silver <w>studs</w> to give it a punk style.",
                            "target_lang_translations": {
                                "ru": "Её кожаная куртка была покрыта серебряными <w>заклёпками</w> для придания панк-стиля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "metal studs",
                        "football studs",
                        "press stud"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой металлический или пластиковый выступ на поверхности, используемый для украшения, сцепления (на обуви) или как застежка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шип",
                                "target_lang_sense_clarification": "на подошве обуви или шине"

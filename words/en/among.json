{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "0536db58-2857-30a5-a9d5-74be02a615a6",
                    "sense_definition": "Surrounded by or in the middle of other people or things.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small house was hidden <w>among</w> the tall trees.",
                            "target_lang_translations": {
                                "ru": "Маленький домик был спрятан <w>среди</w> высоких деревьев."
                            }
                        },
                        {
                            "text": "She felt very happy to be back <w>among</w> friends.",
                            "target_lang_translations": {
                                "ru": "Она была очень счастлива снова оказаться <w>среди</w> друзей."
                            }
                        }
                    ],
                    "synonyms": [
                        "amongst",
                        "amidst",
                        "amid"
                    ],
                    "common_phrases": [
                        "among the crowd",
                        "among friends",
                        "among the trees"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находясь в окружении или посередине других людей или предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "среди́",
                                "target_lang_sense_clarification": "в окружении, посреди"
                            },
                            {
                                "target_lang_word": "ме́жду",
                                "target_lang_sense_clarification": "в пространстве, разделяющем объекты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39cf6826-78cb-3335-8979-ac3122a6a965",
                    "sense_definition": "Being included in a particular group or class.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>among</w> the best students in our school.",
                            "target_lang_translations": {
                                "ru": "Он <w>среди</w> лучших учеников нашей школы."
                            }
                        },
                        {
                            "text": "This painting is <w>among</w> the most famous works of art.",
                            "target_lang_translations": {
                                "ru": "Эта картина находится <w>среди</w> самых известных произведений искусства."
                            }
                        }
                    ],
                    "synonyms": [
                        "amongst"
                    ],
                    "common_phrases": [
                        "among the best",
                        "among the few",
                        "among others"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть включенным в определенную группу, класс или список."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "среди́",
                                "target_lang_sense_clarification": "в составе группы"
                            },
                            {
                                "target_lang_word": "в числе́",
                                "target_lang_sense_clarification": "входя в количество, список"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8477a71-3a0c-39c4-92f2-35d31335559f",
                    "sense_definition": "Happening to or shared by people in a specific group.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "distribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The news spread quickly <w>among</w> the villagers.",
                            "target_lang_translations": {
                                "ru": "Новости быстро распространились <w>среди</w> жителей деревни."
                            }
                        },
                        {
                            "text": "We need to discuss this problem <w>among</w> ourselves.",
                            "target_lang_translations": {
                                "ru": "Нам нужно обсудить эту проблему <w>между</w> собой."
                            }
                        }
                    ],
                    "synonyms": [
                        "amongst"
                    ],
                    "common_phrases": [
                        "popular among",
                        "common among",
                        "among ourselves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящее с людьми определенной группы или разделяемое ими (взаимодействие)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "среди́",
                                "target_lang_sense_clarification": "распространение внутри группы"
                            },
                            {
                                "target_lang_word": "ме́жду",
                                "target_lang_sense_clarification": "взаимно, друг с другом"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
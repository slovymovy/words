    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e3422268-73ce-308b-a0e5-cfd900b3fce3",
                    "sense_definition": "To demand firmly that something must happen or be done, refusing to accept any refusal or opposition.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "demand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I must <w>insist</w> that you join us for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>настоять</w> на том, чтобы вы поужинали с нами сегодня вечером."
                            }
                        },
                        {
                            "text": "The teacher <w>insisted</w> on complete silence during the final exam.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>настаивал</w> на полной тишине во время итогового экзамена."
                            }
                        }
                    ],
                    "synonyms": [
                        "demand",
                        "require"
                    ],
                    "antonyms": [
                        "yield",
                        "concede"
                    ],
                    "common_phrases": [
                        "insist on",
                        "insist upon",
                        "have to insist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердо требовать, чтобы что-то произошло или было сделано, не принимая отказов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настаивать",
                                "target_lang_sense_clarification": "требовать выполнения"
                            },
                            {
                                "target_lang_word": "настоять",
                                "target_lang_sense_clarification": "добиться выполнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81935725-53ce-3418-8508-743add528da2",
                    "sense_definition": "To state clearly and firmly that something is true, especially when others do not believe it or disagree.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "assert",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The suspect <w>insisted</w> on his innocence throughout the long trial.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый <w>настаивал</w> на своей невиновности в течение всего длительного судебного процесса."
                            }
                        },
                        {
                            "text": "She <w>insists</w> that she saw a strange light in the sky last night.",
                            "target_lang_translations": {
                                "ru": "Она <w>настаивает</w>, что видела странный свет в небе прошлой ночью."
                            }
                        }
                    ],
                    "synonyms": [
                        "maintain",
                        "assert",
                        "claim",
                        "contend"
                    ],
                    "antonyms": [
                        "deny",
                        "retract"
                    ],
                    "common_phrases": [
                        "insist on innocence",
                        "insist that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упорно утверждать, что что-то является правдой, особенно когда другие не верят или не соглашаются."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настаивать",
                                "target_lang_sense_clarification": "на мнении или факте"
                            },
                            {
                                "target_lang_word": "утверждать",
                                "target_lang_sense_clarification": "заявлять уверенно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bde5de33-7426-3694-b402-1f4bfd542250",
                    "sense_definition": "To stand or rest upon something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the diagram, the angles <w>insist</w> on the diameter of the circle.",
                            "target_lang_translations": {
                                "ru": "На чертеже углы <w>опираются</w> на диаметр окружности."
                            }
                        },
                        {
                            "text": "The heavy structure seems to <w>insist</w> on the bedrock beneath it.",
                            "target_lang_translations": {
                                "ru": "Кажется, что тяжелая конструкция <w>опирается</w> на скальное основание под ней."
                            }
                        }
                    ],
                    "synonyms": [
                        "rest",
                        "stand"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стоять, лежать или покоиться на чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опираться",

                {
                    "sense_id": "2b36bdd8-29ee-3cfb-9fe7-f6f150e356f3",
                    "sense_definition": "Het beginpunt, de plaats of de oorzaak waaruit iets of iemand voortkomt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "source_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is van <w>oorsprong</w> een lerares, maar werkt nu als schrijfster.",
                            "target_lang_translations": {
                                "ru": "По <w>происхождению</w> она учительница, но сейчас работает писательницей."
                            }
                        },
                        {
                            "text": "De wetenschappers onderzoeken de <w>oorsprong</w> van het mysterieuze virus.",
                            "target_lang_translations": {
                                "ru": "Ученые исследуют <w>происхождение</w> таинственного вируса."
                            }
                        }
                    ],
                    "synonyms": [
                        "herkomst",
                        "afkomst",
                        "bron",
                        "begin"
                    ],
                    "antonyms": [
                        "einde",
                        "slot"
                    ],
                    "common_phrases": [
                        "van oorsprong",
                        "oorsprong vinden",
                        "oorsprong hebben"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начальная точка, место или причина, откуда что-то или кто-то происходит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "происхождение",
                                "target_lang_sense_clarification": "возникновение, начало или родословная"
                            },
                            {
                                "target_lang_word": "источник",
                                "target_lang_sense_clarification": "место или причина возникновения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cf6e1aa-e8a9-32a2-84fa-56a1053c8ead",
                    "sense_definition": "De plaats waar een rivier of beek uit de grond komt en begint te stromen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "source_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens onze vakantie wandelden we naar de <w>oorsprong</w> van de Rijn.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска мы прогулялись к <w>истоку</w> Рейна."
                            }
                        },
                        {
                            "text": "Het water is bij de <w>oorsprong</w> van de beek nog heel helder en koud.",
                            "target_lang_translations": {
                                "ru": "Вода у <w>истока</w> ручья еще очень прозрачная и холодная."
                            }
                        }
                    ],
                    "synonyms": [
                        "bron"
                    ],
                    "antonyms": [
                        "monding"
                    ],
                    "common_phrases": [
                        "oorsprong van de rivier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где река или ручей выходят из-под земли и начинают течь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исток",
                                "target_lang_sense_clarification": "начало реки или ручья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fe4c1f4-2ee4-3eca-9bd5-40960dbb0725",
                    "sense_definition": "Het snijpunt van de assen in een coördinatenstelsel, meestal aangeduid met het punt (0,0).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze grafiek gaat de rechte lijn precies door de <w>oorsprong</w>.",
                            "target_lang_translations": {
                                "ru": "На этом графике прямая линия проходит точно через <w>начало координат</w>."
                            }
                        },
                        {
                            "text": "Je moet de afstand berekenen van punt P tot de <w>oorsprong</w>.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно вычислить расстояние от точки P до <w>начала координат</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nulpunt"
                    ],
                    "common_phrases": [
                        "door de oorsprong"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка пересечения осей в системе координат, обычно обозначаемая как (0,0)."

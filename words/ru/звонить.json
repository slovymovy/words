            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0b6c3e03-fa8c-3b88-a023-2f89d34866c1",
                    "sense_definition": "Связываться с кем-либо, вести разговор или вызывать кого-либо по телефону.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я буду <звонить> тебе каждый вечер после работы."
                        },
                        {
                            "text": "Ты можешь <звонить> мне в любое время дня и ночи."
                        }
                    ],
                    "synonyms": [
                        "вызывать"
                    ],
                    "common_phrases": [
                        "звонить по телефону",
                        "звонить другу",
                        "звонить домой",
                        "звонить на работу"
                    ]
                },
                {
                    "sense_id": "f9e7703e-7de0-38b7-8372-564576191900",
                    "sense_definition": "Издавать высокий, резкий звук (о телефоне, будильнике, колоколе или другом устройстве).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В коридоре громко <звонит> старый телефон."
                        },
                        {
                            "text": "Будильник <звонил> уже пять минут, но я не просыпался."
                        }
                    ],
                    "synonyms": [
                        "звенеть"
                    ],
                    "common_phrases": [
                        "телефон звонит",
                        "будильник звонит",
                        "колокола звонят"
                    ]
                },
                {
                    "sense_id": "4bdbb77c-a4cb-3ea2-be57-eec4237d9256",
                    "sense_definition": "Подавать звуковой сигнал, нажимая на кнопку звонка или раскачивая колокол.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто-то настойчиво <звонит> в дверь нашей квартиры."
                        },
                        {
                            "text": "Почтальон <звонил> два раза, чтобы ему открыли."
                        }
                    ],
                    "common_phrases": [
                        "звонить в дверь",
                        "звонить в колокол"
                    ]
                },
                {
                    "sense_id": "46437469-86d0-3882-a31f-598b9ff7dc41",
                    "sense_definition": "Распространять слухи или сплетни, рассказывать всем подряд о чём-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gossip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит <звонить> об этом секрете на каждом углу."
                        },
                        {
                            "text": "Она любит <звонить> о чужих проблемах всем знакомым."
                        }
                    ],
                    "synonyms": [
                        "трезвонить"
                    ],
                    "common_phrases": [
                        "звонить на всех углах"
                    ],

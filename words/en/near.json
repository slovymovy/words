    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "b6a98257-6ed1-3c78-b7ff-dbc272b2e37f",
                    "sense_definition": "Physically close to; in close proximity to something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are many nice restaurants located <w>near</w> our hotel.",
                            "target_lang_translations": {
                                "ru": "Рядом с нашим отелем находится много хороших ресторанов, расположенных <w>рядом с</w> ним."
                            }
                        },
                        {
                            "text": "She sat <w>near</w> the window to watch the rain falling outside.",
                            "target_lang_translations": {
                                "ru": "Она села <w>возле</w> окна, чтобы смотреть на дождь, идущий на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "close by",
                        "close",
                        "nearby",
                        "close by"
                    ],
                    "antonyms": [
                        "far from"
                    ],
                    "common_phrases": [
                        "near here",
                        "near the end",
                        "near the top"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в непосредственной близости от чего-либо; расположенный рядом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рядом с",
                                "target_lang_sense_clarification": "указывает на нахождение вблизи"
                            },
                            {
                                "target_lang_word": "возле",
                                "target_lang_sense_clarification": "указывает на нахождение около"
                            },
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "указывает на нахождение рядом"
                            },
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "указывает на нахождение вплотную"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc08388b-2d99-3f8c-8a7b-9e08748f26eb",
                    "sense_definition": "Close to a specific point in time.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The construction work on the new bridge was <w>near</w> completion.",
                            "target_lang_translations": {
                                "ru": "Строительные работы на новом мосту были <w>близки</w> к завершению."
                            }
                        },
                        {
                            "text": "It was <w>near</w> midnight when the guests finally decided to leave.",
                            "target_lang_translations": {
                                "ru": "Было <w>около</w> полуночи, когда гости наконец решили уйти."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Незадолго до определенного момента времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "приблизительно в указанное время"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "по направлению к моменту времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca36d391-e5c5-3404-a27c-8b5355a8ff09",

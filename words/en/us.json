        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "5a97a000-7265-39df-9510-b40f4be7372f",
                    "sense_definition": "The objective case of <w>we</w>: the speaker and at least one other person.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She told <w>us</w> to wait here for the bus.",
                            "target_lang_translations": {
                                "ru": "Она сказала <w>нам</w> подождать автобус здесь."
                            }
                        },
                        {
                            "text": "Please come with <w>us</w> to the cinema tonight.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, пойдем с <w>нами</w> в кино сегодня вечером."
                            }
                        }
                    ],
                    "common_phrases": [
                        "join us",
                        "come with us",
                        "tell us",
                        "give us"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объектный падеж местоимения <w>мы</w>: говорящий и по крайней мере еще один человек."
                    },
                    "translations": {
                        "ru": [
                            {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b8c4d070-7254-377c-baaa-87f34b0a2ed6",
                    "sense_definition": "Dostarczać wodę roślinom, lejąc ją na ziemię wokół nich lub bezpośrednio na nie, aby mogły rosnąć.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "gardening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę codziennie rano <w>podlewać</w> kwiaty na balkonie, ponieważ jest bardzo gorąco.",
                            "target_lang_translations": {
                                "ru": "Мне нужно каждое утро <w>поливать</w> цветы на балконе, потому что очень жарко."
                            }
                        },
                        {
                            "text": "Mój sąsiad obiecał <w>podlewać</w> ogródek, kiedy wyjadę na dwutygodniowe wakacje.",
                            "target_lang_translations": {
                                "ru": "Мой сосед обещал <w>поливать</w> огород, когда я уеду в двухнедельный отпуск."
                            }
                        }
                    ],
                    "synonyms": [
                        "nawadniać"
                    ],
                    "antonyms": [
                        "suszyć",
                        "przesuszać"
                    ],
                    "common_phrases": [
                        "podlewać kwiaty",
                        "podlewać ogródek",
                        "podlewać trawnik",
                        "podlewać rośliny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать растения водой, наливая её на землю вокруг них или непосредственно на них, чтобы они могли расти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поливать",
                                "target_lang_sense_clarification": "растения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "249395d5-4412-303a-8564-d62998b85589",
                    "sense_definition": "Dodawać niewielką ilość płynu (np. wody, sosu, wina) do potrawy w trakcie jej gotowania lub pieczenia, aby była wilgotna i smaczna.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas pieczenia mięsa w piekarniku należy często <w>podlewać</w> je wytworzonym sosem.",
                            "target_lang_translations": {
                                "ru": "Во время запекания мяса в духовке нужно часто <w>поливать</w> его образовавшимся соусом."
                            }
                        },
                        {
                            "text": "Kucharz postanowił <w>podlewać</w> risotto białym winem, aby nadać mu wyjątkowy aromat.",
                            "target_lang_translations": {
                                "ru": "Повар решил <w>подливать</w> в ризотто белое вино, чтобы придать ему особый аромат."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrapiać",
                        "dolewać"
                    ],
                    "common_phrases": [
                        "podlewać sosem",
                        "podlewać winem",
                        "podlewać pieczeń",
                        "podlewać wodą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавлять небольшое количество жидкости (например, воды, соуса, вина) в блюдо во время его приготовления или запекания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поливать",
                                "target_lang_sense_clarification": "мясо соусом"
                            },
                            {
                                "target_lang_word": "подливать",
                                "target_lang_sense_clarification": "жидкость"
                            }
                        ]
                    }
                }
            ]
        }
    ]

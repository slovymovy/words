    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "431d6c66-bc01-3e3a-b07e-1409a0108fc8",
                    "sense_definition": "The quantity of people or things present in a specific area or space.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "distribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city has a very high population <w>density</w> because many people live in small apartments.",
                            "target_lang_translations": {
                                "ru": "В городе очень высокая <w>плотность</w> населения, потому что многие люди живут в маленьких квартирах."
                            }
                        },
                        {
                            "text": "Farmers worry about the <w>density</w> of weeds in the field affecting the crops.",
                            "target_lang_translations": {
                                "ru": "Фермеры беспокоятся, что <w>густота</w> сорняков на поле повлияет на урожай."
                            }
                        }
                    ],
                    "synonyms": [
                        "number density"
                    ],
                    "common_phrases": [
                        "population density",
                        "high density",
                        "low density"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество людей или предметов, находящихся в определенной области или пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плотность",
                                "target_lang_sense_clarification": "населения, застройки, размещения"
                            },
                            {
                                "target_lang_word": "густота",
                                "target_lang_sense_clarification": "растительности, толпы, тумана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08720895-cbea-3048-9ae0-112f36b3c05a",
                    "sense_definition": "A measure of how much mass is contained in a specific volume of a substance.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oil floats on water because it has a lower <w>density</w>.",
                            "target_lang_translations": {
                                "ru": "Масло плавает на воде, потому что у него более низкая <w>плотность</w>."
                            }
                        },
                        {
                            "text": "Scientists measure the <w>density</w> of the rock to determine what minerals it contains.",
                            "target_lang_translations": {
                                "ru": "Ученые измеряют <w>плотность</w> породы, чтобы определить, какие минералы она содержит."
                            }
                        }
                    ],
                    "synonyms": [
                        "mass density"
                    ],
                    "common_phrases": [
                        "high density",
                        "low density",
                        "density of water"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мера количества массы, содержащейся в определенном объеме вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плотность",
                                "target_lang_sense_clarification": "физическая величина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ee0169d-270f-31c1-afe5-1227dc68e187",
                    "sense_definition": "A statistical concept describing the likelihood that a random variable will fall within a certain range.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "statistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The probability <w>density</w> function helps predict where the data points will cluster.",
                            "target_lang_translations": {
                                "ru": "Функция <w>плотности</w> вероятности помогает предсказать, где будут группироваться точки данных."
                            }
                        },
                        {
                            "text": "In this chart, the <w>density</w> of the curve represents the distribution of test scores.",
                            "target_lang_translations": {
                                "ru": "На этом графике <w>плотность</w> кривой представляет распределение результатов тестов."
                            }
                        }
                    ],
                    "synonyms": [
                        "probability density"
                    ],
                    "common_phrases": [
                        "probability density"
                    ],
                    "target_lang_definitions": {
                        "ru": "Статистическое понятие, описывающее вероятность того, что случайная величина попадет в определенный диапазон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плотность",
                                "target_lang_sense_clarification": "вероятности, распределения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f7f09a5-bce1-34b6-b402-2f406dc3a86f",
                    "sense_definition": "The quality of being slow to understand or stupid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was frustrated by the sheer <w>density</w> of his mind; he simply could not grasp the concept.",
                            "target_lang_translations": {
                                "ru": "Меня расстраивала явная <w>тупость</w> его ума; он просто не мог уловить суть."
                            }
                        },
                        {
                            "text": "Her <w>density</w> regarding the joke made the situation awkward for everyone.",
                            "target_lang_translations": {
                                "ru": "Её <w>непробиваемость</w> в отношении этой шутки создала неловкую ситуацию для всех."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Refers to mental slowness."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество, характеризующееся медленным пониманием или глупостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тупость",
                                "target_lang_sense_clarification": "глупость, бестолковость"
                            },
                            {
                                "target_lang_word": "непробиваемость",
                                "target_lang_sense_clarification": "неспособность понять что-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
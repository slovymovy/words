    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "15d5bed5-a1a6-38a6-b584-fa98fee10255",
                    "sense_definition": "Een open platform dat uit de gevel van een gebouw naar voren steekt, meestal voorzien van een hek of balustrade.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer zitten we graag buiten op het <w>balkon</w>.",
                            "target_lang_translations": {
                                "ru": "Летом мы любим сидеть на <w>балконе</w>."
                            }
                        },
                        {
                            "text": "Het appartement heeft een klein <w>balkon</w> op het zuiden.",
                            "target_lang_translations": {
                                "ru": "В квартире есть маленький <w>балкон</w>, выходящий на юг."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op het balkon",
                        "Frans balkon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытая площадка с ограждением, выступающая из стены здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балкон",
                                "target_lang_sense_clarification": "архитектурный элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e9eb863-09f2-319c-8773-a19e008d53f5",
                    "sense_definition": "Een bovenverdieping met zitplaatsen die uitsteekt boven de zaal in een theater of bioscoop.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hadden goedkope plaatsen op het tweede <w>balkon</w>.",
                            "target_lang_translations": {
                                "ru": "У нас были дешевые места на втором <w>ярусе</w>."
                            }
                        },
                        {
                            "text": "Vanaf het <w>balkon</w> heb je een goed overzicht over het hele podium.",
                            "target_lang_translations": {
                                "ru": "С <w>балкона</w> открывается хороший обзор всей сцены."
                            }
                        }
                    ],
                    "antonyms": [
                        "zaal"
                    ],
                    "common_phrases": [
                        "eerste balkon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхний ярус с местами для зрителей в театре или кинотеатре, нависающий над партером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балкон",
                                "target_lang_sense_clarification": "места в театре"
                            },
                            {
                                "target_lang_word": "ярус",
                                "target_lang_sense_clarification": "этаж в зрительном зале"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20e26388-8617-33ee-94c8-236e4d145c34",
                    "sense_definition": "De ruimte bij de in- en uitgang van een treinwagon of tram.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De conducteur controleerde de kaartjes van de mensen op het <w>balkon</w>.",
                            "target_lang_translations": {
                                "ru": "Кондуктор проверял билеты у людей в <w>тамбуре</w>."
                            }
                        },
                        {
                            "text": "Het is niet toegestaan om tijdens de rit op het <w>balkon</w> te blijven staan.",
                            "target_lang_translations": {
                                "ru": "Во время движения запрещено стоять в <w>тамбуре</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пространство у дверей вагона поезда или трамвая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тамбур",
                                "target_lang_sense_clarification": "в вагоне"
                            },
                            {
                                "target_lang_word": "площадка",
                                "target_lang_sense_clarification": "в трамвае"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
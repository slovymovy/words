    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cd670644-d4eb-32aa-a669-45aa720219d9",
                    "sense_definition": "Приближаться к кому-либо или чему-либо, сокращая расстояние.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я видел, как наш автобус <w>подходит</w> к остановке.",
                            "target_lang_translations": {
                                "en": "I saw our bus <w>approaching</w> the stop."
                            }
                        },
                        {
                            "text": "Не <w>подходи</w> к краю платформы, это может быть опасно.",
                            "target_lang_translations": {
                                "en": "Do not <w>approach</w> the edge of the platform, it can be dangerous."
                            }
                        }
                    ],
                    "synonyms": [
                        "подниматься"
                    ],
                    "antonyms": [
                        "отходить"
                    ],
                    "common_phrases": [
                        "подходить к двери",
                        "подходить ближе",
                        "подходить к телефону"
                    ],
                    "target_lang_definitions": {
                        "en": "To move closer to someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "approach",
                                "target_lang_sense_clarification": "move towards"
                            },
                            {
                                "target_lang_word": "come up",
                                "target_lang_sense_clarification": "move towards, usually to a person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e6435c8-d0e9-3efb-947b-a1ad2d03f789",
                    "sense_definition": "Быть годным, приемлемым, соответствовать требованиям, размеру или внешнему виду.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот ключ не <w>подходит</w> к замку, попробуй другой.",
                            "target_lang_translations": {
                                "en": "This key does not <w>fit</w> the lock, try another one."
                            }
                        },
                        {
                            "text": "Тебе очень <w>подходит</w> это синее платье.",
                            "target_lang_translations": {
                                "en": "That blue dress really <w>suits</w> you."
                            }
                        }
                    ],
                    "synonyms": [
                        "годиться"
                    ],
                    "common_phrases": [
                        "идеально подходить",
                        "подходить по размеру",
                        "это мне не подходит"
                    ],
                    "target_lang_definitions": {
                        "en": "To be the right size, shape, or type; to look good on someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fit",
                                "target_lang_sense_clarification": "be the right size or shape"
                            },
                            {
                                "target_lang_word": "suit",
                                "target_lang_sense_clarification": "look good on or be convenient"
                            },
                            {
                                "target_lang_word": "do",
                                "target_lang_sense_clarification": "be sufficient or acceptable"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a1965af-09f6-3aa8-aa89-c0c47718bc00",
                    "sense_definition": "Относиться к чему-либо определенным образом, выбирать способ решения задачи или общения.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К выбору будущей профессии нужно <w>подходить</w> очень ответственно.",
                            "target_lang_translations": {
                                "en": "One must <w>approach</w> the choice of a future profession very responsibly."
                            }
                        },
                        {
                            "text": "Учитель старается индивидуально <w>подходить</w> к каждому ученику.",
                            "target_lang_translations": {
                                "en": "The teacher tries to <w>approach</w> each student individually."
                            }
                        }
                    ],
                    "common_phrases": [
                        "подходить творчески",
                        "подходить с умом",
                        "подходить серьезно"
                    ],
                    "target_lang_definitions": {
                        "en": "To deal with a problem, task, or person in a specific way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "approach",
                                "target_lang_sense_clarification": "deal with or handle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df631deb-51da-32b3-8058-00bdad13f3af",
                    "sense_definition": "Увеличиваться в объёме в процессе брожения (о тесте).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поставь миску в теплое место, чтобы тесто быстрее <w>подходило</w>.",
                            "target_lang_translations": {
                                "en": "Put the bowl in a warm place so the dough <w>rises</w> faster."
                            }
                        },
                        {
                            "text": "Мама проверяла, хорошо ли <w>подходит</w> опара для пирогов.",
                            "target_lang_translations": {
                                "en": "Mom checked if the sponge dough was <w>rising</w> well for the pies."
                            }
                        }
                    ],
                    "synonyms": [
                        "подниматься"
                    ],
                    "antonyms": [
                        "опадать"
                    ],
                    "common_phrases": [
                        "тесто подходит",
                        "опара подходит"
                    ],
                    "target_lang_definitions": {
                        "en": "To expand or increase in volume (specifically of dough)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "expand (of dough)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "9362cb6a-98c3-386f-b412-7c29dee70ce4",
                    "sense_definition": "A religious society or brotherhood, especially one devoted to charitable causes.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_org",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local <w>confraternity</w> organized a festival to raise money for the poor.",
                            "target_lang_translations": {
                                "ru": "Местное <w>братство</w> организовало фестиваль, чтобы собрать деньги для бедных."
                            }
                        },
                        {
                            "text": "He was a dedicated member of a religious <w>confraternity</w> for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Он был преданным членом религиозного <w>братства</w> более двадцати лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "brotherhood",
                        "order",
                        "guild"
                    ],
                    "common_phrases": [
                        "religious confraternity",
                        "local confraternity"
                    ],
                    "target_lang_definitions": {

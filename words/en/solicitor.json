{
    "word_family": [
        "solicit",
        "solicitation",
        "solicitous",
        "solicitude"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bbabd753-ba16-3146-ac2c-8400d6effe04",
                    "sense_definition": "A qualified lawyer in the UK and some other countries who gives legal advice to clients, prepares legal documents, and handles cases, but typically does not represent people in higher courts.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_professional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to hire a <w>solicitor</w> to help us buy the new house.",
                            "target_lang_translations": {
                                "ru": "Нам нужно нанять <w>солиситора</w>, чтобы он помог нам купить новый дом."
                            }
                        },
                        {
                            "text": "Her <w>solicitor</w> advised her not to sign the contract until the terms were changed.",
                            "target_lang_translations": {
                                "ru": "Её <w>адвокат</w> посоветовал ей не подписывать контракт, пока условия не будут изменены."
                            }
                        }
                    ],
                    "synonyms": [
                        "lawyer",
                        "legal practitioner"
                    ],
                    "antonyms": [
                        "barrister"
                    ],
                    "common_phrases": [
                        "firm of solicitors",
                        "instruct a solicitor",
                        "duty solicitor"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK, Australia, New Zealand"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Квалифицированный юрист в Великобритании и некоторых других странах, который консультирует клиентов и готовит документы, но обычно не выступает в высших судах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солиситор",
                                "target_lang_sense_clarification": "адвокат в Великобритании"
                            },
                            {
                                "target_lang_word": "адвокат",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "юрисконсульт",
                                "target_lang_sense_clarification": "консультант по правовым вопросам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb52729a-5809-39b3-b35e-6a93b3a66538",
                    "sense_definition": "A person who approaches people, especially door-to-door, to sell products or ask for donations.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "sales_agent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sign on the front gate clearly stated that no <w>solicitors</w> were allowed.",
                            "target_lang_translations": {
                                "ru": "Табличка на воротах ясно гласила, что <w>торговым агентам</w> вход воспрещён."
                            }
                        },
                        {
                            "text": "A <w>solicitor</w> knocked on the door trying to sell magazine subscriptions.",
                            "target_lang_translations": {
                                "ru": "<w>Коммивояжёр</w> постучал в дверь, пытаясь продать подписку на журналы."
                            }
                        }
                    ],
                    "synonyms": [
                        "canvasser",
                        "peddler",
                        "hawker"
                    ],
                    "common_phrases": [
                        "no solicitors",
                        "door-to-door solicitor"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который подходит к людям, часто ходя от двери к двери, чтобы продать товары или попросить о пожертвованиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коммивояжёр",
                                "target_lang_sense_clarification": "разъездной торговец"
                            },
                            {
                                "target_lang_word": "торговый агент",
                                "target_lang_sense_clarification": "представитель по продажам"
                            },
                            {
                                "target_lang_word": "распространитель",
                                "target_lang_sense_clarification": "товаров или билетов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aed34b20-47f9-39bc-a0ff-37a2c9e5b281",
                    "sense_definition": "The chief law officer of a city, town, or government department.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The town <w>solicitor</w> reviewed the proposed zoning laws for legal issues.",
                            "target_lang_translations": {
                                "ru": "Городской <w>юрисконсульт</w> рассмотрел предлагаемые законы о зонировании на предмет юридических проблем."
                            }
                        },
                        {
                            "text": "She was appointed as the city <w>solicitor</w> after years of private practice.",
                            "target_lang_translations": {
                                "ru": "После многих лет частной практики её назначили городским <w>юрисконсультантом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "legal officer"
                    ],
                    "common_phrases": [
                        "town solicitor",
                        "city solicitor",
                        "Solicitor General"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Главный юрист города, поселка или правительственного ведомства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юрисконсульт",
                                "target_lang_sense_clarification": "должностное лицо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afc9fb67-faa2-3a90-af70-79ea6c61d8a3",
                    "sense_definition": "A person who asks for or requests something earnestly.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "requester",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was a persistent <w>solicitor</w> of favors from his wealthy relatives.",
                            "target_lang_translations": {
                                "ru": "Он был настойчивым <w>просителем</w> услуг у своих богатых родственников."
                            }
                        },
                        {
                            "text": "As a <w>solicitor</w> of peace, she traveled to many conflict zones.",
                            "target_lang_translations": {
                                "ru": "Как <w>ходатай</w> мира, она посетила множество зон конфликтов."
                            }
                        }
                    ],
                    "synonyms": [
                        "petitioner",
                        "supplicant",
                        "applicant"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто настойчиво просит или ходатайствует о чём-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проситель",
                                "target_lang_sense_clarification": "тот, кто просит"
                            },
                            {
                                "target_lang_word": "ходатай",
                                "target_lang_sense_clarification": "официальное или формальное прошение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "cool",
        "cooling-down"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1634a965-678b-3360-a254-2df73a1df873",
                    "sense_definition": "Lichte oefeningen of een rustige activiteit na een zware inspanning om het lichaam weer tot rust te laten komen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sport_recovery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vergeet niet om een <w>cooldown</w> te doen na het hardlopen.",
                            "target_lang_translations": {
                                "ru": "Не забудь сделать <w>заминку</w> после бега.",
                                "en": "Don't forget to do a <w>cool-down</w> after running."
                            }
                        },
                        {
                            "text": "De trainer gaf ons tien minuten voor de <w>cooldown</w>.",
                            "target_lang_translations": {
                                "ru": "Тренер дал нам десять минут на <w>заминку</w>.",
                                "en": "The trainer gave us ten minutes for the <w>cool-down</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cooling-down",
                        "uitlopen"
                    ],
                    "antonyms": [
                        "warming-up"
                    ],
                    "common_phrases": [
                        "een korte cooldown",
                        "tijdens de cooldown",
                        "na de cooldown"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкие упражнения или спокойная активность после интенсивной нагрузки для восстановления организма.",
                        "en": "Light exercises or a quiet activity performed after heavy exertion to allow the body to return to a resting state."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заминка",
                                "target_lang_sense_clarification": "спорт"
                            },
                            {
                                "target_lang_word": "восстановление",
                                "target_lang_sense_clarification": "отдых после нагрузки"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "cool-down",
                                "target_lang_sense_clarification": "exercise recovery"
                            },
                            {
                                "target_lang_word": "warm-down",
                                "target_lang_sense_clarification": "alternative term for exercise recovery"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1739685e-620c-348c-95fd-208be0ed8f0e",
                    "sense_definition": "Het proces van afkoelen van een machine, systeem of persoon die te warm is geworden.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "general_cooling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De motor heeft een korte <w>cooldown</w> nodig voordat hij weer veilig kan starten.",
                            "target_lang_translations": {
                                "ru": "Двигателю нужно немного времени на <w>остывание</w>, прежде чем он сможет безопасно запуститься снова.",
                                "en": "The engine needs a short <w>cool-down</w> before it can safely start again."
                            }
                        },
                        {
                            "text": "Tijdens de <w>cooldown</w> daalt de temperatuur van het apparaat langzaam.",
                            "target_lang_translations": {
                                "ru": "Во время <w>охлаждения</w> температура устройства медленно падает.",
                                "en": "During the <w>cool-down</w>, the temperature of the device drops slowly."
                            }
                        }
                    ],
                    "synonyms": [
                        "afkoeling"
                    ],
                    "antonyms": [
                        "opwarming"
                    ],
                    "common_phrases": [
                        "automatische cooldown",
                        "tijd voor cooldown"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс остывания машины, системы или человека после перегрева.",
                        "en": "The process of lowering the temperature of a machine, system, or person that has become too hot."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остывание",
                                "target_lang_sense_clarification": "процесс снижения температуры"
                            },
                            {
                                "target_lang_word": "охлаждение",
                                "target_lang_sense_clarification": "технический процесс"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "cooling down",
                                "target_lang_sense_clarification": "process of temperature reduction"
                            },
                            {
                                "target_lang_word": "cool-down",
                                "target_lang_sense_clarification": "period of cooling"
                            }
                        ]
                    }
                }
            ]
        }
    ]

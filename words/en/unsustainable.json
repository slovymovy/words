    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5c8ea5ae-9da3-3703-9619-6a488eae7e05",
                    "sense_definition": "Unable to be continued or maintained at the same level or rate.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The current rate of spending is <w>unsustainable</w> and will lead to debt.",
                            "target_lang_translations": {
                                "ru": "Текущий уровень расходов <w>неустойчив</w> и приведет к долгам."
                            }
                        },
                        {
                            "text": "Rapid industrialization often results in <w>unsustainable</w> growth that harms the environment.",
                            "target_lang_translations": {
                                "ru": "Быстрая индустриализация часто приводит к <w>неустойчивому</w> росту, который наносит вред окружающей среде."
                            }
                        }
                    ],
                    "synonyms": [
                        "unmaintainable",
                        "untenable",
                        "insupportable"
                    ],
                    "antonyms": [
                        "sustainable",
                        "viable",
                        "maintainable"
                    ],
                    "common_phrases": [
                        "unsustainable growth",
                        "unsustainable debt",
                        "environmentally unsustainable",
                        "unsustainable level"
                    ],
                    "target_lang_definitions": {
                        "ru": "Невозможность продолжать или поддерживать что-либо на том же уровне или с той же скоростью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неустойчивый",
                                "target_lang_sense_clarification": "о развитии, росте или состоянии, которое нельзя поддерживать долго"
                            },
                            {
                                "target_lang_word": "нежизнеспособный",
                                "target_lang_sense_clarification": "о планах, методах или стратегиях, обреченных на провал"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1eafd854-0527-3c72-8c01-5a60af51876f",
                    "sense_definition": "Something that cannot be maintained or continued.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to identify the <w>unsustainables</w> in our business model and remove them.",
                            "target_lang_translations": {
                                "ru": "Нам нужно выявить <w>нежизнеспособные элементы</w> в нашей бизнес-модели и устранить их."
                            }
                        },
                        {
                            "text": "The report lists various economic <w>unsustainables</w> that threaten the market stability.",
                            "target_lang_translations": {
                                "ru": "В отчете перечислены различные экономические <w>факторы нестабильности</w>, которые угрожают стабильности рынка."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что невозможно поддерживать или продолжать (обычно используется во множественном числе для обозначения факторов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неустойчивый фактор",
                                "target_lang_sense_clarification": "элемент системы, нарушающий её стабильность"
                            },
                            {
                                "target_lang_word": "слабое звено",
                                "target_lang_sense_clarification": "уязвимый элемент, который нельзя сохранить"
                            }
                        ]
                    }
                }
            ]
        }

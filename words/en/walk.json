    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0da0f2a8-3ce4-36d6-9365-c6e71cf53648",
                    "sense_definition": "To move on foot at a moderate pace, having at least one foot on the ground at all times.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locomotion_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>walk</w> to school every single day to stay healthy.",
                            "target_lang_translations": {
                                "ru": "Я <w>хожу</w> в школу каждый день, чтобы оставаться здоровым."
                            }
                        },
                        {
                            "text": "She likes to <w>walk</w> around the park in the afternoon.",
                            "target_lang_translations": {
                                "ru": "Ей нравится <w>гулять</w> по парку днем."
                            }
                        }
                    ],
                    "synonyms": [
                        "stroll",
                        "ambulate"
                    ],
                    "antonyms": [
                        "run"
                    ],
                    "common_phrases": [
                        "walk away",
                        "walk around",
                        "walk in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передвигаться пешком с умеренной скоростью, всегда касаясь земли хотя бы одной ногой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "способность двигаться или повторяющееся действие"
                            },
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "процесс движения в конкретный момент или в одном направлении"
                            },
                            {
                                "target_lang_word": "гулять",
                                "target_lang_sense_clarification": "ходить ради удовольствия, прогуливаться"
                            },
                            {
                                "target_lang_word": "шагать",
                                "target_lang_sense_clarification": "делать шаги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8396770e-3296-3bf8-b008-a2f418cbf6a8",
                    "sense_definition": "To accompany someone on foot or guide an animal while walking.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "escort_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is my responsibility to <w>walk</w> the dog every morning before breakfast.",
                            "target_lang_translations": {
                                "ru": "Моя обязанность — <w>выгуливать</w> собаку каждое утро перед завтраком."
                            }
                        },
                        {
                            "text": "He offered to <w>walk</w> her home after the movie ended.",
                            "target_lang_translations": {
                                "ru": "Он предложил <w>проводить</w> её до дома после окончания фильма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "walk the dog",
                        "walk someone home"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сопровождать кого-либо пешком или выводить животное на прогулку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выгуливать",
                                "target_lang_sense_clarification": "выводить на прогулку (о животных)"
                            },
                            {
                                "target_lang_word": "провожать",
                                "target_lang_sense_clarification": "сопровождать кого-то до места назначения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "902998bd-90e0-3f83-bdc5-1432458f492d",
                    "sense_definition": "To travel a specific distance or route by walking.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "traverse_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to <w>walk</w> the trail despite the heavy rain.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>пройти</w> по тропе, несмотря на сильный дождь."
                            }
                        },
                        {
                            "text": "He <w>walked</w> the streets for hours looking for a place to eat.",
                            "target_lang_translations": {
                                "ru": "Он часами <w>бродил</w> по улицам в поисках места, где можно поесть."
                            }
                        }
                    ],
                    "synonyms": [
                        "hike",
                        "trek"
                    ],
                    "common_phrases": [
                        "walk the streets",
                        "walk the path"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить определенное расстояние или маршрут пешком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проходить",
                                "target_lang_sense_clarification": "преодолевать расстояние"
                            },
                            {
                                "target_lang_word": "идти по",
                                "target_lang_sense_clarification": "следовать маршруту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f69da6c8-4cfa-312b-acf6-19cc114f0278",
                    "sense_definition": "To leave a job, negotiation, or situation abruptly, often due to disagreement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leave_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If they do not agree to our terms, we are prepared to <w>walk</w>.",
                            "target_lang_translations": {
                                "ru": "Если они не согласятся на наши условия, мы готовы <w>уйти</w>."
                            }
                        },
                        {
                            "text": "The manager threatened to <w>walk</w> if the budget was cut again.",
                            "target_lang_translations": {
                                "ru": "Менеджер пригрозил <w>уволиться</w>, если бюджет снова урежут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "walk away from",
                        "threaten to walk"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in business or negotiation contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно покинуть работу, переговоры или ситуацию, часто из-за несогласия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "покидать место или должность"
                            },
                            {
                                "target_lang_word": "отказываться",
                                "target_lang_sense_clarification": "прекращать участие в чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52d4df91-f959-3525-8bdf-663f516a1ebf",
                    "sense_definition": "To be acquitted or released from a criminal charge.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Without more evidence, the suspect is likely to <w>walk</w>.",
                            "target_lang_translations": {
                                "ru": "Без дополнительных улик подозреваемый, скорее всего, <w>выйдет на свободу</w>."
                            }
                        },
                        {
                            "text": "Everyone was shocked when the thief managed to <w>walk</w> free.",
                            "target_lang_translations": {
                                "ru": "Все были шокированы, когда вору удалось <w>избежать наказания</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "get off",
                        "go free",
                        "be acquit"
                    ],
                    "common_phrases": [
                        "walk free"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in legal or crime contexts"
                        }

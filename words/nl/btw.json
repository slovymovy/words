                                "target_lang_sense_clarification": "general consumption tax"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "НДС",
                                "target_lang_sense_clarification": "налог"
                            },
                            {
                                "target_lang_word": "налог на добавленную стоимость",
                                "target_lang_sense_clarification": "полная форма"
                            }

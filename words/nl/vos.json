            "senses": [
                {
                    "sense_id": "c65358bc-e363-3c1b-a13c-33ce525d9f62",
                    "sense_definition": "Een veelvoorkomende Nederlandse achternaam.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Meneer <w>Vos</w> woont al jaren in deze straat.",
                            "target_lang_translations": {
                                "en": "Mr. <w>Vos</w> has lived on this street for years.",
                                "ru": "Господин <w>Вос</w> живет на этой улице уже много лет."
                            }
                        },
                        {
                            "text": "Ik heb gisteren een afspraak gemaakt met mevrouw <w>Vos</w>.",
                            "target_lang_translations": {
                                "en": "I made an appointment with Mrs. <w>Vos</w> yesterday.",
                                "ru": "Вчера я договорился о встрече с госпожой <w>Вос</w>."
                            }
                        }
                    ],

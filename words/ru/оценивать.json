    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "02501517-e49f-3fcc-93bb-f6eda180957d",
                    "sense_definition": "Составлять мнение о качестве, характере, значении или достоинствах кого-либо или чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "judgment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель должен объективно <w>оценивать</w> знания своих студентов.",
                            "target_lang_translations": {
                                "en": "A teacher must objectively <w>evaluate</w> their students' knowledge."
                            }
                        },
                        {
                            "text": "Не стоит <w>оценивать</w> книгу только по её обложке.",
                            "target_lang_translations": {
                                "en": "One should not <w>judge</w> a book only by its cover."
                            }
                        }
                    ],
                    "synonyms": [
                        "судить",
                        "расценивать"
                    ],
                    "common_phrases": [
                        "оценивать ситуацию",
                        "оценивать возможности",
                        "оценивать результаты",
                        "высоко оценивать",
                        "трезво оценивать"
                    ],
                    "target_lang_definitions": {
                        "en": "To form an opinion about the quality, character, importance, or merits of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "evaluate",
                                "target_lang_sense_clarification": "to analyze and determine quality or merit"
                            },
                            {
                                "target_lang_word": "assess",
                                "target_lang_sense_clarification": "to judge a situation, facts, or results"
                            },
                            {
                                "target_lang_word": "judge",
                                "target_lang_sense_clarification": "to form a critical opinion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b889d208-531b-32bc-911f-c79839be5f55",
                    "sense_definition": "Определять стоимость чего-либо в денежном выражении.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "monetary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Специалист приедет завтра, чтобы <w>оценивать</w> нашу квартиру для продажи.",
                            "target_lang_translations": {
                                "en": "A specialist will arrive tomorrow to <w>appraise</w> our apartment for sale."
                            }
                        },
                        {
                            "text": "Страховая компания будет <w>оценивать</w> ущерб от аварии.",
                            "target_lang_translations": {
                                "en": "The insurance company will <w>assess</w> the damage from the accident."
                            }
                        }
                    ],
                    "common_phrases": [
                        "оценивать ущерб",
                        "оценивать стоимость",
                        "оценивать имущество",
                        "оценивать в рублях"
                    ],
                    "target_lang_definitions": {
                        "en": "To determine the monetary value or cost of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "value",
                                "target_lang_sense_clarification": "to determine the price or worth"
                            },
                            {
                                "target_lang_word": "appraise",
                                "target_lang_sense_clarification": "to estimate value officially (e.g., real estate, jewelry)"
                            },
                            {
                                "target_lang_word": "assess",
                                "target_lang_sense_clarification": "to determine value for tax or damage purposes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f442000c-97b0-392f-afb3-266767955a7e",
                    "sense_definition": "Приблизительно определять размер, количество или объём чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный водитель умеет мгновенно <w>оценивать</w> расстояние до других машин.",
                            "target_lang_translations": {
                                "en": "An experienced driver knows how to instantly <w>estimate</w> the distance to other cars."
                            }
                        },
                        {
                            "text": "Мы можем лишь приблизительно <w>оценивать</w> время, необходимое для работы.",
                            "target_lang_translations": {
                                "en": "We can only roughly <w>estimate</w> the time needed for the work."
                            }
                        }
                    ],
                    "synonyms": [
                        "прикидывать"
                    ],
                    "common_phrases": [
                        "оценивать на глаз",
                        "оценивать расстояние",
                        "оценивать объём"
                    ],
                    "target_lang_definitions": {
                        "en": "To determine the approximate size, quantity, or volume of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "estimate",
                                "target_lang_sense_clarification": "to calculate approximately"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
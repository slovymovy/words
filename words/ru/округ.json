    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f54e78f1-6842-3e95-80ef-0757ff296cc1",
                    "sense_definition": "Административно-территориальная единица, часть территории государства или города, имеющая собственное управление.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "territorial_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В состав России входят края, области и автономные <w>округа</w>.",
                            "target_lang_translations": {
                                "en": "Russia includes territories, regions, and autonomous <w>districts</w>."
                            }
                        },
                        {
                            "text": "Наш городской <w>округ</w> активно развивается, и здесь строят новые школы.",
                            "target_lang_translations": {
                                "en": "Our urban <w>district</w> is developing actively, and new schools are being built here."
                            }
                        }
                    ],
                    "synonyms": [
                        "район"
                    ],
                    "common_phrases": [
                        "автономный округ",
                        "федеральный округ",
                        "городской округ",
                        "административный округ"
                    ],
                    "target_lang_definitions": {
                        "en": "An administrative-territorial unit, a part of a state or city territory having its own administration."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "district",
                                "target_lang_sense_clarification": "administrative division"
                            },
                            {
                                "target_lang_word": "region",
                                "target_lang_sense_clarification": "broad administrative area"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27fa2820-0802-3484-9e19-b0e4ad4936a2",
                    "sense_definition": "Территория, специально выделенная для проведения выборов (избирательный округ).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "territorial_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Депутат объехал весь свой избирательный <w>округ</w> перед выборами.",
                            "target_lang_translations": {
                                "en": "The deputy toured his entire electoral <w>district</w> before the elections."
                            }
                        },
                        {
                            "text": "Голосование в одномандатных <w>округах</w> прошло без серьёзных нарушений.",
                            "target_lang_translations": {
                                "en": "Voting in single-mandate <w>districts</w> proceeded without serious violations."
                            }
                        }
                    ],
                    "common_phrases": [
                        "избирательный округ",
                        "одномандатный округ"
                    ],
                    "target_lang_definitions": {
                        "en": "A territory specifically designated for holding elections (electoral district)."
                    },
                    "translations": {

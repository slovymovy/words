    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "14f26b9f-88af-3e51-a318-ed7df18daffa",
                    "sense_definition": "Het kleinste tandwiel aan de trapas van een fiets, gebruikt om lichter te trappen, bijvoorbeeld bij het klimmen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen de berg steiler werd, schakelde hij direct naar het <w>binnenblad</w>.",
                            "target_lang_translations": {
                                "en": "When the mountain became steeper, he immediately shifted to the <w>inner ring</w>."
                            }
                        },
                        {
                            "text": "Mijn ketting liep eraf toen ik probeerde naar het <w>binnenblad</w> te wisselen.",
                            "target_lang_translations": {
                                "en": "My chain came off when I tried to switch to the <w>inner ring</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "buitenblad"
                    ],
                    "common_phrases": [
                        "op het binnenblad",
                        "naar het binnenblad schakelen"
                    ],
                    "target_lang_definitions": {
                        "en": "The smallest chainring on a bicycle's crankset, used for easier pedaling, for example when climbing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inner ring",
                                "target_lang_sense_clarification": "cycling"
                            },
                            {
                                "target_lang_word": "small ring",
                                "target_lang_sense_clarification": "cycling"
                            },
                            {
                                "target_lang_word": "inner chainring",
                                "target_lang_sense_clarification": "cycling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "faa72be8-a0cd-3c4a-91a7-476d6038d558",
                    "sense_definition": "De binnenste muur van een spouwmuur, die meestal de dragende functie van de constructie vervult.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aannemer bracht een dikke laag isolatie aan tegen het <w>binnenblad</w>.",
                            "target_lang_translations": {
                                "en": "The contractor applied a thick layer of insulation against the <w>inner leaf</w>."
                            }
                        },
                        {
                            "text": "Bij deze woning is het <w>binnenblad</w> gemaakt van kalkzandsteen.",
                            "target_lang_translations": {
                                "en": "In this house, the <w>inner leaf</w> is made of calcium silicate brick."
                            }
                        }
                    ],
                    "synonyms": [
                        "binnenspouwblad"
                    ],
                    "antonyms": [
                        "buitenblad"
                    ],
                    "target_lang_definitions": {
                        "en": "The inner wall of a cavity wall, usually fulfilling the load-bearing function of the structure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inner leaf",
                                "target_lang_sense_clarification": "construction"
                            },
                            {
                                "target_lang_word": "inner skin",
                                "target_lang_sense_clarification": "construction"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "f5e00e51-9fc8-30b0-9d03-99888b5850b4",
                    "sense_definition": "Имеющий большую толщину, большое расстояние между противоположными поверхностями (о неодушевлённых предметах).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "dimensions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта <толстая> книга не помещается в мою школьную сумку.",
                            "target_lang_translations": {
                                "en": "This <thick> book does not fit in my school bag."
                            }
                        },
                        {
                            "text": "Зимой на озере образовался очень <толстый> лёд.",
                            "target_lang_translations": {
                                "en": "In winter, very <thick> ice formed on the lake."
                            }
                        }
                    ],
                    "antonyms": [
                        "тонкий"
                    ],
                    "common_phrases": [
                        "толстая книга",
                        "толстый слой",
                        "толстые стены",
                        "толстый лёд"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a relatively great distance between opposite surfaces (of inanimate objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thick",
                                "target_lang_sense_clarification": "general term for dimensions (walls, ice, books)"
                            },
                            {
                                "target_lang_word": "fat",
                                "target_lang_sense_clarification": "colloquial, often used for books, wallets, or envelopes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2a5cd7e-39aa-321e-9fa0-a7b1963dceed",
                    "sense_definition": "Имеющий избыточный вес, много жира; полный, упитанный (о человеке или животном).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой <толстый> кот любит спать на диване весь день.",
                            "target_lang_translations": {
                                "en": "My <fat> cat loves to sleep on the sofa all day."
                            }
                        },
                        {
                            "text": "<Толстый> пассажир с трудом занял своё место в автобусе.",
                            "target_lang_translations": {
                                "en": "The <fat> passenger had trouble taking his seat on the bus."
                            }
                        }
                    ],
                    "synonyms": [
                        "полный",
                        "упитанный",
                        "тучный",
                        "грузный",
                        "жирный"
                    ],
                    "antonyms": [
                        "худой",
                        "худощавый",
                        "тонкий"
                    ],
                    "common_phrases": [
                        "толстый кот",
                        "очень толстый",
                        "стать толстым",
                        "толстый живот"
                    ],
                    "target_lang_definitions": {
                        "en": "Having excess body weight or fat; corpulent (of a person or animal)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fat",
                                "target_lang_sense_clarification": "most common, direct term"
                            },
                            {
                                "target_lang_word": "overweight",
                                "target_lang_sense_clarification": "more formal or polite"
                            },
                            {
                                "target_lang_word": "stout",
                                "target_lang_sense_clarification": "heavily built, thickset"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ee22ddd-82ac-336b-bd38-60b93ce20a1d",
                    "sense_definition": "Полный, упитанный человек (используется как существительное).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "body_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Эй, <толстый>, подожди нас! — крикнули ребята во дворе.",
                            "target_lang_translations": {
                                "en": "\"Hey, <fatty>, wait for us!\" shouted the kids in the yard."
                            }
                        },
                        {
                            "text": "В комнату вошёл улыбающийся <толстый> с большим тортом в руках.",
                            "target_lang_translations": {
                                "en": "A smiling <fat man> entered the room with a big cake in his hands."
                            }
                        }
                    ],

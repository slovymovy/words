                {
                    "sense_id": "f1c3f76f-831a-3b7b-9121-483e3f27bb18",
                    "sense_definition": "De hoeveelheid vloeistof of gas die per tijdseenheid door een leiding of rivier stroomt.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de hevige regenval is het <w>debiet</w> van de Maas sterk toegenomen.",
                            "target_lang_translations": {
                                "en": "Due to the heavy rainfall, the <w>flow rate</w> of the Meuse has increased sharply.",
                                "ru": "Из-за сильных дождей <w>расход</w> воды в Маасе значительно увеличился."
                            }
                        },
                        {
                            "text": "De ingenieur berekende het gemiddeld <w>debiet</w> van de waterleiding.",
                            "target_lang_translations": {
                                "en": "The engineer calculated the average <w>flow rate</w> of the water pipe.",
                                "ru": "Инженер рассчитал средний <w>расход</w> воды в трубопроводе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gemiddeld debiet",
                        "constant debiet"
                    ],
                    "target_lang_definitions": {
                        "en": "The volume of fluid or gas flowing through a pipe or river per unit of time.",
                        "ru": "Объем жидкости или газа, протекающий через трубу или русло реки за единицу времени."
                    },

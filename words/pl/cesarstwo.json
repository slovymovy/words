    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f95e6579-b59f-34b4-a1b6-b287e7c9fc3e",
                    "sense_definition": "Państwo rządzone przez cesarza lub cesarzową.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "politics_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starożytne <w>cesarstwo</w> obejmowało ogromne terytoria wokół morza.",
                            "target_lang_translations": {
                                "ru": "Древняя <w>империя</w> охватывала огромные территории вокруг моря."
                            }
                        },
                        {
                            "text": "To potężne <w>cesarstwo</w> przetrwało wiele stuleci mimo licznych wojen.",
                            "target_lang_translations": {
                                "ru": "Эта могущественная <w>империя</w> просуществовала много столетий, несмотря на многочисленные войны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Cesarstwo Rzymskie",
                        "granice cesarstwa",
                        "upadek cesarstwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государство, во главе которого стоит император или императрица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "империя",
                                "target_lang_sense_clarification": "государство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4990f042-0228-354e-b219-0e7c310c5161",
                    "sense_definition": "Ustrój polityczny, w którym najwyższą władzę sprawuje cesarz; także godność i tytuł cesarza.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "politics_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Japonia jest obecnie jedynym krajem, który zachował <w>cesarstwo</w>.",
                            "target_lang_translations": {
                                "ru": "Япония в настоящее время является единственной страной, сохранившей <w>империю</w>."
                            }
                        },
                        {
                            "text": "Napoleon dążył do tego, aby we Francji oficjalnie przywrócono <w>cesarstwo</w>.",
                            "target_lang_translations": {
                                "ru": "Наполеон стремился к тому, чтобы во Франции была официально восстановлена <w>империя</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "republika"
                    ],
                    "common_phrases": [
                        "ogłoszenie cesarstwa",
                        "przywrócenie cesarstwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Политическая система, в которой верховная власть принадлежит императору; также титул и достоинство императора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "империя",
                                "target_lang_sense_clarification": "политический строй"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56353fa2-111e-37ac-82d1-4a9aae354682",
                    "sense_definition": "Kategoria w klasyfikacji biologicznej wyższa od królestwa.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biology_taxonomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Badana istota należy do <w>cesarstwa</w> bakterii.",
                            "target_lang_translations": {
                                "ru": "Исследуемое существо относится к <w>надцарству</w> бактерий."
                            }
                        },
                        {
                            "text": "Współczesna biologia wyróżnia <w>cesarstwo</w> eukariontów, do którego należą zwierzęta.",
                            "target_lang_translations": {
                                "ru": "Современная биология выделяет <w>надцарство</w> эукариот, к которому относятся животные."
                            }
                        }
                    ],
                    "synonyms": [
                        "domena"
                    ],
                    "target_lang_definitions": {
                        "ru": "Категория в биологической классификации выше царства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надцарство",
                                "target_lang_sense_clarification": "биология"
                            },
                            {
                                "target_lang_word": "домен",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1996575d-f474-3f2c-8195-d464e24cd45e",
                    "sense_definition": "Cesarz wraz z małżonką.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na uroczysty bal przybyło ich <w>cesarstwo</w> w otoczeniu gwardii.",
                            "target_lang_translations": {
                                "ru": "На торжественный бал прибыли <w>Их Императорские Величества</w> в окружении гвардии."
                            }
                        },
                        {
                            "text": "Podczas wizyty w stolicy ich <w>cesarstwo</w> zamieszkało w letniej rezydencji.",
                            "target_lang_translations": {
                                "ru": "Во время визита в столицу <w>Их Императорские Величества</w> остановились в летней резиденции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ich cesarstwo"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "tytuł grzecznościowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Император вместе с супругой (обычно как почтительное обращение или титул)."
                    },
                    "translations": {
                        "ru": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9d1a52dd-8e80-32d6-b270-bf3b005eb9b3",
                    "sense_definition": "Ощущать тупую, тягучую, непрекращающуюся боль (обычно о зубах, голове, суставах или мышцах).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня весь день ужасно <w>ноет</w> зуб, придётся идти к стоматологу.",
                            "target_lang_translations": {
                                "en": "My tooth has been <w>aching</w> terribly all day, I'll have to go to the dentist."
                            }
                        },
                        {
                            "text": "После долгой прогулки у бабушки снова <w>ныли</w> ноги.",
                            "target_lang_translations": {
                                "en": "After the long walk, Grandma's legs were <w>aching</w> again."
                            }
                        }
                    ],
                    "synonyms": [
                        "ломить",
                        "мозжить"
                    ],
                    "common_phrases": [
                        "зуб ноет",
                        "сердце ноет",
                        "тело ноет",
                        "ноет спина"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel a dull, nagging, continuous pain (usually regarding teeth, head, joints, or muscles)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ache",
                                "target_lang_sense_clarification": "continuous dull pain"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "788be5c8-76de-3713-9059-5df994446836",
                    "sense_definition": "Надоедливо жаловаться на что-либо, выражать недовольство плаксивым, унылым тоном.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хватит <w>ныть</w> из-за плохой погоды, давай лучше посмотрим фильм.",
                            "target_lang_translations": {
                                "en": "Stop <w>whining</w> about the bad weather, let's watch a movie instead."
                            }
                        },
                        {
                            "text": "Он постоянно <w>ноет</w> о том, как тяжело ему живётся, но ничего не меняет.",
                            "target_lang_translations": {
                                "en": "He constantly <w>whines</w> about how hard his life is, but doesn't change anything."
                            }
                        }
                    ],
                    "synonyms": [
                        "жаловаться",
                        "плакаться",
                        "хныкать",
                        "сетовать",
                        "причитать"
                    ],
                    "common_phrases": [
                        "хватит ныть",
                        "перестань ныть",
                        "вечно ноет"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To complain annoyingly about something, expressing dissatisfaction in a tearful or dismal tone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whine",
                                "target_lang_sense_clarification": "complain peevishly"
                            },
                            {
                                "target_lang_word": "moan",
                                "target_lang_sense_clarification": "complain persistently"
                            },
                            {
                                "target_lang_word": "complain",
                                "target_lang_sense_clarification": "express dissatisfaction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e8e6f17-a681-3909-9d8d-4e010b34470f",
                    "sense_definition": "Издавать протяжные, жалобные звуки (о животных, ветре или музыкальных инструментах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За окном всю ночь тоскливо <w>ныл</w> осенний ветер.",
                            "target_lang_translations": {
                                "en": "Outside the window, the autumn wind <w>moaned</w> mournfully all night."
                            }
                        },
                        {
                            "text": "Маленький щенок сидел у двери и тихо <w>ныл</w>, ожидая хозяина.",
                            "target_lang_translations": {
                                "en": "The little puppy sat by the door and quietly <w>whined</w>, waiting for its owner."
                            }
                        }
                    ],
                    "synonyms": [
                        "скулить",
                        "выть",
                        "хныкать"
                    ],
                    "common_phrases": [
                        "ветер ноет"
                    ],
                    "target_lang_definitions": {
                        "en": "To make prolonged, mournful sounds (referring to animals, wind, or musical instruments)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whine",
                                "target_lang_sense_clarification": "make a high-pitched sound"
                            },
                            {
                                "target_lang_word": "moan",
                                "target_lang_sense_clarification": "make a mournful sound"
                            },
                            {
                                "target_lang_word": "howl",
                                "target_lang_sense_clarification": "make a long, loud sound (wind)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

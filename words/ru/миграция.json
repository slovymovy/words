    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c2badc4c-1087-399b-9ac5-1b151474e762",
                    "sense_definition": "Перемещение людей из одного региона или страны в другое место жительства, часто массовое.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "human_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Трудовая <w>миграция</w> оказывает большое влияние на экономику страны.",
                            "target_lang_translations": {
                                "en": "Labor <w>migration</w> has a major impact on the country's economy."
                            }
                        },
                        {
                            "text": "В последние годы <w>миграция</w> населения в крупные города значительно выросла.",
                            "target_lang_translations": {
                                "en": "In recent years, the <w>migration</w> of the population to large cities has increased significantly."
                            }
                        }
                    ],
                    "synonyms": [
                        "переселение"
                    ],
                    "common_phrases": [
                        "трудовая миграция",
                        "незаконная миграция",
                        "внутренняя миграция",
                        "внешняя миграция"
                    ],
                    "target_lang_definitions": {
                        "en": "The movement of people from one region or country to another place of residence, often en masse."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "migration",
                                "target_lang_sense_clarification": "movement of people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a9512a6-3e76-331d-8fab-7dc03d876ecf",
                    "sense_definition": "Сезонное или регулярное перемещение животных, птиц или рыб на большие расстояния.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Осенью начинается массовая <w>миграция</w> птиц на юг.",
                            "target_lang_translations": {
                                "en": "In autumn, the mass <w>migration</w> of birds to the south begins."
                            }
                        },
                        {
                            "text": "Учёные внимательно изучают пути <w>миграции</w> китов в океане.",
                            "target_lang_translations": {
                                "en": "Scientists carefully study whale <w>migration</w> routes in the ocean."
                            }
                        }
                    ],
                    "synonyms": [
                        "перелёт",
                        "кочёвка"
                    ],
                    "common_phrases": [
                        "сезонная миграция",
                        "пути миграции",
                        "миграция птиц"
                    ],
                    "target_lang_definitions": {
                        "en": "Seasonal or regular movement of animals, birds, or fish over long distances."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "migration",
                                "target_lang_sense_clarification": "seasonal animal movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e72ef89a-844c-3eef-9aa0-f327f819404e",
                    "sense_definition": "Переход с одной компьютерной системы, программы или базы данных на другую.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "tech_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Миграция</w> данных на новый сервер прошла успешно и без ошибок.",
                            "target_lang_translations": {
                                "en": "Data <w>migration</w> to the new server was successful and error-free."
                            }
                        },
                        {
                            "text": "Нам предстоит сложная <w>миграция</w> в облачное хранилище.",
                            "target_lang_translations": {
                                "en": "We are facing a complex <w>migration</w> to cloud storage."
                            }
                        }
                    ],
                    "synonyms": [
                        "переход",
                        "перенос"
                    ],
                    "common_phrases": [
                        "миграция данных",
                        "процесс миграции",
                        "миграция на сервер"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "компьютерный жаргон"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Transition from one computer system, program, or database to another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "migration",
                                "target_lang_sense_clarification": "computing/data transfer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b81a82f-fae5-3120-8d37-c0d92db2ff93",
                    "sense_definition": "Перемещение или перенос вещества внутри какой-либо среды (в физике и химии).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "scientific_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом растворе наблюдается быстрая <w>миграция</w> заряженных частиц.",
                            "target_lang_translations": {
                                "en": "Rapid <w>migration</w> of charged particles is observed in this solution."
                            }
                        },
                        {
                            "text": "<w>Миграция</w> нефти в земной коре может занимать тысячи лет.",
                            "target_lang_translations": {
                                "en": "Oil <w>migration</w> in the earth's crust can take thousands of years."
                            }
                        }
                    ],
                    "synonyms": [
                        "перемещение"
                    ],
                    "common_phrases": [
                        "миграция ионов",
                        "миграция энергии"
                    ],
                    "target_lang_definitions": {
                        "en": "Movement or transfer of a substance within a medium (in physics and chemistry)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "migration",
                                "target_lang_sense_clarification": "physics/chemistry"
                            }
                        ]
                    }
                }
            ]
        }
    ]

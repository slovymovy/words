            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d289373e-6a45-391a-ace2-76563bc7b63e",
                    "sense_definition": "Met een vorm die aan de bovenkant, onderkant of zijkanten plat is gemaakt of geworden; niet meer perfect rond of bol.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aarde is geen perfecte bol, maar is aan de polen enigszins <w>afgeplat</w>.",
                            "target_lang_translations": {
                                "ru": "Земля не является идеальным шаром, она слегка <w>сплюснута</w> у полюсов."
                            }
                        },
                        {
                            "text": "Hij vond een gladde, <w>afgeplat</w> steentje op het strand om mee te keilen.",
                            "target_lang_translations": {
                                "ru": "Он нашел на пляже гладкий <w>приплюснутый</w> камешек, чтобы пускать «блинчики»."
                            }
                        }
                    ],
                    "synonyms": [
                        "plat",
                        "vlak",
                        "geplet"
                    ],
                    "antonyms": [
                        "bol",
                        "rond"
                    ],
                    "common_phrases": [
                        "afgeplatte aarde",
                        "afgeplatte bol",
                        "licht afgeplat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий форму, которая стала плоской сверху, снизу или с боков; больше не идеально круглый или сферический."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сплюснутый",
                                "target_lang_sense_clarification": "о форме, например, планеты"
                            },
                            {
                                "target_lang_word": "приплюснутый",
                                "target_lang_sense_clarification": "слегка плоский"
                            }
                        ]
                    }
                }
            ]
        },
        {

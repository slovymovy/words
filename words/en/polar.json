                {
                    "sense_id": "a256b733-0159-3e46-bd65-ce6bfc76489f",
                    "sense_definition": "Characterized by opposite extremes; completely different or contradictory.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Despite being twins, the two brothers have <w>polar</w> personalities.",
                            "target_lang_translations": {
                                "ru": "Несмотря на то, что они близнецы, у этих двух братьев <w>полярные</w> характеры."
                            }

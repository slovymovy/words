            "senses": [
                {
                    "sense_id": "87e6b31d-e8b3-3c84-9128-a2655c2c1038",
                    "sense_definition": "Gebruikt als versterkend voorvoegsel of bijvoeglijk naamwoord om een negatieve betekenis of ergernis te benadrukken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moesten gisteren een <w>klere</w> eind lopen naar het station.",
                            "target_lang_translations": {
                                "en": "We had to walk a <w>bloody</w> long way to the station yesterday.",
                                "ru": "Вчера нам пришлось тащиться <w>чертову</w> даль до станции."
                            }
                        },
                        {
                            "text": "Met dit <w>klere</w> weer blijf ik liever de hele dag binnen.",
                            "target_lang_translations": {
                                "en": "With this <w>shitty</w> weather, I'd rather stay inside all day.",
                                "ru": "В такую <w>мерзкую</w> погоду я лучше просижу весь день дома."
                            }
                        }
                    ],
                    "synonyms": [

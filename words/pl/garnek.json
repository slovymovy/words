{
    "word_family": [
        "garncarz",
        "garncarstwo",
        "garncarnia",
        "garnuszek",
        "garnczek",
        "gar"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f5a18932-e899-3d96-a978-852230bc0ff6",
                    "sense_definition": "Naczynie kuchenne, zwykle metalowe i z dwoma uchwytami, służące do gotowania potraw na ogniu lub płycie grzewczej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cooking_vessel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama postawiła duży <garnek> z wodą na kuchence.",
                            "target_lang_translations": {
                                "ru": "Мама поставила большую <кастрюлю> с водой на плиту."
                            }
                        },
                        {
                            "text": "Na obiad ugotowałem cały <garnek> pysznej zupy jarzynowej.",
                            "target_lang_translations": {
                                "ru": "На обед я сварил целую <кастрюлю> вкусного овощного супа."
                            }
                        }
                    ],
                    "synonyms": [
                        "gar"
                    ],
                    "common_phrases": [
                        "garnek zupy",
                        "pokrywka na garnek",
                        "wspólny garnek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кухонная посуда, обычно металлическая и с двумя ручками, используемая для варки пищи на огне или плите."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кастрюля",
                                "target_lang_sense_clarification": "основное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7147d59-651a-35ea-a6d6-6276809c328a",
                    "sense_definition": "Bardzo duży kubek, często używany do picia dużej ilości herbaty lub kawy.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "drinking_vessel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rano muszę wypić wielki <garnek> kawy, żeby się obudzić.",
                            "target_lang_translations": {
                                "ru": "Утром мне нужно выпить огромную <бадью> кофе, чтобы проснуться."
                            }
                        },
                        {
                            "text": "Zrobiła sobie <garnek> herbaty i usiadła przed telewizorem.",
                            "target_lang_translations": {
                                "ru": "Она сделала себе <бадью> чая и села перед телевизором."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "expressive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень большая кружка, часто используемая для питья большого количества чая или кофе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бадья",
                                "target_lang_sense_clarification": "разг., о большой емкости для питья"
                            },
                            {
                                "target_lang_word": "кружка",
                                "target_lang_sense_clarification": "контекстуальный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e10c6548-c5c2-3858-af00-fe9c09b1f1e7",
                    "sense_definition": "Cylinder w silniku spalinowym samochodu lub motocykla.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "automotive_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten sportowy samochód ma silnik na sześć <garnków>.",
                            "target_lang_translations": {
                                "ru": "У этой спортивной машины двигатель на шесть <горшков>."
                            }
                        },
                        {
                            "text": "Mechanik powiedział, że jeden <garnek> nie trzyma ciśnienia.",
                            "target_lang_translations": {
                                "ru": "Механик сказал, что один <горшок> не держит давление."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "automotive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Цилиндр в двигателе внутреннего сгорания автомобиля или мотоцикла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горшок",
                                "target_lang_sense_clarification": "слэнг, о цилиндре двигателя"
                            },
                            {
                                "target_lang_word": "цилиндр",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dabbdfd-1166-3e4c-a7a2-75b984814d76",
                    "sense_definition": "Pustak ceramiczny stanowiący element wypełniający w stropach gęstożebrowych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "construction_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na stropie ułożono już wszystkie ceramiczne <garnki>.",
                            "target_lang_translations": {
                                "ru": "На перекрытии уже уложили все керамические <блоки>."
                            }
                        },
                        {
                            "text": "Do budowy tego stropu potrzebny jest specjalny <garnek> Ackermana.",
                            "target_lang_translations": {
                                "ru": "Для строительства этого перекрытия нужен специальный <блок> Аккермана."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "construction jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Керамический пустотелый блок, используемый как элемент заполнения в перекрытиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "керамический блок",
                                "target_lang_sense_clarification": "строительный термин"
                            },
                            {
                                "target_lang_word": "пустотелый блок",
                                "target_lang_sense_clarification": "строительный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
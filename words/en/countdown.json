    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e321766d-e670-3eb1-8c33-25c40bc3f15f",
                    "sense_definition": "The act of counting backwards to zero to mark the time remaining before a specific event occurs.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd shouted the final <w>countdown</w> to midnight on New Year's Eve.",
                            "target_lang_translations": {
                                "ru": "Толпа громко вела финальный <w>обратный отсчёт</w> до полуночи в канун Нового года."
                            }
                        },
                        {
                            "text": "Engineers stopped the <w>countdown</w> because of a technical problem with the rocket.",
                            "target_lang_translations": {
                                "ru": "Инженеры остановили <w>обратный отсчёт</w> из-за технической проблемы с ракетой."
                            }
                        }
                    ],
                    "synonyms": [
                        "count"
                    ],
                    "common_phrases": [
                        "final countdown",
                        "countdown clock",
                        "start the countdown",
                        "countdown to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Счёт в обратном порядке до нуля, отмечающий время, оставшееся до наступления определенного события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обратный отсчёт",
                                "target_lang_sense_clarification": "до события"
                            },
                            {
                                "target_lang_word": "отсчёт",
                                "target_lang_sense_clarification": "времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5437b74-6cec-3d50-bdc9-aec747287fd5",
                    "sense_definition": "A media program that presents a list of popular items, such as songs, in reverse order of rank.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "media_list",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She listens to the weekly music <w>countdown</w> on the radio every Sunday.",
                            "target_lang_translations": {
                                "ru": "Она слушает еженедельный музыкальный <w>хит-парад</w> по радио каждое воскресенье."
                            }
                        },
                        {
                            "text": "The <w>countdown</w> revealed the number one hit song of the year.",
                            "target_lang_translations": {
                                "ru": "<w>Хит-парад</w> показал песню, ставшую хитом номер один в этом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "chart",
                        "list"
                    ],
                    "common_phrases": [
                        "music countdown",
                        "top ten countdown",
                        "weekly countdown"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медиа-программа, представляющая список популярных элементов (например, песен) в обратном порядке занятых мест."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хит-парад",
                                "target_lang_sense_clarification": "музыкальный"
                            },
                            {
                                "target_lang_word": "чарт",
                                "target_lang_sense_clarification": "список популярного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "905b557a-145d-36be-82e5-81495388d0b5",
                    "sense_definition": "The final preparations and procedures leading up to a scheduled event.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "time_measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team is in the final <w>countdown</w> to the product launch next week.",
                            "target_lang_translations": {
                                "ru": "Команда начала финальный <w>обратный отсчёт</w> до запуска продукта на следующей неделе."
                            }
                        },
                        {
                            "text": "During the diplomatic <w>countdown</w>, officials tried to negotiate a peace treaty.",
                            "target_lang_translations": {
                                "ru": "Во время дипломатического <w>обратного отсчёта</w> чиновники пытались договориться о мирном договоре."
                            }
                        }
                    ],
                    "synonyms": [
                        "preparation",
                        "run-up"
                    ],
                    "common_phrases": [
                        "countdown phase",
                        "in the countdown"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последние приготовления и процедуры, непосредственно предшествующие запланированному событию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обратный отсчёт",
                                "target_lang_sense_clarification": "перен. подготовка"
                            },
                            {
                                "target_lang_word": "последние приготовления",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2dec3a03-a77e-3fd3-a575-52c5542aa9e3",
                    "sense_definition": "To count numbers in reverse order to zero or to wait for a specific deadline.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The digital timer was <w>countdowning</w> the seconds until the bomb detonated.",
                            "target_lang_translations": {
                                "ru": "Цифровой таймер <w>отсчитывал</w> секунды до детонации бомбы."
                            }
                        },
                        {
                            "text": "Whole families stood watching the clock, <w>countdowning</w> the remaining minutes.",
                            "target_lang_translations": {
                                "ru": "Целые семьи стояли, глядя на часы и <w>отсчитывая</w> оставшиеся минуты."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Standard English prefers the phrasal verb 'count down'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести счёт в обратном порядке или ждать наступления определенного срока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отсчитывать",
                                "target_lang_sense_clarification": "время"
                            },
                            {
                                "target_lang_word": "вести обратный отсчёт",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

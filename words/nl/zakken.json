    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ea34b727-4241-3057-b736-752b6361dc54",
                    "sense_definition": "Naar beneden bewegen, dalen of zinken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zon <w>zakt</w> langzaam in de zee aan het einde van de dag.",
                            "target_lang_translations": {
                                "en": "The sun slowly <w>sinks</w> into the sea at the end of the day.",
                                "ru": "Солнце медленно <w>опускается</w> в море в конце дня."
                            }
                        },
                        {
                            "text": "Het waterpeil is gelukkig snel <w>gezakt</w> na de overstroming.",
                            "target_lang_translations": {
                                "en": "Fortunately, the water level <w>dropped</w> quickly after the flood.",
                                "ru": "К счастью, уровень воды быстро <w>упал</w> после наводнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "dalen",
                        "zinken",
                        "afnemen"
                    ],
                    "antonyms": [
                        "stijgen",
                        "klimmen",
                        "toenemen"
                    ],
                    "common_phrases": [
                        "door het ijs zakken",
                        "in waarde zakken",
                        "laten zakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To move downwards, descend, or sink.",
                        "ru": "Двигаться вниз, опускаться или тонуть."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "general movement to a lower position"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "to decrease in level, value, or position"
                            },
                            {
                                "target_lang_word": "descend",
                                "target_lang_sense_clarification": "formal movement from a higher to a lower place"
                            },
                            {
                                "target_lang_word": "sink",
                                "target_lang_sense_clarification": "to move downwards, especially in liquid or soft substance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "опускаться",
                                "target_lang_sense_clarification": "общее движение в более низкое положение"
                            },
                            {
                                "target_lang_word": "снижаться",
                                "target_lang_sense_clarification": "уменьшаться в уровне, значении или положении"
                            },
                            {
                                "target_lang_word": "тонуть",
                                "target_lang_sense_clarification": "погружаться, особенно в жидкость"
                            },
                            {
                                "target_lang_word": "садиться",
                                "target_lang_sense_clarification": "о солнце: заходить за горизонт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c13d5f4-eefc-3b76-9de1-5aa44dfcc0b9",
                    "sense_definition": "Niet slagen voor een examen of toets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben helaas voor mijn rijexamen <w>gezakt</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, I <w>failed</w> my driving test.",
                                "ru": "К сожалению, я <w>провалил</w> экзамен по вождению."
                            }
                        },
                        {
                            "text": "Als je niet goed studeert, zul je waarschijnlijk <w>zakken</w>.",
                            "target_lang_translations": {
                                "en": "If you don't study well, you will probably <w>fail</w>.",
                                "ru": "Если ты не будешь хорошо учиться, ты, вероятно, <w>провалишься</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "falen"
                    ],
                    "antonyms": [
                        "slagen"
                    ],
                    "common_phrases": [
                        "voor een examen zakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To not pass an exam or test.",
                        "ru": "Не сдать экзамен или тест."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fail",
                                "target_lang_sense_clarification": "to be unsuccessful in a test or exam"
                            },
                            {
                                "target_lang_word": "flunk",
                                "target_lang_sense_clarification": "informal; to fail a course or exam"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "провалить",
                                "target_lang_sense_clarification": "потерпеть неудачу на экзамене или тесте"
                            },
                            {
                                "target_lang_word": "завалить",
                                "target_lang_sense_clarification": "разговорное; не сдать экзамен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b488dd1-5915-3b3e-8afe-b4d80162f62d",
                    "sense_definition": "Neervallen op de grond, vaak door flauwte, verwonding of uitputting.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De marathonloper <w>zakte</w> na de finish direct in elkaar.",
                            "target_lang_translations": {
                                "en": "The marathon runner immediately <w>collapsed</w> after the finish.",
                                "ru": "Марафонец сразу же <w>рухнул</w> после финиша."
                            }
                        },
                        {
                            "text": "Door de hitte <w>zakten</w> verschillende mensen op het plein in elkaar.",
                            "target_lang_translations": {
                                "en": "Due to the heat, several people in the square <w>collapsed</w>.",
                                "ru": "Из-за жары несколько человек на площади <w>упали</w> в обморок."
                            }
                        }
                    ],
                    "synonyms": [
                        "instorten",
                        "ineenzakken",
                        "flauwvallen"
                    ],
                    "common_phrases": [
                        "in elkaar zakken",
                        "door de knieën zakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To fall to the ground, often due to fainting, injury, or exhaustion.",
                        "ru": "Падать на землю, часто из-за обморока, травмы или истощения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "collapse",
                                "target_lang_sense_clarification": "to fall down from weakness or exhaustion"
                            },
                            {
                                "target_lang_word": "slump",
                                "target_lang_sense_clarification": "to fall or sink heavily and loosely"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оседать",
                                "target_lang_sense_clarification": "бессильно опускаться вниз"
                            },
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "валиться с ног"
                            },
                            {
                                "target_lang_word": "рухнуть",
                                "target_lang_sense_clarification": "тяжело упасть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "725a02dd-c542-3535-bdbb-9048198b6c38",
                    "sense_definition": "Rustig worden of kalmeren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je bent veel te druk, je moet echt even <w>zakken</w>.",
                            "target_lang_translations": {
                                "en": "You are way too busy, you really need to <w>calm down</w> for a bit.",
                                "ru": "Ты слишком суетишься, тебе действительно нужно немного <w>успокоиться</w>."
                            }
                        },
                        {
                            "text": "Laat hem maar even <w>zakken</w> voordat we het gesprek voortzetten.",
                            "target_lang_translations": {
                                "en": "Just let him <w>calm down</w> for a moment before we continue the conversation.",
                                "ru": "Дай ему немного <w>остыть</w>, прежде чем мы продолжим разговор."
                            }
                        }
                    ],
                    "synonyms": [
                        "kalmeren",
                        "dimmen"
                    ],
                    "antonyms": [
                        "opwinden"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become quiet or calm.",
                        "ru": "Становиться тихим или спокойным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "calm down",
                                "target_lang_sense_clarification": "to become less agitated or active"
                            },
                            {
                                "target_lang_word": "settle down",
                                "target_lang_sense_clarification": "to become steady, quiet, or orderly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "успокаиваться",
                                "target_lang_sense_clarification": "становиться менее возбужденным или активным"
                            },
                            {
                                "target_lang_word": "остывать",
                                "target_lang_sense_clarification": "переставать злиться или волноваться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "afbf7f01-5fed-326a-ac95-a30dc6ad7219",
                    "sense_definition": "Meervoud van <w>zak</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij droeg twee zware <w>zakken</w> met boodschappen naar de auto.",
                            "target_lang_translations": {
                                "en": "He carried two heavy <w>bags</w> of groceries to the car.",
                                "ru": "Он нес к машине два тяжелых <w>пакета</w> с продуктами."
                            }
                        },
                        {
                            "text": "Mijn jas heeft diepe <w>zakken</w> waar ik mijn sleutels in bewaar.",
                            "target_lang_translations": {
                                "en": "My coat has deep <w>pockets</w> where I keep my keys.",
                                "ru": "У моего пальто глубокие <w>карманы</w>, в которых я храню ключи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "volle zakken",
                        "zakken vullen"
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "009214f9-ea43-3274-bbd8-03c4fbaf58b4",
                    "sense_definition": "Osoba naiwna, którą łatwo oszukać, wykorzystać lub namówić do zrobienia czegoś niekorzystnego.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź takim <frajerem> i nie pożyczaj mu znowu pieniędzy, bo nigdy ich nie odda.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <лохом> и не одалживай ему снова деньги, потому что он никогда их не вернет."
                            }
                        },
                        {
                            "text": "Oszukali go w sklepie, bo zobaczyli w nim <frajera>, który nie zna się na cenach.",
                            "target_lang_translations": {
                                "ru": "Его обманули в магазине, потому что увидели в нём <лоха>, который не разбирается в ценах."
                            }
                        }
                    ],
                    "synonyms": [
                        "naiwniak",
                        "jeleń",
                        "łoś",
                        "lolek",
                        "durniacha",
                        "frajerzyna",
                        "dureń"
                    ],
                    "common_phrases": [
                        "szukać frajera",
                        "robić z kogoś frajera"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo potoczne, często o zabarwieniu lekceważącym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наивный человек, которого легко обмануть, использовать или склонить к чему-то невыгодному."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лох",
                                "target_lang_sense_clarification": "разг., наивный человек, жертва обмана"
                            },
                            {
                                "target_lang_word": "фраер",
                                "target_lang_sense_clarification": "жарг., простак, жертва преступления"
                            },
                            {
                                "target_lang_word": "простак",
                                "target_lang_sense_clarification": "наивный, простодушный человек"
                            },
                            {
                                "target_lang_word": "лошара",
                                "target_lang_sense_clarification": "разг., пренебр., то же, что лох"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d7856bd-d39f-3d2e-a5d1-4e3bd62ed1be",
                    "sense_definition": "Osoba początkująca w danej dziedzinie, niemająca doświadczenia ani wiedzy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej grze jestem jeszcze kompletnym <frajerem>, więc musisz mi wytłumaczyć zasady.",
                            "target_lang_translations": {
                                "ru": "В этой игре я ещё полный <чайник>, так что ты должен объяснить мне правила."
                            }
                        },
                        {
                            "text": "Doświadczeni pracownicy traktowali go jak <frajera>, ponieważ był to jego pierwszy dzień.",
                            "target_lang_translations": {
                                "ru": "Опытные сотрудники относились к нему как к <салаге>, так как это был его первый день."
                            }
                        }
                    ],
                    "synonyms": [
                        "nowicjusz",
                        "żółtodziób"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, начинающий какое-либо дело, не имеющий опыта или знаний в данной области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чайник",
                                "target_lang_sense_clarification": "разг., неопытный новичок"
                            },
                            {
                                "target_lang_word": "новичок",
                                "target_lang_sense_clarification": "начинающий"
                            },
                            {
                                "target_lang_word": "дилетант",
                                "target_lang_sense_clarification": "непрофессионал"
                            },
                            {
                                "target_lang_word": "лох",
                                "target_lang_sense_clarification": "жарг., человек, не сведущий в чём-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73448860-72a5-3392-bc5b-556cd6ca7a02",
                    "sense_definition": "W gwarze więziennej: więzień nienależący do grupy grypsujących (elity więziennej), często wykorzystywany przez innych.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W celi siedziało dwóch grypsujących oraz jeden <frajer>, który musiał sprzątać.",
                            "target_lang_translations": {
                                "ru": "В камере сидели двое блатных и один <фраер>, который должен был убирать."
                            }
                        },
                        {
                            "text": "Dla członków tej grupy każdy spoza ich kręgu to zwykły <frajer>.",
                            "target_lang_translations": {
                                "ru": "Для членов этой группы любой не из их круга — обычный <фраер>."
                            }
                        }
                    ],
                    "antonyms": [
                        "git"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gwara więzienna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В тюремном жаргоне: заключенный, не принадлежащий к касте «блатных» (тюремной элите), часто эксплуатируемый другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фраер",
                                "target_lang_sense_clarification": "крим. жарг., человек, не принадлежащий к воровскому миру"
                            },
                            {
                                "target_lang_word": "терпила",
                                "target_lang_sense_clarification": "крим. жарг., жертва"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c921dcc-8452-3b7f-adf0-6de59e977b67",
                    "sense_definition": "Rzecz błaha, mało ważna; drobnostka niewarta uwagi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pokłócili się o jakiegoś <frajera>, który nie miał żadnego znaczenia dla sprawy.",
                            "target_lang_translations": {
                                "ru": "Они поссорились из-за какой-то <ерунды>, которая не имела никакого значения для дела."
                            }
                        },
                        {
                            "text": "Szkoda tracić czas i nerwy na takiego <frajera>, zajmijmy się czymś ważnym.",
                            "target_lang_translations": {
                                "ru": "Жаль тратить время и нервы на такой <пустяк>, давайте займемся чем-то важным."
                            }
                        }
                    ],
                    "synonyms": [
                        "bagatela",
                        "bagatelka",
                        "betka",
                        "błahostka",
                        "bzdet",
                        "drobiazg",
                        "drobnostka",
                        "fraszka",
                        "pryszczyk"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "użycie rzadkie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Незначительная вещь, пустяк, не стоящий внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ерунда",
                                "target_lang_sense_clarification": "пустяк, вздор"
                            },
                            {
                                "target_lang_word": "мелочь",
                                "target_lang_sense_clarification": "незначительный предмет или обстоятельство"
                            },
                            {
                                "target_lang_word": "пустяк",
                                "target_lang_sense_clarification": "что-то неважное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4cb0fc6e-562f-3fca-839c-c0a5bb31e8c3",
                    "sense_definition": "Mężczyzna starający się o względy kobiety; zalotnik.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody <frajer> wystawał pod oknem panny, licząc na jej przychylne spojrzenie.",
                            "target_lang_translations": {
                                "ru": "Молодой <ухажёр> торчал под окном барышни, рассчитывая на её благосклонный взгляд."
                            }
                        },
                        {
                            "text": "Ona miała wielu <frajerów>, ale żaden z nich nie przypadł jej do gustu.",
                            "target_lang_translations": {
                                "ru": "У неё было много <кавалеров>, но ни один из них не пришелся ей по вкусу."
                            }
                        }
                    ],
                    "traits": [

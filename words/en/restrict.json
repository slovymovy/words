    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "53028a83-9b0f-3e9e-964f-e01c894339a2",
                    "sense_definition": "To keep someone or something within certain limits or boundaries; to limit or confine.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university decided to <w>restrict</w> access to the library during the holidays.",
                            "target_lang_translations": {
                                "ru": "Университет решил <w>ограничить</w> доступ в библиотеку во время каникул."
                            }
                        },
                        {
                            "text": "We need to <w>restrict</w> our spending this month to save money for the trip.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>ограничить</w> наши расходы в этом месяце, чтобы сэкономить деньги на поездку."
                            }
                        }
                    ],
                    "synonyms": [
                        "limit",
                        "restrain",
                        "curb"
                    ],
                    "antonyms": [
                        "expand",
                        "widen",
                        "free"
                    ],
                    "common_phrases": [
                        "restrict access",
                        "restrict movement",
                        "restrict usage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Держать кого-либо или что-либо в определенных границах; ограничивать или стеснять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограничивать",
                                "target_lang_sense_clarification": "устанавливать пределы или рамки"
                            },
                            {
                                "target_lang_word": "ограничить",
                                "target_lang_sense_clarification": "совершенный вид: поставить в рамки"
                            },
                            {
                                "target_lang_word": "лимитировать",
                                "target_lang_sense_clarification": "устанавливать определенное количество или норму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fabc70ad-2a11-3532-a767-9fab9bdd0bd0",
                    "sense_definition": "To consider a mathematical function as defined on a smaller subset of its original domain.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If we <w>restrict</w> the function to positive numbers, we can find its inverse.",
                            "target_lang_translations": {
                                "ru": "Если мы <w>ограничим</w> функцию положительными числами, мы сможем найти её обратную функцию."
                            }
                        },
                        {
                            "text": "You must <w>restrict</w> the domain to real numbers for this equation to hold true.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>ограничить</w> область определения действительными числами, чтобы это уравнение было верным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "restrict the domain",
                        "restrict the function"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассматривать математическую функцию как определенную на меньшем подмножестве её исходной области определения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограничивать",
                                "target_lang_sense_clarification": "сужать область определения функции"
                            },
                            {
                                "target_lang_word": "сужать",
                                "target_lang_sense_clarification": "математический термин: уменьшать область действия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "23f05f0f-42c1-3dec-8ceb-0c590f5fde4c",
                    "sense_definition": "Restricted or limited.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This specific form is <w>restrict</w> to very old documents and is rarely seen today.",
                            "target_lang_translations": {
                                "ru": "Эта конкретная форма <w>ограничена</w> очень старыми документами и редко встречается сегодня."
                            }
                        },
                        {
                            "text": "The view was <w>restrict</w> by the tall buildings surrounding the courtyard.",
                            "target_lang_translations": {
                                "ru": "Вид был <w>ограничен</w> высокими зданиями, окружающими внутренний двор."
                            }
                        }
                    ],
                    "synonyms": [
                        "limit"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rarely used as an adjective in modern English; 'restricted' is the standard form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограниченный или лимитированный (устаревшая или редкая форма)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограниченный",
                                "target_lang_sense_clarification": "имеющий пределы, стесненный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "f5ddd179-5293-3dd3-9198-5f8cbadfe731",
                    "sense_definition": "The 71st sura (chapter) of the Qur'an.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "work_of_art_name",
                    "examples": [
                        {
                            "text": "The Imam recited verses from Surah <w>Noah</w> during the evening prayer.",
                            "target_lang_translations": {
                                "ru": "Имам читал стихи из суры <w>Ной</w> во время вечерней молитвы."
                            }
                        },
                        {
                            "text": "Scholars analyze the themes of patience and warning found in <w>Noah</w>.",
                            "target_lang_translations": {
                                "ru": "Ученые анализируют темы терпения и предостережения, найденные в суре <w>Ной</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "71-я сура (глава) Корана."
                    },
                    "translations": {
                        "ru": [

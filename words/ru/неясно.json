    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "333ca9d4-406e-31ed-a9a2-f02cefe77037",
                    "sense_definition": "О ситуации, когда нет ясности, понимания или уверенности в чём-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mental_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне пока <w>неясно</w>, как решить эту сложную задачу.",
                            "target_lang_translations": {
                                "en": "It is still <w>unclear</w> to me how to solve this complex problem."
                            }
                        },
                        {
                            "text": "Почему он так поступил, для всех осталось <w>неясно</w>.",
                            "target_lang_translations": {
                                "en": "Why he acted that way remained <w>unclear</w> to everyone."
                            }
                        }
                    ],
                    "synonyms": [
                        "непонятно",
                        "неочевидно"
                    ],
                    "antonyms": [
                        "ясно",
                        "понятно"
                    ],
                    "common_phrases": [
                        "пока неясно",
                        "совершенно неясно",
                        "остается неясно"
                    ],
                    "target_lang_definitions": {
                        "en": "About a situation where there is no clarity, understanding, or certainty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unclear",
                                "target_lang_sense_clarification": "lacking certainty or understanding"
                            },
                            {
                                "target_lang_word": "not clear",
                                "target_lang_sense_clarification": "phrase indicating lack of understanding"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "978c7c8c-82ba-3355-a46c-ebe8c2150412",
                    "sense_definition": "Плохо различимо для зрения или слуха; смутно, расплывчато.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sensory_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В густом тумане очертания дома виднелись <w>неясно</w>.",
                            "target_lang_translations": {
                                "en": "In the thick fog, the outlines of the house could be seen <w>indistinctly</w>."
                            }
                        },
                        {
                            "text": "Сквозь шум дождя голоса людей звучали <w>неясно</w>.",
                            "target_lang_translations": {
                                "en": "Through the noise of the rain, people's voices sounded <w>indistinct</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "смутно"
                    ],
                    "antonyms": [
                        "чётко",
                        "отчётливо"
                    ],
                    "common_phrases": [
                        "неясно видеть",
                        "неясно слышать",
                        "неясно различать"
                    ],
                    "target_lang_definitions": {
                        "en": "Poorly distinguishable to sight or hearing; vaguely, blurredly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "indistinctly",
                                "target_lang_sense_clarification": "not clearly defined or perceptible"
                            },
                            {
                                "target_lang_word": "vaguely",
                                "target_lang_sense_clarification": "in a way that is uncertain or indefinite"
                            },
                            {
                                "target_lang_word": "dimly",
                                "target_lang_sense_clarification": "faintly, not brightly or clearly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d4fb37a-a8d9-3193-9d6c-947286d7cc62",
                    "sense_definition": "Неопределённо, уклончиво или непонятно (о речи, мыслях или действиях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автор пишет слишком <w>неясно</w>, поэтому текст трудно читать.",
                            "target_lang_translations": {
                                "en": "The author writes too <w>unclearly</w>, so the text is difficult to read."
                            }
                        },
                        {
                            "text": "Он выразился <w>неясно</w>, и мы не поняли его намерения.",
                            "target_lang_translations": {
                                "en": "He expressed himself <w>vaguely</w>, and we didn't understand his intentions."
                            }
                        }
                    ],
                    "synonyms": [
                        "непонятно"
                    ],
                    "antonyms": [
                        "ясно",
                        "понятно"
                    ],
                    "common_phrases": [
                        "говорить неясно",
                        "выражаться неясно",
                        "формулировать неясно"
                    ],
                    "target_lang_definitions": {
                        "en": "Indefinitely, evasively, or incomprehensibly (regarding speech, thoughts, or actions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vaguely",
                                "target_lang_sense_clarification": "not clearly expressed"
                            },
                            {
                                "target_lang_word": "unclearly",
                                "target_lang_sense_clarification": "in a manner difficult to understand"
                            }
                        ]
                    }
                }
            ]
        },

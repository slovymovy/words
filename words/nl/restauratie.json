    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "357c89ac-b461-39f8-8241-9edbb32f6fde",
                    "sense_definition": "Het herstellen van een gebouw, kunstwerk of voorwerp in de oorspronkelijke staat.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>restauratie</w> van het oude schilderij heeft erg lang geduurd.",
                            "target_lang_translations": {
                                "en": "The <w>restoration</w> of the old painting took a very long time.",
                                "ru": "<w>Реставрация</w> старой картины заняла очень много времени."
                            }
                        },
                        {
                            "text": "Het museum is tijdelijk gesloten vanwege een grondige <w>restauratie</w>.",
                            "target_lang_translations": {
                                "en": "The museum is temporarily closed due to a thorough <w>restoration</w>.",
                                "ru": "Музей временно закрыт из-за капитальной <w>реставрации</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "herstel",
                        "renovatie",
                        "opknapbeurt"
                    ],
                    "antonyms": [
                        "verval"
                    ],
                    "common_phrases": [
                        "in restauratie",
                        "tijdens de restauratie",
                        "grondige restauratie"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of repairing a building, artwork, or object to its original condition.",
                        "ru": "Восстановление здания, произведения искусства или предмета до первоначального состояния."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "restoration",
                                "target_lang_sense_clarification": "process of restoring"
                            },
                            {
                                "target_lang_word": "renovation",
                                "target_lang_sense_clarification": "of a building"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "реставрация",
                                "target_lang_sense_clarification": "искусство, архитектура"
                            },
                            {
                                "target_lang_word": "восстановление",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1da693dc-0c83-3b7a-a6de-b896f04fef57",
                    "sense_definition": "Een eetgelegenheid in een treinstation of in een trein.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Laten we afspreken in de <w>restauratie</w> op het station voordat we vertrekken.",
                            "target_lang_translations": {
                                "en": "Let's meet in the <w>station restaurant</w> before we leave.",
                                "ru": "Давай встретимся в <w>ресторане</w> на вокзале перед отъездом."
                            }
                        },
                        {
                            "text": "Tijdens de lange treinreis haalden we koffie en broodjes in de <w>restauratie</w>.",
                            "target_lang_translations": {
                                "en": "During the long train journey, we got coffee and sandwiches in the <w>dining car</w>.",
                                "ru": "Во время долгой поездки на поезде мы взяли кофе и бутерброды в <w>вагоне-ресторане</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stationsrestauratie",
                        "stationsbuffet"
                    ],
                    "common_phrases": [
                        "in de restauratie"
                    ],
                    "target_lang_definitions": {

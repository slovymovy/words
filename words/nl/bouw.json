                        {
                            "trait_type": "dated",
                            "comment": "historical measure"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная мера площади на Яве, равная примерно 0,7 гектара.",
                        "en": "An old unit of area on Java, equal to approximately 0.7 hectares."

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замедление",
                                "target_lang_sense_clarification": "процесс снижения темпа"
                            },
                            {
                                "target_lang_word": "снижение скорости",
                                "target_lang_sense_clarification": "автомобильный контекст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54af12c8-fdc0-3838-8fa9-ea2176938923",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>zwolnić</w>: uwolnienie kogoś od kary, długu, podatku lub innego zobowiązania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_exempt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd zdecydował o warunkowym <zwolnieniu> skazanego z odbywania reszty kary.",
                            "target_lang_translations": {
                                "ru": "Суд принял решение об условно-досрочном <освобождении> осужденного от отбывания оставшейся части наказания."
                            }
                        },
                        {
                            "text": "Emerytom przysługuje ustawowe <zwolnienie> z niektórych opłat administracyjnych.",
                            "target_lang_translations": {
                                "ru": "Пенсионерам полагается законное <освобождение> от некоторых административных сборов."
                            }
                        }
                    ],
                    "synonyms": [
                        "uwolnienie",
                        "ulga"
                    ],
                    "antonyms": [
                        "obciążenie",
                        "uwięzienie"
                    ],
                    "common_phrases": [

{
    "word_family": [
        "scatter",
        "scattering"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "304d676b-9c05-3bb0-a2fa-b678a5482ac2",
                    "sense_definition": "Spread over a wide area or occurring at irregular intervals; not grouped together.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "distribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Small white houses were <scattered> along the hillside.",
                            "target_lang_translations": {
                                "ru": "Маленькие белые домики были <разбросаны> по склону холма."
                            }
                        },
                        {
                            "text": "There was only <scattered> applause from the audience after the speech.",
                            "target_lang_translations": {
                                "ru": "После речи из зала послышались лишь <отдельные> аплодисменты."
                            }
                        }
                    ],
                    "synonyms": [
                        "dispersed",
                        "spread out"
                    ],
                    "antonyms": [
                        "clustered",
                        "concentrated",
                        "gathered"
                    ],
                    "common_phrases": [
                        "scattered across",
                        "widely scattered",
                        "scattered remnants"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распределенный по большой территории или встречающийся с нерегулярными интервалами; не сгруппированный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбросанный",
                                "target_lang_sense_clarification": "расположенный в разных местах на большом расстоянии друг от друга"
                            },
                            {
                                "target_lang_word": "отдельный",
                                "target_lang_sense_clarification": "единичный, происходящий не массово (например, об аплодисментах)"
                            },
                            {
                                "target_lang_word": "редкий",
                                "target_lang_sense_clarification": "встречающийся нечасто или на большом расстоянии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05246b4b-5177-35b6-9ec9-21050ec8e18b",
                    "sense_definition": "Used to describe rain or clouds that cover only parts of an area.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The forecast predicts <scattered> showers for the weekend.",
                            "target_lang_translations": {
                                "ru": "Прогноз обещает <отдельные> ливни в выходные."
                            }
                        },
                        {
                            "text": "It will be a sunny day with some <scattered> clouds.",
                            "target_lang_translations": {
                                "ru": "День будет солнечным, с <разрозненными> облаками."
                            }
                        }
                    ],
                    "synonyms": [
                        "isolated"
                    ],
                    "antonyms": [
                        "widespread",
                        "overcast"
                    ],
                    "common_phrases": [
                        "scattered showers",
                        "scattered thunderstorms",
                        "scattered clouds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для описания дождя или облаков, которые покрывают лишь часть территории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "местами",
                                "target_lang_sense_clarification": "наречие, часто используемое в прогнозах погоды (дождь местами)"
                            },
                            {
                                "target_lang_word": "отдельный",
                                "target_lang_sense_clarification": "изолированный, локальный (об осадках)"
                            },
                            {
                                "target_lang_word": "разрозненный",
                                "target_lang_sense_clarification": "не сплошной (об облаках)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "af421214-5327-36a3-9b37-aac538bc4ff4",
                    "sense_definition": "Simple past and past participle of <w>scatter</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd <scattered> in all directions when the rain started.",
                            "target_lang_translations": {
                                "ru": "Толпа <разбежалась> во все стороны, когда начался дождь."
                            }
                        },
                        {
                            "text": "She <scattered> the breadcrumbs on the ground for the birds.",
                            "target_lang_translations": {
                                "ru": "Она <рассыпала> хлебные крошки по земле для птиц."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола scatter."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбежаться",
                                "target_lang_sense_clarification": "быстро разойтись в разные стороны (о людях или животных)"
                            },
                            {
                                "target_lang_word": "разбросать",
                                "target_lang_sense_clarification": "раскидать предметы в беспорядке"
                            },
                            {
                                "target_lang_word": "рассыпать",
                                "target_lang_sense_clarification": "уронить или бросить мелкие частицы (крошки, семена)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
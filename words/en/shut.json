{
    "word_family": [
        "shutter",
        "shutdown",
        "shutout",
        "shuttering"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c0219c4d-eec0-3bb0-8b92-583d5940a309",
                    "sense_definition": "To move something, such as a door, lid, or window, into a position that covers an opening.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>shut</w> the door behind you to keep the heat inside.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>закрой</w> за собой дверь, чтобы сохранить тепло внутри."
                            }
                        },
                        {
                            "text": "The sunlight was too bright, so I <w>shut</w> my eyes.",
                            "target_lang_translations": {
                                "ru": "Солнечный свет был слишком ярким, поэтому я <w>закрыл</w> глаза."
                            }
                        }
                    ],
                    "synonyms": [
                        "close"
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "shut the door",
                        "shut your eyes",
                        "shut your mouth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместить что-либо (дверь, крышку, окно) в положение, закрывающее отверстие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрывать",
                                "target_lang_sense_clarification": "дверь, окно, глаза (несов. вид)"
                            },
                            {
                                "target_lang_word": "закрыть",
                                "target_lang_sense_clarification": "дверь, окно, глаза (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45a03b91-1540-395e-9a67-64f3b4012d4f",
                    "sense_definition": "To stop operating a business or service, either for a short time or permanently.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "business_closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local supermarket <w>shuts</w> at ten o'clock every night.",
                            "target_lang_translations": {
                                "ru": "Местный супермаркет <w>закрывается</w> в десять часов каждый вечер."
                            }
                        },
                        {
                            "text": "Many small shops had to <w>shut</w> down during the economic crisis.",
                            "target_lang_translations": {
                                "ru": "Многим маленьким магазинам пришлось <w>закрыться</w> во время экономического кризиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "close"
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "shut down",
                        "shut up shop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекратить работу предприятия или службы (временно или навсегда)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрываться",
                                "target_lang_sense_clarification": "о магазине, учреждении"
                            },
                            {
                                "target_lang_word": "прекращать работу",
                                "target_lang_sense_clarification": "о бизнесе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6359db9-e067-37ee-afed-21ebf48d3f4f",
                    "sense_definition": "To keep a person or animal inside a space by closing the door so they cannot leave.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "confinement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>shut</w> the dog in the kitchen while the guests were eating.",
                            "target_lang_translations": {
                                "ru": "Мы <w>заперли</w> собаку на кухне, пока гости ели."
                            }
                        },
                        {
                            "text": "He accidentally <w>shut</w> the cat in the garage for the whole night.",
                            "target_lang_translations": {
                                "ru": "Он случайно <w>запер</w> кошку в гараже на всю ночь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shut in",
                        "shut inside"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удерживать человека или животное внутри помещения, закрыв дверь, чтобы они не могли выйти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запереть",
                                "target_lang_sense_clarification": "на замок или задвижку"
                            },
                            {
                                "target_lang_word": "закрыть",
                                "target_lang_sense_clarification": "не выпускать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb9196f7-79e6-3e09-bccf-4c1296ca965c",
                    "sense_definition": "To accidentally catch a part of the body or an object in a closing door, window, or drawer.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to <w>shut</w> your fingers in the car door.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, не <w>прищеми</w> пальцы дверью машины."
                            }
                        },
                        {
                            "text": "The child cried loudly after he <w>shut</w> his thumb in the drawer.",
                            "target_lang_translations": {
                                "ru": "Ребенок громко заплакал после того, как <w>прищемил</w> большой палец ящиком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shut your finger in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случайно зажать часть тела или предмет закрывающейся дверью, окном или ящиком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прищемить",
                                "target_lang_sense_clarification": "палец, руку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ea6b6b3-3a59-316b-8ab5-573828873fdf",
                    "sense_definition": "To terminate a computer program, file, or connection.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure you save your work before you <w>shut</w> the application.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что вы сохранили работу, прежде чем <w>закрыть</w> приложение."
                            }
                        },
                        {
                            "text": "The system will automatically <w>shut</w> the connection if there is no activity.",
                            "target_lang_translations": {
                                "ru": "Система автоматически <w>прервет</w> соединение, если не будет активности."
                            }
                        }
                    ],
                    "synonyms": [
                        "close"
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "shut down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Завершить работу компьютерной программы, файла или соединения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрыть",
                                "target_lang_sense_clarification": "приложение, файл"
                            },
                            {
                                "target_lang_word": "завершить",
                                "target_lang_sense_clarification": "работу программы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6aaf2734-4f4c-3c7e-b52f-fa96357a43c8",
                    "sense_definition": "In a closed position; not open.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The door remained <w>shut</w> despite our efforts to open it.",
                            "target_lang_translations": {
                                "ru": "Дверь оставалась <w>закрытой</w>, несмотря на наши попытки открыть её."
                            }
                        },
                        {
                            "text": "Keep your mouth <w>shut</w> while you are chewing your food.",
                            "target_lang_translations": {
                                "ru": "Держи рот <w>закрытым</w>, пока пережевываешь пищу."
                            }
                        }
                    ],
                    "synonyms": [
                        "close"
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "keep it shut",
                        "slam shut"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в закрытом положении; не открытый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрытый",
                                "target_lang_sense_clarification": "о двери, рте"
                            },
                            {
                                "target_lang_word": "сомкнутый",
                                "target_lang_sense_clarification": "о губах, зубах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7d6467d-d15b-30e5-a7af-869fe2cb7b2e",
                    "sense_definition": "Not currently open for business, trade, or visitors.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "business_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I wanted to buy some bread, but the bakery was already <w>shut</w>.",
                            "target_lang_translations": {
                                "ru": "Я хотел купить хлеба, но пекарня была уже <w>закрыта</w>."
                            }
                        },
                        {
                            "text": "Most banks are <w>shut</w> on public holidays and Sundays.",
                            "target_lang_translations": {
                                "ru": "Большинство банков <w>закрыты</w> в праздничные дни и по воскресеньям."
                            }
                        }
                    ],
                    "synonyms": [
                        "close"
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "shut for lunch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не работающий в данный момент (о бизнесе, торговле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрыт",
                                "target_lang_sense_clarification": "краткое прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f03f0a0-08d3-3a22-ac4a-efa52f3966d2",
                    "sense_definition": "To be rid of something or someone unwanted.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rid_of",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was glad to finally get <w>shut</w> of his unreliable car.",
                            "target_lang_translations": {
                                "ru": "Он был рад наконец <w>избавиться</w> от своей ненадежной машины."
                            }
                        },
                        {
                            "text": "It is very difficult to get <w>shut</w> of a bad reputation once you have it.",
                            "target_lang_translations": {
                                "ru": "Очень трудно <w>избавиться</w> от плохой репутации, если она у вас уже есть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get shut of"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Common in Northern English and Southern US dialects."
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Избавиться от чего-то или кого-то нежелательного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избавиться",
                                "target_lang_sense_clarification": "от кого-либо/чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "82d7819e-b248-3035-9e53-f6cc571a6054",
                    "sense_definition": "A narrow path, passage, or alleyway between buildings.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "passage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We took a shortcut through the dark <w>shut</w> to reach the main street.",
                            "target_lang_translations": {
                                "ru": "Мы срезали путь через темный <w>переулок</w>, чтобы выйти на главную улицу."
                            }
                        },
                        {
                            "text": "The historic town is famous for its many narrow <w>shuts</w> and cobblestone lanes.",
                            "target_lang_translations": {
                                "ru": "Исторический город славится своими многочисленными узкими <w>проходами</w> и мощеными улочками."
                            }
                        }
                    ],
                    "synonyms": [
                        "alley"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Used in parts of England, such as Shropshire."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкий проход или переулок между зданиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переулок",
                                "target_lang_sense_clarification": "узкая улица"
                            },
                            {
                                "target_lang_word": "проход",
                                "target_lang_sense_clarification": "между домами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
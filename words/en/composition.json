    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba3fb3d1-7058-3c8e-a5ea-a300bb6ed514",
                    "sense_definition": "A short essay or story written as a school assignment.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "creative_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our teacher asked us to write a <w>composition</w> about our summer holidays.",
                            "target_lang_translations": {
                                "ru": "Наш учитель попросил нас написать <w>сочинение</w> о наших летних каникулах."
                            }
                        },
                        {
                            "text": "I made a few spelling mistakes in my English <w>composition</w>.",
                            "target_lang_translations": {
                                "ru": "Я сделал несколько орфографических ошибок в своём <w>сочинении</w> по английскому языку."
                            }
                        }
                    ],
                    "synonyms": [
                        "melange",
                        "form",
                        "arrangement"
                    ],
                    "common_phrases": [
                        "write a composition",
                        "English composition"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "education"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткое эссе или рассказ, написанный в качестве школьного задания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сочинение",
                                "target_lang_sense_clarification": "школьное задание"
                            },
                            {
                                "target_lang_word": "эссе",
                                "target_lang_sense_clarification": "творческая работа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03edd302-2c9c-307a-b2f6-6ff25e93551a",
                    "sense_definition": "The way in which a whole is made up of different parts; the nature of its ingredients or constituents.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists are studying the chemical <w>composition</w> of the soil.",
                            "target_lang_translations": {
                                "ru": "Учёные изучают химический <w>состав</w> почвы."
                            }
                        },
                        {
                            "text": "The demographic <w>composition</w> of the city has changed significantly over the last decade.",
                            "target_lang_translations": {
                                "ru": "Демографический <w>состав</w> города значительно изменился за последнее десятилетие."
                            }
                        }
                    ],
                    "synonyms": [
                        "makeup",
                        "structure",
                        "constitution",
                        "configuration",
                        "form"
                    ],
                    "common_phrases": [
                        "chemical composition",
                        "social composition",
                        "composition of the population"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, из каких частей состоит целое; природа его ингредиентов или составляющих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состав",
                                "target_lang_sense_clarification": "строение, структура"
                            },
                            {
                                "target_lang_word": "структура",
                                "target_lang_sense_clarification": "внутреннее устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37ffe193-fdc7-3a16-a075-2c4032b7698b",
                    "sense_definition": "A creative work, especially a poem, piece of music, or painting.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "creative_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The orchestra performed a famous <w>composition</w> by Mozart.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ef2d52f7-b778-3224-8caa-98915bb05341",
                    "sense_definition": "Iemand die als beroep in een mijn werkt om grondstoffen te delven.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "beroep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>mijner</w> kwam met een zwart gezicht uit de schacht.",
                            "target_lang_translations": {
                                "en": "The <w>miner</w> came out of the shaft with a black face.",
                                "ru": "<w>Шахтёр</w> вышел из шахты с чёрным лицом."
                            }
                        },
                        {
                            "text": "Vroeger was hij een hardwerkende <w>mijner</w> in Limburg.",
                            "target_lang_translations": {
                                "en": "He used to be a hard-working <w>miner</w> in Limburg.",
                                "ru": "Раньше он был трудолюбивым <w>шахтёром</w> в Лимбурге."
                            }
                        }
                    ],
                    "synonyms": [
                        "mijnwerker",
                        "kompel",
                        "mijnarbeider"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who works in a mine by profession to extract raw materials.",
                        "ru": "Человек, работающий в шахте по добыче полезных ископаемых."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "miner",
                                "target_lang_sense_clarification": "profession"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шахтёр",
                                "target_lang_sense_clarification": "профессия"
                            },
                            {
                                "target_lang_word": "горняк",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "329714db-6ae7-3ca8-95c4-ae2a90918b71",
                    "sense_definition": "Iemand die bij een openbare verkoping (veiling) een bod uitbrengt en het object koopt.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "handel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>mijner</w> moest direct na de veiling betalen.",
                            "target_lang_translations": {
                                "en": "The <w>successful bidder</w> had to pay immediately after the auction.",
                                "ru": "<w>Покупатель</w> должен был заплатить сразу после аукциона."
                            }
                        },
                        {
                            "text": "Als <w>mijner</w> ben je verplicht het goed af te nemen.",
                            "target_lang_translations": {
                                "en": "As the <w>buyer</w>, you are obliged to purchase the good.",
                                "ru": "Как <w>покупатель</w>, вы обязаны выкупить товар."
                            }
                        }
                    ],
                    "synonyms": [
                        "bieder"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who places a bid at a public sale (auction) and purchases the object.",
                        "ru": "Человек, который делает ставку и покупает предмет на публичных торгах (аукционе)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "successful bidder",
                                "target_lang_sense_clarification": "at an auction"
                            },
                            {
                                "target_lang_word": "buyer",
                                "target_lang_sense_clarification": "at an auction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "покупатель",
                                "target_lang_sense_clarification": "на аукционе"
                            },
                            {
                                "target_lang_word": "участник торгов",
                                "target_lang_sense_clarification": "сделавший ставку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "b3a96d60-bf84-3266-8f54-dd5a3e169bca",
                    "sense_definition": "Een verouderde verbuiging van het bezittelijk voornaamwoord 'mijn' (van mij), voornamelijk gebruikt in de tweede naamval.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "grammatica",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is het huis <w>mijner</w> ouders.",
                            "target_lang_translations": {
                                "en": "This is the house <w>of my</w> parents.",
                                "ru": "Это дом <w>моих</w> родителей."
                            }
                        },
                        {
                            "text": "Ik handelde ter bescherming <w>mijner</w> eer.",
                            "target_lang_translations": {
                                "en": "I acted in defense <w>of my</w> honor.",
                                "ru": "Я действовал для защиты <w>моей</w> чести."
                            }
                        }
                    ],
                    "common_phrases": [
                        "te mijner beschikking",
                        "te mijner verdediging"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An archaic inflection of the possessive pronoun 'mijn' (my), mainly used in the genitive case.",
                        "ru": "Устаревшая форма притяжательного местоимения 'mijn' (мой), используемая преимущественно в родительном падеже."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of my",
                                "target_lang_sense_clarification": "archaic genitive"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "моих",
                                "target_lang_sense_clarification": "родительный падеж, мн.ч."
                            },
                            {
                                "target_lang_word": "моей",
                                "target_lang_sense_clarification": "родительный падеж, жен.р."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "14c6360f-dc88-3e57-84e5-6e1aa101a97f",
                    "sense_definition": "Een verouderde verbuiging van het persoonlijk voornaamwoord 'ik' in de tweede naamval (genitief).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammatica",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heer, ontferm U <w>mijner</w>.",
                            "target_lang_translations": {
                                "en": "Lord, have mercy <w>on me</w>.",
                                "ru": "Господи, помилуй <w>меня</w>."
                            }
                        },
                        {
                            "text": "Hij is <w>mijner</w> niet waard.",
                            "target_lang_translations": {
                                "en": "He is not worthy <w>of me</w>.",
                                "ru": "Он <w>меня</w> не достоин."
                            }
                        }
                    ],
                    "synonyms": [
                        "mijns"
                    ],
                    "common_phrases": [
                        "ontferm u mijner"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An archaic inflection of the personal pronoun 'ik' (I) in the genitive case.",
                        "ru": "Устаревшая форма личного местоимения 'ik' (я) в родительном падеже."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of me",
                                "target_lang_sense_clarification": "archaic genitive"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "меня",
                                "target_lang_sense_clarification": "устаревшее употребление"
                            }
                        ]
                    }
                }
            ]
        }
    ]

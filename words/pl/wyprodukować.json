            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cca4067c-8a4e-3a8f-9411-cd2813b70640",
                    "sense_definition": "Wytworzyć towary lub dobra materialne, zazwyczaj masowo przy użyciu maszyn w procesie przemysłowym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "production_manufacturing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fabryka planuje <wyprodukować> tysiąc samochodów w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Завод планирует <произвести> тысячу автомобилей в следующем месяце."
                            }
                        },
                        {
                            "text": "Musimy <wyprodukować> więcej energii, aby zaspokoić rosnące potrzeby miasta.",
                            "target_lang_translations": {
                                "ru": "Мы должны <выработать> больше энергии, чтобы удовлетворить растущие потребности города."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytworzyć",
                        "stworzyć",
                        "wykonać",
                        "zrobić"
                    ],
                    "antonyms": [
                        "zniszczyć",
                        "zużyć"
                    ],
                    "common_phrases": [
                        "wyprodukować towary",
                        "wyprodukować energię",
                        "wyprodukować żywność",
                        "masowo wyprodukować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произвести товары или материальные блага, обычно массово с использованием машин в промышленном процессе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "произвести",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "выпустить",
                                "target_lang_sense_clarification": "о серийной продукции"
                            },
                            {
                                "target_lang_word": "изготовить",
                                "target_lang_sense_clarification": "сделать, создать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7991a0d-6a2b-3337-a341-00308fa11922",
                    "sense_definition": "Zorganizować i sfinansować powstanie dzieła, najczęściej filmu, programu telewizyjnego lub nagrania muzycznego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "production_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany reżyser chce <wyprodukować> nowy film akcji we współpracy z zagranicznym studiem.",
                            "target_lang_translations": {
                                "ru": "Известный режиссер хочет <спродюсировать> новый боевик в сотрудничестве с зарубежной студией."
                            }
                        },
                        {
                            "text": "Stacja telewizyjna zdecydowała się <wyprodukować> kolejny sezon popularnego serialu.",
                            "target_lang_translations": {
                                "ru": "Телеканал решил <спродюсировать> следующий сезон популярного сериала."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrealizować",
                        "sfinansować",
                        "nakręcić"
                    ],
                    "common_phrases": [
                        "wyprodukować film",
                        "wyprodukować serial",
                        "wyprodukować album"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организовать и профинансировать создание произведения, чаще всего фильма, телепрограммы или музыкальной записи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спродюсировать",
                                "target_lang_sense_clarification": "о фильмах, музыке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a928e489-c384-3daf-996f-bc94de568d84",
                    "sense_definition": "Wytworzyć substancję biologiczną (np. hormony, enzymy) wewnątrz organizmu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "production_biological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Organizm człowieka potrafi <wyprodukować> witaminę D pod wpływem promieni słonecznych.",
                            "target_lang_translations": {
                                "ru": "Человеческий организм способен <выработать> витамин D под воздействием солнечных лучей."
                            }
                        },
                        {
                            "text": "Po podaniu szczepionki układ odpornościowy zaczyna <wyprodukować> odpowiednie przeciwciała.",
                            "target_lang_translations": {
                                "ru": "После введения вакцины иммунная система начинает <вырабатывать> соответствующие антитела."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytworzyć",
                        "wydzielić"
                    ],
                    "common_phrases": [
                        "wyprodukować przeciwciała",
                        "wyprodukować insulinę",
                        "wyprodukować hormony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выработать биологическое вещество (например, гормоны, ферменты) внутри организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выработать",
                                "target_lang_sense_clarification": "о веществах в организме"
                            },
                            {
                                "target_lang_word": "синтезировать",
                                "target_lang_sense_clarification": "научный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
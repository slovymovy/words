{
    "word_family": [
        "тащить",
        "протаскивать",
        "вытащить",
        "затащить",
        "оттащить",
        "притащить",
        "перетащить",
        "стащить",
        "утащить",
        "втащить",
        "растащить",
        "подтащить",
        "натащить",

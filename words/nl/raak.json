            "senses": [
                {
                    "sense_id": "21d39eec-0675-376f-8c02-2e5020e5c1f4",
                    "sense_definition": "Het doel treffend; niet mis (zowel letterlijk bij het gooien of schieten, als figuurlijk bij een gebeurtenis).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_hitting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aanvaller schoot in de laatste minuut eindelijk <w>raak</w>.",
                            "target_lang_translations": {
                                "en": "The attacker finally shot <w>on target</w> in the last minute.",
                                "ru": "Нападающий на последней минуте наконец-то пробил <w>в цель</w>."
                            }
                        },
                        {
                            "text": "Ik bleef het proberen met de bal en gooide pas na een kwartier <w>raak</w>.",
                            "target_lang_translations": {
                                "en": "I kept trying with the ball and only threw <w>on target</w> after fifteen minutes.",
                                "ru": "Я продолжал попытки с мячом и бросил <w>точно в цель</w> только спустя пятнадцать минут."
                            }
                        }
                    ],
                    "synonyms": [
                        "hark"
                    ],
                    "antonyms": [
                        "mis"
                    ],
                    "common_phrases": [
                        "in één keer raak",
                        "raak schieten",
                        "raak gooien",
                        "het is raak"
                    ],
                    "target_lang_definitions": {
                        "en": "Hitting the target; not missing (both literally in throwing or shooting, and figuratively in an event).",
                        "ru": "Попадающий в цель; точный (о выстреле, броске или событии)."
                    },
                    "translations": {

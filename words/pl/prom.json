{
    "word_family": [
        "promowy",
        "promik"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2b89c498-d119-3c5c-adb0-0e9c0509aac8",
                    "sense_definition": "Statek wodny służący do przewozu osób, pojazdów i towarów, kursujący regularnie między portami lub brzegami.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W te wakacje popłynęliśmy wielkim <w>promem</w> do Szwecji.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы отправились на большом <w>пароме</w> в Швецию."
                            }
                        },
                        {
                            "text": "Musieliśmy czekać godzinę, zanim nasz samochód wjechał na <w>prom</w>.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось ждать час, прежде чем наша машина заехала на <w>паром</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prom pasażerski",
                        "prom samochodowy",
                        "płynąć promem",
                        "wjechać na prom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Судно, используемое для перевозки людей, транспортных средств и грузов, регулярно курсирующее между портами или берегами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паром",
                                "target_lang_sense_clarification": "судно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d26a3557-4b7b-3ddd-938e-cfca9f28295e",
                    "sense_definition": "Statek kosmiczny wielokrotnego użytku, zdolny do startu na orbitę i lądowania na Ziemi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "space_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Amerykański <w>prom</w> kosmiczny odbył swoją ostatnią misję wiele lat temu.",
                            "target_lang_translations": {
                                "ru": "Американский космический <w>шаттл</w> совершил свою последнюю миссию много лет назад."
                            }
                        },
                        {
                            "text": "Astronauci spędzili dwa tygodnie na orbicie, zanim <w>prom</w> wrócił na Ziemię.",
                            "target_lang_translations": {
                                "ru": "Астронавты провели две недели на орбите, прежде чем <w>шаттл</w> вернулся на Землю."
                            }
                        }
                    ],
                    "synonyms": [
                        "wahadłowiec"
                    ],
                    "common_phrases": [
                        "prom kosmiczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Многоразовый космический корабль, способный выходить на орбиту и возвращаться на Землю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаттл",
                                "target_lang_sense_clarification": "космический корабль"
                            },
                            {
                                "target_lang_word": "челнок",
                                "target_lang_sense_clarification": "космический"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
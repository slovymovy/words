                    "sense_id": "52f1c88b-86ce-3766-a0fc-6f69afce204b",
                    "sense_definition": "Een getrokken lijn of langwerpig teken op een oppervlak.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_line",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar zette een rode <w>streep</w> door het foute antwoord.",
                            "target_lang_translations": {
                                "en": "The teacher put a red <w>line</w> through the wrong answer.",
                                "ru": "Учитель провел красную <w>черту</w> через неправильный ответ."
                            }
                        },
                        {
                            "text": "Trek een rechte <w>streep</w> van punt A naar punt B.",
                            "target_lang_translations": {
                                "en": "Draw a straight <w>line</w> from point A to point B.",
                                "ru": "Проведите прямую <w>линию</w> от точки А к точке Б."
                            }
                        }
                    ],
                    "synonyms": [
                        "lijn"

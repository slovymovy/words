{
    "word_family": [
        "duly",
        "undue",
        "unduly"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b920b0d9-365d-3042-bc4e-84eef0736397",
                    "sense_definition": "Expected to happen, arrive, or be born at a particular time.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "scheduled_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train is <w>due</w> in five minutes.",
                            "target_lang_translations": {
                                "ru": "Поезд <w>ожидается</w> через пять минут."
                            }
                        },
                        {
                            "text": "Her baby is <w>due</w> in late October.",
                            "target_lang_translations": {
                                "ru": "Ее ребенок <w>должен</w> родиться в конце октября."
                            }
                        }
                    ],
                    "synonyms": [
                        "expected",
                        "forecast"
                    ],
                    "common_phrases": [
                        "due date",
                        "due to arrive",
                        "when is it due"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ожидаемый или запланированный к определенному времени (о прибытии, событии или рождении)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ожида́емый",
                                "target_lang_sense_clarification": "о событии или прибытии"
                            },
                            {
                                "target_lang_word": "до́лжный",
                                "target_lang_sense_clarification": "запланированный по сроку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7c19f8c-5141-3af4-9319-0b7fcfbf27cc",
                    "sense_definition": "Caused by or ascribable to something; resulting from.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "caused_by",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cancellation was <w>due</w> to heavy rain.",
                            "target_lang_translations": {
                                "ru": "Отмена была <w>вызвана</w> сильным дождем."
                            }
                        },
                        {
                            "text": "His success is largely <w>due</w> to hard work.",
                            "target_lang_translations": {
                                "ru": "Его успех во многом <w>обусловлен</w> упорным трудом."
                            }
                        }
                    ],
                    "synonyms": [
                        "owing"
                    ],
                    "common_phrases": [
                        "due to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызванный чем-либо или являющийся следствием чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обусло́вленный",
                                "target_lang_sense_clarification": "являющийся следствием"
                            },
                            {
                                "target_lang_word": "вы́званный",
                                "target_lang_sense_clarification": "спровоцированный чем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7e4116d-592b-3385-b718-13601eb7f76b",
                    "sense_definition": "Required to be paid or given, especially as a debt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "owed_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The full amount is <w>due</w> by the end of the month.",
                            "target_lang_translations": {
                                "ru": "Полная сумма <w>подлежит оплате</w> к концу месяца."
                            }
                        },
                        {
                            "text": "He still has some back pay <w>due</w> to him.",
                            "target_lang_translations": {
                                "ru": "Ему все еще <w>причитается</w> задолженность по зарплате."
                            }
                        }
                    ],
                    "synonyms": [
                        "owing",
                        "required"
                    ],
                    "common_phrases": [
                        "amount due",
                        "past due",
                        "balance due"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подлежащий оплате или возврату (о долге)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подлежа́щий опла́те",
                                "target_lang_sense_clarification": "о счетах или долгах"
                            },
                            {
                                "target_lang_word": "причита́ющийся",
                                "target_lang_sense_clarification": "положенный к выплате"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0457cdf-b5ab-3fb7-9127-4f8f42a9bf37",
                    "sense_definition": "Suitable, proper, or right for the circumstances.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "appropriate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With all <w>due</w> respect, I think you are mistaken.",
                            "target_lang_translations": {
                                "ru": "При всем <w>должном</w> уважении, я думаю, вы ошибаетесь."
                            }
                        },
                        {
                            "text": "You will receive more information in <w>due</w> course.",
                            "target_lang_translations": {
                                "ru": "Вы получите дополнительную информацию в <w>надлежащее</w> время."
                            }
                        }
                    ],
                    "common_phrases": [
                        "with all due respect",
                        "in due course"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходящий, правильный или уместный в данных обстоятельствах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надлежа́щий",
                                "target_lang_sense_clarification": "соответствующий правилам"
                            },
                            {
                                "target_lang_word": "подоба́ющий",
                                "target_lang_sense_clarification": "уместный"
                            },
                            {
                                "target_lang_word": "до́лжный",
                                "target_lang_sense_clarification": "необходимый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "352e7c99-2d55-382d-9944-aac31e9008ca",
                    "sense_definition": "Directly or exactly towards a specific compass direction.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ship sailed <w>due</w> west for three days.",
                            "target_lang_translations": {
                                "ru": "Корабль плыл <w>строго</w> на запад в течение трех дней."
                            }
                        },
                        {
                            "text": "The nearest town is five miles <w>due</w> north of here.",
                            "target_lang_translations": {
                                "ru": "Ближайший город находится в пяти милях <w>прямо</w> к северу отсюда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "due north",
                        "due south",
                        "due east",
                        "due west"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямо или точно в определенном направлении (по компасу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пря́мо",
                                "target_lang_sense_clarification": "непосредственно"
                            },
                            {
                                "target_lang_word": "то́чно",
                                "target_lang_sense_clarification": "без отклонений"
                            },
                            {
                                "target_lang_word": "стро́го",
                                "target_lang_sense_clarification": "ровно по курсу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac4b1724-b577-30f5-9017-4e622ff54e7d",
                    "sense_definition": "A required payment for membership in an organization or club.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fee",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to pay my annual union <w>dues</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно оплатить ежегодные профсоюзные <w>взносы</w>."
                            }
                        },
                        {
                            "text": "The club decided to raise its membership <w>dues</w> this year.",
                            "target_lang_translations": {
                                "ru": "Клуб решил повысить членские <w>взносы</w> в этом году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pay dues",
                        "membership dues"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обязательный платеж за членство в организации или клубе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взнос",
                                "target_lang_sense_clarification": "членский платеж"
                            },
                            {
                                "target_lang_word": "сбор",
                                "target_lang_sense_clarification": "плата за участие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a7f1054-2ca3-3fc1-b00a-d67b1f4f01b8",
                    "sense_definition": "Something that a person deserves or has a right to have.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "deserved_right",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You have to give him his <w>due</w>; he is a very good actor.",
                            "target_lang_translations": {
                                "ru": "Нужно отдать ему <w>должное</w>, он очень хороший актер."
                            }
                        },
                        {
                            "text": "He finally received the recognition that was his <w>due</w>.",
                            "target_lang_translations": {
                                "ru": "Он наконец получил признание, которое было его <w>заслугой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "give someone their due"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что человек заслуживает или на что имеет право."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до́лжное",
                                "target_lang_sense_clarification": "заслуженное"
                            },
                            {
                                "target_lang_word": "заслу́га",
                                "target_lang_sense_clarification": "признание прав"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "e8257a21-bd88-3158-9777-966196117077",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Due</w> signed the document.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Дью</w> подписал документ."
                            }
                        },
                        {
                            "text": "We met a family named <w>Due</w>.",
                            "target_lang_translations": {
                                "ru": "Мы встретили семью по фамилии <w>Дью</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Дью",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "b5812d0b-09fb-300b-b719-555938527698",
                    "sense_definition": "De eigenschap van welwillendheid, mededogen en respect tegenover anderen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "virtue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaat toonde zijn <w>menselijkheid</w> door de gewonde vijand water te geven.",
                            "target_lang_translations": {
                                "en": "The soldier showed his <w>humanity</w> by giving water to the wounded enemy.",
                                "ru": "Солдат проявил свою <w>человечность</w>, дав воды раненому врагу."
                            }
                        },
                        {
                            "text": "Misdaden tegen de <w>menselijkheid</w> worden door het internationale gerechtshof bestraft.",
                            "target_lang_translations": {
                                "en": "Crimes against <w>humanity</w> are punished by the international court of justice.",
                                "ru": "Преступления против <w>человечности</w> караются международным судом."
                            }
                        }
                    ],
                    "synonyms": [
                        "humaniteit",
                        "mededogen",
                        "barmhartigheid",
                        "menslievendheid"
                    ],
                    "antonyms": [
                        "onmenselijkheid",
                        "wreedheid",
                        "barbaarsheid"
                    ],
                    "common_phrases": [
                        "misdaden tegen de menselijkheid",
                        "gevoel voor menselijkheid",
                        "blijk geven van menselijkheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of benevolence, compassion, and respect towards others.",
                        "ru": "Качество доброжелательности, сострадания и уважения к другим."
                    },

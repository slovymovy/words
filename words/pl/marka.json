                    }
                },
                {
                    "sense_id": "bbd2c590-316a-3a7d-ae60-aad848e26773",
                    "sense_definition": "Dawne określenie znaczka pocztowego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "post",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym albumie znalazłem kolekcję rzadkich <w>marek</w>.",
                            "target_lang_translations": {
                                "ru": "В старом альбоме я нашел коллекцию редких <w>марок</w>."
                            }
                        },
                        {
                            "text": "Na kopercie widniała zniszczona <w>marka</w> za dziesięć groszy.",
                            "target_lang_translations": {
                                "ru": "На конверте была потрепанная <w>марка</w> за десять грошей."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название почтовой марки."

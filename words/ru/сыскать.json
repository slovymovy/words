            "senses": [
                {
                    "sense_id": "1d53d1e9-de11-3d92-ab4d-2d1575a973fe",
                    "sense_definition": "Найти или обнаружить кого-либо или что-либо в результате целенаправленных поисков или усилий.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "find_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Такого хорошего специалиста сейчас днём с огнём не <w>сыщешь</w>.",
                            "target_lang_translations": {
                                "en": "You won't <w>find</w> such a good specialist nowadays even if you look everywhere."
                            }
                        },
                        {
                            "text": "Ему удалось <w>сыскать</w> редкую книгу в старой городской библиотеке.",
                            "target_lang_translations": {
                                "en": "He managed to <w>find</w> a rare book in the old city library."
                            }
                        }
                    ],
                    "synonyms": [
                        "отыскать",
                        "разыскать"
                    ],
                    "antonyms": [
                        "потерять"
                    ],
                    "common_phrases": [
                        "днём с огнём не сыщешь",
                        "сыскать средства",
                        "сыскать славу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in expressive speech or idioms"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To find or discover someone or something through targeted search or effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "find",
                                "target_lang_sense_clarification": "general effort to discover something"
                            },
                            {
                                "target_lang_word": "locate",
                                "target_lang_sense_clarification": "to find the exact position of something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b405fef-feb6-357a-9152-21c87da96197",
                    "sense_definition": "Выследить и разыскать виновного или преступника (устаревшее значение).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "find_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Губернатор отдал строгий приказ немедленно <w>сыскать</w> беглых каторжников.",
                            "target_lang_translations": {
                                "en": "The governor gave a strict order to immediately <w>track down</w> the fugitive convicts."
                            }
                        },
                        {
                            "text": "Местной полиции так и не удалось <w>сыскать</w> настоящего виновника происшествия.",
                            "target_lang_translations": {
                                "en": "The local police never managed to <w>track down</w> the real culprit of the incident."
                            }
                        }
                    ],
                    "synonyms": [
                        "расследовать"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical or legal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To track down and locate a culprit or criminal (archaic)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "track down",
                                "target_lang_sense_clarification": "to find someone after a search or pursuit"
                            },
                            {
                                "target_lang_word": "hunt down",
                                "target_lang_sense_clarification": "to pursue and capture someone"
                            }

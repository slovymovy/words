                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The waves were <w>crashing</w> against the rocks during the storm.",
                            "target_lang_translations": {
                                "ru": "Во время шторма волны <w>разбивались</w> о скалы."
                            }
                        },
                        {
                            "text": "He woke up when he heard the pots <w>crashing</w> to the floor.",
                            "target_lang_translations": {
                                "ru": "Он проснулся, услышав, как кастрюли <w>с грохотом упали</w> на пол."
                            }
                        }
                    ],
                    "common_phrases": [
                        "crashing down",
                        "crashing into",
                        "crashing through"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {

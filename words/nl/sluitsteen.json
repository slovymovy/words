    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e297d28-9c72-3a1e-be1d-2adf01cd416d",
                    "sense_definition": "Een wigvormige steen in de top van een boog of gewelf die de constructie vastzet en draagt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De metselaar plaatste voorzichtig de <w>sluitsteen</w> om de boog te voltooien.",
                            "target_lang_translations": {
                                "ru": "Каменщик осторожно установил <w>замковый камень</w>, чтобы завершить арку."
                            }
                        },
                        {
                            "text": "Zonder de centrale <w>sluitsteen</w> zou het hele gewelf instorten.",
                            "target_lang_translations": {
                                "ru": "Без центрального <w>замкового камня</w> весь свод обрушился бы."
                            }
                        }
                    ],
                    "synonyms": [
                        "kruinsteen"
                    ],
                    "common_phrases": [
                        "de sluitsteen plaatsen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Клиновидный камень в вершине арки или свода, который закрепляет и поддерживает конструкцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замковый камень",
                                "target_lang_sense_clarification": "архитектура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "774bc711-c21f-3acf-b65e-03d1c40d931a",
                    "sense_definition": "Het laatste en belangrijkste onderdeel dat een geheel of een proces compleet maakt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_completion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit akkoord vormt de <w>sluitsteen</w> van maandenlange onderhandelingen.",
                            "target_lang_translations": {
                                "ru": "Это соглашение является <w>венцом</w> многомесячных переговоров."
                            }
                        },
                        {
                            "text": "De nieuwe wetgeving is de <w>sluitsteen</w> van het milieubeleid van de regering.",
                            "target_lang_translations": {
                                "ru": "Новое законодательство служит <w>замковым камнем</w> экологической политики правительства."
                            }
                        }
                    ],
                    "synonyms": [
                        "sluitstuk",
                        "bekroning"
                    ],
                    "common_phrases": [
                        "de sluitsteen vormen",
                        "als sluitsteen dienen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последний и важнейший элемент, завершающий целое или процесс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "венец",
                                "target_lang_sense_clarification": "переносное значение; завершение"
                            },
                            {
                                "target_lang_word": "замковый камень",
                                "target_lang_sense_clarification": "переносное значение; ключевой элемент"
                            }
                        ]
                    }
                }
            ]
        }
    ]

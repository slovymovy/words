            "senses": [
                {
                    "sense_id": "2591b88e-4fdb-385e-9eb6-e2ef0ab7089b",
                    "sense_definition": "The objective form of \"they\", referring to a group of people, animals, or things previously mentioned.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "object_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She treated <w>them</w> for a cold.",
                            "target_lang_translations": {
                                "ru": "Она лечила <w>их</w> от простуды."
                            }
                        },
                        {
                            "text": "Please give the documents to <w>them</w> immediately.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, немедленно передайте <w>им</w> документы."
                            }
                        },
                        {
                            "text": "I saw <w>them</w> walking in the park yesterday.",
                            "target_lang_translations": {
                                "ru": "Я видел <w>их</w> вчера гуляющими в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "'em"
                    ],
                    "common_phrases": [
                        "one of them",
                        "all of them",
                        "some of them",
                        "none of them",
                        "both of them"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объектная форма местоимения \"они\", относящаяся к группе людей, животных или предметов, упомянутых ранее."
                    },
                    "translations": {

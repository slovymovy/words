                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гематинический",
                                "target_lang_sense_clarification": "способствующий кроветворению"
                            },
                            {
                                "target_lang_word": "кровотворный",
                                "target_lang_sense_clarification": "стимулирующий образование крови"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f85d6376-5980-3a70-9c82-36fa9859b42b",
                    "sense_definition": "Relating to or derived from the chemical compound hematin.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "chemical_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The researcher analyzed the <w>hematinic</w> derivatives found in the sample.",

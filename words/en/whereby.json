            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "6fad8e0b-38e7-318b-b814-185ae30f4fd8",
                    "sense_definition": "By which; through which; according to which.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company introduced a new system <w>whereby</w> employees could work from home twice a week.",
                            "target_lang_translations": {
                                "ru": "Компания внедрила новую систему, <w>при которой</w> сотрудники могли работать из дома дважды в неделю."
                            }
                        },
                        {
                            "text": "They agreed on a process <w>whereby</w> all disputes would be settled by an independent arbitrator.",
                            "target_lang_translations": {
                                "ru": "Они согласовали процесс, <w>посредством которого</w> все споры будут разрешаться независимым арбитром."
                            }
                        }
                    ],
                    "synonyms": [
                        "by which",
                        "through which"
                    ],
                    "common_phrases": [
                        "a system whereby",
                        "a process whereby",
                        "a means whereby",
                        "an arrangement whereby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Посредством которого; с помощью которого; в силу которого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посредством которого",
                                "target_lang_sense_clarification": "формальный стиль, указывает на средство или способ"
                            },
                            {
                                "target_lang_word": "при котором",
                                "target_lang_sense_clarification": "используется при описании систем, ситуаций или договоренностей"
                            },
                            {
                                "target_lang_word": "в силу которого",
                                "target_lang_sense_clarification": "юридический или официальный контекст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e16a0132-db5a-32d4-bf81-836e52919c5e",
                    "sense_definition": "By what means; how; in what direction.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Whereby</w> shall we know that the messenger speaks the truth?",
                            "target_lang_translations": {
                                "ru": "<w>Каким образом</w> мы узнаем, что гонец говорит правду?"
                            }
                        },
                        {
                            "text": "He asked the guard <w>whereby</w> the king intended to travel to the castle.",
                            "target_lang_translations": {
                                "ru": "Он спросил стражника, <w>как</w> король намерен добраться до замка."
                            }
                        }
                    ],
                    "synonyms": [
                        "how",
                        "by what means"
                    ],

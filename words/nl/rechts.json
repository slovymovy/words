                    ],
                    "common_phrases": [
                        "naar rechts",
                        "rechts afslaan",
                        "rechts houden"
                    ],
                    "target_lang_definitions": {
                        "en": "On the side opposite to the left; in the direction of the right hand.",
                        "ru": "На стороне, противоположной левой; в направлении правой руки."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9a37d719-3efd-3572-b472-a66d360e3765",
                    "sense_definition": "Наступающий, появляющийся непосредственно после кого-либо или чего-либо (по времени или по порядку); ближайший в будущем.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы обязательно встретимся в <w>следующий</w> раз.",
                            "target_lang_translations": {
                                "en": "We will definitely meet <w>next</w> time."
                            }
                        },
                        {
                            "text": "На <w>следующий</w> день погода значительно улучшилась.",
                            "target_lang_translations": {
                                "en": "The <w>following</w> day the weather improved significantly."
                            }
                        },
                        {
                            "text": "Кто <w>следующий</w> в очереди к врачу?",
                            "target_lang_translations": {
                                "en": "Who is <w>next</w> in line to the doctor?"
                            }
                        }
                    ],
                    "synonyms": [
                        "очередной",
                        "последующий"
                    ],
                    "antonyms": [
                        "предыдущий"
                    ],
                    "common_phrases": [
                        "в следующий раз",
                        "на следующий день",
                        "следующий год",
                        "следующая неделя",
                        "кто следующий"
                    ],
                    "target_lang_definitions": {
                        "en": "Coming or appearing immediately after someone or something (in time or order); the nearest in the future."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "next",
                                "target_lang_sense_clarification": "nearest in time, place, or sequence"
                            },
                            {
                                "target_lang_word": "following",
                                "target_lang_sense_clarification": "subsequent; coming after"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe71d28f-f060-34eb-b3bb-e2385c46eb65",
                    "sense_definition": "Такой, о котором говорится дальше; который приводится или перечисляется ниже.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "listing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для регистрации вам потребуются <w>следующие</w> документы: паспорт и заявление.",
                            "target_lang_translations": {
                                "en": "For registration, you will need the <w>following</w> documents: a passport and an application."
                            }
                        },
                        {
                            "text": "Обратите внимание на <w>следующие</w> важные детали этого контракта.",
                            "target_lang_translations": {
                                "en": "Pay attention to the <w>following</w> important details of this contract."
                            }
                        }
                    ],
                    "synonyms": [
                        "нижеследующий",
                        "нижеперечисленный",
                        "нижеупомя́нутый"
                    ],
                    "antonyms": [
                        "вышеназванный"
                    ],
                    "common_phrases": [
                        "следующим образом",
                        "следующие вопросы",
                        "следующие правила"
                    ],
                    "target_lang_definitions": {
                        "en": "That which is mentioned further on; which is cited or listed below."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "following",
                                "target_lang_sense_clarification": "about to be mentioned or listed"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7f18cace-a9fe-3c01-b3fb-18ce2e591dd8",
                    "sense_definition": "Действительное причастие настоящего времени от глагола <w>следовать</w>; движущийся по маршруту или логически вытекающий из чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "action_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поезд, <w>следующий</w> до Москвы, отправляется со второй платформы.",
                            "target_lang_translations": {
                                "en": "The train <w>traveling</w> to Moscow departs from the second platform."
                            }
                        },
                        {
                            "text": "Это вывод, логически <w>следующий</w> из ваших аргументов.",
                            "target_lang_translations": {
                                "en": "This is a conclusion logically <w>following</w> from your arguments."
                            }
                        }
                    ],
                    "synonyms": [
                        "идущий"
                    ],
                    "common_phrases": [
                        "следующий до",
                        "следующий из"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Active present participle of the verb 'to follow'; moving along a route or logically resulting from something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "traveling",
                                "target_lang_sense_clarification": "moving towards a destination (of transport)"
                            },
                            {
                                "target_lang_word": "following",
                                "target_lang_sense_clarification": "resulting from or coming after"
                            }
                        ]
                    }
                }
            ]
        }
    ]

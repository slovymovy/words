                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыбаки поставили <w>экран</w> в тихой заводи реки.",
                            "target_lang_translations": {
                                "en": "The fishermen set a <w>lift net</w> in a quiet backwater of the river."

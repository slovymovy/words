    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "febb58bb-db33-353d-90c5-44eac2ecb64c",
                    "sense_definition": "Dotyczący niżu barycznego, czyli obszaru niskiego ciśnienia atmosferycznego.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "meteorology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad Polskę nadciąga układ <w>niżowy</w>, który przyniesie ochłodzenie i opady deszczu.",
                            "target_lang_translations": {
                                "ru": "На Польшу надвигается <w>циклоническая</w> система, которая принесет похолодание и дожди."
                            }
                        },
                        {
                            "text": "Pogoda <w>niżowa</w> często wpływa negatywnie na samopoczucie osób wrażliwych na zmiany ciśnienia.",
                            "target_lang_translations": {
                                "ru": "<w>Циклоническая</w> погода часто негативно влияет на самочувствие людей, чувствительных к изменениям давления."
                            }
                        }
                    ],
                    "antonyms": [
                        "wyżowy"
                    ],
                    "common_phrases": [
                        "układ niżowy",
                        "front niżowy",
                        "pogoda niżowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к барическому минимуму, то есть области низкого атмосферного давления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "циклонический",
                                "target_lang_sense_clarification": "связанный с циклоном"
                            },
                            {
                                "target_lang_word": "пониженного давления",
                                "target_lang_sense_clarification": "о погоде или атмосферном фронте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49534872-6d83-3704-bdb7-5ab56653be39",
                    "sense_definition": "Związany z niżem geograficznym (obszarem nizin) lub niskim stanem wód w rzekach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten gęsty las <w>niżowy</w> jest siedliskiem wielu rzadkich gatunków ptaków.",
                            "target_lang_translations": {
                                "ru": "Этот густой <w>низменный</w> лес является средой обитания многих редких видов птиц."
                            }
                        },
                        {
                            "text": "Długotrwała susza spowodowała, że na rzece utrzymuje się stan <w>niżowy</w>.",
                            "target_lang_translations": {
                                "ru": "Длительная засуха привела к тому, что на реке сохраняется <w>меженный</w> уровень."
                            }
                        }
                    ],
                    "antonyms": [
                        "wyżowy"
                    ],
                    "common_phrases": [
                        "las niżowy",
                        "stan niżowy",
                        "krajobraz niżowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с географической низменностью или низким уровнем воды в реках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "низменный",
                                "target_lang_sense_clarification": "относящийся к низменности"
                            },
                            {
                                "target_lang_word": "меженный",
                                "target_lang_sense_clarification": "относящийся к низкому уровню воды"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                                "target_lang_sense_clarification": "police record"
                            },
                            {
                                "target_lang_word": "ticket",
                                "target_lang_sense_clarification": "fine for an offense"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "протокол",
                                "target_lang_sense_clarification": "документ о нарушении"
                            },
                            {
                                "target_lang_word": "штраф",
                                "target_lang_sense_clarification": "денежное взыскание"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e3354726-79ee-386f-918a-7d36dc501a38",
                    "sense_definition": "Een juridisch ambtenaar die namens de overheid verdachten vervolgt en straffen eist in een rechtszaak.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De openbaar <w>aanklager</w> eiste een gevangenisstraf van vijf jaar voor de verdachte.",
                            "target_lang_translations": {
                                "en": "The public <w>prosecutor</w> demanded a prison sentence of five years for the suspect."
                            }
                        },
                        {
                            "text": "Tijdens het proces legde de <w>aanklager</w> al het bewijsmateriaal voor aan de rechter.",
                            "target_lang_translations": {
                                "en": "During the trial, the <w>prosecutor</w> presented all the evidence to the judge."
                            }
                        }
                    ],
                    "synonyms": [
                        "officier van justitie"
                    ],
                    "antonyms": [
                        "verdediger",
                        "advocaat"
                    ],
                    "common_phrases": [
                        "openbaar aanklager"
                    ],
                    "target_lang_definitions": {
                        "en": "A legal official who prosecutes suspects on behalf of the government and demands penalties in a court case."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prosecutor",
                                "target_lang_sense_clarification": "legal official"
                            },
                            {
                                "target_lang_word": "public prosecutor",
                                "target_lang_sense_clarification": "government official"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbfacc7e-8df2-3840-9e55-fcd110109c04",
                    "sense_definition": "Iemand die een ander ergens van beschuldigt of een klacht indient.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stond in de rechtbank oog in oog met zijn <w>aanklager</w>.",
                            "target_lang_translations": {
                                "en": "He stood face to face with his <w>accuser</w> in court."
                            }
                        },
                        {
                            "text": "De <w>aanklager</w> beweerde dat zijn buurman de schutting opzettelijk had vernield.",
                            "target_lang_translations": {
                                "en": "The <w>accuser</w> claimed that his neighbor had intentionally destroyed the fence."
                            }
                        }
                    ],
                    "synonyms": [
                        "beschuldiger"
                    ],
                    "antonyms": [
                        "beklaagde"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who accuses another person of something or files a complaint."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accuser",
                                "target_lang_sense_clarification": "one who accuses"
                            },
                            {
                                "target_lang_word": "complainant",
                                "target_lang_sense_clarification": "one who files a complaint"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "353fc359-4f51-3791-8020-7d81f01b44d8",
                    "sense_definition": "Słowo oznaczające, że dane zdarzenie nastąpiło wcześniej, niż się spodziewano, lub jest faktem w chwili mówienia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "completion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy <w>już</w> skończyłeś odrabiać pracę domową?"
                        },
                        {
                            "text": "Moi rodzice <w>już</w> wrócili z wakacji i są w domu."
                        }
                    ],
                    "antonyms": [
                        "dopiero",
                        "jeszcze"
                    ],
                    "common_phrases": [

                {
                    "sense_id": "c7621b18-5203-35e9-97bb-406fe12088ba",
                    "sense_definition": "Van invloed zijn op een bepaalde situatie, keuze of uitkomst; een rol spelen in het geheel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij de keuze voor deze baan <w>speelde</w> het salaris zeker <w>mee</w>.",
                            "target_lang_translations": {
                                "en": "The salary certainly <w>played a role</w> in the choice for this job.",
                                "ru": "При выборе этой работы зарплата, безусловно, <w>сыграла роль</w>."
                            }
                        },
                        {
                            "text": "Er <w>speelde</w> <w>mee</w> dat men angst had voor een inval van het buurland.",
                            "target_lang_translations": {
                                "en": "It <w>played a role</w> that people were afraid of an invasion by the neighboring country.",
                                "ru": "<w>Сыграло роль</w> то, что люди боялись вторжения соседней страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "meetellen",
                        "bijdragen"
                    ],
                    "common_phrases": [
                        "dat speelt mee",
                        "factoren die meespelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To influence a certain situation, choice, or outcome; to be a contributing factor.",
                        "ru": "Влиять на определенную ситуацию, выбор или результат; играть роль в общем деле."
                    },
                    "translations": {

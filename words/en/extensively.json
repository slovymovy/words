            "senses": [
                {
                    "sense_id": "a35401ed-a6b5-3745-a451-52bdecb36890",
                    "sense_definition": "In a way that covers a large area or has a wide scope.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has traveled <w>extensively</w> throughout Europe and Asia to learn about different cultures.",
                            "target_lang_translations": {
                                "ru": "Она <w>много</w> путешествовала по Европе и Азии, чтобы узнать о разных культурах."
                            }
                        },
                        {
                            "text": "The professor has read <w>extensively</w> on the subject of ancient history.",
                            "target_lang_translations": {
                                "ru": "Профессор <w>много</w> читал по предмету древней истории."
                            }
                        }
                    ],
                    "synonyms": [
                        "widely",
                        "broadly"
                    ],
                    "antonyms": [
                        "narrowly"
                    ],
                    "common_phrases": [
                        "travel extensively",
                        "read extensively",
                        "write extensively"
                    ],
                    "target_lang_definitions": {
                        "ru": "Таким образом, который охватывает большую область или имеет широкий размах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широко",
                                "target_lang_sense_clarification": "касательно охвата или распространения"
                            },
                            {
                                "target_lang_word": "много",
                                "target_lang_sense_clarification": "касательно количества действий (например, путешествий)"
                            },
                            {

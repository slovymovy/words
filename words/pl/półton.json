            "pos": "noun",
            "senses": [
                {
                    "sense_id": "34f8a5db-bbc9-37fd-9416-fd4afcfbe347",
                    "sense_definition": "Najmniejsza odległość między dwoma dźwiękami w tradycyjnym zachodnim systemie muzycznym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_interval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aby obniżyć dźwięk o <w>półton</w>, musisz użyć odpowiedniego znaku przy kluczu.",
                            "target_lang_translations": {
                                "ru": "Чтобы понизить звук на <w>полутон</w>, нужно использовать соответствующий знак при ключе."
                            }
                        },
                        {
                            "text": "Kwinta czysta składa się dokładnie z siedmiu <w>półtonów</w>.",
                            "target_lang_translations": {
                                "ru": "Чистая квинта состоит ровно из семи <w>полутонов</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "cały ton"
                    ],
                    "common_phrases": [
                        "obniżyć o półton",
                        "podwyższyć o półton",
                        "odległość półtonu",
                        "półton chromatyczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наименьшее расстояние между двумя звуками в традиционной западной музыкальной системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полутон",
                                "target_lang_sense_clarification": "музыкальный интервал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41e45d8c-6588-394e-8d00-597c24923643",
                    "sense_definition": "Pośredni odcień barwy lub stopień nasilenia koloru, łagodzący kontrast między barwami podstawowymi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "color_nuance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten obraz zachwyca delikatnymi <w>półtonami</w> szarości i błękitu.",
                            "target_lang_translations": {
                                "ru": "Эта картина восхищает нежными <w>полутонами</w> серого и голубого."
                            }
                        },
                        {
                            "text": "Kolor oliwkowy jest <w>półtonem</w> znajdującym się między żółtym a zielonym.",
                            "target_lang_translations": {
                                "ru": "Оливковый цвет — это <w>полутон</w>, находящийся между желтым и зеленым."
                            }
                        }
                    ],
                    "synonyms": [
                        "odcień",
                        "niuans"
                    ],
                    "common_phrases": [
                        "gra półtonów",
                        "bogactwo półtonów",
                        "malować półtonami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Промежуточный оттенок цвета или степень насыщенности, смягчающая контраст между основными цветами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полутон",
                                "target_lang_sense_clarification": "оттенок цвета"
                            }
                        ]
                    }
                }
            ]
        }
    ]

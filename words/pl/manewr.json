                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Generał nakazał wykonać szybki <w>manewr</w> oskrzydlający siły wroga.",
                            "target_lang_translations": {
                                "ru": "Генерал приказал выполнить быстрый обходной <w>манёвр</w> сил врага."
                            }

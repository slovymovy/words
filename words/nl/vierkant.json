                            "target_lang_translations": {
                                "en": "My student room is twelve <w>square</w> meters."
                            }
                        },
                        {
                            "text": "We hebben een nieuwe <w>vierkante</w> tafel voor in de keuken gekocht.",
                            "target_lang_translations": {
                                "en": "We bought a new <w>square</w> table for the kitchen."
                            }
                        }
                    ],
                    "antonyms": [
                        "rond"
                    ],
                    "common_phrases": [
                        "vierkante meter",
                        "vierkante kilometer"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8663bc3b-926e-3c9b-bb2f-7b6c43270a85",
                    "sense_definition": "The amount of goods or items that can be purchased for the value of one penny.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "value_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The child ran to the shop to buy a <w>pennyworth</w> of sweets.",
                            "target_lang_translations": {
                                "ru": "Ребенок побежал в магазин, чтобы купить конфет <w>на пенни</w>."
                            }
                        },
                        {
                            "text": "In the Victorian era, a <w>pennyworth</w> of bread could feed a person for a meal.",
                            "target_lang_translations": {
                                "ru": "В викторианскую эпоху хлеба <w>на пенни</w> хватало, чтобы накормить человека."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pennyworth of"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to historical value of currency"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество товара, которое можно приобрести за один пенни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на пенни",
                                "target_lang_sense_clarification": "количество товара на эту сумму"
                            },
                            {
                                "target_lang_word": "грошовое количество",
                                "target_lang_sense_clarification": "устаревшее, малое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd5a6047-4058-395a-903d-547862ca0e91",
                    "sense_definition": "A very small quantity, degree, or value of something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "value_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is not a <w>pennyworth</w> of truth in the rumors spreading around town.",
                            "target_lang_translations": {
                                "ru": "В слухах, распространяемых по городу, нет <w>ни на грош</w> правды."
                            }
                        },
                        {
                            "text": "He does not have a <w>pennyworth</w> of patience when dealing with rude customers.",
                            "target_lang_translations": {
                                "ru": "У него нет <w>ни капли</w> терпения при общении с грубыми клиентами."
                            }
                        }
                    ],
                    "synonyms": [
                        "mite",
                        "modicum",
                        "scrap",
                        "whit",
                        "particle",
                        "bit",
                        "iota",
                        "jot"
                    ],
                    "common_phrases": [
                        "not a pennyworth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень малое количество, степень или ценность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ни на грош",
                                "target_lang_sense_clarification": "идиома, обычно в отрицаниях"
                            },
                            {
                                "target_lang_word": "толика",
                                "target_lang_sense_clarification": "малая часть"
                            },
                            {
                                "target_lang_word": "капля",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "119c2f76-10de-3bd5-80b3-04fabfa88d2f",
                    "sense_definition": "A purchase that is considered good value for money; a bargain.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was pleased to find such a <w>pennyworth</w> at the market stall.",
                            "target_lang_translations": {
                                "ru": "Она была рада найти такую <w>выгодную покупку</w> в торговой палатке."
                            }
                        },
                        {
                            "text": "He is always looking for a good <w>pennyworth</w> to save his coins.",
                            "target_lang_translations": {
                                "ru": "Он всегда ищет <w>выгодную сделку</w>, чтобы сэкономить свои монеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "bargain",
                        "deal"
                    ],
                    "common_phrases": [
                        "good pennyworth"
                    ],
                    "traits": [

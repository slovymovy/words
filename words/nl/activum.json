            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f9f0752a-968a-3867-9b3b-6af4cacee438",
                    "sense_definition": "De grammaticale vorm van een werkwoord waarbij het onderwerp de handeling verricht; de bedrijvende vorm.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_voice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zin 'De hond bijt de man' staat het werkwoord in het <w>activum</w>.",
                            "target_lang_translations": {
                                "en": "In the sentence 'The dog bites the man', the verb is in the <w>active voice</w>."
                            }
                        },
                        {
                            "text": "Het is vaak duidelijker om het <w>activum</w> te gebruiken in plaats van de lijdende vorm.",
                            "target_lang_translations": {
                                "en": "It is often clearer to use the <w>active voice</w> instead of the passive voice."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedrijvende vorm",
                        "actief"
                    ],
                    "antonyms": [
                        "passivum",
                        "lijdende vorm"
                    ],
                    "common_phrases": [
                        "in het activum"
                    ],
                    "target_lang_definitions": {
                        "en": "The grammatical form of a verb in which the subject performs the action; the active voice."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "active voice",
                                "target_lang_sense_clarification": "grammar"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e6002e3-c015-305b-ae96-8825358cc47d",
                    "sense_definition": "Een economisch goed of bezitting dat waarde vertegenwoordigt voor een bedrijf of persoon.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "finance_asset",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een gebouw is een vast <w>activum</w> op de balans van de onderneming.",
                            "target_lang_translations": {
                                "en": "A building is a fixed <w>asset</w> on the company's balance sheet."
                            }
                        },
                        {
                            "text": "Intellectueel eigendom wordt beschouwd als een immaterieel <w>activum</w>.",
                            "target_lang_translations": {
                                "en": "Intellectual property is considered an intangible <w>asset</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezitting",
                        "asset",
                        "bedrijfsmiddel"
                    ],
                    "antonyms": [
                        "passivum"
                    ],
                    "common_phrases": [
                        "vast activum",
                        "vlottend activum"
                    ],
                    "target_lang_definitions": {
                        "en": "An economic good or possession that represents value for a company or person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "asset",
                                "target_lang_sense_clarification": "finance"
                            }
                        ]
                    }
                }
            ]

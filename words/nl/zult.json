{
    "word_family": [
        "zullen",
        "zulten",
        "zout"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "21162449-b899-3bfb-a2cb-13025f1b8ed1",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>zullen</w>",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je <w>zult</w> je ogen niet geloven als je het ziet.",
                            "target_lang_translations": {
                                "ru": "Ты не <w>поверишь</w> своим глазам, когда увидишь это."
                            }
                        },
                        {
                            "text": "U <w>zult</w> even moeten wachten tot de dokter beschikbaar is.",
                            "target_lang_translations": {
                                "ru": "Вам <w>придется</w> немного подождать, пока врач освободится."
                            }
                        }
                    ],
                    "common_phrases": [
                        "je zult wel",
                        "u zult"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма второго лица единственного числа глагола \"zullen\" (вспомогательный глагол будущего времени)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "будешь",
                                "target_lang_sense_clarification": "вспомогательный глагол будущего времени (ты)"
                            },
                            {
                                "target_lang_word": "будете",
                                "target_lang_sense_clarification": "вспомогательный глагол будущего времени (вы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5eae1518-58b9-3d80-9f3c-b98730444e2c",
                    "sense_definition": "Vervoeging (enkelvoud tegenwoordige tijd) van <w>zulten</w> (inleggen in zout of zuur)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer <w>zult</w> de groenten om ze de hele winter te bewaren.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>солит</w> овощи, чтобы сохранить их на всю зиму."
                            }
                        },
                        {
                            "text": "Hij <w>zult</w> de haringen direct na de vangst.",
                            "target_lang_translations": {
                                "ru": "Он <w>солит</w> сельдь сразу после вылова."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола \"zulten\" (солить, мариновать)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солит",
                                "target_lang_sense_clarification": "консервирует в соли"
                            },
                            {
                                "target_lang_word": "маринует",
                                "target_lang_sense_clarification": "консервирует в рассоле"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "81551784-a1d5-39fb-b441-f757d713e167",
                    "sense_definition": "Een gerecht gemaakt van de kop van een varken, meestal enigszins zuur van smaak",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "food_dish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn grootvader eet graag een boterham met zure <w>zult</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка любит бутерброды с кислым <w>зельцем</w>."
                            }
                        },
                        {
                            "text": "Bij de ambachtelijke slager kun je nog echte <w>zult</w> kopen.",
                            "target_lang_translations": {
                                "ru": "У мясника, работающего по старинке, все еще можно купить настоящий <w>зельц</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoofdkaas"
                    ],
                    "common_phrases": [
                        "zure zult"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясное блюдо из свиной головы, обычно кисловатое на вкус (зельц)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зельц",
                                "target_lang_sense_clarification": "колбасное изделие"
                            },
                            {
                                "target_lang_word": "студень",
                                "target_lang_sense_clarification": "холодное блюдо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d2d9969-4f18-36a0-aa4d-7a5e49f2db3f",
                    "sense_definition": "Pekel of andere vloeistof gebruikt voor het conserveren van voedsel",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "preservation_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De augurken moeten lang in de <w>zult</w> staan voor de juiste smaak.",
                            "target_lang_translations": {
                                "ru": "Огурцы должны долго стоять в <w>рассоле</w> для получения правильного вкуса."
                            }
                        },
                        {
                            "text": "Zorg dat het vlees volledig onder de <w>zult</w> staat.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что мясо полностью покрыто <w>рассолом</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассол или другая жидкость для консервирования продуктов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассол",
                                "target_lang_sense_clarification": "соленая жидкость"
                            },
                            {
                                "target_lang_word": "маринад",
                                "target_lang_sense_clarification": "жидкость для консервации"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "694342e2-5b8c-3218-87aa-68c71f07f1a3",
                    "sense_definition": "Zout smakend of met zout geconserveerd",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze vis is erg <w>zult</w> en moet goed worden afgespoeld.",
                            "target_lang_translations": {
                                "ru": "Эта рыба очень <w>соленая</w>, и ее нужно хорошо промыть."
                            }
                        },
                        {
                            "text": "Ik heb dorst gekregen van de <w>zulte</w> spijzen.",
                            "target_lang_translations": {
                                "ru": "Меня замучила жажда от <w>соленых</w> блюд."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Соленый на вкус или консервированный в соли"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соленый",
                                "target_lang_sense_clarification": "имеющий вкус соли"
                            }
                        ]
                    }
                }
            ]
        }
    ]

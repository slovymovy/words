    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5dfef9e-62d8-3d69-b488-55ef63ca2f73",
                    "sense_definition": "Передать что-либо кому-либо в собственность безвозмездно, сделать подарок.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <w>подарить</w> маме большой букет цветов.",
                            "target_lang_translations": {
                                "en": "I want to <w>give</w> my mom a big bouquet of flowers."
                            }
                        },
                        {
                            "text": "На день рождения родители решили <w>подарить</w> сыну новый велосипед.",
                            "target_lang_translations": {
                                "en": "For his birthday, the parents decided to <w>give</w> their son a new bicycle."
                            }
                        }
                    ],
                    "synonyms": [
                        "преподнести",
                        "вручить"
                    ],
                    "antonyms": [
                        "отнять",
                        "забрать",
                        "отобрать"
                    ],
                    "common_phrases": [
                        "подарить цветы",
                        "подарить книгу",
                        "подарить на память",
                        "подарить от души"
                    ],
                    "target_lang_definitions": {
                        "en": "To give something to someone as a gift without payment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "as a present"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ba1352c-ec65-3806-adb8-aecd66c1f86a",
                    "sense_definition": "Предоставить, доставить или вызвать что-либо (обычно нематериальное: чувство, возможность, жизнь).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта встреча <w>подарила</w> мне надежду на лучшее будущее.",
                            "target_lang_translations": {
                                "en": "This meeting <w>gave</w> me hope for a better future."
                            }
                        },
                        {
                            "text": "Девушка обернулась и <w>подарила</w> прохожему лучезарную улыбку.",
                            "target_lang_translations": {
                                "en": "The girl turned around and <w>gave</w> the passerby a radiant smile."
                            }
                        }
                    ],
                    "synonyms": [
                        "даровать",
                        "дать"
                    ],
                    "antonyms": [
                        "лишить"
                    ],
                    "common_phrases": [
                        "подарить жизнь",
                        "подарить надежду",
                        "подарить улыбку",
                        "подарить взгляд"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide, deliver, or evoke something intangible (such as a feeling, opportunity, or life)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "something abstract (hope, a smile, etc.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

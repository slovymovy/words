    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ec071cb2-28d0-362c-a217-3edaaf7cbc90",
                    "sense_definition": "Начать издавать протяжные, заунывные звуки (о животных, обычно о волках или собаках).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Где-то в лесу страшно <завыл> одинокий волк.",
                            "target_lang_translations": {
                                "en": "Somewhere in the forest, a lone wolf <w>howled</w> terrifyingly."
                            }
                        },
                        {
                            "text": "Дворовая собака <завыла> на полную луну.",
                            "target_lang_translations": {
                                "en": "The yard dog <w>howled</w> at the full moon."
                            }
                        }
                    ],
                    "antonyms": [
                        "замолкнуть",
                        "замолчать"
                    ],
                    "common_phrases": [
                        "волк завыл",
                        "собака завыла",
                        "тоскливо завыть"
                    ],
                    "target_lang_definitions": {
                        "en": "To begin making long, mournful sounds (usually of animals like wolves or dogs)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "howl",
                                "target_lang_sense_clarification": "of animals"
                            },
                            {
                                "target_lang_word": "start howling",
                                "target_lang_sense_clarification": "emphasizing the beginning of the action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7cbda417-eeab-30b6-a33c-79714ba37da8",
                    "sense_definition": "Начать издавать резкий, протяжный гул или свист (о ветре, буре, механизмах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "inanimate_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За окном внезапно <завыл> холодный зимний ветер.",
                            "target_lang_translations": {
                                "en": "Outside the window, a cold winter wind suddenly <w>began to howl</w>."
                            }
                        },
                        {
                            "text": "На улице громко <завыла> полицейская сирена.",
                            "target_lang_translations": {
                                "en": "A police siren loudly <w>started wailing</w> on the street."
                            }
                        }
                    ],
                    "synonyms": [
                        "загудеть",
                        "засвистеть"
                    ],
                    "antonyms": [
                        "стихнуть"
                    ],
                    "common_phrases": [
                        "ветер завыл",
                        "вьюга завыла",
                        "сирена завыла",
                        "мотор завыл"
                    ],
                    "target_lang_definitions": {
                        "en": "To begin making a sharp, prolonged drone or whistling sound (of wind, storms, or mechanisms)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "howl",
                                "target_lang_sense_clarification": "of wind or storms"
                            },
                            {
                                "target_lang_word": "wail",
                                "target_lang_sense_clarification": "of sirens or alarms"
                            },
                            {
                                "target_lang_word": "whine",
                                "target_lang_sense_clarification": "of engines or mechanisms"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8066de91-b255-3d5f-888c-ef62fd1b3094",
                    "sense_definition": "Громко и протяжно заплакать, выражая сильное горе, боль или отчаяние.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "human_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок упал, ударился коленом и громко <завыл>.",
                            "target_lang_translations": {
                                "en": "The child fell, hit his knee, and loudly <w>started bawling</w>."
                            }
                        },
                        {
                            "text": "От невыносимой тоски ему хотелось <завыть> в голос.",
                            "target_lang_translations": {
                                "en": "From unbearable anguish, he wanted to <w>howl</w> out loud."
                            }
                        }
                    ],
                    "synonyms": [
                        "зарыдать",
                        "заголосить",
                        "зареветь"
                    ],
                    "antonyms": [
                        "рассмеяться"
                    ],
                    "common_phrases": [
                        "завыть от боли",
                        "завыть от тоски",
                        "завыть белугой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To start crying loudly and lengthily, expressing strong grief, pain, or despair."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wail",
                                "target_lang_sense_clarification": "cry loudly from grief"
                            },
                            {
                                "target_lang_word": "howl",
                                "target_lang_sense_clarification": "cry out in pain or despair"
                            },
                            {
                                "target_lang_word": "bawl",
                                "target_lang_sense_clarification": "colloquial, cry noisily"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
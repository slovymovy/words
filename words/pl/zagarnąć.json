                {
                    "sense_id": "099e65d5-b4bc-3b89-9017-62ef63be1ef2",
                    "sense_definition": "Bezprawnie wziąć coś na własność, często używając siły lub podstępu; przejąć kontrolę nad czymś.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "appropriation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wrogie wojska próbowały siłą <zagarnąć> terytorium sąsiedniego państwa.",
                            "target_lang_translations": {
                                "ru": "Вражеские войска пытались силой <захватить> территорию соседнего государства."
                            }
                        },
                        {
                            "text": "Nieuczciwy urzędnik chciał <zagarnąć> publiczne pieniądze przeznaczone na budowę drogi.",
                            "target_lang_translations": {
                                "ru": "Нечестный чиновник хотел <присвоить> государственные деньги, предназначенные для строительства дороги."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywłaszczyć",
                        "zagrabić",
                        "zrabować",
                        "zawładnąć"
                    ],
                    "antonyms": [
                        "oddać",
                        "zwrócić"
                    ],
                    "common_phrases": [
                        "zagarnąć władzę",
                        "zagarnąć terytorium",
                        "zagarnąć mienie",
                        "zagarnąć dla siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Незаконно завладеть чем-либо, часто с применением силы или хитростью; захватить контроль над чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "захватить",
                                "target_lang_sense_clarification": "взять силой (территорию, власть)"
                            },
                            {
                                "target_lang_word": "присвоить",
                                "target_lang_sense_clarification": "обратить в свою собственность (деньги, имущество)"
                            },
                            {
                                "target_lang_word": "захапать",
                                "target_lang_sense_clarification": "разг. жадно присвоить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cd12d49-4f5f-3af1-a66c-504be43ecc31",
                    "sense_definition": "Zebrać coś lub przysunąć do siebie szerokim, energicznym ruchem ręki lub narzędzia.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krupier w kasynie musiał sprawnie <zagarnąć> żetony ze stołu po zakończonej grze.",
                            "target_lang_translations": {
                                "ru": "Крупье в казино должен был ловко <сгрести> фишки со стола после окончания игры."
                            }
                        },
                        {
                            "text": "Dziewczynka postanowiła <zagarnąć> wszystkie rozsypane klocki w jedno miejsce.",
                            "target_lang_translations": {
                                "ru": "Девочка решила <сгрести> все рассыпанные кубики в одно место."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgarnąć",
                        "zebrać"
                    ],
                    "antonyms": [
                        "rozsypać",
                        "rozrzucić"
                    ],
                    "common_phrases": [
                        "zagarnąć ręką",
                        "zagarnąć do siebie",
                        "zagarnąć włosy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собрать что-либо или придвинуть к себе широким, энергичным движением руки или инструмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сгрести",
                                "target_lang_sense_clarification": "собрать в кучу движением руки или инструмента"
                            },
                            {
                                "target_lang_word": "загрести",
                                "target_lang_sense_clarification": "зачерпнуть или подгрести к себе"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cee8e264-04ec-37ca-9b5b-efa29eaf3db6",
                    "sense_definition": "The visible part of fire; a stream of burning vapour or gas, emitting light and heat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fire_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The candle <w>flame</w> flickered gently in the draft from the window.",
                            "target_lang_translations": {
                                "ru": "<w>Пламя</w> свечи мягко мерцало на сквозняке из окна."
                            }
                        },
                        {
                            "text": "The car burst into <w>flames</w> shortly after the accident occurred.",
                            "target_lang_translations": {
                                "ru": "Машина была охвачена <w>пламенем</w> вскоре после аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "follower",
                        "sparker",
                        "love",
                        "lover",
                        "sweetheart",
                        "darling",
                        "baby",
                        "beloved",
                        "truelove",
                        "paramour",
                        "leman",
                        "loteby",
                        "enamorado",
                        "wooer",
                        "courter",
                        "admirer",
                        "adorer",
                        "crush",
                        "love interest"
                    ],
                    "common_phrases": [
                        "burst into flames",
                        "in flames",
                        "go down in flames",
                        "candle flame"
                    ],
                    "target_lang_definitions": {
                        "ru": "Видимая часть огня; поток горящего пара или газа, излучающий свет и тепло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пламя",
                                "target_lang_sense_clarification": "основное значение; видимая часть огня"
                            },
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "синоним; процесс горения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "697ab9aa-8012-3bf1-88a3-aa22c1257065",
                    "sense_definition": "A romantic partner or lover in a usually short-lived but passionate affair.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "lover_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was surprised to run into an old <w>flame</w> at the grocery store.",
                            "target_lang_translations": {
                                "ru": "Он удивился, встретив свою старую <w>любовь</w> в продуктовом магазине."
                            }
                        },
                        {
                            "text": "She is not just a casual <w>flame</w>, but someone I truly care about.",
                            "target_lang_translations": {
                                "ru": "Она не просто случайное <w>увлечение</w>, а та, кто мне действительно дорог."
                            }
                        }
                    ],
                    "synonyms": [
                        "lover",
                        "sweetheart",
                        "crush",
                        "love interest",
                        "paramour",
                        "beloved"
                    ],
                    "common_phrases": [
                        "old flame"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the phrase 'old flame'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Романтический партнер или возлюбленный, обычно в контексте прошлых отношений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любовь",

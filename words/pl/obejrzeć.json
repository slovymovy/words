    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e44e40ac-00d0-3a19-b37b-413f087559a9",
                    "sense_definition": "Spędzić czas na patrzeniu na film, program telewizyjny lub przedstawienie teatralne od początku do końca.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "watching_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wieczorem całą rodziną <obejrzeliśmy> bardzo ciekawy film przyrodniczy.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы всей семьёй <посмотрели> очень интересный фильм о природе."
                            }
                        },
                        {
                            "text": "Musisz koniecznie <obejrzeć> ten nowy serial, o którym wszyscy teraz mówią.",
                            "target_lang_translations": {
                                "ru": "Ты обязательно должен <посмотреть> этот новый сериал, о котором сейчас все говорят."
                            }
                        }
                    ],
                    "synonyms": [
                        "zobaczyć"
                    ],
                    "common_phrases": [
                        "obejrzeć film",
                        "obejrzeć wiadomości",
                        "chętnie obejrzeć",
                        "obejrzeć do końca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Посмотреть фильм, телепередачу или спектакль от начала до конца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посмотреть",
                                "target_lang_sense_clarification": "фильм, передачу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d854654-f002-3552-91a8-2d1b28491855",
                    "sense_definition": "Dokładnie sprawdzić wygląd lub stan czegoś, patrząc na to z uwagą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "examining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz dokładnie <obejrzał> moją nogę, aby upewnić się, że nie jest złamana.",
                            "target_lang_translations": {
                                "ru": "Врач внимательно <осмотрел> мою ногу, чтобы убедиться, что она не сломана."
                            }
                        },
                        {
                            "text": "Przed zakupem samochodu warto go dokładnie <obejrzeć> w świetle dziennym.",
                            "target_lang_translations": {
                                "ru": "Перед покупкой автомобиля стоит внимательно <осмотреть> его при дневном свете."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyjrzeć się",
                        "przypatrzeć się",
                        "zobaczyć",
                        "zobaczyć"
                    ],
                    "common_phrases": [
                        "obejrzeć dokładnie",
                        "obejrzeć z każdej strony",
                        "obejrzeć towar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательно проверить внешний вид или состояние чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осмотреть",
                                "target_lang_sense_clarification": "проверить состояние"
                            },
                            {
                                "target_lang_word": "рассмотреть",
                                "target_lang_sense_clarification": "разглядеть детали"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8f56971-f6d0-3e42-8bbd-f1e5ddbc7961",
                    "sense_definition": "Odwiedzić jakieś miejsce, aby zobaczyć znajdujące się tam atrakcje, zabytki lub wystawy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "visiting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wycieczki do Krakowa udało nam się <obejrzeć> Wawel i Rynek Główny.",
                            "target_lang_translations": {
                                "ru": "Во время поездки в Краков нам удалось <осмотреть> Вавель и Главную рыночную площадь."
                            }
                        },
                        {
                            "text": "Chcielibyśmy <obejrzeć> to mieszkanie, zanim podejmiemy ostateczną decyzję o wynajmie.",
                            "target_lang_translations": {
                                "ru": "Мы хотели бы <осмотреть> эту квартиру, прежде чем примем окончательное решение об аренде."
                            }
                        }
                    ],
                    "synonyms": [
                        "zobaczyć",
                        "oglądnąć"
                    ],
                    "common_phrases": [
                        "obejrzeć wystawę",
                        "obejrzeć zamek",
                        "obejrzeć mieszkanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Посетить место для осмотра достопримечательностей или выставок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осмотреть",
                                "target_lang_sense_clarification": "достопримечательности"
                            },
                            {
                                "target_lang_word": "посмотреть",
                                "target_lang_sense_clarification": "взглянуть на что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e27bc44d-40b1-3af5-90e3-1558eb0c8edc",
                    "sense_definition": "Odwrócić głowę lub całe ciało, aby spojrzeć na to, co znajduje się z tyłu (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_look",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szedłem szybko ciemną ulicą i bałem się <obejrzeć> za siebie.",
                            "target_lang_translations": {
                                "ru": "Я быстро шёл по тёмной улице и боялся <оглянуться> назад."
                            }
                        },
                        {
                            "text": "Kiedy usłyszała swoje imię, natychmiast <obejrzała> się w stronę wołającego.",
                            "target_lang_translations": {
                                "ru": "Услышав своё имя, она тут же <оглянулась> в сторону зовущего."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obejrzeć się za siebie",
                        "obejrzeć się przez ramię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повернуть голову или тело, чтобы посмотреть назад (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оглянуться",
                                "target_lang_sense_clarification": "посмотреть назад"
                            },
                            {
                                "target_lang_word": "обернуться",
                                "target_lang_sense_clarification": "повернуться назад"
                            }
                        ]
                    }
                }
            ]
        }
    ]

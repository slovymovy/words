                {
                    "sense_id": "db30e14d-4435-3515-badb-d58e0c200036",
                    "sense_definition": "W pierwszych godzinach dnia, krótko po wschodzie słońca.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę jutro <w>wcześnie</w> wstać, żeby zdążyć na pociąg.",
                            "target_lang_translations": {
                                "ru": "Мне нужно завтра <w>рано</w> встать, чтобы успеть на поезд."
                            }
                        },
                        {
                            "text": "Było bardzo <w>wcześnie</w> rano, gdy wyjechaliśmy z domu.",
                            "target_lang_translations": {
                                "ru": "Было очень <w>рано</w> утром, когда мы выехали из дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "rano"
                    ],
                    "antonyms": [
                        "późno"
                    ],
                    "common_phrases": [

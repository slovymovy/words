    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa22871c-2684-3b7d-a723-820a2a2a6c5a",
                    "sense_definition": "Moment startu samolotu lub ptaka; opuszczenie jakiegoś miejsca drogą powietrzną",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "departure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz <w>wylot</w> do Londynu opóźnił się o godzinę.",
                            "target_lang_translations": {
                                "ru": "Наш <w>вылет</w> в Лондон задержался на час."
                            }
                        },
                        {
                            "text": "Sprawdzamy godzinę <w>wylotu</w> na tablicy informacyjnej.",
                            "target_lang_translations": {
                                "ru": "Мы проверяем время <w>вылета</w> на информационном табло."
                            }
                        }
                    ],
                    "synonyms": [
                        "odlot"
                    ],
                    "antonyms": [
                        "przylot"
                    ],
                    "common_phrases": [
                        "wylot samolotu",
                        "godzina wylotu",
                        "planowany wylot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Момент взлета самолета или птицы; отправление из какого-либо места по воздуху"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вылет",
                                "target_lang_sense_clarification": "отправление самолета"
                            },
                            {
                                "target_lang_word": "отлёт",
                                "target_lang_sense_clarification": "отправление птиц или самолета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8291082-8e08-36d2-83d1-13f073475150",
                    "sense_definition": "Miejsce, w którym kończy się droga, tunel lub dolina, prowadzące na otwartą przestrzeń",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "opening_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Korek utworzył się na <w>wylocie</w> z miasta.",
                            "target_lang_translations": {
                                "ru": "Пробка образовалась на <w>выезде</w> из города."
                            }
                        },
                        {
                            "text": "Spotkajmy się przy <w>wylocie</w> tunelu.",
                            "target_lang_translations": {
                                "ru": "Давай встретимся у <w>выезда</w> из туннеля."
                            }
                        }
                    ],
                    "antonyms": [
                        "wlot"
                    ],
                    "common_phrases": [
                        "wylot z miasta",
                        "wylot tunelu",
                        "u wylotu ulicy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где заканчивается дорога, туннель или долина, ведущее на открытое пространство"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выезд",
                                "target_lang_sense_clarification": "из города, с дороги"
                            },
                            {
                                "target_lang_word": "выход",
                                "target_lang_sense_clarification": "из туннеля, ущелья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5e82d41-cafe-3ddb-b95b-d8ddadcd21d4",
                    "sense_definition": "Otwór końcowy rury, lufy lub przewodu, przez który wydostaje się zawartość",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "opening_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zatkal się <w>wylot</w> rury wydechowej w samochodzie.",
                            "target_lang_translations": {
                                "ru": "Забилось <w>выходное отверстие</w> выхлопной трубы в машине."
                            }
                        },
                        {
                            "text": "Żołnierz czyścił <w>wylot</w> lufy karabinu.",
                            "target_lang_translations": {
                                "ru": "Солдат чистил <w>дуло</w> винтовки."
                            }
                        }
                    ],
                    "antonyms": [
                        "wlot"
                    ],
                    "common_phrases": [
                        "wylot lufy",
                        "wylot rury",
                        "wylot powietrza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конечное отверстие трубы, ствола оружия или провода, через которое выходит содержимое"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуло",
                                "target_lang_sense_clarification": "ствола оружия"
                            },
                            {
                                "target_lang_word": "выходное отверстие",
                                "target_lang_sense_clarification": "трубы, канала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "863961b7-c47e-3eed-b807-f0589c09b28e",
                    "sense_definition": "Rozcięty, długi rękaw w dawnym polskim stroju szlacheckim, zwykle odrzucany na plecy",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szlachcic dumnie zarzucił <w>wyloty</w> kontusza na plecy.",
                            "target_lang_translations": {
                                "ru": "Шляхтич гордо закинул <w>вылеты</w> кунтуша за спину."
                            }
                        },
                        {
                            "text": "Jego strój miał bogato zdobione <w>wyloty</w>.",
                            "target_lang_translations": {
                                "ru": "У его костюма были богато украшенные <w>вылеты</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyloty kontusza",
                        "zarzucać wyloty"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "element historycznego stroju (kontusza)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрезанный длинный рукав в старинном польском дворянском костюме, обычно откидываемый за спину"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вылет",
                                "target_lang_sense_clarification": "элемент одежды"
                            },
                            {
                                "target_lang_word": "разрезной рукав",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        },

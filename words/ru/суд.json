    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0342bd22-af2b-30c8-83ec-955d8ef09cd7",
                    "sense_definition": "Государственный орган, осуществляющий правосудие, рассматривающий гражданские, уголовные и административные дела.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили подать в <w>суд</w>, чтобы защитить свои права.",
                            "target_lang_translations": {
                                "en": "We decided to take it to <w>court</w> to protect our rights."
                            }
                        },
                        {
                            "text": "Верховный <w>суд</w> принял окончательное решение по этому сложному вопросу.",
                            "target_lang_translations": {
                                "en": "The Supreme <w>Court</w> made a final decision on this complex issue."
                            }
                        }
                    ],
                    "common_phrases": [
                        "подать в суд",
                        "решение суда",
                        "Верховный суд",
                        "районный суд"
                    ],
                    "target_lang_definitions": {
                        "en": "A state body that administers justice and hears civil, criminal, and administrative cases."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "court",
                                "target_lang_sense_clarification": "legal institution"
                            },
                            {
                                "target_lang_word": "court of law",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e11600a-2835-38b1-84f3-bc1ba51a1066",
                    "sense_definition": "Процесс рассмотрения и разрешения дел в судебном заседании.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Завтра состоится <w>суд</w> над человеком, который ограбил банк.",
                            "target_lang_translations": {
                                "en": "The <w>trial</w> of the man who robbed the bank will take place tomorrow."
                            }
                        },
                        {
                            "text": "Пока <w>суд</w> идёт, никто не имеет права входить в зал без разрешения.",
                            "target_lang_translations": {
                                "en": "While the <w>trial</w> is in progress, no one has the right to enter the room without permission."
                            }
                        }
                    ],
                    "synonyms": [
                        "судопроизводство"
                    ],
                    "common_phrases": [
                        "суд идёт",
                        "отдать под суд",
                        "вызвать в суд",
                        "день суда"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of examining and deciding cases in a court hearing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trial",
                                "target_lang_sense_clarification": "legal proceeding"
                            },
                            {
                                "target_lang_word": "proceedings",
                                "target_lang_sense_clarification": "legal process"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "138f815c-6a90-3edc-8cd3-a6d4a31bcf19",
                    "sense_definition": "Здание или помещение, в котором располагаются судебные органы и проходят заседания.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Возле входа в <w>суд</w> собралось много журналистов и репортёров.",
                            "target_lang_translations": {
                                "en": "Many journalists and reporters gathered near the entrance to the <w>courthouse</w>."
                            }
                        },
                        {
                            "text": "Я забыл важные документы в <w>суде</w> и был вынужден вернуться.",
                            "target_lang_translations": {
                                "en": "I forgot important documents at the <w>courthouse</w> and had to return."
                            }
                        }
                    ],
                    "common_phrases": [
                        "здание суда",
                        "прийти в суд",
                        "ехать в суд"
                    ],
                    "target_lang_definitions": {
                        "en": "The building or room where judicial bodies are located and hearings take place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "courthouse",
                                "target_lang_sense_clarification": "building"
                            },
                            {
                                "target_lang_word": "court",
                                "target_lang_sense_clarification": "place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0dd46f14-2bfb-38de-9b7d-0cb32917a9ae",
                    "sense_definition": "Мнение, суждение или оценка чьих-либо поступков, действий или качеств.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я представляю свою новую книгу на ваш строгий <w>суд</w>.",
                            "target_lang_translations": {
                                "en": "I present my new book for your strict <w>judgment</w>."
                            }
                        },
                        {
                            "text": "Только история совершит свой справедливый <w>суд</w> над этими событиями.",
                            "target_lang_translations": {
                                "en": "Only history will pass its fair <w>judgment</w> on these events."
                            }
                        }
                    ],
                    "synonyms": [
                        "мнение",
                        "суждение"
                    ],
                    "common_phrases": [
                        "на ваш суд",
                        "суд истории",
                        "суд потомков"
                    ],
                    "target_lang_definitions": {
                        "en": "An opinion, judgment, or evaluation of someone's actions or qualities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "judgment",
                                "target_lang_sense_clarification": "opinion"
                            },
                            {
                                "target_lang_word": "verdict",
                                "target_lang_sense_clarification": "metaphorical evaluation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92f30ce9-5871-3577-88d5-697a6d3af3fa",
                    "sense_definition": "Состав судей, рассматривающих конкретное дело.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Встать, <w>суд</w> идёт!",
                            "target_lang_translations": {
                                "en": "All rise, the <w>court</w> is in session!"
                            }
                        },
                        {
                            "text": "После долгих слушаний <w>суд</w> удалился в совещательную комнату для вынесения приговора.",
                            "target_lang_translations": {
                                "en": "After long hearings, the <w>court</w> retired to the deliberation room to pass a sentence."
                            }
                        }
                    ],
                    "common_phrases": [
                        "состав суда",
                        "суд удалился"
                    ],
                    "target_lang_definitions": {
                        "en": "The panel of judges considering a specific case."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "court",
                                "target_lang_sense_clarification": "judges collectively"
                            },
                            {
                                "target_lang_word": "bench",
                                "target_lang_sense_clarification": "judges"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78b0f383-a5ae-39e3-a133-37641880c0c4",
                    "sense_definition": "Система организации судебных органов и правосудия в государстве.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Реформа <w>суда</w> является важным шагом для развития демократического общества.",
                            "target_lang_translations": {
                                "en": "Reform of the <w>courts</w> is an important step for the development of a democratic society."
                            }
                        },
                        {
                            "text": "В этой стране существует независимый <w>суд</w>, который не подчиняется исполнительной власти.",
                            "target_lang_translations": {
                                "en": "In this country, there is an independent <w>judiciary</w> that is not subordinate to the executive branch."
                            }
                        }
                    ],
                    "synonyms": [
                        "судоустройство"
                    ],
                    "common_phrases": [
                        "реформа суда",
                        "институт суда"
                    ],
                    "target_lang_definitions": {
                        "en": "The system of organizing judicial bodies and justice in the state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "judiciary",
                                "target_lang_sense_clarification": "system"
                            },
                            {
                                "target_lang_word": "courts",
                                "target_lang_sense_clarification": "judicial system"
                            }
                        ]
                    }
                }
            ]
        }
    ]

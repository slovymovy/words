    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c8a85b17-eb42-37ff-9646-a697e63160e9",
                    "sense_definition": "Taki, który następuje bezpośrednio po obecnym (zwykle w odniesieniu do jednostek czasu).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <w>przyszłym</w> tygodniu wyjeżdżamy na długie wakacje.",
                            "target_lang_translations": {
                                "ru": "На <w>следующей</w> неделе мы уезжаем в длинный отпуск."
                            }
                        },
                        {
                            "text": "Mam nadzieję, że <w>przyszły</w> rok będzie dla nas lepszy.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, что <w>будущий</w> год будет для нас лучше."
                            }
                        }
                    ],
                    "synonyms": [
                        "następny",
                        "kolejny"
                    ],
                    "antonyms": [
                        "zeszły",
                        "poprzedni",
                        "miniony",
                        "poprzedni"
                    ],
                    "common_phrases": [
                        "w przyszłym tygodniu",
                        "w przyszłym roku",
                        "w przyszłym miesiącu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий, наступающий непосредственно за текущим (обычно об отрезках времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следующий",
                                "target_lang_sense_clarification": "идущий следом, очередной"
                            },
                            {
                                "target_lang_word": "будущий",
                                "target_lang_sense_clarification": "предстоящий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69e24cb5-501d-3267-8ac3-50552ae31e6a",
                    "sense_definition": "Taki, który wydarzy się lub będzie istniał w czasie późniejszym niż obecny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy zadbać o czyste środowisko dla <w>przyszłych</w> pokoleń.",
                            "target_lang_translations": {
                                "ru": "Мы должны позаботиться о чистой окружающей среде для <w>будущих</w> поколений."
                            }
                        },
                        {
                            "text": "W szkole uczymy się, jak tworzyć czas <w>przyszły</w>.",
                            "target_lang_translations": {
                                "ru": "В школе мы учим, как образовывать <w>будущее</w> время."
                            }
                        }
                    ],
                    "antonyms": [
                        "przeszły",
                        "teraźniejszy"
                    ],
                    "common_phrases": [
                        "czas przyszły",
                        "przyszłe pokolenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к будущему; тот, который произойдет или будет существовать позже, чем сейчас."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "будущий",
                                "target_lang_sense_clarification": "относящийся к будущему времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b645ce7-6f44-3789-a54f-42b056ba303d",
                    "sense_definition": "Taki, który wkrótce obejmie jakąś funkcję, rolę lub stanie się osobą, o której mowa.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>przyszły</w> mąż jest bardzo dobrym człowiekiem.",
                            "target_lang_translations": {
                                "ru": "Мой <w>будущий</w> муж — очень хороший человек."
                            }
                        },
                        {
                            "text": "To jest <w>przyszły</w> dyrektor naszej dużej firmy.",
                            "target_lang_translations": {
                                "ru": "Это <w>будущий</w> директор нашей крупной фирмы."
                            }
                        }
                    ],
                    "antonyms": [
                        "zeszły",
                        "poprzedni"
                    ],
                    "common_phrases": [
                        "przyszły mąż",
                        "przyszła żona",
                        "przyszła mama"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто вскоре станет кем-либо или займет определенную должность или роль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "будущий",
                                "target_lang_sense_clarification": "о человеке, который станет кем-то"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c175d6f6-a227-3ace-8c00-3641abc282cc",
                    "sense_definition": "Trzecia osoba liczby mnogiej (rodzaj niemęskoosobowy) czasu przeszłego od: <w>przyjść</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje koleżanki <w>przyszły</w> na spotkanie trochę spóźnione.",
                            "target_lang_translations": {
                                "ru": "Мои подруги <w>пришли</w> на встречу с небольшим опозданием."
                            }
                        },
                        {
                            "text": "Dzieci <w>przyszły</w> ze szkoły bardzo głodne i zmęczone.",
                            "target_lang_translations": {
                                "ru": "Дети <w>пришли</w> из школы очень голодными и уставшими."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czas przeszły, 3. os. lm, rodzaj niemęskoosobowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени множественного числа от глагола «przyjść» (прийти)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пришли",

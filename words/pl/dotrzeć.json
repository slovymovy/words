    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "caa508cd-b18e-3882-b9ad-7c5137fa822b",
                    "sense_definition": "Przybyć do jakiegoś miejsca, osiągnąć cel podróży.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <dotrzeć> na lotnisko przynajmniej dwie godziny przed odlotem.",
                            "target_lang_translations": {
                                "ru": "Мы должны <добраться> до аэропорта как минимум за два часа до вылета."
                            }
                        },
                        {
                            "text": "Mimo gęstej mgły udało im się bezpiecznie <dotrzeć> do schroniska.",
                            "target_lang_translations": {
                                "ru": "Несмотря на густой туман, им удалось благополучно <добраться> до приюта."
                            }
                        }
                    ],
                    "synonyms": [
                        "dograć się",
                        "dopasować się"
                    ],
                    "antonyms": [
                        "wyruszyć",
                        "wyjechać"
                    ],
                    "common_phrases": [
                        "dotrzeć do celu",
                        "dotrzeć na miejsce",
                        "dotrzeć na czas",
                        "dotrzeć do domu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибыть в какое-либо место, достичь цели путешествия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добраться",
                                "target_lang_sense_clarification": "попасть в какое-то место, часто преодолевая трудности"
                            },
                            {
                                "target_lang_word": "достичь",
                                "target_lang_sense_clarification": "дойти или доехать до определенного места назначения"
                            },
                            {
                                "target_lang_word": "прибыть",
                                "target_lang_sense_clarification": "официально или с помощью транспорта приехать в место назначения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3d02117-f759-3fb3-9cbe-6ea00f199784",
                    "sense_definition": "Zostać usłyszanym, odebranym lub zrozumianym; trafić do czyjejś świadomości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_reach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dopiero po dłuższej chwili <dotarło> do mnie znaczenie jego słów.",
                            "target_lang_translations": {
                                "ru": "Только спустя некоторое время до меня <дошло> значение его слов."
                            }
                        },
                        {
                            "text": "Żadne logiczne argumenty nie <dotrą> do niego w tym stanie.",
                            "target_lang_translations": {
                                "ru": "Никакие логические аргументы не <дойдут> до него в этом состоянии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dotrzeć do świadomości",
                        "dotrzeć do wiadomości",
                        "nie docierać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть услышанным или понятым; дойти до чьего-либо сознания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дойти",
                                "target_lang_sense_clarification": "стать понятным, осознанным (разг., напр. «до меня дошло»)"
                            },
                            {
                                "target_lang_word": "достучаться",
                                "target_lang_sense_clarification": "добиться того, чтобы тебя услышали или поняли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a54089b8-8bae-3604-a3da-2393570a760d",
                    "sense_definition": "Przyzwyczaić się do siebie wzajemnie i nauczyć się zgodnie współpracować (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relationship_adjustment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy zespół musi się <dotrzeć>, zanim zacznie działać w pełni efektywnie.",
                            "target_lang_translations": {
                                "ru": "Новая команда должна <притереться>, прежде чем начнет работать в полную силу."
                            }
                        },
                        {
                            "text": "Potrzebowaliśmy kilku miesięcy, żeby się <dotrzeć> i zrozumieć swoje potrzeby.",
                            "target_lang_translations": {
                                "ru": "Нам понадобилось несколько месяцев, чтобы <притереться> и понять потребности друг друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "dograć się",
                        "dopasować się"
                    ],
                    "common_phrases": [
                        "dotrzeć się w pracy",
                        "dotrzeć się w związku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привыкнуть друг к другу и научиться слаженно сотрудничать (притирка характеров)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "притереться",
                                "target_lang_sense_clarification": "привыкнуть друг к другу, сгладить разногласия в отношениях"
                            },
                            {
                                "target_lang_word": "сработаться",
                                "target_lang_sense_clarification": "наладить успешную совместную работу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bd22059-c783-3993-afd8-f5bd1582602a",
                    "sense_definition": "Zakończyć czynność tarcia czegoś (np. warzyw na tarce).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cooking_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <dotrzeć> te ziemniaki do końca, żeby usmażyć placki.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <дотереть> этот картофель до конца, чтобы пожарить драники."
                            }
                        },
                        {
                            "text": "Kucharz poprosił pomocnika, aby <dotrzeć> resztę sera do sosu.",
                            "target_lang_translations": {
                                "ru": "Повар попросил помощника <дотереть> остатки сыра для соуса."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Закончить тереть что-либо (например, овощи на терке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дотереть",
                                "target_lang_sense_clarification": "закончить измельчение чего-либо на терке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
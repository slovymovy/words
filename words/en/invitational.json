{
    "word_family": [
        "invite",
        "invitation",
        "inviting",
        "invitee",
        "inviter"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "93aa4f0a-bd01-31b8-a50d-9f3b270c4300",
                    "sense_definition": "Restricted to participants who have been invited.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "restricted_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The club hosts an <w>invitational</w> golf match every summer.",
                            "target_lang_translations": {
                                "ru": "Клуб каждое лето проводит <w>пригласительный</w> матч по гольфу."
                            }
                        },
                        {
                            "text": "Participation in the conference is strictly <w>invitational</w>.",
                            "target_lang_translations": {
                                "ru": "Участие в конференции осуществляется строго <w>по приглашениям</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "invitation-only",
                        "exclusive"
                    ],
                    "antonyms": [
                        "open",
                        "public"
                    ],
                    "common_phrases": [
                        "invitational tournament",
                        "invitational event",
                        "invitational match"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограниченный кругом участников, получивших специальное приглашение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по приглашениям",
                                "target_lang_sense_clarification": "о мероприятии или доступе"
                            },
                            {
                                "target_lang_word": "пригласительный",
                                "target_lang_sense_clarification": "связанный с приглашением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2781f429-1470-30da-a136-31449223c654",
                    "sense_definition": "Serving as an invitation or having the nature of an invitation.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He made an <w>invitational</w> gesture toward the empty chair.",
                            "target_lang_translations": {
                                "ru": "Он сделал <w>приглашающий</w> жест в сторону пустого стула."
                            }
                        },
                        {
                            "text": "The rhetoric used in the speech was <w>invitational</w> rather than demanding.",
                            "target_lang_translations": {
                                "ru": "Риторика в речи была скорее <w>приглашающей</w>, чем требовательной."
                            }
                        }
                    ],
                    "synonyms": [
                        "inviting",
                        "welcoming"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служащий приглашением или имеющий характер приглашения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приглашающий",
                                "target_lang_sense_clarification": "о жесте или тоне"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a3cc724-6dec-352d-82c3-03c790dbb597",
                    "sense_definition": "An event, especially a tournament, that is restricted to invited participants.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "restricted_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She won the charity <w>invitational</w> last weekend.",
                            "target_lang_translations": {
                                "ru": "В прошлые выходные она выиграла благотворительный <w>турнир по приглашениям</w>."
                            }
                        },
                        {
                            "text": "Several top players withdrew from the <w>invitational</w> due to injury.",
                            "target_lang_translations": {
                                "ru": "Несколько ведущих игроков снялись с <w>турнира по приглашениям</w> из-за травм."
                            }
                        }
                    ],
                    "synonyms": [
                        "tournament"
                    ],
                    "common_phrases": [
                        "golf invitational",
                        "annual invitational",
                        "charity invitational"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мероприятие, особенно спортивный турнир, в котором участвуют только приглашенные лица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "турнир по приглашениям",
                                "target_lang_sense_clarification": "спортивное соревнование"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
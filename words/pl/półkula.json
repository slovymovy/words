    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4a207e82-a6f3-377a-b4e6-11cad892e193",
                    "sense_definition": "Jedna z dwóch części kuli ziemskiej, oddzielona równikiem (północna i południowa) lub południkami (wschodnia i zachodnia).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polska leży na <w>półkuli</w> północnej.",
                            "target_lang_translations": {
                                "ru": "Польша находится в Северном <w>полушарии</w>."
                            }
                        },
                        {
                            "text": "Podróż na drugą <w>półkulę</w> zajęła nam kilkanaście godzin.",
                            "target_lang_translations": {
                                "ru": "Путешествие в другое <w>полушарие</w> заняло у нас больше десяти часов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "półkula północna",
                        "półkula południowa",
                        "półkula wschodnia",
                        "półkula zachodnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух частей земного шара, отделенная экватором (северная и южная) или меридианами (восточная и западная)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полушарие",
                                "target_lang_sense_clarification": "географическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b21dc84-6e35-3b35-9450-a2cb25e0f34f",
                    "sense_definition": "Połowa kuli; bryła geometryczna lub przedmiot o takim kształcie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dach obserwatorium ma kształt wielkiej <w>półkuli</w>.",
                            "target_lang_translations": {
                                "ru": "Крыша обсерватории имеет форму огромной <w>полусферы</w>."
                            }
                        },
                        {
                            "text": "Na lekcji matematyki obliczaliśmy objętość <w>półkuli</w>.",
                            "target_lang_translations": {
                                "ru": "На уроке математики мы вычисляли объём <w>полушария</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kształt półkuli",
                        "powierzchnia półkuli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Половина шара; геометрическое тело или предмет такой формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полусфера",
                                "target_lang_sense_clarification": "форма"
                            },
                            {
                                "target_lang_word": "полушарие",
                                "target_lang_sense_clarification": "геометрическое тело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31a3cfda-b116-336a-8e36-3264cf6ed2f3",
                    "sense_definition": "Jedna z dwóch głównych części mózgu, odpowiedzialna za różne funkcje organizmu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "U osób praworęcznych dominuje zazwyczaj lewa <w>półkula</w>.",
                            "target_lang_translations": {
                                "ru": "У правшей обычно доминирует левое <w>полушарие</w>."
                            }
                        },
                        {
                            "text": "Prawa <w>półkula</w> mózgu odpowiada za wyobraźnię i kreatywność.",
                            "target_lang_translations": {
                                "ru": "Правое <w>полушарие</w> мозга отвечает за воображение и творчество."
                            }
                        }
                    ],
                    "synonyms": [
                        "hemisfera"
                    ],
                    "common_phrases": [
                        "lewa półkula",
                        "prawa półkula",
                        "półkula mózgu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух главных частей мозга, отвечающая за различные функции организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полушарие",
                                "target_lang_sense_clarification": "мозга"
                            }
                        ]
                    }
                }
            ]
        }
    ]

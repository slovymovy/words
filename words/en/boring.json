        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "54487946-3555-3acf-b560-0efd1e7b615b",
                    "sense_definition": "Causing boredom or tiredness; making one feel tired and impatient because something is not interesting.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This movie is so <w>boring</w> that I want to leave the cinema.",
                            "target_lang_translations": {
                                "ru": "Этот фильм такой <w>скучный</w>, что я хочу уйти из кинотеатра."
                            }
                        },
                        {
                            "text": "He has a very <w>boring</w> job and wants to find a new one.",
                            "target_lang_translations": {
                                "ru": "У него очень <w>скучная</w> работа, и он хочет найти новую."
                            }
                        }
                    ],
                    "synonyms": [
                        "dull",
                        "uninteresting",
                        "tedious",
                        "monotonous",
                        "mind-numbing"
                    ],
                    "antonyms": [
                        "interesting",
                        "exciting",
                        "entertaining",
                        "fascinating",
                        "engaging"
                    ],
                    "common_phrases": [

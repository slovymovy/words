    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d57940e1-56aa-3625-83ce-215a2b2dce55",
                    "sense_definition": "Инструмент для нанесения краски, клея или лака, состоящий из пучка волосков или щетины, закреплённых на рукоятке",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Художник взял тонкую <кисть> и начал рисовать.",
                            "target_lang_translations": {
                                "en": "The artist took a thin <brush> and began to draw."
                            }
                        },
                        {
                            "text": "Для покраски стен нам нужна широкая малярная <кисть>.",
                            "target_lang_translations": {
                                "en": "We need a wide paint <brush> to paint the walls."
                            }
                        }
                    ],
                    "common_phrases": [
                        "малярная кисть",
                        "художественная кисть",
                        "взять кисть"
                    ],
                    "target_lang_definitions": {
                        "en": "An instrument for applying paint, glue, or varnish, consisting of a tuft of hairs or bristles secured to a handle"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brush",
                                "target_lang_sense_clarification": "tool for painting or cleaning"
                            },
                            {
                                "target_lang_word": "paintbrush",
                                "target_lang_sense_clarification": "specifically for painting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae5969ce-7531-351d-b852-137907862ab7",
                    "sense_definition": "Часть руки человека от запястья до кончиков пальцев",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При встрече он крепко пожал мне <кисть>.",
                            "target_lang_translations": {
                                "en": "Upon meeting, he firmly shook my <hand>."
                            }
                        },
                        {
                            "text": "У пианиста должны быть сильные и гибкие <кисти>.",
                            "target_lang_translations": {
                                "en": "A pianist must have strong and flexible <hands>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "кисть руки",
                        "движение кисти",
                        "сломать кисть"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of the human arm from the wrist to the fingertips"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hand",
                                "target_lang_sense_clarification": "anatomy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aebc2083-dc12-381d-a43c-e603ba321ed9",
                    "sense_definition": "Скопление плодов или цветков на одной ветке; гроздь",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На столе лежала тяжёлая <кисть> спелого винограда.",
                            "target_lang_translations": {
                                "en": "A heavy <bunch> of ripe grapes lay on the table."
                            }
                        },
                        {
                            "text": "Осенью в саду краснеют яркие <кисти> рябины.",
                            "target_lang_translations": {
                                "en": "In autumn, bright <clusters> of rowan berries turn red in the garden."
                            }
                        }
                    ],
                    "synonyms": [
                        "гроздь"
                    ],
                    "common_phrases": [
                        "кисть винограда",
                        "кисть сирени",
                        "цветочная кисть"
                    ],
                    "target_lang_definitions": {
                        "en": "A cluster of fruits or flowers on a single branch; a bunch"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bunch",
                                "target_lang_sense_clarification": "of grapes, keys, etc."
                            },
                            {
                                "target_lang_word": "cluster",
                                "target_lang_sense_clarification": "group of similar items"
                            },
                            {
                                "target_lang_word": "spray",
                                "target_lang_sense_clarification": "of flowers or berries"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a207dfb-e048-3499-b3cd-0aff94dadfc8",
                    "sense_definition": "Украшение из пучка нитей или шнуров, связанных вместе с одного конца",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "decoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старинные шторы были украшены тяжёлыми золотыми <кистями>.",
                            "target_lang_translations": {
                                "en": "The antique curtains were decorated with heavy gold <tassels>."
                            }
                        },
                        {
                            "text": "На его шапке весело болталась яркая <кисть>.",
                            "target_lang_translations": {
                                "en": "A bright <tassel> dangled cheerfully on his hat."
                            }
                        }
                    ],
                    "common_phrases": [
                        "шторы с кистями",
                        "шапка с кистью",
                        "украсить кистями"
                    ],
                    "target_lang_definitions": {
                        "en": "A decoration made of a bunch of threads or cords tied together at one end"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tassel",
                                "target_lang_sense_clarification": "decoration"
                            }
                        ]
                    }
                }
            ]
        },

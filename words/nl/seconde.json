            "pos": "noun",
            "senses": [
                {
                    "sense_id": "50166755-965d-366d-a17b-c60043a10841",
                    "sense_definition": "De basis-eenheid van tijd, gelijk aan een zestigste deel van een minuut.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een minuut bestaat uit zestig <w>seconden</w>.",
                            "target_lang_translations": {
                                "ru": "Минута состоит из шестидесяти <w>секунд</w>.",
                                "en": "A minute consists of sixty <w>seconds</w>."
                            }
                        },
                        {
                            "text": "De atleet rende de honderd meter in minder dan tien <w>seconden</w>.",
                            "target_lang_translations": {
                                "ru": "Атлет пробежал сто метров менее чем за десять <w>секунд</w>.",
                                "en": "The athlete ran the hundred meters in less than ten <w>seconds</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tel"
                    ],
                    "common_phrases": [
                        "aantal seconden",
                        "per seconde",
                        "halve seconde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Базовая единица времени, равная одной шестидесятой части минуты.",
                        "en": "The basic unit of time, equal to one sixtieth of a minute."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секунда",
                                "target_lang_sense_clarification": "единица времени"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "second",
                                "target_lang_sense_clarification": "unit of time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bfd4c47-ee29-338b-b949-4a639bdc2ac4",
                    "sense_definition": "Een zeer korte tijdsduur of een ogenblik.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je een <w>seconde</w> om mij even te helpen?",
                            "target_lang_translations": {
                                "ru": "У тебя есть <w>секунда</w>, чтобы помочь мне?",
                                "en": "Do you have a <w>second</w> to help me?"
                            }
                        },
                        {
                            "text": "Hij twijfelde geen <w>seconde</w> aan zijn beslissing.",
                            "target_lang_translations": {
                                "ru": "Он ни <w>секунды</w> не сомневался в своём решении.",
                                "en": "He didn't doubt his decision for a <w>second</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogenblik",
                        "tel"
                    ],
                    "common_phrases": [
                        "wacht een seconde",
                        "geen seconde",
                        "elke seconde"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень короткий промежуток времени или мгновение.",
                        "en": "A very short period of time or a moment."
                    },
                    "translations": {

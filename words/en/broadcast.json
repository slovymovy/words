    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0c09e97a-31bf-3ded-93f5-c1dfbce865d2",
                    "sense_definition": "To transmit a message or signal through radio waves or electronic means.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "media_transmission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local radio station will <w>broadcast</w> the interview at noon.",
                            "target_lang_translations": {
                                "ru": "Местная радиостанция <w>передаст</w> интервью в полдень."
                            }
                        },
                        {
                            "text": "They plan to <w>broadcast</w> the concert live to millions of viewers.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>транслировать</w> концерт в прямом эфире для миллионов зрителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "air",
                        "transmit",
                        "send"
                    ],
                    "antonyms": [
                        "narrowcast"
                    ],
                    "common_phrases": [
                        "broadcast live",
                        "broadcast news",
                        "broadcast a signal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать сообщение или сигнал посредством радиоволн или электронных средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вещать",
                                "target_lang_sense_clarification": "о радио или телевидении"
                            },
                            {
                                "target_lang_word": "транслировать",
                                "target_lang_sense_clarification": "передавать в эфир программу или событие"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "отправлять сигнал или сообщение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46a511d7-98df-3bc9-81e7-85e394cd5df7",
                    "sense_definition": "To make information known to many people; to spread widely.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_spreading",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>broadcast</w> my secrets to the entire office.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>разглашай</w> мои секреты всему офису."
                            }
                        },
                        {
                            "text": "He decided to <w>broadcast</w> his complaints on social media.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>распространить</w> свои жалобы в социальных сетях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "broadcast intentions",
                        "broadcast emotions"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать информацию известной многим людям; широко распространять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распространять",
                                "target_lang_sense_clarification": "делать известным широкому кругу"
                            },
                            {
                                "target_lang_word": "разглашать",
                                "target_lang_sense_clarification": "раскрывать тайну или секрет"
                            },
                            {
                                "target_lang_word": "раструбить",
                                "target_lang_sense_clarification": "разг. всем рассказать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7af0751-8522-37d4-8386-99fe9ea9a851",

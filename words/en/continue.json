{
    "word_family": [
        "continuation",
        "continuity",
        "continuous",
        "continual",
        "discontinuance",
        "discontinue"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6077aa0d-7d50-3634-ad24-3f39aa77a431",
                    "sense_definition": "To keep doing an activity or to make it last longer without stopping.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "proceed_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>continue</w> with your work while I am away.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>продолжайте</w> работать, пока меня нет."
                            }
                        },
                        {
                            "text": "The rain is expected to <w>continue</w> for the rest of the day.",
                            "target_lang_translations": {
                                "ru": "Ожидается, что дождь будет <w>продолжаться</w> до конца дня."
                            }
                        },
                        {
                            "text": "She wants to <w>continue</w> learning English until she is fluent.",
                            "target_lang_translations": {
                                "ru": "Она хочет <w>продолжать</w> учить английский, пока не заговорит свободно."
                            }
                        }
                    ],
                    "synonyms": [
                        "carry on",
                        "keep on",
                        "go on with",
                        "proceed with",
                        "keep up"
                    ],
                    "antonyms": [
                        "stop",
                        "discontinue",
                        "terminate"
                    ],
                    "common_phrases": [
                        "continue to",
                        "continue with",
                        "continue doing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжать делать что-либо или длиться в течение какого-то времени без остановки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продолжать",
                                "target_lang_sense_clarification": "делать начатое ранее"
                            },
                            {
                                "target_lang_word": "продолжаться",
                                "target_lang_sense_clarification": "длиться, происходить (о событиях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3571ce10-17ad-3d53-9993-6293c50f8ed0",
                    "sense_definition": "To start something again after a pause or interruption.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "resume_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We will <w>continue</w> the meeting after a short lunch break.",
                            "target_lang_translations": {
                                "ru": "Мы <w>продолжим</w> собрание после короткого перерыва на обед."
                            }
                        },
                        {
                            "text": "The story <w>continues</w> in the next chapter of the book.",
                            "target_lang_translations": {
                                "ru": "История <w>продолжается</w> в следующей главе книги."
                            }
                        }
                    ],
                    "synonyms": [
                        "resume",
                        "go on",
                        "proceed"
                    ],
                    "common_phrases": [
                        "continue after",
                        "continue from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать что-либо снова после паузы или перерыва."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продолжить",
                                "target_lang_sense_clarification": "возобновить действие после паузы"
                            },
                            {
                                "target_lang_word": "возобновить",
                                "target_lang_sense_clarification": "начать снова"
                            },
                            {
                                "target_lang_word": "продолжиться",
                                "target_lang_sense_clarification": "начаться снова (о сюжете, событии)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4147aba4-813e-3234-b265-f8f5905d5550",
                    "sense_definition": "To stay in a particular state, place, or job for a period of time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "remain_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to <w>continue</w> as the team captain for another year.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>остаться</w> капитаном команды ещё на один год."
                            }
                        },
                        {
                            "text": "The weather <w>continued</w> cold and windy for several weeks.",
                            "target_lang_translations": {
                                "ru": "Погода <w>оставалась</w> холодной и ветреной в течение нескольких недель."
                            }
                        }
                    ],
                    "common_phrases": [
                        "continue as",
                        "continue in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставаться в определенном состоянии, месте или должности в течение некоторого времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставаться",
                                "target_lang_sense_clarification": "сохранять статус или положение"
                            },
                            {
                                "target_lang_word": "сохраняться",
                                "target_lang_sense_clarification": "оставаться неизменным (о погоде, состоянии)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffc66eb8-a023-3c81-800a-f158a1acf088",
                    "sense_definition": "To postpone a legal proceeding or meeting to a later date.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal_postpone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge decided to <w>continue</w> the case until further evidence could be found.",
                            "target_lang_translations": {
                                "ru": "Судья решил <w>отложить</w> рассмотрение дела, пока не будут найдены дополнительные улики."
                            }
                        },
                        {
                            "text": "This meeting has been <w>continued</w> to the thirteenth of July.",
                            "target_lang_translations": {
                                "ru": "Это заседание было <w>перенесено</w> на тринадцатое июля."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отложить судебное разбирательство или встречу на более поздний срок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "переносить на более поздний срок"
                            },
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "назначать на другое время"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d04c5cb8-d5ad-3417-8b9b-e98c3ffffa44",
                    "sense_definition": "An option in a video game that allows a player to resume play after losing all their lives.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gaming_option",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I used a <w>continue</w> to try the difficult level again.",
                            "target_lang_translations": {
                                "ru": "Я использовал <w>попытку</w>, чтобы снова попробовать пройти сложный уровень."
                            }
                        },
                        {
                            "text": "You only have one <w>continue</w> left before the game ends completely.",
                            "target_lang_translations": {
                                "ru": "У тебя осталась только одна <w>попытка</w>, прежде чем игра окончательно закончится."
                            }
                        }
                    ],
                    "common_phrases": [
                        "use a continue",
                        "infinite continues"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "video games"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Опция в видеоигре, позволяющая игроку возобновить игру после потери всех жизней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попытка",
                                "target_lang_sense_clarification": "шанс продолжить игру"
                            },
                            {
                                "target_lang_word": "продолжение",
                                "target_lang_sense_clarification": "возобновление игры"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
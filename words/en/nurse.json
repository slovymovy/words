{
    "word_family": [
        "nursery",
        "nursemaid",
        "nursing"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c421f213-26cc-321f-bf58-6348db6bc246",
                    "sense_definition": "A medical professional trained to care for sick or injured people, usually working in a hospital or clinic.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "medical_professional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>nurse</w> checked my temperature and gave me some medicine.",
                            "target_lang_translations": {
                                "ru": "<w>Медсестра</w> измерила мне температуру и дала лекарство."
                            }
                        },
                        {
                            "text": "Please ask the <w>nurse</w> for a glass of water if you are thirsty.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, попросите <w>медсестру</w> дать стакан воды, если хотите пить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "registered nurse",
                        "school nurse",
                        "night nurse",
                        "head nurse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медицинский работник, обученный уходу за больными или ранеными, обычно работающий в больнице или клинике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медсестра",
                                "target_lang_sense_clarification": "женщина"
                            },
                            {
                                "target_lang_word": "медбрат",
                                "target_lang_sense_clarification": "мужчина"
                            },
                            {
                                "target_lang_word": "сиделка",
                                "target_lang_sense_clarification": "ухаживающая за тяжелобольными"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13c62015-41af-398f-91db-7ab1665f0a27",
                    "sense_definition": "A person employed to take care of young children, especially in the past.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "childcare",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children were walking in the park with their <w>nurse</w>.",
                            "target_lang_translations": {
                                "ru": "Дети гуляли в парке со своей <w>няней</w>."
                            }
                        },
                        {
                            "text": "She worked as a <w>nurse</w> for a wealthy family in London during the Victorian era.",
                            "target_lang_translations": {
                                "ru": "Она работала <w>няней</w> в богатой семье в Лондоне в викторианскую эпоху."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wet nurse",
                        "nursery nurse"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "modern usage typically prefers 'nanny' or 'au pair'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, нанятый для ухода за маленькими детьми (преимущественно устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "няня",
                                "target_lang_sense_clarification": "воспитательница детей"
                            },
                            {
                                "target_lang_word": "кормилица",
                                "target_lang_sense_clarification": "женщина, вскармливающая чужого ребенка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0db95d91-f3d3-3600-9ac6-5fd14c8710e8",
                    "sense_definition": "To look after a sick or injured person to help them recover.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "care_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She stayed home to <w>nurse</w> her sick husband back to health.",
                            "target_lang_translations": {
                                "ru": "Она осталась дома, чтобы <w>выхаживать</w> больного мужа."
                            }
                        },
                        {
                            "text": "It took weeks to <w>nurse</w> the injured bird until it could fly again.",
                            "target_lang_translations": {
                                "ru": "Потребовались недели, чтобы <w>выходить</w> раненую птицу, пока она снова не смогла летать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nurse back to health",
                        "nurse a patient"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ухаживать за больным или раненым человеком, помогая ему выздороветь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выхаживать",
                                "target_lang_sense_clarification": "помогать выздороветь"
                            },
                            {
                                "target_lang_word": "ухаживать",
                                "target_lang_sense_clarification": "заботиться о больном"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ae5beb1-6308-3549-9499-b8e0e90b0eb0",
                    "sense_definition": "To feed a baby with milk from the breast.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "feeding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mother sat quietly to <w>nurse</w> her newborn baby.",
                            "target_lang_translations": {
                                "ru": "Мать тихо сидела, чтобы <w>покормить грудью</w> своего новорожденного ребенка."
                            }
                        },
                        {
                            "text": "Doctors recommend that women <w>nurse</w> their infants for at least six months.",
                            "target_lang_translations": {
                                "ru": "Врачи рекомендуют женщинам <w>кормить грудью</w> младенцев не менее шести месяцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "feed#Verb"
                    ],
                    "common_phrases": [
                        "nurse a baby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кормить младенца грудным молоком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кормить грудью",
                                "target_lang_sense_clarification": "естественное вскармливание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "283297f6-f235-33d6-bb9e-941d0a047008",
                    "sense_definition": "To drink a beverage slowly over a long period of time.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drinking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sat at the bar and decided to <w>nurse</w> his beer all night.",
                            "target_lang_translations": {
                                "ru": "Он сидел в баре и решил весь вечер <w>цедить</w> свое пиво."
                            }
                        },
                        {
                            "text": "She was <w>nursing</w> a cup of coffee while waiting for the train.",
                            "target_lang_translations": {
                                "ru": "Она <w>потягивала</w> чашку кофе, ожидая поезда."
                            }
                        }
                    ],
                    "synonyms": [
                        "sip"
                    ],
                    "common_phrases": [
                        "nurse a drink",
                        "nurse a beer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пить напиток медленно, растягивая его на долгое время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потягивать",
                                "target_lang_sense_clarification": "пить маленькими глотками"
                            },
                            {
                                "target_lang_word": "цедить",
                                "target_lang_sense_clarification": "пить очень медленно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f651426-e3e8-3463-950e-fd0ffe4fbb31",
                    "sense_definition": "To keep a feeling, usually a negative one, in one's mind for a long time.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He continued to <w>nurse</w> a grudge against his former boss.",
                            "target_lang_translations": {
                                "ru": "Он продолжал <w>таить</w> обиду на своего бывшего начальника."
                            }
                        },
                        {
                            "text": "There is no point in <w>nursing</w> old regrets about the past.",
                            "target_lang_translations": {
                                "ru": "Нет смысла <w>лелеять</w> старые сожаления о прошлом."
                            }
                        }
                    ],
                    "synonyms": [
                        "harbor#Verb",
                        "dwell on"
                    ],
                    "common_phrases": [
                        "nurse a grudge",
                        "nurse a grievance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Долго сохранять в душе чувство, обычно негативное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таить",
                                "target_lang_sense_clarification": "обиду или злобу"
                            },
                            {
                                "target_lang_word": "лелеять",
                                "target_lang_sense_clarification": "мысль или чувство"
                            },
                            {
                                "target_lang_word": "вынашивать",
                                "target_lang_sense_clarification": "план или чувство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d3a953d3-a300-37bd-84a9-e2a879835174",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Nurse</w> signed the documents at the front desk.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Нёрс</w> подписал документы на стойке регистрации."
                            }
                        },
                        {
                            "text": "We are looking for the records of a family named <w>Nurse</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ищем записи о семье по фамилии <w>Нёрс</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Нёрс",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
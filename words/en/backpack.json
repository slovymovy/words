{
    "word_family": [
        "backpacker",
        "backpacking"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "92056ff3-373c-3167-b9fc-057adc748ff8",
                    "sense_definition": "A bag with shoulder straps, worn on the back, used for carrying things like hiking gear or books.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "bag_container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She put her textbooks in her <w>backpack</w> before walking to school.",
                            "target_lang_translations": {
                                "ru": "Она положила учебники в свой <w>рюкзак</w> перед тем, как пойти в школу."
                            }
                        },
                        {
                            "text": "The hiker carried a heavy <w>backpack</w> filled with water and food.",
                            "target_lang_translations": {
                                "ru": "Турист нес тяжелый <w>рюкзак</w>, наполненный водой и едой."
                            }
                        }
                    ],
                    "synonyms": [
                        "rucksack",
                        "knapsack",
                        "bookbag"
                    ],
                    "common_phrases": [
                        "heavy backpack",
                        "school backpack",
                        "wear a backpack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумка с лямками для ношения на спине, используемая для переноски вещей, таких как походное снаряжение или книги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рюкзак",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "ранец",
                                "target_lang_sense_clarification": "школьный или жесткий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85a2d153-54da-3aa9-b926-3be87608b033",
                    "sense_definition": "A container worn on the back that holds a parachute or life-support equipment.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "bag_container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The skydiver checked his <w>backpack</w> carefully before jumping from the plane.",
                            "target_lang_translations": {
                                "ru": "Парашютист тщательно проверил свой <w>ранец</w> перед прыжком из самолета."
                            }
                        },
                        {
                            "text": "Astronauts wear a life-support <w>backpack</w> when they work outside the spacecraft.",
                            "target_lang_translations": {
                                "ru": "Космонавты носят <w>ранец</w> жизнеобеспечения, когда работают снаружи космического корабля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "parachute backpack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Контейнер, носимый на спине, в котором хранится парашют или система жизнеобеспечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранец",
                                "target_lang_sense_clarification": "парашютный или технический"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "691429b6-6668-3bed-81f7-99c8313ad8fd",
                    "sense_definition": "To travel cheaply for a long period, often in cities, carrying one's belongings in a backpack.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "travel_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many students decide to <w>backpack</w> across Europe during their summer break.",
                            "target_lang_translations": {
                                "ru": "Многие студенты решают <w>путешествовать с рюкзаком</w> по Европе во время летних каникул."
                            }
                        },
                        {
                            "text": "She wants to <w>backpack</w> around the world before starting her new job.",
                            "target_lang_translations": {
                                "ru": "Она хочет <w>попутешествовать с рюкзаком</w> по миру, прежде чем начать новую работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "backpack across Europe",
                        "backpack around the world"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествовать экономно в течение длительного периода, часто по городам, с вещами в рюкзаке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путешествовать с рюкзаком",
                                "target_lang_sense_clarification": "вид туризма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81325e8d-0cf6-358e-a3e3-a4ee55cae6a7",
                    "sense_definition": "To hike and camp in the wilderness while carrying gear in a backpack.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "travel_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We plan to <w>backpack</w> through the mountains for three days.",
                            "target_lang_translations": {
                                "ru": "Мы планируем <w>пойти в поход с рюкзаками</w> по горам на три дня."
                            }
                        },
                        {
                            "text": "He loves to <w>backpack</w> in remote areas where there are no cars.",
                            "target_lang_translations": {
                                "ru": "Он любит <w>ходить в походы с рюкзаком</w> в отдаленных районах, где нет машин."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go backpacking"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершать пешие походы и ночевать на природе, перенося снаряжение в рюкзаке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить в поход",
                                "target_lang_sense_clarification": "пеший туризм"
                            },
                            {
                                "target_lang_word": "путешествовать пешком",
                                "target_lang_sense_clarification": "с рюкзаком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c011f77-c9a8-3b8f-be09-ab18f01d6bc1",
                    "sense_definition": "To place or carry items in a backpack.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>backpacked</w> my lunch and a raincoat for the day trip.",
                            "target_lang_translations": {
                                "ru": "Я <w>положил в рюкзак</w> обед и дождевик для однодневной поездки."
                            }
                        },
                        {
                            "text": "He <w>backpacked</w> all the necessary supplies before the sun came up.",
                            "target_lang_translations": {
                                "ru": "Он <w>уложил в рюкзак</w> все необходимые припасы до восхода солнца."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать или переносить предметы в рюкзаке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нести в рюкзаке",
                                "target_lang_sense_clarification": "переносить"
                            },
                            {
                                "target_lang_word": "укладывать в рюкзак",
                                "target_lang_sense_clarification": "собирать вещи"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
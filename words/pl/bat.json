        "batog"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c787f49-e1be-3437-8b34-b18d3ae4f723",
                    "sense_definition": "Narzędzie składające się z rzemienia lub sznura przymocowanego do trzonka, służące do popędzania zwierząt lub wymierzania kar.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woźnica chwycił za <bat> i mocno smagnął konie, aby przyspieszyły.",
                            "target_lang_translations": {
                                "ru": "Кучер схватил <кнут> и сильно стегнул лошадей, чтобы они ускорились."
                            }
                        },
                        {
                            "text": "Dozorca trzymał w ręku długi <bat>, pilnując porządku wśród pracujących.",
                            "target_lang_translations": {
                                "ru": "Надзиратель держал в руке длинный <кнут>, следя за порядком среди работающих."
                            }
                        }
                    ],
                    "synonyms": [
                        "bicz",
                        "batog",
                        "bykowiec"
                    ],
                    "common_phrases": [
                        "kręcić na kogoś bat",
                        "mieć nad sobą bat",
                        "bat na tyłek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Орудие, состоящее из ремня или веревки, прикрепленной к рукоятке, используемое для понукания животных или наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кнут",
                                "target_lang_sense_clarification": "орудие для понукания или наказания"
                            },
                            {
                                "target_lang_word": "бич",
                                "target_lang_sense_clarification": "длинная плеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "423710a5-7517-3f6f-8bc8-260259cb25e8",
                    "sense_definition": "Dotkliwa porażka w rywalizacji, zwykle sportowej, lub surowa kara cielesna.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "punishment_defeat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza drużyna zebrała straszne <baty> w ostatnim meczu ligowym.",
                            "target_lang_translations": {
                                "ru": "Наша команда потерпела страшный <разгром> в последнем матче лиги."
                            }
                        },
                        {
                            "text": "Za swoje nieposłuszeństwo chłopiec dostał solidne <baty> od ojca.",
                            "target_lang_translations": {
                                "ru": "За свое непослушание мальчик получил от отца солидную <порку>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lanie",
                        "cięgi",
                        "bicie"
                    ],
                    "common_phrases": [
                        "dostać baty",
                        "sprawić baty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used primarily in plural form 'baty' for defeat or beating"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разгромное поражение в соревновании (обычно спортивном) или суровое телесное наказание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порка",
                                "target_lang_sense_clarification": "телесное наказание"
                            },
                            {
                                "target_lang_word": "разгром",
                                "target_lang_sense_clarification": "крупное поражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3b16dca-9611-30e2-a40d-040c47cbae14",
                    "sense_definition": "Ręcznie skręcony papieros zawierający marihuanę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drug",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na imprezie ktoś wyciągnął <bata> i poczęstował nim resztę znajomych.",
                            "target_lang_translations": {
                                "ru": "На вечеринке кто-то достал <косяк> и угостил им остальных знакомых."
                            }
                        },
                        {
                            "text": "Młodzi ludzie schowali się za blokiem, żeby w spokoju wypalić <bata>.",
                            "target_lang_translations": {
                                "ru": "Молодые люди спрятались за домом, чтобы спокойно выкурить <косяк>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wał"
                    ],
                    "common_phrases": [
                        "skręcić bata",
                        "palić bata"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Самокрутка с марихуаной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "косяк",
                                "target_lang_sense_clarification": "сленг: самокрутка с наркотиком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ff6a734-9867-3e39-9349-e119b86e03d9",
                    "sense_definition": "Podstawowa jednostka monetarna używana w Tajlandii.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeden <bat> dzieli się na sto satangów, co jest ważne przy drobnych zakupach.",
                            "target_lang_translations": {
                                "ru": "Один <бат> делится на сто сатангов, что важно при мелких покупках."
                            }
                        },
                        {
                            "text": "Wymieniłem dolary na <baty>, aby móc zapłacić za hotel w Bangkoku.",
                            "target_lang_translations": {
                                "ru": "Я обменял доллары на <баты>, чтобы иметь возможность заплатить за отель в Бангкоке."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная единица Таиланда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бат",
                                "target_lang_sense_clarification": "валюта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82330a39-3fa6-3887-95ba-e7c647b728fc",
                    "sense_definition": "Długa wędka bez kołowrotka i przelotek, służąca do łowienia ryb metodą spławikową.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wędkarz zarzucił swój lekki <bat> w pobliże trzcin, licząc na branie.",
                            "target_lang_translations": {
                                "ru": "Рыбак забросил свою легкую <маховую удочку> поближе к камышам, надеясь на клев."
                            }
                        },
                        {
                            "text": "Łowienie na <bat> wymaga dużej precyzji i wprawy w operowaniu zestawem.",
                            "target_lang_translations": {
                                "ru": "Ловля на <маховую удочку> требует большой точности и сноровки в управлении снастью."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "jargon used by anglers"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная удочка без катушки и пропускных колец для поплавочной ловли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маховая удочка",
                                "target_lang_sense_clarification": "рыболовная снасть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f7dde44-b380-35ab-a383-60d03956fe91",
                    "sense_definition": "Niewielka łódź rybacka lub statek rzeczny, zazwyczaj o jednym maszcie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "boat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rybacy wypłynęli na morze starym, drewnianym <batem>, aby zarzucić sieci.",
                            "target_lang_translations": {
                                "ru": "Рыбаки вышли в море на старом деревянном <боте>, чтобы забросить сети."
                            }
                        },
                        {
                            "text": "Przy nabrzeżu stał zacumowany duży <bat> towarowy, czekający na załadunek.",
                            "target_lang_translations": {
                                "ru": "У причала стоял пришвартованный большой грузовой <бот>, ожидающий погрузки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical or historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая рыбацкая лодка или речное судно, обычно одномачтовое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бот",
                                "target_lang_sense_clarification": "тип судна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d274074-2c49-362d-8ab2-f2384c9a64d0",
                    "sense_definition": "Starożytna, biblijna miara objętości płynów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Według starych zapisków naczynie to mieściło dokładnie jeden <bat> oliwy.",
                            "target_lang_translations": {
                                "ru": "Согласно старым записям, этот сосуд вмещал ровно один <бат> масла."
                            }
                        },
                        {
                            "text": "Biblijny <bat> odpowiadał w przybliżeniu dwudziestu dwóm litrom współczesnej miary.",
                            "target_lang_translations": {
                                "ru": "Библейский <бат> соответствовал примерно двадцати двум литрам современной меры."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical unit"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Древняя библейская мера объёма жидкостей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бат",
                                "target_lang_sense_clarification": "мера объема"
                            }
                        ]
                    }
                }
            ]
        }
    ]

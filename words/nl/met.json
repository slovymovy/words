{
    "word_family": [
        "mee"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "f1ced21e-829e-3da8-9c76-3f2a8a9816dc",
                    "sense_definition": "In gezelschap van of samen met iemand of iets.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "accompaniment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga vanavond <met> mijn vrienden naar de bioscoop.",
                            "target_lang_translations": {
                                "en": "I am going to the cinema <with> my friends tonight.",
                                "ru": "Я иду сегодня вечером в кино <с> друзьями."
                            }
                        },
                        {
                            "text": "Wil je koffie <met> melk en suiker of drink je het zwart?",
                            "target_lang_translations": {
                                "en": "Do you want coffee <with> milk and sugar or do you drink it black?",
                                "ru": "Ты хочешь кофе <с> молоком и сахаром или пьёшь чёрный?"
                            }
                        }
                    ],
                    "antonyms": [
                        "zonder"
                    ],
                    "common_phrases": [
                        "samen met",
                        "met elkaar",
                        "met z'n tweeën"
                    ],
                    "target_lang_definitions": {
                        "en": "In the company of or together with someone or something.",
                        "ru": "В компании или вместе с кем-либо или чем-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "with",
                                "target_lang_sense_clarification": "accompaniment"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "совместность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5b01c95-3b94-3fa5-8aeb-f1a12889114a",
                    "sense_definition": "Door middel van; gebruikmakend van een voorwerp, instrument of vervoermiddel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Morgen ga ik <met> de trein naar mijn werk in Amsterdam.",
                            "target_lang_translations": {
                                "en": "Tomorrow I am going to work in Amsterdam <by> train.",
                                "ru": "Завтра я поеду на работу в Амстердам <на> поезде."
                            }
                        },
                        {
                            "text": "Je kunt de soep het beste <met> een lepel eten.",
                            "target_lang_translations": {
                                "en": "It is best to eat the soup <with> a spoon.",
                                "ru": "Суп лучше всего есть <ложкой>."
                            }
                        }
                    ],
                    "antonyms": [
                        "zonder"
                    ],
                    "common_phrases": [
                        "met de auto",
                        "met de hand",
                        "met behulp van"
                    ],
                    "target_lang_definitions": {
                        "en": "By means of; using an object, instrument, or mode of transport.",
                        "ru": "Посредством чего-либо; с использованием предмета, инструмента или транспортного средства."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "transport or means"
                            },
                            {
                                "target_lang_word": "with",
                                "target_lang_sense_clarification": "instrument"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "транспорт"
                            },
                            {
                                "target_lang_word": "с помощью",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d0cf52f-1121-3bc8-ac62-88a797d61541",
                    "sense_definition": "Op een bepaalde manier; gekenmerkt door een bepaald gevoel of een bepaalde houding.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij luisterde <met> grote aandacht naar het spannende verhaal.",
                            "target_lang_translations": {
                                "en": "He listened to the exciting story <with> great attention.",
                                "ru": "Он слушал захватывающую историю <с> большим вниманием."
                            }
                        },
                        {
                            "text": "Zij doet haar werk altijd <met> veel plezier en inzet.",
                            "target_lang_translations": {
                                "en": "She always does her work <with> great pleasure and dedication.",
                                "ru": "Она всегда делает свою работу <с> большим удовольствием и самоотдачей."
                            }
                        }
                    ],
                    "antonyms": [
                        "zonder"
                    ],
                    "common_phrases": [
                        "met plezier",
                        "met rust",
                        "met opzet"
                    ],
                    "target_lang_definitions": {
                        "en": "In a certain way; characterized by a certain feeling or attitude.",
                        "ru": "Определенным образом; с определенным чувством или отношением."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "with",
                                "target_lang_sense_clarification": "manner or attitude"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "образ действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ad2d992-256f-31a9-a7ce-fa32a30443a7",
                    "sense_definition": "Gebruikt als introductie van de eigen naam bij het opnemen van de telefoon.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "telephone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hallo, <met> Jan de Vries, kan ik iemand spreken?",
                            "target_lang_translations": {
                                "en": "Hello, <this is> Jan de Vries, can I speak to someone?",
                                "ru": "Алло, <это> Ян де Врис, могу я с кем-нибудь поговорить?"
                            }
                        },
                        {
                            "text": "Goedemiddag, u spreekt <met> de receptie van het hotel.",
                            "target_lang_translations": {
                                "en": "Good afternoon, you are speaking <with> the hotel reception.",
                                "ru": "Добрый день, вы говорите <с> администрацией отеля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "u spreekt met"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to introduce one's own name when answering the telephone.",
                        "ru": "Используется для того, чтобы представиться, отвечая на телефонный звонок."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "this is",
                                "target_lang_sense_clarification": "telephone introduction"
                            },
                            {
                                "target_lang_word": "speaking",
                                "target_lang_sense_clarification": "telephone introduction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "это",
                                "target_lang_sense_clarification": "представление по телефону"
                            },
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "в разговоре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12a7e055-be85-3de4-9752-4d3fe1ef72fd",
                    "sense_definition": "Tijdens; gedurende een periode of ter gelegenheid van een gebeurtenis.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Met> kerst zijn de meeste winkels in de stad gesloten.",
                            "target_lang_translations": {
                                "en": "<At> Christmas, most shops in the city are closed.",
                                "ru": "<На> Рождество большинство магазинов в городе закрыты."
                            }
                        },
                        {
                            "text": "Het wordt buiten <met> de dag kouder en donkerder.",
                            "target_lang_translations": {
                                "en": "It is getting colder and darker outside <by> the day.",
                                "ru": "На улице становится холоднее и темнее <с> каждым днём."
                            }
                        }
                    ],
                    "common_phrases": [
                        "met de tijd",
                        "met kerst",
                        "met vakantie"
                    ],
                    "target_lang_definitions": {
                        "en": "During; throughout a period or on the occasion of an event.",
                        "ru": "Во время; в течение периода или по случаю события."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "occasion (e.g., Christmas)"
                            },
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "progression (e.g., by the day)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "событие (напр., на Рождество)"
                            },
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "течение времени (напр., с каждым днём)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d663975c-a577-3d74-942b-fedfedd3235a",
                    "sense_definition": "Fijngemalen varkensvlees of worst, vaak gegeten op brood.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij de slager bestelde hij een broodje <met> voor de lunch.",
                            "target_lang_translations": {
                                "en": "At the butcher's he ordered a roll with <minced pork> for lunch.",
                                "ru": "У мясника он заказал на обед булочку с <фаршем>."
                            }
                        },
                        {
                            "text": "In sommige streken noemt men gehakt ook wel gewoon <met>.",
                            "target_lang_translations": {
                                "en": "In some regions, minced meat is also simply called <minced pork>.",
                                "ru": "В некоторых регионах рубленое мясо называют просто <фаршем>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "broodje met"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral gebruikt in bepaalde dialecten of spreektaal"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "informele benaming voor gehakt of metworst"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Finely minced pork or sausage, often eaten on bread.",
                        "ru": "Мелко рубленная свинина или колбаса, которую часто едят с хлебом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "minced pork",
                                "target_lang_sense_clarification": "food"
                            },
                            {
                                "target_lang_word": "minced meat",
                                "target_lang_sense_clarification": "food"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "фарш",
                                "target_lang_sense_clarification": "еда"
                            },
                            {
                                "target_lang_word": "рубленое мясо",
                                "target_lang_sense_clarification": "еда"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
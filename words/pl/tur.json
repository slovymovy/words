    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2a4603bc-5596-3e41-adc2-5cc0700288b2",
                    "sense_definition": "Forma gramatyczna (dopełniacz liczby mnogiej) rzeczownika <w>tura</w>, oznaczającego etap gry, głosowania lub cykl czynności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym turnieju szachowym przewidziano aż dziewięć <w>tur</w> dla wszystkich zawodników.",
                            "target_lang_translations": {
                                "ru": "В этом шахматном турнире предусмотрено целых девять <w>туров</w> для всех участников."
                            }
                        },
                        {
                            "text": "Podczas wyborów nie było potrzeby przeprowadzania dodatkowych <w>tur</w> głosowania.",
                            "target_lang_translations": {
                                "ru": "Во время выборов не было необходимости проводить дополнительные <w>туры</w> голосования."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного tura (тур, этап, раунд)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тур",
                                "target_lang_sense_clarification": "этап состязания"
                            },
                            {
                                "target_lang_word": "раунд",
                                "target_lang_sense_clarification": "часть игры или переговоров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "599f53e5-57ac-3273-a451-7f30d74b585e",
                    "sense_definition": "Wymarły, potężny dziki ssak z rodziny wołowatych, przodek bydła domowego, zamieszkujący dawniej puszcze Europy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ostatni <w>tur</w> padł w Puszczy Jaktorowskiej w pierwszej połowie siedemnastego wieku.",
                            "target_lang_translations": {
                                "ru": "Последний <w>тур</w> пал в Якторовской пуще в первой половине семнадцатого века."
                            }
                        },
                        {
                            "text": "Mój dziadek był silny jak <w>tur</w> i potrafił sam przenieść ciężkie kłody.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был силен как <w>тур</w> и мог в одиночку переносить тяжелые бревна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "silny jak tur",
                        "róg tura",
                        "ostatni tur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вымершее крупное дикое животное из семейства полорогих, предок домашнего скота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тур",
                                "target_lang_sense_clarification": "вымершее животное"
                            },
                            {
                                "target_lang_word": "первобытный бык",
                                "target_lang_sense_clarification": "зоологическое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64fdfafb-2254-3bf4-8778-67c8d3dc11b8",
                    "sense_definition": "Kolędnik przebrany za rogate zwierzę, biorący udział w ludowych obrzędach zimowych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "folklore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wesoły <w>tur</w> skakał po izbie, strasząc panny i domagając się datków.",
                            "target_lang_translations": {
                                "ru": "Веселый <w>тур</w> скакал по избе, пугая девушек и требуя подаяния."
                            }
                        },
                        {
                            "text": "Tradycyjny orszak kolędniczy nie mógł się obejść bez postaci takich jak diabeł, śmierć i <w>tur</w>.",
                            "target_lang_translations": {
                                "ru": "Традиционное шествие колядующих не могло обойтись без таких персонажей, как черт, смерть и <w>тур</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "turoń"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "folklore"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ряженый в маске быка, участвующий в народных зимних обрядах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туронь",
                                "target_lang_sense_clarification": "фольклорный персонаж"
                            },
                            {
                                "target_lang_word": "тур",
                                "target_lang_sense_clarification": "маска ряженого"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "588db5e3-4dd5-3b63-af1b-03b1f5439fde",
                    "sense_definition": "Polskie nazwisko (męskie lub żeńskie).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Tur</w> jest znanym specjalistą w dziedzinie historii średniowiecza.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Тур</w> является известным специалистом в области истории средневековья."
                            }
                        },
                        {
                            "text": "Na liście obecności brakuje dzisiaj tylko ucznia o nazwisku <w>Tur</w>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих сегодня не хватает только ученика по фамилии <w>Тур</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тур",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b4387c4-79d9-33fa-a1e6-e7b89a788d44",
                    "sense_definition": "Marka polskich pojazdów opancerzonych lub typ statków rzecznych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "brand",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "Wojsko Polskie testowało nowy pojazd opancerzony typu <w>Tur</w> na poligonie.",
                            "target_lang_translations": {
                                "ru": "Польская армия испытывала на полигоне новый бронеавтомобиль типа <w>Тур</w>."
                            }
                        },
                        {
                            "text": "Stocznia rzeczna wyprodukowała kolejny pchacz typu <w>Tur</w> do transportu barek.",
                            "target_lang_translations": {
                                "ru": "Речная верфь выпустила очередной толкач типа <w>Тур</w> для транспортировки барж."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Марка польской военной техники или речных судов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тур",
                                "target_lang_sense_clarification": "название марки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "954a5b8a-3000-3fd7-b9c8-b8e3c2b9ff19",
                    "sense_definition": "Nazwa geograficzna odnosząca się do kilku wsi lub rzek w Polsce i Europie Wschodniej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geo",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mała rzeka <w>Tur</w> wije się malowniczo wśród łąk i wpada do Biebrzy.",
                            "target_lang_translations": {
                                "ru": "Маленькая река <w>Тур</w> живописно вьется среди лугов и впадает в Бебжу."
                            }
                        },
                        {
                            "text": "Moi przodkowie pochodzili z miejscowości <w>Tur</w> położonej w województwie łódzkim.",
                            "target_lang_translations": {
                                "ru": "Мои предки были родом из местности <w>Тур</w>, расположенной в Лодзинском воеводстве."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Географическое название (река или населенный пункт)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тур",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e86468d4-813a-34bd-a106-52f3aa1fd24a",
                    "sense_definition": "Wyraz dźwiękonaśladowczy imitujący terkot, stukot lub hałas mechaniczny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silnik starego traktora robił tylko <w>tur</w>, tur, tur i nagle zgasł.",
                            "target_lang_translations": {
                                "ru": "Двигатель старого трактора делал только <w>тыр</w>, тыр, тыр и внезапно заглох."
                            }
                        },
                        {
                            "text": "Z oddali słychać było rytmiczne <w>tur</w> kół pociągu jadącego po szynach.",
                            "target_lang_translations": {
                                "ru": "Издали слышался ритмичный <w>стук</w> колес поезда, идущего по рельсам."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Звукоподражание, имитирующее шум мотора или стук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тыр-тыр",
                                "target_lang_sense_clarification": "звук мотора"
                            },
                            {
                                "target_lang_word": "тук-тук",
                                "target_lang_sense_clarification": "ритмичный звук"
                            }
                        ]
                    }
                }
            ]
        }
    ]

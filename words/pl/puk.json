{
    "word_family": [
        "pukać",
        "zapukać",
        "odpukać",
        "pukawka"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "c80afde0-8c5a-370b-9e6a-c818e1ba6553",
                    "sense_definition": "Wykrzyknik naśladujący odgłos pukania, używany często dla zasygnalizowania czyjejś obecności pod drzwiami lub w celu zwrócenia uwagi.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Puk</w>, <w>puk</w>! Czy mogę wejść do środka?",
                            "target_lang_translations": {
                                "ru": "<w>Тук-тук</w>! Можно войти?"
                            }
                        },
                        {
                            "text": "Dziecko zawołało głośno „<w>puk</w>, <w>puk</w>” i wbiegło do pokoju.",
                            "target_lang_translations": {
                                "ru": "Ребенок громко крикнул «<w>тук-тук</w>» и вбежал в комнату."
                            }
                        }
                    ],
                    "common_phrases": [
                        "puk-puk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, имитирующее звук стука, часто используется, чтобы обозначить чье-то присутствие за дверью или привлечь внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тук-тук",
                                "target_lang_sense_clarification": "звукоподражание стуку"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9ca8d34-899f-33b2-89e1-47c3708c00bb",
                    "sense_definition": "Krótki, głuchy odgłos uderzenia, najczęściej w twardą powierzchnię.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagle usłyszeliśmy głuchy <w>puk</w> dochodzący z poddasza.",
                            "target_lang_translations": {
                                "ru": "Вдруг мы услышали глухой <w>стук</w>, доносящийся с чердака."
                            }
                        },
                        {
                            "text": "Jeden głośny <w>puk</w> w okno wystarczył, by nas obudzić.",
                            "target_lang_translations": {
                                "ru": "Одного громкого <w>стука</w> в окно хватило, чтобы нас разбудить."
                            }
                        }
                    ],
                    "synonyms": [
                        "pukanie",
                        "stukanie"
                    ],
                    "common_phrases": [
                        "głośny puk",
                        "pojedynczy puk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий глухой звук удара, чаще всего о твердую поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стук",
                                "target_lang_sense_clarification": "звук удара"
                            },
                            {
                                "target_lang_word": "тук",
                                "target_lang_sense_clarification": "короткий звук"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "913c575a-5d5c-3d14-bac1-6586ba9d8a49",
                    "sense_definition": "Postać z mitologii celtyckiej i folkloru angielskiego, psotny duszek leśny.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mythology_astro",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Szekspirowski <w>Puk</w> to postać pełna humoru i magii.",
                            "target_lang_translations": {
                                "ru": "Шекспировский <w>Пак</w> — это персонаж, полный юмора и магии."
                            }
                        },
                        {
                            "text": "W dawnych wierzeniach <w>Puk</w> mógł sprowadzać wędrowców na manowce.",
                            "target_lang_translations": {
                                "ru": "В древних поверьях <w>Пак</w> мог сбивать путников с пути."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Персонаж кельтской мифологии и английского фольклора, озорной лесной дух."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пак",
                                "target_lang_sense_clarification": "мифологический персонаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e91c94bb-7100-3b06-8939-c59d7ec9b3e5",
                    "sense_definition": "Jeden z wewnętrznych księżyców Urana.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mythology_astro",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Sonda kosmiczna przesłała zdjęcia, na których widoczny jest <w>Puk</w>.",
                            "target_lang_translations": {
                                "ru": "Космический зонд передал снимки, на которых виден <w>Пак</w>."
                            }
                        },
                        {
                            "text": "<w>Puk</w> krąży wokół Urana w niewielkiej odległości od planety.",
                            "target_lang_translations": {
                                "ru": "<w>Пак</w> вращается вокруг Урана на небольшом расстоянии от планеты."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из внутренних спутников Урана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пак",
                                "target_lang_sense_clarification": "спутник Урана"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
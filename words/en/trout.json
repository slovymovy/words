            "senses": [
                {
                    "sense_id": "832d9e97-4077-3d0d-8c6e-cf4b52a0ebd2",
                    "sense_definition": "Any of several species of fish in the salmon family, closely related to salmon, and distinguished by spawning more than once.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many anglers consider <w>trout</w> to be the perfect catch for a weekend trip.",
                            "target_lang_translations": {
                                "ru": "Многие рыболовы считают, что <w>форель</w> — идеальный улов для поездки на выходные."
                            }
                        },
                        {
                            "text": "We will cast for <w>trout</w> in the river early tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Завтра рано утром мы будем ловить <w>форель</w> в реке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rainbow trout",
                        "brown trout",
                        "trout fishing",
                        "smoked trout",
                        "sea trout"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общее название нескольких видов рыб семейства лососевых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "форель",
                                "target_lang_sense_clarification": "рыба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "505b5e9c-10cb-3dad-9be4-a9cdc501e622",
                    "sense_definition": "An objectionable or unpleasant elderly woman.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Look, you silly old <w>trout</w>, you cannot keep bringing home stray cats!",
                            "target_lang_translations": {
                                "ru": "Послушай, ты, глупая старая <w>карга</w>, ты не можешь продолжать таскать домой бездомных кошек!"
                            }
                        },
                        {
                            "text": "He muttered that his neighbor was a miserable old <w>trout</w> after she complained about the noise.",
                            "target_lang_translations": {
                                "ru": "Он пробормотал, что его соседка — жалкая старая <w>грымза</w>, после того как она пожаловалась на шум."
                            }
                        }
                    ],
                    "common_phrases": [
                        "old trout"

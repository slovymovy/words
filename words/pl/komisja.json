                        "komisja egzaminacyjna",
                        "komisja śledcza",
                        "powołać komisję",
                        "komisja wojskowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, выбранная или назначенная для выполнения определенных задач, контроля или расследования какого-либо дела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комиссия",
                                "target_lang_sense_clarification": "группа людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92f1961c-bf16-3d75-a0fa-bd29d948e73c",
                    "sense_definition": "Posiedzenie lub spotkanie grupy osób powołanej do pełnienia określonych funkcji.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę stawić się na <w>komisję</w> w przyszłym tygodniu.",
                            "target_lang_translations": {

                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "drop downwards"
                            },
                            {
                                "target_lang_word": "go flying",
                                "target_lang_sense_clarification": "be thrown through the air"
                            }

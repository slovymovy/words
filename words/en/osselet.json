    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f364d20c-071a-3624-8ecb-f40356b04bb5",
                    "sense_definition": "A small bone.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The archaeologist carefully brushed away the dirt to reveal a tiny <w>osselet</w>.",
                            "target_lang_translations": {
                                "ru": "Археолог осторожно смахнул грязь, обнаружив крошечную <w>косточку</w>."
                            }
                        },
                        {
                            "text": "Detailed diagrams showed every <w>osselet</w> located within the joint structure.",
                            "target_lang_translations": {
                                "ru": "Подробные схемы показывали каждую <w>косточку</w>, находящуюся внутри структуры сустава."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькая кость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "косточка",
                                "target_lang_sense_clarification": "маленькая кость"
                            },
                            {
                                "target_lang_word": "мелкая кость",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6db7de02-ed1c-35cc-bd24-dd937fd1099c",
                    "sense_definition": "The internal bone or shell of a cuttlefish.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We found a white, chalky <w>osselet</w> washed up on the beach after the storm.",
                            "target_lang_translations": {
                                "ru": "Мы нашли белую известковую <w>кость каракатицы</w>, выброшенную на берег после шторма."
                            }
                        },
                        {
                            "text": "The <w>osselet</w> provides the creature with structural support and buoyancy control.",
                            "target_lang_translations": {
                                "ru": "<w>Кость каракатицы</w> обеспечивает существу структурную поддержку и контроль плавучести."
                            }
                        }
                    ],
                    "synonyms": [
                        "cuttlebone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя кость или раковина каракатицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кость каракатицы",
                                "target_lang_sense_clarification": "внутренняя раковина моллюска"
                            },
                            {
                                "target_lang_word": "сепион",
                                "target_lang_sense_clarification": "научный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9ac0b59-5d19-38c3-bc18-95b93112aadf",
                    "sense_definition": "A hard bony growth or substance on the inside of a horse's knee or fetlock.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The veterinarian examined the horse's leg and diagnosed a developing <w>osselet</w>.",
                            "target_lang_translations": {
                                "ru": "Ветеринар осмотрел ногу лошади и диагностировал развивающийся <w>костный нарост</w>."
                            }
                        },
                        {
                            "text": "Chronic stress on the joint can cause an <w>osselet</w> to form, leading to lameness.",
                            "target_lang_translations": {
                                "ru": "Хроническая нагрузка на сустав может вызвать образование <w>костного нароста</w>, что приведет к хромоте."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый костный нарост на внутренней стороне колена или путового сустава лошади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "костный нарост",
                                "target_lang_sense_clarification": "патологическое образование"
                            },
                            {
                                "target_lang_word": "экзостоз",
                                "target_lang_sense_clarification": "ветеринарный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
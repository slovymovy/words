            "senses": [
                {
                    "sense_id": "974af7a3-a9e1-3463-bcda-6c9677249c56",
                    "sense_definition": "Słowo używane dla podkreślenia, że dana czynność w ogóle nie zachodzi lub cecha nie występuje; wcale.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film mnie <w>nic</w> nie obchodzi.",
                            "target_lang_translations": {
                                "ru": "Этот фильм меня <w>совсем</w> не интересует."
                            }
                        },
                        {
                            "text": "Mimo upływu lat on <w>nic</w> się nie zmienił.",
                            "target_lang_translations": {
                                "ru": "Несмотря на прошедшие годы, он <w>нисколько</w> не изменился."
                            }

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e10971de-fe39-3ad7-9475-c3d6293eade2",
                    "sense_definition": "Раскатистый сильный звук в небе, сопровождающий разряд молнии во время грозы.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound_phenomenon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вдали послышались глухие раскаты <w>грома</w>, предвещающие скорый дождь.",
                            "target_lang_translations": {
                                "en": "Dull peals of <w>thunder</w> were heard in the distance, foreshadowing imminent rain."
                            }
                        },
                        {
                            "text": "Вдруг сверкнула яркая молния, и сразу же загремел сильный <w>гром</w>.",
                            "target_lang_translations": {
                                "en": "Suddenly bright lightning flashed, and immediately loud <w>thunder</w> crashed."
                            }
                        }
                    ],
                    "synonyms": [
                        "грохот"
                    ],
                    "antonyms": [
                        "тишина"
                    ],
                    "common_phrases": [
                        "раскаты грома",
                        "грянул гром",
                        "гром и молния",
                        "ударил гром"
                    ],
                    "target_lang_definitions": {
                        "en": "A rolling, loud sound in the sky accompanying a lightning discharge during a thunderstorm."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thunder",
                                "target_lang_sense_clarification": "atmospheric sound"
                            },
                            {
                                "target_lang_word": "thunderclap",
                                "target_lang_sense_clarification": "single loud crash of thunder"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1780bad-61f9-3eaf-b5d3-468e423b5fde",
                    "sense_definition": "Очень сильный шум или грохот, производимый чем-либо (выстрелами, аплодисментами, техникой).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_phenomenon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После выступления артиста в зале раздался <w>гром</w> аплодисментов.",
                            "target_lang_translations": {
                                "en": "After the artist's performance, a <w>thunder</w> of applause broke out in the hall."
                            }
                        },
                        {
                            "text": "С улицы доносился непрерывный <w>гром</w> проезжающих тяжелых грузовиков.",
                            "target_lang_translations": {
                                "en": "The continuous <w>thunder</w> of passing heavy trucks could be heard from the street."
                            }
                        }
                    ],
                    "synonyms": [
                        "грохот",
                        "громыхание"
                    ],
                    "antonyms": [
                        "тишина"
                    ],
                    "common_phrases": [
                        "гром аплодисментов",
                        "гром орудий",
                        "гром музыки"
                    ],
                    "target_lang_definitions": {
                        "en": "A very loud noise or roar produced by something (shots, applause, machinery)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thunder",
                                "target_lang_sense_clarification": "loud rumbling noise"
                            },
                            {
                                "target_lang_word": "roar",
                                "target_lang_sense_clarification": "deep prolonged sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efbb981f-487f-3fe8-b276-135e778ef8e9",
                    "sense_definition": "Разряд молнии, который ударяет в землю или объект (обычно вызывая разрушения).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "electric_discharge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старое дерево на высоком холме разбило <w>громом</w> во время ночной бури.",
                            "target_lang_translations": {
                                "en": "The old tree on the high hill was shattered by a <w>thunderbolt</w> during the night storm."
                            }
                        },
                        {
                            "text": "В деревне говорили, что этот дом загорелся, потому что в него ударил <w>гром</w>.",
                            "target_lang_translations": {
                                "en": "People in the village said that this house caught fire because <w>lightning</w> struck it."
                            }
                        }
                    ],
                    "synonyms": [
                        "молния",
                        "грозовой разряд"
                    ],
                    "common_phrases": [
                        "убило громом",
                        "разбило громом"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to refer to the lightning strike itself rather than just the sound"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A lightning discharge that strikes the ground or an object (usually causing destruction)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thunderbolt",
                                "target_lang_sense_clarification": "destructive lightning strike"
                            },
                            {
                                "target_lang_word": "lightning",
                                "target_lang_sense_clarification": "electrical discharge"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95dd03b5-3cca-3e3b-8c3a-cd70bd40d7c7",
                    "sense_definition": "Трамвай (на уголовном жаргоне).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный карманник незаметно запрыгнул в <w>гром</w> и скрылся от преследования.",
                            "target_lang_translations": {
                                "en": "The experienced pickpocket unnoticedly jumped into the <w>tram</w> and escaped pursuit."
                            }
                        },
                        {
                            "text": "На старом жаргоне городской <w>гром</w> означал обычный трамвай.",
                            "target_lang_translations": {
                                "en": "In old jargon, the city <w>tram</w> meant a regular streetcar."
                            }
                        }
                    ],
                    "synonyms": [
                        "трамвай"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "criminal jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Tram (in criminal slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tram",
                                "target_lang_sense_clarification": "public transport vehicle"
                            },
                            {
                                "target_lang_word": "streetcar",
                                "target_lang_sense_clarification": "US term for tram"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
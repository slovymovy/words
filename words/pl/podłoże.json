    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a1c16e60-b7b3-35d4-820c-30df04d84ff7",
                    "sense_definition": "Warstwa naturalna lub sztuczna, na której coś się znajduje, jest zbudowane lub po której się porusza.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_base",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biegacz poślizgnął się na mokrym i śliskim <w>podłożu</w>.",
                            "target_lang_translations": {
                                "ru": "Бегун поскользнулся на мокрой и скользкой <w>поверхности</w>."
                            }
                        },
                        {
                            "text": "Ten budynek musi zostać postawiony na stabilnym i twardym <w>podłożu</w>.",
                            "target_lang_translations": {
                                "ru": "Это здание должно быть построено на устойчивом и твердом <w>грунте</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "grunt",
                        "ziemia"
                    ],
                    "common_phrases": [
                        "twarde podłoże",
                        "śliskie podłoże",
                        "stabilne podłoże",
                        "skaliste podłoże"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественный или искусственный слой, на котором что-либо находится, строится или по которому происходит движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грунт",
                                "target_lang_sense_clarification": "земля, почва как основание"
                            },
                            {
                                "target_lang_word": "поверхность",
                                "target_lang_sense_clarification": "верхний слой чего-либо"
                            },
                            {
                                "target_lang_word": "основание",
                                "target_lang_sense_clarification": "фундамент, база"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b561f79e-ab07-3633-9d0c-7853422788a0",
                    "sense_definition": "Specjalnie przygotowana ziemia lub inny materiał służący do uprawy roślin.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gardening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem w sklepie ogrodniczym specjalne <w>podłoże</w> do storczyków.",
                            "target_lang_translations": {
                                "ru": "Я купил в садовом магазине специальный <w>грунт</w> для орхидей."
                            }
                        },
                        {
                            "text": "Rośliny te najlepiej rosną, gdy <w>podłoże</w> jest stale wilgotne.",
                            "target_lang_translations": {
                                "ru": "Эти растения лучше всего растут, когда <w>грунт</w> постоянно влажный."
                            }
                        }
                    ],
                    "synonyms": [
                        "ziemia",
                        "gleba"
                    ],
                    "common_phrases": [
                        "podłoże do kwiatów",
                        "podłoże torfowe",
                        "wilgotne podłoże"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специально подготовленная земля или другой материал для выращивания растений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грунт",
                                "target_lang_sense_clarification": "земля для растений"
                            },
                            {
                                "target_lang_word": "субстрат",
                                "target_lang_sense_clarification": "питательная смесь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3cc8b14-bec5-3349-804b-b0cb2c8945fa",
                    "sense_definition": "Zespół czynników, okoliczności lub przyczyn stanowiących podstawę jakiegoś zjawiska lub wydarzenia.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_basis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele konfliktów w tym regionie ma <w>podłoże</w> ekonomiczne.",
                            "target_lang_translations": {
                                "ru": "Многие конфликты в этом регионе имеют экономическую <w>подоплёку</w>."
                            }
                        },
                        {
                            "text": "Lekarz podejrzewa, że choroba pacjenta może mieć <w>podłoże</w> genetyczne.",
                            "target_lang_translations": {
                                "ru": "Врач подозревает, что болезнь пациента может иметь генетическую <w>основу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na podłożu nerwowym",
                        "podłoże konfliktu",
                        "podłoże historyczne",
                        "mieć podłoże"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность факторов, обстоятельств или причин, лежащих в основе какого-либо явления или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почва",
                                "target_lang_sense_clarification": "переносное значение: основание, причина"
                            },
                            {
                                "target_lang_word": "подоплёка",
                                "target_lang_sense_clarification": "скрытая причина, сущность"
                            },
                            {
                                "target_lang_word": "основа",
                                "target_lang_sense_clarification": "база, фундамент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb16fc22-6632-3b90-87dc-ada9de37b1e9",
                    "sense_definition": "Substancja zawierająca składniki odżywcze, wykorzystywana w laboratoriach do hodowli bakterii, grzybów lub tkanek.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "scientific_medium",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Laborant przygotował <w>podłoże</w> agarowe, aby wyhodować próbki bakterii.",
                            "target_lang_translations": {
                                "ru": "Лаборант подготовил агаровую <w>среду</w>, чтобы вырастить образцы бактерий."
                            }
                        },
                        {
                            "text": "Drobnoustroje bardzo szybko namnażają się na tym bogatym w cukry <w>podłożu</w>.",
                            "target_lang_translations": {
                                "ru": "Микробы очень быстро размножаются на этой богатой сахарами <w>среде</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pożywka"
                    ],
                    "common_phrases": [
                        "podłoże bakteryjne",
                        "podłoże hodowlane",
                        "wzrost na podłożu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество, содержащее питательные элементы, используемое в лабораториях для выращивания бактерий, грибов или тканей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "среда",
                                "target_lang_sense_clarification": "питательная среда"
                            }
                        ]
                    }
                }
            ]
        }

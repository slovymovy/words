                    "sense_id": "1fae8d86-e6b2-33db-9c6d-af400c493345",
                    "sense_definition": "Wzrost temperatury powietrza lub atmosfery; nadejście cieplejszej pogody.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Globalne <w>ocieplenie</w> jest jednym z najważniejszych wyzwań współczesnego świata.",
                            "target_lang_translations": {
                                "ru": "Глобальное <w>потепление</w> является одним из важнейших вызовов современного мира."
                            }
                        },
                        {
                            "text": "Po długiej i mroźnej zimie wreszcie nadeszło zapowiadane <w>ocieplenie</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой и морозной зимы наконец наступило обещанное <w>потепление</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "odwilż"
                    ],
                    "antonyms": [
                        "ochłodzenie",
                        "oziębienie"
                    ],
                    "common_phrases": [
                        "globalne ocieplenie",
                        "strefa ocieplenia",
                        "gwałtowne ocieplenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повышение температуры воздуха или атмосферы; наступление более теплой погоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потепление",
                                "target_lang_sense_clarification": "погода, климат"

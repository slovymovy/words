    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a408dafe-c8a5-3b65-b2f2-3d9381a31275",
                    "sense_definition": "That operates to produce an effect; functioning, effective, or active.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The psychologist explained the principles of <w>operant</w> conditioning to the students.",
                            "target_lang_translations": {
                                "ru": "Психолог объяснил студентам принципы <w>оперантного</w> обусловливания."
                            }
                        },
                        {
                            "text": "We need to find an <w>operant</w> solution that actively resolves the issue.",
                            "target_lang_translations": {
                                "ru": "Нам нужно найти <w>действенное</w> решение, которое активно устранит проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "operative"
                    ],
                    "antonyms": [
                        "inoperant"
                    ],
                    "common_phrases": [
                        "operant conditioning",
                        "operant behavior"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действующий с целью произвести эффект; функционирующий, эффективный или активный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оперантный",
                                "target_lang_sense_clarification": "в психологии (оперантное обусловливание)"
                            },
                            {
                                "target_lang_word": "действующий",
                                "target_lang_sense_clarification": "функционирующий или активный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "880c6ff4-b24e-3c6b-8bf7-6d3eff8c1770",
                    "sense_definition": "Any of a class of behaviors that produce consequences by operating or acting upon the environment.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the experiment, pressing the lever was the <w>operant</w> that resulted in a reward.",
                            "target_lang_translations": {
                                "ru": "В эксперименте нажатие на рычаг было тем <w>оперантом</w>, который приводил к получению вознаграждения."
                            }
                        },
                        {
                            "text": "Skinner defined a verbal <w>operant</w> as a unit of language behavior.",
                            "target_lang_translations": {
                                "ru": "Скиннер определил вербальный <w>оперант</w> как единицу речевого поведения."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой из класса поведенческих актов, которые вызывают последствия путем воздействия на окружающую среду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оперант",
                                "target_lang_sense_clarification": "единица поведения в психологии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a03ad13c-346d-39cd-8261-7718743dd7e2",
                    "sense_definition": "An operative person or thing that performs a function.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was a key <w>operant</w> in the complex machinery of the organization.",
                            "target_lang_translations": {
                                "ru": "Он был ключевой <w>действующей силой</w> в сложном механизме организации."
                            }
                        },
                        {
                            "text": "The chemical acts as an <w>operant</w> to speed up the reaction.",
                            "target_lang_translations": {
                                "ru": "Химическое вещество действует как <w>агент</w> для ускорения реакции."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действующее лицо или объект, выполняющий определенную функцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "действующая сила",
                                "target_lang_sense_clarification": "активный участник процесса"
                            },
                            {
                                "target_lang_word": "агент",
                                "target_lang_sense_clarification": "вещество или фактор, вызывающий действие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

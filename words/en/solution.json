    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4837730f-c151-3416-a440-0d4a69e8019a",
                    "sense_definition": "An answer to a problem or a way of dealing with a difficult situation.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_resolution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to find a <w>solution</w> to this problem before it gets worse.",
                            "target_lang_translations": {
                                "ru": "Нам нужно найти <w>решение</w> этой проблемы, пока она не усугубилась."
                            }
                        },
                        {
                            "text": "Talking to a friend was the best <w>solution</w> for her stress.",
                            "target_lang_translations": {
                                "ru": "Разговор с другом стал лучшим <w>решением</w> для снятия её стресса."
                            }
                        }
                    ],
                    "synonyms": [
                        "resolution"
                    ],
                    "antonyms": [
                        "problem",
                        "challenge",
                        "issue"
                    ],
                    "common_phrases": [
                        "find a solution",
                        "perfect solution",
                        "solution to a problem",
                        "possible solution"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ответ на проблему или способ справиться с трудной ситуацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реше́ние",
                                "target_lang_sense_clarification": "проблемы, задачи"
                            },
                            {
                                "target_lang_word": "вы́ход",
                                "target_lang_sense_clarification": "из трудной ситуации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b319da3-1b1b-3fb4-aa1f-c877c8189764",
                    "sense_definition": "A liquid mixture in which a solid or gas is distributed evenly within a liquid.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nurse cleaned the wound with a saline <w>solution</w>.",
                            "target_lang_translations": {
                                "ru": "Медсестра промыла рану солевым <w>раствором</w>."
                            }
                        },
                        {
                            "text": "Stir the sugar into the water until you have a clear <w>solution</w>.",
                            "target_lang_translations": {
                                "ru": "Размешивайте сахар в воде, пока не получите прозрачный <w>раствор</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "saline solution",
                        "chemical solution",
                        "aqueous solution",
                        "in solution"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкая смесь, в которой твердое вещество или газ равномерно распределены в жидкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раство́р",
                                "target_lang_sense_clarification": "жидкая смесь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3db76900-831a-3fef-8bbf-6db61b137d15",
                    "sense_definition": "A specific product or service designed to meet a business need, especially in software.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_resolution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company provides a comprehensive <w>solution</w> for data security.",
                            "target_lang_translations": {
                                "ru": "Компания предоставляет комплексное <w>решение</w> для защиты данных."
                            }
                        },
                        {
                            "text": "We are looking for a cost-effective <w>solution</w> to manage our inventory.",
                            "target_lang_translations": {
                                "ru": "Мы ищем экономически выгодное <w>решение</w> для управления нашими запасами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "software solution",
                        "business solution",
                        "IT solution"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный продукт или услуга, разработанные для удовлетворения потребностей бизнеса, особенно в сфере программного обеспечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реше́ние",
                                "target_lang_sense_clarification": "продукт или услуга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7df1a489-b3ab-3f2f-90e2-49e68b653e5b",
                    "sense_definition": "The process of a substance dissolving in a liquid.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_chemistry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heating the water will speed up the <w>solution</w> of the salt.",
                            "target_lang_translations": {
                                "ru": "Нагревание воды ускорит <w>растворение</w> соли."
                            }
                        },
                        {
                            "text": "The rapid <w>solution</w> of the tablet provided quick relief.",
                            "target_lang_translations": {
                                "ru": "Быстрое <w>растворение</w> таблетки принесло быстрое облегчение."
                            }
                        }
                    ],
                    "synonyms": [
                        "dissolution"
                    ],
                    "antonyms": [
                        "precipitation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс растворения вещества в жидкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растворе́ние",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f427d4f0-f74c-39dd-88d7-95d618f9e349",
                    "sense_definition": "To treat or coat something with a liquid rubber or chemical mixture.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic <w>solutioned</w> the patch onto the tire to ensure it stuck firmly.",
                            "target_lang_translations": {
                                "ru": "Механик <w>промазал раствором</w> заплатку на шину, чтобы она крепко приклеилась."
                            }
                        },
                        {
                            "text": "The fabric was <w>solutioned</w> to make it resistant to water.",
                            "target_lang_translations": {
                                "ru": "Ткань была <w>прорезинена</w>, чтобы сделать её водонепроницаемой."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обрабатывать или покрывать что-либо жидкой резиной или химической смесью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прорези́нивать",
                                "target_lang_sense_clarification": "покрывать резиной"
                            },
                            {
                                "target_lang_word": "покрыва́ть раство́ром",
                                "target_lang_sense_clarification": "наносить химическую смесь"
                            }
                        ]
                    }
                }
            ]
        }
    ]

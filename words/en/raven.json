            "senses": [
                {
                    "sense_id": "39df9db5-4c10-34de-a250-a713504ef6e3",
                    "sense_definition": "Having the glossy black color of a raven; jet-black.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "color_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The princess was famous for her pale skin and long <w>raven</w> hair.",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "25c19d59-3321-3c3c-b4b2-391a5972e53c",
                    "sense_definition": "Arbeid verrichten, een taak uitvoeren (vaak in loondienst of voor een specifiek doel)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "labor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader <werkt> vijf dagen per week op een kantoor in de stad.",
                            "target_lang_translations": {
                                "en": "My father <works> five days a week at an office in the city.",
                                "ru": "Мой отец <работает> пять дней в неделю в офисе в городе."
                            }
                        },
                        {
                            "text": "Wij moeten hard <werken> om het project op tijd af te krijgen.",
                            "target_lang_translations": {
                                "en": "We have to <work> hard to finish the project on time.",
                                "ru": "Мы должны усердно <работать>, чтобы закончить проект вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "arbeiden"
                    ],
                    "antonyms": [
                        "luieren"
                    ],
                    "common_phrases": [
                        "hard werken",
                        "gaan werken",
                        "samen werken"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform labor or carry out a task, often as part of a job or for a specific goal",
                        "ru": "Выполнять работу, трудиться (часто по найму или с определенной целью)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "to perform a job or task"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "работать",
                                "target_lang_sense_clarification": "выполнять работу или задачу"
                            },
                            {
                                "target_lang_word": "трудиться",
                                "target_lang_sense_clarification": "усердно работать, прилагать усилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb63f4f6-4f1f-36e0-b783-74311dcc310d",
                    "sense_definition": "Functioneren, in bedrijf zijn of het doen (van apparaten, machines of systemen)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude wasmachine <werkt> nog steeds prima ondanks zijn leeftijd.",
                            "target_lang_translations": {
                                "en": "The old washing machine still <works> fine despite its age.",
                                "ru": "Старая стиральная машина всё ещё отлично <работает>, несмотря на свой возраст."
                            }
                        },
                        {
                            "text": "Ik kan je niet bellen omdat mijn telefoon vandaag niet goed <werkt>.",
                            "target_lang_translations": {
                                "en": "I cannot call you because my phone is not <working> well today.",
                                "ru": "Я не могу тебе позвонить, потому что мой телефон сегодня плохо <работает>."
                            }
                        }
                    ],
                    "synonyms": [
                        "functioneren",
                        "het doen",
                        "zwoegen"
                    ],
                    "common_phrases": [
                        "goed werken",
                        "niet werken"
                    ],
                    "target_lang_definitions": {
                        "en": "To function or operate properly (of devices, machines, or systems)",
                        "ru": "Функционировать, работать исправно (об устройствах, машинах или системах)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "to operate properly"
                            },
                            {
                                "target_lang_word": "function",
                                "target_lang_sense_clarification": "to perform its function"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "работать",
                                "target_lang_sense_clarification": "быть в рабочем состоянии"
                            },
                            {
                                "target_lang_word": "функционировать",
                                "target_lang_sense_clarification": "выполнять свою функцию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ab87587-1ddd-31f5-80be-9283ac521277",
                    "sense_definition": "Het gewenste effect hebben of een gunstig resultaat opleveren",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit medicijn <werkt> meestal heel snel tegen zware hoofdpijn.",
                            "target_lang_translations": {
                                "en": "This medicine usually <works> very quickly against severe headaches.",
                                "ru": "Это лекарство обычно очень быстро <действует> против сильной головной боли."
                            }
                        },
                        {
                            "text": "Jouw plan om geld te besparen <werkt> helaas niet in de praktijk.",
                            "target_lang_translations": {
                                "en": "Unfortunately, your plan to save money does not <work> in practice.",
                                "ru": "Твой план по экономии денег, к сожалению, не <работает> на практике."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dat werkt"
                    ],
                    "target_lang_definitions": {
                        "en": "To have the desired effect or yield a favorable result",
                        "ru": "Иметь желаемый эффект или приносить благоприятный результат"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "to be effective or successful"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "действовать",
                                "target_lang_sense_clarification": "оказывать воздействие (о лекарствах, методах)"
                            },
                            {
                                "target_lang_word": "работать",
                                "target_lang_sense_clarification": "быть эффективным (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "969cc26b-b4c1-31dd-9b25-e73d2c96d5f2",
                    "sense_definition": "Vervormen, uitzetten of krimpen door invloeden van buitenaf (zoals vocht of temperatuur), vaak gezegd van hout",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de hoge luchtvochtigheid begon de houten vloer te <werken>.",
                            "target_lang_translations": {
                                "en": "Due to the high humidity, the wooden floor began to <warp>.",
                                "ru": "Из-за высокой влажности деревянный пол начал <коробиться>."
                            }
                        },
                        {
                            "text": "Je moet oppassen dat de planken niet gaan <werken> als het weer omslaat.",
                            "target_lang_translations": {
                                "en": "You must be careful that the planks do not start to <warp> when the weather changes.",
                                "ru": "Нужно следить, чтобы доски не начали <коробиться> при смене погоды."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To deform, expand, or shrink due to external influences like moisture (often said of wood)",
                        "ru": "Деформироваться, менять форму под воздействием влаги или температуры (часто о дереве)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "warp",
                                "target_lang_sense_clarification": "to change shape (of material)"
                            },
                            {
                                "target_lang_word": "work",
                                "target_lang_sense_clarification": "to move or shift (technical)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "коробиться",
                                "target_lang_sense_clarification": "искривляться (о материале)"
                            },
                            {
                                "target_lang_word": "деформироваться",
                                "target_lang_sense_clarification": "менять форму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b570bdf-2c73-3af6-b340-fefa729c3163",
                    "sense_definition": "Iets tot stand brengen of bewerkstelligen (vaak gebruikt met archaïsche vervoegingen)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "create",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kunstenaar <wrocht> jarenlang aan zijn meesterwerk in de kathedraal.",
                            "target_lang_translations": {
                                "en": "The artist <worked> for years on his masterpiece in the cathedral.",
                                "ru": "Художник годами <трудился> над своим шедевром в соборе."
                            }
                        },
                        {
                            "text": "Hij heeft in zijn leven veel goeds <gewrocht> voor de arme bevolking.",
                            "target_lang_translations": {
                                "en": "He has <wrought> much good for the poor population in his life.",
                                "ru": "За свою жизнь он <сотворил> много добра для бедного населения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "often uses forms like 'wrocht' and 'gewrocht'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To bring about, create, or accomplish something (often used in archaic or literary contexts)",
                        "ru": "Создавать, творить или осуществлять что-либо (часто используется в архаичном или литературном контексте)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring about",
                                "target_lang_sense_clarification": "to cause to happen"
                            },
                            {
                                "target_lang_word": "wrought",
                                "target_lang_sense_clarification": "archaic past form: created or caused"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "творить",
                                "target_lang_sense_clarification": "создавать (высокий стиль)"
                            },
                            {
                                "target_lang_word": "созидать",
                                "target_lang_sense_clarification": "строить, создавать"
                            }
                        ]
                    }
                }
            ]
        },

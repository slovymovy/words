            "senses": [
                {
                    "sense_id": "4c148e89-ce14-3aae-aaec-9e5c65fdeee1",
                    "sense_definition": "Niet meer dan een bepaalde hoeveelheid of aantal; geeft een beperking aan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "limitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De emmer bevat <w>slechts</w> een paar erwten.",
                            "target_lang_translations": {
                                "en": "The bucket contains <w>only</w> a few peas.",
                                "ru": "В ведре <w>всего</w> несколько горошин."
                            }
                        },
                        {
                            "text": "Er waren <w>slechts</w> vier mensen aanwezig bij de vergadering.",
                            "target_lang_translations": {
                                "en": "There were <w>only</w> four people present at the meeting.",
                                "ru": "На собрании присутствовало <w>всего</w> четыре человека."
                            }
                        }
                    ],
                    "synonyms": [
                        "maar",
                        "maar",
                        "maar"
                    ],
                    "common_phrases": [
                        "slechts een paar",
                        "slechts enkele",
                        "slechts weinig"
                    ],
                    "target_lang_definitions": {
                        "en": "No more than a certain amount or number; indicates a limitation.",
                        "ru": "Не более определенного количества или числа; указывает на ограничение."
                    },

                    "sense_id": "6262f17f-086e-3c7f-8ba5-25fb926b28d1",
                    "sense_definition": "Уместить печатный текст на каком-либо ограниченном пространстве.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fit_text",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Верстальщику с трудом удалось <упечатать> длинный заголовок в одну строку.",
                            "target_lang_translations": {
                                "en": "The typesetter barely managed to <squeeze> the long headline into a single line."
                            }
                        },
                        {
                            "text": "Мы попробуем <упечатать> это объявление в свободный угол страницы.",
                            "target_lang_translations": {
                                "en": "We will try to <fit> this ad into the free corner of the page."
                            }
                        }
                    ],
                    "synonyms": [
                        "уместить",
                        "втиснуть"
                    ],
                    "common_phrases": [
                        "упечатать в строку",
                        "упечатать текст"
                    ],
                    "target_lang_definitions": {
                        "en": "To fit printed text into a limited space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "squeeze in",
                                "target_lang_sense_clarification": "text into a layout"
                            },
                            {
                                "target_lang_word": "fit",
                                "target_lang_sense_clarification": "text into a specific space"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4e6083f-1b36-39a6-aece-7bbe0074b6f3",
                    "sense_definition": "Заполнить поверхность печатным текстом целиком, не оставляя свободного места.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fill_text",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Редактор решил плотно <упечатать> страницу мелким шрифтом.",
                            "target_lang_translations": {
                                "en": "The editor decided to tightly <fill> the page with small print."
                            }
                        },
                        {
                            "text": "Весь лист был <упечатан> сложными таблицами и графиками.",
                            "target_lang_translations": {
                                "en": "The entire sheet was <covered> with complex tables and graphs."
                            }
                        }
                    ],
                    "synonyms": [
                        "заполнить"
                    ],
                    "common_phrases": [
                        "упечатать страницу",
                        "упечатать лист"
                    ],
                    "target_lang_definitions": {
                        "en": "To fill a surface entirely with printed text."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fill up",
                                "target_lang_sense_clarification": "a page with text"
                            },
                            {
                                "target_lang_word": "cover",
                                "target_lang_sense_clarification": "a surface with print"
                            }
                        ]
                    }
                }
            ]
        }

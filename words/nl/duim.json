            "senses": [
                {
                    "sense_id": "636e442a-c3c5-3fb5-8451-366d3715a92f",
                    "sense_definition": "Vervoeging van <w>duimen</w>: eerste persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>duim</w> voor je dat je morgen slaagt voor het rijexamen.",

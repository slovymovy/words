            "senses": [
                {
                    "sense_id": "fb5a5e70-bc04-37ed-86cd-2592b6d1a3be",
                    "sense_definition": "В недалёком будущем, через небольшой промежуток времени",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "short_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы надеемся <вскоре> увидеть вас снова в нашем доме.",
                            "target_lang_translations": {
                                "en": "We hope to see you again <soon> at our house."
                            }
                        },
                        {
                            "text": "Она обещала <вскоре> позвонить и рассказать все новости.",
                            "target_lang_translations": {
                                "en": "She promised to call <soon> and tell all the news."
                            }
                        }
                    ],
                    "synonyms": [
                        "скоро"
                    ],
                    "common_phrases": [
                        "вскоре вернётся",
                        "вскоре увидимся"
                    ],
                    "target_lang_definitions": {
                        "en": "In the near future, after a short period of time"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "soon",
                                "target_lang_sense_clarification": "general reference to the near future"
                            },
                            {
                                "target_lang_word": "before long",
                                "target_lang_sense_clarification": "idiomatic expression indicating the near future"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d69b643c-e82f-372a-b20e-00ae1df107d4",
                    "sense_definition": "Спустя совсем немного времени после какого-либо события",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "short_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дождь закончился, и <вскоре> на небе появилась яркая радуга.",
                            "target_lang_translations": {
                                "en": "The rain stopped, and <soon> a bright rainbow appeared in the sky."
                            }
                        },
                        {
                            "text": "<Вскоре> после обеда гости начали расходиться по домам.",
                            "target_lang_translations": {
                                "en": "<Shortly> after lunch, the guests began to head home."
                            }
                        }
                    ],
                    "synonyms": [

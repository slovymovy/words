                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кошелёк",
                                "target_lang_sense_clarification": "истор. мешочек с деньгами"
                            },
                            {
                                "target_lang_word": "мошна",
                                "target_lang_sense_clarification": "устар. богатство"
                            }

{
    "word_family": [
        "office",
        "official",
        "officiate",
        "officious"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f64d50a2-8942-3bd8-8fe1-ec37378169b5",
                    "sense_definition": "A person who has a position of authority in an organization, such as the police, the military, or a government department.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police <w>officer</w> helped the lost child find her parents.",
                            "target_lang_translations": {
                                "ru": "<w>Офицер</w> полиции помог потерявшемуся ребенку найти родителей."
                            }
                        },
                        {
                            "text": "My brother is a commanding <w>officer</w> in the navy and travels often.",
                            "target_lang_translations": {
                                "ru": "Мой брат — командный <w>офицер</w> военно-морского флота и часто путешествует."
                            }
                        }
                    ],
                    "common_phrases": [
                        "police officer",
                        "army officer",
                        "commanding officer",
                        "chief executive officer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, занимающее командную или руководящую должность в организации, например, в полиции, армии или государственном учреждении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "офицер",
                                "target_lang_sense_clarification": "в армии или полиции"
                            },
                            {
                                "target_lang_word": "чиновник",
                                "target_lang_sense_clarification": "государственный служащий"
                            },
                            {
                                "target_lang_word": "должностное лицо",
                                "target_lang_sense_clarification": "официальное лицо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd200d3a-5328-3eb5-80de-594d6dead8ab",
                    "sense_definition": "A respectful word used to speak to a police officer.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "\"Excuse me, <w>officer</w>, could you tell me the time?\"",
                            "target_lang_translations": {
                                "ru": "«Простите, <w>офицер</w>, не подскажете, который час?»"
                            }
                        },
                        {
                            "text": "\"Good morning, <w>officer</w>,\" said the driver as he rolled down the window.",
                            "target_lang_translations": {
                                "ru": "«Доброе утро, <w>офицер</w>», — сказал водитель, опуская стекло."
                            }
                        }
                    ],
                    "common_phrases": [
                        "excuse me officer",
                        "yes officer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уважительное обращение к сотруднику полиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "офицер",
                                "target_lang_sense_clarification": "обращение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4e2133a4-bfe6-33d7-9a86-44d62a405315",
                    "sense_definition": "To lead, command, or manage a group of people, especially in a military context.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new unit was <w>officered</w> by experienced veterans from the previous war.",
                            "target_lang_translations": {
                                "ru": "Новым подразделением <w>командовали</w> опытные ветераны прошлой войны."
                            }
                        },
                        {
                            "text": "He was chosen to <w>officer</w> the expedition due to his strong leadership skills.",
                            "target_lang_translations": {
                                "ru": "Его выбрали <w>возглавить</w> экспедицию благодаря его сильным лидерским качествам."
                            }
                        }
                    ],
                    "synonyms": [
                        "direct",
                        "conduct",
                        "manage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Руководить, командовать или управлять группой людей, особенно в военном контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командовать",
                                "target_lang_sense_clarification": "руководить подразделением"
                            },
                            {
                                "target_lang_word": "возглавлять",
                                "target_lang_sense_clarification": "быть во главе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1b59cb4-f07e-36d1-b953-c58d08f29115",
                    "sense_definition": "To provide a group or unit with officers.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It takes time to train and <w>officer</w> a new regiment properly.",
                            "target_lang_translations": {
                                "ru": "Требуется время, чтобы обучить и должным образом <w>укомплектовать офицерами</w> новый полк."
                            }
                        },
                        {
                            "text": "The government promised to <w>officer</w> the police force with better-trained staff.",
                            "target_lang_translations": {
                                "ru": "Правительство обещало <w>укомплектовать</w> полицию лучше подготовленными кадрами."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать группу или подразделение офицерами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укомплектовывать офицерами",
                                "target_lang_sense_clarification": "назначать командный состав"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "92096355-2f7c-3027-90ac-64bb92669e56",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Officer</w> has lived in this town for over fifty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Офисер</w> живет в этом городе уже более пятидесяти лет."
                            }
                        },
                        {
                            "text": "We looked up the name <w>Officer</w> in the local telephone directory.",
                            "target_lang_translations": {
                                "ru": "Мы искали фамилию <w>Офисер</w> в местном телефонном справочнике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Офисер",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
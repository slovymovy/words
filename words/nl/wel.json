                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vind deze film <wel> leuk, maar niet geweldig.",
                            "target_lang_translations": {

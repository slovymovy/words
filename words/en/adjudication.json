                    ],
                    "target_lang_definitions": {
                        "ru": "Судебный приказ или декларация о признании лица или компании банкротом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признание банкротом",
                                "target_lang_sense_clarification": "юридический статус"
                            },
                            {
                                "target_lang_word": "объявление несостоятельности",
                                "target_lang_sense_clarification": "финансовый статус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48dd688f-7746-3e19-8846-37f8a80fa2f8",

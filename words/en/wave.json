                    ],
                    "synonyms": [
                        "und",
                        "rush"
                    ],
                    "common_phrases": [
                        "ride a wave",
                        "huge wave",
                        "crashing waves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поднятая гряда воды, движущаяся по поверхности моря, океана или озера."
                    },
                    "translations": {
                        "ru": [

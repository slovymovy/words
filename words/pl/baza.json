    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cbbd9346-3156-3a1d-91ed-77dd6443cc93",
                    "sense_definition": "Główny ośrodek wojskowy lub logistyczny, w którym stacjonują ludzie, sprzęt i zapasy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_facilities",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze wrócili bezpiecznie do <w>bazy</w> po zakończeniu nocnego patrolu.",
                            "target_lang_translations": {
                                "ru": "Солдаты благополучно вернулись на <w>базу</w> после завершения ночного патруля."
                            }
                        },
                        {
                            "text": "Samoloty wojskowe wystartowały z <w>bazy</w> lotniczej o świcie.",
                            "target_lang_translations": {
                                "ru": "Военные самолеты взлетели с авиационной <w>базы</w> на рассвете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "baza wojskowa",
                        "baza lotnicza",
                        "wracać do bazy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главный военный или логистический центр, где размещаются люди, техника и припасы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "военная или логистическая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92bbe411-2d1f-3a47-9403-b6561982490a",
                    "sense_definition": "Podstawa, fundament czegoś lub zbiór uporządkowanych danych niezbędnych do funkcjonowania systemu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_foundation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma posiada ogromną <w>bazę</w> danych klientów z całego kraju.",
                            "target_lang_translations": {
                                "ru": "У компании есть огромная <w>база</w> данных клиентов со всей страны."
                            }
                        },
                        {
                            "text": "Ten podręcznik stanowi solidną <w>bazę</w> do dalszej nauki języka.",
                            "target_lang_translations": {
                                "ru": "Этот учебник служит надежной <w>базой</w> для дальнейшего изучения языка."
                            }
                        }
                    ],
                    "synonyms": [
                        "podstawa",
                        "fundament"
                    ],
                    "common_phrases": [
                        "baza danych",
                        "na bazie",
                        "stanowić bazę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основа, фундамент чего-либо или набор упорядоченных данных, необходимых для функционирования системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "данных, основа"
                            },
                            {
                                "target_lang_word": "основа",
                                "target_lang_sense_clarification": "фундамент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff804210-7993-384a-9f7f-22365e22436e",
                    "sense_definition": "Miejsce zakwaterowania lub obozowisko stanowiące punkt startowy wycieczek turystycznych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "location_facilities",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To małe miasteczko to doskonała <w>baza</w> wypadowa w pobliskie góry.",
                            "target_lang_translations": {
                                "ru": "Этот маленький городок — отличная отправная <w>база</w> для походов в близлежащие горы."
                            }
                        },
                        {
                            "text": "Hotel oferuje świetną <w>bazę</w> noclegową dla turystów z zagranicy.",
                            "target_lang_translations": {
                                "ru": "Отель предлагает отличную <w>базу</w> для размещения иностранных туристов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "baza wypadowa",
                        "baza noclegowa",
                        "baza namiotowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место размещения или лагерь, служащий отправной точкой для туристических поездок."

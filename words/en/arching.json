            "senses": [
                {
                    "sense_id": "8f6af8fc-42a4-34c6-9fd5-6097e0f51e97",
                    "sense_definition": "Forming a curved shape like an arch.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We walked beneath the <w>arching</w> branches of the ancient oak trees.",
                            "target_lang_translations": {
                                "ru": "Мы шли под <w>сводчатыми</w> ветвями древних дубов."
                            }
                        },
                        {
                            "text": "The <w>arching</w> roof of the stadium provides shelter for all the spectators.",
                            "target_lang_translations": {
                                "ru": "<w>Дугообразная</w> крыша стадиона обеспечивает укрытие для всех зрителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "curved",
                        "bowed",
                        "arcuate"
                    ],
                    "antonyms": [
                        "straight",
                        "flat"
                    ],
                    "common_phrases": [
                        "arching branches",
                        "high arching"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образующий изогнутую форму, напоминающую арку или свод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сводчатый",
                                "target_lang_sense_clarification": "напоминающий свод"
                            },
                            {
                                "target_lang_word": "дугообразный",
                                "target_lang_sense_clarification": "в форме дуги"
                            },
                            {
                                "target_lang_word": "изогнутый",
                                "target_lang_sense_clarification": "кривой формы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

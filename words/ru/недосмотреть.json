    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9b8b46e5-004f-3153-a23e-76ab0bb4d257",
                    "sense_definition": "Плохо присматривая за кем-либо или чем-либо, не уберечь от неприятности или допустить ошибку.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "lack_of_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Няня <недосмотрела> за ребёнком, и он упал с качелей.",
                            "target_lang_translations": {
                                "en": "The nanny <w>failed to keep an eye</w> on the child, and he fell off the swing."
                            }
                        },
                        {
                            "text": "Я <недосмотрел> за молоком на плите, и оно убежало.",
                            "target_lang_translations": {
                                "en": "I <w>failed to watch</w> the milk on the stove, and it boiled over."
                            }
                        }
                    ],
                    "synonyms": [
                        "недоглядеть"
                    ],
                    "common_phrases": [
                        "недосмотреть за ребёнком",
                        "недосмотреть за больным",
                        "недосмотреть за домом"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To fail to look after someone or something properly, allowing something bad to happen."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fail to keep an eye on",
                                "target_lang_sense_clarification": "fail to supervise properly"
                            },
                            {
                                "target_lang_word": "fail to watch",
                                "target_lang_sense_clarification": "not watch closely enough"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13f07aed-8a56-3056-865b-931d7834cb04",
                    "sense_definition": "Не заметить чего-либо при осмотре или проверке; пропустить по невнимательности.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "lack_of_attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Редактор <недосмотрел> досадную опечатку в заголовке статьи.",
                            "target_lang_translations": {
                                "en": "The editor <w>overlooked</w> an annoying typo in the article's headline."
                            }
                        },
                        {
                            "text": "Механик <недосмотрел> неисправность в тормозной системе.",
                            "target_lang_translations": {
                                "en": "The mechanic <w>missed</w> a malfunction in the brake system."
                            }
                        }
                    ],
                    "common_phrases": [
                        "недосмотреть ошибку",
                        "недосмотреть дефект"
                    ],
                    "target_lang_definitions": {
                        "en": "To fail to notice something during an inspection or check; to miss due to inattention."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overlook",
                                "target_lang_sense_clarification": "fail to notice something"
                            },
                            {
                                "target_lang_word": "miss",
                                "target_lang_sense_clarification": "fail to spot"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
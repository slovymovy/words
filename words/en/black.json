    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "788d9ee4-6a35-3b0f-a73e-b67d6af88438",
                    "sense_definition": "Absorbing all light and reflecting none; dark and hueless.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "color_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a simple <w>black</w> dress to the party.",
                            "target_lang_translations": {
                                "ru": "Она надела простое <w>чёрное</w> платье на вечеринку."
                            }
                        },
                        {
                            "text": "The cat has beautiful <w>black</w> fur.",
                            "target_lang_translations": {
                                "ru": "У кошки красивая <w>чёрная</w> шерсть."
                            }
                        }
                    ],
                    "synonyms": [
                        "inky",
                        "jet-black",
                        "coal-black",
                        "ebony"
                    ],
                    "antonyms": [
                        "white"
                    ],
                    "common_phrases": [
                        "black and white",
                        "in black and white"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поглощающий весь свет и не отражающий ничего; тёмный, лишённый оттенка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрный",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "тёмный",
                                "target_lang_sense_clarification": "о цвете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "135de175-9dc5-36bb-a98e-48f82dd68a24",
                    "sense_definition": "Served without any cream, milk, or creamer.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I prefer to drink my coffee <w>black</w>.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю пить <w>чёрный</w> кофе."
                            }
                        },
                        {
                            "text": "He ordered a <w>black</w> tea with lemon.",
                            "target_lang_translations": {
                                "ru": "Он заказал <w>чёрный</w> чай с лимоном."
                            }
                        }
                    ],
                    "antonyms": [
                        "white"
                    ],
                    "common_phrases": [
                        "black coffee",
                        "black tea"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подаваемый без сливок, молока или других добавок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрный",
                                "target_lang_sense_clarification": "о кофе или чае"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09aca80d-af70-3d6f-a35d-a8a93a5ece48",
                    "sense_definition": "Belonging to or descended from any of various ethnic groups which typically have dark pigmentation of the skin, especially of African or Aboriginal ancestry.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ethnicity_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university has a large population of <w>black</w> students.",
                            "target_lang_translations": {
                                "ru": "В университете много <w>чернокожих</w> студентов."
                            }
                        },
                        {
                            "text": "The author is a prominent <w>black</w> woman.",
                            "target_lang_translations": {
                                "ru": "Автор — известная <w>чернокожая</w> женщина."
                            }
                        }
                    ],
                    "antonyms": [
                        "white"
                    ],
                    "common_phrases": [
                        "Black History Month",
                        "black culture"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often capitalized"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к этническим группам с тёмной пигментацией кожи, особенно африканского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чернокожий",
                                "target_lang_sense_clarification": "раса"
                            },
                            {
                                "target_lang_word": "темнокожий",
                                "target_lang_sense_clarification": "цвет кожи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "590fbfaa-83b4-32e2-bae1-be385e9e9e50",
                    "sense_definition": "Without light; completely dark.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "color_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sky was <w>black</w> and starless.",
                            "target_lang_translations": {
                                "ru": "Небо было <w>чёрным</w> и беззвёздным."
                            }
                        },
                        {
                            "text": "It was a cold, <w>black</w> night.",
                            "target_lang_translations": {
                                "ru": "Это была холодная, <w>чёрная</w> ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "dark",
                        "unlit",
                        "pitch-black",
                        "sunless"
                    ],
                    "antonyms": [
                        "light",
                        "bright",
                        "illuminated",
                        "lit"
                    ],
                    "common_phrases": [
                        "pitch black"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишённый света; совершенно тёмный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрный",
                                "target_lang_sense_clarification": "о темноте"
                            },
                            {
                                "target_lang_word": "тёмный",
                                "target_lang_sense_clarification": "без света"
                            },
                            {
                                "target_lang_word": "непроглядный",
                                "target_lang_sense_clarification": "о тьме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df9e1680-7cb7-30dc-8424-4a560c707653",
                    "sense_definition": "Of or relating to the playing pieces of a board game deemed to belong to the 'black' set, or the suits spades and clubs in cards.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "games_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>black</w> queen was taken early in the game.",
                            "target_lang_translations": {
                                "ru": "<w>Чёрная</w> королева была взята в начале игры."
                            }
                        },
                        {
                            "text": "He played a <w>black</w> card to win the trick.",
                            "target_lang_translations": {
                                "ru": "Он сыграл <w>чёрной</w> картой, чтобы выиграть взятку."
                            }
                        }
                    ],
                    "antonyms": [
                        "white"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к игровым фигурам «чёрного» набора или к мастям пики и трефы в картах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрный",
                                "target_lang_sense_clarification": "в играх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e61293e4-c9b1-38b5-b5da-e508fc938bbc",
                    "sense_definition": "Illegitimate, illegal, or involving trade in violation of official regulations.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They bought the goods on the <w>black</w> market.",
                            "target_lang_translations": {
                                "ru": "Они купили товары на <w>чёрном</w> рынке."
                            }
                        },
                        {
                            "text": "The country is fighting against the <w>black</w> economy.",
                            "target_lang_translations": {
                                "ru": "Страна борется с <w>теневой</w> экономикой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "black market",
                        "black economy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Незаконный, нелегальный или связанный с торговлей в нарушение официальных правил."

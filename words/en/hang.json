                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "The street musician played a mesmerizing tune on his <w>Hang</w>.",
                            "target_lang_translations": {

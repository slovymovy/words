{
    "word_family": [
        "ruch",
        "ruszać",
        "poruszyć",
        "wyruszyć",
        "naruszyć",
        "wzruszyć",
        "rozruszać",
        "uruchomić",
        "poruszenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "971b8583-a784-3768-abb0-95dcab4893f4",
                    "sense_definition": "Zacząć się przemieszczać, iść lub jechać w określonym kierunku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pociąg powoli <ruszył> ze stacji w kierunku Warszawy.",
                            "target_lang_translations": {
                                "ru": "Поезд медленно <тронулся> со станции в направлении Варшавы."
                            }
                        },
                        {
                            "text": "Po krótkim odpoczynku turyści <ruszyli> w dalszą drogę na szczyt.",
                            "target_lang_translations": {
                                "ru": "После короткого отдыха туристы <двинулись> дальше в путь на вершину."
                            }
                        },
                        {
                            "text": "Mężczyzna dopił kawę i <ruszył> w stronę baru.",
                            "target_lang_translations": {
                                "ru": "Мужчина допил кофе и <направился> в сторону бара."
                            }
                        }
                    ],
                    "synonyms": [
                        "zacząć",
                        "przesunąć"
                    ],
                    "antonyms": [
                        "zatrzymać się",
                        "stanąć"
                    ],
                    "common_phrases": [
                        "ruszyć w drogę",
                        "ruszyć z miejsca",
                        "ruszyć przed siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать перемещаться, идти или ехать в определенном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тронуться",
                                "target_lang_sense_clarification": "начать движение (о транспорте)"
                            },
                            {
                                "target_lang_word": "двинуться",
                                "target_lang_sense_clarification": "направиться куда-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d7f8925-0016-3bb6-948e-86c9e527df0d",
                    "sense_definition": "Zmienić położenie jakiegoś obiektu, przesunąć go lub wprawić w ruch.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta szafa jest tak ciężka, że nie mogę jej nawet <ruszyć>.",
                            "target_lang_translations": {
                                "ru": "Этот шкаф такой тяжелый, что я не могу его даже <сдвинуть>."
                            }
                        },
                        {
                            "text": "Wiatr był tak silny, że <ruszył> dachówki na starym domu.",
                            "target_lang_translations": {
                                "ru": "Ветер был таким сильным, что <сдвинул> черепицу на старом доме."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesunąć"
                    ],
                    "common_phrases": [
                        "ruszyć palcem",
                        "ruszyć głową"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить положение объекта, сдвинуть его или привести в движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдвинуть",
                                "target_lang_sense_clarification": "переместить с места"
                            },
                            {
                                "target_lang_word": "пошевелить",
                                "target_lang_sense_clarification": "слегка двинуть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77320c11-fde2-30b2-99b1-73a7b7566a50",
                    "sense_definition": "Dotknąć czegoś, wziąć coś do ręki lub zacząć czegoś używać (często o jedzeniu lub cudzych rzeczach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "touch_consume",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Byłem tak zdenerwowany, że przez cały dzień nie <ruszyłem> jedzenia.",
                            "target_lang_translations": {
                                "ru": "Я так нервничал, что весь день не <притронулся> к еде."
                            }
                        },
                        {
                            "text": "Kto <ruszył> moje dokumenty, które leżały na biurku?",
                            "target_lang_translations": {
                                "ru": "Кто <трогал> мои документы, которые лежали на столе?"
                            }
                        }
                    ],
                    "synonyms": [
                        "dotknąć"
                    ],
                    "common_phrases": [
                        "nie ruszyć jedzenia",
                        "nie ruszyć alkoholu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дотронуться до чего-либо, взять в руки или начать использовать (часто о еде или чужих вещах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тронуть",
                                "target_lang_sense_clarification": "коснуться чего-либо"
                            },
                            {
                                "target_lang_word": "притронуться",
                                "target_lang_sense_clarification": "начать есть или пить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b232b429-bad2-3230-8e14-cd56a84a36d3",
                    "sense_definition": "Sprawić, że coś zacznie działać, funkcjonować lub toczyć się (np. maszyna, projekt).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "start_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki nowym funduszom budowa osiedla w końcu <ruszyła>.",
                            "target_lang_translations": {
                                "ru": "Благодаря новым средствам строительство жилого комплекса наконец <началось>."
                            }
                        },
                        {
                            "text": "Musimy <ruszyć> ten temat na najbliższym spotkaniu zespołu.",
                            "target_lang_translations": {
                                "ru": "Мы должны <поднять> эту тему на ближайшей встрече команды."
                            }
                        }
                    ],
                    "synonyms": [
                        "zacząć"
                    ],
                    "antonyms": [
                        "zatrzymać"
                    ],
                    "common_phrases": [
                        "ruszyć z kopyta",
                        "ruszyć sprawę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить что-то работать, функционировать или развиваться (например, машину, проект)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начаться",
                                "target_lang_sense_clarification": "стартовать (о процессе)"
                            },
                            {
                                "target_lang_word": "сдвинуться",
                                "target_lang_sense_clarification": "прийти в движение (о деле)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
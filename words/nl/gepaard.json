                {
                    "sense_id": "6f55851d-ce63-38c5-8a01-3839cab1ba7d",
                    "sense_definition": "Tegelijkertijd optredend; samengaand met iets anders.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De griep gaat vaak <w>gepaard</w> met koorts en spierpijn.",
                            "target_lang_translations": {
                                "ru": "Грипп часто <w>сопровождается</w> жаром и мышечной болью."
                            }
                        },
                        {
                            "text": "Grote veranderingen gaan meestal <w>gepaard</w> met enige weerstand.",
                            "target_lang_translations": {
                                "ru": "Большие перемены обычно <w>сопряжены</w> с некоторым сопротивлением."
                            }
                        }
                    ],
                    "synonyms": [
                        "samenhangend"
                    ],
                    "common_phrases": [
                        "gepaard gaan met"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий одновременно; сопровождающийся чем-то другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопряжённый",
                                "target_lang_sense_clarification": "связанный с чем-то, влекущий за собой что-то"
                            },
                            {
                                "target_lang_word": "сопровождающийся",
                                "target_lang_sense_clarification": "происходящий вместе с чем-то"

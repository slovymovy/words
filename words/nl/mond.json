            "pos": "noun",
            "senses": [
                {
                    "sense_id": "890d8398-f104-3a2c-b812-486292ede92e",
                    "sense_definition": "De opening in het gezicht die mensen en dieren gebruiken om te eten, te ademen en geluid te maken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stopte een stukje brood in zijn <w>mond</w>.",
                            "target_lang_translations": {
                                "en": "He put a piece of bread in his <w>mouth</w>.",
                                "ru": "Он положил кусочек хлеба в <w>рот</w>."
                            }
                        },
                        {
                            "text": "Praat alsjeblieft niet met volle <w>mond</w> als we aan tafel zitten.",
                            "target_lang_translations": {
                                "en": "Please do not talk with your <w>mouth</w> full when we are sitting at the table.",
                                "ru": "Пожалуйста, не говори с набитым <w>ртом</w>, когда мы сидим за столом."
                            }
                        }
                    ],
                    "synonyms": [
                        "muil",
                        "bek",
                        "snater"
                    ],
                    "common_phrases": [
                        "houd je mond",
                        "met volle mond",
                        "mond-op-mondbeademing"
                    ],
                    "target_lang_definitions": {
                        "en": "The opening in the face that people and animals use to eat, breathe, and make sound.",
                        "ru": "Отверстие на лице, которое люди и животные используют для еды, дыхания и издания звуков."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mouth",
                                "target_lang_sense_clarification": "anatomy"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рот",
                                "target_lang_sense_clarification": "часть тела"
                            },
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "рот животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2f64916-cc6f-346a-be55-8a2b33107319",
                    "sense_definition": "De plaats waar een rivier uitstroomt in een groter water, zoals de zee of een meer.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stad ligt precies bij de <w>mond</w> van de rivier.",
                            "target_lang_translations": {
                                "en": "The city lies exactly at the <w>mouth</w> of the river.",
                                "ru": "Город расположен прямо в <w>устье</w> реки."
                            }
                        },
                        {
                            "text": "Bij de <w>mond</w> van de rivier is de stroming vaak erg sterk.",
                            "target_lang_translations": {
                                "en": "At the <w>mouth</w> of the river, the current is often very strong.",
                                "ru": "В <w>устье</w> реки течение часто очень сильное."
                            }
                        }
                    ],
                    "synonyms": [
                        "monding"
                    ],
                    "common_phrases": [
                        "de mond van de rivier"
                    ],
                    "target_lang_definitions": {
                        "en": "The place where a river flows into a larger body of water, such as the sea or a lake.",
                        "ru": "Место, где река впадает в более крупный водоем, например, в море или озеро."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mouth",
                                "target_lang_sense_clarification": "of a river"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "устье",
                                "target_lang_sense_clarification": "реки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f4407b9-3976-31a4-b1d2-628a22052f65",
                    "sense_definition": "De opening of ingang van een hol voorwerp, zoals een kanon, tunnel of fles.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er kwam donkere rook uit de <w>mond</w> van het kanon.",
                            "target_lang_translations": {
                                "en": "Dark smoke came out of the <w>mouth</w> of the cannon.",
                                "ru": "Из <w>жерла</w> пушки валил темный дым."
                            }
                        },
                        {
                            "text": "De kinderen keken nieuwsgierig in de donkere <w>mond</w> van de grot.",
                            "target_lang_translations": {
                                "en": "The children looked curiously into the dark <w>mouth</w> of the cave.",
                                "ru": "Дети с любопытством заглядывали в темный <w>вход</w> пещеры."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The opening or entrance of a hollow object, such as a cannon, tunnel, or bottle.",
                        "ru": "Отверстие или вход в полый предмет, например, пушку, туннель или бутылку."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mouth",
                                "target_lang_sense_clarification": "opening of an object"
                            },
                            {

                    "frequency": "VeryLow",
                    "semantic_group_id": "surname_m",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Sopot</w> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Сопот</w> был избран председателем совета жилого комплекса."
                            }
                        },
                        {
                            "text": "Rozmawiałem wczoraj z dyrektorem <w>Sopotem</w> o nowym projekcie.",
                            "target_lang_translations": {
                                "ru": "Вчера я разговаривал с директором <w>Сопотом</w> о новом проекте."
                            }
                        }
                    ],

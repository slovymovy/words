            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4b74cd05-9ab2-3fae-9831-785c0387fff2",
                    "sense_definition": "Een groep kleine arbeiderswoningen gebouwd rond een binnenplaats of steegje, vaak niet zichtbaar vanaf de straat.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "housing_complex",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De studenten wonen tegenwoordig in een mooi gerenoveerd <w>beluik</w> in het centrum.",
                            "target_lang_translations": {
                                "ru": "Студенты в наши дни живут в красиво отреставрированном <w>жилом дворике</w> в центре.",
                                "en": "Nowadays, the students live in a beautifully renovated <w>courtyard complex</w> in the center."
                            }
                        },
                        {
                            "text": "Vroeger woonden arme gezinnen dicht op elkaar in een donker en vochtig <w>beluik</w>.",
                            "target_lang_translations": {
                                "ru": "Раньше бедные семьи жили в тесноте в темном и сыром <w>тупике</w>.",
                                "en": "In the past, poor families lived close together in a dark and damp <w>worker's court</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hofje"
                    ],
                    "common_phrases": [
                        "een Gents beluik",
                        "wonen in een beluik"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België (Vlaanderen)"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "verwijst vaak naar 19e-eeuwse arbeidershuisvesting"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа небольших рабочих домиков, построенных вокруг внутреннего двора или переулка, часто скрытая от улицы.",
                        "en": "A group of small workers' houses built around a courtyard or alley, often not visible from the street."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жилой дворик",
                                "target_lang_sense_clarification": "комплекс жилых домов вокруг двора"
                            },
                            {
                                "target_lang_word": "тупик",
                                "target_lang_sense_clarification": "застроенный переулок"
                            }
                        ],
                        "en": [

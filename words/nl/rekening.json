                    "synonyms": [
                        "factuur",
                        "nota",
                        "afrekening"
                    ],
                    "common_phrases": [
                        "de rekening vragen",
                        "de rekening betalen",
                        "de rekening delen"
                    ],
                    "target_lang_definitions": {

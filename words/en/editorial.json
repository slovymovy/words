    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ef93a465-00d2-3e67-ab10-d78a06b51f35",
                    "sense_definition": "An article in a newspaper or magazine that expresses the opinion of the editors or publishers on a specific topic.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "opinion_piece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newspaper published a controversial <w>editorial</w> about the upcoming election.",
                            "target_lang_translations": {
                                "ru": "Газета опубликовала спорную <w>редакционную статью</w> о предстоящих выборах."
                            }
                        },
                        {
                            "text": "She wrote a powerful <w>editorial</w> criticizing the local government's decision.",
                            "target_lang_translations": {
                                "ru": "Она написала сильную <w>передовицу</w> с критикой решения местных властей."
                            }
                        }
                    ],
                    "synonyms": [
                        "leader",
                        "opinion piece",
                        "leading article"
                    ],
                    "common_phrases": [
                        "newspaper editorial",
                        "write an editorial",
                        "front-page editorial"
                    ],
                    "target_lang_definitions": {
                        "ru": "Статья в газете или журнале, выражающая мнение редакторов или издателей по конкретной теме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "редакционная статья",
                                "target_lang_sense_clarification": "статья от имени редакции"
                            },
                            {
                                "target_lang_word": "передовица",
                                "target_lang_sense_clarification": "передовая статья"
                            },
                            {
                                "target_lang_word": "передовая",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bf473e2-4381-3d16-b243-7e1aaee61271",
                    "sense_definition": "A commentary broadcast on radio or television that expresses the station's opinion on a subject.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "opinion_piece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The news anchor delivered a brief <w>editorial</w> at the end of the broadcast.",
                            "target_lang_translations": {
                                "ru": "Ведущий новостей выступил с кратким <w>редакционным комментарием</w> в конце эфира."
                            }
                        },
                        {
                            "text": "Listeners called the station to respond to the morning <w>editorial</w>.",
                            "target_lang_translations": {
                                "ru": "Слушатели звонили на станцию, чтобы ответить на утренний <w>редакционный комментарий</w>."
                            }

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "34be00ea-5d0d-3c85-b2cd-fb6eb35f0ab3",
                    "sense_definition": "Очень сильно, в значительной степени, серьезно (обычно об удаче, ошибках или неприятностях).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам сегодня <крупно> повезло, мы успели на последний поезд.",
                            "target_lang_translations": {
                                "en": "We lucked out <big time> today; we made it to the last train."
                            }
                        },
                        {
                            "text": "Если ты думаешь, что это сойдет тебе с рук, ты <крупно> ошибаешься.",
                            "target_lang_translations": {
                                "en": "If you think you can get away with this, you are <badly> mistaken."
                            }
                        }
                    ],
                    "common_phrases": [
                        "крупно повезло",
                        "крупно ошибиться",
                        "крупно проиграть",
                        "крупно влипнуть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very strongly, significantly, or seriously (usually regarding luck, mistakes, or trouble)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "big time",
                                "target_lang_sense_clarification": "colloquial intensifier for luck or trouble"
                            },
                            {
                                "target_lang_word": "badly",
                                "target_lang_sense_clarification": "regarding mistakes or errors"
                            },
                            {
                                "target_lang_word": "heavily",
                                "target_lang_sense_clarification": "regarding financial losses or gambling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2cead9b-b43a-3e7d-a939-c781de3bea4f",
                    "sense_definition": "На большие части или куски; в виде больших частиц.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для этого салата овощи нужно нарезать <крупно>.",
                            "target_lang_translations": {
                                "en": "For this salad, the vegetables need to be cut <into large pieces>."
                            }
                        },
                        {
                            "text": "Соль была помолота слишком <крупно> и хрустела на зубах.",
                            "target_lang_translations": {
                                "en": "The salt was ground too <coarsely> and crunched on the teeth."
                            }
                        }
                    ],
                    "antonyms": [
                        "мелко"
                    ],
                    "common_phrases": [
                        "крупно нарезать",
                        "крупно порубить"
                    ],
                    "target_lang_definitions": {
                        "en": "Into large parts or pieces; in the form of large particles."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coarsely",
                                "target_lang_sense_clarification": "regarding grinding or chopping texture"
                            },
                            {
                                "target_lang_word": "into large pieces",
                                "target_lang_sense_clarification": "regarding cutting or slicing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ffcf3b7-2494-3d03-b0f1-0fa88dd6571d",
                    "sense_definition": "В большом масштабе, с приближением (о фото- или видеосъемке); большими буквами (о почерке или шрифте).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_scale",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Оператор снял лицо актера <крупно>, чтобы показать его эмоции.",
                            "target_lang_translations": {
                                "en": "The cameraman shot the actor's face <close-up> to show his emotions."
                            }
                        },
                        {
                            "text": "Название магазина было написано <крупно> и ярко светилось в темноте.",
                            "target_lang_translations": {
                                "en": "The name of the shop was written <large> and glowed brightly in the dark."
                            }
                        }
                    ],
                    "antonyms": [

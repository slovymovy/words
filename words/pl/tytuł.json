            "pos": "noun",
            "senses": [
                {
                    "sense_id": "061160fb-caff-35aa-98dc-0e60d50facb5",
                    "sense_definition": "Nazwa nadana utworowi literackiemu, naukowemu, dziełu sztuki, filmowi lub artykułowi, wyróżniająca go spośród innych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "name_of_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie pamiętam, jaki <w>tytuł</w> nosi ta interesująca książka.",
                            "target_lang_translations": {
                                "ru": "Я не помню, какое <w>название</w> носит эта интересная книга."
                            }
                        },
                        {
                            "text": "Artykuł ukazał się w gazecie pod <w>tytułem</w> sugerującym wielki skandal.",
                            "target_lang_translations": {
                                "ru": "Статья вышла в газете под <w>заголовком</w>, намекающим на грандиозный скандал."
                            }
                        }
                    ],
                    "synonyms": [
                        "nazwa",
                        "nagłówek"
                    ],
                    "common_phrases": [
                        "tytuł filmu",
                        "tytuł książki",
                        "pod tytułem",
                        "tytuł roboczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Название, данное литературному, научному произведению, произведению искусства, фильму или статье, выделяющее его среди других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "название",
                                "target_lang_sense_clarification": "общее слово для книг, фильмов, произведений"
                            },
                            {
                                "target_lang_word": "заглавие",
                                "target_lang_sense_clarification": "литературного или научного произведения"
                            },
                            {
                                "target_lang_word": "заголовок",
                                "target_lang_sense_clarification": "статьи, газетного материала"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e5a4a03-951d-30d4-82e2-529e9e5340e4",
                    "sense_definition": "Stopień naukowy, zawodowy, sportowy lub godność przysługująca danej osobie, często związana z pełnioną funkcją lub osiągnięciami.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "rank_honor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu latach badań profesor otrzymał zaszczytny <w>tytuł</w> doktora honoris causa.",
                            "target_lang_translations": {
                                "ru": "После многих лет исследований профессор получил почётный <w>титул</w> доктора honoris causa."
                            }
                        },
                        {
                            "text": "Nasza drużyna walczy w tym sezonie o <w>tytuł</w> mistrza kraju.",
                            "target_lang_translations": {
                                "ru": "В этом сезоне наша команда борется за <w>титул</w> чемпиона страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "godność"
                    ],
                    "common_phrases": [
                        "tytuł naukowy",
                        "tytuł mistrza",
                        "tytuł szlachecki",
                        "tytuł zawodowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учёная, профессиональная, спортивная степень или звание, присвоенное лицу, часто связанное с выполняемой функцией или достижениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "титул",
                                "target_lang_sense_clarification": "дворянский, спортивный или почётный"
                            },
                            {
                                "target_lang_word": "звание",
                                "target_lang_sense_clarification": "воинское, учёное, профессиональное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbe85dff-7aa4-3d05-918b-48ec22c48813",
                    "sense_definition": "Podstawa prawna, powód lub uzasadnienie jakiegoś działania, roszczenia lub posiadania czegoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_basis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracownikowi należy się dodatkowe wynagrodzenie z <w>tytułu</w> pracy w godzinach nadliczbowych.",
                            "target_lang_translations": {
                                "ru": "Работнику полагается дополнительное вознаграждение на <w>основании</w> работы в сверхурочные часы."
                            }
                        },
                        {
                            "text": "Musisz przedstawić w urzędzie ważny <w>tytuł</w> prawny do zajmowanego lokalu.",
                            "target_lang_translations": {
                                "ru": "Вы должны представить в учреждение действительный правовой <w>титул</w> на занимаемое помещение."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyczyna",
                        "przyczyna"
                    ],
                    "common_phrases": [
                        "z tytułu",
                        "tytuł prawny",
                        "tytuł własności",
                        "tytuł wykonawczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правовое основание, причина или обоснование какого-либо действия, претензии или владения чем-либо."

                    "sense_definition": "De toestand van dronken zijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>zatheid</w> was stuitend voor de andere gasten.",
                            "target_lang_translations": {
                                "ru": "Его <w>опьянение</w> было отвратительным для других гостей."
                            }
                        },
                        {
                            "text": "Door zijn <w>zatheid</w> kon hij niet meer recht op zijn benen staan.",
                            "target_lang_translations": {
                                "ru": "Из-за своего <w>опьянения</w> он больше не мог ровно стоять на ногах."
                            }
                        }
                    ],
                    "synonyms": [
                        "dronkenschap",
                        "beschonkenheid",
                        "roes"
                    ],
                    "antonyms": [
                        "nuchterheid"
                    ],
                    "target_lang_definitions": {

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8b6c317f-2189-362e-9d34-7815bbd2e051",
                    "sense_definition": "Схватить или задержать кого-либо, кто убегает, прячется или сопротивляется.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "catch_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиции удалось <изловить> опасного преступника, который скрывался в лесу.",
                            "target_lang_translations": {
                                "en": "The police managed to <capture> the dangerous criminal who was hiding in the forest."
                            }
                        },
                        {
                            "text": "Мы пытались <изловить> кота, сбежавшего из дома, но он был слишком быстр.",
                            "target_lang_translations": {
                                "en": "We tried to <catch> the cat that ran away from home, but it was too fast."
                            }
                        }
                    ],
                    "synonyms": [
                        "поймать"
                    ],
                    "common_phrases": [
                        "изловить преступника",
                        "изловить беглеца",
                        "изловить вора"
                    ],
                    "target_lang_definitions": {
                        "en": "To seize or detain someone who is fleeing, hiding, or resisting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "capture",
                                "target_lang_sense_clarification": "to take into one's possession or control by force or stratagem"
                            },
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "to capture or seize, especially after a chase"
                            },
                            {
                                "target_lang_word": "apprehend",
                                "target_lang_sense_clarification": "formal; to arrest someone for a crime"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e6092e3-319b-3295-91b9-2a11dc266e56",
                    "sense_definition": "Поймать животное, птицу или рыбу во время охоты или ловли.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "catch_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охотники хотели <изловить> медведя живым для зоопарка.",
                            "target_lang_translations": {
                                "en": "The hunters wanted to <capture> the bear alive for the zoo."
                            }
                        },
                        {
                            "text": "Рыбакам посчастливилось <изловить> огромную щуку в этом озере.",
                            "target_lang_translations": {
                                "en": "The fishermen were lucky enough to <catch> a huge pike in this lake."
                            }
                        }
                    ],
                    "synonyms": [
                        "поймать",
                        "добыть"
                    ],
                    "common_phrases": [
                        "изловить зверя",
                        "изловить мышь"
                    ],
                    "target_lang_definitions": {
                        "en": "To catch an animal, bird, or fish during hunting or fishing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "to capture an animal or fish"
                            },
                            {
                                "target_lang_word": "capture",
                                "target_lang_sense_clarification": "to take an animal alive"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b634c0b-203e-3d2f-9a9a-97466f2116fc",
                    "sense_definition": "Уличить кого-либо в обмане, лжи или скрываемом поступке.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Журналист пытался <изловить> политика на противоречиях в его речи.",
                            "target_lang_translations": {
                                "en": "The journalist tried to <catch> the politician in contradictions in his speech."
                            }
                        },
                        {
                            "text": "Ему не удалось <изловить> меня на ошибке, так как я всё тщательно проверил.",
                            "target_lang_translations": {
                                "en": "He failed to <catch> me in a mistake, as I had checked everything thoroughly."
                            }
                        }
                    ],
                    "synonyms": [
                        "поймать"
                    ],
                    "common_phrases": [
                        "изловить на лжи",
                        "изловить на слове"
                    ],
                    "target_lang_definitions": {
                        "en": "To expose someone in a deception, lie, or hidden act."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "to discover or detect someone doing something wrong"
                            },
                            {
                                "target_lang_word": "catch out",
                                "target_lang_sense_clarification": "to detect someone in a mistake or lie"
                            }
                        ]
                    }
                }
            ]
        }
    ]

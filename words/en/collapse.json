    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b3908eb0-defa-3de6-a3b9-21de69c52706",
                    "sense_definition": "To break apart and fall down suddenly, often because of a lack of support or structural weakness.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_structural_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old wooden bridge <w>collapsed</w> into the river during the heavy storm."
                        },
                        {
                            "text": "If you remove that supporting wall, the entire roof might <w>collapse</w>."
                        }
                    ],
                    "synonyms": [
                        "cave in",
                        "fall down",
                        "crumble"
                    ],
                    "common_phrases": [
                        "roof collapsed",
                        "building collapsed",
                        "suddenly collapse"
                    ]
                },
                {
                    "sense_id": "65424de7-8797-327e-87e2-879fd328a92d",
                    "sense_definition": "To fail suddenly and completely, often referring to systems, organizations, economies, or negotiations.",

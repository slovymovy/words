                    "frequency": "Low",
                    "semantic_group_id": "slang_trouble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am completely <w>rooted</w> if my boss finds out about this mistake.",
                            "target_lang_translations": {
                                "ru": "Я полностью <w>влип</w>, если мой начальник узнает об этой ошибке."
                            }
                        },
                        {
                            "text": "My old car is <w>rooted</w> and will not start anymore.",
                            "target_lang_translations": {
                                "ru": "Моя старая машина <w>накрылась</w> и больше не заводится."
                            }
                        }
                    ],
                    "synonyms": [
                        "screwed",
                        "ruined",
                        "broken"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often used in Australian or NZ English"
                        }
                    ],
                    "target_lang_definitions": {

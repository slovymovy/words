    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "96ff0c24-82b0-33b0-88b7-3b2a2a56c2f2",
                    "sense_definition": "Принимать пищу, есть для поддержания жизни и сил.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи рекомендуют <питаться> регулярно и не пропускать завтрак.",
                            "target_lang_translations": {
                                "en": "Doctors recommend <w>eating</w> regularly and not skipping breakfast."
                            }
                        },
                        {
                            "text": "В студенческие годы мы <питались> довольно скромно и просто.",
                            "target_lang_translations": {
                                "en": "In our student years, we <w>ate</w> quite modestly and simply."
                            }
                        }
                    ],
                    "synonyms": [
                        "есть"
                    ],
                    "common_phrases": [
                        "правильно питаться",
                        "питаться дома",
                        "питаться в столовой"
                    ],
                    "target_lang_definitions": {
                        "en": "To take food, to eat to maintain life and strength."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eat",
                                "target_lang_sense_clarification": "general action of consuming food"
                            },
                            {
                                "target_lang_word": "feed",
                                "target_lang_sense_clarification": "to take food (often used for animals or in specific contexts)"
                            },
                            {
                                "target_lang_word": "subsist",
                                "target_lang_sense_clarification": "to maintain life, often implying basic or minimal food"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "faf8314a-4250-36f2-91e8-649664cea6d6",
                    "sense_definition": "Употреблять определённые продукты в качестве основной еды (обычно с творительным падежом).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Многие хищные животные <питаются> исключительно мясом.",
                            "target_lang_translations": {
                                "en": "Many predatory animals <w>feed on</w> meat exclusively."
                            }
                        },
                        {
                            "text": "Мой друг решил <питаться> только овощами и фруктами.",
                            "target_lang_translations": {
                                "en": "My friend decided to <w>live on</w> only vegetables and fruits."
                            }
                        }
                    ],
                    "synonyms": [
                        "есть"
                    ],
                    "common_phrases": [
                        "питаться мясом",
                        "питаться овощами",
                        "питаться всухомятку"
                    ],
                    "target_lang_definitions": {
                        "en": "To consume specific products as the main food source."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "feed on",
                                "target_lang_sense_clarification": "to eat a specific food (especially animals)"
                            },
                            {
                                "target_lang_word": "live on",
                                "target_lang_sense_clarification": "to subsist on a specific diet"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f9148c7-d3b3-32d8-8f8f-86f97c99e44b",
                    "sense_definition": "Получать энергию или топливо, необходимые для работы (о механизмах, приборах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот портативный приёмник <питается> от двух маленьких батареек.",
                            "target_lang_translations": {
                                "en": "This portable receiver <w>is powered</w> by two small batteries."
                            }
                        },
                        {
                            "text": "Система аварийного освещения <питается> от независимого генератора.",
                            "target_lang_translations": {
                                "en": "The emergency lighting system <w>runs</w> on an independent generator."
                            }
                        }
                    ],
                    "synonyms": [
                        "работать"
                    ],
                    "common_phrases": [
                        "питаться от сети",
                        "питаться от аккумулятора",
                        "питаться от батареек"
                    ],
                    "target_lang_definitions": {
                        "en": "To receive energy or fuel necessary for operation (about mechanisms, devices)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be powered",
                                "target_lang_sense_clarification": "receive energy (passive)"
                            },
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "operate using a specific energy source"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4d57d14-f12d-398d-a661-fcb2d9bde4d6",
                    "sense_definition": "Страдательный залог к глаголу <w>питать</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Озеро <питается> водами многочисленных горных рек и ручьёв.",
                            "target_lang_translations": {
                                "en": "The lake <w>is fed</w> by the waters of numerous mountain rivers and streams."
                            }
                        },
                        {
                            "text": "Любая надежда <питается> верой в лучшее будущее.",
                            "target_lang_translations": {
                                "en": "Any hope <w>is fed</w> by faith in a better future."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive voice of the verb 'to feed'; to be supplied with something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be fed",
                                "target_lang_sense_clarification": "receive supply (water, hope, etc.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
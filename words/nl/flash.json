            "senses": [
                {
                    "sense_id": "1eafb121-92db-3c3f-9f9f-2f9b3440ed87",
                    "sense_definition": "Een korte, felle lichtuitstraling, met name geproduceerd door een camera.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fotograaf gebruikte een <w>flash</w> omdat het binnen te donker was.",
                            "target_lang_translations": {
                                "en": "The photographer used a <w>flash</w> because it was too dark inside.",
                                "ru": "Фотограф использовал <w>вспышку</w>, потому что внутри было слишком темно."
                            }
                        },
                        {
                            "text": "Door de felle <w>flash</w> zag ik even helemaal niets meer.",
                            "target_lang_translations": {
                                "en": "Because of the bright <w>flash</w>, I couldn't see anything for a moment.",
                                "ru": "Из-за яркой <w>вспышки</w> я мгновение ничего не видел."
                            }
                        }
                    ],
                    "synonyms": [
                        "flits",
                        "flitslicht"
                    ],
                    "common_phrases": [
                        "met flash",
                        "zonder flash"
                    ],
                    "target_lang_definitions": {
                        "en": "A short, bright burst of light, especially one produced by a camera.",
                        "ru": "Короткая, яркая вспышка света, особенно производимая камерой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flash",
                                "target_lang_sense_clarification": "burst of light"
                            },
                            {
                                "target_lang_word": "camera flash",
                                "target_lang_sense_clarification": "device"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вспышка",
                                "target_lang_sense_clarification": "фотоаппарата или света"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d1ed2f8-8b72-36e1-ae34-4855471baea8",
                    "sense_definition": "Een vorm van elektronisch geheugen dat gegevens bewaart zonder stroomtoevoer, afkorting van <w>flashgeheugen</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn nieuwe laptop heeft een snelle schijf gebaseerd op <w>flash</w>.",

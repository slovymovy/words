                            {
                                "target_lang_word": "сверстница",
                                "target_lang_sense_clarification": "синоним, подразумевает принадлежность к одному поколению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f92e5529-d254-3e6f-ba53-32f0e4fe0f73",
                    "sense_definition": "Rzecz, instytucja lub zjawisko powstałe w tym samym czasie co inne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "age_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta zabytkowa kamienica jest <w>rówieśniczką</w> miejskiego ratusza.",
                            "target_lang_translations": {
                                "ru": "Этот старинный дом — <w>ровесник</w> городской ратуши."
                            }
                        },
                        {
                            "text": "Nasza konstytucja jest <w>rówieśniczką</w> wielu przemian ustrojowych w Europie.",
                            "target_lang_translations": {
                                "ru": "Наша конституция — <w>ровесница</w> многих политических преобразований в Европе."
                            }

{
    "word_family": [
        "feel",
        "feeling",
        "felting"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "807eacd0-5a6e-327f-91fd-dcf4151eac31",
                    "sense_definition": "Simple past and past participle of <w>feel</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <felt> tired after working in the garden all day.",
                            "target_lang_translations": {
                                "ru": "Она <чувствовала> усталость после работы в саду весь день."
                            }
                        },
                        {
                            "text": "I have never <felt> this happy before.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не <чувствовал> себя таким счастливым."
                            }
                        }
                    ],
                    "common_phrases": [
                        "felt like",
                        "felt better",
                        "felt sorry",
                        "felt sure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола <w>feel</w> (чувствовать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствовал",
                                "target_lang_sense_clarification": "прошедшее время, несовершенный вид"
                            },
                            {
                                "target_lang_word": "почувствовал",
                                "target_lang_sense_clarification": "прошедшее время, совершенный вид"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5c70fc75-bbec-3692-8571-f86c95bf9abb",
                    "sense_definition": "A thick, soft fabric made by pressing wool or other fibers together rather than weaving them.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "material_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The billiard table was covered in smooth green <felt>.",
                            "target_lang_translations": {
                                "ru": "Бильярдный стол был покрыт гладким зеленым <фетром>."
                            }
                        },
                        {
                            "text": "We used red <felt> to make decorations for the holiday.",
                            "target_lang_translations": {
                                "ru": "Мы использовали красный <фетр> для изготовления праздничных украшений."
                            }
                        }
                    ],
                    "common_phrases": [
                        "green felt",
                        "roofing felt",
                        "wool felt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плотный мягкий материал, получаемый путем спрессовывания шерсти или других волокон, а не ткачества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фетр",
                                "target_lang_sense_clarification": "тонкий, мягкий сорт для шляп и поделок"
                            },
                            {
                                "target_lang_word": "войлок",
                                "target_lang_sense_clarification": "плотный, грубый материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00c7c079-dbd9-3734-9233-6224c5f4d520",
                    "sense_definition": "A pen with a writing tip made of porous material; a felt-tip pen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "tool_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist used black <felts> to outline the drawing.",
                            "target_lang_translations": {
                                "ru": "Художник использовал черные <фломастеры>, чтобы обвести рисунок."
                            }
                        },
                        {
                            "text": "Please bring a set of colored <felts> to the art class.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, принесите набор цветных <фломастеров> на урок рисования."
                            }
                        }
                    ],
                    "synonyms": [
                        "marker",
                        "felt-tip"
                    ],
                    "common_phrases": [
                        "felt pen",
                        "felt tip"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often referred to as a felt-tip or marker"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ручка с пишущим наконечником из пористого материала; фломастер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фломастер",
                                "target_lang_sense_clarification": "инструмент для рисования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2155daaf-2872-3aee-8177-6071fd941a6d",
                    "sense_definition": "A hat made of felt material.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tipped his old brown <felt> as he walked by.",
                            "target_lang_translations": {
                                "ru": "Проходя мимо, он приподнял свою старую коричневую <фетровую шляпу>."
                            }
                        },
                        {
                            "text": "She wore a stylish grey <felt> to the winter event.",
                            "target_lang_translations": {
                                "ru": "Она надела стильную серую <фетровую шляпу> на зимнее мероприятие."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "referring to the hat simply as 'a felt' is less common in modern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шляпа, изготовленная из фетра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фетровая шляпа",
                                "target_lang_sense_clarification": "головной убор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10b1df6f-8045-30ff-8e22-2aaffd4d5a4d",
                    "sense_definition": "Acronym for fast-evolving luminous transient, a specific type of supernova.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Astronomers recently observed a <FELT> in a distant galaxy.",
                            "target_lang_translations": {
                                "ru": "Астрономы недавно наблюдали <FELT> в далекой галактике."
                            }
                        },
                        {
                            "text": "The study of <FELTs> helps scientists understand stellar explosions.",
                            "target_lang_translations": {
                                "ru": "Изучение <FELT> помогает ученым понять звездные взрывы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical acronym used in astronomy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акроним для \"fast-evolving luminous transient\" (быстро эволюционирующий светящийся транзиент), специфического типа сверхновой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "FELT",
                                "target_lang_sense_clarification": "тип астрономического явления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "014ca6bb-0ec9-36bf-aaab-56b8336b1d2d",
                    "sense_definition": "That has been experienced or perceived intensely or emotionally.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please accept my deeply <felt> apologies for the mistake.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, примите мои глубокие и <искренние> извинения за ошибку."
                            }
                        },
                        {
                            "text": "There is a <felt> need for better communication in the team.",
                            "target_lang_translations": {
                                "ru": "В команде существует <ощутимая> потребность в лучшей коммуникации."
                            }
                        }
                    ],
                    "synonyms": [
                        "perceived",
                        "experienced"
                    ],
                    "common_phrases": [
                        "deeply felt",
                        "felt need"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что интенсивно переживается или воспринимается; глубокий, осознанный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ощутимый",
                                "target_lang_sense_clarification": "явно воспринимаемый"
                            },
                            {
                                "target_lang_word": "искренний",
                                "target_lang_sense_clarification": "глубоко переживаемый (об эмоциях)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7aa6462d-9ed5-34c8-9706-a167a1fbffd6",
                    "sense_definition": "To make fibers into felt by matting and pressing them together.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "manufacturing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <felted> the wool layers to create a dense fabric.",
                            "target_lang_translations": {
                                "ru": "Она <сваляла> слои шерсти, чтобы создать плотную ткань."
                            }
                        },
                        {
                            "text": "The artisan <felts> the material by hand using hot water.",
                            "target_lang_translations": {
                                "ru": "Ремесленник <валяет> материал вручную, используя горячую воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "mat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превращать волокна в войлок или фетр путем сваливания и прессования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "валять",
                                "target_lang_sense_clarification": "изготавливать войлок/фетр"
                            },
                            {
                                "target_lang_word": "сваливать",
                                "target_lang_sense_clarification": "шерсть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e4ea017-306a-31d7-a5ca-6219aeb1bce9",
                    "sense_definition": "To cover an object with felt material.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "manufacturing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <felted> the roof to make it waterproof.",
                            "target_lang_translations": {
                                "ru": "Они <покрыли войлоком> крышу, чтобы сделать ее водонепроницаемой."
                            }
                        },
                        {
                            "text": "The cylinder was <felted> to provide insulation.",
                            "target_lang_translations": {
                                "ru": "Цилиндр был <обшит войлоком> для обеспечения изоляции."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать или обивать предмет слоем войлока или фетра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обивать войлоком",
                                "target_lang_sense_clarification": "покрывать материалом"
                            },
                            {
                                "target_lang_word": "покрывать фетром",
                                "target_lang_sense_clarification": "изолировать или декорировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69dd8101-21f8-3485-b279-22578433ddad",
                    "sense_definition": "To cause a player to lose all their chips in a gambling game.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <felted> after going all-in on a bad hand.",
                            "target_lang_translations": {
                                "ru": "Он <остался без фишек> после того, как пошел ва-банк с плохой картой."
                            }
                        },
                        {
                            "text": "The professional player <felted> his opponent in just three rounds.",
                            "target_lang_translations": {
                                "ru": "Профессиональный игрок <обыграл> своего противника всего за три раунда."
                            }
                        }
                    ],
                    "synonyms": [
                        "bankrupt"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used in poker contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить игрока проиграть все свои фишки в азартной игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обыграть",
                                "target_lang_sense_clarification": "лишить всех фишек"
                            },
                            {
                                "target_lang_word": "оставить без фишек",
                                "target_lang_sense_clarification": "слэнг в покере"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "2efb474c-409d-3df7-a4d3-7db87c2c7e8d",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <Felt> presented his findings to the committee.",
                            "target_lang_translations": {
                                "ru": "Мистер <Фелт> представил свои выводы комитету."
                            }
                        },
                        {
                            "text": "We are waiting for Ms. <Felt> to arrive.",
                            "target_lang_translations": {
                                "ru": "Мы ждем прибытия мисс <Фелт>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Фелт",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
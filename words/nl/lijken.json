    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "625366ca-3f28-3918-8472-de0fdd3fd901",
                    "sense_definition": "De indruk wekken iets te zijn of te doen; eruitzien alsof het zo is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "seem_appear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>lijkt</w> erop dat het morgen de hele dag gaat regenen.",
                            "target_lang_translations": {
                                "en": "It <w>looks</w> like it is going to rain all day tomorrow.",
                                "ru": "<w>Похоже</w>, что завтра весь день будет идти дождь."
                            }
                        },
                        {
                            "text": "Dat <w>lijkt</w> me een heel goed plan voor de vakantie.",
                            "target_lang_translations": {
                                "en": "That <w>seems</w> like a very good plan for the holiday to me.",
                                "ru": "Это <w>кажется</w> мне очень хорошим планом на отпуск."
                            }
                        }
                    ],
                    "synonyms": [
                        "schijnen",
                        "toeschijnen",
                        "overkomen"
                    ],
                    "common_phrases": [
                        "het lijkt erop",
                        "dat lijkt me",
                        "lijken te zijn",
                        "het lijkt wel"
                    ],
                    "target_lang_definitions": {
                        "en": "To give the impression of being or doing something; to appear as if it is so.",
                        "ru": "Производить впечатление, казаться кем-то или чем-то; выглядеть так, будто это правда."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seem",
                                "target_lang_sense_clarification": "general impression"
                            },
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "formal or visual impression"
                            },
                            {
                                "target_lang_word": "look like",
                                "target_lang_sense_clarification": "based on visual evidence"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "казаться",
                                "target_lang_sense_clarification": "производить впечатление"
                            },
                            {
                                "target_lang_word": "выглядеть",
                                "target_lang_sense_clarification": "иметь вид"
                            },
                            {
                                "target_lang_word": "представляться",
                                "target_lang_sense_clarification": "видеться, думаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e96edab-a28a-3dc4-98bb-ab56124f37d1",
                    "sense_definition": "Overeenkomst vertonen met iemand of iets; er hetzelfde uitzien als.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "resemble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn jongste broer <w>lijkt</w> sprekend op onze vader.",
                            "target_lang_translations": {
                                "en": "My youngest brother <w>looks</w> exactly like our father.",
                                "ru": "Мой младший брат как две капли воды <w>похож</w> на нашего отца."
                            }
                        },
                        {
                            "text": "Deze situatie <w>lijkt</w> erg op wat we vorig jaar hebben meegemaakt.",
                            "target_lang_translations": {
                                "en": "This situation <w>resembles</w> what we experienced last year a lot.",
                                "ru": "Эта ситуация очень <w>похожа</w> на то, что мы пережили в прошлом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "weghebben van"
                    ],
                    "antonyms": [
                        "verschillen"
                    ],
                    "common_phrases": [
                        "lijken op",
                        "sprekend lijken op",
                        "nergens op lijken"
                    ],
                    "target_lang_definitions": {
                        "en": "To show similarities with someone or something; to have the same appearance as.",
                        "ru": "Иметь сходство с кем-либо или чем-либо; выглядеть так же."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look like",
                                "target_lang_sense_clarification": "physical appearance"
                            },
                            {
                                "target_lang_word": "resemble",
                                "target_lang_sense_clarification": "formal similarity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть похожим",
                                "target_lang_sense_clarification": "иметь сходство"
                            },
                            {
                                "target_lang_word": "походить",
                                "target_lang_sense_clarification": "напоминать кого-то"
                            },
                            {
                                "target_lang_word": "смахивать",
                                "target_lang_sense_clarification": "разговорное: иметь сходство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f1c1a7d-a93f-3569-a685-7d8702fbae95",
                    "sense_definition": "(scheepvaart) Een lijk (touw) aan de rand van een zeil naaien.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_sew",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zeilmaker moest de oude zeilen opnieuw <w>lijken</w> voordat het seizoen begon.",
                            "target_lang_translations": {
                                "en": "The sailmaker had to <w>rope</w> the old sails again before the season started.",
                                "ru": "Парусный мастер должен был заново <w>обшить ликтросом</w> старые паруса перед началом сезона."
                            }
                        },
                        {
                            "text": "Hij heeft het grootzeil vakkundig <w>gelijkt</w> met stevig touw.",
                            "target_lang_translations": {
                                "en": "He skillfully <w>roped</w> the mainsail with sturdy rope.",
                                "ru": "Он искусно <w>обшил ликтросом</w> грот, используя прочную веревку."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "(Nautical) To sew a bolt rope to the edge of a sail.",
                        "ru": "(Морское) Пришивать ликтрос (веревку) к краю паруса."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rope",
                                "target_lang_sense_clarification": "nautical context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обшивать ликтросом",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        },

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "69f0bd66-49a1-31d0-87a7-0ba951cb1f5c",
                    "sense_definition": "An evergreen shrub or small tree with prickly, dark green leaves and bright red berries, often used as a decoration at Christmas.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hung a wreath made of <w>holly</w> on the front door to welcome guests.",
                            "target_lang_translations": {
                                "ru": "Мы повесили венок из <w>остролиста</w> на входную дверь, чтобы приветствовать гостей."
                            }
                        },
                        {
                            "text": "The birds love to eat the red berries from the <w>holly</w> in the garden during winter.",
                            "target_lang_translations": {
                                "ru": "Птицы любят клевать красные ягоды <w>падуба</w> в саду зимой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "holly wreath",
                        "holly berry",
                        "sprig of holly",
                        "holly bush"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вечнозеленый кустарник или небольшое дерево с колючими темно-зелеными листьями и ярко-красными ягодами, часто используемое для украшения на Рождество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остролист",

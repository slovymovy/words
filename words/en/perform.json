    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "def908f4-a420-3484-b9bd-4365dde4b1a9",
                    "sense_definition": "To present a play, piece of music, or other entertainment to an audience.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "entertainment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band will <w>perform</w> live at the stadium tonight.",
                            "target_lang_translations": {
                                "ru": "Сегодня вечером группа будет <w>выступать</w> вживую на стадионе."
                            }
                        },
                        {
                            "text": "She loves to <w>perform</w> on stage in front of a large crowd.",
                            "target_lang_translations": {
                                "ru": "Она любит <w>выступать</w> на сцене перед большой толпой."
                            }
                        }
                    ],
                    "synonyms": [
                        "act",
                        "play",
                        "present",
                        "stage"
                    ],
                    "common_phrases": [
                        "perform live",
                        "perform on stage",
                        "perform a song",
                        "perform a play"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представлять пьесу, музыкальное произведение или другое развлечение перед публикой"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выступать",
                                "target_lang_sense_clarification": "на сцене, перед публикой"
                            },
                            {
                                "target_lang_word": "исполнять",
                                "target_lang_sense_clarification": "произведение, песню, роль"
                            },
                            {
                                "target_lang_word": "играть",
                                "target_lang_sense_clarification": "роль или на инструменте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6646e24-aac3-3ede-8633-515bac13d7ad",
                    "sense_definition": "To carry out an action, task, or duty, especially one that requires care or skill.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surgeon <w>performed</w> the operation successfully.",
                            "target_lang_translations": {
                                "ru": "Хирург успешно <w>провел</w> операцию."
                            }
                        },
                        {
                            "text": "You must <w>perform</w> your duties with great care and attention.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>выполнять</w> свои обязанности с большой тщательностью и вниманием."
                            }
                        }
                    ],
                    "synonyms": [
                        "carry out",
                        "execute",
                        "do",
                        "accomplish",
                        "conduct"
                    ],
                    "antonyms": [
                        "neglect"
                    ],
                    "common_phrases": [
                        "perform a task",
                        "perform a duty",
                        "perform an experiment",
                        "perform surgery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять действие, задачу или обязанность, особенно требующую внимания или мастерства"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выполнять",
                                "target_lang_sense_clarification": "задачу, работу"
                            },
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "операцию, эксперимент"
                            },
                            {
                                "target_lang_word": "осуществлять",
                                "target_lang_sense_clarification": "формально: действие, замысел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0a42dad-ae1b-3c15-8aff-3075df649dce",
                    "sense_definition": "To work or function in a specific way or with a certain level of success.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new computer <w>performs</w> much faster than the old one.",
                            "target_lang_translations": {
                                "ru": "Новый компьютер <w>работает</w> намного быстрее старого."
                            }
                        },
                        {
                            "text": "Our team did not <w>perform</w> well in the last match.",
                            "target_lang_translations": {
                                "ru": "Наша команда не очень хорошо <w>выступила</w> в прошлом матче."
                            }
                        }
                    ],
                    "synonyms": [
                        "function",
                        "operate",
                        "run",
                        "work"
                    ],
                    "antonyms": [
                        "malfunction",
                        "fail"
                    ],
                    "common_phrases": [
                        "perform well",
                        "perform poorly",
                        "perform better"
                    ],
                    "target_lang_definitions": {
                        "ru": "Работать или функционировать определенным образом или с определенным уровнем успеха"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работать",
                                "target_lang_sense_clarification": "о механизмах или системах"
                            },
                            {
                                "target_lang_word": "показывать результаты",
                                "target_lang_sense_clarification": "об эффективности"
                            },
                            {
                                "target_lang_word": "выступать",
                                "target_lang_sense_clarification": "в спорте или состязании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b740472-d74b-398f-9abd-0145177b8830",
                    "sense_definition": "To fulfill the terms of a contract or formal agreement.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Both parties must <w>perform</w> their obligations under the contract.",
                            "target_lang_translations": {
                                "ru": "Обе стороны должны <w>исполнять</w> свои обязательства по контракту."
                            }
                        },
                        {
                            "text": "Failure to <w>perform</w> the agreement can lead to legal action.",
                            "target_lang_translations": {
                                "ru": "Неспособность <w>исполнить</w> соглашение может привести к судебному иску."
                            }
                        }
                    ],
                    "synonyms": [
                        "fulfill",
                        "execute",
                        "discharge",
                        "satisfy"
                    ],
                    "antonyms": [
                        "breach"
                    ],
                    "common_phrases": [
                        "perform a contract",
                        "perform an obligation"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Исполнять условия контракта или официального соглашения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исполнять",
                                "target_lang_sense_clarification": "обязательства, долг"
                            },
                            {
                                "target_lang_word": "выполнять",
                                "target_lang_sense_clarification": "условия договора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fc0e8e3-0daa-352b-847a-cdb511ffa072",
                    "sense_definition": "To behave in a theatrical way to create a specific impression or feign a quality.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He seemed to <w>perform</w> his grief for the cameras rather than truly feel it.",
                            "target_lang_translations": {
                                "ru": "Казалось, он <w>разыгрывал</w> свое горе на камеру, а не чувствовал его по-настоящему."
                            }
                        },
                        {
                            "text": "People often <w>perform</w> social roles without realizing they are doing so.",
                            "target_lang_translations": {
                                "ru": "Люди часто <w>играют</w> социальные роли, не осознавая этого."
                            }
                        }
                    ],
                    "synonyms": [
                        "feign",
                        "simulate",
                        "act out"
                    ],
                    "common_phrases": [
                        "perform a role",
                        "perform gender"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "academic or sociological"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя театрально, чтобы создать определенное впечатление или симулировать качество"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разыгрывать",
                                "target_lang_sense_clarification": "притворяться, изображать чувство"
                            },
                            {
                                "target_lang_word": "изображать",
                                "target_lang_sense_clarification": "кого-либо или что-либо"
                            },
                            {
                                "target_lang_word": "играть",
                                "target_lang_sense_clarification": "социальную роль"
                            }
                        ]
                    }
                }
            ]
        }
    ]

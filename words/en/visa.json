                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The official refused to <w>visa</w> the traveler's passport due to missing paperwork.",
                            "target_lang_translations": {
                                "ru": "Чиновник отказался <w>визировать</w> паспорт путешественника из-за отсутствия документов."
                            }

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "130c2e48-f9fe-3f01-b87d-e1311fdc131b",
                    "sense_definition": "Een eetbare vrucht met een kenmerkende vorm die van boven smal is en van onderen breed.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fruit_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik eet elke dag een verse <w>peer</w> bij het ontbijt.",
                            "target_lang_translations": {
                                "en": "I eat a fresh <w>pear</w> with breakfast every day.",
                                "ru": "Я съедаю свежую <w>грушу</w> каждый день за завтраком."
                            }
                        },
                        {
                            "text": "De supermarkt verkoopt heerlijke <w>peren</w> van lokale boomgaarden.",
                            "target_lang_translations": {
                                "en": "The supermarket sells delicious <w>pears</w> from local orchards.",
                                "ru": "Супермаркет продает вкусные <w>груши</w> из местных садов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een rijpe peer",
                        "een sappige peer",
                        "met de gebakken peren zitten"
                    ],
                    "target_lang_definitions": {
                        "en": "An edible fruit with a characteristic shape that is narrow at the top and wide at the bottom.",
                        "ru": "Съедобный фрукт характерной формы, сужающийся кверху и расширяющийся книзу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pear",
                                "target_lang_sense_clarification": "fruit"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "груша",
                                "target_lang_sense_clarification": "фрукт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d585ebbb-c7f0-3690-b8c4-6928fce1121c",
                    "sense_definition": "Een gloeilamp, vaak in de vorm van de gelijknamige vrucht.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>peertje</w> in de gang is kapot en moet vervangen worden.",

                        "batterij opladen",
                        "volledig opladen",
                        "accu opladen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать новой электрической энергией, например, батарею или аккумулятор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заряжать",

                {
                    "sense_id": "3fafc853-3e2a-3b88-a21b-48e3a398f1e7",
                    "sense_definition": "De harde, spiraalvormige schelp die een slak op zijn rug draagt ter bescherming.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De slak trok zich snel terug in zijn <w>huisje</w> toen ik hem aanraakte.",
                            "target_lang_translations": {

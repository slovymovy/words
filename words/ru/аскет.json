    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2bcf9519-da29-3369-8828-af9f7ea463bd",
                    "sense_definition": "Человек, который ведёт крайне строгий образ жизни, добровольно отказываясь от жизненных удовольствий, роскоши и материальных благ.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "А он настоящий <w>аскет</w>, всегда себя в строгости держит.",
                            "target_lang_translations": {
                                "en": "He is a true <w>ascetic</w>, always keeping himself under strict discipline."
                            }
                        },
                        {
                            "text": "Трудоголик и <w>аскет</w> ― так будут вспоминать этого человека после смерти.",
                            "target_lang_translations": {
                                "en": "A workaholic and an <w>ascetic</w> ― that is how this man will be remembered after death."
                            }
                        }
                    ],
                    "synonyms": [
                        "воздержанник"
                    ],
                    "antonyms": [
                        "гедонист",
                        "сибарит",
                        "жизнелюб",
                        "эпикуреец"
                    ],
                    "common_phrases": [
                        "строгий аскет",
                        "настоящий аскет",
                        "жить как аскет"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who leads an extremely strict lifestyle, voluntarily giving up life's pleasures, luxury, and material goods."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ascetic",
                                "target_lang_sense_clarification": "person practicing self-denial"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05965aa6-cbaf-3f8b-a87a-910e30c989fe",
                    "sense_definition": "Христианский подвижник или монах, проводящий жизнь в строгом воздержании, посте и молитвах.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Древний <w>аскет</w> поселился в глубокой пещере вдали от людей.",
                            "target_lang_translations": {
                                "en": "An ancient <w>ascetic</w> settled in a deep cave far from people."
                            }
                        },
                        {
                            "text": "Молодой послушник решил стать <w>аскетом</w> и посвятить свою жизнь служению Богу.",
                            "target_lang_translations": {
                                "en": "The young novice decided to become an <w>ascetic</w> and dedicate his life to serving God."
                            }
                        }
                    ],
                    "synonyms": [
                        "отшельник",
                        "подвижник",
                        "схимник"
                    ],
                    "common_phrases": [
                        "религиозный аскет",
                        "великий аскет"
                    ],
                    "target_lang_definitions": {
                        "en": "A Christian ascetic or monk living a life of strict abstinence, fasting, and prayer."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ascetic",
                                "target_lang_sense_clarification": "religious practitioner"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
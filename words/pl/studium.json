                {
                    "sense_id": "3a4059be-f0ea-38ac-bd76-bd29821045cd",
                    "sense_definition": "Pisemne, naukowe lub literackie opracowanie konkretnego zagadnienia, będące wynikiem wnikliwych badań i analiz.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "group_analysis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Profesor opublikował obszerne <w>studium</w> na temat architektury gotyckiej w Polsce.",
                            "target_lang_translations": {
                                "ru": "Профессор опубликовал обширное <w>исследование</w> на тему готической архитектуры в Польше."
                            }
                        },
                        {
                            "text": "To szczegółowe <w>studium</w> przypadku pokazuje, jak firma poradziła sobie z kryzysem.",
                            "target_lang_translations": {
                                "ru": "Это подробное <w>исследование</w> конкретного случая показывает, как фирма справилась с кризисом."
                            }

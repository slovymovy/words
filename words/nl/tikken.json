{
    "word_family": [
        "tik",
        "tikker",
        "tikkertje",
        "getik"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8978c40c-7756-39de-913c-c82eafca852a",
                    "sense_definition": "Een kort, regelmatig en droog geluid maken, zoals een mechanisch uurwerk.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kon gisteravond niet slapen door het <w>tikken</w> van de wekker.",
                            "target_lang_translations": {
                                "en": "I couldn't sleep last night because of the <w>ticking</w> of the alarm clock.",
                                "ru": "Я не мог уснуть вчера вечером из-за <w>тиканья</w> будильника."
                            }
                        },
                        {
                            "text": "Je hoorde alleen de regen zachtjes tegen het raam <w>tikken</w>.",
                            "target_lang_translations": {
                                "en": "You could only hear the rain gently <w>tapping</w> against the window.",
                                "ru": "Было слышно только, как дождь тихо <w>стучит</w> в окно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het tikken van de klok",
                        "zachtjes tikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a short, regular, and dry sound, like a mechanical clock.",
                        "ru": "Издавать короткий, регулярный и сухой звук, как механические часы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tick",
                                "target_lang_sense_clarification": "make a rhythmic sound like a clock"
                            },
                            {
                                "target_lang_word": "tap",
                                "target_lang_sense_clarification": "make a light striking sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тикать",
                                "target_lang_sense_clarification": "о часах или механизме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73f1a1da-730d-3d7c-a489-a427b50130c0",
                    "sense_definition": "Zachtjes ergens op slaan of kloppen, vaak met de vingertoppen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "touch_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar <w>tikte</w> met zijn pen op het bord om de aandacht te vragen.",
                            "target_lang_translations": {
                                "en": "The teacher <w>tapped</w> his pen on the board to ask for attention.",
                                "ru": "Учитель <w>постучал</w> ручкой по доске, чтобы привлечь внимание."
                            }
                        },
                        {
                            "text": "Je moet niet op het glas <w>tikken</w>, want dat is storend voor de vissen.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>tap</w> on the glass, because that disturbs the fish.",
                                "ru": "Не нужно <w>стучать</w> по стеклу, потому что это беспокоит рыб."
                            }
                        }
                    ],
                    "synonyms": [
                        "typen"
                    ],
                    "common_phrases": [
                        "op de schouder tikken",
                        "op het raam tikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To hit or knock gently on something, often with the fingertips.",
                        "ru": "Легонько ударять или стучать по чему-либо, часто кончиками пальцев."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tap",
                                "target_lang_sense_clarification": "strike lightly"
                            },
                            {
                                "target_lang_word": "pat",
                                "target_lang_sense_clarification": "touch gently with the flat of the hand"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "постукивать",
                                "target_lang_sense_clarification": "легко ударять пальцами"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "издавать звук удара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b25aa5f7-0403-3b43-be6a-3f863d5996bf",
                    "sense_definition": "Tekst invoeren door toetsen in te drukken op een toetsenbord of schrijfmachine.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "write_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun jij deze brief even snel voor mij <w>tikken</w>?",
                            "target_lang_translations": {
                                "en": "Can you quickly <w>type</w> this letter for me?",
                                "ru": "Ты можешь быстро <w>напечатать</w> это письмо для меня?"
                            }
                        },
                        {
                            "text": "Hij zat de hele avond een verslag te <w>tikken</w> op zijn laptop.",
                            "target_lang_translations": {
                                "en": "He sat <w>typing</w> a report on his laptop all evening.",
                                "ru": "Он весь вечер сидел и <w>печатал</w> отчет на своем ноутбуке."
                            }
                        }
                    ],
                    "synonyms": [
                        "typen"
                    ],
                    "common_phrases": [
                        "een brief tikken",
                        "een verslag tikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To enter text by pressing keys on a keyboard or typewriter.",
                        "ru": "Вводить текст, нажимая клавиши на клавиатуре или пишущей машинке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "type",
                                "target_lang_sense_clarification": "write using a keyboard"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "печатать",
                                "target_lang_sense_clarification": "на клавиатуре"
                            },
                            {
                                "target_lang_word": "набирать",
                                "target_lang_sense_clarification": "текст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e39a0923-53f2-3f38-99f3-d14b7f95be48",
                    "sense_definition": "Iemand aanraken in een kinderspel waarbij de ander daarna de vanger is.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "touch_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen renden hard weg om niet <w>getikt</w> te worden.",
                            "target_lang_translations": {
                                "en": "The children ran away fast to avoid being <w>tagged</w>.",
                                "ru": "Дети быстро убегали, чтобы их не <w>осалили</w>."
                            }
                        },
                        {
                            "text": "Jij bent hem, want ik heb je net <w>getikt</w>!",
                            "target_lang_translations": {
                                "en": "You're it, because I just <w>tagged</w> you!",
                                "ru": "Ты водишь, потому что я тебя только что <w>осалил</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "iemand tikken",
                        "tikkertje spelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To touch someone in a children's game where the other person becomes the catcher.",
                        "ru": "Дотрагиваться до кого-то в детской игре, после чего этот человек становится водящим."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tag",
                                "target_lang_sense_clarification": "touch a player in a game"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "осалить",
                                "target_lang_sense_clarification": "в игре в пятнашки"
                            },
                            {
                                "target_lang_word": "запятнать",
                                "target_lang_sense_clarification": "коснуться в игре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72efbe9f-63c7-390f-a58b-a98911f4613d",
                    "sense_definition": "Iets afvinken op een lijst of iets snel verwerken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben alle punten op de agenda kunnen <w>tikken</w> tijdens de vergadering.",
                            "target_lang_translations": {
                                "en": "We were able to <w>tick off</w> all the items on the agenda during the meeting.",
                                "ru": "Мы смогли <w>пройти</w> все пункты повестки дня во время собрания."
                            }
                        },
                        {
                            "text": "Hij zat in de bar een paar biertjes weg te <w>tikken</w>.",
                            "target_lang_translations": {
                                "en": "He was sitting in the bar <w>knocking back</w> a few beers.",
                                "ru": "Он сидел в баре и <w>опрокидывал</w> пару бокалов пива."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een biertje tikken"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "vooral in de betekenis van drinken of snel afwerken"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To check something off a list or to process/consume something quickly.",
                        "ru": "Отмечать что-то в списке или быстро с чем-то справляться (например, выпивать)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tick off",
                                "target_lang_sense_clarification": "mark items on a list"
                            },
                            {
                                "target_lang_word": "knock back",
                                "target_lang_sense_clarification": "drink quickly (colloquial)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выпивать",
                                "target_lang_sense_clarification": "разг., быстро пить алкоголь"
                            },
                            {
                                "target_lang_word": "отмечать",
                                "target_lang_sense_clarification": "пункты в списке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c6ebbe49-a3e4-31a6-9498-330da831952a",
                    "sense_definition": "Meervoud van <w>tik</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kreeg een paar flinke <w>tikken</w> op zijn vingers.",
                            "target_lang_translations": {
                                "en": "He got a few good <w>raps</w> on his knuckles.",
                                "ru": "Он получил пару сильных <w>ударов</w> по пальцам."
                            }
                        },
                        {
                            "text": "De klok gaf zachte <w>tikken</w> in de stille kamer.",
                            "target_lang_translations": {
                                "en": "The clock gave soft <w>ticks</w> in the quiet room.",
                                "ru": "Часы издавали тихие <w>щелчки</w> в тихой комнате."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een paar tikken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'tik' (a light blow or sound).",
                        "ru": "Множественное число от слова 'tik' (легкий удар или звук)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rap",
                                "target_lang_sense_clarification": "sharp, light blow"
                            },
                            {
                                "target_lang_word": "slap",
                                "target_lang_sense_clarification": "blow with flat hand"
                            },
                            {
                                "target_lang_word": "tick",
                                "target_lang_sense_clarification": "short, dry sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "удары",
                                "target_lang_sense_clarification": "физические шлепки"
                            },
                            {
                                "target_lang_word": "щелчки",
                                "target_lang_sense_clarification": "звуки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
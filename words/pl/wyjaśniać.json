    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e432d756-71c0-3f03-ba66-d8599f5de3a2",
                    "sense_definition": "Sprawiać, że coś staje się zrozumiałe i jasne dla kogoś; przekazywać wiedzę lub informacje na dany temat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "explain_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel cierpliwie <w>wyjaśniał</w> uczniom nowe zasady gramatyki.",
                            "target_lang_translations": {
                                "ru": "Учитель терпеливо <w>объяснял</w> ученикам новые правила грамматики."
                            }
                        },
                        {
                            "text": "Czy możesz mi <w>wyjaśnić</w>, jak działa to urządzenie?",
                            "target_lang_translations": {
                                "ru": "Ты можешь мне <w>объяснить</w>, как работает это устройство?"
                            }
                        }
                    ],
                    "synonyms": [
                        "tłumaczyć",
                        "objaśniać",
                        "interpretować",
                        "czynić jasnym"
                    ],
                    "antonyms": [
                        "zaciemniać",
                        "mylić",
                        "wprowadzać w błąd"
                    ],
                    "common_phrases": [
                        "wyjaśniać znaczenie",
                        "wyjaśniać zasady",
                        "wyjaśniać problem",
                        "wyjaśniać sytuację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо понятным и ясным для кого-то; передавать знания или информацию по данной теме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объяснять",
                                "target_lang_sense_clarification": "делать понятным"
                            },
                            {
                                "target_lang_word": "разъяснять",
                                "target_lang_sense_clarification": "растолковывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c56ac34-32a2-362a-9dad-6008c4406953",
                    "sense_definition": "Podawać powody swojego lub czyjegoś zachowania, często w celu obrony przed zarzutami.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "justify",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracownik długo <w>wyjaśniał</w> szefowi przyczyny swojego spóźnienia.",
                            "target_lang_translations": {
                                "ru": "Сотрудник долго <w>объяснял</w> начальнику причины своего опоздания."
                            }
                        },
                        {
                            "text": "Nie musisz mi <w>wyjaśniać</w> swojej decyzji, bo doskonale ją rozumiem.",
                            "target_lang_translations": {
                                "ru": "Тебе не нужно <w>объяснять</w> мне своё решение, так как я прекрасно его понимаю."
                            }
                        }
                    ],
                    "synonyms": [
                        "uzasadniać",
                        "usprawiedliwiać",
                        "tłumaczyć",
                        "podawać szczegóły"
                    ],
                    "common_phrases": [
                        "wyjaśniać przyczyny",
                        "wyjaśniać powody",
                        "wyjaśniać zachowanie",
                        "wyjaśniać nieporozumienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить причины своего или чужого поведения, часто с целью защиты от обвинений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объяснять",
                                "target_lang_sense_clarification": "приводить доводы"
                            },
                            {
                                "target_lang_word": "оправдываться",
                                "target_lang_sense_clarification": "защищать себя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "058632db-ed3c-38f1-9d31-c95f34c96557",
                    "sense_definition": "O sytuacji, problemie lub zagadce: stawać się zrozumiałym, rozwiązywać się (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "become_clear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po rozmowie z policją cała tajemnicza sprawa powoli się <w>wyjaśniała</w>.",
                            "target_lang_translations": {
                                "ru": "После разговора с полицией всё это таинственное дело понемногу <w>прояснялось</w>."
                            }
                        },
                        {
                            "text": "Wszystko się <w>wyjaśni</w>, gdy otrzymamy oficjalne wyniki badań.",
                            "target_lang_translations": {
                                "ru": "Всё <w>выяснится</w>, когда мы получим официальные результаты анализов."
                            }
                        }
                    ],
                    "antonyms": [
                        "zaciemniać"
                    ],
                    "common_phrases": [
                        "sytuacja się wyjaśnia",
                        "sprawa się wyjaśnia",
                        "wszystko się wyjaśnia"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "z zaimkiem \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О ситуации, проблеме или загадке: становиться понятным, разрешаться (используется с местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проясняться",
                                "target_lang_sense_clarification": "становиться ясным"
                            },
                            {
                                "target_lang_word": "выясняться",
                                "target_lang_sense_clarification": "обнаруживаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fe35947-ec39-3b9c-98f7-40412b29a77a",
                    "sense_definition": "O niebie lub pogodzie: stawać się pogodnym, bezchmurnym (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po gwałtownej burzy niebo zaczęło się <w>wyjaśniać</w> i wyszło słońce.",
                            "target_lang_translations": {
                                "ru": "После сильной грозы небо начало <w>проясняться</w> и выглянуло солнце."
                            }
                        },
                        {
                            "text": "Mamy nadzieję, że do południa pogoda się <w>wyjaśni</w>.",
                            "target_lang_translations": {
                                "ru": "Мы надеемся, что к полудню погода <w>прояснится</w>."
                            }

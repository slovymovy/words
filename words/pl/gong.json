{
    "word_family": [],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a92e69a6-9a3a-3b7f-9ccb-859b0c126de3",
                    "sense_definition": "Instrument perkusyjny w postaci dużej, metalowej tarczy, która wydaje donośny, wibrujący dźwięk po uderzeniu pałką.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "instrument_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mnich uderzył w mosiężny <w>gong</w>, wzywając wszystkich na wieczorną modlitwę.",
                            "target_lang_translations": {
                                "ru": "Монах ударил в медный <w>гонг</w>, созывая всех на вечернюю молитву."
                            }
                        },
                        {
                            "text": "W orkiestrze symfonicznej <w>gong</w> jest używany do budowania napięcia w utworze.",
                            "target_lang_translations": {
                                "ru": "В симфоническом оркестре <w>гонг</w> используется для нагнетания напряжения в произведении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uderzyć w gong",
                        "gong obiadowy",
                        "wielki gong"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ударный музыкальный инструмент в виде большого металлического диска, издающий громкий вибрирующий звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гонг",
                                "target_lang_sense_clarification": "музыкальный инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7c866e8-8e9c-3ec0-a757-bcef6f24925d",
                    "sense_definition": "Dźwięk wydawany przez ten instrument lub podobny sygnał dźwiękowy, używany np. w teatrze, szkole lub podczas walki bokserskiej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "instrument_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Usłyszeliśmy trzeci <w>gong</w> i szybko zajęliśmy swoje miejsca na widowni.",
                            "target_lang_translations": {
                                "ru": "Мы услышали третий <w>звонок</w> и быстро заняли свои места в зрительном зале."
                            }
                        },
                        {
                            "text": "Sędzia przerwał walkę, gdy tylko rozbrzmiał końcowy <w>gong</w>.",
                            "target_lang_translations": {
                                "ru": "Судья остановил бой, как только прозвучал финальный <w>гонг</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sygnał",
                        "dźwięk"
                    ],
                    "common_phrases": [
                        "pierwszy gong",
                        "gong na przerwę",
                        "usłyszeć gong"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук этого инструмента или похожий звуковой сигнал, используемый, например, в театре, школе или во время боксерского поединка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гонг",
                                "target_lang_sense_clarification": "сигнал в спорте или телешоу"
                            },
                            {
                                "target_lang_word": "звонок",
                                "target_lang_sense_clarification": "сигнал в театре или школе"
                            },
                            {
                                "target_lang_word": "сигнал",
                                "target_lang_sense_clarification": "звуковое оповещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68bda759-42d2-3496-a4ea-437403764c15",
                    "sense_definition": "Silne uderzenie, zazwyczaj zadane pięścią w głowę.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopak dostał mocnego <w>gonga</w> w nos i przewrócił się na ziemię.",
                            "target_lang_translations": {
                                "ru": "Парень получил сильный <w>удар</w> в нос и упал на землю."
                            }
                        },
                        {
                            "text": "Uważaj na słowa, bo zaraz możesz wyłapać <w>gonga</w>.",
                            "target_lang_translations": {
                                "ru": "Следи за словами, а то сейчас можешь получить <w>удар</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cios",
                        "uderzenie"
                    ],
                    "common_phrases": [
                        "sprzedać gonga",
                        "dostać gonga",
                        "wyłapać gonga"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "używane w mowie potocznej środowiskowej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный удар, обычно наносимый кулаком в голову (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "физическое воздействие"
                            },
                            {
                                "target_lang_word": "затрещина",
                                "target_lang_sense_clarification": "разговорное/сленг"
                            },
                            {
                                "target_lang_word": "плюха",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "a367ae79-7682-3c23-8750-5728e0aeb57a",
                    "sense_definition": "Verbogen vorm van <w>tegengesteld</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auto's reden in <w>tegengestelde</w> richting over de snelweg.",
                            "target_lang_translations": {
                                "ru": "Машины ехали в <w>противоположном</w> направлении по шоссе."
                            }
                        },
                        {
                            "text": "Mijn broer en ik hebben vaak <w>tegengestelde</w> belangen.",
                            "target_lang_translations": {
                                "ru": "У нас с братом часто <w>противоположные</w> интересы."
                            }

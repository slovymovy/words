{
    "word_family": [
        "kneuterig",
        "kneuterigheid",
        "kneuter"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3a6d5ef0-37de-39cc-a203-e75fec999c71",
                    "sense_definition": "Gezellig en ontspannen met elkaar praten of bezig zijn, vaak in een huiselijke sfeer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op zondagochtend zitten we graag samen aan de ontbijttafel te <w>kneuteren</w>.",
                            "target_lang_translations": {
                                "en": "On Sunday mornings we like to sit together at the breakfast table and <w>chat cozily</w>."
                            }
                        },
                        {
                            "text": "De vriendinnen waren de hele avond gezellig aan het <w>kneuteren</w> met een kop thee.",
                            "target_lang_translations": {
                                "en": "The friends were <w>nattering</w> cozily all evening with a cup of tea."
                            }
                        }
                    ],
                    "synonyms": [
                        "keuvelen",
                        "babbelen",
                        "kletsen"
                    ],
                    "common_phrases": [
                        "lekker zitten kneuteren",
                        "wat af kneuteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To talk or occupy oneself in a cozy and relaxed manner, often in a domestic atmosphere."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chat cozily",
                                "target_lang_sense_clarification": "social interaction"
                            },
                            {
                                "target_lang_word": "potter about",
                                "target_lang_sense_clarification": "doing small, cozy activities"
                            },
                            {
                                "target_lang_word": "natter",
                                "target_lang_sense_clarification": "informal chatting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a370dd84-b15c-3cc4-9798-7a6bdba29b85",
                    "sense_definition": "Op een zachte, binnensmondse manier klagen of mopperen over onbelangrijke dingen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "complaining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude man zat zachtjes te <w>kneuteren</w> over het koude weer buiten.",
                            "target_lang_translations": {
                                "en": "The old man sat softly <w>grumbling</w> about the cold weather outside."
                            }
                        },
                        {
                            "text": "Zij heeft altijd wel iets te <w>kneuteren</w> als de plannen onverwachts veranderen.",
                            "target_lang_translations": {
                                "en": "She always has something to <w>mutter</w> about when plans change unexpectedly."
                            }
                        }
                    ],
                    "synonyms": [
                        "mopperen",
                        "pruttelen",
                        "morren"
                    ],
                    "common_phrases": [
                        "wat te kneuteren",
                        "lopen te kneuteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To complain or grumble in a soft, mumbling way about unimportant things."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grumble",
                                "target_lang_sense_clarification": "complaining in a low voice"
                            },
                            {
                                "target_lang_word": "mutter",
                                "target_lang_sense_clarification": "speaking indistinctly"
                            },
                            {
                                "target_lang_word": "grouse",
                                "target_lang_sense_clarification": "complaining"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18592c97-03ed-389c-8e63-76fc12852b72",
                    "sense_definition": "Het maken van een zingend of kwinkelend geluid door vogels, specifiek de kneu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_sounds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de boomgaard hoorde ik een vogel vrolijk <w>kneuteren</w>.",
                            "target_lang_translations": {
                                "en": "In the orchard I heard a bird <w>twittering</w> cheerfully."
                            }
                        },
                        {
                            "text": "Het vogeltje begon te <w>kneuteren</w> zodra de zon opkwam.",
                            "target_lang_translations": {
                                "en": "The little bird started to <w>warble</w> as soon as the sun came up."
                            }
                        }
                    ],
                    "synonyms": [
                        "kwinkeleren",
                        "fluiten"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a singing or warbling sound (of birds, specifically the linnet)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "twitter",
                                "target_lang_sense_clarification": "light bird sound"
                            },
                            {
                                "target_lang_word": "warble",
                                "target_lang_sense_clarification": "melodic bird sound"
                            },
                            {
                                "target_lang_word": "chirp",
                                "target_lang_sense_clarification": "short bird sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef8446e8-944c-388f-8c6e-5b5dc5a56a64",
                    "sense_definition": "Kreukels krijgen of niet glad blijven zitten, vaak gezegd van kleding of stof.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "material_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die linnen broek begint snel te <w>kneuteren</w> als je hem een tijdje draagt.",
                            "target_lang_translations": {
                                "en": "Those linen trousers start to <w>crease</w> quickly when you wear them for a while."
                            }
                        },
                        {
                            "text": "Let op dat de stof niet gaat <w>kneuteren</w> tijdens het wassen.",
                            "target_lang_translations": {
                                "en": "Make sure the fabric doesn't <w>wrinkle</w> during washing."
                            }
                        }
                    ],
                    "synonyms": [
                        "kreuken"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become creased or lose smoothness, often said of clothing or fabric."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crease",
                                "target_lang_sense_clarification": "forming lines in fabric"
                            },
                            {
                                "target_lang_word": "wrinkle",
                                "target_lang_sense_clarification": "becoming crumpled"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
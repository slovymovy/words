{
    "word_family": [
        "voorwaarde",
        "voorwaardelijkheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "07f215c5-e7d7-3fe8-b996-8692d70c22a1",
                    "sense_definition": "Betrekking hebbend op een straf die niet ten uitvoer wordt gelegd als de veroordeelde zich gedurende een proeftijd goed gedraagt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter legde de man een <w>voorwaardelijke</w> gevangenisstraf op.",
                            "target_lang_translations": {
                                "en": "The judge imposed a <w>suspended</w> prison sentence on the man."
                            }
                        },
                        {
                            "text": "Bij een <w>voorwaardelijke</w> straf hoef je niet direct de cel in, tenzij je opnieuw in de fout gaat.",
                            "target_lang_translations": {
                                "en": "With a <w>suspended</w> sentence, you do not have to go to jail immediately, unless you reoffend."
                            }
                        }
                    ],
                    "antonyms": [
                        "onvoorwaardelijk"
                    ],
                    "common_phrases": [
                        "voorwaardelijke straf",
                        "voorwaardelijke invrijheidstelling",
                        "voorwaardelijk sepot"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a sentence that is not executed if the convicted person behaves well during a probationary period."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suspended",
                                "target_lang_sense_clarification": "legal context, regarding sentences"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d737f8d-6c20-3b90-8a52-ecd0adc85f5c",
                    "sense_definition": "Afhankelijk van bepaalde eisen of omstandigheden; niet absoluut.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kreeg <w>voorwaardelijke</w> toestemming om de documenten in te zien.",
                            "target_lang_translations": {
                                "en": "He received <w>conditional</w> permission to view the documents."
                            }
                        },
                        {
                            "text": "De verkoop van het huis is nog <w>voorwaardelijk</w> en hangt af van de financiering.",
                            "target_lang_translations": {
                                "en": "The sale of the house is still <w>conditional</w> and depends on the financing."
                            }
                        }
                    ],
                    "synonyms": [
                        "conditioneel"
                    ],
                    "antonyms": [
                        "onvoorwaardelijk"
                    ],
                    "common_phrases": [
                        "voorwaardelijke goedkeuring",
                        "voorwaardelijk opzet",
                        "voorwaardelijke toezegging"
                    ],
                    "target_lang_definitions": {
                        "en": "Dependent on certain requirements or circumstances; not absolute."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conditional",
                                "target_lang_sense_clarification": "dependent on conditions"
                            },
                            {
                                "target_lang_word": "provisional",
                                "target_lang_sense_clarification": "subject to change"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e99c7bb-060b-3947-9716-3ecf0b0b2983",
                    "sense_definition": "Verwijzend naar de wijs van een werkwoord die een voorwaarde, veronderstelling of mogelijkheid uitdrukt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de <w>voorwaardelijke</w> wijs gebruiken we vaak het werkwoord 'zouden'.",
                            "target_lang_translations": {
                                "en": "In the <w>conditional</w> mood, we often use the verb 'would'."
                            }
                        },
                        {
                            "text": "Deze zin staat in de <w>voorwaardelijke</w> vorm om een hypothetische situatie te beschrijven.",
                            "target_lang_translations": {
                                "en": "This sentence is in the <w>conditional</w> form to describe a hypothetical situation."
                            }
                        }
                    ],
                    "synonyms": [
                        "conditioneel"
                    ],
                    "common_phrases": [
                        "voorwaardelijke wijs",
                        "voorwaardelijke zin"
                    ],
                    "target_lang_definitions": {
                        "en": "Referring to the verb mood expressing a condition, supposition, or possibility."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conditional",
                                "target_lang_sense_clarification": "grammar"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "synonyms": [
                        "commentary"
                    ],
                    "common_phrases": [
                        "television editorial",
                        "broadcast editorial"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комментарий, транслируемый по радио или телевидению, выражающий мнение станции по какому-либо вопросу."
                    },
                    "translations": {
                        "ru": [
                            {

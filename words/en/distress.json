{
    "word_family": [
        "distressed",
        "distressing",
        "distressful",
        "distressingly"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3b94fd14-ca7b-33cc-b424-c6558b8f364e",
                    "sense_definition": "Great mental or physical pain, suffering, or anxiety.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "suffering_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The news of the accident caused the family great <w>distress</w>.",
                            "target_lang_translations": {
                                "ru": "Известие об аварии причинило семье большое <w>горе</w>."
                            }
                        },
                        {
                            "text": "She was in obvious <w>distress</w> after hearing the bad news.",
                            "target_lang_translations": {
                                "ru": "Она испытывала явное <w>страдание</w>, услышав плохие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "anguish",
                        "suffering",
                        "misery",
                        "grief",
                        "torment",
                        "sorrow",
                        "pain"
                    ],
                    "antonyms": [
                        "pleasure",
                        "calmness"
                    ],
                    "common_phrases": [
                        "in distress",
                        "great distress",
                        "emotional distress",
                        "cause distress"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильная душевная или физическая боль, страдание или глубокое горе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страдание",
                                "target_lang_sense_clarification": "физическая или душевная боль"
                            },
                            {
                                "target_lang_word": "горе",
                                "target_lang_sense_clarification": "глубокая печаль, несчастье"
                            },
                            {
                                "target_lang_word": "душевная боль",
                                "target_lang_sense_clarification": "эмоциональное потрясение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec15e12c-a879-3219-8657-ff27427a2554",
                    "sense_definition": "A situation in which a ship, plane, or person is in extreme danger and needs immediate help.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain sent a <w>distress</w> signal when the engines failed during the storm.",
                            "target_lang_translations": {
                                "ru": "Капитан подал сигнал <w>бедствия</w>, когда во время шторма отказали двигатели."
                            }
                        },
                        {
                            "text": "The coast guard spotted a ship in <w>distress</w> near the rocky shore.",
                            "target_lang_translations": {
                                "ru": "Береговая охрана заметила судно, терпящее <w>бедствие</w> у скалистого берега."
                            }
                        }
                    ],
                    "synonyms": [
                        "danger",
                        "peril",
                        "jeopardy",
                        "emergency"
                    ],
                    "common_phrases": [
                        "distress signal",
                        "distress call",
                        "ship in distress"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация крайней опасности, требующая немедленной помощи (обычно о судах или самолетах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бедствие",
                                "target_lang_sense_clarification": "катастрофическая ситуация, авария"
                            },
                            {
                                "target_lang_word": "беда",
                                "target_lang_sense_clarification": "опасное положение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2aba9f5d-de1d-3a44-b512-2c7d38f5f482",
                    "sense_definition": "The legal seizure of property to enforce the payment of a debt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_seizure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The landlord threatened <w>distress</w> of goods if the rent was not paid immediately.",
                            "target_lang_translations": {
                                "ru": "Арендодатель угрожал <w>наложением ареста</w> на имущество, если арендная плата не будет внесена немедленно."
                            }
                        },
                        {
                            "text": "Laws regarding <w>distress</w> vary significantly between different jurisdictions.",
                            "target_lang_translations": {
                                "ru": "Законы касательно <w>изъятия имущества</w> существенно различаются в разных юрисдикциях."
                            }
                        }
                    ],
                    "synonyms": [
                        "seizure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридическое изъятие (арест) имущества для обеспечения уплаты долга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наложение ареста",
                                "target_lang_sense_clarification": "юридическая процедура"
                            },
                            {
                                "target_lang_word": "конфискация",
                                "target_lang_sense_clarification": "изъятие имущества"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3246cba5-0f7e-32f6-966b-6bdf190471e4",
                    "sense_definition": "To make someone feel very upset, worried, or anxious.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "suffering_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It would deeply <w>distress</w> him to know that you are leaving so soon.",
                            "target_lang_translations": {
                                "ru": "Его бы глубоко <w>огорчило</w>, если бы он узнал, что вы уезжаете так скоро."
                            }
                        },
                        {
                            "text": "Please do not <w>distress</w> yourself over problems that you cannot control.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>изводи</w> себя из-за проблем, которые ты не можешь контролировать."
                            }
                        }
                    ],
                    "synonyms": [
                        "upset",
                        "trouble",
                        "worry",
                        "grieve",
                        "torment"
                    ],
                    "common_phrases": [
                        "deeply distress",
                        "distress someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-либо чувствовать себя очень расстроенным, обеспокоенным или тревожным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расстраивать",
                                "target_lang_sense_clarification": "вызывать огорчение"
                            },
                            {
                                "target_lang_word": "огорчать",
                                "target_lang_sense_clarification": "причинять горе"
                            },
                            {
                                "target_lang_word": "тревожить",
                                "target_lang_sense_clarification": "вызывать беспокойство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be2af683-a346-3df5-816c-984462c891ec",
                    "sense_definition": "To treat a new object, such as furniture or clothing, to give it an appearance of age and wear.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used sandpaper to <w>distress</w> the wooden table for a rustic look.",
                            "target_lang_translations": {
                                "ru": "Она использовала наждачную бумагу, чтобы <w>состарить</w> деревянный стол для придания ему деревенского вида."
                            }
                        },
                        {
                            "text": "Many fashion brands <w>distress</w> their denim jeans to make them look vintage.",
                            "target_lang_translations": {
                                "ru": "Многие модные бренды <w>искусственно старят</w> свои джинсы, чтобы они выглядели винтажными."
                            }
                        }
                    ],
                    "synonyms": [
                        "age",
                        "antique"
                    ],
                    "common_phrases": [
                        "distress furniture",
                        "distress wood",
                        "distress jeans"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обрабатывать новый предмет (мебель, одежду), чтобы придать ему вид старого и изношенного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старить",
                                "target_lang_sense_clarification": "придавать старый вид"
                            },
                            {
                                "target_lang_word": "состаривать",
                                "target_lang_sense_clarification": "искусственно делать старым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ddf79f6-991a-33a1-837c-50cdce7a181e",
                    "sense_definition": "To seize someone's property legally in order to obtain payment for a debt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_seizure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The authorities had to <w>distress</w> his assets to cover the unpaid taxes.",
                            "target_lang_translations": {
                                "ru": "Властям пришлось <w>наложить арест</w> на его активы, чтобы покрыть неуплаченные налоги."
                            }
                        },
                        {
                            "text": "It is rare to <w>distress</w> a tenant's personal items for rent arrears nowadays.",
                            "target_lang_translations": {
                                "ru": "В наши дни редко <w>изымают</w> личные вещи жильца за задолженность по аренде."
                            }
                        }
                    ],
                    "synonyms": [
                        "distrain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Законно изымать чье-либо имущество для получения уплаты долга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накладывать арест",
                                "target_lang_sense_clarification": "на имущество"
                            },
                            {
                                "target_lang_word": "изымать",
                                "target_lang_sense_clarification": "забирать за долги"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
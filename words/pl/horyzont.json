    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ccf3d0dc-3d99-3ce6-a363-b8aa59c82a3a",
                    "sense_definition": "Linia pozornego zetknięcia nieba z ziemią lub morzem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_line",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słońce powoli chowało się za <w>horyzontem</w>.",
                            "target_lang_translations": {
                                "ru": "Солнце медленно скрывалось за <w>горизонтом</w>."
                            }
                        },
                        {
                            "text": "Statek pojawił się na <w>horyzoncie</w> i płynął w naszą stronę.",
                            "target_lang_translations": {
                                "ru": "Корабль появился на <w>горизонте</w> и поплыл в нашу сторону."
                            }
                        }
                    ],
                    "synonyms": [
                        "widnokrąg",
                        "widnokres"
                    ],
                    "common_phrases": [
                        "na horyzoncie",
                        "za horyzontem",
                        "zniknąć za horyzontem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Линия, где небо кажется соприкасающимся с землей или морем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горизонт",
                                "target_lang_sense_clarification": "линия схождения неба и земли"
                            },
                            {
                                "target_lang_word": "небосклон",
                                "target_lang_sense_clarification": "часть неба над горизонтом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fa834f6-7db0-3b38-97ab-e1ba6a10863e",
                    "sense_definition": "Zakres wiedzy, zainteresowań lub wyobraźni danej osoby.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czytanie książek pozwala poszerzać <w>horyzonty</w> myślowe.",

                    "sense_id": "70faefad-879d-3389-881a-6003dcd7a336",
                    "sense_definition": "Zawołanie służące do przywabienia kota, zazwyczaj używane w powtórzeniu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Kici</w>, <w>kici</w>, chodź do mnie, mam dla ciebie jedzenie.",
                            "target_lang_translations": {
                                "ru": "<w>Кис-кис</w>, иди ко мне, у меня есть для тебя еда."
                            }
                        },
                        {
                            "text": "Wołałem na podwórku <w>kici</w>, ale żaden kot nie przyszedł.",
                            "target_lang_translations": {
                                "ru": "Я звал во дворе «<w>кис-кис</w>», но ни один кот не пришел."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kici kici"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, используемый для подзывания кошки, обычно повторяется."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кис-кис",
                                "target_lang_sense_clarification": "подзывание кошки"
                            }
                        ]
                    }
                }
            ]
        },

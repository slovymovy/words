                },
                {
                    "sense_id": "ce6d5719-1a9b-3d84-ac51-7c8951783021",
                    "sense_definition": "A term of affection used for someone small, cute, or loved.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Come here and give me a hug, my little <w>pumpkin</w>.",

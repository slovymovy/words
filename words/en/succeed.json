{
    "word_family": [
        "success",
        "successful",
        "successfully",
        "succession",
        "successor",
        "successive"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c356e95b-a86e-3ef0-aefc-7cee2e16430c",
                    "sense_definition": "To achieve a desired aim or result; to do what you tried or wanted to do.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She worked hard and finally <w>succeeded</w> in passing the exam.",
                            "target_lang_translations": {
                                "ru": "Она усердно работала, и, наконец, <w>сумела</w> сдать экзамен."
                            }
                        },
                        {
                            "text": "If you want to <w>succeed</w> in life, you must be persistent.",
                            "target_lang_translations": {
                                "ru": "Если вы хотите <w>преуспеть</w> в жизни, вы должны быть настойчивы."
                            }
                        }
                    ],
                    "synonyms": [
                        "prosper",
                        "flourish",
                        "do well"
                    ],
                    "antonyms": [
                        "fail"
                    ],
                    "common_phrases": [
                        "succeed in",
                        "succeed at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добиться желаемой цели или результата; сделать то, что вы пытались или хотели сделать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преуспеть",
                                "target_lang_sense_clarification": "в делах или жизни"
                            },
                            {
                                "target_lang_word": "добиться успеха",
                                "target_lang_sense_clarification": "достичь желаемого"
                            },
                            {
                                "target_lang_word": "суметь",
                                "target_lang_sense_clarification": "справиться с задачей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c85cdbfb-f4a9-3a8a-b500-c8869f750396",
                    "sense_definition": "To take an official position, title, or role after someone else.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prince will <w>succeed</w> his father as king when the time comes.",
                            "target_lang_translations": {
                                "ru": "Принц <w>сменит</w> своего отца на посту короля, когда придет время."
                            }
                        },
                        {
                            "text": "She <w>succeeded</w> to the throne at a very young age.",
                            "target_lang_translations": {
                                "ru": "Она <w>наследовала</w> трон в очень юном возрасте."
                            }
                        }
                    ],
                    "synonyms": [
                        "take the place of",
                        "follow",
                        "succeed"
                    ],
                    "antonyms": [
                        "precede"
                    ],
                    "common_phrases": [
                        "succeed to the throne",
                        "succeed someone as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занять официальную должность, титул или роль после кого-то другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наследовать",
                                "target_lang_sense_clarification": "титул или право"
                            },
                            {
                                "target_lang_word": "сменить",
                                "target_lang_sense_clarification": "кого-либо на посту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3489d071-6faf-38a8-9a46-7670a526095c",
                    "sense_definition": "To come next after something in time or a series.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A long silence <w>succeeded</w> the loud explosion.",
                            "target_lang_translations": {
                                "ru": "За громким взрывом <w>последовала</w> долгая тишина."
                            }
                        },
                        {
                            "text": "Summer <w>succeeds</w> spring in the cycle of the seasons.",
                            "target_lang_translations": {
                                "ru": "Лето <w>сменяет</w> весну в цикле времен года."
                            }
                        }
                    ],
                    "synonyms": [
                        "follow",
                        "come after",
                        "ensue"
                    ],
                    "antonyms": [
                        "precede"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идти следом за чем-то во времени или в последовательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следовать за",
                                "target_lang_sense_clarification": "идти после"
                            },
                            {
                                "target_lang_word": "сменять",
                                "target_lang_sense_clarification": "приходить на смену"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
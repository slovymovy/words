    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a35a0f2c-b10e-39d4-a53e-a0beb3161479",
                    "sense_definition": "Vraagt naar de tijdsduur; gedurende welke tijd.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hoelang</w> duurt de reis naar Amsterdam met de trein?",
                            "target_lang_translations": {
                                "ru": "<w>Сколько времени</w> занимает поездка в Амстердам на поезде?",
                                "en": "<w>How long</w> does the trip to Amsterdam take by train?"
                            }
                        },
                        {
                            "text": "Ik weet niet precies <w>hoelang</w> we moeten wachten.",
                            "target_lang_translations": {
                                "ru": "Я точно не знаю, <w>как долго</w> нам придется ждать.",
                                "en": "I don't know exactly <w>how long</w> we have to wait."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoeveel tijd"
                    ],
                    "common_phrases": [
                        "hoelang duurt het",
                        "weet je hoelang",
                        "hoelang nog"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для вопроса о продолжительности чего-либо; в течение какого времени.",
                        "en": "Used to ask about the duration of something; for how much time."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как долго",
                                "target_lang_sense_clarification": "о длительности"
                            },
                            {
                                "target_lang_word": "сколько времени",
                                "target_lang_sense_clarification": "о количестве времени"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "how long",
                                "target_lang_sense_clarification": "duration"
                            },
                            {
                                "target_lang_word": "how much time",
                                "target_lang_sense_clarification": "amount of time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57262177-5286-3ccd-abdb-f3592f219b6c",
                    "sense_definition": "Vraagt naar het eindtijdstip van een gebeurtenis; tot wanneer.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "end_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tot <w>hoelang</w> is de supermarkt vandaag open?",
                            "target_lang_translations": {
                                "ru": "До <w>скольки</w> сегодня открыт супермаркет?",
                                "en": "Until <w>what time</w> is the supermarket open today?"
                            }
                        },
                        {
                            "text": "Hij vroeg zich af tot <w>hoelang</w> het feest zou duren.",
                            "target_lang_translations": {
                                "ru": "Он гадал, до <w>какого времени</w> продлится вечеринка.",
                                "en": "He wondered until <w>what time</w> the party would last."
                            }
                        }
                    ],
                    "synonyms": [
                        "tot wanneer",
                        "tot hoe laat"
                    ],
                    "common_phrases": [
                        "tot hoelang"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для вопроса о времени окончания события; до какого момента.",
                        "en": "Used to ask about the end time of an event; until when."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до скольки",
                                "target_lang_sense_clarification": "разговорное, о времени окончания"
                            },
                            {
                                "target_lang_word": "до какого времени",
                                "target_lang_sense_clarification": "о предельном сроке"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "until when",
                                "target_lang_sense_clarification": "end time"
                            },
                            {
                                "target_lang_word": "until what time",
                                "target_lang_sense_clarification": "specific end time"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
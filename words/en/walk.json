                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть оправданным",
                                "target_lang_sense_clarification": "юридический термин"
                            },
                            {
                                "target_lang_word": "выйти на свободу",
                                "target_lang_sense_clarification": "освободиться из заключения"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7758f85b-15d5-358f-8dde-fce739614f86",
                    "sense_definition": "Een tekst, plan, document of lijst bedenken en uitschrijven.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "formulate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De advocaat zal morgen het contract voor ons <w>opstellen</w>.",
                            "target_lang_translations": {
                                "en": "The lawyer will <w>draw up</w> the contract for us tomorrow.",
                                "ru": "Адвокат завтра <w>составит</w> для нас контракт."
                            }
                        },
                        {
                            "text": "We hebben samen een lijst met regels <w>opgesteld</w> voor in de klas.",
                            "target_lang_translations": {
                                "en": "We <w>drew up</w> a list of rules for the classroom together.",
                                "ru": "Мы вместе <w>составили</w> список правил для класса."
                            }
                        }
                    ],
                    "synonyms": [
                        "formuleren",
                        "schrijven",
                        "ontwerpen",
                        "uitwerken"
                    ],
                    "common_phrases": [
                        "een contract opstellen",
                        "een plan opstellen",
                        "een lijst opstellen",
                        "regels opstellen"
                    ],
                    "target_lang_definitions": {
                        "en": "To conceive and write out a text, plan, document, or list.",
                        "ru": "Придумывать и записывать текст, план, документ или список."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "draw up",
                                "target_lang_sense_clarification": "a contract, plan, or list"
                            },
                            {
                                "target_lang_word": "draft",
                                "target_lang_sense_clarification": "a document or text"
                            },
                            {
                                "target_lang_word": "compose",
                                "target_lang_sense_clarification": "a text or letter"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "составлять",
                                "target_lang_sense_clarification": "документ, список, план"
                            },
                            {
                                "target_lang_word": "разрабатывать",
                                "target_lang_sense_clarification": "план, проект"
                            },
                            {
                                "target_lang_word": "оформлять",
                                "target_lang_sense_clarification": "документально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a596300-4919-386b-9a6c-ab0491498984",
                    "sense_definition": "Iets of iemand op een bepaalde plaats neerzetten of in een bepaalde volgorde zetten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arrange",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie had zich verdekt <w>opgesteld</w> langs de snelweg.",
                            "target_lang_translations": {
                                "en": "The police had <w>positioned</w> themselves covertly along the highway.",
                                "ru": "Полиция скрытно <w>расположилась</w> вдоль шоссе."
                            }
                        },
                        {
                            "text": "Voor de foto moeten alle leerlingen zich in een rij <w>opstellen</w>.",
                            "target_lang_translations": {
                                "en": "For the photo, all students must <w>line up</w> in a row.",
                                "ru": "Для фотографии все ученики должны <w>выстроиться</w> в ряд."
                            }
                        }
                    ],
                    "synonyms": [
                        "plaatsen",
                        "neerzetten",
                        "formeren",
                        "groeperen"
                    ],
                    "common_phrases": [
                        "zich verdekt opstellen",
                        "in een rij opstellen",
                        "in slagorde opstellen"
                    ],
                    "target_lang_definitions": {
                        "en": "To place something or someone in a specific location or arrange in a specific order.",
                        "ru": "Размещать кого-либо или что-либо в определенном месте или порядке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "position",
                                "target_lang_sense_clarification": "place someone or oneself in a specific spot"
                            },
                            {
                                "target_lang_word": "line up",
                                "target_lang_sense_clarification": "form a row or queue"
                            },
                            {
                                "target_lang_word": "set up",
                                "target_lang_sense_clarification": "arrange or place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выстраивать",
                                "target_lang_sense_clarification": "в ряд, в порядок"
                            },
                            {
                                "target_lang_word": "расставлять",
                                "target_lang_sense_clarification": "размещать по местам"
                            },
                            {
                                "target_lang_word": "располагать",
                                "target_lang_sense_clarification": "занимать позицию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "903ccfa6-d58d-3e44-a8e9-3ccd427150f0",
                    "sense_definition": "Zich op een bepaalde manier gedragen of een bepaalde houding aannemen tegenover iets of iemand.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet je tijdens de onderhandelingen flexibel <w>opstellen</w>.",
                            "target_lang_translations": {
                                "en": "You must <w>adopt</w> a flexible <w>attitude</w> during the negotiations.",
                                "ru": "Во время переговоров тебе нужно <w>вести себя</w> гибко."
                            }
                        },
                        {
                            "text": "Hij <w>stelde</w> zich erg verdedigend <w>op</w> toen hij kritiek kreeg van zijn baas.",
                            "target_lang_translations": {
                                "en": "He <w>took</w> a very defensive <w>stance</w> when he received criticism from his boss.",
                                "ru": "Он <w>занял</w> очень оборонительную <w>позицию</w>, когда получил критику от начальника."
                            }
                        }
                    ],
                    "synonyms": [
                        "gedragen"
                    ],
                    "common_phrases": [
                        "zich flexibel opstellen",
                        "zich kwetsbaar opstellen",
                        "zich kritisch opstellen",
                        "zich verdedigend opstellen"
                    ],
                    "target_lang_definitions": {
                        "en": "To behave in a certain way or adopt a particular attitude towards something or someone.",
                        "ru": "Вести себя определенным образом или занимать определенную позицию по отношению к кому-либо или чему-либо."
                    },
                    "translations": {

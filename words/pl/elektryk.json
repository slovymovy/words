            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cb93e5bf-5a4b-3cea-99ae-afd764bb2a2a",
                    "sense_definition": "Specjalista zajmujący się montażem, naprawą i konserwacją instalacji oraz urządzeń elektrycznych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę wezwać <w>elektryka</w>, ponieważ w kuchni nie ma prądu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно вызвать <w>электрика</w>, так как на кухне нет электричества."
                            }
                        },
                        {
                            "text": "Mój brat pracuje jako <w>elektryk</w> na dużej budowie w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Мой брат работает <w>электриком</w> на большой стройке в центре города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wezwać elektryka",
                        "uprawnienia elektryka",
                        "doświadczony elektryk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист, занимающийся монтажом, ремонтом и обслуживанием электрических установок и оборудования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электрик",
                                "target_lang_sense_clarification": "профессия"
                            },
                            {
                                "target_lang_word": "электромонтёр",
                                "target_lang_sense_clarification": "специальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2ceb26f-3739-3716-bd9b-cedbd281210c",
                    "sense_definition": "Samochód napędzany silnikiem elektrycznym, a nie spalinowym.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Coraz więcej osób decyduje się na zakup <w>elektryka</w> do jazdy po mieście.",
                            "target_lang_translations": {
                                "ru": "Всё больше людей решают купить <w>электромобиль</w> для езды по городу."
                            }
                        },
                        {
                            "text": "Ładowanie <w>elektryka</w> trwa zazwyczaj dłużej niż tankowanie paliwa.",
                            "target_lang_translations": {
                                "ru": "Зарядка <w>электромобиля</w> обычно длится дольше, чем заправка топливом."
                            }
                        }
                    ],
                    "synonyms": [
                        "samochód elektryczny"
                    ],
                    "common_phrases": [
                        "ładować elektryka",
                        "zasięg elektryka",
                        "używany elektryk"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "skrót od 'samochód elektryczny'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Автомобиль, приводимый в движение электродвигателем, а не двигателем внутреннего сгорания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электромобиль",

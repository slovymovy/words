                    }
                },
                {
                    "sense_id": "bb82ba7d-db47-3593-a75f-d19af1080a4e",
                    "sense_definition": "The moment when a tunnel or wall is penetrated to connect two areas.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "penetration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineers celebrated the <w>breakthrough</w> when the two ends of the tunnel finally met.",
                            "target_lang_translations": {

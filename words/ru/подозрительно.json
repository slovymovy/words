    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c1d8a4ec-553a-39e3-84f1-477a7c9325b8",
                    "sense_definition": "Странно, необычно, вызывая сомнения, недоверие или опасения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В пустом доме было <подозрительно> тихо для середины дня.",
                            "target_lang_translations": {
                                "en": "It was <suspiciously> quiet in the empty house for the middle of the day."
                            }
                        },
                        {
                            "text": "Этот дешёвый товар выглядит слишком <подозрительно>, чтобы быть качественным.",
                            "target_lang_translations": {
                                "en": "This cheap product looks too <suspicious> to be of high quality."
                            }
                        }
                    ],
                    "synonyms": [
                        "сомнительно",
                        "странно",
                        "нечисто"
                    ],
                    "antonyms": [
                        "естественно",
                        "обычно",
                        "убедительно"
                    ],
                    "common_phrases": [
                        "подозрительно тихо",
                        "подозрительно знакомый",
                        "выглядеть подозрительно",
                        "вести себя подозрительно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that causes suspicion, doubt, or seems strange and unusual."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suspiciously",
                                "target_lang_sense_clarification": "causing doubt or suspicion"
                            },
                            {
                                "target_lang_word": "strangely",
                                "target_lang_sense_clarification": "unusually or oddly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "533203e8-a308-3cb6-a977-3a0db8f194f1",
                    "sense_definition": "С недоверием, выражая сомнение в честности или истинности чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охранник <подозрительно> посмотрел на мой большой рюкзак.",
                            "target_lang_translations": {
                                "en": "The security guard looked <suspiciously> at my large backpack."
                            }
                        },
                        {
                            "text": "Она <подозрительно> спросила, где я пропадал весь вечер.",
                            "target_lang_translations": {
                                "en": "She asked <suspiciously> where I had been all evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "недоверчиво",
                        "настороженно"
                    ],
                    "antonyms": [
                        "доверчиво",
                        "одобрительно"
                    ],
                    "common_phrases": [
                        "подозрительно смотреть",
                        "подозрительно коситься",
                        "подозрительно относиться"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that shows a lack of trust or belief in someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suspiciously",
                                "target_lang_sense_clarification": "showing mistrust"
                            },
                            {
                                "target_lang_word": "distrustfully",
                                "target_lang_sense_clarification": "with a lack of trust"
                            },
                            {
                                "target_lang_word": "warily",
                                "target_lang_sense_clarification": "cautiously"
                            }
                        ]
                    }
                }
            ]
        },

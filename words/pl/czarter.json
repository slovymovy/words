            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa2e62b8-b7d1-3b7a-9ccb-5f82c4acd2d6",
                    "sense_definition": "Wynajęcie środka transportu, najczęściej statku wodnego lub samolotu, na określony czas lub w celu odbycia konkretnej podróży.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_rental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Planujemy <w>czarter</w> jachtu na Mazurach w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Мы планируем <w>чартер</w> яхты на Мазурских озерах в следующем месяце."
                            }
                        },
                        {
                            "text": "Firma podpisała umowę na <w>czarter</w> samolotu dla swoich pracowników.",
                            "target_lang_translations": {
                                "ru": "Компания подписала договор на <w>чартер</w> самолета для своих сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "wynajem",
                        "dzierżawa"
                    ],
                    "common_phrases": [
                        "czarter jachtu",
                        "umowa czarteru",
                        "czarter samolotu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аренда транспортного средства, чаще всего судна или самолета, на определенное время или для конкретного путешествия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чартер",
                                "target_lang_sense_clarification": "аренда судна или самолета"
                            },
                            {
                                "target_lang_word": "фрахт",
                                "target_lang_sense_clarification": "наем судна для перевозки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7dfe73a-5f35-3b68-acb4-1223ecd8f580",
                    "sense_definition": "Rejs lub lot odbywający się wynajętym środkiem transportu, często organizowany przez biura podróży poza regularnym rozkładem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_trip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sezonie wakacyjnym łatwo znaleźć tani <w>czarter</w> do Turcji.",
                            "target_lang_translations": {
                                "ru": "В сезон отпусков легко найти дешевый <w>чартер</w> в Турцию."
                            }
                        },
                        {
                            "text": "Nasz <w>czarter</w> wylatuje z Warszawy bardzo wcześnie rano.",
                            "target_lang_translations": {
                                "ru": "Наш <w>чартер</w> вылетает из Варшавы очень рано утром."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lecieć czarterem",
                        "tani czarter",
                        "wakacyjny czarter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рейс или полет на арендованном транспорте, часто организуемый турфирмами вне регулярного расписания."
                    },
                    "translations": {

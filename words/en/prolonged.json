{
    "word_family": [
        "prolongation",
        "prolonger"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "968e034d-af7a-3b5d-b298-69a9b4650727",
                    "sense_definition": "Continuing for a long time, often longer than usual or expected.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The landslip followed a period of <w>prolonged</w> rainfall.",
                            "target_lang_translations": {
                                "ru": "Оползень сошел после периода <w>затяжных</w> дождей."
                            }
                        },
                        {
                            "text": "He returned to work after a <w>prolonged</w> illness that kept him away for months.",
                            "target_lang_translations": {
                                "ru": "Он вернулся на работу после <w>длительной</w> болезни, из-за которой отсутствовал несколько месяцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "lasting",
                        "enduring"
                    ],
                    "antonyms": [
                        "brief",
                        "temporary"
                    ],
                    "common_phrases": [
                        "prolonged period",
                        "prolonged exposure",
                        "prolonged silence",
                        "prolonged absence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжающийся в течение длительного времени, часто дольше, чем обычно или ожидалось."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "длительный",
                                "target_lang_sense_clarification": "продолжающийся долго (о времени, периоде)"
                            },
                            {
                                "target_lang_word": "затяжной",
                                "target_lang_sense_clarification": "медленный, долгий (часто с негативным оттенком: о болезни, дожде, конфликте)"
                            },
                            {
                                "target_lang_word": "продолжительный",
                                "target_lang_sense_clarification": "занимающий много времени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f24a8fdf-4b27-3f6a-b745-ca0771d5be6a",
                    "sense_definition": "Simple past and past participle of <w>prolong</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The unexpected debate <w>prolonged</w> the meeting by another hour.",
                            "target_lang_translations": {
                                "ru": "Неожиданные дебаты <w>продлили</w> собрание еще на час."
                            }
                        },
                        {
                            "text": "Modern medicine has <w>prolonged</w> the average human lifespan significantly.",
                            "target_lang_translations": {
                                "ru": "Современная медицина значительно <w>продлила</w> среднюю продолжительность жизни человека."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prolonged the agony",
                        "prolonged the life"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола prolong (продлевать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продлил",
                                "target_lang_sense_clarification": "сделал более долгим по времени"
                            },
                            {
                                "target_lang_word": "затянул",
                                "target_lang_sense_clarification": "заставил длиться дольше, чем нужно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

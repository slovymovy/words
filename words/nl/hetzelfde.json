    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "b52271c2-3e5e-31dd-a8a1-e5cf7c994a39",
                    "sense_definition": "Het onzijdige enkelvoud van <w>dezelfde</w>; duidt aan dat iets of iemand identiek is aan het eerder genoemde.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb gisteren precies <w>hetzelfde</w> boek gekocht als jij.",
                            "target_lang_translations": {
                                "en": "I bought exactly <w>the same</w> book as you yesterday.",
                                "ru": "Вчера я купил точно <w>такую же</w> книгу, как и ты."
                            }
                        },
                        {
                            "text": "Uiteindelijk komt zijn verhaal op <w>hetzelfde</w> neer.",
                            "target_lang_translations": {
                                "en": "In the end, his story amounts to <w>the same thing</w>.",
                                "ru": "В конечном счете его история сводится к <w>тому же самому</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "identiek",
                        "eender"
                    ],
                    "antonyms": [
                        "ander",
                        "verschillend"
                    ],
                    "common_phrases": [
                        "op hetzelfde neerkomen",
                        "in hetzelfde schuitje",
                        "hetzelfde laken een pak"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "neuter singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The neuter singular of <w>dezelfde</w>; indicates that something or someone is identical to what was mentioned earlier.",
                        "ru": "Средний род от <w>dezelfde</w>; указывает на то, что предмет или лицо идентичны ранее упомянутым."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the same",
                                "target_lang_sense_clarification": "identical to something previously mentioned"
                            },
                            {
                                "target_lang_word": "the same thing",
                                "target_lang_sense_clarification": "referring to an identical object or situation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "то же самое",
                                "target_lang_sense_clarification": "субстантивированное местоимение"
                            },
                            {
                                "target_lang_word": "то же",
                                "target_lang_sense_clarification": "прилагательное/местоимение (ср. род)"
                            },
                            {
                                "target_lang_word": "одно и то же",
                                "target_lang_sense_clarification": "выражение идентичности"
                            }
                        ]
                    }
                }

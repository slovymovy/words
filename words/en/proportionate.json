    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4f62a5a2-f10d-3a05-a4e8-c2e0ecf72bbb",
                    "sense_definition": "Corresponding in size, amount, or degree to something else.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The punishment should always be <w>proportionate</w> to the crime committed.",
                            "target_lang_translations": {
                                "ru": "Наказание всегда должно быть <w>соразмерным</w> совершенному преступлению."
                            }
                        },
                        {
                            "text": "Employees receive a salary that is <w>proportionate</w> to their experience.",
                            "target_lang_translations": {
                                "ru": "Сотрудники получают зарплату, <w>пропорциональную</w> их опыту."
                            }
                        }
                    ],
                    "synonyms": [
                        "proportional",
                        "commensurate"
                    ],
                    "antonyms": [
                        "disproportionate"
                    ],
                    "common_phrases": [
                        "proportionate to",
                        "proportionate share",
                        "proportionate response"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствующий по размеру, количеству или степени чему-либо другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропорциональный",
                                "target_lang_sense_clarification": "находящийся в определенном количественном соотношении"
                            },
                            {
                                "target_lang_word": "соразмерный",
                                "target_lang_sense_clarification": "соответствующий по величине или степени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f024b63a-8c47-3666-9be3-6bf025cf2b95",
                    "sense_definition": "Having parts that are in correct or pleasing balance with each other; harmonious.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist drew a perfectly <w>proportionate</w> figure with balanced features.",
                            "target_lang_translations": {
                                "ru": "Художник нарисовал идеально <w>пропорциональную</w> фигуру со сбалансированными чертами."
                            }
                        },
                        {
                            "text": "Her small house was <w>proportionate</w> and fit well within the garden.",
                            "target_lang_translations": {
                                "ru": "Её маленький домик был <w>пропорциональным</w> и хорошо вписывался в сад."
                            }
                        }
                    ],
                    "synonyms": [
                        "balanced",
                        "symmetrical",
                        "harmonious"
                    ],
                    "antonyms": [
                        "disproportionate"
                    ],
                    "common_phrases": [
                        "proportionate body",
                        "proportionate features"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий части, находящиеся в правильном или приятном равновесии друг с другом; гармоничный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропорциональный",
                                "target_lang_sense_clarification": "имеющий правильные пропорции, стройный"
                            },
                            {
                                "target_lang_word": "гармоничный",
                                "target_lang_sense_clarification": "согласованный, стройный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d96b6cd-cf5d-37e3-b911-55f15d7e2587",
                    "sense_definition": "To adjust something so that it corresponds in size or amount to something else.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer tried to <w>proportionate</w> the fuel mixture for better efficiency.",
                            "target_lang_translations": {
                                "ru": "Инженер пытался <w>соразмерить</w> топливную смесь для повышения эффективности."
                            }
                        },
                        {
                            "text": "They must <w>proportionate</w> the distribution of resources across the region.",
                            "target_lang_translations": {
                                "ru": "Они должны <w>соразмерить</w> распределение ресурсов по всему региону."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулировать что-либо так, чтобы оно соответствовало по размеру или количеству чему-либо другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соразмерять",
                                "target_lang_sense_clarification": "устанавливать правильное соотношение"
                            },
                            {
                                "target_lang_word": "пропорционировать",
                                "target_lang_sense_clarification": "делить или смешивать в определенных пропорциях"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
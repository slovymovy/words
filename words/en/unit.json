        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b05a336d-94ae-3c98-bd18-1ff96174dbc8",
                    "sense_definition": "Calculated or measured for each single item or individual.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "business_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to reduce the <w>unit</w> cost to make the product more profitable.",
                            "target_lang_translations": {
                                "ru": "Нам нужно снизить <w>удельную</w> себестоимость, чтобы сделать продукт более прибыльным."
                            }

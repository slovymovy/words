            "senses": [
                {
                    "sense_id": "a727329c-47e3-3b79-bc48-cc4ff85f8824",
                    "sense_definition": "Глава мужского христианского монастыря.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious_head",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Иногда сам <w>настоятель</w> монастыря появлялся в аллее и беседовал с монахами.",
                            "target_lang_translations": {
                                "en": "Sometimes the <w>abbot</w> of the monastery himself appeared in the alley and talked with the monks."
                            }
                        },
                        {
                            "text": "Новый <w>настоятель</w> ввёл в обители строгий устав и дисциплину.",
                            "target_lang_translations": {
                                "en": "The new <w>abbot</w> introduced a strict charter and discipline in the monastery."
                            }
                        }
                    ],
                    "synonyms": [
                        "игумен",
                        "абба́т"
                    ],
                    "common_phrases": [
                        "настоятель монастыря",
                        "строгий настоятель",
                        "новый настоятель"
                    ],
                    "target_lang_definitions": {
                        "en": "The head of a male Christian monastery."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "abbot",
                                "target_lang_sense_clarification": "Head of an abbey or monastery"
                            },
                            {
                                "target_lang_word": "father superior",
                                "target_lang_sense_clarification": "Formal title for the head of a religious community"
                            },
                            {
                                "target_lang_word": "prior",
                                "target_lang_sense_clarification": "Head of a priory or a lower-ranking superior"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "164feb80-b763-338f-ad06-8881b366cf03",
                    "sense_definition": "Старший священник собора или приходской церкви.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious_head",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я — <w>настоятель</w> храма Зосимы и Савватия, и моя основная работа — священник.",
                            "target_lang_translations": {
                                "en": "I am the <w>rector</w> of the Church of Zosima and Savvatiy, and my main job is being a priest."
                            }
                        },
                        {
                            "text": "<w>Настоятель</w> церкви обратился к прихожанам с воскресной проповедью.",
                            "target_lang_translations": {
                                "en": "The <w>rector</w> of the church addressed the parishioners with a Sunday sermon."
                            }
                        }
                    ],
                    "common_phrases": [
                        "настоятель храма",
                        "настоятель церкви",
                        "настоятель собора"
                    ],
                    "target_lang_definitions": {
                        "en": "The senior priest in charge of a cathedral or parish church."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rector",
                                "target_lang_sense_clarification": "Priest in charge of a parish or church"
                            },
                            {
                                "target_lang_word": "dean",
                                "target_lang_sense_clarification": "Senior priest of a cathedral"
                            }
                        ]
                    }
                }
            ]
        }
    ]

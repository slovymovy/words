    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6591bb41-8d1d-34df-8cae-067966d25b58",
                    "sense_definition": "Przejęcie i utrzymywanie władzy na terytorium innego państwa przy użyciu siły zbrojnej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "military_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zachodni Brzeg Jordanu znalazł się pod <w>okupacją</w> po krótkiej wojnie.",
                            "target_lang_translations": {
                                "ru": "Западный берег реки Иордан оказался под <w>оккупацией</w> после короткой войны."
                            }
                        },
                        {
                            "text": "Rząd wezwał społeczność międzynarodową do potępienia nielegalnej <w>okupacji</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство призвало международное сообщество осудить незаконную <w>оккупацию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zajęcie",
                        "zabór",
                        "niewola"
                    ],
                    "antonyms": [
                        "wyzwolenie"
                    ],
                    "common_phrases": [
                        "okupacja wojskowa",
                        "okupacja terytorium",
                        "pod okupacją"
                    ],
                    "target_lang_definitions": {
                        "ru": "Захват и удержание территории другого государства с помощью военной силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оккупация",
                                "target_lang_sense_clarification": "военный захват территории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b816c6b5-0deb-390f-ad95-743d627e3c55",
                    "sense_definition": "Okres historyczny, w którym terytorium państwa znajduje się pod władzą obcych wojsk.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "military_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek podczas <w>okupacji</w> działał w partyzantce.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка во время <w>оккупации</w> был в партизанах."
                            }
                        },
                        {
                            "text": "W czasach <w>okupacji</w> życie w mieście było bardzo niebezpieczne.",
                            "target_lang_translations": {
                                "ru": "Во времена <w>оккупации</w> жизнь в городе была очень опасной."
                            }
                        }
                    ],
                    "synonyms": [
                        "niewola"
                    ],
                    "antonyms": [
                        "wyzwolenie"
                    ],
                    "common_phrases": [
                        "podczas okupacji",
                        "czasy okupacji",
                        "lata okupacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исторический период, в течение которого территория государства находится под властью иностранных войск."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оккупация",
                                "target_lang_sense_clarification": "исторический период"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb003a74-160b-3680-ba5c-512bb5b9b762",
                    "sense_definition": "Forma protestu polegająca na zajęciu budynku lub pomieszczenia w celu wymuszenia spełnienia żądań.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "protest_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ramach protestu pielęgniarki rozpoczęły <w>okupację</w> gabinetu dyrektora.",
                            "target_lang_translations": {
                                "ru": "В знак протеста медсестры начали <w>захват</w> кабинета директора."
                            }
                        },
                        {
                            "text": "Policja usunęła studentów prowadzących <w>okupację</w> uniwersytetu.",
                            "target_lang_translations": {
                                "ru": "Полиция вывела студентов, проводивших <w>захват</w> университета."
                            }
                        }
                    ],
                    "synonyms": [
                        "zajęcie"
                    ],
                    "common_phrases": [
                        "okupacja budynku",
                        "okupacja strajkowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма протеста, заключающаяся в захвате здания или помещения с целью принуждения к выполнению требований."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "захват",
                                "target_lang_sense_clarification": "здания или помещения"
                            },
                            {
                                "target_lang_word": "занятие",
                                "target_lang_sense_clarification": "помещения как протест"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
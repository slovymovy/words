                        ],
                        "ru": [
                            {
                                "target_lang_word": "когда-нибудь",
                                "target_lang_sense_clarification": "в вопросах и условиях"
                            },
                            {
                                "target_lang_word": "когда-либо",
                                "target_lang_sense_clarification": "более формальный вариант"
                            }
                        ]

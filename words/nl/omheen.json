        "waaromheen"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ba5278f1-804c-34cf-9486-be5e6be21966",
                    "sense_definition": "Aan alle kanten van iets of iemand; in een cirkel rondom een object of plaats.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_surrounding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er staat een heel hoog hek <w>omheen</w>.",
                            "target_lang_translations": {
                                "en": "There is a very high fence <w>around</w> it.",
                                "ru": "<w>Вокруг</w> стоит очень высокий забор."
                            }
                        },
                        {
                            "text": "Wij liepen er met een grote boog <w>omheen</w>.",
                            "target_lang_translations": {
                                "en": "We walked <w>around</w> it in a wide arc.",
                                "ru": "Мы прошли <w>вокруг</w> по большой дуге."
                            }
                        }
                    ],
                    "synonyms": [
                        "rondom",
                        "rond"
                    ],
                    "common_phrases": [
                        "er omheen draaien",
                        "er omheen lopen"
                    ],
                    "target_lang_definitions": {
                        "en": "On all sides of something or someone; in a circle around an object or place.",
                        "ru": "Со всех сторон чего-либо или кого-либо; по кругу вокруг объекта или места."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "around",
                                "target_lang_sense_clarification": "surrounding something"
                            },
                            {
                                "target_lang_word": "around it",
                                "target_lang_sense_clarification": "referring to a specific object"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вокруг",
                                "target_lang_sense_clarification": "наречие: со всех сторон, кругом"
                            },
                            {
                                "target_lang_word": "кругом",
                                "target_lang_sense_clarification": "наречие: повсюду, со всех сторон"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "d356f781-1f59-3fcc-b4ab-dba50be531c5",
                    "sense_definition": "Deel van de constructie 'om ... heen', gebruikt om aan te geven dat iets of iemand zich rond een object bevindt of beweegt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_surrounding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aarde draait in een jaar om de zon <w>heen</w>.",
                            "target_lang_translations": {
                                "en": "The earth orbits <w>around</w> the sun in a year.",
                                "ru": "Земля вращается <w>вокруг</w> Солнца за один год."
                            }
                        },
                        {
                            "text": "Hij keek verbaasd om zich <w>heen</w>.",
                            "target_lang_translations": {
                                "en": "He looked <w>around</w> in surprise.",
                                "ru": "Он удивленно посмотрел <w>вокруг</w> себя."
                            }
                        }
                    ],
                    "synonyms": [
                        "rondom"
                    ],
                    "common_phrases": [
                        "om zich heen",
                        "om iets heen"
                    ],
                    "target_lang_definitions": {
                        "en": "Part of the construction 'om ... heen', used to indicate that something or someone is located or moving around an object.",
                        "ru": "Часть конструкции 'om ... heen', используемая для указания на то, что кто-то или что-то находится или движется вокруг объекта."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "around",
                                "target_lang_sense_clarification": "encircling or moving about an object"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вокруг",
                                "target_lang_sense_clarification": "предлог: используется для обозначения движения или нахождения по окружности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
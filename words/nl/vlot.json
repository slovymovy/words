    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c74c78af-9c35-3b1a-916d-60c59f881bd4",
                    "sense_definition": "Gemakkelijk, soepel en zonder haperingen of problemen verlopend.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het gesprek verliep heel <w>vlot</w> en we waren het snel eens.",
                            "target_lang_translations": {
                                "ru": "Разговор прошел очень <w>гладко</w>, и мы быстро пришли к согласию."
                            }
                        },
                        {
                            "text": "Zij spreekt <w>vlot</w> drie verschillende talen zonder na te denken.",
                            "target_lang_translations": {
                                "ru": "Она <w>бегло</w> говорит на трех разных языках, не задумываясь."
                            }
                        }
                    ],
                    "synonyms": [
                        "soepel",
                        "vloeiend",
                        "gemakkelijk"
                    ],
                    "antonyms": [
                        "stroef",
                        "moeizaam"
                    ],
                    "common_phrases": [
                        "vlot spreken",
                        "een vlot verloop",
                        "vlot lezen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий легко, плавно, без заминок или проблем; о речи — беглый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беглый",
                                "target_lang_sense_clarification": "о речи или чтении"
                            },
                            {
                                "target_lang_word": "гладкий",
                                "target_lang_sense_clarification": "о процессе или течении дел"
                            },
                            {
                                "target_lang_word": "плавный",
                                "target_lang_sense_clarification": "без затруднений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64be9706-ced9-3de7-87cc-a3d1f3c60431",
                    "sense_definition": "Snel, levendig en spontaan in de omgang; soms ook modieus.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een <w>vlotte</w> jongen die heel makkelijk nieuwe vrienden maakt.",
                            "target_lang_translations": {
                                "ru": "Он <w>общительный</w> парень, который очень легко заводит новых друзей."
                            }
                        },
                        {
                            "text": "Met haar <w>vlotte</w> babbel wist ze de klant direct te overtuigen.",
                            "target_lang_translations": {
                                "ru": "Своей <w>бойкой</w> речью она сумела сразу убедить клиента."
                            }
                        }
                    ],
                    "synonyms": [
                        "spontaan",
                        "levendig",
                        "losjes"
                    ],
                    "antonyms": [
                        "verlegen",
                        "stijf",
                        "formeel"
                    ],
                    "common_phrases": [
                        "een vlotte babbel",
                        "vlotte kleding",
                        "een vlotte jongen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстрый, живой и спонтанный в общении; иногда также модный или стильный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бойкий",
                                "target_lang_sense_clarification": "живой, энергичный"
                            },
                            {
                                "target_lang_word": "раскованный",
                                "target_lang_sense_clarification": "непринужденный"
                            },
                            {
                                "target_lang_word": "общительный",
                                "target_lang_sense_clarification": "легкий в общении"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a1741dc3-5b9e-3c9d-8a6b-9728ebb3d984",
                    "sense_definition": "Een eenvoudige drijvende constructie van samengebonden balken of ander materiaal, gebruikt om over water te varen.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen bouwden een houten <w>vlot</w> om de rivier over te steken.",
                            "target_lang_translations": {
                                "ru": "Дети построили деревянный <w>плот</w>, чтобы переправиться через реку."
                            }
                        },
                        {
                            "text": "Na de schipbreuk dreven ze dagenlang op een klein <w>vlot</w> op zee.",
                            "target_lang_translations": {
                                "ru": "После кораблекрушения они днями дрейфовали в море на маленьком <w>плоту</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een vlot bouwen",
                        "op een vlot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простая плавучая конструкция из скрепленных бревен или другого материала, используемая для передвижения по воде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плот",
                                "target_lang_sense_clarification": "средство для плавания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "79210308-10fd-373b-991b-82f48f094155",
                    "sense_definition": "Vervoeging van <w>vlotten</w>: eerste, tweede en derde persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het werk <w>vlot</w> vandaag erg goed, we zijn bijna klaar.",
                            "target_lang_translations": {
                                "ru": "Работа сегодня <w>спорится</w> очень хорошо, мы почти закончили."
                            }
                        },
                        {
                            "text": "Als de samenwerking niet <w>vlot</w>, moeten we duidelijke afspraken maken.",
                            "target_lang_translations": {
                                "ru": "Если сотрудничество не <w>ладится</w>, нам нужно заключить четкие соглашения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола vlotten: идти успешно, продвигаться вперед (о работе или деле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спориться",
                                "target_lang_sense_clarification": "идти успешно (о работе)"
                            },
                            {
                                "target_lang_word": "ладиться",
                                "target_lang_sense_clarification": "получаться хорошо"
                            },
                            {
                                "target_lang_word": "продвигаться",
                                "target_lang_sense_clarification": "идти вперед"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2fd141d6-a329-35a5-ada6-8134977cc699",
                    "sense_definition": "Расположенный, находящийся рядом, поблизости, по соседству с кем-либо или чем-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В <w>соседней</w> комнате громко играла музыка, и я не мог уснуть.",
                            "target_lang_translations": {
                                "en": "Loud music was playing in the <w>next</w> room, and I couldn't fall asleep."
                            }
                        },
                        {
                            "text": "Мы часто ходим за продуктами в <w>соседний</w> магазин, потому что он ближе всего.",
                            "target_lang_translations": {
                                "en": "We often go grocery shopping at the <w>nearby</w> store because it is the closest."
                            }
                        }
                    ],
                    "synonyms": [
                        "близлежащий",
                        "ближний"
                    ],
                    "antonyms": [
                        "далёкий"
                    ],
                    "common_phrases": [
                        "соседний дом",
                        "соседняя комната",
                        "соседняя улица",
                        "соседний стол",
                        "в соседнем здании"
                    ],
                    "target_lang_definitions": {
                        "en": "Located or situated nearby, close by, or next to something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "neighboring",
                                "target_lang_sense_clarification": "situated next to or very near"
                            },
                            {
                                "target_lang_word": "next",
                                "target_lang_sense_clarification": "immediately adjacent (e.g., room, door)"
                            },
                            {
                                "target_lang_word": "nearby",
                                "target_lang_sense_clarification": "close by in distance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b5c1350-8496-376f-a674-45f93bba46c2",
                    "sense_definition": "Живущий или обитающий по соседству (обычно о странах, народах или группах).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Соседние</w> страны подписали важное соглашение о торговле и сотрудничестве.",
                            "target_lang_translations": {
                                "en": "<w>Neighboring</w> countries signed an important agreement on trade and cooperation."
                            }
                        },
                        {
                            "text": "На протяжении веков <w>соседние</w> народы обменивались культурными традициями.",
                            "target_lang_translations": {
                                "en": "For centuries, <w>neighboring</w> peoples have exchanged cultural traditions."
                            }
                        }
                    ],
                    "common_phrases": [
                        "соседние страны",
                        "соседние государства",
                        "соседние народы"
                    ],
                    "target_lang_definitions": {
                        "en": "Living or existing nearby (usually referring to countries, peoples, or groups)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "neighboring",
                                "target_lang_sense_clarification": "bordering or adjacent (countries, peoples)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0eb96638-e303-37da-a0d3-ca8ee0ab97b3",
                    "sense_definition": "Исповедующий одну и ту же веру, религию с кем-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы всегда готовы поддержать наших <w>единоверных</w> братьев в трудную минуту.",
                            "target_lang_translations": {
                                "en": "We are always ready to support our brothers <w>of the same faith</w> in difficult times."
                            }
                        },
                        {
                            "text": "В этот монастырь часто приезжают <w>единоверные</w> паломники из соседних стран.",
                            "target_lang_translations": {
                                "en": "Pilgrims <w>of the same faith</w> from neighboring countries often come to this monastery."
                            }
                        }
                    ],
                    "antonyms": [
                        "иноверный"
                    ],
                    "common_phrases": [
                        "единоверный народ",
                        "единоверные братья",
                        "единоверная церковь"
                    ],
                    "target_lang_definitions": {
                        "en": "Professing the same faith or religion as someone else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of the same faith",
                                "target_lang_sense_clarification": "adjectival phrase indicating shared religion"
                            },
                            {
                                "target_lang_word": "co-religionist",
                                "target_lang_sense_clarification": "attributive use"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf37309a-516e-35fd-a35f-a65bf9456dd2",
                    "sense_definition": "Имеющий общие взгляды, убеждения или интересы с кем-либо.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом кружке он надеялся найти <w>единоверных</w> соратников для обсуждения философии.",
                            "target_lang_translations": {
                                "en": "In this circle, he hoped to find <w>like-minded</w> companions to discuss philosophy."
                            }
                        },
                        {
                            "text": "Старый учитель был рад встретить <w>единоверную</w> душу, понимающую его идеи.",
                            "target_lang_translations": {
                                "en": "The old teacher was glad to meet a <w>kindred</w> spirit who understood his ideas."
                            }
                        }
                    ],
                    "synonyms": [
                        "единомышленный"
                    ],
                    "antonyms": [
                        "чуждый"
                    ],
                    "common_phrases": [
                        "единоверный друг",
                        "единоверная душа"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'единомышленный'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Sharing common views, convictions, or interests with someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "like-minded",
                                "target_lang_sense_clarification": "sharing similar opinions or interests"
                            },
                            {

            "senses": [
                {
                    "sense_id": "d0d42768-ac28-3ce8-8c2d-d8dd24fafcd7",
                    "sense_definition": "Сокращение от <w>чрезвычайное происшествие</w>; неожиданное, часто опасное или неприятное событие, нарушающее нормальный порядок.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "incident",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На заводе произошло <w>ЧП</w>, и всех работников эвакуировали."
                        },
                        {
                            "text": "Хочется пожелать, чтобы в новом году было поменьше катастроф и <w>ЧП</w>."
                        }
                    ],
                    "synonyms": [
                        "происшествие",
                        "инцидент"
                    ],
                    "common_phrases": [
                        "произошло ЧП",
                        "серьезное ЧП",
                        "место ЧП"
                    ]
                },
                {
                    "sense_id": "55207fe3-a598-3f4a-bb2b-f2eabc45c904",
                    "sense_definition": "Сокращение от <w>чрезвычайное положение</w>; особый правовой режим, вводимый властями в случае бедствий или угрозы безопасности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_emergency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В городе ввели режим <w>ЧП</w> из-за сильного наводнения."
                        },
                        {
                            "text": "В школе было объявлено <w>ЧП</w>, и всех учеников отпустили домой."
                        }
                    ],
                    "common_phrases": [
                        "режим ЧП",
                        "ввести ЧП",
                        "объявить ЧП"
                    ]
                },
                {
                    "sense_id": "40cea269-5636-3bed-a22f-5b51dd96daa8",
                    "sense_definition": "Сокращение от <w>частное предприятие</w>; форма организации бизнеса, принадлежащего частному лицу.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "business_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой сосед зарегистрировал <w>ЧП</w> и открыл продуктовый магазин."
                        },
                        {
                            "text": "Это небольшое <w>ЧП</w> производит качественную мебель для кухни."
                        }
                    ],
                    "synonyms": [
                        "фирма",
                        "компания"
                    ],
                    "common_phrases": [
                        "открыть ЧП",
                        "директор ЧП"
                    ]
                },
                {
                    "sense_id": "4b904b2a-b69f-3ffa-bf8f-ceec1f29822d",
                    "sense_definition": "Сокращение от <w>частный предприниматель</w>; человек, занимающийся бизнесом индивидуально (в современной терминологии чаще ИП).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "business_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он оформился как <w>ЧП</w>, чтобы легально сдавать квартиру в аренду."
                        },
                        {
                            "text": "Многие <w>ЧП</w> перешли на новую упрощённую систему налогообложения."
                        }
                    ],
                    "synonyms": [
                        "предприниматель",
                        "бизнесмен"
                    ],
                    "common_phrases": [

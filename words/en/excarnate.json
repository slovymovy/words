                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лишенный плоти",
                                "target_lang_sense_clarification": "о физических останках или костях"
                            },
                            {
                                "target_lang_word": "бесплотный",
                                "target_lang_sense_clarification": "не имеющий физического тела, развоплощённый"

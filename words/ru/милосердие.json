{
    "word_family": [
        "милосердный",
        "милосердно"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d075b6ab-9cfa-33de-80f2-8b7ccefc5ddb",
                    "sense_definition": "Готовность из сострадания оказать помощь тому, кто в ней нуждается.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Надеюсь на <w>милосердие</w> добрых людей, готовых разделить наше горе.",
                            "target_lang_translations": {
                                "en": "I hope for the <w>mercy</w> of kind people ready to share our grief."
                            }
                        },
                        {
                            "text": "В больнице работала опытная сестра <w>милосердия</w>, которая помогала всем раненым.",
                            "target_lang_translations": {
                                "en": "An experienced sister of <w>mercy</w> worked in the hospital, helping all the wounded."
                            }
                        }
                    ],
                    "synonyms": [
                        "сердобольность",
                        "доброта"
                    ],
                    "antonyms": [
                        "жестокость"
                    ],
                    "common_phrases": [
                        "сестра милосердия",
                        "проявлять милосердие",
                        "дела милосердия"
                    ],
                    "target_lang_definitions": {
                        "en": "Readiness to help those in need out of compassion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mercy",
                                "target_lang_sense_clarification": "compassion or pity shown to those in distress"
                            },
                            {
                                "target_lang_word": "compassion",
                                "target_lang_sense_clarification": "deep sympathy and desire to help"
                            },
                            {
                                "target_lang_word": "charity",
                                "target_lang_sense_clarification": "benevolence and generosity towards others"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96fae617-f836-3742-a944-1265dcc1a439",
                    "sense_definition": "Готовность простить кого-либо, проявить снисхождение и не наказывать.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пленный враг упал на колени и стал просить победителя о <w>милосердии</w>.",
                            "target_lang_translations": {
                                "en": "The captured enemy fell to his knees and began to beg the victor for <w>mercy</w>."
                            }
                        },
                        {
                            "text": "Король проявил истинное <w>милосердие</w> и отменил суровый смертный приговор.",
                            "target_lang_translations": {
                                "en": "The king showed true <w>mercy</w> and cancelled the harsh death sentence."
                            }
                        }
                    ],
                    "synonyms": [
                        "милостивость"
                    ],
                    "antonyms": [
                        "жестокость"
                    ],
                    "common_phrases": [
                        "просить о милосердии",
                        "взывать к милосердию",
                        "без милосердия"
                    ],
                    "target_lang_definitions": {
                        "en": "Readiness to forgive someone, show leniency, and not punish."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mercy",
                                "target_lang_sense_clarification": "forgiveness shown to an offender or enemy"
                            },
                            {
                                "target_lang_word": "clemency",
                                "target_lang_sense_clarification": "leniency shown by an authority"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
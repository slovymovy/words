{
    "word_family": [
        "elder",
        "elderliness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "aa8a4bee-5be2-3796-9596-f33ff798feae",
                    "sense_definition": "Approaching old age; having lived for a long time (usually referring to people).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "people_age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>elderly</w> couple walked slowly through the park holding hands.",
                            "target_lang_translations": {
                                "ru": "<w>Пожилая</w> пара медленно шла по парку, держась за руки."
                            }
                        },
                        {
                            "text": "She is concerned about the health of her <w>elderly</w> parents.",
                            "target_lang_translations": {
                                "ru": "Она беспокоится о здоровье своих <w>пожилых</w> родителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "old",
                        "aged",
                        "senior",
                        "geriatric",
                        "older"
                    ],
                    "antonyms": [
                        "young"
                    ],
                    "common_phrases": [
                        "elderly couple",
                        "elderly parents",
                        "elderly man",
                        "elderly woman",
                        "elderly people"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приближающийся к старости; проживший долгую жизнь (обычно о людях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожилой",
                                "target_lang_sense_clarification": "вежливое, наиболее употребительное"
                            },
                            {
                                "target_lang_word": "престарелый",
                                "target_lang_sense_clarification": "очень старый, дряхлый"
                            },
                            {
                                "target_lang_word": "в возрасте",
                                "target_lang_sense_clarification": "вежливый эвфемизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f9999db-f4a5-30a6-955b-69dff0f2b792",
                    "sense_definition": "Of an object or machinery, showing signs of age; old and needing replacement or repair.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object_age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>elderly</w> car finally broke down on the highway.",
                            "target_lang_translations": {
                                "ru": "Его <w>старенькая</w> машина окончательно сломалась на шоссе."
                            }
                        },
                        {
                            "text": "The factory replaced the <w>elderly</w> machinery with modern equipment.",
                            "target_lang_translations": {
                                "ru": "Завод заменил <w>устаревшее</w> оборудование на современное."
                            }
                        }
                    ],
                    "synonyms": [
                        "antiquated",
                        "obsolete",
                        "old",
                        "decrepit",
                        "aging"
                    ],
                    "antonyms": [
                        "young"
                    ],
                    "common_phrases": [
                        "elderly car",
                        "elderly vehicle",
                        "elderly equipment"
                    ],
                    "target_lang_definitions": {
                        "ru": "О предмете или механизме: имеющий признаки старения; старый и требующий замены или ремонта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старый",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "устаревший",
                                "target_lang_sense_clarification": "о технике или оборудовании"
                            },
                            {
                                "target_lang_word": "старенький",
                                "target_lang_sense_clarification": "разговорное, часто с оттенком иронии или привязанности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "19fc6c50-9658-3a4e-a81f-cc9fde6d620c",
                    "sense_definition": "People who are old considered as a group (usually used with 'the').",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "people_age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government announced new healthcare benefits for the <w>elderly</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство объявило о новых льготах на медицинское обслуживание для <w>пожилых людей</w>."
                            }
                        },
                        {
                            "text": "We need more nurses trained to care for the <w>elderly</w>.",
                            "target_lang_translations": {
                                "ru": "Нам нужно больше медсестер, обученных уходу за <w>престарелыми</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "seniors",
                        "senior citizens",
                        "the aged",
                        "older people"
                    ],
                    "common_phrases": [
                        "care for the elderly",
                        "homes for the elderly",
                        "services for the elderly",
                        "the elderly and disabled"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди преклонного возраста как социальная группа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожилые люди",
                                "target_lang_sense_clarification": "собирательное значение"
                            },
                            {
                                "target_lang_word": "престарелые",
                                "target_lang_sense_clarification": "часто используется в официальных названиях учреждений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da318b0c-8780-367a-8bc1-3f6c126f644c",
                    "sense_definition": "An elderly person.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "people_age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The community center organizes weekly activities for local <w>elderlies</w>.",
                            "target_lang_translations": {
                                "ru": "Общественный центр организует еженедельные мероприятия для местных <w>пожилых людей</w>."
                            }
                        },
                        {
                            "text": "She works as a caregiver for several <w>elderlies</w> in the neighborhood.",
                            "target_lang_translations": {
                                "ru": "Она работает сиделкой у нескольких <w>пожилых людей</w> по соседству."
                            }
                        }
                    ],
                    "synonyms": [
                        "senior",
                        "senior citizen",
                        "old-timer",
                        "oldster"
                    ],
                    "traits": [
                        {

                },
                {
                    "sense_id": "7591fb4c-ad61-3021-af22-3872b9b13248",
                    "sense_definition": "Dwie zewnętrzne, czerwone fałdy skórne otaczające otwór gębowy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten chłopak ma bardzo zmysłowe <w>usta</w>.",

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "котловина",
                                "target_lang_sense_clarification": "география"
                            },
                            {
                                "target_lang_word": "солончак",
                                "target_lang_sense_clarification": "соляная равнина"
                            }
                        ]
                    }

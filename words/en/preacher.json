            "senses": [
                {
                    "sense_id": "85f262a9-4c30-3ea5-a0e9-5dd3e89cc75d",
                    "sense_definition": "A title referring to the book of Ecclesiastes in the Old Testament of the Bible, or its author.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "document_or_program_name",
                    "examples": [
                        {
                            "text": "The <w>Preacher</w> famously wrote that there is a time for everything under the sun.",
                            "target_lang_translations": {
                                "ru": "<w>Екклесиаст</w> знаменит своими словами о том, что всему свое время под солнцем."
                            }
                        },
                        {
                            "text": "Scholars often discuss the philosophical themes found in the words of the <w>Preacher</w>.",
                            "target_lang_translations": {
                                "ru": "Ученые часто обсуждают философские темы, найденные в словах <w>Екклесиаста</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название, относящееся к книге Екклесиаста в Ветхом Завете или к её автору."
                    },
                    "translations": {

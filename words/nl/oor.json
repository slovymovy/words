                        "tot over je oren",
                        "iemand een oor aannaaien",
                        "grote oren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Орган слуха, расположенный по обеим сторонам головы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухо",
                                "target_lang_sense_clarification": "часть тела"

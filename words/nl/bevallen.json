            "senses": [
                {
                    "sense_id": "48ac06a1-43f7-32b0-971f-565f3d53897a",
                    "sense_definition": "Iets als aangenaam of prettig ervaren; in de smaak vallen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pleasing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het nieuwe werk <w>bevalt</w> hem erg goed.",
                            "target_lang_translations": {
                                "en": "The new job <w>suits</w> him very well."
                            }
                        },
                        {
                            "text": "Die arrogante houding <w>beviel</w> mij helemaal niet.",
                            "target_lang_translations": {
                                "en": "That arrogant attitude did not <w>please</w> me at all."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanstaan",
                        "behagen"
                    ],
                    "antonyms": [
                        "tegenvallen"
                    ],
                    "common_phrases": [
                        "goed bevallen",
                        "slecht bevallen",
                        "hoe bevalt het"
                    ],
                    "target_lang_definitions": {
                        "en": "To be experienced as pleasant or agreeable; to be to one's liking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "please",
                                "target_lang_sense_clarification": "to give satisfaction or pleasure"
                            },
                            {
                                "target_lang_word": "suit",
                                "target_lang_sense_clarification": "to be convenient or acceptable"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bfc0029-1a5a-33d8-a8f3-0e4c46ec6193",
                    "sense_definition": "Het leven schenken aan een kind; een kind ter wereld brengen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "birth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is thuis van een gezonde zoon <w>bevallen</w>.",
                            "target_lang_translations": {
                                "en": "She <w>gave birth</w> to a healthy son at home."
                            }
                        },
                        {
                            "text": "Mijn zus moet volgende week in het ziekenhuis <w>bevallen</w>.",
                            "target_lang_translations": {
                                "en": "My sister has to <w>give birth</w> in the hospital next week."
                            }
                        }
                    ],
                    "synonyms": [
                        "bekoren"
                    ],
                    "common_phrases": [
                        "bevallen van",
                        "thuis bevallen",
                        "in het ziekenhuis bevallen"
                    ],
                    "target_lang_definitions": {

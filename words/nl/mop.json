        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "985b80a0-6515-3920-a118-2ea6b43b6ee7",
                    "sense_definition": "Een kort verhaal dat wordt verteld om mensen te laten lachen, meestal met een verrassende en grappige ontknoping.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "joke",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vertelde een leuke <w>mop</w> waar iedereen hard om moest lachen.",
                            "target_lang_translations": {
                                "en": "He told a funny <w>joke</w> that made everyone laugh out loud."
                            }
                        },
                        {
                            "text": "Ken jij die <w>mop</w> van de twee Belgen die naar de markt gingen?",
                            "target_lang_translations": {
                                "en": "Do you know that <w>joke</w> about the two Belgians who went to the market?"
                            }
                        }
                    ],
                    "synonyms": [
                        "grap",
                        "bak"
                    ],
                    "common_phrases": [
                        "een mop vertellen",
                        "een flauwe mop",
                        "een schuine mop"
                    ],
                    "target_lang_definitions": {
                        "en": "A short story told to make people laugh, usually with a surprising and funny ending."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "joke",
                                "target_lang_sense_clarification": "amusing story"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e76c028-c012-3e6a-a3ac-41e344d36ab1",
                    "sense_definition": "Een huishoudelijk voorwerp om vloeren nat mee schoon te maken, bestaande uit een steel met daaraan een bundel touwen of lappen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet de emmer en de <w>mop</w> maar in de gang als je klaar bent.",
                            "target_lang_translations": {
                                "en": "Just put the bucket and the <w>mop</w> in the hallway when you are done."
                            }
                        },
                        {
                            "text": "Met een natte <w>mop</w> maakte ze de keukenvloer grondig schoon.",
                            "target_lang_translations": {
                                "en": "She cleaned the kitchen floor thoroughly with a wet <w>mop</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwabber"

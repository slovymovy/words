    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c9e24620-8a28-358a-aabe-30a02e96517e",
                    "sense_definition": "Een gebied, stad of vesting met geweld of strijd in bezit nemen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "military_conquest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten wisten de vijandelijke stad na een lange strijd te <w>veroveren</w>.",
                            "target_lang_translations": {
                                "ru": "Солдатам удалось <w>завоевать</w> вражеский город после долгой битвы."
                            }
                        },
                        {
                            "text": "In de geschiedenisles leerden we hoe de Romeinen grote gebieden <w>veroverden</w>.",
                            "target_lang_translations": {
                                "ru": "На уроке истории мы узнали, как римляне <w>завоёвывали</w> обширные территории."
                            }
                        }
                    ],
                    "synonyms": [
                        "innemen",
                        "bezetten"
                    ],
                    "antonyms": [
                        "verliezen",
                        "prijsgeven"
                    ],
                    "common_phrases": [
                        "een stad veroveren",
                        "land veroveren",
                        "het gebied veroveren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Захватить территорию, город или крепость с применением силы или в ходе борьбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завоевать",
                                "target_lang_sense_clarification": "территорию, страну"
                            },
                            {
                                "target_lang_word": "захватить",
                                "target_lang_sense_clarification": "взять под контроль силой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ba55fe9-a168-37c9-a607-9ba10871d88b",
                    "sense_definition": "Iets verkrijgen, winnen of voor zich winnen door er moeite voor te doen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met zijn charme en humor wist hij haar hart snel te <w>veroveren</w>.",
                            "target_lang_translations": {
                                "ru": "Своим обаянием и юмором он сумел быстро <w>завоевать</w> её сердце."
                            }
                        },
                        {
                            "text": "Het nieuwe product heeft in korte tijd de hele markt <w>veroverd</w>.",
                            "target_lang_translations": {
                                "ru": "Новый продукт за короткое время <w>завоевал</w> весь рынок."
                            }
                        }
                    ],
                    "synonyms": [
                        "winnen",
                        "bemachtigen",
                        "binnenslepen"
                    ],
                    "antonyms": [
                        "verliezen"
                    ],
                    "common_phrases": [
                        "een hart veroveren",
                        "een plaats veroveren",
                        "de markt veroveren",
                        "goud veroveren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить что-либо, выиграть или добиться чьего-либо расположения благодаря усилиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завоевать",
                                "target_lang_sense_clarification": "рынок, медаль, доверие"
                            },
                            {
                                "target_lang_word": "покорить",
                                "target_lang_sense_clarification": "сердце, вершину"
                            }
                        ]
                    }
                }
            ]
        }
    ]

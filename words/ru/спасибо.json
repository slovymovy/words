    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "624214ef-7b90-3ef6-8136-d208dae87f52",
                    "sense_definition": "Слово, которое используют для выражения благодарности за помощь, услугу, внимание или подарок.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "gratitude_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Большое <w>спасибо</w> за твой прекрасный подарок на день рождения."
                        },
                        {
                            "text": "Я хочу сказать тебе <w>спасибо</w> за помощь в трудную минуту."
                        },
                        {
                            "text": "Всем <w>спасибо</w> за внимание, лекция окончена."
                        }
                    ],
                    "synonyms": [
                        "благодарю",
                        "мерси"
                    ],
                    "common_phrases": [
                        "большое спасибо",
                        "огромное спасибо",
                        "спасибо за помощь",
                        "заранее спасибо",
                        "спасибо за внимание"
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "37b53d55-0e54-3aa9-bfe9-90d320798539",
                    "sense_definition": "Слова благодарности или признательности, рассматриваемые как существительное (что?).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gratitude_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он ушёл молча, не сказав нам даже простого человеческого <w>спасибо</w>."
                        },
                        {
                            "text": "Я не жду от них никакого <w>спасибо</w> за эту сложную работу."
                        }
                    ],
                    "common_phrases": [
                        "ждать спасиба",
                        "сказать спасибо",
                        "услышать спасибо",
                        "вместо спасиба"
                    ]
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "a7e84248-3d07-393b-8d12-857091fa6ce5",
                    "sense_definition": "Слово, выражающее удовлетворение тем, что произошло; используется в значении «хорошо, что» или «к счастью».",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Спасибо</w>, дождь закончился, и мы смогли пойти гулять в парк."
                        },
                        {
                            "text": "<w>Спасибо</w> ещё, что мы успели купить билеты на последний поезд."
                        }
                    ],
                    "synonyms": [
                        "хорошо, что"
                    ],
                    "common_phrases": [
                        "спасибо, что",
                        "спасибо ещё"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "прост."
                        }
                    ]
                }
            ]
        }
    ]

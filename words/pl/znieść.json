            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7a7f16fb-94e7-3f96-be92-601e4583bd1e",
                    "sense_definition": "Przemieścić kogoś lub coś z miejsca położonego wyżej do miejsca położonego niżej, trzymając w rękach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <znieść> te ciężkie pudła do piwnicy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <снести> эти тяжелые коробки в подвал."
                            }
                        },
                        {
                            "text": "Czy mógłbyś mi pomóc <znieść> wózek po schodach?",
                            "target_lang_translations": {
                                "ru": "Ты не мог бы помочь мне <спустить> коляску по лестнице?"
                            }
                        }
                    ],
                    "synonyms": [
                        "sprowadzić"
                    ],
                    "antonyms": [
                        "wnieść"
                    ],
                    "common_phrases": [
                        "znieść na dół",
                        "znieść bagaże",
                        "znieść ze schodów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместить кого-то или что-то с более высокого места на более низкое, держа в руках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снести",
                                "target_lang_sense_clarification": "переместить вниз"
                            },
                            {
                                "target_lang_word": "спустить",
                                "target_lang_sense_clarification": "помочь сойти или переместить вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e743681c-2fe5-3365-b056-a533d513d11b",
                    "sense_definition": "Wydobyć z organizmu jajko (o ptakach lub gadach).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza kura wczoraj <zniosła> dwa jajka.",
                            "target_lang_translations": {
                                "ru": "Наша курица вчера <снесла> два яйца."
                            }
                        },
                        {
                            "text": "Ptaki szukają bezpiecznego miejsca, aby <znieść> jaja.",
                            "target_lang_translations": {
                                "ru": "Птицы ищут безопасное место, чтобы <снести> яйца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znieść jajko",
                        "kura zniosła jajko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отложить яйцо (о птицах или рептилиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снести",
                                "target_lang_sense_clarification": "о яйцах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02a4bb2c-58d8-33a6-a5d0-ba7ffeeb11cb",
                    "sense_definition": "Wytrzymać coś trudnego, bolesnego lub nieprzyjemnego; poradzić sobie z ciężarem psychicznym lub fizycznym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "endurance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mogę dłużej <znieść> tego hałasu dobiegającego z ulicy.",
                            "target_lang_translations": {
                                "ru": "Я не могу больше <выносить> этот шум, доносящийся с улицы."
                            }
                        },
                        {
                            "text": "Pacjent zdołał dzielnie <znieść> bolesny zabieg bez znieczulenia.",
                            "target_lang_translations": {
                                "ru": "Пациент сумел мужественно <перенести> болезненную процедуру без анестезии."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytrzymać",
                        "przetrzymać",
                        "ścierpieć"
                    ],
                    "antonyms": [
                        "poddać się"
                    ],
                    "common_phrases": [
                        "nie móc znieść",
                        "znieść ból",
                        "znieść krytykę",
                        "znieść upokorzenie"

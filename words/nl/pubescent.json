                    ],
                    "antonyms": [
                        "volwassen"
                    ],
                    "common_phrases": [
                        "pubescente fase",
                        "pubescente leeftijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в стадии полового созревания; относящийся к пубертатному периоду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пубертатный",
                                "target_lang_sense_clarification": "относящийся к периоду полового созревания"
                            },
                            {
                                "target_lang_word": "подростковый",
                                "target_lang_sense_clarification": "свойственный подростку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb9a3413-4dd3-38d2-a9da-ac2aabc336df",
                    "sense_definition": "Bedekt met fijne, zachte haartjes of dons (vooral gebruikt in de biologie en plantkunde).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stengel van deze bijzondere plant is licht <w>pubescent</w> en voelt daardoor zacht aan.",
                            "target_lang_translations": {
                                "ru": "Стебель этого необычного растения слегка <w>опушён</w> и поэтому мягкий на ощупь."
                            }
                        },
                        {
                            "text": "Onder de microscoop is de <w>pubescente</w> structuur van het blad duidelijk zichtbaar.",
                            "target_lang_translations": {
                                "ru": "Под микроскопом чётко видна <w>опушённая</w> структура листа."
                            }
                        }
                    ],
                    "synonyms": [
                        "donzig",
                        "behaard"
                    ],
                    "antonyms": [
                        "kaal",
                        "onbehaard"
                    ],
                    "common_phrases": [
                        "pubescente bladeren",
                        "pubescente stengel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытый короткими мягкими волосками или пушком (термин в биологии и ботанике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опушённый",

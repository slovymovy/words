            "senses": [
                {
                    "sense_id": "e705312f-b804-3029-8e65-2660364fed0d",
                    "sense_definition": "To oppose or confront someone boldly and bravely, often in their own territory.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",

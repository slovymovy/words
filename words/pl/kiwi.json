            "senses": [
                {
                    "sense_id": "9c35c6f2-479c-3cec-9ccd-cfea226015b7",
                    "sense_definition": "Jadalny owoc o brązowej, włochatej skórce i soczystym, zielonym miąższu z czarnymi pestkami.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fruit_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na drugie śniadanie zjadłem jogurt naturalny z kawałkami <w>kiwi</w>.",
                            "target_lang_translations": {
                                "ru": "На второй завтрак я съел натуральный йогурт с кусочками <w>киви</w>."
                            }
                        },
                        {
                            "text": "To <w>kiwi</w> jest jeszcze bardzo twarde i kwaśne, musi dojrzeć.",
                            "target_lang_translations": {
                                "ru": "Это <w>киви</w> еще очень твердое и кислое, оно должно дозреть."
                            }
                        }
                    ],
                    "synonyms": [
                        "aktinidia"
                    ],
                    "common_phrases": [
                        "dojrzałe kiwi",
                        "kwaśne kiwi",
                        "obrać kiwi",
                        "sałatka z kiwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный фрукт с коричневой мохнатой кожурой и сочной зеленой мякотью с черными семенами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "киви",
                                "target_lang_sense_clarification": "фрукт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37c51a22-5548-3aa3-9114-9eeb04e84d18",
                    "sense_definition": "Nielotny ptak o krępej budowie i długim dziobie, zamieszkujący Nową Zelandię.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ptak <w>kiwi</w> jest rozpoznawalnym symbolem narodowym Nowej Zelandii.",
                            "target_lang_translations": {
                                "ru": "Птица <w>киви</w> является узнаваемым национальным символом Новой Зеландии."
                            }
                        },
                        {
                            "text": "W zoo udało nam się zobaczyć, jak <w>kiwi</w> szuka pożywienia w ściółce.",
                            "target_lang_translations": {
                                "ru": "В зоопарке нам удалось увидеть, как <w>киви</w> ищет пищу в подстилке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ptak kiwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нелетающая птица с плотным телосложением и длинным клювом, обитающая в Новой Зеландии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "киви",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab990008-e9d3-395d-81db-5756065ba261",
                    "sense_definition": "Roślina pnąca z rodzaju aktinidia, na której rosną jadalne owoce o tej samej nazwie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fruit_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie posadziliśmy <w>kiwi</w>, ale wymaga ono dużo słońca i ciepła.",
                            "target_lang_translations": {
                                "ru": "В нашем саду мы посадили <w>киви</w>, но оно требует много солнца и тепла."
                            }
                        },
                        {
                            "text": "Liście <w>kiwi</w> są duże i sercowate, co czyni je rośliną ozdobną.",
                            "target_lang_translations": {
                                "ru": "Листья <w>киви</w> крупные и сердцевидные, что делает его декоративным растением."
                            }
                        }
                    ],
                    "synonyms": [
                        "aktinidia"
                    ],
                    "common_phrases": [
                        "uprawa kiwi",
                        "sadzonki kiwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вьющееся растение рода актинидия, на котором растут одноименные съедобные плоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "киви",
                                "target_lang_sense_clarification": "растение"
                            },
                            {
                                "target_lang_word": "актинидия",
                                "target_lang_sense_clarification": "ботаническое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]

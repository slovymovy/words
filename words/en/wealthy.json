    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8d909c13-b30a-31ad-939a-48f4adf811b0",
                    "sense_definition": "Possessing a large amount of money, property, or valuable possessions.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "financial_wealth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He comes from a very <w>wealthy</w> family that owns several businesses.",
                            "target_lang_translations": {
                                "ru": "Он из очень <w>богатой</w> семьи, владеющей несколькими предприятиями."
                            }
                        },
                        {
                            "text": "The <w>wealthy</w> businessman donated a large sum to the local hospital.",
                            "target_lang_translations": {
                                "ru": "<w>Состоятельный</w> бизнесмен пожертвовал крупную сумму местной больнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "rich",
                        "well off",
                        "well-to-do",
                        "moneyed",
                        "well-heeled",
                        "loaded"
                    ],
                    "antonyms": [
                        "poor",
                        "impoverished"
                    ],
                    "common_phrases": [
                        "wealthy family",
                        "wealthy businessman",
                        "wealthy nation",
                        "wealthy elite"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий большим количеством денег, имущества или ценностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "богатый",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "состоятельный",
                                "target_lang_sense_clarification": "формальный, подчеркивает статус"
                            },
                            {
                                "target_lang_word": "обеспеченный",
                                "target_lang_sense_clarification": "имеющий достаток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "731475ca-e56e-3bb8-8c8e-131954073a1f",
                    "sense_definition": "Having a lot of something desirable, such as history, culture, or resources.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abundance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The region is <w>wealthy</w> in natural resources like coal and iron.",
                            "target_lang_translations": {
                                "ru": "Этот регион <w>богат</w> природными ресурсами, такими как уголь и железо."
                            }
                        },
                        {
                            "text": "This ancient city is <w>wealthy</w> in cultural heritage and art.",
                            "target_lang_translations": {
                                "ru": "Этот древний город <w>богат</w> культурным наследием и искусством."
                            }
                        }
                    ],
                    "synonyms": [
                        "rich"
                    ],
                    "common_phrases": [
                        "wealthy in resources",
                        "wealthy in history"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изобилующий чем-либо ценным, например, историей, культурой или ресурсами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "богатый",
                                "target_lang_sense_clarification": "изобилующий чем-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bc502bfd-049a-3948-935a-2b8af4f13435",
                    "sense_definition": "Rich people considered as a group; usually preceded by \"the\".",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_wealth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government proposed new taxes on the <w>wealthy</w> to fund public services.",
                            "target_lang_translations": {
                                "ru": "Правительство предложило новые налоги для <w>богатых</w>, чтобы финансировать государственные услуги."
                            }
                        },
                        {
                            "text": "There is often a significant gap between the poor and the <w>wealthy</w>.",
                            "target_lang_translations": {
                                "ru": "Часто существует значительный разрыв между бедными и <w>богатыми</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rich"
                    ],
                    "antonyms": [
                        "poor"
                    ],
                    "common_phrases": [
                        "the wealthy",
                        "tax the wealthy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Богатые люди как социальная группа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "богатые",
                                "target_lang_sense_clarification": "субстантивированное прилагательное"
                            },
                            {
                                "target_lang_word": "состоятельные",
                                "target_lang_sense_clarification": "люди с достатком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f5e5986-25f3-320a-bca3-c4ef8516c9c7",
                    "sense_definition": "A rich person.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "financial_wealth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The club was full of young <w>wealthies</w> showing off their expensive cars.",
                            "target_lang_translations": {
                                "ru": "Клуб был полон молодых <w>богачей</w>, хвастающихся своими дорогими машинами."
                            }
                        },
                        {
                            "text": "He hoped to become a <w>wealthy</w> one day and travel the world.",
                            "target_lang_translations": {
                                "ru": "Он надеялся однажды стать <w>богачом</w> и путешествовать по миру."
                            }
                        }
                    ],
                    "synonyms": [
                        "moneybags"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in plural form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Богатый человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "богач",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12db73c9-f2bf-30dd-ad07-7ab372331508",
                    "sense_definition": "A specific variety of American apple originally from Minnesota.",
                    "learner_level": "C2",

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "82055733-24e2-3cbc-a835-e237ad9bc780",
                    "sense_definition": "Blijven glijden of verder glijden, vaak langer of verder dan oorspronkelijk bedoeld.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_continue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auto bleef op het gladde wegdek maar <w>doorglijden</w>.",
                            "target_lang_translations": {
                                "en": "The car just kept <w>sliding on</w> on the slippery road surface.",
                                "ru": "Машина продолжала <w>скользить</w> по скользкой дороге."
                            }
                        },
                        {
                            "text": "Als je nu vaart maakt, kun je een heel stuk <w>doorglijden</w> zonder te steppen.",
                            "target_lang_translations": {
                                "en": "If you pick up speed now, you can <w>slide on</w> for quite a distance without kicking.",
                                "ru": "Если сейчас разогнаться, можно долго <w>скользить</w> по инерции, не отталкиваясь."
                            }
                        },
                        {
                            "text": "De verdediger raakte zijn tegenstander door gevaarlijk te <w>doorglijden</w>.",
                            "target_lang_translations": {
                                "en": "The defender hit his opponent by dangerously <w>sliding through</w>.",
                                "ru": "Защитник задел соперника, опасно <w>проскользив</w> в него."
                            }
                        }
                    ],
                    "synonyms": [
                        "verder glijden"
                    ],
                    "antonyms": [
                        "stoppen",
                        "remmen",
                        "tot stilstand komen"
                    ],
                    "common_phrases": [
                        "op de tegenstander doorglijden",
                        "blijven doorglijden"
                    ],
                    "target_lang_definitions": {
                        "en": "To continue sliding or slide further, often longer or further than originally intended.",
                        "ru": "Продолжать скользить или скользить дальше, часто дольше или дальше, чем предполагалось."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slide on",
                                "target_lang_sense_clarification": "continue moving over a smooth surface"
                            },
                            {
                                "target_lang_word": "skid on",
                                "target_lang_sense_clarification": "continue sliding uncontrollably (e.g., a vehicle)"
                            },
                            {
                                "target_lang_word": "slide through",
                                "target_lang_sense_clarification": "continue a sliding tackle to hit a player (sports)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проскользить",
                                "target_lang_sense_clarification": "проехать какое-то расстояние скользя"
                            },
                            {
                                "target_lang_word": "продолжать скользить",
                                "target_lang_sense_clarification": "не останавливаться, скользить дальше"
                            },
                            {
                                "target_lang_word": "проехать юзом",
                                "target_lang_sense_clarification": "продолжать движение скольжением (об автомобиле)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c97c1580-3669-3acb-8827-e3b1cd1965e1",
                    "sense_definition": "Zich glijdend door een opening, ruimte of barrière heen bewegen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "motion_through",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt makkelijk onder het hek <w>doorglijden</w> als er sneeuw ligt.",
                            "target_lang_translations": {
                                "en": "You can easily <w>slide through</w> under the fence when there is snow.",
                                "ru": "Когда лежит снег, можно легко <w>проскользнуть</w> под забором."
                            }
                        },
                        {
                            "text": "Het pakketje moet soepel door de buis <w>doorglijden</w>.",
                            "target_lang_translations": {
                                "en": "The package must <w>slide through</w> the tube smoothly.",
                                "ru": "Пакет должен плавно <w>проскользнуть</w> сквозь трубу."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorschuiven"
                    ],
                    "antonyms": [
                        "vastlopen",
                        "blijven steken"
                    ],
                    "common_phrases": [
                        "ergens onder doorglijden"
                    ],
                    "target_lang_definitions": {
                        "en": "To move through an opening, space, or barrier by sliding.",
                        "ru": "Проходить сквозь отверстие, пространство или барьер скользящим движением."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slide through",
                                "target_lang_sense_clarification": "pass through an opening while sliding"
                            },
                            {
                                "target_lang_word": "slip through",
                                "target_lang_sense_clarification": "pass smoothly or quickly through a narrow space"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проскользнуть",
                                "target_lang_sense_clarification": "быстро или легко пройти сквозь узкое место"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "schok",
        "schokkend",
        "geschokt"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d2c18d2e-37bc-3176-a331-0ce2c4d1114c",
                    "sense_definition": "Iemand hevig doen schrikken of emotioneel raken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotional_shock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het vreselijke nieuws <w>schokte</w> de hele gemeenschap.",
                            "target_lang_translations": {
                                "en": "The terrible news <w>shocked</w> the entire community.",
                                "ru": "Ужасная новость <w>потрясла</w> всё сообщество."
                            }
                        },
                        {
                            "text": "Zijn onbeleefde opmerkingen zullen de gasten zeker <w>schokken</w>.",
                            "target_lang_translations": {
                                "en": "His rude remarks will certainly <w>shock</w> the guests.",
                                "ru": "Его грубые замечания наверняка <w>шокируют</w> гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "choqueren"
                    ],
                    "common_phrases": [
                        "de wereld schokken",
                        "diep schokken"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause someone to be severely startled or emotionally affected.",
                        "ru": "Вызывать у кого-то сильный испуг или эмоциональное потрясение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to shock",
                                "target_lang_sense_clarification": "emotional impact"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шокировать",
                                "target_lang_sense_clarification": "вызывать шок"
                            },
                            {
                                "target_lang_word": "потрясать",
                                "target_lang_sense_clarification": "глубоко волновать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df205281-aba7-39fe-beda-f30d3f6532c2",
                    "sense_definition": "Met korte, krachtige bewegingen heen en weer gaan of trillen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude auto <w>schokte</w> hevig toen hij over de onverharde weg reed.",
                            "target_lang_translations": {
                                "en": "The old car <w>jolted</w> violently as it drove over the unpaved road.",
                                "ru": "Старая машина сильно <w>тряслась</w>, когда ехала по грунтовой дороге."
                            }
                        },
                        {
                            "text": "Door de kou begon de man over zijn hele lichaam te <w>schokken</w>.",
                            "target_lang_translations": {
                                "en": "Because of the cold, the man began to <w>shake</w> all over his body.",
                                "ru": "От холода мужчина начал <w>трястись</w> всем телом."
                            }
                        }
                    ],
                    "synonyms": [
                        "schudden",
                        "daveren"
                    ],
                    "common_phrases": [
                        "schokken en stoten"
                    ],
                    "target_lang_definitions": {
                        "en": "To move back and forth or vibrate with short, powerful movements.",
                        "ru": "Двигаться рывками, трястись или вибрировать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to jolt",
                                "target_lang_sense_clarification": "sudden, rough movement"
                            },
                            {
                                "target_lang_word": "to jerk",
                                "target_lang_sense_clarification": "spasmodic movement"
                            },
                            {
                                "target_lang_word": "to shake",
                                "target_lang_sense_clarification": "trembling movement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "трястись",
                                "target_lang_sense_clarification": "о движении или дрожи"
                            },
                            {
                                "target_lang_word": "дёргаться",
                                "target_lang_sense_clarification": "двигаться рывками"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b761177c-eff9-34b5-871a-706ee3227373",
                    "sense_definition": "Meervoud van <w>schok</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de aardbeving voelden de bewoners nog enkele lichte <w>schokken</w>.",
                            "target_lang_translations": {
                                "en": "After the earthquake, the residents felt a few more light <w>shocks</w>.",
                                "ru": "После землетрясения жители ощутили ещё несколько слабых <w>толчков</w>."
                            }
                        },
                        {
                            "text": "De economie heeft de afgelopen jaren diverse zware <w>schokken</w> moeten opvangen.",
                            "target_lang_translations": {
                                "en": "The economy has had to absorb several severe <w>shocks</w> in recent years.",
                                "ru": "В последние годы экономике пришлось пережить несколько тяжелых <w>потрясений</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "elektrische schokken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'schok' (shock).",
                        "ru": "Множественное число от слова \"schok\" (удар, толчок, потрясение)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shocks",
                                "target_lang_sense_clarification": "plural noun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "толчки",
                                "target_lang_sense_clarification": "физические удары или колебания"
                            },
                            {
                                "target_lang_word": "потрясения",
                                "target_lang_sense_clarification": "эмоциональные или экономические удары"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
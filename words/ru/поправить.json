                    "frequency": "Middle",
                    "semantic_group_id": "fix_correct",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если я ошибусь в датах, пожалуйста, <поправьте> меня.",
                            "target_lang_translations": {
                                "en": "If I make a mistake with the dates, please <correct> me."
                            }

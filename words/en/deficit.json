    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6a9ae63c-696d-3c16-8e75-c3a60305c376",
                    "sense_definition": "The amount by which money spent is greater than money received.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government is trying to reduce the budget <w>deficit</w> by cutting spending.",
                            "target_lang_translations": {
                                "ru": "Правительство пытается сократить бюджетный <w>дефицит</w> за счет урезания расходов."
                            }
                        },
                        {
                            "text": "Economists are worried about the growing trade <w>deficit</w>.",
                            "target_lang_translations": {
                                "ru": "Экономисты обеспокоены растущим торговым <w>дефицитом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fiscal deficit"
                    ],
                    "antonyms": [
                        "surplus"
                    ],
                    "common_phrases": [
                        "budget deficit",
                        "trade deficit",
                        "reduce the deficit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумма, на которую расходы превышают доходы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дефицит",
                                "target_lang_sense_clarification": "финансовый, бюджетный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c395ae1-4f93-3089-8600-dfe2a331e9da",
                    "sense_definition": "A lack or shortage of something required or expected.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team faced a <w>deficit</w> of experience compared to their opponents.",
                            "target_lang_translations": {
                                "ru": "Команда столкнулась с <w>нехваткой</w> опыта по сравнению с соперниками."
                            }
                        },
                        {
                            "text": "Farmers are struggling due to a severe <w>deficit</w> in rainfall this year.",
                            "target_lang_translations": {
                                "ru": "Фермеры испытывают трудности из-за серьезного <w>дефицита</w> осадков в этом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "shortfall"
                    ],
                    "antonyms": [
                        "surplus"
                    ],
                    "common_phrases": [
                        "deficit in",
                        "attention deficit"
                    ],
                    "target_lang_definitions": {

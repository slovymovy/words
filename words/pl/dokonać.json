    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2410b7fb-f818-34c7-b188-6071296b3760",
                    "sense_definition": "Zrealizować coś, przeprowadzić jakąś czynność lub doprowadzić sprawę do skutku (często wymaga to wysiłku lub jest formalne).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "accomplishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klient <dokonał> płatności kartą kredytową w naszym sklepie internetowym.",
                            "target_lang_translations": {
                                "ru": "Клиент <произвел> оплату кредитной картой в нашем интернет-магазине."
                            }
                        },
                        {
                            "text": "Musimy <dokonać> ostatecznego wyboru przed końcem tego tygodnia.",
                            "target_lang_translations": {
                                "ru": "Мы должны <сделать> окончательный выбор до конца этой недели."
                            }
                        },
                        {
                            "text": "Lekarze <dokonali> skomplikowanej operacji, która uratowała życie pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врачи <провели> сложную операцию, которая спасла жизнь пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrealizować",
                        "przeprowadzić",
                        "wykonać",
                        "zrobić",
                        "uczynić"
                    ],
                    "antonyms": [
                        "zaniechać"
                    ],
                    "common_phrases": [
                        "dokonać wyboru",
                        "dokonać zmiany",
                        "dokonać zakupu",
                        "dokonać wpłaty",
                        "dokonać odkrycia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Реализовать что-либо, выполнить какое-либо действие или довести дело до конца (часто требует усилий или носит формальный характер)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совершить",
                                "target_lang_sense_clarification": "выполнить действие (напр. сделку, покупку, акт)"
                            },
                            {
                                "target_lang_word": "произвести",
                                "target_lang_sense_clarification": "сделать, осуществить (напр. оплату, изменения, расчет)"
                            },
                            {
                                "target_lang_word": "осуществить",
                                "target_lang_sense_clarification": "реализовать, воплотить в жизнь"
                            },
                            {
                                "target_lang_word": "сделать",
                                "target_lang_sense_clarification": "общий синоним (напр. сделать выбор)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "160a3917-32e8-3c60-9c76-65386bf6abb3",
                    "sense_definition": "Dopuścić się jakiegoś czynu, zazwyczaj złego, niezgodnego z prawem lub normami moralnymi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "negative_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja wciąż poszukuje sprawcy, który <dokonał> napadu na bank.",
                            "target_lang_translations": {
                                "ru": "Полиция всё ещё ищет преступника, который <совершил> налет на банк."
                            }
                        },
                        {
                            "text": "Nie mogę uwierzyć, że on mógł <dokonać> tak strasznej zbrodni.",
                            "target_lang_translations": {
                                "ru": "Не могу поверить, что он мог <совершить> такое ужасное преступление."
                            }
                        }
                    ],
                    "synonyms": [
                        "popełnić",
                        "dopuścić się"
                    ],
                    "common_phrases": [
                        "dokonać przestępstwa",
                        "dokonać zbrodni",
                        "dokonać kradzieży",
                        "dokonać zamachu",
                        "dokonać gwałtu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершить какой-либо поступок, обычно плохой, противоречащий закону или моральным нормам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совершить",
                                "target_lang_sense_clarification": "сделать что-то противозаконное или предосудительное (преступление, ошибку)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11de33cb-10bd-38d1-8c6d-cfe97028a5cb",
                    "sense_definition": "Wydarzyć się, nastąpić lub dojść do skutku (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na naszych oczach <dokonała> się prawdziwa rewolucja technologiczna.",
                            "target_lang_translations": {
                                "ru": "На наших глазах <свершилась> настоящая технологическая революция."
                            }
                        },
                        {
                            "text": "Wczoraj w nocy <dokonał> się cud i pacjent odzyskał przytomność.",
                            "target_lang_translations": {
                                "ru": "Вчера ночью <свершилось> чудо, и пациент пришел в сознание."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydarzyć się",
                        "nastąpić",
                        "stać się",
                        "zajść"
                    ],
                    "common_phrases": [
                        "dokonał się postęp",
                        "dokonała się zmiana",
                        "dokonał się cud",
                        "sprawiedliwości stało się zadość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случиться, произойти или осуществиться (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свершиться",
                                "target_lang_sense_clarification": "произойти (о важных событиях, фактах, чудесах)"
                            },
                            {
                                "target_lang_word": "произойти",
                                "target_lang_sense_clarification": "случиться, настать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2a9d948-cba9-3b24-8424-6bad83a26831",
                    "sense_definition": "Zakończyć życie; umrzeć (zazwyczaj w kontekście podniosłym lub literackim).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędziwy pisarz <dokonał> żywota w swoim rodzinnym dworku na wsi.",
                            "target_lang_translations": {
                                "ru": "Пожилой писатель <скончался> в своей родной усадьбе в деревне."
                            }
                        },
                        {
                            "text": "Po długiej walce z chorobą pacjent <dokonał> dni w szpitalu.",
                            "target_lang_translations": {
                                "ru": "После долгой борьбы с болезнью пациент <окончил> свои дни в больнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "umrzeć",
                        "skonać",
                        "odejść"
                    ],
                    "antonyms": [
                        "narodzić się"
                    ],
                    "common_phrases": [
                        "dokonać żywota",
                        "dokonać dni"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary style"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Закончить жизнь; умереть (обычно в возвышенном или литературном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скончаться",
                                "target_lang_sense_clarification": "умереть (официально или возвышенно)"
                            },
                            {
                                "target_lang_word": "окончить",
                                "target_lang_sense_clarification": "завершить (свои дни, жизнь)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

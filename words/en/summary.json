            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ebd3cae9-35d5-3467-8842-01c642f06ab3",
                    "sense_definition": "A short statement that gives the main information about something without the details.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please write a brief <w>summary</w> of the article for the class.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, напишите <w>краткое изложение</w> статьи для класса."
                            }
                        },
                        {
                            "text": "In <w>summary</w>, the project was a success despite the delays.",
                            "target_lang_translations": {
                                "ru": "В <w>заключение</w>, проект оказался успешным, несмотря на задержки."
                            }
                        }
                    ],
                    "synonyms": [
                        "abstract",
                        "outline",
                        "overview",
                        "synopsis",
                        "digest"
                    ],
                    "common_phrases": [
                        "in summary",
                        "brief summary",
                        "executive summary",
                        "summary of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткое изложение основных положений или информации без деталей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "краткое изложение",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "резюме",
                                "target_lang_sense_clarification": "формальное или заключительное"
                            },
                            {
                                "target_lang_word": "сводка",
                                "target_lang_sense_clarification": "новостей или данных"
                            },
                            {
                                "target_lang_word": "итог",
                                "target_lang_sense_clarification": "вывод, заключение"
                            }
                        ]
                    }
                }
            ]
        },
        {

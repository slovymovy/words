            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a40876f4-06a0-3c7b-85ce-f703c951fa30",
                    "sense_definition": "Чувствовать жалость, сострадание к кому-либо, кто находится в беде или трудной ситуации.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка всегда <жалела> бездомных кошек и подкармливала их."
                        },
                        {
                            "text": "Не нужно <жалеть> себя, лучше попробуй исправить ситуацию."
                        }
                    ],
                    "synonyms": [
                        "сожалеть"
                    ],
                    "common_phrases": [
                        "жалеть сироту",
                        "жалеть себя",
                        "искренне жалеть"
                    ]
                },
                {
                    "sense_id": "cf73972c-c33d-3f8e-8568-3f58070a11f8",
                    "sense_definition": "Испытывать грусть или досаду по поводу чего-либо свершившегося, утраченного или упущенного.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я начал <жалеть>, что рассказал ему эту тайну."
                        },
                        {
                            "text": "Ты будешь <жалеть> об этом решении, когда станет поздно."
                        }
                    ],
                    "synonyms": [
                        "сожалеть"
                    ],
                    "common_phrases": [
                        "жалеть о прошлом",
                        "жалеть о сделанном",
                        "жалеть о случившемся"
                    ]
                },
                {
                    "sense_id": "5815b908-b92c-39cf-9475-8ac1a2841bec",
                    "sense_definition": "Беречь, воздерживаться от траты чего-либо (сил, средств, времени); скупиться.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители не <жалели> денег на хорошее образование для детей."
                        },
                        {
                            "text": "Спортсмен тренировался каждый день, не <жалея> сил."
                        }
                    ],
                    "synonyms": [
                        "сожалеть"
                    ],
                    "common_phrases": [
                        "не жалеть сил",
                        "не жалеть денег",
                        "не жалеть времени"
                    ]
                }
            ]
        }
    ]

                {
                    "sense_id": "27824328-ecdb-304b-abba-738d2ca4e072",
                    "sense_definition": "Błyszcząca ozdoba w formie krążka przyszywana do ubioru.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "decoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Suknię Iwony zdobiły liczne, kolorowe <w>cekiny</w>.",
                            "target_lang_translations": {
                                "ru": "Платье Ивоны украшали многочисленные разноцветные <w>пайетки</w>."
                            }
                        },
                        {
                            "text": "Na imprezę sylwestrową kupiła błyszczącą bluzkę w <w>cekiny</w>.",
                            "target_lang_translations": {
                                "ru": "На новогоднюю вечеринку она купила блестящую блузку с <w>пайетками</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pajeta"
                    ],
                    "common_phrases": [
                        "suknia z cekinami",
                        "bluzka w cekiny",
                        "wyszywany cekinami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Блестящее украшение в форме маленького кружка, пришиваемое к одежде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пайетка",
                                "target_lang_sense_clarification": "украшение для одежды"
                            },
                            {
                                "target_lang_word": "блёстка",
                                "target_lang_sense_clarification": "блестящая чешуйка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17a0e584-ee1e-33f2-9a93-5e273e56c7ad",
                    "sense_definition": "Złota moneta bita w Wenecji od XIII wieku.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wenecki <w>cekin</w> był niegdyś ważnym środkiem płatniczym w handlu.",
                            "target_lang_translations": {
                                "ru": "Венецианский <w>цехин</w> был когда-то важным платежным средством в торговле."
                            }
                        },
                        {
                            "text": "W muzeum numizmatycznym oglądaliśmy stary, złoty <w>cekin</w>.",
                            "target_lang_translations": {
                                "ru": "В нумизматическом музее мы рассматривали старинный золотой <w>цехин</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wenecki cekin",
                        "złoty cekin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная венецианская золотая монета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цехин",
                                "target_lang_sense_clarification": "старинная монета"
                            }
                        ]
                    }
                }
            ]
        }
    ]

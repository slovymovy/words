                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Романия",
                                "target_lang_sense_clarification": "историческое название Византии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2cb421a-fbe4-38e6-b9ae-4494a38a2041",
                    "sense_definition": "The Latin or Romance-speaking areas of Europe, collectively.",

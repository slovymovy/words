    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f872ded0-2bad-360e-b725-b0354a441488",
                    "sense_definition": "A covering for the head and neck, usually attached to a larger garment such as a jacket, coat, or sweatshirt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "covering_garment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It started to rain, so she pulled up her <w>hood</w> to keep her hair dry.",
                            "target_lang_translations": {
                                "ru": "Начался дождь, поэтому она натянула <w>капюшон</w>, чтобы не намочить волосы."
                            }
                        },
                        {
                            "text": "His winter coat has a fur-lined <w>hood</w> that protects his face from the wind.",
                            "target_lang_translations": {
                                "ru": "У его зимнего пальто есть отороченный мехом <w>капюшон</w>, который защищает лицо от ветра."
                            }
                        }
                    ],
                    "synonyms": [
                        "cowl"
                    ],
                    "common_phrases": [
                        "pull up your hood",
                        "coat with a hood",
                        "wear a hood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытие для головы и шеи, обычно прикрепленное к верхней одежде, такой как куртка, пальто или толстовка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капюшон",
                                "target_lang_sense_clarification": "Часть одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bcd16fb-f328-37d0-99b5-9cadd404765e",
                    "sense_definition": "The hinged metal cover over the engine of a motor vehicle.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "vehicle_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic opened the <w>hood</w> to check the oil level in the engine.",
                            "target_lang_translations": {
                                "ru": "Механик открыл <w>капот</w>, чтобы проверить уровень масла в двигателе."
                            }
                        },
                        {
                            "text": "Smoke began to rise from under the <w>hood</w> of the old truck.",
                            "target_lang_translations": {
                                "ru": "Из-под <w>капота</w> старого грузовика начал подниматься дым."
                            }
                        }
                    ],
                    "synonyms": [
                        "bonnet"
                    ],
                    "common_phrases": [
                        "under the hood",
                        "pop the hood",
                        "open the hood"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "United States"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Откидная металлическая крышка над двигателем автомобиля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капот",
                                "target_lang_sense_clarification": "Крышка двигателя автомобиля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b6e4185-51b2-3e55-b5b0-1cdff4b1b3ce",
                    "sense_definition": "A neighborhood, particularly a poor or urban one; often used to refer to the place where someone grew up.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "place_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He moved out of the <w>hood</w> years ago, but he still visits his family there.",
                            "target_lang_translations": {
                                "ru": "Он уехал из <w>района</w> много лет назад, но всё ещё навещает там свою семью."
                            }
                        },
                        {
                            "text": "The rapper writes songs about life in the <w>hood</w> and the struggles people face.",
                            "target_lang_translations": {
                                "ru": "Рэпер пишет песни о жизни в <w>гетто</w> и трудностях, с которыми сталкиваются люди."
                            }
                        }
                    ],
                    "synonyms": [
                        "neighborhood",
                        "ghetto"
                    ],
                    "common_phrases": [
                        "in the hood",
                        "from the hood"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "short for neighborhood"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Район, особенно бедный или городской; часто используется для обозначения места, где кто-то вырос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "район",
                                "target_lang_sense_clarification": "Разг. сокращение от neighborhood"
                            },
                            {
                                "target_lang_word": "гетто",
                                "target_lang_sense_clarification": "Неблагополучный район"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "beb9b134-639c-3ffd-9e2f-076e686fcca9",
                    "sense_definition": "A metal covering containing a fan, placed over a stove or cooker to remove smoke, steam, and fumes.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "kitchen_appliance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please turn on the <w>hood</w> while you are frying the fish to stop the smell spreading.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, включи <w>вытяжку</w>, пока жаришь рыбу, чтобы запах не распространялся."
                            }
                        },
                        {
                            "text": "The kitchen <w>hood</w> needs to be cleaned regularly to remove grease.",
                            "target_lang_translations": {
                                "ru": "Кухонную <w>вытяжку</w> нужно регулярно чистить, чтобы удалять жир."
                            }
                        }
                    ],
                    "common_phrases": [
                        "range hood",
                        "cooker hood",
                        "exhaust hood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство с вентилятором над кухонной плитой для удаления дыма и запахов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытяжка",
                                "target_lang_sense_clarification": "Кухонное устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bafdc258-8967-31a7-b4fc-473ee27acdb5",
                    "sense_definition": "A violent criminal, gangster, or troublemaker.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A group of young <w>hoods</w> was causing trouble in the downtown area last night.",
                            "target_lang_translations": {
                                "ru": "Группа молодых <w>хулиганов</w> вчера вечером устроила беспорядки в центре города."
                            }
                        },
                        {
                            "text": "In the movie, he plays a small-time <w>hood</w> who tries to rob a bank.",
                            "target_lang_translations": {
                                "ru": "В фильме он играет мелкого <w>бандита</w>, который пытается ограбить банк."
                            }
                        }
                    ],
                    "synonyms": [
                        "ghetto",
                        "shithole",
                        "nabe",
                        "neighborhood",
                        "blinder",
                        "cowl",
                        "bonnet",
                        "frill"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "short for hoodlum"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жестокий преступник, гангстер или хулиган."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бандит",
                                "target_lang_sense_clarification": "Преступник"
                            },
                            {
                                "target_lang_word": "хулиган",
                                "target_lang_sense_clarification": "Нарушитель порядка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4f79521-f110-31e0-a290-2bc3cd6afa8b",
                    "sense_definition": "An expansion of skin on the neck of certain snakes, such as cobras, which is spread when the animal is threatened.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cobra spread its <w>hood</w> and hissed loudly to warn the predator.",
                            "target_lang_translations": {
                                "ru": "Кобра раздула <w>капюшон</w> и громко зашипела, предупреждая хищника."
                            }
                        },
                        {
                            "text": "We saw a snake with a distinctive pattern on the back of its <w>hood</w>.",
                            "target_lang_translations": {
                                "ru": "Мы видели змею с характерным узором на задней части <w>капюшона</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cobra hood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расширяющаяся кожа на шее некоторых змей (например, кобр), которая раздувается при угрозе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капюшон",
                                "target_lang_sense_clarification": "Часть тела змеи"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "71db71ab-3723-3043-96ed-a8a8b45eaf79",
                    "sense_definition": "To cover something with a hood or similar covering.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_cover",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The falconer <w>hooded</w> the bird to keep it calm during the transport.",
                            "target_lang_translations": {
                                "ru": "Сокольник <w>надел клобук на</w> птицу, чтобы она оставалась спокойной во время перевозки."
                            }
                        },
                        {
                            "text": "The mysterious figure was <w>hooded</w> and cloaked in black.",
                            "target_lang_translations": {
                                "ru": "Таинственная фигура была <w>в капюшоне</w> и чёрном плаще."
                            }
                        }
                    ],
                    "antonyms": [
                        "unhood"
                    ],
                    "common_phrases": [
                        "hooded eyes",
                        "hooded figure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Накрывать что-либо капюшоном или подобным покрытием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надевать капюшон",
                                "target_lang_sense_clarification": "Действие"
                            },
                            {
                                "target_lang_word": "надевать клобук",
                                "target_lang_sense_clarification": "Термин соколиной охоты"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3a9cf8ee-6787-3277-b70e-294567f4621e",
                    "sense_definition": "Relating to the culture, lifestyle, or characteristics of inner-city neighborhoods.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adj_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie portrays the gritty reality of <w>hood</w> life in the 1990s.",
                            "target_lang_translations": {
                                "ru": "Фильм изображает суровую реальность <w>уличной</w> жизни 90-х."
                            }
                        },
                        {
                            "text": "He has a lot of <w>hood</w> pride and works to improve his community.",
                            "target_lang_translations": {
                                "ru": "Он гордится своим <w>районом</w> и работает над улучшением своего сообщества."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к культуре, образу жизни или характеристикам бедных городских районов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уличный",
                                "target_lang_sense_clarification": "Сленг, относящийся к жизни района"
                            },
                            {
                                "target_lang_word": "дворовый",
                                "target_lang_sense_clarification": "Относящийся к дворовой культуре"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "994f48d1-d0cb-3be9-9498-770a4f49f157",
                    "sense_definition": "A surname.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "proper_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Robin <w>Hood</w> is a legendary heroic outlaw originally depicted in English folklore.",
                            "target_lang_translations": {
                                "ru": "Робин <w>Гуд</w> — легендарный герой-разбойник, изначально описанный в английском фольклоре."
                            }
                        },
                        {
                            "text": "Mount <w>Hood</w> in Oregon was named after a British admiral.",
                            "target_lang_translations": {
                                "ru": "Гора <w>Худ</w> в Орегоне была названа в честь британского адмирала."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",

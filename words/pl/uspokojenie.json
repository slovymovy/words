    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b800322-260e-3138-887c-0ada8c4de2af",
                    "sense_definition": "Czynność polegająca na przywróceniu spokoju, ciszy lub równowagi; rzeczownik odczasownikowy od słowa <w>uspokoić</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz przepisał pacjentowi ziołowe tabletki na <uspokojenie> nerwów.",
                            "target_lang_translations": {
                                "ru": "Врач прописал пациенту травяные таблетки для <w>успокоения</w> нервов."
                            }
                        },
                        {
                            "text": "Jej łagodny głos wpłynął na szybkie <uspokojenie> płaczącego dziecka.",
                            "target_lang_translations": {
                                "ru": "Её мягкий голос способствовал быстрому <w>успокоению</w> плачущего ребёнка."
                            }
                        }
                    ],
                    "synonyms": [
                        "uciszenie",
                        "ukojenie"
                    ],
                    "antonyms": [
                        "zdenerwowanie",
                        "rozgniewanie",
                        "zezłoszczenie"
                    ],
                    "common_phrases": [
                        "na uspokojenie",
                        "środki na uspokojenie",
                        "tabletki na uspokojenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в восстановлении спокойствия, тишины или равновесия; отглагольное существительное от слова «успокоить»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "успокоение",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "умиротворение",
                                "target_lang_sense_clarification": "процесс восстановления мира"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b49eb33-e6cb-38ed-9133-602397d6f0f7",
                    "sense_definition": "Stan wewnętrznego spokoju, ulgi i odprężenia, odczuwany po ustąpieniu lęku lub nerwowości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długiej i szczerej rozmowie poczuła wreszcie głębokie <uspokojenie>.",
                            "target_lang_translations": {
                                "ru": "После долгого и искреннего разговора она наконец почувствовала глубокое <w>успокоение</w>."
                            }
                        },
                        {
                            "text": "Widok spokojnego morza zawsze przynosi mi <uspokojenie> i radość.",
                            "target_lang_translations": {
                                "ru": "Вид спокойного моря всегда приносит мне <w>умиротворение</w> и радость."
                            }
                        }
                    ],
                    "synonyms": [
                        "ulga",
                        "odprężenie",
                        "relaks",
                        "ukojenie"
                    ],
                    "antonyms": [
                        "zdenerwowanie",
                        "nieuspokojenie"
                    ],
                    "common_phrases": [
                        "poczuć uspokojenie",
                        "głębokie uspokojenie",
                        "przynosić uspokojenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние внутреннего покоя, облегчения и расслабления, ощущаемое после того, как проходит страх или нервозность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "успокоение",
                                "target_lang_sense_clarification": "душевное состояние"
                            },
                            {
                                "target_lang_word": "умиротворение",
                                "target_lang_sense_clarification": "глубокий покой"
                            },
                            {
                                "target_lang_word": "облегчение",
                                "target_lang_sense_clarification": "чувство снятия напряжения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b94f07e4-f457-3f80-add4-60e26ac9e714",
                    "sense_definition": "Sytuacja charakteryzująca się brakiem napięcia, hałasu, gwałtownych zmian lub konfliktów w otoczeniu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Analitycy przewidują wkrótce <uspokojenie> na rynkach finansowych.",
                            "target_lang_translations": {
                                "ru": "Аналитики прогнозируют скорую <w>стабилизацию</w> на финансовых рынках."
                            }
                        },
                        {
                            "text": "Po burzliwych protestach w mieście nastąpiło chwilowe <uspokojenie>.",
                            "target_lang_translations": {
                                "ru": "После бурных протестов в городе наступило временное <w>затишье</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stabilizacja",
                        "cisza"
                    ],
                    "common_phrases": [
                        "uspokojenie sytuacji",
                        "uspokojenie nastrojów",
                        "uspokojenie na rynku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, характеризующаяся отсутствием напряжения, шума, резких изменений или конфликтов в окружающей среде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стабилизация",
                                "target_lang_sense_clarification": "ситуации, рынков"
                            },
                            {
                                "target_lang_word": "затишье",
                                "target_lang_sense_clarification": "временное спокойствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9919367-2bc0-3f50-a998-dff5aea021a4",
                    "sense_definition": "Doprowadzenie spraw do porządku lub uregulowanie jakiejś kwestii.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prawnicy pracują nad formalnym dokumentem, który ma na celu <uspokojenie> roszczeń obu stron.",
                            "target_lang_translations": {
                                "ru": "Юристы работают над официальным документом, цель которого — <w>урегулирование</w> претензий обеих сторон."
                            }
                        },
                        {
                            "text": "Ostateczne <uspokojenie> tej sprawy wymaga jeszcze wielu negocjacji.",
                            "target_lang_translations": {
                                "ru": "Окончательное <w>урегулирование</w> этого дела требует ещё множества переговоров."
                            }
                        }
                    ],
                    "synonyms": [
                        "uregulowanie",
                        "uporządkowanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приведение дел в порядок или урегулирование какого-либо вопроса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "урегулирование",
                                "target_lang_sense_clarification": "спора, вопроса"
                            },
                            {
                                "target_lang_word": "разрешение",
                                "target_lang_sense_clarification": "конфликта"
                            }
                        ]
                    }
                }
            ]

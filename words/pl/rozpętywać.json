    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "54a45679-8289-3fe1-aaf5-69927750268f",
                    "sense_definition": "Powodować gwałtowne rozpoczęcie się czegoś, zazwyczaj niebezpiecznego lub intensywnego, jak wojna, kłótnia lub żywioł.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieodpowiedzialne wypowiedzi polityków mogą łatwo <w>rozpętywać</w> konflikty dyplomatyczne.",
                            "target_lang_translations": {
                                "ru": "Безответственные высказывания политиков могут легко <w>развязывать</w> дипломатические конфликты."
                            }
                        },
                        {
                            "text": "Nie chciałem <w>rozpętywać</w> burzy w szklance wody, więc przemilczałem ten komentarz.",
                            "target_lang_translations": {
                                "ru": "Я не хотел <w>поднимать</w> бурю в стакане воды, поэтому промолчал."
                            }
                        }
                    ],
                    "synonyms": [
                        "wywoływać",
                        "wszczynać",
                        "inicjować"
                    ],
                    "antonyms": [
                        "hamować",
                        "powstrzymywać",
                        "łagodzić",
                        "uśmierzać"
                    ],
                    "common_phrases": [
                        "rozpętywać wojnę",
                        "rozpętywać piekło",
                        "rozpętywać burzę",
                        "rozpętywać nagonkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать внезапное и бурное начало чего-либо, обычно опасного или интенсивного (войны, ссоры, стихии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развязывать",
                                "target_lang_sense_clarification": "о войне, конфликте или кампании"
                            },
                            {
                                "target_lang_word": "затевать",
                                "target_lang_sense_clarification": "о ссоре, споре или интриге"
                            },
                            {
                                "target_lang_word": "разжигать",
                                "target_lang_sense_clarification": "о страстях, ненависти или конфликте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8159bd72-d81e-3162-9be2-77f7495dc571",
                    "sense_definition": "Zaczynać się w sposób nagły i gwałtowny (używane z zaimkiem zwrotnym „się”), dotyczy zazwyczaj zjawisk atmosferycznych lub chaosu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad miastem <w>rozpętuje</w> się potężna nawałnica, która łamie drzewa.",
                            "target_lang_translations": {
                                "ru": "Над городом <w>разыгрывается</w> мощная буря, которая ломает деревья."
                            }
                        },
                        {
                            "text": "W internecie często <w>rozpętuje</w> się gorąca dyskusja pod kontrowersyjnymi artykułami.",
                            "target_lang_translations": {
                                "ru": "В интернете под спорными статьями часто <w>разгорается</w> горячая дискуссия."
                            }
                        }
                    ],
                    "synonyms": [
                        "wybuchać"
                    ],
                    "antonyms": [
                        "cichnąć",
                        "ustawać",
                        "wygasać"
                    ],
                    "common_phrases": [
                        "piekło się rozpętuje",
                        "burza się rozpętuje",
                        "rozpętywać się na dobre"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинаться внезапно и бурно (обычно о погодных явлениях или хаосе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разыгрываться",
                                "target_lang_sense_clarification": "о буре, шторме или трагедии"
                            },
                            {
                                "target_lang_word": "разражаться",
                                "target_lang_sense_clarification": "о грозе, скандале или смехе"
                            },
                            {
                                "target_lang_word": "вспыхивать",
                                "target_lang_sense_clarification": "внезапно возникать (о споре, огне)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28b15e94-09ee-3aef-86b2-597743b8b43b",
                    "sense_definition": "Uwalniać kogoś lub coś z więzów, sznurów lub pęt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "literal_untie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stajenny musiał szybko <w>rozpętywać</w> konie, aby wyprowadzić je z płonącego budynku.",
                            "target_lang_translations": {
                                "ru": "Конюх должен был быстро <w>отвязывать</w> лошадей, чтобы вывести их из горящего здания."
                            }
                        },
                        {
                            "text": "Bohater powieści pod osłoną nocy próbował <w>rozpętywać</w> swoich uwięzionych towarzyszy.",
                            "target_lang_translations": {
                                "ru": "Герой романа под покровом ночи пытался <w>развязывать</w> своих пленных товарищей."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozwiązywać",
                        "oswobadzać",
                        "uwalniać"
                    ],
                    "antonyms": [
                        "pętać",
                        "wiązać",
                        "krępować"
                    ],
                    "common_phrases": [
                        "rozpętywać konia",
                        "rozpętywać więźnia"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Освобождать кого-либо или что-либо от уз, веревок или пут."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распутывать",
                                "target_lang_sense_clarification": "освобождать от пут, расправлять"
                            },
                            {
                                "target_lang_word": "развязывать",
                                "target_lang_sense_clarification": "снимать веревки или узы"
                            },
                            {
                                "target_lang_word": "освобождать от пут",
                                "target_lang_sense_clarification": "буквальное значение"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "czarny",
        "czernić",
        "czernieć",
        "czernidło",
        "czarnina"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "93c4c32a-3d60-3f8a-8d8e-d33a5b645924",
                    "sense_definition": "Najciemniejsza z barw, kolor czarny; przeciwstawny do bieli.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głęboka <w>czerń</w> jej włosów kontrastowała z jasną cerą.",
                            "target_lang_translations": {
                                "ru": "Глубокая <w>чернота</w> её волос контрастировала со светлой кожей."
                            }
                        },
                        {
                            "text": "Artysta użył dużo <w>czerni</w>, aby oddać mroczny nastrój obrazu.",
                            "target_lang_translations": {
                                "ru": "Художник использовал много <w>чёрного</w>, чтобы передать мрачное настроение картины."
                            }
                        }
                    ],
                    "synonyms": [
                        "czarność"
                    ],
                    "antonyms": [
                        "biel"
                    ],
                    "common_phrases": [
                        "głęboka czerń",
                        "kolor czerni",
                        "absolutna czerń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый тёмный из цветов, чёрный цвет; противоположность белому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрный цвет",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "чернота",
                                "target_lang_sense_clarification": "свойство цвета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "617c85f3-8e68-3ce0-8889-c1609d60b7d8",
                    "sense_definition": "Całkowity brak światła, głęboka ciemność.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "darkness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód zniknął w gęstej <w>czerni</w> nocy.",
                            "target_lang_translations": {
                                "ru": "Машина исчезла в густой <w>черноте</w> ночи."
                            }
                        },
                        {
                            "text": "Z jaskini wiało chłodem i nieprzeniknioną <w>czernią</w>.",
                            "target_lang_translations": {
                                "ru": "Из пещеры веяло холодом и непроглядной <w>тьмой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciemność"
                    ],
                    "antonyms": [
                        "jasność"
                    ],
                    "common_phrases": [
                        "czerń nocy",
                        "ziać czernią",
                        "egipskie ciemności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полное отсутствие света, глубокая темнота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тьма",
                                "target_lang_sense_clarification": "отсутствие света"
                            },
                            {
                                "target_lang_word": "чернота",
                                "target_lang_sense_clarification": "мрак"
                            },
                            {
                                "target_lang_word": "мрак",
                                "target_lang_sense_clarification": "густая темнота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05868c51-512f-3c12-80d9-28e8bef697a3",
                    "sense_definition": "Ubranie w czarnym kolorze, noszone często na znak żałoby lub jako strój wieczorowy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po śmierci męża wdowa nosiła wyłącznie <w>czerń</w>.",
                            "target_lang_translations": {
                                "ru": "После смерти мужа вдова носила исключительно <w>чёрное</w>."
                            }
                        },
                        {
                            "text": "Na uroczystą kolację wszyscy goście założyli elegancką <w>czerń</w>.",
                            "target_lang_translations": {
                                "ru": "На торжественный ужин все гости надели элегантное <w>чёрное</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "biel"
                    ],
                    "common_phrases": [
                        "nosić czerń",
                        "ubrać się w czerń",
                        "żałobna czerń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда чёрного цвета, которую часто носят в знак траура или как вечерний наряд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёрное",
                                "target_lang_sense_clarification": "одежда"
                            },
                            {
                                "target_lang_word": "траур",
                                "target_lang_sense_clarification": "траурная одежда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39acea1e-142f-3694-84d6-29c221b4e16f",
                    "sense_definition": "Czarny barwnik, pigment lub farba używana w przemyśle i sztuce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Czerń</w> kostna jest pigmentem otrzymywanym ze zwęglonych kości.",
                            "target_lang_translations": {
                                "ru": "Костная <w>чернь</w> — это пигмент, получаемый из обугленных костей."
                            }
                        },
                        {
                            "text": "Drukarz sprawdził poziom <w>czerni</w> w maszynie.",
                            "target_lang_translations": {
                                "ru": "Печатник проверил уровень <w>чёрной краски</w> в машине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czerń żelazowa",
                        "czerń anilinowa",
                        "czerń amidowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чёрный краситель, пигмент или краска, используемые в промышленности и искусстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чернь",
                                "target_lang_sense_clarification": "пигмент"
                            },
                            {
                                "target_lang_word": "чёрная краска",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ef7009b-0d09-30ae-8983-5881c1f0979a",
                    "sense_definition": "Historyczne, zbiorowe określenie niższych warstw społecznych, zwłaszcza chłopstwa na Ukrainie w XVII wieku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "social_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zbuntowana <w>czerń</w> domagała się poprawy swojego losu.",
                            "target_lang_translations": {
                                "ru": "Взбунтовавшаяся <w>чернь</w> требовала улучшения своей участи."
                            }
                        },
                        {
                            "text": "Hetman próbował negocjować, lecz <w>czerń</w> była nieustępliwa.",
                            "target_lang_translations": {
                                "ru": "Гетман пытался вести переговоры, но <w>чернь</w> была неуступчива."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy historycznych grup społecznych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Историческое собирательное название низших слоёв общества, особенно крестьянства на Украине в XVII веке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чернь",
                                "target_lang_sense_clarification": "простонародье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "deb2813f-08be-37d4-b4f4-5a684fd5d55e",
                    "sense_definition": "Nazwa chorób grzybowych roślin, które objawiają się czernieniem tkanek.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uprawy pszenicy zostały zniszczone przez <w>czerń</w>.",
                            "target_lang_translations": {
                                "ru": "Посевы пшеницы были уничтожены <w>чернью</w>."
                            }
                        },
                        {
                            "text": "Rolnicy stosują opryski, aby zwalczyć <w>czerń</w> krzyżowych.",
                            "target_lang_translations": {
                                "ru": "Фермеры используют опрыскивание, чтобы побороть <w>чернь</w> крестоцветных."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название грибковых заболеваний растений, проявляющихся почернением тканей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чернь",
                                "target_lang_sense_clarification": "болезнь растений"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d151de20-35fe-3d13-82f5-fb1c425cb156",
                    "sense_definition": "Forma trybu rozkazującego (2. os. lp) od czasownika <w>czernić</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>czerń</w> tak bardzo tego tła, bo obraz będzie zbyt smutny.",
                            "target_lang_translations": {
                                "ru": "Не <w>черни</w> так сильно этот фон, а то картина будет слишком грустной."
                            }
                        },
                        {
                            "text": "<w>Czerń</w> buty pastą, aż będą lśniły.",
                            "target_lang_translations": {
                                "ru": "<w>Черни</w> ботинки кремом, пока они не заблестят."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "tryb rozkazujący"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения (2-е лицо ед. ч.) от глагола czernić (чернить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черни",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
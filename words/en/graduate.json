    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "60902527-61db-3ee4-acae-cfe69efc6cd1",
                    "sense_definition": "A person who has successfully completed a course of study or training, especially one who has received an academic degree from a university or college.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "education_completion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a recent <w>graduate</w>, she is looking for her first full-time job.",
                            "target_lang_translations": {
                                "ru": "Как недавняя <w>выпускница</w>, она ищет свою первую работу на полный день."
                            }
                        },
                        {
                            "text": "The university <w>graduate</w> threw his cap in the air to celebrate.",
                            "target_lang_translations": {
                                "ru": "<w>Выпускник</w> университета подбросил свою шапочку в воздух, чтобы отпраздновать."
                            }
                        }
                    ],
                    "antonyms": [
                        "student",
                        "drop-out"
                    ],
                    "common_phrases": [
                        "university graduate",
                        "recent graduate",
                        "college graduate",
                        "high school graduate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, успешно завершивший курс обучения или подготовки, особенно получивший ученую степень в университете или колледже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпускник",
                                "target_lang_sense_clarification": "человек, окончивший учебное заведение"
                            },
                            {
                                "target_lang_word": "выпускница",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74737cbb-dd15-38ac-8b2a-3a605596dcac",
                    "sense_definition": "A container, such as a cylinder or cup, marked with lines for measuring liquids.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chemist carefully poured the blue solution into the glass <w>graduate</w>.",
                            "target_lang_translations": {
                                "ru": "Химик осторожно налил синий раствор в стеклянную <w>мензурку</w>."
                            }
                        },
                        {
                            "text": "Use a <w>graduate</w> to measure exactly one hundred milliliters of water.",
                            "target_lang_translations": {
                                "ru": "Используйте <w>мензурку</w>, чтобы отмерить ровно сто миллилитров воды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "glass graduate",
                        "measuring graduate"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical or scientific context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Емкость, например цилиндр или стакан, с нанесенными делениями для измерения жидкостей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мензурка",
                                "target_lang_sense_clarification": "лабораторная посуда с делениями"
                            },
                            {
                                "target_lang_word": "мерный стакан",
                                "target_lang_sense_clarification": "сосуд для измерения объема"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4fb849c1-070e-300f-b83a-7a93fdd9bb7a",
                    "sense_definition": "To successfully complete an academic degree, course of training, or high school education.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "education_completion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother will <w>graduate</w> from university next summer.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>окончит</w> университет следующим летом."
                            }
                        },
                        {
                            "text": "She worked very hard to <w>graduate</w> with honors.",
                            "target_lang_translations": {
                                "ru": "Она очень усердно работала, чтобы <w>окончить</w> учебу с отличием."
                            }
                        }
                    ],
                    "common_phrases": [
                        "graduate from university",
                        "graduate from college",
                        "graduate with honors",
                        "graduate from high school"

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "45041346-adf8-3705-bf9c-d48a1c83c493",
                    "sense_definition": "A line of objects, often regularly spaced, such as seats in a theatre or plants in a garden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "linear_arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We managed to get seats in the front <w>row</w> of the cinema.",
                            "target_lang_translations": {
                                "ru": "Нам удалось взять места в первом <w>ряду</w> кинотеатра."
                            }
                        },
                        {
                            "text": "There is a long <w>row</w> of trees planted along the driveway.",
                            "target_lang_translations": {
                                "ru": "Вдоль подъездной дорожки высажен длинный <w>ряд</w> деревьев."
                            }
                        }
                    ],
                    "synonyms": [
                        "line",
                        "tier",
                        "series",
                        "sequence"
                    ],
                    "common_phrases": [
                        "in a row",
                        "front row",
                        "back row",
                        "row of seats"
                    ],
                    "target_lang_definitions": {

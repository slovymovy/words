                {
                    "sense_id": "79371cf9-167d-3725-b9cf-8f99d3ff130d",
                    "sense_definition": "Iets verstrekken of uitdelen aan mensen die er recht op hebben, vaak documenten of goederen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente kan paspoorten alleen op afspraak <w>uitreiken</w>.",
                            "target_lang_translations": {
                                "en": "The municipality can only <w>issue</w> passports by appointment.",
                                "ru": "Муниципалитет может <w>выдавать</w> паспорта только по предварительной записи."
                            }
                        },
                        {
                            "text": "De hulporganisatie <w>reikte</w> voedselpakketten <w>uit</w> aan de vluchtelingen.",
                            "target_lang_translations": {
                                "en": "The aid organization <w>distributed</w> food parcels to the refugees.",
                                "ru": "Благотворительная организация <w>раздавала</w> продуктовые наборы беженцам."
                            }
                        }
                    ],
                    "synonyms": [
                        "verstrekken",
                        "uitdelen"
                    ],
                    "antonyms": [
                        "inzamelen"
                    ],
                    "common_phrases": [
                        "paspoorten uitreiken",
                        "documenten uitreiken"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide or distribute something to people entitled to it, often documents or goods.",
                        "ru": "Выдавать или распределять что-либо людям, имеющим на это право, часто документы или товары."
                    },
                    "translations": {

                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train <w>approaches</w> the station at high speed.",
                            "target_lang_translations": {
                                "ru": "Поезд <w>приближается</w> к станции на высокой скорости."

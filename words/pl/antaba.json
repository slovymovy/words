    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "975db362-0999-3eda-807f-64e0adb757b1",
                    "sense_definition": "Metalowa sztaba służąca do ryglowania drzwi lub bramy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "security_bar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Strażnik opuścił ciężką <w>antabę</w>, aby zablokować główne wejście do zamku.",
                            "target_lang_translations": {
                                "ru": "Стражник опустил тяжелый <w>засов</w>, чтобы заблокировать главный вход в замок."
                            }
                        },
                        {
                            "text": "Stare, dębowe wrota były zabezpieczone od wewnątrz solidną, żelazną <w>antabą</w>.",
                            "target_lang_translations": {
                                "ru": "Старые дубовые ворота были защищены изнутри прочным железным <w>засовом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rygiel",
                        "zasuwa"
                    ],
                    "common_phrases": [
                        "ciężka antaba",
                        "żelazna antaba",
                        "zabezpieczyć antabą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический брус, служащий для запирания дверей или ворот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засов",
                                "target_lang_sense_clarification": "запорное устройство"
                            },
                            {
                                "target_lang_word": "задвижка",
                                "target_lang_sense_clarification": "механизм для запирания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "905566e1-9b5a-3f7d-baf9-140d164683b6",
                    "sense_definition": "Ozdobny uchwyt, zwykle w kształcie obręczy lub pierścienia, umieszczony na drzwiach, bramie lub skrzyni, służący do otwierania lub jako kołatka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "handle_knocker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gość chwycił za mosiężną <w>antabę</w> i głośno zastukał w drzwi.",
                            "target_lang_translations": {
                                "ru": "Гость взялся за латунное <w>дверное кольцо</w> и громко постучал в дверь."
                            }
                        },
                        {
                            "text": "Wielka skrzynia posagowa miała po bokach dwie <w>antaby</w>, które ułatwiały jej przenoszenie.",
                            "target_lang_translations": {
                                "ru": "Большой сундук с приданым имел по бокам две <w>ручки-кольца</w>, которые облегчали его переноску."
                            }
                        }
                    ],
                    "synonyms": [
                        "kołatka"
                    ],
                    "common_phrases": [
                        "mosiężna antaba",
                        "chwycić za antabę",
                        "stukać antabą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративная ручка, обычно в форме кольца, размещенная на дверях, воротах или сундуке, служащая для открывания или в качестве дверного молотка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дверное кольцо",
                                "target_lang_sense_clarification": "ручка в форме кольца"
                            },
                            {
                                "target_lang_word": "ручка-кольцо",
                                "target_lang_sense_clarification": "фурнитура сундука или двери"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "73a59f62-3ae6-3d7d-acd0-98d161b003e8",
                    "sense_definition": "Meervoud van <w>vat</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wijn wordt jarenlang in eikenhouten <w>vaten</w> bewaard om te rijpen.",
                            "target_lang_translations": {
                                "ru": "Вино годами выдерживается в дубовых <w>бочках</w> для созревания."
                            }
                        },
                        {
                            "text": "De arts onderzocht de grote <w>vaten</w> rondom het hart van de patiënt.",
                            "target_lang_translations": {
                                "ru": "Врач обследовал крупные <w>сосуды</w> вокруг сердца пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "vatten",
                        "vatten"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова \"vat\" (бочка, сосуд)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бочки",
                                "target_lang_sense_clarification": "деревянные или металлические емкости"
                            },
                            {
                                "target_lang_word": "сосуды",
                                "target_lang_sense_clarification": "трубчатые органы в теле (кровеносные)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fbec02ae-dd45-384c-a6ec-fbf8ed2ea80a",
                    "sense_definition": "De vuile vaat wassen; de afwas doen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het avondeten moesten de kinderen helpen <w>vaten</w> in de keuken.",
                            "target_lang_translations": {
                                "ru": "После ужина дети должны были помочь <w>мыть посуду</w> на кухне."
                            }
                        },
                        {
                            "text": "Zij staat elke avond te <w>vaten</w> omdat ze geen vaatwasser heeft.",
                            "target_lang_translations": {
                                "ru": "Она каждый вечер <w>моет посуду</w>, так как у нее нет посудомоечной машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "vatten",
                        "vatten"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in parts of the Netherlands and Belgium"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мыть грязную посуду; заниматься мытьем посуды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мыть посуду",
                                "target_lang_sense_clarification": "очищать кухонную утварь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a41a659-cb09-3e4d-8f9f-86741563f148",
                    "sense_definition": "Vloeistof of goederen in vaten of tonnen stoppen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brouwerij zal het bier morgenochtend <w>vaten</w> voor transport.",
                            "target_lang_translations": {
                                "ru": "Пивоварня завтра утром будет <w>разливать</w> пиво <w>по бочкам</w> для транспортировки."
                            }
                        },
                        {
                            "text": "Vroeger <w>vaatten</w> ze de haring direct na de vangst op het schip.",
                            "target_lang_translations": {
                                "ru": "Раньше сельдь <w>укладывали в бочки</w> прямо на корабле сразу после вылова."
                            }
                        }
                    ],
                    "synonyms": [
                        "vatten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать жидкость или товары в бочки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разливать по бочкам",
                                "target_lang_sense_clarification": "о жидкостях"
                            },
                            {
                                "target_lang_word": "укладывать в бочки",
                                "target_lang_sense_clarification": "о твердых товарах (например, рыбе)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "powołanie",
        "powoływać",
        "powołany"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "54b62988-1b5b-37bb-84c6-ce9c663f115d",
                    "sense_definition": "Wyznaczyć kogoś do pełnienia określonej funkcji, roli lub stanowiska.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "nominacje_i_tworzenie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rada nadzorcza postanowiła <w>powołać</w> nowego prezesa spółki.",
                            "target_lang_translations": {
                                "ru": "Наблюдательный совет решил <w>назначить</w> нового президента компании."
                            }
                        },
                        {
                            "text": "Prezydent zamierza <w>powołać</w> go na stanowisko ministra sprawiedliwości.",
                            "target_lang_translations": {
                                "ru": "Президент намерен <w>назначить</w> его на должность министра юстиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "mianować",
                        "wyznaczyć",
                        "nominować"
                    ],
                    "antonyms": [
                        "odwołać",
                        "zdymisjonować"
                    ],
                    "common_phrases": [
                        "powołać na stanowisko",
                        "powołać rząd",
                        "powołać zarząd",
                        "powołać biegłego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Назначить кого-либо на должность или для выполнения определенной функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "назначить",
                                "target_lang_sense_clarification": "на должность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a058b39c-90cd-383a-96ea-2ea8773eb613",
                    "sense_definition": "Utworzyć nową instytucję, organizację, zespół lub organ władzy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "nominacje_i_tworzenie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sejm zdecydował <w>powołać</w> specjalną komisję śledczą do zbadania tej afery.",
                            "target_lang_translations": {
                                "ru": "Сейм решил <w>создать</w> специальную следственную комиссию для расследования этой аферы."
                            }
                        },
                        {
                            "text": "Chcemy <w>powołać</w> do życia fundację, która będzie pomagać bezdomnym zwierzętom.",
                            "target_lang_translations": {
                                "ru": "Мы хотим <w>учредить</w> фонд, который будет помогать бездомным животным."
                            }
                        }
                    ],
                    "synonyms": [
                        "utworzyć",
                        "stworzyć",
                        "zawiązać"
                    ],
                    "antonyms": [
                        "rozwiązać",
                        "zlikwidować"
                    ],
                    "common_phrases": [
                        "powołać do życia",
                        "powołać komisję",
                        "powołać zespół",
                        "powołać fundację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создать новую организацию, учреждение, группу или орган власти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "создать",
                                "target_lang_sense_clarification": "организацию, комиссию"
                            },
                            {
                                "target_lang_word": "учредить",
                                "target_lang_sense_clarification": "официально основать"
                            },
                            {
                                "target_lang_word": "сформировать",
                                "target_lang_sense_clarification": "собрать состав людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1076f92a-6bcc-3869-92d7-c201037e1dd4",
                    "sense_definition": "Wskazać na coś lub kogoś jako na dowód, uzasadnienie lub autorytet (wymaga użycia zaimka \"się\").",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "argumentacja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W swoim piśmie musisz <w>powołać</w> się na odpowiedni paragraf kodeksu cywilnego.",
                            "target_lang_translations": {
                                "ru": "В своем письме ты должен <w>сослаться</w> на соответствующий параграф гражданского кодекса."
                            }
                        },
                        {
                            "text": "Student podczas egzaminu chciał <w>powołać</w> się na słowa znanego profesora.",
                            "target_lang_translations": {
                                "ru": "Студент во время экзамена хотел <w>сослаться</w> на слова известного профессора."
                            }
                        }
                    ],
                    "synonyms": [
                        "zacytować",
                        "przytoczyć"
                    ],
                    "common_phrases": [
                        "powołać się na przepis",
                        "powołać się na autorytet",
                        "powołać się na słowa",
                        "powołać się na znajomości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указать на кого-либо или что-либо как на источник, доказательство или авторитет (используется с возвратной частицей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сослаться",
                                "target_lang_sense_clarification": "на источник или закон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a9efe8b-99d5-3d46-9efb-82aa517f8c78",
                    "sense_definition": "Wezwać urzędowo do odbycia obowiązkowej służby wojskowej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "wojskowosc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W razie wybuchu wojny armia może natychmiast <w>powołać</w> rezerwistów.",
                            "target_lang_translations": {
                                "ru": "В случае начала войны армия может немедленно <w>призвать</w> резервистов."
                            }
                        },
                        {
                            "text": "Mój dziadek został <w>powołany</w> do wojska, gdy miał zaledwie dwadzieścia lat.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был <w>призван</w> в армию, когда ему было всего двадцать лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmobilizować",
                        "wcielić"
                    ],
                    "antonyms": [
                        "zwolnić"
                    ],
                    "common_phrases": [
                        "powołać do wojska",
                        "powołać pod broń",
                        "powołać do armii",
                        "powołać rezerwistów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально вызвать для прохождения обязательной военной службы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "призвать",
                                "target_lang_sense_clarification": "в армию"
                            }
                        ]
                    }
                }
            ]
        }
    ]

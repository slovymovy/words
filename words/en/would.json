{
    "word_family": [
        "will",
        "willing",
        "willful",
        "willingness",
        "willingly"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "43f808f2-71d3-391a-96e7-228e5e4b467d",
                    "sense_definition": "Used to ask someone to do something or to offer something politely.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "volition_request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Would</w> you like a cup of tea?",
                            "target_lang_translations": {
                                "ru": "<w>Хотели бы</w> вы чашку чая?"
                            }
                        },
                        {
                            "text": "<w>Would</w> you please pass me the salt?",
                            "target_lang_translations": {
                                "ru": "Вы не <w>могли бы</w> передать мне соль, пожалуйста?"
                            }
                        }
                    ],
                    "synonyms": [
                        "please",
                        "kindly"
                    ],
                    "common_phrases": [
                        "would you like",
                        "would you mind",
                        "would you please"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для вежливой просьбы сделать что-то или для вежливого предложения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не могли бы вы",
                                "target_lang_sense_clarification": "вежливая форма просьбы"
                            },
                            {
                                "target_lang_word": "хотели бы",
                                "target_lang_sense_clarification": "в предложениях предложения чего-либо (would like)"
                            },
                            {
                                "target_lang_word": "бы",
                                "target_lang_sense_clarification": "частица, смягчающая просьбу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e1a1242-d305-3be9-b82d-7326534dc072",
                    "sense_definition": "Used to describe a situation or action that is possible or imagined but not real, often depending on a condition.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hypothetical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If I had enough money, I <w>would</w> travel around the world.",
                            "target_lang_translations": {
                                "ru": "Если бы у меня было достаточно денег, я <w>бы</w> путешествовал по всему миру."
                            }
                        },
                        {
                            "text": "I <w>would</w> love to visit your new house someday.",
                            "target_lang_translations": {
                                "ru": "Я <w>бы</w> с удовольствием посетил твой новый дом когда-нибудь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "would like to",
                        "would love to",
                        "would rather"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для описания ситуации или действия, которые возможны или воображаемы, но не реальны, часто зависят от условий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бы",
                                "target_lang_sense_clarification": "частица условного наклонения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9a924bf-cc59-30e5-8501-942d3be13791",
                    "sense_definition": "Used to refer to the future from the perspective of the past.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "past_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She promised that she <w>would</w> call me as soon as she arrived.",
                            "target_lang_translations": {
                                "ru": "Она пообещала, что <w>позвонит</w> мне, как только приедет."
                            }
                        },
                        {
                            "text": "He knew he <w>would</w> never see the old house again.",
                            "target_lang_translations": {
                                "ru": "Он знал, что больше никогда не <w>увидит</w> старый дом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "said he would",
                        "knew she would",
                        "thought it would"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения будущего времени с точки зрения прошлого."
                    },
                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "fd720f59-8ab7-3967-a2b5-3c60b63026be",
                    "sense_definition": "Used to give advice or make a suggestion by imagining oneself in another person's situation.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "hypothetical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If I were you, I <w>would</w> accept the job offer immediately.",
                            "target_lang_translations": {
                                "ru": "На твоем месте я <w>бы</w> немедленно принял предложение о работе."
                            }
                        },
                        {
                            "text": "I <w>would</w> write a letter to complain about the service.",
                            "target_lang_translations": {
                                "ru": "Я <w>бы</w> написал письмо с жалобой на обслуживание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "I would suggest",
                        "I would recommend",
                        "if I were you"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для совета или предложения, когда говорящий представляет себя на месте другого человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бы",
                                "target_lang_sense_clarification": "в конструкции «на твоем месте я бы...»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45ff88c1-da39-3e9c-bf72-51dbc6d91f52",
                    "sense_definition": "Used to describe actions that happened repeatedly or habitually in the past but do not happen now.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "past_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When we were children, we <w>would</w> play in the park every afternoon.",
                            "target_lang_translations": {
                                "ru": "Когда мы были детьми, мы <w>играли</w> в парке каждый день после обеда."
                            }
                        },
                        {
                            "text": "My grandfather <w>would</w> sit on the porch and tell us stories for hours.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка <w>сидел</w> на крыльце и часами рассказывал нам истории."
                            }
                        }
                    ],
                    "synonyms": [
                        "used to"
                    ],
                    "common_phrases": [
                        "would always",
                        "would often",
                        "would usually"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для описания действий, которые регулярно происходили в прошлом, но не происходят сейчас."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бывало",
                                "target_lang_sense_clarification": "указывает на повторяемость в прошлом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce79beb9-ff20-308e-a812-42e13d81656d",
                    "sense_definition": "Used to express an opinion, belief, or intention in a polite, tentative, or less direct way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hypothetical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>would</w> imagine that the journey is quite difficult in winter.",
                            "target_lang_translations": {
                                "ru": "Я <w>бы</w> предположил, что путешествие зимой довольно трудное."
                            }
                        },
                        {
                            "text": "I <w>would</w> say that this is the best solution to our problem.",
                            "target_lang_translations": {
                                "ru": "Я <w>бы</w> сказал, что это лучшее решение нашей проблемы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "I would say",
                        "I would think",
                        "I would imagine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения мнения, убеждения или намерения в вежливой, осторожной или менее прямой форме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бы",
                                "target_lang_sense_clarification": "смягчает утверждение (я бы сказал...)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9431de45-7a00-3fcf-8ee4-7455d5bd9ee6",
                    "sense_definition": "Used to express a strong determination to do something in the past, or a refusal to function when used in the negative.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "volition_request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car <w>would</w> not start this morning no matter what I did.",
                            "target_lang_translations": {
                                "ru": "Машина <w>никак не</w> заводилась сегодня утром, что бы я ни делал."
                            }
                        },
                        {
                            "text": "I asked her to stay home, but she <w>would</w> go out anyway.",
                            "target_lang_translations": {
                                "ru": "Я просил её остаться дома, но она <w>все-таки</w> пошла гулять."
                            }
                        }
                    ],
                    "common_phrases": [
                        "would not",
                        "would not listen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения твердого намерения сделать что-то в прошлом или отказа функционировать (в отрицании)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никак не",
                                "target_lang_sense_clarification": "при отказе (о технике или людях)"
                            },
                            {
                                "target_lang_word": "настойчиво",
                                "target_lang_sense_clarification": "подразумеваемое упорство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f48d5e7a-5acf-316d-8758-2058fab091f7",
                    "sense_definition": "Used to express a strong probability, belief, or assumption about a situation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "hypothetical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doorbell is ringing; that <w>would</w> be the pizza delivery.",
                            "target_lang_translations": {
                                "ru": "Звонят в дверь; это, <w>должно быть</w>, доставка пиццы."
                            }
                        },
                        {
                            "text": "He is very organized, so he <w>would</w> have the documents ready.",
                            "target_lang_translations": {
                                "ru": "Он очень организован, так что документы у него, <w>наверное</w>, готовы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "that would be",
                        "it would seem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения высокой вероятности, уверенности или предположения о ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "должно быть",
                                "target_lang_sense_clarification": "предположение"
                            },
                            {
                                "target_lang_word": "наверное",
                                "target_lang_sense_clarification": "вероятность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b700f44-1085-336d-9d26-6183d8b59cbd",
                    "sense_definition": "Used to express a strong wish or desire.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "volition_request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>would</w> that I could fly away from here.",
                            "target_lang_translations": {
                                "ru": "<w>Как бы я хотел</w> улететь отсюда."
                            }
                        },
                        {
                            "text": "I <w>would</w> that we had never met.",
                            "target_lang_translations": {
                                "ru": "<w>Лучше бы</w> мы никогда не встречались."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary style"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения сильного желания (устаревшее или литературное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "если бы",
                                "target_lang_sense_clarification": "в восклицаниях о желаемом"
                            },
                            {
                                "target_lang_word": "как бы",
                                "target_lang_sense_clarification": "в значении «как бы я хотел»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2fa0096e-8fd9-31c7-941e-bb6e9089934e",
                    "sense_definition": "A possibility, potential event, or hypothetical circumstance that did not happen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should stop worrying about the <w>woulds</w> and coulds of the past.",
                            "target_lang_translations": {
                                "ru": "Тебе следует перестать беспокоиться о прошлых «<w>если бы</w>» да «кабы»."
                            }
                        },
                        {
                            "text": "Life is full of <w>woulds</w> that never became reality.",
                            "target_lang_translations": {
                                "ru": "Жизнь полна «<w>если бы</w>», которые так и не стали реальностью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "woulds and coulds",
                        "woulds and shoulds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность, потенциальное событие или гипотетическое обстоятельство, которое не произошло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "если бы да кабы",
                                "target_lang_sense_clarification": "идиома о несбыточном"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "3673bca5-9983-3087-baef-68924cb5a85d",
                    "sense_definition": "Used to indicate that the speaker finds another person sexually attractive.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I saw the new actor on TV and I definitely <w>would</w>.",
                            "target_lang_translations": {
                                "ru": "Я увидела нового актера по телевизору, и я <w>бы</w> точно не отказалась."
                            }
                        },
                        {
                            "text": "She looked at the photo of him and just said, \"<w>Would</w>.\"",
                            "target_lang_translations": {
                                "ru": "Она посмотрела на его фото и просто сказала: «<w>Я бы да</w>»."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на то, что говорящий находит другого человека сексуально привлекательным (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "я бы...",
                                "target_lang_sense_clarification": "подразумевается сексуальный подтекст"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
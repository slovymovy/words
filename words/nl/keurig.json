                            },
                            {
                                "target_lang_word": "воспитанный",
                                "target_lang_sense_clarification": "обладающий хорошими манерами"
                            },
                            {
                                "target_lang_word": "порядочный",
                                "target_lang_sense_clarification": "честный и достойный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b7112be-d506-3349-8dd5-72948618626c",
                    "sense_definition": "Erg netjes, schoon en goed verzorgd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij waren <keurig> gekleed voor het feest.",
                            "target_lang_translations": {

                    "sense_id": "afaf2cd4-e4bb-3ff9-a92c-12a4d68a5550",
                    "sense_definition": "A place name used for various locations in Ireland and the United States.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They visited the small village of <w>Curry</w> during their trip to Ireland.",
                            "target_lang_translations": {
                                "ru": "Они посетили небольшую деревню <w>Карри</w> во время поездки в Ирландию."

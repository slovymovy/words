                {
                    "sense_id": "a271ab0f-1464-3e15-a122-f822da448696",
                    "sense_definition": "Met intuïtieve, gemakkelijk manipuleerbare eigenschappen, zonder onregelmatigheden.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een functie is <w>braaf</w> als ze overal differentieerbaar is.",
                            "target_lang_translations": {
                                "en": "A function is <w>well-behaved</w> if it is differentiable everywhere.",
                                "ru": "Функция называется <w>гладкой</w>, если она дифференцируема везде."
                            }
                        },
                        {
                            "text": "In dit wiskundige model gaan we uit van een <w>brave</w> functie.",
                            "target_lang_translations": {
                                "en": "In this mathematical model, we assume a <w>well-behaved</w> function.",
                                "ru": "В этой математической модели мы исходим из <w>регулярной</w> функции."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "With intuitive, easily manipulable properties, without irregularities.",
                        "ru": "Обладающий удобными математическими свойствами, без аномалий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "well-behaved",
                                "target_lang_sense_clarification": "mathematics"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гладкий",
                                "target_lang_sense_clarification": "дифференцируемый (математический термин)"
                            },
                            {
                                "target_lang_word": "регулярный",
                                "target_lang_sense_clarification": "без особенностей"
                            }
                        ]
                    }
                }

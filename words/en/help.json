                    "frequency": "High",
                    "semantic_group_id": "restraint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot <w>help</w> worrying about the results of the exam."
                        },
                        {
                            "text": "She could not <w>help</w> laughing when she heard the funny joke."
                        }
                    ],
                    "common_phrases": [
                        "cannot help",
                        "can't help it"
                    ]
                }
            ]
        },

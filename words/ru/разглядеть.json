    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "02853dfa-dbd0-3731-808c-b59a36383c40",
                    "sense_definition": "Увидеть, рассмотреть или распознать кого-либо или что-либо, напрягая зрение или внимательно всматриваясь.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visual_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В густом тумане водитель с трудом смог <w>разглядеть</w> дорогу.",
                            "target_lang_translations": {
                                "en": "In the thick fog, the driver could hardly <w>make out</w> the road."
                            }
                        },
                        {
                            "text": "Я пытался <w>разглядеть</w> номер дома, но на улице было слишком темно.",
                            "target_lang_translations": {
                                "en": "I tried to <w>make out</w> the house number, but it was too dark outside."
                            }
                        }
                    ],
                    "synonyms": [
                        "рассмотреть",
                        "увидеть",
                        "заметить"
                    ],
                    "antonyms": [
                        "проглядеть"
                    ],
                    "common_phrases": [
                        "с трудом разглядеть",
                        "разглядеть в темноте",
                        "не разглядеть лица",
                        "успеть разглядеть"
                    ],
                    "target_lang_definitions": {
                        "en": "To manage to see, identify, or distinguish something, often with difficulty, by straining one's eyes or looking closely."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make out",
                                "target_lang_sense_clarification": "to see with difficulty"
                            },
                            {
                                "target_lang_word": "discern",
                                "target_lang_sense_clarification": "formal; to detect visually"
                            },
                            {
                                "target_lang_word": "distinguish",
                                "target_lang_sense_clarification": "to see clearly or identify details"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b095a5a9-d03b-3f60-bab1-31f245daa6ef",
                    "sense_definition": "Понять, обнаружить скрытые свойства, качества или способности кого-либо или чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный учитель сумел <w>разглядеть</w> в этом скромном мальчике талантливого музыканта.",
                            "target_lang_translations": {
                                "en": "The experienced teacher managed to <w>discern</w> a talented musician in this modest boy."
                            }
                        },
                        {
                            "text": "За внешней суровостью начальника было сложно <w>разглядеть</w> добрую душу.",
                            "target_lang_translations": {
                                "en": "Behind the boss's outward severity, it was hard to <w>discern</w> a kind soul."
                            }
                        }
                    ],
                    "synonyms": [
                        "распознать",
                        "обнаружить",
                        "постичь"
                    ],
                    "antonyms": [
                        "проглядеть"
                    ],
                    "common_phrases": [
                        "разглядеть талант",
                        "разглядеть человека",
                        "разглядеть потенциал"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To perceive, discover, or understand hidden qualities, potential, or the true nature of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "discern",
                                "target_lang_sense_clarification": "to perceive hidden qualities"
                            },
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "to perceive potential or nature"
                            },
                            {
                                "target_lang_word": "recognize",
                                "target_lang_sense_clarification": "to identify a quality"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
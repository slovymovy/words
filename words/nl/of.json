{
    "word_family": [
        "oftewel",
        "ofschoon"
    ],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "3a912d74-0a0d-3fef-be9c-8698cafa527d",
                    "sense_definition": "Voegwoord dat een keuze aangeeft tussen twee of meer mogelijkheden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "choice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil je koffie <w>of</w> thee bij het ontbijt?",
                            "target_lang_translations": {
                                "en": "Do you want coffee <w>or</w> tea with breakfast?",
                                "ru": "Ты хочешь кофе <w>или</w> чай на завтрак?"
                            }
                        },
                        {
                            "text": "We kunnen vanavond thuisblijven <w>of</w> naar de bioscoop gaan.",
                            "target_lang_translations": {
                                "en": "We can stay home tonight <w>or</w> go to the cinema.",
                                "ru": "Мы можем сегодня вечером остаться дома <w>или</w> пойти в кино."
                            }
                        }
                    ],
                    "synonyms": [
                        "ofwel",
                        "danwel"
                    ],
                    "common_phrases": [
                        "of niet",
                        "of zo",
                        "of wat"
                    ],
                    "target_lang_definitions": {
                        "en": "Conjunction indicating a choice between two or more possibilities.",
                        "ru": "Союз, указывающий на выбор между двумя или более возможностями."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "or",
                                "target_lang_sense_clarification": "conjunction linking alternatives"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "или",
                                "target_lang_sense_clarification": "выбор"
                            },
                            {
                                "target_lang_word": "либо",
                                "target_lang_sense_clarification": "выбор (более формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1418de52-3a69-39a5-bdab-6a214675d5cb",
                    "sense_definition": "Voegwoord dat een bijzin inleidt om onzekerheid of een indirecte vraag uit te drukken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "condition_question",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik weet niet <w>of</w> hij vandaag nog langskomt.",
                            "target_lang_translations": {
                                "en": "I don't know <w>if</w> he is still coming by today.",
                                "ru": "Я не знаю, зайдет <w>ли</w> он сегодня."
                            }
                        },
                        {
                            "text": "Ze vroeg zich af <w>of</w> het morgen gaat regenen.",
                            "target_lang_translations": {
                                "en": "She wondered <w>whether</w> it is going to rain tomorrow.",
                                "ru": "Она гадала, пойдет <w>ли</w> завтра дождь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "of niet"
                    ],
                    "target_lang_definitions": {
                        "en": "Conjunction introducing a subordinate clause to express uncertainty or an indirect question.",
                        "ru": "Союз, вводящий придаточное предложение для выражения неуверенности или косвенного вопроса."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whether",
                                "target_lang_sense_clarification": "introducing an indirect question or doubt"
                            },
                            {
                                "target_lang_word": "if",
                                "target_lang_sense_clarification": "introducing a condition or indirect question"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ли",
                                "target_lang_sense_clarification": "частица в значении союза (косвенный вопрос)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01ff3ca0-10b1-3e8e-8787-c1d3ef0bc05c",
                    "sense_definition": "Bij benadering; gebruikt in combinatie met een telwoord om een schatting aan te geven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "approximation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er waren een stuk <w>of</w> twintig mensen op het feest.",
                            "target_lang_translations": {
                                "en": "There were <w>about</w> twenty people at the party.",
                                "ru": "На вечеринке было <w>около</w> двадцати человек."
                            }
                        },
                        {
                            "text": "Hij heeft een jaar <w>of</w> vijf in het buitenland gewoond.",
                            "target_lang_translations": {
                                "en": "He lived abroad for <w>about</w> five years.",
                                "ru": "Он прожил за границей <w>около</w> пяти лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "ongeveer"
                    ],
                    "common_phrases": [
                        "een stuk of",
                        "een jaar of",
                        "een uur of"
                    ],
                    "target_lang_definitions": {
                        "en": "Approximately; used in combination with a numeral to indicate an estimate.",
                        "ru": "Приблизительно; используется в сочетании с числительным для указания примерного количества."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "about",
                                "target_lang_sense_clarification": "indicating an approximate number"
                            },
                            {
                                "target_lang_word": "approximately",
                                "target_lang_sense_clarification": "formal estimation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "приблизительное количество"
                            },
                            {
                                "target_lang_word": "примерно",
                                "target_lang_sense_clarification": "наречие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ef58947-0c80-3e29-85bc-f6dcf680ea88",
                    "sense_definition": "Gebruikt in een herhalende constructie om twee exclusieve mogelijkheden te benadrukken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "choice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet kiezen: <w>of</w> je gaat mee, <w>of</w> je blijft hier.",
                            "target_lang_translations": {
                                "en": "You have to choose: <w>either</w> you come along, <w>or</w> you stay here.",
                                "ru": "Ты должен выбрать: <w>или</w> ты идешь с нами, <w>или</w> остаешься здесь."
                            }
                        },
                        {
                            "text": "Het is <w>of</w> buigen, <w>of</w> barsten in deze situatie.",
                            "target_lang_translations": {
                                "en": "It is <w>either</w> bend <w>or</w> break in this situation.",
                                "ru": "В этой ситуации <w>или</w> пан, <w>или</w> пропал."
                            }
                        }
                    ],
                    "synonyms": [
                        "hetzij"
                    ],
                    "common_phrases": [
                        "of ... of"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in a repetitive construction to emphasize two exclusive possibilities.",
                        "ru": "Используется в повторяющейся конструкции для подчеркивания двух взаимоисключающих возможностей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "either ... or",
                                "target_lang_sense_clarification": "paired conjunctions for exclusive choice"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "или",
                                "target_lang_sense_clarification": "в конструкции или... или"
                            },
                            {
                                "target_lang_word": "либо",
                                "target_lang_sense_clarification": "в конструкции либо... либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58710e86-4f4f-3137-8972-9c29a570fd5d",
                    "sense_definition": "Anders gezegd; ter inleiding van een verduidelijking of een andere naam.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "clarification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De protagonist, <w>of</w> hoofdpersoon, speelt een cruciale rol in het verhaal.",
                            "target_lang_translations": {
                                "en": "The protagonist, <w>or</w> main character, plays a crucial role in the story.",
                                "ru": "Протагонист, <w>или</w> главный герой, играет ключевую роль в истории."
                            }
                        },
                        {
                            "text": "De Canis lupus familiaris, <w>of</w> de hond, is een populair huisdier.",
                            "target_lang_translations": {
                                "en": "The Canis lupus familiaris, <w>or</w> the dog, is a popular pet.",
                                "ru": "Canis lupus familiaris, <w>или</w> собака, — популярное домашнее животное."
                            }
                        }
                    ],
                    "synonyms": [
                        "oftewel"
                    ],
                    "target_lang_definitions": {
                        "en": "In other words; introducing a clarification or an alternative name.",
                        "ru": "Иначе говоря; для введения пояснения или другого названия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "or",
                                "target_lang_sense_clarification": "introducing an equivalent term"
                            },
                            {
                                "target_lang_word": "aka",
                                "target_lang_sense_clarification": "also known as"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "или",
                                "target_lang_sense_clarification": "пояснение"
                            },
                            {
                                "target_lang_word": "то есть",
                                "target_lang_sense_clarification": "уточнение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
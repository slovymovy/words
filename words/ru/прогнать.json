            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e50de987-bbac-3526-89a2-5890358f45a1",
                    "sense_definition": "Заставить уйти, удалиться откуда-либо; грубо выпроводить.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "force_leave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хозяин решил <прогнать> злую собаку со двора.",
                            "target_lang_translations": {
                                "en": "The owner decided to <w>chase away</w> the angry dog from the yard."
                            }
                        },
                        {
                            "text": "Она хотела <прогнать> его, но не смогла сказать ни слова.",
                            "target_lang_translations": {
                                "en": "She wanted to <w>kick</w> him <w>out</w>, but couldn't say a word."
                            }
                        }
                    ],
                    "synonyms": [
                        "выгнать",
                        "выставить"
                    ],
                    "antonyms": [
                        "пригласить",
                        "принять"
                    ],
                    "common_phrases": [
                        "прогнать из дома",
                        "прогнать прочь",
                        "прогнать собаку"
                    ],
                    "target_lang_definitions": {
                        "en": "To force someone to leave or go away; to send away roughly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chase away",
                                "target_lang_sense_clarification": "to force to leave"
                            },
                            {
                                "target_lang_word": "kick out",
                                "target_lang_sense_clarification": "informal, to force to leave roughly"
                            },
                            {
                                "target_lang_word": "drive away",
                                "target_lang_sense_clarification": "to force to leave (people or animals)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "574580fd-6426-3a1e-a050-34c4aed15e98",
                    "sense_definition": "Заставить исчезнуть, прекратиться (о мыслях, сне, чувствах или состояниях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "dispel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я вышел на улицу, чтобы <прогнать> сон свежим воздухом.",
                            "target_lang_translations": {
                                "en": "I went outside to <w>shake off</w> the sleepiness with fresh air."
                            }
                        },
                        {
                            "text": "Он пытался <прогнать> мрачные мысли, но они возвращались.",
                            "target_lang_translations": {
                                "en": "He tried to <w>drive away</w> the gloomy thoughts, but they kept coming back."
                            }
                        }
                    ],
                    "antonyms": [
                        "нагнать"
                    ],
                    "common_phrases": [
                        "прогнать сон",
                        "прогнать тоску",
                        "прогнать страх",
                        "прогнать мысли"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cause thoughts, sleep, feelings, or states to disappear or cease."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive away",
                                "target_lang_sense_clarification": "thoughts or feelings"
                            },
                            {
                                "target_lang_word": "dispel",
                                "target_lang_sense_clarification": "formal, thoughts or feelings"
                            },
                            {
                                "target_lang_word": "shake off",
                                "target_lang_sense_clarification": "sleep or drowsiness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a89b0ae1-e84c-3ac4-99ec-fdf18f9669a1",
                    "sense_definition": "Уволить с работы, лишить должности (часто с оттенком грубости или позора).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fire_job",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор пригрозил <прогнать> ленивого сотрудника, если тот снова опоздает.",
                            "target_lang_translations": {
                                "en": "The director threatened to <w>fire</w> the lazy employee if he was late again."
                            }
                        },
                        {
                            "text": "Его <прогнали> с работы за постоянные прогулы.",
                            "target_lang_translations": {
                                "en": "He was <w>kicked out</w> of his job for constant absenteeism."
                            }
                        }
                    ],
                    "synonyms": [

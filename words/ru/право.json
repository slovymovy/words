            "senses": [
                {
                    "sense_id": "595528a9-ba3e-3adf-bbf6-d8d3722074c8",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>правый</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наше дело <w>право</w>, мы обязательно победим.",
                            "target_lang_translations": {
                                "en": "Our cause is <w>right</w>, we will definitely win."
                            }
                        },
                        {
                            "text": "Твоё решение абсолютно <w>право</w> и справедливо.",
                            "target_lang_translations": {
                                "en": "Your decision is absolutely <w>right</w> and fair."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "краткое прилагательное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Being in accordance with fact, reason, or truth; correct or just."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "right",
                                "target_lang_sense_clarification": "correct/just"
                            }
                        ]
                    }
                }
            ]
        }
    ]

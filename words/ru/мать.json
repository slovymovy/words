            "senses": [
                {
                    "sense_id": "4ef33a07-8839-3f02-997b-d2aa0debadcf",
                    "sense_definition": "Женщина по отношению к своим детям.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "biological_parent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя <мать> работает учителем в школе.",
                            "target_lang_translations": {
                                "en": "My <mother> works as a teacher at school."
                            }
                        },
                        {
                            "text": "Ребёнок крепко держал <мать> за руку.",
                            "target_lang_translations": {
                                "en": "The child held his <mother> tightly by the hand."
                            }
                        },
                        {
                            "text": "Она стала <матерью> в двадцать пять лет.",
                            "target_lang_translations": {
                                "en": "She became a <mother> at twenty-five."
                            }
                        }
                    ],
                    "synonyms": [
                        "мама",
                        "матушка",
                        "родительница",
                        "маменька",
                        "мамаша"
                    ],
                    "antonyms": [
                        "отец"
                    ],
                    "common_phrases": [
                        "родная мать",
                        "одинокая мать",
                        "многодетная мать",
                        "стать матерью",
                        "любовь матери"
                    ],
                    "target_lang_definitions": {
                        "en": "A woman in relation to her children."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mother",
                                "target_lang_sense_clarification": "female parent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c26cd5c4-edc2-3e56-b3a7-70745ddf5884",
                    "sense_definition": "Самка животного по отношению к своим детёнышам.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_parent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Медвежата везде следовали за своей <матерью>.",
                            "target_lang_translations": {
                                "en": "The bear cubs followed their <mother> everywhere."
                            }
                        },
                        {
                            "text": "Котята спали в корзинке рядом с <матерью>.",
                            "target_lang_translations": {
                                "en": "The kittens slept in the basket next to their <mother>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "инстинкт матери",
                        "разлучить с матерью"
                    ],
                    "target_lang_definitions": {
                        "en": "A female animal in relation to its young."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mother",
                                "target_lang_sense_clarification": "female animal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d41a470-a04d-373b-be24-7056e52e8239",
                    "sense_definition": "То, что является источником чего-либо, или то, что дорого и близко (обычно о Родине или земле).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдаты сражались за Родину-<мать>.",
                            "target_lang_translations": {
                                "en": "The soldiers fought for the <Mother>land."
                            }
                        },
                        {
                            "text": "Фермеры надеются, что <мать>-земля даст хороший урожай.",
                            "target_lang_translations": {
                                "en": "Farmers hope that <Mother> Earth will yield a good harvest."
                            }
                        }
                    ],
                    "synonyms": [
                        "родина",
                        "земля"
                    ],
                    "common_phrases": [
                        "Родина-мать",
                        "мать-земля",
                        "мать сыра земля"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "poetic or rhetorical style"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Something that is the source of something, or something dear and close (usually about the Motherland or the earth)."

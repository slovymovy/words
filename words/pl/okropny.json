                {
                    "sense_id": "fd631a60-e619-326d-8a31-9df0e9cbd519",
                    "sense_definition": "O czymś bardzo złym, brzydkim, nieprzyjemnym lub posiadającym negatywne cechy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quality_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta zupa jest <w>okropna</w>, zupełnie mi nie smakuje.",
                            "target_lang_translations": {
                                "ru": "Этот суп <w>ужасный</w>, мне совершенно не нравится."
                            }
                        },
                        {
                            "text": "Mieliśmy dzisiaj <w>okropną</w> pogodę, cały dzień padał deszcz.",
                            "target_lang_translations": {
                                "ru": "У нас сегодня была <w>ужасная</w> погода, весь день шел дождь."
                            }
                        }

{
    "word_family": [
        "un-PC",
        "non-PC"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c2b7baa8-ef82-3030-9d1b-ea689d92fd24",
                    "sense_definition": "Initialism of <w>personal computer</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "computer_tech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to buy a new <w>PC</w> for my home office.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый <w>ПК</w> для домашнего офиса."
                            }
                        },
                        {
                            "text": "Do you prefer playing games on a console or a <w>PC</w>?",
                            "target_lang_translations": {
                                "ru": "Ты предпочитаешь играть на консоли или на <w>ПК</w>?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "desktop PC",
                        "gaming PC",
                        "PC user"
                    ],
                    "target_lang_definitions": {
                        "ru": "Персональный компьютер (ПК)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ПК",
                                "target_lang_sense_clarification": "аббревиатура"
                            },
                            {
                                "target_lang_word": "компьютер",
                                "target_lang_sense_clarification": "общее название"
                            },
                            {
                                "target_lang_word": "персональный компьютер",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf1fd0ee-b076-3bf9-9850-d3005352b6e9",
                    "sense_definition": "Abbreviation of <w>piece</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The price is listed as fifty cents per <w>pc</w>.",
                            "target_lang_translations": {
                                "ru": "Цена указана как пятьдесят центов за <w>шт.</w>"
                            }
                        },
                        {
                            "text": "Please order at least one <w>pc</w> of each item.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, закажите хотя бы одну <w>штуку</w> каждого товара."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от слова \"штука\" (единица измерения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шт.",
                                "target_lang_sense_clarification": "сокращение на письме"
                            },
                            {
                                "target_lang_word": "штука",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8046233-f0ff-3c5f-ad18-76ca76c15854",
                    "sense_definition": "Initialism of <w>police constable</w>, a police rank used in Commonwealth countries.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "law_enforcement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The witness gave a full statement to <w>PC</w> Smith.",
                            "target_lang_translations": {
                                "ru": "Свидетель дал полные показания <w>констеблю</w> Смиту."
                            }
                        },
                        {
                            "text": "A local <w>PC</w> was patrolling the neighborhood on foot.",
                            "target_lang_translations": {
                                "ru": "Местный <w>констебль</w> патрулировал район пешком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "PC Smith",
                        "local PC"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Commonwealth"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полицейский констебль (звание в полиции Великобритании и стран Содружества)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "констебль",
                                "target_lang_sense_clarification": "звание"
                            },
                            {
                                "target_lang_word": "полицейский",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce7720a0-9b22-3cc8-a3f1-991b72c0c45a",
                    "sense_definition": "Initialism of <w>political correctness</w>.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He complained that there is too much <w>PC</w> in modern media.",
                            "target_lang_translations": {
                                "ru": "Он жаловался, что в современных СМИ слишком много <w>политкорректности</w>."
                            }
                        },
                        {
                            "text": "The debate focused on the influence of <w>PC</w> on free speech.",
                            "target_lang_translations": {
                                "ru": "Дебаты были сосредоточены на влиянии <w>политкорректности</w> на свободу слова."
                            }
                        }
                    ],
                    "common_phrases": [
                        "PC culture",
                        "gone PC"
                    ],
                    "target_lang_definitions": {
                        "ru": "Политическая корректность (политкорректность)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "политкорректность",
                                "target_lang_sense_clarification": "существительное"
                            },
                            {
                                "target_lang_word": "политическая корректность",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a356723-0320-308a-ae03-8dfefb9ed7f6",
                    "sense_definition": "Initialism of <w>player character</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gaming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can customize the appearance of your <w>PC</w> at the start of the game.",
                            "target_lang_translations": {
                                "ru": "Вы можете настроить внешность своего <w>персонажа</w> в начале игры."
                            }
                        },
                        {
                            "text": "The <w>PC</w> must interact with villagers to get the quest.",
                            "target_lang_translations": {
                                "ru": "<w>Игровой персонаж</w> должен взаимодействовать с жителями деревни, чтобы получить квест."
                            }
                        }
                    ],
                    "antonyms": [
                        "NPC"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "gaming"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Игровой персонаж, управляемый игроком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "персонаж",
                                "target_lang_sense_clarification": "в играх"
                            },
                            {
                                "target_lang_word": "игровой персонаж",
                                "target_lang_sense_clarification": "термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae77d71b-320f-34a7-a2e2-0e9109ef75ac",
                    "sense_definition": "Initialism of <w>protective custody</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "prison_law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prisoner was placed in <w>PC</w> for his own safety.",
                            "target_lang_translations": {
                                "ru": "Заключенного поместили под <w>защитный надзор</w> ради его собственной безопасности."
                            }
                        },
                        {
                            "text": "Inmates in <w>PC</w> are separated from the general population.",
                            "target_lang_translations": {
                                "ru": "Заключенные под <w>защитным надзором</w> отделены от остальных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in PC"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "prison slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитный надзор (изоляция заключенного ради его безопасности)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защитный надзор",
                                "target_lang_sense_clarification": "юридический термин"
                            },
                            {
                                "target_lang_word": "изоляция",
                                "target_lang_sense_clarification": "условия содержания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a19d6e9b-6a4a-3297-b9fd-cf5cdd02f432",
                    "sense_definition": "Initialism of <w>politically correct</w>.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "social_politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His jokes are funny, but they are not very <w>PC</w>.",
                            "target_lang_translations": {
                                "ru": "Его шутки смешные, но не очень <w>политкорректные</w>."
                            }
                        },
                        {
                            "text": "Companies try to be <w>PC</w> to avoid offending their customers.",
                            "target_lang_translations": {
                                "ru": "Компании стараются быть <w>политкорректными</w>, чтобы не обидеть своих клиентов."
                            }
                        }
                    ],
                    "antonyms": [
                        "non-PC",
                        "un-PC"
                    ],
                    "common_phrases": [
                        "not very PC",
                        "strictly PC"
                    ],
                    "target_lang_definitions": {
                        "ru": "Политкорректный (соответствующий нормам политической корректности)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "политкорректный",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8cac44de-64a5-3403-81e8-e35909784948",
                    "sense_definition": "To make politically correct.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "social_politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The studio <w>PCed</w> the script to appeal to a modern audience.",
                            "target_lang_translations": {
                                "ru": "Студия <w>сделала политкорректным</w> сценарий, чтобы понравиться современной аудитории."
                            }
                        },
                        {
                            "text": "He felt that they were <w>PCing</w> the holiday traditions too much.",
                            "target_lang_translations": {
                                "ru": "Ему казалось, что они делают слишком <w>политкорректными</w> праздничные традиции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо политкорректным; приводить в соответствие с нормами политкорректности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать политкорректным",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "цензурировать",
                                "target_lang_sense_clarification": "в контексте политкорректности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
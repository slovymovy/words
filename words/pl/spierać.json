        "prać",
        "pranie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "df1b7a69-421e-3a67-b043-43df410813f7",
                    "sense_definition": "Prowadzić z kimś spór, wymieniać odmienne poglądy lub kłócić się (zwykle używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice często <spierają> się o politykę podczas rodzinnych spotkań.",
                            "target_lang_translations": {
                                "ru": "Мои родители часто <спорят> о политике во время семейных встреч."
                            }
                        },
                        {
                            "text": "Nie warto <spierać> się o drobiazgi, które nie mają znaczenia.",
                            "target_lang_translations": {
                                "ru": "Не стоит <спорить> по пустякам, которые не имеют значения."
                            }
                        }
                    ],
                    "synonyms": [
                        "kłócić się",
                        "sprzeczać się",
                        "dyskutować"
                    ],
                    "common_phrases": [
                        "spierać się o rację",
                        "spierać się o drobiazgi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести спор, обмениваться противоположными мнениями или ссориться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спорить",
                                "target_lang_sense_clarification": "выражать несогласие"
                            },
                            {
                                "target_lang_word": "препираться",
                                "target_lang_sense_clarification": "спорить по мелочам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a29645ca-8f82-3f60-ad63-b6e601d2f23b",
                    "sense_definition": "Usuwać brud, plamy lub kolor z tkaniny poprzez pranie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "washing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę ręcznie <spierać> te trudne plamy z białego obrusu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно вручную <отстирывать> эти сложные пятна с белой скатерти."
                            }
                        },
                        {
                            "text": "Ona cierpliwie <spierała> błoto ze spodni swojego syna.",
                            "target_lang_translations": {
                                "ru": "Она терпеливо <отстирывала> грязь с брюк своего сына."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpierać"
                    ],
                    "common_phrases": [
                        "spierać plamy",
                        "spierać brud"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять грязь, пятна или краску с ткани посредством стирки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отстирывать",
                                "target_lang_sense_clarification": "удалять пятна стиркой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d03e200-31fe-3cc4-aec9-de4b35a14f68",
                    "sense_definition": "O plamie, brudzie lub kolorze: dawać się usunąć lub znikać pod wpływem prania (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "washing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety, ta tłusta plama nie <spiera> się w niskiej temperaturze.",
                            "target_lang_translations": {
                                "ru": "К сожалению, это жирное пятно не <отстирывается> при низкой температуре."
                            }
                        },
                        {
                            "text": "Tani nadruk na koszulce szybko <spiera> się po kilku praniach.",
                            "target_lang_translations": {
                                "ru": "Дешёвый принт на футболке быстро <смывается> после нескольких стирок."
                            }
                        }
                    ],
                    "synonyms": [
                        "schodzić"
                    ],
                    "common_phrases": [
                        "plama się spiera",
                        "kolor się spiera"
                    ],
                    "target_lang_definitions": {
                        "ru": "О пятне, грязи или цвете: удаляться или исчезать под воздействием стирки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отстирываться",
                                "target_lang_sense_clarification": "о пятнах"
                            },
                            {
                                "target_lang_word": "смываться",
                                "target_lang_sense_clarification": "о краске или принте"
                            },
                            {
                                "target_lang_word": "линять",
                                "target_lang_sense_clarification": "терять цвет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9624c6fb-a009-3891-b680-851202bc595a",
                    "sense_definition": "Bić kogoś mocno i dotkliwie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Widziałem, jak chuligani <spierali> bezbronnego chłopaka w ciemnej uliczce.",
                            "target_lang_translations": {
                                "ru": "Я видел, как хулиганы <избивали> беззащитного парня в тёмном переулке."
                            }
                        },
                        {
                            "text": "Napastnicy <spierali> go tak długo, aż stracił przytomność.",
                            "target_lang_translations": {
                                "ru": "Нападавшие <избивали> его так долго, пока он не потерял сознание."
                            }
                        }
                    ],
                    "synonyms": [
                        "bić",
                        "tłuc",
                        "walić"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно и жестоко бить кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избивать",
                                "target_lang_sense_clarification": "наносить побои"
                            },
                            {
                                "target_lang_word": "колотить",
                                "target_lang_sense_clarification": "разг. бить"
                            }
                        ]
                    }
                }
            ]
        }
    ]

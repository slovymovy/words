                    ],
                    "common_phrases": [
                        "liść laurowy",
                        "zielony liść",
                        "opadłe liście",
                        "liść dębu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент строения растения, обычно зеленый и плоский, выполняющий питательные функции благодаря содержанию хлорофилла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лист",

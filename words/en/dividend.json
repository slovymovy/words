            "senses": [
                {
                    "sense_id": "6fc4eab1-59d0-33ea-95e9-29f71a083ece",
                    "sense_definition": "A cash payment of money by a company to its shareholders, usually made periodically.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company pays a quarterly <w>dividend</w> to its investors.",
                            "target_lang_translations": {
                                "ru": "Компания выплачивает ежеквартальный <w>дивиденд</w> своим инвесторам."
                            }
                        },
                        {
                            "text": "Shareholders were pleased when the annual <w>dividend</w> increased.",
                            "target_lang_translations": {
                                "ru": "Акционеры были довольны, когда годовой <w>дивиденд</w> вырос."
                            }
                        }
                    ],
                    "synonyms": [
                        "payout"
                    ],
                    "common_phrases": [
                        "annual dividend",
                        "quarterly dividend",
                        "pay dividends"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная выплата акционерам из прибыли компании, обычно производимая периодически."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дивиденд",
                                "target_lang_sense_clarification": "финансы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32c60269-b42e-3f6d-93c0-0b8264101e43",
                    "sense_definition": "Beneficial results or rewards gained from a past effort or action.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her hard work in school finally paid a <w>dividend</w> when she got the scholarship.",
                            "target_lang_translations": {
                                "ru": "Её усердная учеба в школе наконец принесла <w>дивиденды</w>, когда она получила стипендию."
                            }
                        },
                        {
                            "text": "Patience often pays a <w>dividend</w> in the long run.",
                            "target_lang_translations": {
                                "ru": "Терпение часто приносит <w>дивиденды</w> в долгосрочной перспективе."
                            }
                        }
                    ],
                    "synonyms": [

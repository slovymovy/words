            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cd822cbd-40a3-3147-be04-5f4a9d9f2b5d",
                    "sense_definition": "Liczne przybycie ludzi lub nagromadzenie się rzeczy, pieniędzy lub informacji w jednym miejscu.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "influx_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obserwujemy stały <w>napływ</w> nowych pracowników do naszej firmy.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдаем постоянный <w>приток</w> новых сотрудников в нашу компанию."
                            }
                        },
                        {
                            "text": "Zagraniczny <w>napływ</w> kapitału pozwolił na szybki rozwój gospodarki.",
                            "target_lang_translations": {
                                "ru": "Иностранный <w>приток</w> капитала позволил экономике быстро развиваться."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopływ",
                        "przypływ"
                    ],
                    "antonyms": [
                        "odpływ"
                    ],
                    "common_phrases": [
                        "napływ ludności",
                        "napływ imigrantów",
                        "napływ kapitału",
                        "napływ inwestycji",
                        "napływ towarów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Массовое прибытие людей или скопление вещей, денег или информации в одном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приток",
                                "target_lang_sense_clarification": "капитала, инвестиций, рабочей силы"
                            },
                            {
                                "target_lang_word": "наплыв",
                                "target_lang_sense_clarification": "людей, туристов, посетителей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "834f1098-2967-3b35-9173-711f83078821",
                    "sense_definition": "Pojawienie się dużej ilości płynu lub powietrza w określonym miejscu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "influx_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tama została uszkodzona przez gwałtowny <w>napływ</w> wody z rzeki.",
                            "target_lang_translations": {
                                "ru": "Плотина была повреждена резким <w>притоком</w> воды из реки."
                            }
                        },
                        {
                            "text": "Otwórz okno, aby zapewnić <w>napływ</w> świeżego powietrza do pokoju.",
                            "target_lang_translations": {
                                "ru": "Открой окно, чтобы обеспечить <w>приток</w> свежего воздуха в комнату."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopływ"
                    ],
                    "antonyms": [
                        "odpływ"
                    ],
                    "common_phrases": [
                        "napływ wody",
                        "napływ powietrza",
                        "napływ krwi",
                        "napływ chłodnego powietrza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появление большого количества жидкости или воздуха в определенном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приток",
                                "target_lang_sense_clarification": "воздуха, воды"
                            },
                            {
                                "target_lang_word": "поступление",
                                "target_lang_sense_clarification": "жидкости, газа (техническое)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "244fca14-f8c8-3d29-8640-1c40790d5f34",
                    "sense_definition": "Nagłe pojawienie się silnych uczuć, emocji lub sił witalnych.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "influx_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poczuł nagły <w>napływ</w> gniewu, gdy usłyszał te kłamstwa.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал внезапный <w>прилив</w> гнева, когда услышал эту ложь."
                            }
                        },
                        {
                            "text": "Wiosenne słońce sprawiło, że poczuła <w>napływ</w> nowej energii.",
                            "target_lang_translations": {
                                "ru": "Весеннее солнце заставило её почувствовать <w>прилив</w> новой энергии."
                            }
                        }
                    ],
                    "synonyms": [
                        "przypływ"
                    ],
                    "antonyms": [
                        "odpływ"
                    ],
                    "common_phrases": [
                        "napływ energii",
                        "napływ sił",
                        "napływ radości",
                        "napływ odwagi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное появление сильных чувств, эмоций или жизненных сил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прилив",
                                "target_lang_sense_clarification": "сил, энергии, чувств"
                            },
                            {
                                "target_lang_word": "наплыв",
                                "target_lang_sense_clarification": "чувств, воспоминаний"
                            }
                        ]
                    }
                }
            ]
        }

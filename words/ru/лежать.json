    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d981f558-5ad2-3297-9a99-f0a7cbf81276",
                    "sense_definition": "Находиться в горизонтальном положении (о людях и животных).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В выходные я люблю просто <w>лежать</w> на диване и читать.",
                            "target_lang_translations": {
                                "en": "On weekends I like to just <w>lie</w> on the sofa and read."
                            }
                        },
                        {
                            "text": "Огромная собака <w>лежала</w> у входа в дом и спала.",
                            "target_lang_translations": {
                                "en": "A huge dog <w>lay</w> at the entrance to the house, sleeping."
                            }
                        }
                    ],
                    "antonyms": [
                        "стоять",
                        "двигаться"
                    ],
                    "common_phrases": [
                        "лежать на диване",
                        "лежать на спине",
                        "лежать на боку",
                        "лежать ничком"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in a horizontal position (about people and animals)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "general state"
                            },
                            {
                                "target_lang_word": "lie down",
                                "target_lang_sense_clarification": "action or state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f46ef95-3fe1-38ac-b885-9d4e9752ff7d",
                    "sense_definition": "Находиться на какой-либо поверхности плашмя (о неодушевлённых предметах).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Твоя новая книга <w>лежит</w> на кухонном столе.",
                            "target_lang_translations": {
                                "en": "Your new book <w>is lying</w> on the kitchen table."
                            }
                        },
                        {
                            "text": "На полу <w>лежал</w> красивый пушистый ковёр.",
                            "target_lang_translations": {
                                "en": "A beautiful fluffy carpet <w>lay</w> on the floor."
                            }
                        }
                    ],
                    "antonyms": [
                        "стоять"
                    ],
                    "common_phrases": [
                        "лежать на столе",
                        "лежать на полке",
                        "лежать на полу",
                        "лежать в кармане"
                    ],
                    "target_lang_definitions": {
                        "en": "To be situated flat on a surface (about inanimate objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "position on a surface"
                            },
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a965363-798e-30c1-a884-c97746265418",
                    "sense_definition": "Находиться в постели из-за болезни или пребывать в больнице на лечении.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг сейчас <w>лежит</w> в больнице с воспалением лёгких.",
                            "target_lang_translations": {
                                "en": "My friend <w>is</w> currently in the hospital with pneumonia."
                            }
                        },
                        {
                            "text": "Врач сказал, что мне нужно <w>лежать</w> минимум три дня.",
                            "target_lang_translations": {
                                "en": "The doctor said I need to <w>stay in bed</w> for at least three days."
                            }
                        }
                    ],
                    "common_phrases": [
                        "лежать в больнице",
                        "лежать с гриппом",
                        "лежать с температурой",
                        "лежать пластом"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in bed due to illness or stay in a hospital for treatment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stay in bed",
                                "target_lang_sense_clarification": "bed rest"
                            },
                            {
                                "target_lang_word": "be in hospital",
                                "target_lang_sense_clarification": "hospitalization"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9451b13-bfde-3ce0-9a85-1bc8b1b937c0",
                    "sense_definition": "Располагаться в определённом месте или простираться в каком-либо направлении (о местности, городе, пути).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед путешественниками <w>лежала</w> широкая зелёная долина.",
                            "target_lang_translations": {
                                "en": "A wide green valley <w>lay</w> before the travelers."
                            }
                        },
                        {
                            "text": "Наш путь <w>лежал</w> через густой тёмный лес.",
                            "target_lang_translations": {
                                "en": "Our path <w>lay</w> through a thick dark forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "находиться"
                    ],
                    "common_phrases": [
                        "путь лежит через",
                        "город лежит в",
                        "лежать к югу",
                        "лежать в руинах"
                    ],
                    "target_lang_definitions": {
                        "en": "To be located in a certain place or extend in a direction (about terrain, cities, paths)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "geographical location"
                            },
                            {
                                "target_lang_word": "be situated",
                                "target_lang_sense_clarification": "location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b010b6d2-97e9-3dff-b503-a7bccb4800e5",
                    "sense_definition": "Храниться где-либо или находиться без использования.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "storage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все важные документы <w>лежат</w> в железном сейфе.",
                            "target_lang_translations": {
                                "en": "All important documents <w>are kept</w> in an iron safe."
                            }
                        },
                        {
                            "text": "Этот инструмент <w>лежит</w> без дела уже несколько лет.",
                            "target_lang_translations": {
                                "en": "This tool has been <w>lying</w> idle for several years."
                            }
                        }
                    ],
                    "synonyms": [
                        "находиться"
                    ],
                    "common_phrases": [
                        "лежать без дела",
                        "деньги лежат",
                        "товар лежит",
                        "лежать в ящике"
                    ],
                    "target_lang_definitions": {
                        "en": "To be kept somewhere or exist without use."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "remain unused"
                            },
                            {
                                "target_lang_word": "sit",
                                "target_lang_sense_clarification": "colloquial, unused items"
                            },
                            {
                                "target_lang_word": "be kept",
                                "target_lang_sense_clarification": "storage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc922f23-f596-3085-96ef-f9990d32645c",
                    "sense_definition": "Покрывать собой какую-либо поверхность ровным слоем (о снеге, пыли, тенях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На крышах домов и деревьях уже <w>лежал</w> первый снег.",
                            "target_lang_translations": {
                                "en": "The first snow already <w>lay</w> on the roofs of houses and trees."
                            }
                        },
                        {
                            "text": "Толстый слой серой пыли <w>лежал</w> на старой мебели.",
                            "target_lang_translations": {
                                "en": "A thick layer of grey dust <w>lay</w> on the old furniture."
                            }
                        }
                    ],
                    "common_phrases": [
                        "снег лежит",
                        "пыль лежит",
                        "тень лежит",
                        "роса лежит"
                    ],
                    "target_lang_definitions": {
                        "en": "To cover a surface in an even layer (about snow, dust, shadows)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "rest on surface"
                            },
                            {
                                "target_lang_word": "cover",
                                "target_lang_sense_clarification": "spread over"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fc0f9ad-9a89-354a-a0fd-803af822ac0f",
                    "sense_definition": "Быть чьей-либо обязанностью или ответственностью.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся ответственность за этот сложный проект <w>лежит</w> на мне.",
                            "target_lang_translations": {
                                "en": "All responsibility for this complex project <w>lies</w> with me."
                            }
                        },
                        {
                            "text": "Тяжёлая вина за случившееся <w>лежала</w> на его совести.",
                            "target_lang_translations": {
                                "en": "Heavy guilt for what happened <w>lay</w> on his conscience."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ответственность лежит на",
                        "вина лежит на",
                        "обязанность лежит на",
                        "забота лежит на"
                    ],
                    "target_lang_definitions": {
                        "en": "To be someone's duty or responsibility."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "rest with/on"
                            },
                            {
                                "target_lang_word": "rest",
                                "target_lang_sense_clarification": "be the responsibility of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6da50f65-c527-372e-93df-9e32e3832ed8",
                    "sense_definition": "Быть уложенным, располагаться тем или иным образом (о волосах, складках одежды).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её тёмные волосы красиво <w>лежали</w> на плечах.",
                            "target_lang_translations": {
                                "en": "Her dark hair <w>lay</w> beautifully on her shoulders."
                            }
                        },
                        {
                            "text": "Воротник пальто <w>лежал</w> неаккуратно, и она поправила его.",
                            "target_lang_translations": {
                                "en": "The coat collar <w>sat</w> unevenly, and she fixed it."
                            }
                        }
                    ],
                    "common_phrases": [
                        "волосы лежат",
                        "складки лежат",
                        "плохо лежать"
                    ],
                    "target_lang_definitions": {
                        "en": "To be arranged or situated in a certain way (about hair, folds of clothing)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "arrangement"
                            },
                            {
                                "target_lang_word": "sit",
                                "target_lang_sense_clarification": "fit/arrangement"
                            },
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "hair/fabric"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bad47d8b-5ae2-33f6-bd19-479f5fdb83c2",
                    "sense_definition": "Содержаться, заключаться в чём-либо (о характере, причинах).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abstract_existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Причина его странного поведения <w>лежала</w> в глубоком детстве.",
                            "target_lang_translations": {
                                "en": "The reason for his strange behavior <w>lay</w> in his early childhood."
                            }
                        },
                        {
                            "text": "Склонность к искусству всегда <w>лежала</w> в его натуре.",
                            "target_lang_translations": {
                                "en": "An inclination towards art always <w>lay</w> in his nature."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly used in literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be contained or consist in something (about character, reasons)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lie",
                                "target_lang_sense_clarification": "exist in"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
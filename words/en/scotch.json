                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скотч",
                                "target_lang_sense_clarification": "клейкая лента"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07d9635d-612d-3894-8d2d-b2b8ae7f3fdb",
                    "sense_definition": "A block, wedge, or prop used to prevent a wheel or other round object from moving or slipping.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_block",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The driver placed a <w>scotch</w> behind the rear wheel to keep the truck from rolling down the hill.",
                            "target_lang_translations": {
                                "ru": "Водитель подложил <w>тормозной башмак</w> под заднее колесо, чтобы грузовик не скатился с холма."
                            }
                        },
                        {
                            "text": "Ensure the <w>scotch</w> is firmly in place before you disconnect the trailer.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что <w>тормозной башмак</w> надежно установлен, прежде чем отцеплять прицеп."
                            }
                        }
                    ],
                    "synonyms": [
                        "chock",
                        "block"
                    ],
                    "target_lang_definitions": {
                        "ru": "Клиновидная колодка или брусок, подкладываемый под колесо для предотвращения скатывания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тормозной башмак",
                                "target_lang_sense_clarification": "устройство для фиксации колес"
                            },
                            {
                                "target_lang_word": "клин",
                                "target_lang_sense_clarification": "предмет клиновидной формы"
                            }
                        ]
                    }
                }

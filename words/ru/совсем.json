                    "sense_id": "bc3799ed-fb1d-3344-82e4-bf9db73797ac",
                    "sense_definition": "В полной мере, полностью, совершенно.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>совсем</w> забыл о нашей встрече.",
                            "target_lang_translations": {
                                "en": "I <w>completely</w> forgot about our meeting."
                            }
                        },
                        {
                            "text": "Этот фильм <w>совсем</w> неинтересный.",
                            "target_lang_translations": {
                                "en": "This movie is <w>quite</w> uninteresting."
                            }
                        },
                        {
                            "text": "Он был <w>совсем</w> один в большой комнате.",
                            "target_lang_translations": {
                                "en": "He was <w>quite</w> alone in the big room."
                            }
                        }
                    ],
                    "synonyms": [
                        "полностью",
                        "совершенно",
                        "абсолютно",
                        "вполне"
                    ],
                    "antonyms": [
                        "почти"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a485ba65-dcee-31b6-9e43-7434c8a6cc27",
                    "sense_definition": "An essential or characteristic part of something abstract.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "part_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Honesty is a vital <w>element</w> of any healthy friendship.",
                            "target_lang_translations": {
                                "ru": "Честность — жизненно важный <w>элемент</w> любой здоровой дружбы."
                            }
                        },
                        {
                            "text": "The movie combines the <w>elements</w> of comedy and tragedy perfectly.",
                            "target_lang_translations": {
                                "ru": "Фильм идеально сочетает в себе <w>элементы</w> комедии и трагедии."
                            }
                        }
                    ],
                    "synonyms": [
                        "member",
                        "chemical element",
                        "elementary substance"
                    ],
                    "common_phrases": [
                        "key element",
                        "essential element",
                        "basic element"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существенная или характерная часть чего-либо абстрактного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "элемент",
                                "target_lang_sense_clarification": "составная часть"
                            },
                            {
                                "target_lang_word": "составная часть",
                                "target_lang_sense_clarification": "компонент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60dcf5df-c0c4-31b1-8fb5-1811b439c5c6",
                    "sense_definition": "A simple chemical substance that consists of atoms of only one type and cannot be broken down into simpler substances.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "matter_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oxygen is the most common <w>element</w> in the Earth's crust.",
                            "target_lang_translations": {
                                "ru": "Кислород — самый распространенный <w>элемент</w> в земной коре."
                            }
                        },
                        {
                            "text": "Gold is a chemical <w>element</w> with the symbol Au.",
                            "target_lang_translations": {
                                "ru": "Золото — это химический <w>элемент</w> с символом Au."
                            }

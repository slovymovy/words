                {
                    "sense_id": "fdf47a51-cbcc-3b3f-a158-1c228c71ce1d",
                    "sense_definition": "Het land dat grenst aan de zee.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "coast_geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We maakten een lange wandeling langs de <w>kust</w>.",
                            "target_lang_translations": {
                                "en": "We took a long walk along the <w>coast</w>.",
                                "ru": "Мы долго гуляли вдоль <w>побережья</w>."
                            }
                        },
                        {
                            "text": "Vijftien kilometer uit de <w>kust</w> liggen enkele kleine eilanden.",
                            "target_lang_translations": {
                                "en": "Fifteen kilometers off the <w>coast</w> lie a few small islands.",
                                "ru": "В пятнадцати километрах от <w>берега</w> лежат несколько маленьких островов."
                            }
                        }
                    ],
                    "antonyms": [
                        "binnenland"
                    ],
                    "common_phrases": [
                        "aan de kust",
                        "langs de kust",
                        "voor de kust",
                        "naar de kust"
                    ],
                    "target_lang_definitions": {
                        "en": "The land bordering the sea.",
                        "ru": "Земля, граничащая с морем."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coast",
                                "target_lang_sense_clarification": "general term for land along the sea"
                            },
                            {
                                "target_lang_word": "shore",
                                "target_lang_sense_clarification": "land adjoining a body of water"
                            },
                            {
                                "target_lang_word": "seaside",
                                "target_lang_sense_clarification": "area by the sea, often for recreation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "побережье",
                                "target_lang_sense_clarification": "полоса земли вдоль моря"
                            },
                            {
                                "target_lang_word": "берег",
                                "target_lang_sense_clarification": "край суши у воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f094b14f-2a29-3640-94ab-10823e41d5b1",
                    "sense_definition": "De omgeving of situatie, specifiek gebruikt om aan te geven dat er geen gevaar dreigt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "coast_idiom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wachtte een paar minuten tot de <w>kust</w> echt veilig was.",
                            "target_lang_translations": {
                                "en": "I waited a few minutes until the <w>coast</w> was truly clear.",
                                "ru": "Я подождал пару минут, пока <w>горизонт</w> не стал действительно чист."
                            }
                        },
                        {
                            "text": "De inbrekers controleerden of de <w>kust</w> veilig was voordat ze naar binnen gingen.",
                            "target_lang_translations": {
                                "en": "The burglars checked if the <w>coast</w> was clear before they went inside.",
                                "ru": "Грабители проверили, свободен ли <w>путь</w>, прежде чем войти внутрь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de kust is veilig"
                    ],
                    "target_lang_definitions": {
                        "en": "The surroundings or situation, specifically used to indicate that there is no danger.",
                        "ru": "Обстановка или ситуация, когда нет опасности (обычно в идиоме \"de kust is veilig\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coast",
                                "target_lang_sense_clarification": "used in the idiom 'the coast is clear'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "горизонт",
                                "target_lang_sense_clarification": "в выражении \"горизонт чист\""
                            },
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "в выражении \"путь свободен\""
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "30bfeae6-d702-3cbd-95af-14dd3f4790c2",
                    "sense_definition": "A diminutive of the male given name <w>William</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My younger brother <w>Billy</w> loves to play soccer in the park.",
                            "target_lang_translations": {
                                "ru": "Мой младший брат <w>Билли</w> обожает играть в футбол в парке."
                            }
                        },
                        {
                            "text": "Everyone calls him <w>Billy</w>, even though his birth certificate says William.",
                            "target_lang_translations": {
                                "ru": "Все зовут его <w>Билли</w>, хотя в свидетельстве о рождении написано Уильям."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма мужского имени Уильям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Билли",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3735a1f-a9b5-362e-9974-1d0ed90c4e9c",
                    "sense_definition": "A nickname for the B-25 Mitchell bomber aircraft used during World War II.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "The squadron of <w>Billys</w> flew over the coastline on their mission.",
                            "target_lang_translations": {
                                "ru": "Эскадрилья <w>Билли</w> пролетела над береговой линией, выполняя задание."
                            }
                        },
                        {
                            "text": "Pilots often spoke fondly of the reliability of the <w>Billy</w> during the war.",
                            "target_lang_translations": {
                                "ru": "Пилоты часто с теплотой отзывались о надежности <w>Билли</w> во время войны."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "WWII era"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "military nickname"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прозвище бомбардировщика B-25 «Митчелл», использовавшееся во время Второй мировой войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Билли",
                                "target_lang_sense_clarification": "прозвище самолета"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

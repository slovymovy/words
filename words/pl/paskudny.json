    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bad84e29-d9fb-3489-8e3f-39ccd3cf6c8c",
                    "sense_definition": "Bardzo brzydki, wywołujący wstręt swoim wyglądem, zapachem lub smakiem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_sensory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten syrop jest <w>paskudny</w> w smaku, ale musisz go wypić dla zdrowia.",
                            "target_lang_translations": {
                                "ru": "Этот сироп <w>гадкий</w> на вкус, но ты должен его выпить для здоровья."
                            }
                        },
                        {
                            "text": "W piwnicy unosił się <w>paskudny</w> zapach wilgoci i pleśni.",
                            "target_lang_translations": {
                                "ru": "В подвале стоял <w>отвратительный</w> запах сырости и плесени."
                            }
                        }
                    ],
                    "synonyms": [
                        "odrażający",
                        "wstrętny"
                    ],
                    "antonyms": [
                        "piękny"
                    ],
                    "common_phrases": [
                        "paskudny zapach",
                        "paskudny smak",
                        "paskudna rana",
                        "paskudny wygląd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень некрасивый, вызывающий отвращение своим видом, запахом или вкусом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гадкий",
                                "target_lang_sense_clarification": "вызывающий неприязнь (о вкусе, виде)"
                            },
                            {
                                "target_lang_word": "мерзкий",
                                "target_lang_sense_clarification": "вызывающий сильное отвращение"
                            },
                            {
                                "target_lang_word": "отвратительный",
                                "target_lang_sense_clarification": "крайне неприятный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a4af3d6-31cf-3b1d-a3fa-3d937ec7d7ab",
                    "sense_definition": "Bardzo zły, nieprzyjemny, uciążliwy lub trudny do zniesienia (np. o pogodzie, charakterze lub sytuacji).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od trzech dni mamy <w>paskudną</w> pogodę, więc nie wychodzimy na spacery.",
                            "target_lang_translations": {
                                "ru": "Уже три дня стоит <w>мерзкая</w> погода, поэтому мы не выходим на прогулки."
                            }
                        },
                        {
                            "text": "Mój nowy sąsiad ma <w>paskudny</w> charakter i ciągle na wszystkich krzyczy.",
                            "target_lang_translations": {
                                "ru": "У моего нового соседа <w>скверный</w> характер, и он постоянно на всех кричит."
                            }
                        }
                    ],
                    "synonyms": [
                        "wstrętny"
                    ],
                    "antonyms": [
                        "piękny"
                    ],
                    "common_phrases": [
                        "paskudna pogoda",
                        "paskudny charakter",
                        "paskudny humor",
                        "paskudna sprawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень плохой, неприятный, тягостный или невыносимый (например, о погоде, характере или ситуации)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скверный",
                                "target_lang_sense_clarification": "плохой, дурной (о характере, погоде)"
                            },
                            {
                                "target_lang_word": "паршивый",
                                "target_lang_sense_clarification": "разг. плохой, никуда не годный"
                            },
                            {
                                "target_lang_word": "мерзкий",
                                "target_lang_sense_clarification": "очень неприятный (о погоде, поступке)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

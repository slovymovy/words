            "senses": [
                {
                    "sense_id": "8d5099a2-5807-3e1e-a176-81f46c882936",
                    "sense_definition": "Перестать испытывать интерес, любовь или энтузиазм к кому-либо или чему-либо; стать равнодушным.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_cooling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Со временем он <w>охладел</w> к своей прежней работе и решил сменить профессию.",
                            "target_lang_translations": {
                                "en": "Over time, he <w>lost interest in</w> his former job and decided to change professions."
                            }
                        },
                        {
                            "text": "Она боялась, что муж <w>охладеет</w> к ней после долгих лет брака.",
                            "target_lang_translations": {
                                "en": "She was afraid that her husband would <w>grow cold towards</w> her after many years of marriage."
                            }
                        }
                    ],
                    "synonyms": [
                        "разлюбить",
                        "остыть"
                    ],
                    "antonyms": [
                        "заинтересоваться",
                        "воспылать"
                    ],
                    "common_phrases": [
                        "охладеть к работе",
                        "охладеть к учёбе",
                        "охладеть друг к другу",
                        "быстро охладеть"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop feeling interest, love, or enthusiasm for someone or something; to become indifferent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose interest in",
                                "target_lang_sense_clarification": "general loss of enthusiasm or curiosity"
                            },
                            {
                                "target_lang_word": "grow cold towards",
                                "target_lang_sense_clarification": "regarding feelings or relationships"
                            },
                            {
                                "target_lang_word": "cool towards",
                                "target_lang_sense_clarification": "regarding attitude or affection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97ee0264-62de-3aa4-a144-c1fbb2d7f144",
                    "sense_definition": "Стать холодным, понизиться в температуре (обычно о воздухе или теле).",

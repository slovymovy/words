                    "common_phrases": [
                        "czerwony kozak",
                        "zbierać kozaki",
                        "sos z kozaków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный лесной гриб с красноватой или коричневой шляпкой, растущий часто под березами или осинами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подберёзовик",
                                "target_lang_sense_clarification": "гриб с коричневой шляпкой"
                            },
                            {
                                "target_lang_word": "подосиновик",
                                "target_lang_sense_clarification": "гриб с красной шляпкой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "347b6beb-4721-3b58-91d3-7c2ab00c74eb",
                    "sense_definition": "Żywy, skoczny taniec ludowy pochodzenia ukraińskiego, wykonywany zazwyczaj przez mężczyzn.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tancerze wykonali porywającego <kozaka> na scenie teatru.",
                            "target_lang_translations": {
                                "ru": "Танцоры исполнили захватывающего <казачка> на сцене театра."
                            }
                        },
                        {
                            "text": "Nauczenie się tańczyć <kozaka> wymaga dużej sprawności fizycznej.",
                            "target_lang_translations": {
                                "ru": "Чтобы научиться танцевать <казачка>, требуется хорошая физическая подготовка."
                            }
                        }
                    ],
                    "synonyms": [
                        "kazaczok"
                    ],
                    "common_phrases": [
                        "tańczyć kozaka",
                        "skoczny kozak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живой, прыжковый народный танец украинского происхождения, исполняемый обычно мужчинами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "казачок",
                                "target_lang_sense_clarification": "танец"
                            }
                        ]
                    }
                }
            ]

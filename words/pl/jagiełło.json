                    "semantic_group_id": "surname_male",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Jagiełło</w> pracuje w naszym dziale księgowości od pięciu lat.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Ягелло</w> работает в нашем отделе бухгалтерии уже пять лет."
                            }

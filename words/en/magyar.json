    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "477652a2-10af-39b6-9015-0ea8e7405a83",
                    "sense_definition": "A member of the ethnic group native to Hungary.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ethnicity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Magyars</w> settled in the Carpathian Basin over a thousand years ago.",
                            "target_lang_translations": {
                                "ru": "<w>Мадьяры</w> поселились в Карпатском бассейне более тысячи лет назад."
                            }
                        },
                        {
                            "text": "He is proud to be a <w>Magyar</w> and keeps the traditions alive.",
                            "target_lang_translations": {
                                "ru": "Он гордится тем, что он <w>мадьяр</w>, и хранит традиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "Hungarian"
                    ],
                    "common_phrases": [
                        "ancient Magyars",
                        "Magyar tribes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель этнической группы, коренной житель Венгрии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мадьяр",
                                "target_lang_sense_clarification": "этническое самоназвание, часто в историческом контексте"
                            },
                            {
                                "target_lang_word": "венгр",
                                "target_lang_sense_clarification": "общее современное название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "abacf462-7334-39d2-b0fe-bc83bd24a5e1",
                    "sense_definition": "Relating to the Hungarian people, their culture, or language.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ethnicity_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We visited a museum dedicated to <w>Magyar</w> history and art.",
                            "target_lang_translations": {
                                "ru": "Мы посетили музей, посвященный <w>венгерской</w> истории и искусству."
                            }
                        },
                        {
                            "text": "The <w>Magyar</w> population extends beyond the borders of modern Hungary.",
                            "target_lang_translations": {
                                "ru": "<w>Венгерское</w> население проживает и за пределами современной Венгрии."
                            }
                        }
                    ],
                    "synonyms": [
                        "Hungarian"
                    ],
                    "common_phrases": [
                        "Magyar culture",
                        "Magyar heritage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к венгерскому народу, его культуре или языку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "венгерский",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "мадьярский",
                                "target_lang_sense_clarification": "относящийся к этносу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c020dc56-076e-381f-8b47-8fdb2f2e416f",
                    "sense_definition": "The Uralic language spoken by the Hungarian people.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "language",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Learning <w>Magyar</w> can be challenging for English speakers due to its complex grammar.",
                            "target_lang_translations": {
                                "ru": "Изучение <w>венгерского языка</w> может быть трудным для англоговорящих из-за его сложной грамматики."
                            }
                        },
                        {
                            "text": "She spoke fluent <w>Magyar</w> with her grandparents.",
                            "target_lang_translations": {
                                "ru": "Она свободно говорила на <w>венгерском</w> со своими бабушкой и дедушкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "Hungarian"
                    ],
                    "common_phrases": [
                        "speak Magyar",
                        "fluent Magyar"
                    ],
                    "target_lang_definitions": {

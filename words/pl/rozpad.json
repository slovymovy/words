    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dfbd6781-8321-31ba-a6e9-27e1af4edb93",
                    "sense_definition": "Koniec istnienia zorganizowanej całości, takiej jak państwo, grupa, system lub organizacja, zazwyczaj poprzez podział na mniejsze, niezależne części.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "structural_collapse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Historycy do dziś analizują przyczyny, które spowodowały szybki <w>rozpad</w> starożytnego imperium.",
                            "target_lang_translations": {
                                "ru": "Историки до сих пор анализируют причины, вызвавшие быстрый <w>распад</w> древней империи."
                            }
                        },
                        {
                            "text": "Po wielu wewnętrznych konfliktach nastąpił ostateczny <w>rozpad</w> koalicji rządzącej.",
                            "target_lang_translations": {
                                "ru": "После множества внутренних конфликтов произошел окончательный <w>распад</w> правящей коалиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "upadek",
                        "rozłam",
                        "podział",
                        "dezintegracja",
                        "rozbicie"
                    ],
                    "antonyms": [
                        "połączenie"
                    ],
                    "common_phrases": [
                        "rozpad imperium",
                        "rozpad koalicji",
                        "rozpad ZSRR",
                        "całkowity rozpad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекращение существования организованного целого (государства, группы или системы), обычно путем разделения на более мелкие независимые части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распад",
                                "target_lang_sense_clarification": "государства, империи"
                            },
                            {
                                "target_lang_word": "развал",
                                "target_lang_sense_clarification": "системы, организации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d355ca5-8232-382d-bbfc-057c1ff960ba",
                    "sense_definition": "Fizyczny, chemiczny lub biologiczny proces niszczenia struktury materii, w którym całość dzieli się na mniejsze fragmenty lub traci swoje pierwotne właściwości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W elektrowniach jądrowych wykorzystuje się kontrolowany <w>rozpad</w> atomów do produkcji energii.",
                            "target_lang_translations": {
                                "ru": "На атомных электростанциях используется контролируемый <w>распад</w> атомов для производства энергии."
                            }
                        },
                        {
                            "text": "Wilgoć i wysoka temperatura przyspieszyły biologiczny <w>rozpad</w> opadłych liści w lesie.",
                            "target_lang_translations": {
                                "ru": "Влага и высокая температура ускорили биологическое <w>разложение</w> опавших листьев в лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozkład",
                        "dezintegracja",
                        "gnicie",
                        "zwietrzenie"
                    ],
                    "antonyms": [
                        "synteza"
                    ],
                    "common_phrases": [
                        "rozpad promieniotwórczy",
                        "czas połowicznego rozpadu",
                        "rozpad materii",
                        "rozpad cząsteczek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физический, химический или биологический процесс разрушения структуры материи, при котором целое делится на фрагменты или теряет свои свойства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распад",
                                "target_lang_sense_clarification": "атомов, частиц"
                            },
                            {
                                "target_lang_word": "разложение",
                                "target_lang_sense_clarification": "биологическое, химическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a66e3e47-adf1-32c0-b59e-2ad0d59cdb7b",
                    "sense_definition": "Zanik więzi emocjonalnych, moralnych lub społecznych łączących ludzi, prowadzący do zakończenia relacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "structural_collapse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd orzekł trwały i zupełny <w>rozpad</w> pożycia małżeńskiego, udzielając rozwodu.",
                            "target_lang_translations": {
                                "ru": "Суд признал стойкий и полный <w>распад</w> супружеской жизни, оформив развод."
                            }
                        },
                        {
                            "text": "Brak szczerej rozmowy może z czasem spowodować powolny <w>rozpad</w> więzi rodzinnych.",
                            "target_lang_translations": {
                                "ru": "Отсутствие искреннего разговора может со временем привести к медленному <w>распаду</w> семейных уз."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozkład",
                        "poróżnienie"
                    ],
                    "common_phrases": [
                        "rozpad małżeństwa",
                        "rozpad rodziny",
                        "rozpad więzi",
                        "rozpad związku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исчезновение эмоциональных или социальных связей между людьми, ведущее к прекращению отношений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распад",
                                "target_lang_sense_clarification": "семьи, брака"
                            },
                            {
                                "target_lang_word": "разрыв",
                                "target_lang_sense_clarification": "отношений"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "99d7238a-9401-317c-8d83-c4bdb8e8cc40",
                    "sense_definition": "Краткое изречение, выражающее руководящую идею поведения или деятельности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "motto_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его жизненный <w>девиз</w> помогал ему преодолевать любые трудности.",
                            "target_lang_translations": {
                                "en": "His life <w>motto</w> helped him overcome any difficulties."
                            }
                        },
                        {
                            "text": "Компания работает под <w>девизом</w> постоянного совершенствования качества.",
                            "target_lang_translations": {
                                "en": "The company operates under the <w>motto</w> of continuous quality improvement."
                            }
                        }
                    ],
                    "synonyms": [
                        "лозунг"
                    ],
                    "common_phrases": [
                        "жизненный девиз",
                        "под девизом",
                        "наш девиз"
                    ],
                    "target_lang_definitions": {
                        "en": "A short sentence or phrase chosen as encapsulating the beliefs or ideals guiding an individual, family, or institution."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "motto",
                                "target_lang_sense_clarification": "guiding principle"
                            },
                            {
                                "target_lang_word": "slogan",
                                "target_lang_sense_clarification": "catchphrase"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acb909f1-d459-324d-a605-b369ae5729cb",
                    "sense_definition": "Краткая надпись или эмблема на гербе или щите.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "motto_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На старинном рыцарском щите был начертан родовой <w>девиз</w>.",
                            "target_lang_translations": {
                                "en": "The family <w>motto</w> was inscribed on the ancient knight's shield."
                            }
                        },
                        {
                            "text": "Герольд объяснил значение фигур и <w>девиза</w> на королевском гербе.",
                            "target_lang_translations": {
                                "en": "The herald explained the meaning of the figures and the <w>motto</w> on the royal coat of arms."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "149513bd-3a6b-3fa5-9bdf-e0924207cd43",
                    "sense_definition": "Główna celebracja liturgiczna sprawowana w Kościele katolickim i prawosławnym, obejmująca liturgię słowa i eucharystię.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdą niedzielę rano idziemy całą rodziną na <w>mszę</w> do kościoła.",
                            "target_lang_translations": {
                                "ru": "Каждое воскресное утро мы всей семьей идем на <w>мессу</w> в церковь."
                            }
                        },
                        {
                            "text": "Ksiądz odprawił uroczystą <w>mszę</w> w intencji naszego zmarłego dziadka.",
                            "target_lang_translations": {
                                "ru": "Священник отслужил торжественную <w>мессу</w> за нашего покойного дедушку."
                            }
                        }
                    ],
                    "synonyms": [
                        "msza święta"
                    ],
                    "common_phrases": [
                        "iść na mszę",
                        "odprawiać mszę",
                        "służyć do mszy",
                        "zamówić mszę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главное литургическое богослужение в Католической и Православной церкви, включающее литургию слова и евхаристию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "месса",
                                "target_lang_sense_clarification": "католическое богослужение"
                            },
                            {
                                "target_lang_word": "литургия",
                                "target_lang_sense_clarification": "православное богослужение"
                            },
                            {
                                "target_lang_word": "обедня",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "369dee1a-8cbf-34cb-80b9-f0adf63f6ff7",
                    "sense_definition": "Cykliczna forma muzyczna, zazwyczaj wokalno-instrumentalna, oparta na tekstach stałych części liturgii.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jan Sebastian Bach skomponował słynną „<w>Mszę</w> h-moll”, która jest arcydziełem muzyki barokowej.",
                            "target_lang_translations": {
                                "ru": "Иоганн Себастьян Бах сочинил знаменитую «<w>Мессу</w> си минор», которая является шедевром музыки барокко."
                            }
                        },
                        {
                            "text": "Podczas festiwalu muzyki sakralnej chór wykonał <w>mszę</w> Mozarta.",
                            "target_lang_translations": {
                                "ru": "Во время фестиваля духовной музыки хор исполнил <w>мессу</w> Моцарта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "msza żałobna",
                        "skomponować mszę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Циклическая музыкальная форма, обычно вокально-инструментальная, основанная на текстах неизменяемых частей литургии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "месса",
                                "target_lang_sense_clarification": "музыкальное произведение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

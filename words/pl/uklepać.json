    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bff93e0e-5bb5-369a-94d3-f2070b728258",
                    "sense_definition": "Uderzać dłonią lub narzędziem w coś miękkiego lub sypkiego (np. ziemię, piasek), aby to wyrównać, utwardzić lub nadać temu kształt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "flattening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ogrodnik musiał mocno <w>uklepać</w> ziemię wokół nowo posadzonego drzewa.",
                            "target_lang_translations": {
                                "ru": "Садовнику пришлось сильно <w>утрамбовать</w> землю вокруг недавно посаженного дерева."
                            }
                        },
                        {
                            "text": "Dzieci na plaży starannie <w>uklepały</w> piasek, aby zbudować trwały zamek.",
                            "target_lang_translations": {
                                "ru": "Дети на пляже старательно <w>уплотнили</w> песок, чтобы построить прочный замок."
                            }
                        }
                    ],
                    "synonyms": [
                        "ukuć",
                        "ukuć"
                    ],
                    "antonyms": [
                        "spulchnić",
                        "rozgrzebać",
                        "rozkopać"
                    ],
                    "common_phrases": [
                        "uklepać ziemię",
                        "uklepać piasek",
                        "uklepać śnieg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ударять ладонью или инструментом по чему-то мягкому или сыпучему (например, земле, песку), чтобы выровнять, уплотнить или придать форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утрамбовать",
                                "target_lang_sense_clarification": "землю, снег, песок"
                            },
                            {
                                "target_lang_word": "уплотнить",
                                "target_lang_sense_clarification": "сделать более плотным"
                            },
                            {
                                "target_lang_word": "примять",
                                "target_lang_sense_clarification": "придавить сверху"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1df9ee36-d3b6-3665-a300-82ee38ef186a",
                    "sense_definition": "Nadać kształt metalowemu przedmiotowi poprzez wielokrotne uderzanie młotem; wykuć.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "forging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony kowal potrafił <w>uklepać</w> kosę tak, by była ostra i wytrzymała.",
                            "target_lang_translations": {
                                "ru": "Опытный кузнец мог <w>отклепать</w> косу так, чтобы она была острой и прочной."
                            }
                        },
                        {
                            "text": "Z kawałka rozgrzanego żelaza udało mu się <w>uklepać</w> proste ostrze.",
                            "target_lang_translations": {
                                "ru": "Из куска раскаленного железа ему удалось <w>выковать</w> простое лезвие."
                            }
                        }
                    ],
                    "synonyms": [
                        "ukuć",
                        "ukuć"
                    ],
                    "common_phrases": [
                        "uklepać kosę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придать форму металлическому предмету путем многократных ударов молотом; выковать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выковать",
                                "target_lang_sense_clarification": "изготовить ковкой"
                            },
                            {
                                "target_lang_word": "отклепать",
                                "target_lang_sense_clarification": "обработать ударами молотка (например, косу)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

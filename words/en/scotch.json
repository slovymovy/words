                {
                    "sense_id": "d19a02df-f531-3ca2-b3b7-be15ac7e4a74",
                    "sense_definition": "Of or from Scotland; Scottish.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adj_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We enjoyed a traditional <w>Scotch</w> egg during our picnic in the park.",

            "senses": [
                {
                    "sense_id": "fb740f0d-254d-311f-a350-2d243a985713",
                    "sense_definition": "Solid waste passed from the body of an animal or bird.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We identified the animal by the <w>droppings</w> it left on the trail.",
                            "target_lang_translations": {
                                "ru": "Мы определили животное по <w>помёту</w>, который оно оставило на тропе."
                            }
                        },
                        {
                            "text": "The park bench was covered in bird <w>droppings</w>.",
                            "target_lang_translations": {
                                "ru": "Скамейка в парке была покрыта птичьим <w>помётом</w>."
                            }
                        }

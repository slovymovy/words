{
    "word_family": [
        "porwanie",
        "porywacz",
        "poryw",
        "porywczy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2772f808-8d7a-36f9-83e5-a163bd821009",
                    "sense_definition": "Uprowadzać kogoś siłą lub podstępem, pozbawiając go wolności wbrew jego woli.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przestępcy planowali <w>porywać</w> bogatych ludzi dla wysokiego okupu.",
                            "target_lang_translations": {
                                "ru": "Преступники планировали <w>похищать</w> богатых людей ради большого выкупа."
                            }
                        },
                        {
                            "text": "W tym filmie kosmici zaczynają <w>porywać</w> mieszkańców małego miasteczka.",
                            "target_lang_translations": {
                                "ru": "В этом фильме инопланетяне начинают <w>похищать</w> жителей маленького городка."
                            }
                        }
                    ],
                    "synonyms": [
                        "uprowadzać"
                    ],
                    "antonyms": [
                        "uwalniać",
                        "wypuszczać"
                    ],
                    "common_phrases": [
                        "porywać dla okupu",
                        "porywać ludzi",
                        "porywać dzieci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Похищать кого-либо силой или обманом, лишая свободы против воли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "похищать",
                                "target_lang_sense_clarification": "людей"
                            },
                            {
                                "target_lang_word": "красть",
                                "target_lang_sense_clarification": "разг. похищать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78a5c614-0417-3de6-933c-26555d006a02",
                    "sense_definition": "Wzbudzać w kimś silny zachwyt, entuzjazm lub emocje; fascynować.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego charyzmatyczne przemówienia potrafią <w>porywać</w> tłumy słuchaczy.",
                            "target_lang_translations": {
                                "ru": "Его харизматичные речи способны <w>захватывать</w> толпы слушателей."
                            }
                        },
                        {
                            "text": "Ta energetyczna muzyka od razu zaczęła <w>porywać</w> gości do tańca.",
                            "target_lang_translations": {
                                "ru": "Эта энергичная музыка сразу начала <w>увлекать</w> гостей в танец."
                            }
                        }
                    ],
                    "synonyms": [
                        "kraść",
                        "pojmować"
                    ],
                    "antonyms": [
                        "nudzić",
                        "zniechęcać"
                    ],
                    "common_phrases": [
                        "porywać tłumy",
                        "porywać do tańca",
                        "porywać serca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-либо сильный восторг, энтузиазм или эмоции; увлекать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увлекать",
                                "target_lang_sense_clarification": "вызывать интерес, эмоции"
                            },
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "ораторским искусством, вниманием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d9c8956-2e11-3cc3-9da5-dcbeaa75e985",
                    "sense_definition": "Gwałtownie unosić coś lub przemieszczać siłą żywiołu, na przykład wiatru lub wody.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny wiatr zaczął <w>porywać</w> suche liście z drzew w parku.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер начал <w>срывать</w> сухие листья с деревьев в парке."
                            }
                        },
                        {
                            "text": "Rwąca rzeka może <w>porywać</w> ze sobą nawet duże kamienie i drzewa.",
                            "target_lang_translations": {
                                "ru": "Бурная река может <w>уносить</w> с собой даже большие камни и деревья."
                            }
                        }
                    ],
                    "synonyms": [
                        "uprowadzać",
                        "kraść"
                    ],
                    "common_phrases": [
                        "wiatr porywa",
                        "nurt porywa",
                        "porywać dach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремительно уносить или перемещать силой стихии (ветра, воды)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уносить",
                                "target_lang_sense_clarification": "ветром или течением"
                            },
                            {
                                "target_lang_word": "срывать",
                                "target_lang_sense_clarification": "ветром (крышу, листья)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38974dfd-e81c-37b6-8e7a-ebbb863562d9",
                    "sense_definition": "Przejmować przemocą kontrolę nad środkiem transportu, takim jak samolot lub statek.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "abduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Terroryści grozili, że będą <w>porywać</w> kolejne samoloty pasażerskie.",
                            "target_lang_translations": {
                                "ru": "Террористы угрожали, что будут <w>захватывать</w> очередные пассажирские самолеты."
                            }
                        },
                        {
                            "text": "Współcześni piraci wciąż próbują <w>porywać</w> statki handlowe na oceanie.",
                            "target_lang_translations": {
                                "ru": "Современные пираты все еще пытаются <w>захватывать</w> торговые суда в океане."
                            }
                        }
                    ],
                    "synonyms": [
                        "uprowadzać"
                    ],
                    "common_phrases": [
                        "porywać samolot",
                        "porywać statek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Насильственно захватывать контроль над транспортным средством (самолетом, судном)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "транспортное средство"
                            },
                            {
                                "target_lang_word": "угонять",
                                "target_lang_sense_clarification": "самолет, судно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7db87ef-6904-3cc9-bd41-4634469c5e10",
                    "sense_definition": "Podejmować się wykonania czegoś trudnego, ryzykownego lub przekraczającego siły (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiomatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie warto <w>porywać</w> się na zadania, które przekraczają twoje możliwości.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>замахиваться</w> на задачи, которые превышают твои возможности."
                            }
                        },
                        {
                            "text": "Młody artysta postanowił <w>porywać</w> się na bardzo ambitny i kosztowny projekt.",
                            "target_lang_translations": {
                                "ru": "Молодой художник решил <w>замахиваться</w> на очень амбициозный и дорогостоящий проект."
                            }
                        }
                    ],
                    "common_phrases": [
                        "porywać się z motyką na słońce",
                        "porywać się na coś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Браться за выполнение чего-либо трудного или рискованного (обычно с возвратной частицей \"się\")."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замахиваться",
                                "target_lang_sense_clarification": "на что-то сложное (разг.)"
                            },
                            {
                                "target_lang_word": "браться",
                                "target_lang_sense_clarification": "за трудное дело"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4d6a8e21-0e9a-3584-b5f5-9826ca23bcb0",
                    "sense_definition": "Wygłosić mowę, skierować słowa do zebranych osób w sposób oficjalny lub uroczysty.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "public_speaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prezydent postanowił <przemówić> do obywateli w specjalnym orędziu telewizyjnym.",
                            "target_lang_translations": {
                                "ru": "Президент решил <обратиться> к гражданам со специальным телеобращением."
                            }
                        },
                        {
                            "text": "Dyrektor szkoły musiał krótko <przemówić> do uczniów podczas porannego apelu.",
                            "target_lang_translations": {
                                "ru": "Директор школы должен был кратко <выступить> перед учениками во время утренней линейки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przemówić do narodu",
                        "przemówić do zgromadzonych",
                        "uroczyście przemówić",
                        "krótko przemówić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произнести речь, официально обратиться к собравшимся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выступить",
                                "target_lang_sense_clarification": "произнести речь перед публикой"
                            },
                            {
                                "target_lang_word": "обратиться",
                                "target_lang_sense_clarification": "адресовать слова кому-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3270b266-5b6d-3ace-86f4-c1ff4b7d138a",
                    "sense_definition": "Trafić do czyjegoś przekonania, uczuć lub wyobraźni; wpłynąć na kogoś swoimi słowami lub formą.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz mu <przemówić> do rozsądku, zanim popełni ten nieodwracalny błąd.",
                            "target_lang_translations": {
                                "ru": "Ты должен <вразумить> его, пока он не совершил эту непоправимую ошибку."
                            }
                        },
                        {
                            "text": "Sztuka nowoczesna nie zawsze potrafi <przemówić> do przeciętnego odbiorcy w galerii.",
                            "target_lang_translations": {
                                "ru": "Современное искусство не всегда способно <тронуть> рядового посетителя галереи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przemówić do rozsądku",
                        "przemówić do wyobraźni",
                        "przemówić do serca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздействовать на чьи-либо убеждения, чувства или воображение; достучаться до кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воззвать",
                                "target_lang_sense_clarification": "к разуму или совести"
                            },
                            {
                                "target_lang_word": "тронуть",
                                "target_lang_sense_clarification": "эмоционально воздействовать"
                            },
                            {
                                "target_lang_word": "вразумить",
                                "target_lang_sense_clarification": "заставить понять, убедить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4291ed7-6430-3754-8f84-4ef3082eb9b6",
                    "sense_definition": "Zacząć mówić, przerwać milczenie lub odezwać się po raz pierwszy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "start_speaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Według legendy w wigilijną noc zwierzęta mogą <przemówić> ludzkim głosem.",
                            "target_lang_translations": {
                                "ru": "Согласно легенде, в рождественскую ночь животные могут <заговорить> человеческим голосом."
                            }
                        },
                        {
                            "text": "Chłopiec siedział cicho przez godzinę, aż w końcu nieśmiało <przemówił>.",
                            "target_lang_translations": {
                                "ru": "Мальчик сидел тихо целый час, пока наконец робко не <заговорил>."
                            }
                        }
                    ],
                    "synonyms": [
                        "odezwać się"
                    ],
                    "common_phrases": [
                        "przemówić ludzkim głosem",
                        "wreszcie przemówił",
                        "przemówić po polsku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать говорить, нарушить молчание или подать голос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заговорить",
                                "target_lang_sense_clarification": "начать речь"
                            },
                            {
                                "target_lang_word": "промолвить",
                                "target_lang_sense_clarification": "произнести (часто в литературном контексте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bac004b-f1bc-3a5f-bb99-3322f9641f56",
                    "sense_definition": "Stanowić dowód na coś, świadczyć o czymś (zwykle w kontekście faktów, dowodów lub okoliczności).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "evidence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zebrane przez policję dowody musiały <przemówić> na niekorzyść oskarżonego.",
                            "target_lang_translations": {
                                "ru": "Собранные полицией улики должны были <свидетельствовать> против обвиняемого."
                            }
                        },
                        {
                            "text": "Te statystyki powinny <przemówić> do wyobraźni inwestorów i zachęcić ich do działania.",
                            "target_lang_translations": {
                                "ru": "Эта статистика должна <воздействовать> на воображение инвесторов и побудить их к действию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fakty przemówiły",
                        "przemówić za czymś",
                        "przemówić na niekorzyść"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служить доказательством, свидетельствовать о чем-либо (обычно о фактах или обстоятельствах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свидетельствовать",
                                "target_lang_sense_clarification": "быть доказательством"
                            },
                            {
                                "target_lang_word": "говорить",
                                "target_lang_sense_clarification": "указывать на что-то (о фактах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4eb0507-88ce-393a-9698-dabf5bb6a294",
                    "sense_definition": "Wstawić się za kimś, powiedzieć coś na czyjąś korzyść, aby pomóc tej osobie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "intercession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy mógłbyś <przemówić> za mną u szefa w sprawie mojego urlopu?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты <замолвить словечко> за меня перед шефом насчет моего отпуска?"
                            }
                        },
                        {
                            "text": "Nikt nie miał odwagi <przemówić> w jego obronie podczas burzliwego zebrania.",
                            "target_lang_translations": {
                                "ru": "Ни у кого не хватило смелости <выступить> в его защиту во время бурного собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "wstawić się"
                    ],
                    "common_phrases": [
                        "przemówić za kimś",
                        "dobre słowo przemówić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заступиться за кого-то, сказать что-то в чью-то пользу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замолвить слово",
                                "target_lang_sense_clarification": "попросить за кого-то"
                            },
                            {
                                "target_lang_word": "вступиться",
                                "target_lang_sense_clarification": "встать на защиту словами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abe049a5-60a5-3672-be40-423056b2fd42",
                    "sense_definition": "Ujawnić się w czyimś zachowaniu lub słowach (często o emocjach, cechach charakteru lub stanach).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "manifestation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W jego ostrych i niesprawiedliwych słowach zdawała się <przemówić> dawna uraza.",
                            "target_lang_translations": {
                                "ru": "В его резких и несправедливых словах, казалось, <заговорила> старая обида."
                            }
                        },
                        {
                            "text": "Nie bierz jego słów do siebie, to mógł po prostu <przemówić> przez niego alkohol.",
                            "target_lang_translations": {
                                "ru": "Не принимай его слова близко к сердцу, это мог просто <говорить> в нем алкоголь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przemawia przez niego",
                        "przemówiła zazdrość",
                        "przemówiła pycha"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in phrases like 'przemawia przez niego...'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявиться в чьем-либо поведении или словах (о чувствах, чертах характера)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заговорить",
                                "target_lang_sense_clarification": "проявиться (об эмоциях или состояниях)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
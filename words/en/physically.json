{
    "word_family": [
        "physical",
        "physicality",
        "physics",
        "physicist"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "01d59149-7883-3afa-8c43-0aa0527d502c",
                    "sense_definition": "In a way that relates to the body rather than the mind or feelings.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "body_vs_mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>physically</w> fit enough to run a marathon this weekend.",
                            "target_lang_translations": {
                                "ru": "Он достаточно <w>физически</w> подготовлен, чтобы пробежать марафон в эти выходные."
                            }
                        },
                        {
                            "text": "The job is <w>physically</w> demanding, requiring heavy lifting all day.",
                            "target_lang_translations": {
                                "ru": "Эта работа <w>физически</w> тяжелая и требует поднятия тяжестей в течение всего дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "bodily"
                    ],
                    "antonyms": [
                        "mentally",
                        "psychologically",
                        "emotionally"
                    ],
                    "common_phrases": [
                        "physically fit",
                        "physically active",
                        "physically demanding",
                        "physically handicapped"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с телом и организмом, в отличие от разума или чувств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "физически",
                                "target_lang_sense_clarification": "телесно, в отношении организма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d96b41c-94eb-368d-a77c-8980465e597b",
                    "sense_definition": "According to the laws of nature or what is possible in the real world.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>physically</w> impossible for a human to fly without assistance.",
                            "target_lang_translations": {
                                "ru": "Для человека <w>физически</w> невозможно летать без посторонней помощи."
                            }
                        },
                        {
                            "text": "We need to determine if the project is <w>physically</w> possible within the timeframe.",
                            "target_lang_translations": {
                                "ru": "Нам нужно определить, <w>физически</w> ли возможно выполнить проект в эти сроки."
                            }
                        }
                    ],
                    "synonyms": [
                        "materially"
                    ],
                    "antonyms": [
                        "theoretically"
                    ],
                    "common_phrases": [
                        "physically possible",
                        "physically impossible"
                    ],
                    "target_lang_definitions": {
                        "ru": "В соответствии с законами природы или возможностями реального мира."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "физически",
                                "target_lang_sense_clarification": "фактически, в реальности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eafa62c4-9ead-3404-8526-f7364f15d16a",
                    "sense_definition": "By direct personal action or by being in a specific location, rather than remotely or automatically.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "presence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All students must be <w>physically</w> present in the classroom to take the exam.",
                            "target_lang_translations": {
                                "ru": "Все студенты должны <w>физически</w> присутствовать в классе, чтобы сдать экзамен."
                            }
                        },
                        {
                            "text": "I had to <w>physically</w> restart the computer because the remote connection failed.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось <w>физически</w> перезагрузить компьютер, так как удаленное соединение прервалось."
                            }
                        }
                    ],
                    "synonyms": [
                        "manually",
                        "personally"
                    ],
                    "antonyms": [
                        "remotely",
                        "virtually",
                        "automatically"
                    ],
                    "common_phrases": [
                        "physically present",
                        "physically located"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непосредственно, лично или путем прямого действия, а не удаленно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "физически",
                                "target_lang_sense_clarification": "лично, непосредственно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba5aae98-359e-3461-9983-3d5ee5a498d2",
                    "sense_definition": "Using force or violence against someone.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The protesters were <w>physically</w> removed from the building by security guards.",
                            "target_lang_translations": {
                                "ru": "Охранники <w>физически</w> вывели протестующих из здания."
                            }
                        },
                        {
                            "text": "She was afraid that the argument might become <w>physically</w> abusive.",
                            "target_lang_translations": {
                                "ru": "Она боялась, что ссора может стать <w>физически</w> агрессивной."
                            }
                        }
                    ],
                    "synonyms": [
                        "violently",
                        "forcefully"
                    ],
                    "common_phrases": [
                        "physically attack",
                        "physically assault",
                        "physically abusive"
                    ],
                    "target_lang_definitions": {
                        "ru": "С использованием грубой силы или насилия по отношению к кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "физически",
                                "target_lang_sense_clarification": "с применением силы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
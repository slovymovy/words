    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d9e3a049-b12e-3f2b-b91c-73eb0665f6e1",
                    "sense_definition": "Постигать смысл слов, речи, языка или какого-либо явления; осознавать значение чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intellect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я совсем не <w>понимаю</w>, о чём вы сейчас говорите.",
                            "target_lang_translations": {
                                "en": "I don't <w>understand</w> at all what you are talking about right now."
                            }
                        },
                        {
                            "text": "Студенты быстро начали <w>понимать</w> новый материал на лекции.",
                            "target_lang_translations": {
                                "en": "The students quickly began to <w>grasp</w> the new material at the lecture."
                            }
                        },
                        {
                            "text": "Он немного <w>понимает</w> по-русски, но говорить пока стесняется.",
                            "target_lang_translations": {
                                "en": "He <w>understands</w> a little Russian, but is still shy to speak."
                            }
                        }
                    ],
                    "synonyms": [
                        "осознавать",
                        "разуметь",
                        "соображать"
                    ],
                    "common_phrases": [
                        "понимать по-русски",
                        "понимать буквально",
                        "понимать с полуслова",
                        "понимать намёки"
                    ],
                    "target_lang_definitions": {
                        "en": "To grasp the meaning of words, speech, language, or a phenomenon; to realize the significance of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "understand",
                                "target_lang_sense_clarification": "general meaning"
                            },
                            {
                                "target_lang_word": "comprehend",
                                "target_lang_sense_clarification": "formal, implies thorough understanding"
                            },
                            {
                                "target_lang_word": "grasp",
                                "target_lang_sense_clarification": "to catch the meaning of something"
                            },
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "informal, as in 'I see what you mean'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46eeb48b-0a93-303d-bc26-cbd8e752c487",
                    "sense_definition": "Постигать состояние, настроение, намерения или мотивы поступков кого-либо; проявлять сочувствие.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "empathy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хороший учитель всегда старается <w>понимать</w> проблемы своих учеников.",
                            "target_lang_translations": {
                                "en": "A good teacher always tries to <w>understand</w> their students' problems."
                            }
                        },
                        {
                            "text": "Мы с сестрой умеем <w>понимать</w> друг друга даже без слов.",
                            "target_lang_translations": {
                                "en": "My sister and I know how to <w>understand</w> each other even without words."
                            }
                        }
                    ],
                    "common_phrases": [
                        "понимать друг друга",
                        "понимать без слов",
                        "понимать чувства",
                        "понимать людей"
                    ],
                    "target_lang_definitions": {
                        "en": "To perceive someone's state, mood, intentions, or motives; to show empathy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "understand",
                                "target_lang_sense_clarification": "to know someone's character or motives"
                            },
                            {
                                "target_lang_word": "empathize with",
                                "target_lang_sense_clarification": "to share or understand the feelings of another"
                            },
                            {
                                "target_lang_word": "relate to",
                                "target_lang_sense_clarification": "to feel a connection with"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cee5fc6-e4b5-3edd-8bf4-f35dde890c54",
                    "sense_definition": "Толковать или расценивать что-либо определённым образом; иметь своё мнение или точку зрения.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "interpretation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его отказ помочь можно <w>понимать</w> как проявление полного равнодушия.",
                            "target_lang_translations": {
                                "en": "His refusal to help can be <w>interpreted</w> as a display of complete indifference."
                            }
                        },
                        {
                            "text": "Вы неправильно <w>понимаете</w> мои слова, я не это имел в виду.",
                            "target_lang_translations": {
                                "en": "You are <w>interpreting</w> my words incorrectly; that is not what I meant."
                            }
                        }
                    ],
                    "synonyms": [
                        "разуметь"
                    ],
                    "common_phrases": [
                        "понимать превратно",
                        "как это понимать",
                        "понимать иначе",
                        "понимать по-своему"
                    ],
                    "target_lang_definitions": {
                        "en": "To interpret or regard something in a particular way; to have one's own opinion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "interpret",
                                "target_lang_sense_clarification": "to construe or explain the meaning of"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "to receive or accept in a particular sense"
                            },
                            {
                                "target_lang_word": "understand",
                                "target_lang_sense_clarification": "to infer or assume"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ffbd159-f287-3710-b2c9-87c9d29fb7cb",
                    "sense_definition": "Быть знатоком в какой-либо области, иметь опыт и вкус, разбираться в чём-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "expertise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец хорошо <w>понимает</w> в машинах и может починить любую поломку.",
                            "target_lang_translations": {
                                "en": "Father <w>knows a lot about</w> cars and can fix any breakdown."
                            }
                        },
                        {
                            "text": "Она прекрасно <w>понимает</w> в современной моде и высоком искусстве.",
                            "target_lang_translations": {
                                "en": "She really <w>understands</w> modern fashion and high art."
                            }
                        }
                    ],
                    "synonyms": [
                        "разбираться",
                        "знать"
                    ],
                    "common_phrases": [
                        "понимать в искусстве",
                        "понимать толк",
                        "понимать в музыке",
                        "понимать в технике"
                    ],
                    "target_lang_definitions": {
                        "en": "To be knowledgeable or an expert in a specific field; to have experience and taste."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "know a lot about",
                                "target_lang_sense_clarification": "idiomatic equivalent for 'понимать в'"
                            },
                            {
                                "target_lang_word": "be a judge of",
                                "target_lang_sense_clarification": "to have the ability to judge the quality of something"
                            },
                            {
                                "target_lang_word": "understand",
                                "target_lang_sense_clarification": "to have expertise in"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "640fbbf3-5ed1-36f3-b544-87ffc4d48c54",
                    "sense_definition": "Заливать, покрывать водой (о реке, половодье).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждую весну вода <w>понимает</w> низкий берег реки и затапливает луга.",
                            "target_lang_translations": {
                                "en": "Every spring the water <w>floods</w> the low river bank and inundates the meadows."
                            }
                        },
                        {
                            "text": "Старожилы рассказывали, как в прошлом веке деревню <w>понимало</w> до самых крыш.",
                            "target_lang_translations": {
                                "en": "Old-timers told how in the last century the village was <w>flooded</w> up to the very roofs."
                            }
                        }
                    ],
                    "synonyms": [
                        "заливать"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "used in specific regions regarding floods"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To cover with water or overflow (referring to a river or flood)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flood",
                                "target_lang_sense_clarification": "to cover with water"
                            },
                            {
                                "target_lang_word": "inundate",
                                "target_lang_sense_clarification": "to overwhelm with water"
                            }
                        ]
                    }
                }
            ]
        }
    ]

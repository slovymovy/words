            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fd38ce0a-caa8-3f08-876e-6562ae445613",
                    "sense_definition": "Voluntary agreement or permission for something to happen or be done.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "permission_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must obtain written <w>consent</w> from your parents before going on the school trip."
                        },
                        {
                            "text": "The doctor cannot perform the surgery without the patient's informed <w>consent</w>."
                        }
                    ],
                    "synonyms": [
                        "permission",
                        "agreement",
                        "approval",
                        "assent"
                    ],
                    "antonyms": [
                        "refusal",
                        "dissent",
                        "opposition",
                        "disagreement"
                    ],
                    "common_phrases": [
                        "give consent",
                        "written consent",
                        "age of consent",
                        "mutual consent",
                        "informed consent"
                    ],
                    "traits": [

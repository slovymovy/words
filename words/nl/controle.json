    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa880db7-70b5-36b1-a8cf-8458072d1f0d",
                    "sense_definition": "Het nakijken of inspecteren van iets om te zien of het in orde is en aan de regels voldoet.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De douane voert een strenge <w>controle</w> uit bij de grens.",
                            "target_lang_translations": {
                                "en": "Customs carries out a strict <w>inspection</w> at the border.",
                                "ru": "Таможня проводит строгую <w>проверку</w> на границе."
                            }
                        },
                        {
                            "text": "Ik ga elk half jaar naar de tandarts voor een periodieke <w>controle</w>.",
                            "target_lang_translations": {
                                "en": "I go to the dentist every six months for a periodic <w>check-up</w>.",
                                "ru": "Я хожу к стоматологу каждые полгода на плановый <w>осмотр</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "toezicht",
                        "check",
                        "verificatie",
                        "supervisie"
                    ],
                    "common_phrases": [
                        "strenge controle",
                        "controle uitvoeren",
                        "op controle"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of checking or inspecting something to see if it is in order and complies with rules.",
                        "ru": "Проверка или осмотр чего-либо, чтобы убедиться, что всё в порядке и соответствует правилам."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "check",
                                "target_lang_sense_clarification": "general verification"
                            },
                            {
                                "target_lang_word": "inspection",
                                "target_lang_sense_clarification": "formal examination"
                            },
                            {
                                "target_lang_word": "check-up",
                                "target_lang_sense_clarification": "medical or routine assessment"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проверка",
                                "target_lang_sense_clarification": "инспекция, ревизия"
                            },
                            {
                                "target_lang_word": "осмотр",
                                "target_lang_sense_clarification": "медицинское или визуальное обследование"
                            },
                            {
                                "target_lang_word": "контроль",
                                "target_lang_sense_clarification": "надзор, проверка соблюдения норм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "122f6ff8-53d6-311c-b733-8260e3983d23",
                    "sense_definition": "De macht of het vermogen om iets of iemand te beheersen en te sturen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de gladde weg verloor de bestuurder de <w>controle</w> over het stuur.",
                            "target_lang_translations": {
                                "en": "Due to the slippery road, the driver lost <w>control</w> of the steering wheel.",
                                "ru": "Из-за скользкой дороги водитель потерял <w>управление</w> рулём."
                            }
                        },
                        {
                            "text": "Gelukkig is de situatie nu weer volledig onder <w>controle</w>.",
                            "target_lang_translations": {
                                "en": "Fortunately, the situation is now fully under <w>control</w> again.",
                                "ru": "К счастью, ситуация снова полностью под <w>контролем</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "onder controle",
                        "controle verliezen",
                        "buiten controle"
                    ],
                    "target_lang_definitions": {
                        "en": "The power or ability to manage, direct, or restrain something or someone.",
                        "ru": "Власть или способность управлять кем-либо или чем-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "control",
                                "target_lang_sense_clarification": "power or authority over something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "контроль",
                                "target_lang_sense_clarification": "власть, управление ситуацией"
                            },
                            {
                                "target_lang_word": "управление",
                                "target_lang_sense_clarification": "руководство действиями, вождение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

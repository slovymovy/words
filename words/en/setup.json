            "senses": [
                {
                    "sense_id": "5ae11673-18d3-31b3-b284-988604462870",
                    "sense_definition": "The way in which something is organized, arranged, or structured.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "arrangement_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The classroom <w>setup</w> was simple and efficient.",

                },
                {
                    "sense_id": "4fbee277-5993-3fbf-a212-52f2c9a70048",
                    "sense_definition": "Zonder medelijden of emotie; streng en onverbiddelijk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "ruthless",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij staat bekend als een <w>keiharde</w> zakenman.",
                            "target_lang_translations": {
                                "ru": "Он известен как <w>жёсткий</w> бизнесмен."
                            }

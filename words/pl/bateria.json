                        {
                            "text": "Ta <w>bateria</w> wywiera silną presję na pozycję króla przeciwnika.",
                            "target_lang_translations": {
                                "ru": "Эта <w>батарея</w> оказывает сильное давление на позицию короля противника."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В шахматах: расположение двух или более дальнобойных фигур на одной линии для усиления атаки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "батарея",
                                "target_lang_sense_clarification": "шахматный термин"
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "229298b7-12c0-386d-bb5d-5173a691aa61",
                    "sense_definition": "Наличие одинаковых прав и возможностей у людей, групп или государств; официальное признание равенства перед законом.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rights",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Конституция страны гарантирует <w>равноправие</w> всех граждан независимо от их происхождения.",
                            "target_lang_translations": {
                                "en": "The country's constitution guarantees the <w>equality</w> of all citizens regardless of their origin."
                            }
                        },
                        {
                            "text": "Многие общественные организации ведут активную борьбу за <w>равноправие</w> женщин и мужчин.",
                            "target_lang_translations": {
                                "en": "Many public organizations are actively fighting for the <w>equality</w> of women and men."
                            }
                        }
                    ],
                    "synonyms": [
                        "равенство",
                        "полноправие",
                        "равноправность"
                    ],
                    "antonyms": [
                        "неравноправие",
                        "дискриминация"
                    ],
                    "common_phrases": [
                        "полное равноправие",
                        "борьба за равноправие",
                        "принцип равноправия",
                        "гендерное равноправие"
                    ],
                    "target_lang_definitions": {
                        "en": "The possession of equal rights and opportunities by people, groups, or states; official recognition of equality before the law."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "equality",
                                "target_lang_sense_clarification": "rights and opportunities"
                            },
                            {
                                "target_lang_word": "equal rights",
                                "target_lang_sense_clarification": "legal status"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c2d7796-216b-3565-ba89-7de9df878640",
                    "sense_definition": "Одинаковая значимость, ценность или положение предметов, явлений или понятий.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В современном искусстве часто утверждается <w>равноправие</w> различных стилей и жанров.",
                            "target_lang_translations": {
                                "en": "Modern art often asserts the <w>equality</w> of various styles and genres."
                            }
                        },
                        {
                            "text": "Ученый отстаивал <w>равноправие</w> двух гипотез до получения окончательных результатов эксперимента.",
                            "target_lang_translations": {
                                "en": "The scientist argued for the <w>equal status</w> of the two hypotheses until the final results of the experiment were obtained."
                            }
                        }
                    ],
                    "synonyms": [
                        "равенство",
                        "паритет"
                    ],
                    "common_phrases": [
                        "равноправие сторон",
                        "равноправие языков"
                    ],
                    "target_lang_definitions": {
                        "en": "Equal significance, value, or status of objects, phenomena, or concepts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "equality",
                                "target_lang_sense_clarification": "of value or status"
                            },
                            {
                                "target_lang_word": "parity",
                                "target_lang_sense_clarification": "equivalence"
                            }
                        ]
                    }
                }
            ]
        }
    ]

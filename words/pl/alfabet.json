                {
                    "sense_id": "313a1ef4-f53d-39e1-9230-a330576f39d1",
                    "sense_definition": "Zbiór wszystkich liter danego języka ułożony w ogólnie przyjętym, ustalonym porządku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "system_znakow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci w pierwszej klasie szkoły podstawowej poznają cały <w>alfabet</w>.",
                            "target_lang_translations": {
                                "ru": "Дети в первом классе начальной школы изучают весь <w>алфавит</w>."
                            }
                        },
                        {
                            "text": "W polskim <w>alfabecie</w> występują znaki diakrytyczne, których nie ma w języku angielskim.",
                            "target_lang_translations": {
                                "ru": "В польском <w>алфавите</w> есть диакритические знаки, которых нет в английском языке."
                            }
                        }
                    ],
                    "synonyms": [
                        "abecadło"
                    ],
                    "common_phrases": [
                        "polski alfabet",
                        "litery alfabetu",
                        "kolejność alfabetu",
                        "uczyć się alfabetu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность всех букв данного языка, расположенных в установленном порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алфавит",
                                "target_lang_sense_clarification": "перечень букв"
                            },
                            {
                                "target_lang_word": "азбука",
                                "target_lang_sense_clarification": "совокупность букв"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dc6b258-57ea-38c5-88d9-645e00cb82b7",
                    "sense_definition": "Uporządkowany zbiór znaków jakiegoś systemu umownego, które odpowiadają literom i służą do przekazywania informacji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "system_znakow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Harcerze podczas zbiórki uczyli się nadawać wiadomości używając <w>alfabetu</w> Morse'a.",
                            "target_lang_translations": {
                                "ru": "Скауты во время сбора учились передавать сообщения, используя <w>азбуку</w> Морзе."
                            }
                        },
                        {
                            "text": "Osoby niewidome czytają książki napisane specjalnym <w>alfabetem</w> Braille'a.",
                            "target_lang_translations": {
                                "ru": "Слепые люди читают книги, написанные специальной <w>азбукой</w> Брайля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "alfabet Morse'a",
                        "alfabet Braille'a",
                        "alfabet fonetyczny",
                        "alfabet palcowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упорядоченный набор знаков какой-либо условной системы, соответствующих буквам и служащих для передачи информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "азбука",
                                "target_lang_sense_clarification": "код, система знаков (напр. Морзе)"
                            },
                            {
                                "target_lang_word": "алфавит",
                                "target_lang_sense_clarification": "система письма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f823be25-083a-30e4-af58-dba5dba2ecc2",
                    "sense_definition": "Podstawowe wiadomości z danej dziedziny wiedzy lub umiejętności; najważniejsze zasady czegoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znajomość przepisów ruchu drogowego to <w>alfabet</w> każdego odpowiedzialnego kierowcy.",
                            "target_lang_translations": {
                                "ru": "Знание правил дорожного движения — это <w>азбука</w> каждого ответственного водителя."
                            }
                        },
                        {
                            "text": "Ta krótka książka stanowi <w>alfabet</w> ekonomii dla osób początkujących.",
                            "target_lang_translations": {
                                "ru": "Эта короткая книга представляет собой <w>азбуку</w> экономики для начинающих."

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fc2417ed-5a57-3218-a419-7d94a53a9f40",
                    "sense_definition": "To have a strong affection for someone or something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_affection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>love</w> my family more than anything in the world.",
                            "target_lang_translations": {
                                "ru": "Я <w>люблю</w> свою семью больше всего на свете."
                            }
                        },
                        {
                            "text": "She <w>loves</w> her children and would do anything for them.",
                            "target_lang_translations": {
                                "ru": "Она <w>любит</w> своих детей и сделает для них всё что угодно."
                            }
                        }
                    ],
                    "synonyms": [
                        "adore",
                        "cherish",
                        "care for",
                        "dote"
                    ],
                    "antonyms": [
                        "hate",
                        "despise"
                    ],
                    "common_phrases": [
                        "love you",
                        "love him",
                        "love her"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать сильную привязанность к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любить",
                                "target_lang_sense_clarification": "чувствовать глубокую привязанность"
                            },
                            {
                                "target_lang_word": "обожать",
                                "target_lang_sense_clarification": "очень сильно любить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f18762c-809c-3606-89e2-eac0c7cce82a",
                    "sense_definition": "To like something very much; to derive great pleasure from something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_like",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>love</w> walking barefoot on the wet grass in summer.",
                            "target_lang_translations": {
                                "ru": "Я <w>люблю</w> ходить босиком по мокрой траве летом."
                            }
                        },
                        {
                            "text": "We would <w>love</w> to join the team next season.",
                            "target_lang_translations": {
                                "ru": "Мы бы очень <w>хотели</w> присоединиться к команде в следующем сезоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "enjoy",
                        "fancy",
                        "adore"
                    ],
                    "antonyms": [
                        "hate",
                        "despise"
                    ],
                    "common_phrases": [
                        "would love to",
                        "love to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень сильно что-то любить; получать огромное удовольствие от чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любить",
                                "target_lang_sense_clarification": "испытывать удовольствие от чего-либо"
                            },
                            {
                                "target_lang_word": "обожать",
                                "target_lang_sense_clarification": "иметь пристрастие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a20b806-e927-3373-b563-48cd184a00a2",
                    "sense_definition": "To need or thrive on specific conditions (usually regarding plants or organisms).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_thrive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This type of mold <w>loves</w> moist and dark places.",
                            "target_lang_translations": {
                                "ru": "Этот вид плесени <w>любит</w> влажные и темные места."
                            }
                        },
                        {
                            "text": "These tropical plants <w>love</w> direct sunlight and high humidity.",
                            "target_lang_translations": {
                                "ru": "Эти тропические растения <w>любят</w> прямой солнечный свет и высокую влажность."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нуждаться в определенных условиях для роста или процветания (обычно о растениях или организмах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любить",
                                "target_lang_sense_clarification": "нуждаться в благоприятных условиях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bc7bbf0f-6a3e-3c97-9d9c-cbe61c495012",
                    "sense_definition": "A profound and caring affection towards someone.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "noun_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A mother’s <w>love</w> for her child is not easily shaken.",
                            "target_lang_translations": {
                                "ru": "Материнская <w>любовь</w> к ребенку непоколебима."
                            }
                        },
                        {
                            "text": "They fell in <w>love</w> during their summer vacation.",
                            "target_lang_translations": {
                                "ru": "Между ними вспыхнула <w>любовь</w> во время летнего отпуска."
                            }
                        }
                    ],
                    "synonyms": [
                        "affection",
                        "devotion",
                        "passion"
                    ],
                    "antonyms": [
                        "hate",
                        "hatred"
                    ],
                    "common_phrases": [
                        "in love",
                        "fall in love",
                        "true love",
                        "love at first sight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубокая и заботливая привязанность к кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любовь",
                                "target_lang_sense_clarification": "чувство глубокой привязанности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e718e40c-1adb-3749-89d0-29888d4f16bf",
                    "sense_definition": "A person who is the object of romantic feelings; a darling or sweetheart.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Goodbye, my <w>love</w>, I will see you again soon.",
                            "target_lang_translations": {
                                "ru": "Прощай, <w>любовь</w> моя, скоро увидимся."
                            }
                        },
                        {
                            "text": "She introduced him as the <w>love</w> of her life.",
                            "target_lang_translations": {
                                "ru": "Она представила его как <w>любовь</w> всей своей жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "darling",
                        "sweetheart",
                        "honey",
                        "baby",
                        "beloved"
                    ],
                    "common_phrases": [
                        "my love",
                        "love of my life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, являющийся объектом романтических чувств; возлюбленный или возлюбленная."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любовь",
                                "target_lang_sense_clarification": "объект чувств"
                            },
                            {
                                "target_lang_word": "любимый",
                                "target_lang_sense_clarification": "возлюбленный (мужчина)"
                            },
                            {
                                "target_lang_word": "любимая",
                                "target_lang_sense_clarification": "возлюбленная (женщина)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66a0b890-432e-34b1-8878-b375d6364473",
                    "sense_definition": "Used as a closing formula before the signature in a letter to a friend or relative.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "noun_closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "See you soon, lots of <w>love</w>, Alice.",
                            "target_lang_translations": {
                                "ru": "До скорого, с <w>любовью</w>, Алиса."
                            }
                        },
                        {
                            "text": "I hope you are well; with <w>love</w>, Mom.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, у тебя всё хорошо; с <w>любовью</w>, Мама."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lots of love",
                        "with love"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как заключительная формула перед подписью в письме другу или родственнику."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с любовью",
                                "target_lang_sense_clarification": "подпись в письме"
                            },
                            {
                                "target_lang_word": "целую",
                                "target_lang_sense_clarification": "распространенная концовка письма близким"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba6f9992-9fd9-306a-b2b1-9bb13f23aa0f",
                    "sense_definition": "A term of friendly address, regardless of romantic feelings.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hello <w>love</w>, how can I help you today?",
                            "target_lang_translations": {
                                "ru": "Здравствуй, <w>милая</w>, чем могу помочь сегодня?"
                            }
                        },
                        {
                            "text": "Don't worry about it, <w>love</w>, it is not a big deal.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся об этом, <w>дорогая</w>, это пустяки."
                            }
                        }
                    ],
                    "synonyms": [
                        "dear",
                        "mate",
                        "darling"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дружеское обращение, независимо от романтических чувств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дорогой",
                                "target_lang_sense_clarification": "обращение к мужчине"
                            },
                            {
                                "target_lang_word": "дорогая",
                                "target_lang_sense_clarification": "обращение к женщине"
                            },
                            {
                                "target_lang_word": "милый",
                                "target_lang_sense_clarification": "дружеское обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0043c67-e08f-3b2a-8ba0-e1610b929fe1",
                    "sense_definition": "A thing or activity which is the object of one's deep liking or enthusiasm.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Flying was her first <w>love</w>, and she became a pilot.",
                            "target_lang_translations": {
                                "ru": "Полеты были её первой <w>страстью</w>, и она стала пилотом."
                            }
                        },
                        {
                            "text": "His <w>love</w> of cricket knows no bounds.",
                            "target_lang_translations": {
                                "ru": "Его <w>любовь</w> к крикету не знает границ."
                            }
                        }
                    ],
                    "synonyms": [
                        "passion",
                        "interest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещь или занятие, являющееся объектом глубокой симпатии или энтузиазма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страсть",
                                "target_lang_sense_clarification": "сильное увлечение"
                            },
                            {
                                "target_lang_word": "любовь",
                                "target_lang_sense_clarification": "предмет обожания"
                            },
                            {
                                "target_lang_word": "увлечение",
                                "target_lang_sense_clarification": "хобби или интерес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8ca7da9-9024-3426-b2b8-ee0e81503ca0",
                    "sense_definition": "A score of zero in tennis.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_tennis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The score is currently fifteen-<w>love</w> to the challenger.",
                            "target_lang_translations": {
                                "ru": "Счет сейчас пятнадцать-<w>ноль</w> в пользу претендента."
                            }
                        },
                        {
                            "text": "He came back from two sets to <w>love</w> down to win the match.",
                            "target_lang_translations": {
                                "ru": "Он отыгрался со счета два-<w>ноль</w> по сетам и выиграл матч."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Счет ноль в теннисе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ноль",
                                "target_lang_sense_clarification": "счет в спорте"
                            }
                        ]
                    }
                }
            ]
        },
        {

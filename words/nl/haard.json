                {
                    "sense_id": "90384c33-eb12-3443-8171-5d6d54437755",
                    "sense_definition": "Een stookplaats in een woning waar men een open vuur kan maken voor warmte en sfeer",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zaten de hele avond gezellig bij de <w>haard</w> te praten.",
                            "target_lang_translations": {
                                "en": "We sat talking cozily by the <w>fireplace</w> all evening.",
                                "ru": "Мы весь вечер уютно просидели у <w>камина</w> за разговорами."
                            }
                        },
                        {
                            "text": "Hij gooide nog een groot blok hout op de <w>haard</w>.",
                            "target_lang_translations": {
                                "en": "He threw another large log on the <w>fire</w>.",
                                "ru": "Он подбросил в <w>камин</w> большое полено."
                            }
                        }
                    ],
                    "synonyms": [
                        "open haard",
                        "schouw"
                    ],
                    "common_phrases": [
                        "bij de haard",
                        "de haard aansteken",
                        "eigen haard is goud waard"
                    ],
                    "target_lang_definitions": {
                        "en": "A place in a home where an open fire can be made for warmth and atmosphere",
                        "ru": "Место для разведения огня в доме, служащее для отопления и создания уюта"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fireplace",
                                "target_lang_sense_clarification": "structure for a domestic fire"
                            },
                            {
                                "target_lang_word": "hearth",
                                "target_lang_sense_clarification": "the floor of a fireplace or the area in front of it"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "камин",
                                "target_lang_sense_clarification": "открытая печь с дымоходом"
                            },
                            {
                                "target_lang_word": "очаг",
                                "target_lang_sense_clarification": "место для огня; символ домашнего уюта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e464686-a847-36b6-b819-838404c8bb6d",
                    "sense_definition": "De plaats waar een ziekte, brand of onrust ontstaat en zich vandaan verspreidt",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het ziekenhuis bleek de <w>haard</w> van de infectie te zijn.",
                            "target_lang_translations": {
                                "en": "The hospital turned out to be the <w>source</w> of the infection.",
                                "ru": "Больница оказалась <w>очагом</w> инфекции."
                            }
                        },
                        {
                            "text": "De politie probeerde de <w>haard</w> van het verzet snel onder controle te krijgen.",
                            "target_lang_translations": {
                                "en": "The police tried to quickly get the <w>center</w> of the resistance under control.",
                                "ru": "Полиция пыталась быстро взять под контроль <w>очаг</w> сопротивления."
                            }
                        }
                    ],
                    "synonyms": [

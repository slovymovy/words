    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4110bbaa-8cbf-310d-bcd6-f00da712fbf2",
                    "sense_definition": "Проверять что-либо, следить за правильностью действий или состоянием чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель должен внимательно <контролировать> выполнение домашнего задания каждым учеником.",
                            "target_lang_translations": {
                                "en": "The teacher must carefully <monitor> the completion of homework by every student."
                            }
                        },
                        {
                            "text": "Мы будем строго <контролировать> качество продукции на всех этапах производства.",
                            "target_lang_translations": {
                                "en": "We will strictly <control> the quality of products at all stages of production."
                            }
                        }
                    ],
                    "synonyms": [
                        "проверять",
                        "следить",
                        "инспектировать"
                    ],
                    "common_phrases": [
                        "контролировать качество",
                        "контролировать выполнение",
                        "контролировать работу",
                        "строго контролировать"
                    ],
                    "target_lang_definitions": {
                        "en": "To monitor or verify the correctness of actions or the state of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monitor",
                                "target_lang_sense_clarification": "to watch and check a situation or process carefully over a period of time"
                            },
                            {
                                "target_lang_word": "supervise",
                                "target_lang_sense_clarification": "to be in charge of an activity or person and make sure things are done correctly"
                            },
                            {
                                "target_lang_word": "check",
                                "target_lang_sense_clarification": "to examine something to see if it is correct or satisfactory"
                            },
                            {
                                "target_lang_word": "control",
                                "target_lang_sense_clarification": "to verify or regulate a process (e.g., quality control)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3cc6557-3b26-3650-aa3a-ca640b357d39",
                    "sense_definition": "Иметь власть, управлять кем-либо или чем-либо, а также сдерживать свои эмоции или поведение.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В сложной ситуации очень важно уметь <контролировать> свои эмоции и страх.",
                            "target_lang_translations": {
                                "en": "In a difficult situation, it is very important to be able to <control> one's emotions and fear."
                            }
                        },
                        {
                            "text": "Армия полностью <контролирует> ситуацию в этом регионе страны.",
                            "target_lang_translations": {
                                "en": "The army fully <controls> the situation in this region of the country."
                            }
                        }
                    ],
                    "synonyms": [
                        "управлять",
                        "сдерживать",
                        "владеть"
                    ],
                    "common_phrases": [
                        "контролировать ситуацию",
                        "контролировать себя",
                        "контролировать эмоции",
                        "контролировать территорию"
                    ],
                    "target_lang_definitions": {
                        "en": "To have power or authority over someone or something; to restrain one's emotions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "control",
                                "target_lang_sense_clarification": "to exercise power, influence, or authority over; to hold in check"
                            },
                            {
                                "target_lang_word": "manage",
                                "target_lang_sense_clarification": "to be in charge of or handle a situation"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "42caabf7-cfe9-3f59-bb66-252a433757ca",
                    "sense_definition": "Iets wat hoog staat of hangt naar beneden halen of slopen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "construction_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente heeft besloten om de oude fabriek <neer> te <halen>.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет решил <w>снести</w> старую фабрику.",
                                "en": "The municipality has decided to <w>tear down</w> the old factory."
                            }
                        },
                        {
                            "text": "De hoge schoorsteen van de suikerfabriek is gisteren <neergehaald>.",
                            "target_lang_translations": {
                                "ru": "Высокую трубу сахарного завода вчера <w>снесли</w>.",
                                "en": "The sugar factory's tall chimney was <w>brought down</w> yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "slopen",
                        "afbreken",
                        "omverhalen",
                        "omlaaghalen"
                    ],
                    "antonyms": [
                        "opbouwen",
                        "oprichten"
                    ],
                    "common_phrases": [
                        "een gebouw neerhalen",
                        "een vlag neerhalen",
                        "een standbeeld neerhalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спускать вниз или разрушать что-то, что стоит или висит высоко.",
                        "en": "To bring down or demolish something that stands or hangs high."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сносить",
                                "target_lang_sense_clarification": "о здании или сооружении"
                            },
                            {
                                "target_lang_word": "спускать",
                                "target_lang_sense_clarification": "о флаге или парусе"
                            },
                            {
                                "target_lang_word": "валить",
                                "target_lang_sense_clarification": "о статуе, дереве или заборе"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "tear down",
                                "target_lang_sense_clarification": "regarding a building or structure"
                            },
                            {
                                "target_lang_word": "bring down",
                                "target_lang_sense_clarification": "regarding a structure, statue, or flag"
                            },
                            {
                                "target_lang_word": "demolish",
                                "target_lang_sense_clarification": "regarding a building"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ce29d36-645b-33ea-ba43-898a94b27bbf",
                    "sense_definition": "Een vliegend voorwerp, zoals een vliegtuig, met wapens uit de lucht schieten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_aviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het leger wist de vijandelijke drone succesvol <neer> te <halen>.",
                            "target_lang_translations": {
                                "ru": "Армии удалось успешно <w>сбить</w> вражеский дрон.",
                                "en": "The army managed to successfully <w>shoot down</w> the enemy drone."
                            }
                        },
                        {
                            "text": "Men dacht dat het toestel was <neergehaald> door een raket.",
                            "target_lang_translations": {
                                "ru": "Считалось, что самолет был <w>сбит</w> ракетой.",
                                "en": "It was thought that the aircraft was <w>shot down</w> by a missile."
                            }
                        }
                    ],
                    "synonyms": [
                        "neerschieten"
                    ],
                    "common_phrases": [
                        "een vliegtuig neerhalen",
                        "een helikopter neerhalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбивать летящий объект, например самолет, с помощью оружия.",
                        "en": "To shoot a flying object, such as an airplane, out of the sky using weapons."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбивать",
                                "target_lang_sense_clarification": "о самолете, вертолете или дроне"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "shoot down",
                                "target_lang_sense_clarification": "regarding an aircraft or drone"
                            },
                            {
                                "target_lang_word": "bring down",
                                "target_lang_sense_clarification": "regarding an aircraft"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3886b651-1bf5-3068-b594-ffb39412812d",
                    "sense_definition": "De moed of durf om iets te doen wat anderen misschien eng of moeilijk vinden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "courage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet wel <w>lef</w> hebben om voor zo'n groot publiek te spreken.",
                            "target_lang_translations": {
                                "en": "You really need <w>guts</w> to speak in front of such a large audience.",
                                "ru": "Нужно иметь <w>смелость</w>, чтобы выступать перед такой большой аудиторией."
                            }
                        },
                        {
                            "text": "Hij toonde veel <w>lef</w> door de waarheid te vertellen.",
                            "target_lang_translations": {
                                "en": "He showed a lot of <w>courage</w> by telling the truth.",
                                "ru": "Он проявил большую <w>смелость</w>, сказав правду."
                            }
                        }
                    ],
                    "synonyms": [
                        "slap"
                    ],
                    "antonyms": [
                        "lafheid",
                        "angst"
                    ],
                    "common_phrases": [
                        "lef hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "The courage or daring to do something that others might find scary or difficult.",
                        "ru": "Смелость или отвага сделать что-то, что другие могут счесть страшным или трудным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "guts",
                                "target_lang_sense_clarification": "informal, courage/determination"
                            },
                            {
                                "target_lang_word": "courage",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "nerve",
                                "target_lang_sense_clarification": "steadiness/daring"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "смелость",
                                "target_lang_sense_clarification": "общее понятие"
                            },
                            {
                                "target_lang_word": "отвага",
                                "target_lang_sense_clarification": "храбрость"
                            },
                            {
                                "target_lang_word": "дух",
                                "target_lang_sense_clarification": "разг., присутствие духа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a48d2931-c961-3bcd-93d0-aca25e49ac56",
                    "sense_definition": "Brutale durf of onbeschaamdheid; het durven doen van iets dat eigenlijk ongepast is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "audacity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan niet geloven dat hij het <w>lef</w> had om mij de schuld te geven.",
                            "target_lang_translations": {
                                "en": "I can't believe he had the <w>nerve</w> to blame me.",
                                "ru": "Я не могу поверить, что у него хватило <w>наглости</w> обвинить меня."
                            }
                        },
                        {
                            "text": "Wat een <w>lef</w> om zomaar voor te dringen in de rij!",
                            "target_lang_translations": {
                                "en": "What <w>audacity</w> to just cut in line!",
                                "ru": "Какая <w>наглость</w> — просто так влезть без очереди!"
                            }
                        }
                    ],
                    "synonyms": [
                        "slap"
                    ],
                    "antonyms": [
                        "bescheidenheid"
                    ],
                    "common_phrases": [
                        "het gore lef"
                    ],
                    "target_lang_definitions": {
                        "en": "Cheeky daring or shamelessness; the audacity to do something that is actually inappropriate.",
                        "ru": "Наглая смелость или бесстыдство; дерзость сделать что-то неуместное."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nerve",
                                "target_lang_sense_clarification": "impudence/cheek"
                            },
                            {
                                "target_lang_word": "audacity",
                                "target_lang_sense_clarification": "bold impudence"
                            },
                            {
                                "target_lang_word": "gall",
                                "target_lang_sense_clarification": "shameless boldness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наглость",
                                "target_lang_sense_clarification": "бесстыдство"
                            },
                            {
                                "target_lang_word": "дерзость",
                                "target_lang_sense_clarification": "вызывающее поведение"
                            }
                        ]
                    }
                }
            ]
        },
        {

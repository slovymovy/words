    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8b815d33-9f58-3e93-b8da-607367d054b2",
                    "sense_definition": "To have enough courage or boldness to do something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "courage_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I wouldn't <w>dare</w> argue with my boss about the schedule."
                        },
                        {
                            "text": "No one <w>dares</w> to criticize his decisions during the meeting."
                        },
                        {
                            "text": "How <w>dare</w> you speak to your mother like that?"
                        }
                    ],
                    "synonyms": [
                        "venture",
                        "risk"
                    ],
                    "antonyms": [
                        "cower",
                        "fear"
                    ],
                    "common_phrases": [
                        "how dare you",
                        "don't you dare",
                        "dare not"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often used as a semi-modal verb, especially in negative or interrogative sentences"
                        }
                    ]
                },
                {
                    "sense_id": "890fdbd0-9fec-30d3-84a9-3d6b5a5f5aa3",
                    "sense_definition": "To challenge or provoke someone to do something, often to prove their courage.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "challenge_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>dare</w> you to jump into the cold water."
                        },
                        {
                            "text": "She <w>dared</w> him to ask for a promotion."
                        }
                    ],
                    "synonyms": [
                        "challenge",
                        "defy",
                        "provoke"
                    ],
                    "common_phrases": [
                        "I dare you"
                    ]
                },
                {
                    "sense_id": "f891a3a6-2d28-361e-849b-727498eddbcb",
                    "sense_definition": "To face or endure something dangerous or difficult with courage.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "courage_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The explorers decided to <w>dare</w> the harsh conditions of the Arctic."
                        },
                        {
                            "text": "He was willing to <w>dare</w> death to save his friend."
                        }
                    ],
                    "synonyms": [
                        "brave",
                        "confront",
                        "face"
                    ],
                    "antonyms": [
                        "avoid",
                        "flee"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary or rhetorical style"
                        }
                    ]
                }
            ]
        },

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c0662bab-7452-373c-b45a-c8b38392e609",
                    "sense_definition": "W sposób typowy, tradycyjny, zgodny z przyjętym powszechnie wzorcem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "standard_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na spotkanie biznesowe ubrał się <w>klasycznie</w>, w garnitur i krawat.",
                            "target_lang_translations": {
                                "ru": "На деловую встречу он оделся <w>классически</w>, в костюм и галстук."
                            }
                        },
                        {
                            "text": "Ten dom został urządzony <w>klasycznie</w>, bez nowoczesnych udziwnień.",
                            "target_lang_translations": {
                                "ru": "Этот дом был оформлен <w>классически</w>, без современных причуд."
                            }
                        }
                    ],
                    "synonyms": [
                        "typowo",
                        "tradycyjnie",
                        "standardowo"
                    ],
                    "antonyms": [
                        "awangardowo",
                        "awangardowo",
                        "awangardowo"
                    ],
                    "common_phrases": [
                        "klasycznie ubrany",
                        "klasycznie piękna",
                        "wyglądać klasycznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Типично, традиционно, в соответствии с общепринятым стандартом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "классически",
                                "target_lang_sense_clarification": "о стиле или манере"
                            },
                            {
                                "target_lang_word": "традиционно",
                                "target_lang_sense_clarification": "об обычаях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62f4108b-0139-356b-ae98-a1574c96ba1e",
                    "sense_definition": "W sposób przewidywalny i powtarzalny, jak zwykle (często używane ironicznie w odniesieniu do negatywnych zdarzeń).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "predictable_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat <w>klasycznie</w> zapomniał o moich urodzinach.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>как обычно</w> забыл о моем дне рождения."
                            }
                        },
                        {
                            "text": "Pociąg <w>klasycznie</w> spóźnił się o dwadzieścia minut.",
                            "target_lang_translations": {
                                "ru": "Поезд <w>как обычно</w> опоздал на двадцать минут."
                            }
                        }
                    ],
                    "synonyms": [
                        "jak zwykle",
                        "typowo"
                    ],
                    "common_phrases": [
                        "klasycznie spóźniony",
                        "klasycznie zapomniał"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często ironicznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предсказуемо, как всегда (часто используется иронично в отношении негативных событий)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как обычно",
                                "target_lang_sense_clarification": "предсказуемость"
                            },
                            {
                                "target_lang_word": "типично",
                                "target_lang_sense_clarification": "характерное поведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68da0d16-baa9-3ce9-a8b7-d145f23ec274",
                    "sense_definition": "W sposób zgodny z antycznymi kanonami piękna lub stylem klasycznym w sztuce i muzyce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "art_and_history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej rysy twarzy są <w>klasycznie</w> regularne, przypominają grecką rzeźbę.",
                            "target_lang_translations": {
                                "ru": "Черты ее лица <w>классически</w> правильные, напоминают греческую скульптуру."
                            }
                        },
                        {
                            "text": "Artysta malował <w>klasycznie</w>, unikając eksperymentów z formą.",
                            "target_lang_translations": {
                                "ru": "Художник писал <w>классически</w>, избегая экспериментов с формой."
                            }
                        }
                    ],
                    "synonyms": [
                        "antycznie"
                    ],
                    "antonyms": [
                        "awangardowo"
                    ],
                    "common_phrases": [
                        "klasycznie wykształcony",
                        "klasycznie rzeźbiony"
                    ],
                    "target_lang_definitions": {
                        "ru": "В соответствии с античными канонами красоты или классическим стилем в искусстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "классически",
                                "target_lang_sense_clarification": "искусство и эстетика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "23b38ced-525a-3d64-9140-bc92a3e14031",
                    "sense_definition": "Enkelvoud verleden tijd van <w>beschuldigen</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar <w>beschuldigde</w> de leerling van spieken tijdens de toets.",
                            "target_lang_translations": {
                                "en": "The teacher <w>accused</w> the student of cheating during the test.",
                                "ru": "Учитель <w>обвинил</w> ученика в списывании во время контрольной."
                            }
                        },
                        {
                            "text": "Hij <w>beschuldigde</w> zijn buurman van het maken van te veel lawaai.",
                            "target_lang_translations": {
                                "en": "He <w>accused</w> his neighbor of making too much noise.",
                                "ru": "Он <w>обвинил</w> своего соседа в том, что тот слишком шумит."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Singular past tense of <w>beschuldigen</w> (to accuse).",
                        "ru": "Прошедшее время единственного числа от глагола <w>beschuldigen</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accused",
                                "target_lang_sense_clarification": "past tense"
                            },
                            {
                                "target_lang_word": "blamed",
                                "target_lang_sense_clarification": "assigned responsibility"
                            }
                        ],
                        "ru": [
                            {

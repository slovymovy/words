                {
                    "sense_id": "63614d86-b8f8-301d-b777-e403b56280c0",
                    "sense_definition": "Oznaczone miejsce, w którym zatrzymują się autobusy, tramwaje lub inne środki komunikacji miejskiej, aby pasażerowie mogli wsiąść lub wysiąść.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_places",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano czekam na ciebie na <w>przystanku</w> autobusowym koło szkoły."
                        },
                        {
                            "text": "Musisz wysiąść na trzecim <w>przystanku</w> i skręcić w prawo w stronę parku."
                        }
                    ],
                    "common_phrases": [
                        "przystanek autobusowy",
                        "przystanek tramwajowy",
                        "przystanek na żądanie",
                        "następny przystanek"
                    ]
                },
                {
                    "sense_id": "89406ffa-c8b0-3214-ab8f-6c00c4d64852",
                    "sense_definition": "Przerwa w podróży lub wędrówce przeznaczona na krótki odpoczynek.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "travel_actions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po trzech godzinach jazdy zrobiliśmy krótki <w>przystanek</w> na kawę i tankowanie."
                        },
                        {
                            "text": "To idealne miejsce na <w>przystanek</w> i zjedzenie drugiego śniadania w plenerze."
                        }
                    ],
                    "synonyms": [
                        "postój",
                        "odpoczynek"
                    ],
                    "common_phrases": [

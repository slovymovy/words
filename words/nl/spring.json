        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c320f16-a16f-3b6e-8efc-0afcb82a79c0",
                    "sense_definition": "Een getij waarbij het verschil tussen hoog- en laagwater het grootst is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens <w>spring</w> kan het water in de haven erg hoog komen.",
                            "target_lang_translations": {
                                "ru": "Во время <w>сизигийного прилива</w> вода в гавани может подниматься очень высоко."
                            }
                        },
                        {
                            "text": "De dijkbewaking is extra alert omdat het vandaag <w>spring</w> is.",
                            "target_lang_translations": {
                                "ru": "Охрана дамб особенно бдительна, потому что сегодня <w>сизигийный прилив</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "springtij",
                        "springvloed"
                    ],
                    "antonyms": [
                        "doodtij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прилив, при котором разница между полной и малой водой наибольшая (сизигийный прилив)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сизигийный прилив",
                                "target_lang_sense_clarification": "астрономическое/географическое понятие"
                            },
                            {
                                "target_lang_word": "спринг",
                                "target_lang_sense_clarification": "профессиональный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "160e3399-5778-3870-8472-6154308cf23d",
                    "sense_definition": "Een tros die schuin vanaf een schip naar de wal loopt om verplaatsing in de lengterichting te voorkomen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De matroos maakte de <w>spring</w> vast aan de bolder op de kade.",
                            "target_lang_translations": {
                                "ru": "Матрос закрепил <w>шпринг</w> на кнехте на причале."
                            }
                        },
                        {
                            "text": "Gebruik een <w>spring</w> om te voorkomen dat de boot wegdrijft.",
                            "target_lang_translations": {
                                "ru": "Используйте <w>шпринг</w>, чтобы лодку не снесло."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Швартов, идущий от носа или кормы судна под углом к причалу для предотвращения продольного смещения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпринг",
                                "target_lang_sense_clarification": "морской термин (трос)"

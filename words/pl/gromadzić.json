            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1e7fd9f8-25a6-34ab-976b-1f87594f2602",
                    "sense_definition": "Składać, zbierać coś w jednym miejscu lub w większej ilości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transitive_accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brudną bieliznę <w>gromadzimy</w> w osobnym pojemniku.",
                            "target_lang_translations": {
                                "ru": "Грязное белье мы <w>собираем</w> в отдельном контейнере."
                            }
                        },
                        {
                            "text": "Wiewiórka pracowicie <w>gromadzi</w> orzechy na nadchodzącą zimę.",
                            "target_lang_translations": {
                                "ru": "Белка трудолюбиво <w>запасает</w> орехи на предстоящую зиму."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbierać",
                        "składać",
                        "akumulować",
                        "zgarniać"
                    ],
                    "antonyms": [
                        "rozrzucać"
                    ],
                    "common_phrases": [
                        "gromadzić zapasy",
                        "gromadzić pieniądze",
                        "gromadzić dowody",
                        "gromadzić wiedzę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Складывать или собирать что-либо в одном месте или в большом количестве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накапливать",
                                "target_lang_sense_clarification": "постепенно увеличивать количество чего-либо"
                            },
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "сводить воедино, складывать"
                            },
                            {
                                "target_lang_word": "копить",
                                "target_lang_sense_clarification": "сберегать, запасать (обычно о деньгах или ресурсах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4d82640-9a65-3ca7-a5a9-6bf71e05ad47",
                    "sense_definition": "Pozyskiwać i przechowywać przedmioty określonego rodzaju jako hobby.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transitive_accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jan od wielu lat <w>gromadzi</w> znaczki pocztowe.",
                            "target_lang_translations": {
                                "ru": "Ян уже много лет <w>коллекционирует</w> почтовые марки."
                            }
                        },
                        {
                            "text": "Mój dziadek z wielką pasją <w>gromadził</w> stare monety.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка с большой страстью <w>собирал</w> старинные монеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolekcjonować",
                        "zbierać"
                    ],
                    "common_phrases": [
                        "gromadzić znaczki",
                        "gromadzić kolekcję",
                        "gromadzić dzieła sztuki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искать и хранить предметы определенного типа в качестве хобби."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коллекционировать",
                                "target_lang_sense_clarification": "систематически собирать какие-либо предметы"
                            },
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "составлять коллекцию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cfc8ae3-260c-3044-84fd-cae297e0e45d",
                    "sense_definition": "O ludziach lub zwierzętach: zbierać się w grupę, tłoczyć się w jednym miejscu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu ludzi <w>gromadziło się</w> wokół miejsca wypadku.",
                            "target_lang_translations": {
                                "ru": "Много людей <w>собиралось</w> вокруг места происшествия."
                            }
                        },
                        {
                            "text": "Przed wejściem do teatru <w>gromadzili się</w> niecierpliwi widzowie.",
                            "target_lang_translations": {
                                "ru": "Перед входом в театр <w>толпились</w> нетерпеливые зрители."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbierać",
                        "skupiać"
                    ],
                    "antonyms": [
                        "rozchodzić się",
                        "rozpraszać się"
                    ],
                    "common_phrases": [
                        "tłum się gromadzi",
                        "ludzie się gromadzą",
                        "wierni się gromadzą"
                    ],
                    "target_lang_definitions": {
                        "ru": "О людях или животных: сходиться вместе, образовывать группу в одном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "сходиться в одном месте"
                            },
                            {
                                "target_lang_word": "толпиться",
                                "target_lang_sense_clarification": "собираться в тесную группу, тесниться"
                            },
                            {
                                "target_lang_word": "скапливаться",
                                "target_lang_sense_clarification": "сосредотачиваться в большом количестве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4a1a44a-59bd-3fb1-b1b1-15378af14b1d",
                    "sense_definition": "O rzeczach: zbierać się, nagromadzać się w dużej ilości w jednym miejscu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stosy śmieci <w>gromadziły się</w> obok naszego bloku.",
                            "target_lang_translations": {
                                "ru": "Горы мусора <w>скапливались</w> возле нашего дома."
                            }
                        },
                        {
                            "text": "Ciemne chmury <w>gromadzą się</w> na horyzoncie przed burzą.",
                            "target_lang_translations": {
                                "ru": "Темные тучи <w>собираются</w> на горизонте перед грозой."
                            }
                        }
                    ],
                    "synonyms": [
                        "nawarstwiać się",
                        "spiętrzać się",
                        "odkładać się",
                        "akumulować się"
                    ],
                    "common_phrases": [
                        "chmury się gromadzą",
                        "woda się gromadzi",
                        "kurz się gromadzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "О предметах или явлениях: появляться или нарастать в большом количестве в одном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скапливаться",
                                "target_lang_sense_clarification": "собираться в кучу или массу"
                            },
                            {
                                "target_lang_word": "накапливаться",
                                "target_lang_sense_clarification": "постепенно возрастать в количестве"
                            },
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "о тучах, грозе или жидкости"
                            }
                        ]
                    }
                }
            ]
        }

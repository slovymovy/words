                {
                    "sense_id": "e7af4323-4d57-3b77-85b9-be39366c8dbb",
                    "sense_definition": "Een jonge, ongehuwde vrouw of een aanspreekvorm voor een vrouw.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "woman",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Juffrouw</w> De Wit werkt al veertig jaar in de bibliotheek.",
                            "target_lang_translations": {

                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He selected a seven <w>iron</w> for his approach to the green.",
                            "target_lang_translations": {
                                "ru": "Он выбрал седьмой <w>айрон</w> для удара на грин."
                            }
                        },
                        {
                            "text": "The player used an <w>iron</w> to get the ball out of the rough.",
                            "target_lang_translations": {
                                "ru": "Игрок использовал <w>айрон</w>, чтобы выбить мяч из рафа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nine iron",
                        "long iron"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип клюшки для гольфа с металлической головкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "айрон",
                                "target_lang_sense_clarification": "клюшка для гольфа"

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "700fdc52-c576-35a5-be2c-71afe3a49d8a",
                    "sense_definition": "Relating to or operating via mobile phones and wireless technology.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "telephony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to buy a new <w>mobile</w> phone soon.",
                            "target_lang_translations": {
                                "ru": "Мне нужно скоро купить новый <w>мобильный</w> телефон."
                            }
                        },
                        {
                            "text": "Most people use <w>mobile</w> apps for banking these days.",
                            "target_lang_translations": {
                                "ru": "В наши дни большинство людей используют <w>мобильные</w> приложения для банковских операций."
                            }
                        }
                    ],
                    "synonyms": [
                        "cellular",
                        "wireless"
                    ],
                    "common_phrases": [
                        "mobile phone",
                        "mobile app",
                        "mobile device",
                        "mobile network",
                        "mobile data"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к мобильным телефонам, сотовой связи и беспроводным технологиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мобильный",
                                "target_lang_sense_clarification": "относящийся к сотовой связи"
                            },
                            {
                                "target_lang_word": "сотовый",
                                "target_lang_sense_clarification": "о телефоне или сети"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a3ab133-adb1-3c5d-8215-734bf5033b12",
                    "sense_definition": "Able to move or be moved freely or easily from place to place.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The library has a <w>mobile</w> unit that visits rural villages.",
                            "target_lang_translations": {
                                "ru": "У библиотеки есть <w>передвижное</w> отделение, которое посещает сельские деревни."
                            }
                        },
                        {
                            "text": "They decided to buy a <w>mobile</w> home to travel around the country.",
                            "target_lang_translations": {
                                "ru": "Они решили купить <w>мобильный</w> дом, чтобы путешествовать по стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "movable",
                        "portable",
                        "transportable",
                        "moving"
                    ],
                    "antonyms": [
                        "fixed",
                        "immobile",
                        "stationary",
                        "immovable"
                    ],
                    "common_phrases": [
                        "mobile home",
                        "mobile library",
                        "mobile hospital",
                        "upwardly mobile"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный свободно двигаться или перемещаться с места на место; не стационарный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передвижной",
                                "target_lang_sense_clarification": "о библиотеках, госпиталях, домах"
                            },
                            {
                                "target_lang_word": "мобильный",
                                "target_lang_sense_clarification": "способный к быстрому перемещению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1066c0eb-132c-38f7-a483-7f17bf03607a",
                    "sense_definition": "Changing quickly in appearance or expression, usually describing facial features.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>mobile</w> features revealed his surprise before he could speak.",
                            "target_lang_translations": {
                                "ru": "Его <w>подвижные</w> черты лица выдали удивление, прежде чем он успел заговорить."
                            }
                        },
                        {
                            "text": "She has a very <w>mobile</w> face that displays every emotion she feels.",
                            "target_lang_translations": {
                                "ru": "У нее очень <w>подвижное</w> лицо, на котором отражается каждая эмоция."
                            }
                        }
                    ],
                    "synonyms": [
                        "expressive",
                        "changeable"
                    ],
                    "common_phrases": [
                        "mobile face",
                        "mobile features"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро меняющийся в выражении (обычно о чертах лица или мимике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвижный",
                                "target_lang_sense_clarification": "о лице или мимике"

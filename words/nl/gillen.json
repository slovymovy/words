    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2ac7e9c1-9a06-38bf-919f-d46992e69adc",
                    "sense_definition": "Een harde, schelle, ongearticuleerde kreet slaken, vaak uit angst, pijn of plezier.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen <w>gilden</w> van plezier in de snelle achtbaan.",
                            "target_lang_translations": {
                                "en": "The children <w>screamed</w> with pleasure on the fast roller coaster.",
                                "ru": "Дети <w>визжали</w> от восторга на быстрых американских горках."
                            }
                        },
                        {
                            "text": "Hij <w>gilde</w> het uit van de pijn toen hij viel.",
                            "target_lang_translations": {
                                "en": "He <w>screamed</w> out in pain when he fell.",
                                "ru": "Он <w>завопил</w> от боли, когда упал."
                            }
                        },
                        {
                            "text": "We moesten allemaal <w>gillen</w> van het lachen om die grap.",
                            "target_lang_translations": {
                                "en": "We all had to <w>scream</w> with laughter at that joke.",
                                "ru": "Мы все <w>визжали</w> от смеха над этой шуткой."
                            }
                        }
                    ],
                    "synonyms": [
                        "schreeuwen",
                        "schreeuwen"
                    ],
                    "antonyms": [
                        "fluisteren"
                    ],
                    "common_phrases": [
                        "gillen van het lachen",
                        "gillen van de pijn",
                        "gillen van de pret"
                    ],
                    "target_lang_definitions": {
                        "en": "To let out a loud, shrill cry, often out of fear, pain, or pleasure.",
                        "ru": "Издавать громкий, пронзительный, нечленораздельный крик, часто от страха, боли или удовольствия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scream",
                                "target_lang_sense_clarification": "loud, high-pitched cry"
                            },
                            {
                                "target_lang_word": "shriek",
                                "target_lang_sense_clarification": "sharp, piercing cry"
                            },
                            {
                                "target_lang_word": "yell",
                                "target_lang_sense_clarification": "loud shout"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "визжать",
                                "target_lang_sense_clarification": "издавать пронзительный крик"
                            },
                            {
                                "target_lang_word": "вопить",
                                "target_lang_sense_clarification": "громко и протяжно кричать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2572e4af-508d-3678-bedf-4054898b2f23",
                    "sense_definition": "Iets in een schuine richting afzagen of afsnijden.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De timmerman moest de plank <w>gillen</w> om hem passend te maken.",
                            "target_lang_translations": {
                                "en": "The carpenter had to <w>miter</w> the plank to make it fit.",
                                "ru": "Плотник должен был <w>скосить</w> доску, чтобы подогнать её."
                            }
                        },
                        {
                            "text": "Voor deze constructie moet je het hout nauwkeurig <w>gillen</w>.",
                            "target_lang_translations": {
                                "en": "For this construction, you have to <w>bevel</w> the wood precisely.",
                                "ru": "Для этой конструкции нужно точно <w>скосить</w> дерево."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical or dialectal usage in carpentry"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To saw or cut something at an angle or in a slanted direction.",
                        "ru": "Отпиливать или отрезать что-либо под косым углом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "miter",
                                "target_lang_sense_clarification": "cut at an angle"
                            },
                            {
                                "target_lang_word": "bevel",
                                "target_lang_sense_clarification": "cut a sloping edge"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "скашивать",
                                "target_lang_sense_clarification": "делать косой срез"
                            }
                        ]
                    }
                }
            ]
        },

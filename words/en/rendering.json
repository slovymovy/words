{
    "word_family": [
        "render",
        "rendition",
        "renderer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a8f6ca52-6387-3f6c-a168-b41002bebee6",
                    "sense_definition": "The process of generating a photorealistic or non-photorealistic image from a 2D or 3D model by means of a computer program, or the image produced this way.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>rendering</w> of the complex 3D scene took several hours to complete.",
                            "target_lang_translations": {
                                "ru": "<w>Рендеринг</w> сложной 3D-сцены занял несколько часов."
                            }
                        },
                        {
                            "text": "Modern video games rely on real-time <w>rendering</w> to create immersive environments.",
                            "target_lang_translations": {
                                "ru": "Современные видеоигры полагаются на <w>рендеринг</w> в реальном времени для создания эффекта погружения."
                            }
                        }
                    ],
                    "synonyms": [
                        "image generation"
                    ],
                    "common_phrases": [
                        "3D rendering",
                        "real-time rendering",
                        "video rendering"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс создания фотореалистичного или нефотореалистичного изображения по 2D или 3D модели с помощью компьютерной программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рендеринг",
                                "target_lang_sense_clarification": "компьютерная графика"
                            },
                            {
                                "target_lang_word": "визуализация",
                                "target_lang_sense_clarification": "процесс создания изображения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "762621e0-f469-34cb-9fbb-310aa4cc3d01",
                    "sense_definition": "A detailed drawing, illustration, or perspective view, especially one used in architecture or design to show what a project will look like.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The architect presented a beautiful <w>rendering</w> of the proposed library to the committee.",
                            "target_lang_translations": {
                                "ru": "Архитектор представил комитету красивую <w>визуализацию</w> предлагаемой библиотеки."
                            }
                        },
                        {
                            "text": "This artistic <w>rendering</w> shows how the park will look after the renovations.",
                            "target_lang_translations": {
                                "ru": "Это художественное <w>изображение</w> показывает, как парк будет выглядеть после реконструкции."
                            }
                        }
                    ],
                    "synonyms": [
                        "sketch",
                        "illustration",
                        "depiction",
                        "drawing"
                    ],
                    "common_phrases": [
                        "architectural rendering",
                        "artist's rendering",
                        "detailed rendering"
                    ],
                    "target_lang_definitions": {
                        "ru": "Детальный рисунок, иллюстрация или перспективный вид, особенно используемый в архитектуре или дизайне для демонстрации будущего проекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "визуализация",
                                "target_lang_sense_clarification": "архитектурная"
                            },
                            {
                                "target_lang_word": "эскиз",
                                "target_lang_sense_clarification": "набросок"
                            },
                            {
                                "target_lang_word": "изображение",
                                "target_lang_sense_clarification": "иллюстрация проекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ccefd9b-bd58-300b-ad9c-4efc5ce21052",
                    "sense_definition": "A specific interpretation, performance, or translation of a piece of music, a dramatic role, or a text.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actor gave a moving <w>rendering</w> of the famous soliloquy.",
                            "target_lang_translations": {
                                "ru": "Актер представил трогательное <w>исполнение</w> знаменитого монолога."
                            }
                        },
                        {
                            "text": "I prefer this faithful <w>rendering</w> of the ancient poem over the modern adaptation.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю этот точный <w>перевод</w> древней поэмы современной адаптации."
                            }
                        }
                    ],
                    "synonyms": [
                        "interpretation",
                        "version",
                        "translation",
                        "performance",
                        "portrayal"
                    ],
                    "common_phrases": [
                        "faithful rendering",
                        "musical rendering",
                        "poetic rendering"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особая интерпретация, исполнение или перевод музыкального произведения, роли или текста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исполнение",
                                "target_lang_sense_clarification": "роли или музыки"
                            },
                            {
                                "target_lang_word": "интерпретация",
                                "target_lang_sense_clarification": "трактовка произведения"
                            },
                            {
                                "target_lang_word": "перевод",
                                "target_lang_sense_clarification": "художественный перевод текста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b61d9532-2dc1-3b78-a72b-6bd0f3147d40",
                    "sense_definition": "The formal act of giving, presenting, or making something available, such as services or assistance.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company billed us for the <w>rendering</w> of professional services.",
                            "target_lang_translations": {
                                "ru": "Компания выставила нам счет за <w>оказание</w> профессиональных услуг."
                            }
                        },
                        {
                            "text": "Immediate <w>rendering</w> of first aid is crucial in an emergency situation.",
                            "target_lang_translations": {
                                "ru": "Немедленное <w>оказание</w> первой помощи имеет решающее значение в чрезвычайной ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "provision",
                        "supplying"
                    ],
                    "common_phrases": [
                        "rendering of services",
                        "rendering of assistance",
                        "rendering of accounts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный акт предоставления или обеспечения доступности чего-либо, например услуг или помощи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оказание",
                                "target_lang_sense_clarification": "услуг или помощи"
                            },
                            {
                                "target_lang_word": "предоставление",
                                "target_lang_sense_clarification": "услуг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8fa81c78-7e67-37c2-aa4c-7ed633226fbd",
                    "sense_definition": "Present participle and gerund of <w>render</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software is currently <w>rendering</w> the final video file.",
                            "target_lang_translations": {
                                "ru": "Программа в данный момент <w>визуализирует</w> финальный видеофайл."
                            }

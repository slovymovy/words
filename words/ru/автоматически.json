    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "506d0556-10cf-37c2-b8b1-0da522b34f24",
                    "sense_definition": "С помощью технических устройств, механизмов или программ, без непосредственного участия человека.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tech_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Двери супермаркета открываются <w>автоматически</w>, когда к ним подходишь.",
                            "target_lang_translations": {
                                "en": "The supermarket doors open <w>automatically</w> when you approach them."
                            }
                        },
                        {
                            "text": "Эта программа <w>автоматически</w> проверяет текст на наличие ошибок.",
                            "target_lang_translations": {
                                "en": "This program <w>automatically</w> checks the text for errors."
                            }
                        }
                    ],
                    "synonyms": [
                        "автоматично"
                    ],
                    "antonyms": [
                        "вручную"
                    ],
                    "common_phrases": [
                        "включаться автоматически",
                        "обновляться автоматически",
                        "работать автоматически"
                    ],
                    "target_lang_definitions": {
                        "en": "Done by a machine, device, or software without direct human intervention."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "automatically",
                                "target_lang_sense_clarification": "by a machine or system"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21c9cd50-8c8c-3972-92e5-7f18872c4ac3",
                    "sense_definition": "Непроизвольно, без участия сознания, в силу привычки; машинально.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "human_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Услышав громкий звук, он <w>автоматически</w> обернулся назад.",
                            "target_lang_translations": {
                                "en": "Hearing a loud sound, he turned back <w>automatically</w>."
                            }
                        },
                        {
                            "text": "Я <w>автоматически</w> потянулся к выключателю, хотя света не было.",
                            "target_lang_translations": {
                                "en": "I reached for the switch <w>automatically</w>, even though the power was out."
                            }
                        }
                    ],
                    "synonyms": [
                        "машинально",
                        "непроизвольно",
                        "бессознательно",
                        "механически",
                        "невольно",
                        "безотчётно"
                    ],
                    "antonyms": [
                        "сознательно",
                        "намеренно"
                    ],
                    "common_phrases": [
                        "сделать автоматически",
                        "ответить автоматически"
                    ],
                    "target_lang_definitions": {
                        "en": "Done without conscious thought, often out of habit or reflex."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "automatically",
                                "target_lang_sense_clarification": "without thinking"
                            },
                            {
                                "target_lang_word": "mechanically",
                                "target_lang_sense_clarification": "in a machine-like manner"
                            },
                            {
                                "target_lang_word": "instinctively",
                                "target_lang_sense_clarification": "by reflex"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d40d2c4-5185-3b37-8782-8035bfd4d9f4",
                    "sense_definition": "Без дополнительных процедур, заявлений или специальных действий; само собой, в силу установленного порядка.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "procedure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При покупке билета страховка оформляется <w>автоматически</w>.",
                            "target_lang_translations": {
                                "en": "When purchasing a ticket, insurance is issued <w>automatically</w>."
                            }
                        },
                        {
                            "text": "Если вы не отмените подписку, она продлится <w>автоматически</w>.",
                            "target_lang_translations": {
                                "en": "If you do not cancel the subscription, it will renew <w>automatically</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "автоматом"
                    ],
                    "common_phrases": [
                        "продлеваться автоматически",
                        "переходить автоматически",
                        "зачисляться автоматически"
                    ],
                    "target_lang_definitions": {
                        "en": "Occurring as a direct result of a rule or process without requiring additional steps."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "automatically",
                                "target_lang_sense_clarification": "as a necessary consequence"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
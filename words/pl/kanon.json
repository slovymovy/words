            "senses": [
                {
                    "sense_id": "b77a88c8-8539-3476-bc2a-3d7fe53203fb",
                    "sense_definition": "Zbiór dzieł literackich, artystycznych lub tekstów uznanych za najważniejsze, wzorcowe i podstawowe w danej dziedzinie kultury.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "collection_of_works",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powieść ta na stałe weszła do <w>kanonu</w> literatury polskiej.",
                            "target_lang_translations": {
                                "ru": "Этот роман навсегда вошел в <w>канон</w> польской литературы."
                            }
                        },
                        {
                            "text": "Nauczycielka podała uczniom nowy <w>kanon</w> lektur obowiązkowych.",
                            "target_lang_translations": {
                                "ru": "Учительница дала ученикам новый <w>список</w> обязательной литературы."
                            }
                        }
                    ],
                    "synonyms": [
                        "lista",
                        "zestaw",
                        "zestawienie",
                        "zbiór"
                    ],
                    "common_phrases": [
                        "kanon lektur",
                        "kanon literatury",
                        "wejść do kanonu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность литературных, художественных произведений или текстов, признанных образцовыми и наиболее важными в данной области культуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канон",
                                "target_lang_sense_clarification": "совокупность произведений"

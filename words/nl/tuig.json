                {
                    "sense_id": "cea95594-3531-39c1-9a53-b1852fe5a0ef",
                    "sense_definition": "Verzamelnaam voor slecht volk of mensen die zich misdragen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_scum",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik laat me door dat <w>tuig</w> niet in de wielen rijden.",
                            "target_lang_translations": {
                                "en": "I won't let that <w>scum</w> get in my way.",
                                "ru": "Я не позволю этому <w>сброду</w> вставлять мне палки в колёса."
                            }
                        },
                        {
                            "text": "De hele buurt heeft last van het rondhangende <w>tuig</w> op het plein.",
                            "target_lang_translations": {
                                "en": "The whole neighborhood is bothered by the loitering <w>riffraff</w> on the square.",
                                "ru": "Весь район страдает от слоняющегося по площади <w>хулиганья</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "schorem",
                        "geteisem"
                    ],
                    "common_phrases": [
                        "smerig tuig",
                        "links tuig"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Collective term for bad people or people who misbehave.",
                        "ru": "Собирательное название для плохих людей или тех, кто плохо себя ведет."

                {
                    "sense_id": "29d586a7-85cf-3422-8f5b-98fdecdd6ab1",
                    "sense_definition": "At no time; on no occasion; not ever.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have <w>never</w> been to Paris, but I hope to go someday."
                        },
                        {
                            "text": "She <w>never</w> drinks coffee because she prefers tea."
                        },
                        {
                            "text": "You should <w>never</w> forget to lock the door when you leave."
                        }
                    ],
                    "synonyms": [
                        "at no time",
                        "never ever"
                    ],
                    "antonyms": [
                        "always",
                        "ever"
                    ],
                    "common_phrases": [

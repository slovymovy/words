                        ]
                    }
                },
                {
                    "sense_id": "376548ea-c0d1-3831-b624-f08d13972404",
                    "sense_definition": "Wskazuje na relację, wybór lub różnicę dotyczącą dwóch stron, opcji lub stanów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czasem trzeba wybierać <między> pracą a rozrywką.",
                            "target_lang_translations": {
                                "ru": "Иногда приходится выбирать <w>между</w> работой и развлечениями."
                            }
                        },
                        {
                            "text": "To musi zostać tylko <między> nami.",
                            "target_lang_translations": {
                                "ru": "Это должно остаться только <w>между</w> нами."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomiędzy"
                    ],
                    "common_phrases": [
                        "czytać między wierszami",
                        "między młotem a kowadłem",
                        "między Bogiem a prawdą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на отношения, выбор или различие, касающиеся двух сторон, вариантов или состояний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "между",
                                "target_lang_sense_clarification": "отношения, выбор или сравнение"
                            }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "be9ea114-45da-3c9b-a320-92ac88447a95",
                    "sense_definition": "Mówić do kogoś obraźliwe, przykre słowa lub używać wulgaryzmów, aby sprawić tej osobie ból lub ją poniżyć.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinieneś <wyzywać> młodszego brata, bo to bardzo niegrzeczne.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <обзывать> младшего брата, потому что это очень невежливо."
                            }
                        },
                        {
                            "text": "Kiedy kierowca zajechał mu drogę, zaczął go głośno <wyzywać> przez otwarte okno.",
                            "target_lang_translations": {
                                "ru": "Когда водитель подрезал его, он начал громко <обзывать> его через открытое окно."
                            }
                        }
                    ],
                    "synonyms": [
                        "obrażać",
                        "ubliżać",
                        "przezywać",
                        "lżyć",
                        "znieważać"
                    ],
                    "antonyms": [
                        "chwalić",
                        "komplementować"
                    ],
                    "common_phrases": [
                        "wyzywać od najgorszych",
                        "wyzywać bez powodu",
                        "wyzywać się nawzajem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить кому-то оскорбительные слова или использовать ругательства, чтобы причинить боль или унизить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обзывать",
                                "target_lang_sense_clarification": "называть обидными словами"
                            },
                            {
                                "target_lang_word": "оскорблять",
                                "target_lang_sense_clarification": "унижать достоинство словами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efd590de-507e-39db-b322-401636ac35ff",
                    "sense_definition": "Proponować komuś udział w walce, pojedynku lub zawodach, aby sprawdzić, kto jest lepszy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz postanowił <wyzywać> każdego napotkanego wroga na pojedynek.",
                            "target_lang_translations": {
                                "ru": "Рыцарь решил <вызывать> каждого встречного врага на поединок."
                            }
                        },
                        {
                            "text": "Nie warto <wyzywać> losu, podejmując tak duże i niepotrzebne ryzyko.",
                            "target_lang_translations": {
                                "ru": "Не стоит <искушать> судьбу, идя на такой большой и ненужный риск."
                            }
                        }
                    ],
                    "synonyms": [
                        "prowokować",
                        "rzucać wyzwanie"
                    ],
                    "common_phrases": [
                        "wyzywać na pojedynek",
                        "wyzywać los",
                        "wyzywać na walkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлагать кому-то участие в борьбе, дуэли или соревновании, чтобы выяснить, кто лучше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "на поединок или состязание"
                            },
                            {
                                "target_lang_word": "бросать вызов",
                                "target_lang_sense_clarification": "предлагать борьбу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "128b27dc-4e15-3c41-9f9f-ded11528ab13",
                    "sense_definition": "Wołać kogoś po nazwisku, aby wystąpił lub wyszedł z ukrycia; często używane w kontekście szkolnym lub magicznym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka matematyki zaczęła <wyzywać> uczniów do tablicy według listy obecności.",
                            "target_lang_translations": {
                                "ru": "Учительница математики начала <вызывать> учеников к доске по списку."
                            }
                        },
                        {
                            "text": "Podczas seansu spirytystycznego medium próbowało <wyzywać> duchy przodków.",
                            "target_lang_translations": {
                                "ru": "Во время спиритического сеанса медиум пытался <вызывать> духов предков."
                            }
                        }
                    ],
                    "synonyms": [
                        "wywoływać",
                        "wzywać"
                    ],
                    "common_phrases": [
                        "wyzywać do tablicy",
                        "wyzywać duchy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громко звать кого-то по имени или фамилии, требуя выйти или явиться; часто используется в школе или магии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "к доске или призывать духов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

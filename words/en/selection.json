{
    "word_family": [
        "select",
        "selective",
        "selectively",
        "selector",
        "selectivity"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9216de8d-4439-3e12-be0b-f556823cf382",
                    "sense_definition": "A range of different items gathered together to choose from.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store offers a wide <w>selection</w> of organic vegetables.",
                            "target_lang_translations": {
                                "ru": "Магазин предлагает широкий <w>выбор</w> органических овощей."
                            }
                        },
                        {
                            "text": "I've brought a <w>selection</w> of fine cheeses to go with your wine.",
                            "target_lang_translations": {
                                "ru": "Я принес <w>подборку</w> изысканных сыров к твоему вину."
                            }
                        }
                    ],
                    "synonyms": [
                        "choice",
                        "subset",
                        "number",
                        "piece"
                    ],
                    "common_phrases": [
                        "wide selection",
                        "good selection",
                        "selection of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ряд различных предметов, собранных вместе для выбора"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбор",
                                "target_lang_sense_clarification": "наличие вариантов"
                            },
                            {
                                "target_lang_word": "ассортимент",
                                "target_lang_sense_clarification": "совокупность товаров"
                            },
                            {
                                "target_lang_word": "подборка",
                                "target_lang_sense_clarification": "специально подобранные предметы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4266d849-ef2c-315a-ad2b-bb73d083e599",
                    "sense_definition": "The process or act of carefully choosing someone or something from a group.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "act_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The large number of good candidates made <w>selection</w> difficult.",
                            "target_lang_translations": {
                                "ru": "Большое количество хороших кандидатов затруднило <w>выбор</w>."
                            }
                        },
                        {
                            "text": "The <w>selection</w> of a new leader took several weeks.",
                            "target_lang_translations": {
                                "ru": "<w>Выбор</w> нового лидера занял несколько недель."
                            }
                        }
                    ],
                    "synonyms": [
                        "choice"
                    ],
                    "common_phrases": [
                        "selection process",
                        "random selection",
                        "make a selection"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс или действие по тщательному выбору кого-либо или чего-либо из группы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбор",
                                "target_lang_sense_clarification": "принятие решения"
                            },
                            {
                                "target_lang_word": "отбор",
                                "target_lang_sense_clarification": "процесс отсеивания кандидатов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4d8db4b-ed84-304e-af21-962121987293",
                    "sense_definition": "The specific thing or person that has been chosen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "act_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My final <w>selection</w> was a vintage red wine.",
                            "target_lang_translations": {
                                "ru": "Моим окончательным <w>выбором</w> стало марочное красное вино."
                            }
                        },
                        {
                            "text": "The judge's <w>selection</w> for the award surprised the audience.",
                            "target_lang_translations": {
                                "ru": "<w>Выбор</w> судьи для награждения удивил аудиторию."
                            }
                        }
                    ],
                    "synonyms": [
                        "choice",
                        "option"
                    ],
                    "common_phrases": [
                        "final selection"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный предмет или человек, который был выбран"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбор",
                                "target_lang_sense_clarification": "то, что выбрано"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fb0778d-39bd-3c52-a9b9-68df96947b72",
                    "sense_definition": "The natural process by which organisms that are better adapted to their environment tend to survive and reproduce.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Darwin developed the theory of natural <w>selection</w> to explain evolution.",
                            "target_lang_translations": {
                                "ru": "Дарвин разработал теорию естественного <w>отбора</w> для объяснения эволюции."
                            }
                        },
                        {
                            "text": "Artificial <w>selection</w> is used to breed dogs with specific traits.",
                            "target_lang_translations": {
                                "ru": "Искусственный <w>отбор</w> используется для выведения собак с определенными признаками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "natural selection",
                        "artificial selection"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "biology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Биологический процесс выживания наиболее приспособленных организмов или выведение новых пород"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отбор",
                                "target_lang_sense_clarification": "естественный процесс"
                            },
                            {
                                "target_lang_word": "селекция",
                                "target_lang_sense_clarification": "искусственное выведение пород"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfe96a44-c997-3d79-8a0c-f04edaa3a845",
                    "sense_definition": "A part of a document, image, or list that has been highlighted or chosen for an operation on a computer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can copy the current <w>selection</w> by pressing these keys.",
                            "target_lang_translations": {
                                "ru": "Вы можете скопировать текущее <w>выделение</w>, нажав эти клавиши."
                            }
                        },
                        {
                            "text": "The <w>selection</w> turned blue when I clicked on it.",
                            "target_lang_translations": {
                                "ru": "<w>Выделенный фрагмент</w> стал синим, когда я нажал на него."
                            }
                        }
                    ],
                    "common_phrases": [
                        "current selection",
                        "clear selection"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть документа, изображения или списка, которая была выделена для выполнения операции на компьютере"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделение",
                                "target_lang_sense_clarification": "выделенная область"
                            },
                            {
                                "target_lang_word": "выделенный фрагмент",
                                "target_lang_sense_clarification": "часть текста или изображения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc133720-4b9e-36ce-b51f-0f962e8fc59c",
                    "sense_definition": "A specific piece of music chosen to be performed or played.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "For my next <w>selection</w>, I will play a classic jazz tune.",
                            "target_lang_translations": {
                                "ru": "В качестве следующей <w>композиции</w> я сыграю классическую джазовую мелодию."
                            }
                        },
                        {
                            "text": "The band played a <w>selection</w> from their latest album.",
                            "target_lang_translations": {
                                "ru": "Группа сыграла <w>подборку</w> из своего последнего альбома."
                            }
                        }
                    ],
                    "synonyms": [
                        "piece",
                        "number"
                    ],
                    "common_phrases": [
                        "musical selection"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное музыкальное произведение, выбранное для исполнения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "композиция",
                                "target_lang_sense_clarification": "музыкальное произведение"
                            },
                            {
                                "target_lang_word": "номер",
                                "target_lang_sense_clarification": "часть выступления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3d85d76-b411-319f-a4da-19f479d60537",
                    "sense_definition": "A plot of land chosen from government land for farming, historically in Australia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family worked hard to clear their <w>selection</w> for farming.",
                            "target_lang_translations": {
                                "ru": "Семья усердно трудилась, чтобы расчистить свой <w>участок</w> для земледелия."
                            }
                        },
                        {
                            "text": "Our <w>selection</w> adjoined a sheep-run on the Darling Downs.",
                            "target_lang_translations": {
                                "ru": "Наш <w>участок</w> граничил с овечьим пастбищем в Дарлинг-Даунс."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Australia"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Земельный участок, выбранный из государственных земель для ведения сельского хозяйства (исторически в Австралии)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "земля"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
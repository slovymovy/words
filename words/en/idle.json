                        ]
                    }
                },
                {
                    "sense_id": "e0596921-cdf8-35c9-bfe1-92e8a423f2f6",
                    "sense_definition": "The state of being idle or inactive.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ef5efc14-4624-317f-a209-29ebb3609f1d",
                    "sense_definition": "Zonder fouten en in overeenstemming met de feiten of regels.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "correctness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is het <w>juiste</w> antwoord op de moeilijke vraag.",
                            "target_lang_translations": {
                                "en": "That is the <w>correct</w> answer to the difficult question.",
                                "ru": "Это <w>правильный</w> ответ на сложный вопрос."
                            }
                        },
                        {
                            "text": "Het is niet <w>juist</w> om andermans spullen zomaar te pakken.",
                            "target_lang_translations": {
                                "en": "It is not <w>right</w> to just take other people's things.",
                                "ru": "Не <w>правильно</w> просто так брать чужие вещи."
                            }
                        }
                    ],
                    "synonyms": [
                        "correct"
                    ],
                    "antonyms": [
                        "onjuist",
                        "foutief"
                    ],
                    "common_phrases": [
                        "het juiste moment",
                        "de juiste keuze",
                        "niet juist"
                    ],
                    "target_lang_definitions": {
                        "en": "Without errors and in accordance with facts or rules.",
                        "ru": "Правильный, безошибочный; соответствующий фактам или правилам."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "correct",
                                "target_lang_sense_clarification": "free from error"
                            },
                            {
                                "target_lang_word": "right",
                                "target_lang_sense_clarification": "factually correct or morally good"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "правильный",
                                "target_lang_sense_clarification": "верный, не содержащий ошибок"
                            },
                            {
                                "target_lang_word": "верный",
                                "target_lang_sense_clarification": "истинный, точный"
                            },
                            {
                                "target_lang_word": "справедливый",
                                "target_lang_sense_clarification": "обоснованный, честный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ae130a89-c929-3d89-ad5a-bc0595062d8c",
                    "sense_definition": "Precies; om nadruk te leggen op de nauwkeurigheid van een uitspraak.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is <w>juist</w> wat ik probeerde te zeggen.",
                            "target_lang_translations": {

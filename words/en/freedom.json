                    "frequency": "High",
                    "semantic_group_id": "state_of_liberty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After years in prison, the man finally regained his <w>freedom</w>."
                        },
                        {
                            "text": "The wild horses enjoyed their <w>freedom</w> on the open plains."
                        }
                    ],
                    "synonyms": [
                        "liberty"
                    ],
                    "antonyms": [
                        "slavery",
                        "imprisonment",
                        "bondage"
                    ],
                    "common_phrases": [

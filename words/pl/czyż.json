    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "80487337-0121-3779-a5c0-03a8dee1cd30",
                    "sense_definition": "Partykuła wprowadzająca pytanie, często o charakterze retorycznym, wyrażająca zdziwienie, wątpliwość lub zaprzeczenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Czyż</w> to nie jest wspaniały widok?",
                            "target_lang_translations": {
                                "ru": "<w>Разве</w> это не прекрасный вид?"
                            }
                        },
                        {
                            "text": "<w>Czyż</w> nie mówiłem, że to się tak skończy?",
                            "target_lang_translations": {
                                "ru": "<w>Разве</w> я не говорил, что это так закончится?"
                            }
                        }
                    ],
                    "synonyms": [
                        "czy",
                        "azali",
                        "azaliż",
                        "alboż"
                    ],
                    "common_phrases": [
                        "czyż nie",
                        "czyż to możliwe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица, вводящая вопрос, часто риторический, выражающая удивление, сомнение или отрицание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разве",
                                "target_lang_sense_clarification": "вопросительная частица"
                            },
                            {
                                "target_lang_word": "неужели",
                                "target_lang_sense_clarification": "выражение удивления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3701ed3d-7918-3925-b260-0996a73d87b0",
                    "sense_definition": "Używane w zwrocie „czyż nie” na końcu zdania, służącym do uzyskania potwierdzenia od rozmówcy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był bardzo udany wieczór, <w>czyż</w> nie?",
                            "target_lang_translations": {
                                "ru": "Это был очень удачный вечер, <w>не так ли</w>?"
                            }
                        },
                        {
                            "text": "Wyglądasz dzisiaj przepięknie, <w>czyż</w> nie?",
                            "target_lang_translations": {
                                "ru": "Ты выглядишь сегодня прекрасно, <w>не правда ли</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "nieprawdaż"
                    ],
                    "common_phrases": [
                        "czyż nie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в обороте «czyż nie» в конце предложения для получения подтверждения от собеседника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не так ли",
                                "target_lang_sense_clarification": "вопрос-подтверждение"
                            },
                            {
                                "target_lang_word": "не правда ли",
                                "target_lang_sense_clarification": "вопрос-подтверждение"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d590129e-7c21-3e58-b49d-8d92a6dce678",
                    "sense_definition": "A fleshy, thickened underground stem of a plant, usually containing stored starch, for example a potato or arrowroot.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "botany_tuber",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Farmers harvest the edible <w>tuber</w> late in the autumn season.",
                            "target_lang_translations": {
                                "ru": "Фермеры собирают съедобные <w>клубни</w> поздней осенью."
                            }
                        },
                        {
                            "text": "The potato is a common <w>tuber</w> used in cooking around the world.",
                            "target_lang_translations": {
                                "ru": "Картофель — это распространенный <w>клубень</w>, используемый в кулинарии по всему миру."
                            }
                        }
                    ],
                    "synonyms": [
                        "rootstock"
                    ],
                    "common_phrases": [
                        "edible tuber",
                        "plant tuber",
                        "underground tuber"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясистый утолщенный подземный стебель растения, обычно содержащий запасенный крахмал, например, картофель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клубень",
                                "target_lang_sense_clarification": "ботаника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "154bbc4e-e209-3190-a646-6820a74f3550",
                    "sense_definition": "A rounded, protuberant structure in a human or animal body.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy_tuber",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surgeon identified a small <w>tuber</w> on the bone during the examination.",
                            "target_lang_translations": {
                                "ru": "Хирург обнаружил небольшой <w>бугор</w> на кости во время осмотра."
                            }
                        },
                        {
                            "text": "Medical illustrations showed the location of the <w>tuber</w> near the joint.",
                            "target_lang_translations": {
                                "ru": "Медицинские иллюстрации показывали расположение <w>бугра</w> рядом с суставом."
                            }
                        }
                    ],
                    "synonyms": [
                        "protuberance",
                        "tubercle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Округлое выступающее образование в теле человека или животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бугор",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc00e850-2bee-3475-a9f2-ad05ec6ce04d",
                    "sense_definition": "A person who participates in the activity of tubing, such as floating on water or sliding on snow using an inner tube.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every <w>tuber</w> on the river must wear a life jacket for safety.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>катающийся на тюбинге</w> на реке обязан носить спасательный жилет в целях безопасности."
                            }
                        },
                        {
                            "text": "The snowy hill was packed with a happy <w>tuber</w> sliding down every few seconds.",
                            "target_lang_translations": {
                                "ru": "Снежная горка была полна счастливых <w>катающихся на ватрушках</w>, скатывающихся вниз каждые несколько секунд."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, занимающийся тюбингом (катанием на надувной камере по воде или снегу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "катающийся на тюбинге",
                                "target_lang_sense_clarification": "человек"
                            }
                        ]
                    }
                }
            ]
        },

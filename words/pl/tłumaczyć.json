    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1761fea1-151c-35cc-8389-e57256c0852a",
                    "sense_definition": "Przekładać tekst lub wypowiedź z jednego języka na inny, zachowując ich znaczenie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od lat <w>tłumaczę</w> teksty techniczne z języka angielskiego na polski.",
                            "target_lang_translations": {
                                "ru": "Я годами <w>перевожу</w> технические тексты с английского языка на польский."
                            }
                        },
                        {
                            "text": "Musisz dokładnie <w>tłumaczyć</w> każde zdanie, aby nie zmienić sensu wypowiedzi.",
                            "target_lang_translations": {
                                "ru": "Ты должен точно <w>переводить</w> каждое предложение, чтобы не изменить смысл высказывания."
                            }
                        }
                    ],
                    "synonyms": [
                        "przekładać"
                    ],
                    "common_phrases": [
                        "tłumaczyć na polski",
                        "tłumaczyć z angielskiego",
                        "tłumaczyć dosłownie",
                        "tłumaczyć tekst"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переводить текст или речь с одного языка на другой, сохраняя их смысл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переводить",
                                "target_lang_sense_clarification": "текст или речь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e08841d-29a3-3e32-ab1b-94e236561fd0",
                    "sense_definition": "Sprawiać, że coś staje się zrozumiałe; wyjaśniać komuś sens, przyczynę lub sposób działania czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wujek długo mi <w>tłumaczył</w>, na czym polegają procenty w matematyce.",
                            "target_lang_translations": {
                                "ru": "Дядя долго <w>объяснял</w> мне, в чём суть процентов в математике."
                            }
                        },
                        {
                            "text": "Nauczyciel cierpliwie <w>tłumaczy</w> uczniom nowe zagadnienia z gramatyki.",
                            "target_lang_translations": {
                                "ru": "Учитель терпеливо <w>объясняет</w> ученикам новые грамматические темы."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyjaśniać",
                        "objaśniać"
                    ],
                    "common_phrases": [
                        "tłumaczyć zasady",
                        "tłumaczyć dziecku",
                        "tłumaczyć cierpliwie",
                        "tłumaczyć zadanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо понятным; разъяснять кому-либо смысл, причину или способ действия чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объяснять",
                                "target_lang_sense_clarification": "делать понятным"
                            },
                            {
                                "target_lang_word": "разъяснять",
                                "target_lang_sense_clarification": "растолковывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91556b65-2d39-363f-acba-6b2cf6edfe47",
                    "sense_definition": "Podawać powody swojego postępowania, aby uniknąć oskarżeń lub nieporozumień (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczeń długo się <w>tłumaczył</w> nauczycielowi, dlaczego nie odrobił pracy domowej.",
                            "target_lang_translations": {
                                "ru": "Ученик долго <w>оправдывался</w> перед учителем, почему не сделал домашнее задание."
                            }
                        },
                        {
                            "text": "Nie musisz się nikomu <w>tłumaczyć</w> ze swoich prywatnych decyzji.",
                            "target_lang_translations": {
                                "ru": "Ты не обязан ни перед кем <w>оправдываться</w> за свои личные решения."
                            }
                        }
                    ],
                    "synonyms": [
                        "usprawiedliwiać"
                    ],
                    "common_phrases": [
                        "tłumaczyć się gęsto",
                        "tłumaczyć się przed szefem",
                        "nie musisz się tłumaczyć"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "used with 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить доводы в своё оправдание, объяснять причины своих поступков (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оправдываться",
                                "target_lang_sense_clarification": "приводить оправдания"
                            },
                            {
                                "target_lang_word": "объясняться",
                                "target_lang_sense_clarification": "давать объяснения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "380a50a1-2fef-3526-8ad4-0a946c98a612",
                    "sense_definition": "Stanowić powód lub usprawiedliwienie jakiegoś faktu, zdarzenia lub zachowania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagła choroba żony w pełni <w>tłumaczyła</w> jego wczorajszą nieobecność w pracy.",
                            "target_lang_translations": {
                                "ru": "Внезапная болезнь жены полностью <w>объясняла</w> его вчерашнее отсутствие на работе."
                            }
                        },
                        {
                            "text": "Trudne dzieciństwo po części <w>tłumaczy</w> jego problemy z nawiązywaniem relacji.",
                            "target_lang_translations": {
                                "ru": "Трудное детство отчасти <w>объясняет</w> его проблемы с налаживанием отношений."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyjaśniać",
                        "usprawiedliwiać"
                    ],
                    "common_phrases": [
                        "to wszystko tłumaczy",
                        "to nie tłumaczy zachowania",
                        "sytuacja tłumaczy wszystko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служить причиной или оправданием какого-либо факта, события или поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объяснять",
                                "target_lang_sense_clarification": "быть причиной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "029d866e-21fe-3b9c-9170-974ed6f3c648",
                    "sense_definition": "Nadawać się do przekładu na inny język (używane z zaimkiem „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele polskich idiomów nie <w>tłumaczy</w> się dosłownie na języki obce.",
                            "target_lang_translations": {
                                "ru": "Многие польские идиомы не <w>переводятся</w> дословно на иностранные языки."
                            }
                        },
                        {
                            "text": "Ta gra słów bardzo trudno <w>tłumaczy</w> się na język angielski.",
                            "target_lang_translations": {
                                "ru": "Эта игра слов очень трудно <w>переводится</w> на английский язык."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tłumaczyć się dosłownie",
                        "źle się tłumaczyć",
                        "dobrze się tłumaczyć"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "used with 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддаваться переводу на другой язык (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переводиться",
                                "target_lang_sense_clarification": "о тексте или словах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
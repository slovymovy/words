{
    "word_family": [
        "atlasik"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c0ce597e-b35f-390f-98bb-77dd78428a38",
                    "sense_definition": "Zbiór map, ilustracji, wykresów lub tablic, zwykle wydany w formie książkowej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lekcji geografii otworzyliśmy <w>atlasy</w> na stronie z mapą Europy.",
                            "target_lang_translations": {
                                "ru": "На уроке географии мы открыли <w>атласы</w> на странице с картой Европы."
                            }
                        },
                        {
                            "text": "Mój stary <w>atlas</w> samochodowy jest już nieaktualny.",
                            "target_lang_translations": {
                                "ru": "Мой старый автомобильный <w>атлас</w> уже устарел."
                            }
                        }
                    ],
                    "common_phrases": [
                        "atlas geograficzny",
                        "atlas świata",
                        "atlas samochodowy",
                        "atlas anatomiczny",
                        "szkolny atlas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сборник карт, иллюстраций, диаграмм или таблиц, обычно изданный в виде книги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атлас",
                                "target_lang_sense_clarification": "сборник карт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d7fc649-685c-3fbd-afeb-11c9ab167072",
                    "sense_definition": "Wielofunkcyjny przyrząd do ćwiczeń siłowych, pozwalający trenować różne partie mięśni w jednym miejscu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W nowej siłowni znajduje się nowoczesny <w>atlas</w> do ćwiczeń.",
                            "target_lang_translations": {
                                "ru": "В новом спортзале есть современный <w>силовой тренажёр</w>."
                            }
                        },
                        {
                            "text": "Zamiast wolnych ciężarów wolę trenować bezpiecznie na <w>atlasie</w>.",
                            "target_lang_translations": {
                                "ru": "Вместо свободных весов я предпочитаю безопасно тренироваться на <w>силовом тренажёре</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ćwiczyć na atlasie",
                        "atlas do ćwiczeń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Многофункциональный силовой тренажер, позволяющий тренировать различные группы мышц в одном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "силовой тренажёр",
                                "target_lang_sense_clarification": "оборудование"
                            },
                            {
                                "target_lang_word": "мультистанция",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a48f5a31-8c11-3952-8237-a2a1f3de2482",
                    "sense_definition": "Pierwszy kręg szyjny, na którym opiera się czaszka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz stwierdził pęknięcie pierwszego kręgu, czyli <w>atlasa</w>.",
                            "target_lang_translations": {
                                "ru": "Врач диагностировал перелом первого позвонка, то есть <w>атланта</w>."
                            }
                        },
                        {
                            "text": "<w>Atlas</w> umożliwia ruchy głowy w płaszczyźnie pionowej.",
                            "target_lang_translations": {
                                "ru": "<w>Атлант</w> позволяет совершать движения головой в вертикальной плоскости."
                            }
                        }
                    ],
                    "synonyms": [
                        "dźwigacz"
                    ],
                    "common_phrases": [
                        "kręg szczytowy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "anatomy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первый шейный позвонок, на котором держится череп."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атлант",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b233672-5bb4-3c6c-b3c0-93f81719e31d",
                    "sense_definition": "Podpora architektoniczna w formie posągu muskularnego mężczyzny, podtrzymująca belkowanie, balkon lub gzyms.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Balkon zabytkowej kamienicy podtrzymują dwa kamienne <w>atlasy</w>.",
                            "target_lang_translations": {
                                "ru": "Балкон старинного дома поддерживают два каменных <w>атланта</w>."
                            }
                        },
                        {
                            "text": "Ten barokowy <w>atlas</w> jest przykładem niezwykłego kunsztu rzeźbiarskiego.",
                            "target_lang_translations": {
                                "ru": "Этот барочный <w>атлант</w> является примером необычайного скульптурного мастерства."
                            }
                        }
                    ],
                    "synonyms": [
                        "atlant",
                        "telamon"
                    ],
                    "common_phrases": [
                        "postać atlasa"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "architecture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Архитектурная опора в виде статуи мускулистого мужчины, поддерживающая перекрытие, балкон или карниз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атлант",
                                "target_lang_sense_clarification": "архитектура"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "96c6df2c-0ea2-38e8-9eef-c65e57495899",
                    "sense_definition": "Łańcuch górski w północno-zachodniej Afryce.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Podczas wycieczki do Maroka widzieliśmy ośnieżone szczyty <w>Atlasu</w>.",
                            "target_lang_translations": {
                                "ru": "Во время поездки в Марокко мы видели заснеженные вершины <w>Атласа</w>."
                            }
                        },
                        {
                            "text": "<w>Atlas</w> oddziela wybrzeże Morza Śródziemnego od Sahary.",
                            "target_lang_translations": {
                                "ru": "<w>Атлас</w> отделяет побережье Средиземного моря от Сахары."
                            }
                        }
                    ],
                    "common_phrases": [
                        "góry Atlas",
                        "Atlas Wysoki",
                        "Atlas Średni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горная система на северо-западе Африки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Атлас",
                                "target_lang_sense_clarification": "горы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc2d073c-3602-3cfc-9bc8-84d4d225e036",
                    "sense_definition": "Tytan z mitologii greckiej skazany przez Zeusa na dźwiganie sklepienia niebieskiego na barkach.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Według mitologii <w>Atlas</w> podtrzymuje niebo na swoich barkach.",
                            "target_lang_translations": {
                                "ru": "Согласно мифологии, <w>Атлант</w> держит небо на своих плечах."
                            }
                        },
                        {
                            "text": "Rzeźba przedstawia <w>Atlasa</w> uginającego się pod ciężarem kuli ziemskiej.",
                            "target_lang_translations": {
                                "ru": "Скульптура изображает <w>Атланта</w>, сгибающегося под тяжестью земного шара."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mit o Atlasie",
                        "dźwigać jak Atlas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титан из греческой мифологии, осужденный Зевсом держать небесный свод на плечах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Атлант",
                                "target_lang_sense_clarification": "мифология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "835653f8-3726-3fd4-be00-bb1b332ccf01",
                    "sense_definition": "Jeden z małych, wewnętrznych księżyców Saturna.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Sonda kosmiczna przesłała nowe zdjęcia <w>Atlasa</w>.",
                            "target_lang_translations": {
                                "ru": "Космический зонд прислал новые фотографии <w>Атласа</w>."
                            }
                        },
                        {
                            "text": "Astronomowie obserwują orbitę <w>Atlasa</w> wokół Saturna.",
                            "target_lang_translations": {
                                "ru": "Астрономы наблюдают за орбитой <w>Атласа</w> вокруг Сатурна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "księżyc Atlas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из малых внутренних спутников Сатурна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Атлас",
                                "target_lang_sense_clarification": "астрономия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "terugkomst"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bb5a0c53-6beb-3d7d-a1c3-fae3470f0a07",
                    "sense_definition": "Weer naar een plek gaan waar men eerder was.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "return_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kom</w> morgen <w>terug</w> om mijn jas op te halen.",
                            "target_lang_translations": {
                                "en": "I will <w>come</w> <w>back</w> tomorrow to pick up my coat.",
                                "ru": "Я <w>вернусь</w> завтра, чтобы забрать свою куртку."
                            }
                        },
                        {
                            "text": "Wanneer <w>komen</w> jullie <w>terug</w> van vakantie?",
                            "target_lang_translations": {
                                "en": "When are you <w>coming</w> <w>back</w> from vacation?",
                                "ru": "Когда вы <w>вернетесь</w> из отпуска?"
                            }
                        }
                    ],
                    "synonyms": [
                        "terugkeren"
                    ],
                    "antonyms": [
                        "weggaan",
                        "vertrekken"
                    ],
                    "common_phrases": [
                        "snel terugkomen",
                        "niet meer terugkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To go or come back to a place where one was before.",
                        "ru": "Снова пойти или поехать в место, где человек был раньше."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come back",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "slightly more formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "возвращаться",
                                "target_lang_sense_clarification": "физически"
                            },
                            {
                                "target_lang_word": "вернуться",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "884822be-c047-3e15-9a9e-22245d37d134",
                    "sense_definition": "Opnieuw over een onderwerp beginnen te praten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "return_topic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zullen in het volgende hoofdstuk uitgebreid op deze kwestie <w>terugkomen</w>.",
                            "target_lang_translations": {
                                "en": "We will <w>come</w> <w>back</w> to this issue in detail in the next chapter.",
                                "ru": "Мы подробно <w>вернемся</w> к этому вопросу в следующей главе."
                            }
                        },
                        {
                            "text": "Mag ik nog even <w>terugkomen</w> op wat je net zei?",
                            "target_lang_translations": {
                                "en": "Can I just <w>come</w> <w>back</w> to what you just said?",
                                "ru": "Могу я еще раз <w>вернуться</w> к тому, что ты только что сказал?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ergens op terugkomen",
                        "er later op terugkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To start talking about a subject again.",
                        "ru": "Снова начать говорить о какой-либо теме."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come back to",
                                "target_lang_sense_clarification": "in conversation or discussion"
                            },
                            {
                                "target_lang_word": "return to",
                                "target_lang_sense_clarification": "in conversation or text"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "возвращаться",
                                "target_lang_sense_clarification": "к теме разговора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "353b2385-fde9-3a49-948a-ab4d742305c6",
                    "sense_definition": "Een eerdere belofte, afspraak of besluit intrekken of veranderen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "retract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politicus <w>kwam terug</w> op zijn verkiezingsbelofte.",
                            "target_lang_translations": {
                                "en": "The politician <w>went</w> <w>back</w> on his election promise.",
                                "ru": "Политик <w>отказался</w> от своего предвыборного обещания."
                            }
                        },
                        {
                            "text": "Je kunt nu niet meer op die afspraak <w>terugkomen</w>.",
                            "target_lang_translations": {
                                "en": "You can't <w>go</w> <w>back</w> on that agreement now.",
                                "ru": "Ты не можешь теперь <w>отказаться</w> от этой договоренности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op een besluit terugkomen",
                        "op zijn woorden terugkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To withdraw or change an earlier promise, agreement, or decision.",
                        "ru": "Отменить или изменить ранее данное обещание, договоренность или решение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go back on",
                                "target_lang_sense_clarification": "idiomatic, usually negative"
                            },
                            {
                                "target_lang_word": "renege on",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отказываться",
                                "target_lang_sense_clarification": "от своих слов или обещаний"
                            },
                            {
                                "target_lang_word": "пересматривать",
                                "target_lang_sense_clarification": "решение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea69a1d4-34db-304a-b604-b344a60c1476",
                    "sense_definition": "Opnieuw verschijnen of zichtbaar worden (bijvoorbeeld een thema, ziekte of gevoel).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "recurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn liefde voor de natuur <w>komt terug</w> in al zijn schilderijen.",
                            "target_lang_translations": {
                                "en": "His love for nature <w>recurs</w> in all his paintings.",
                                "ru": "Его любовь к природе <w>прослеживается</w> во всех его картинах."
                            }
                        },
                        {
                            "text": "De koorts is gisteren helaas weer <w>teruggekomen</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, the fever <w>came</w> <w>back</w> again yesterday.",
                                "ru": "К сожалению, вчера лихорадка снова <w>вернулась</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "terugkeren"
                    ],
                    "antonyms": [
                        "verdwijnen"
                    ],
                    "common_phrases": [
                        "steeds weer terugkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To appear or become visible again (e.g., a theme, illness, or feeling).",
                        "ru": "Появляться или становиться видимым снова (например, тема, болезнь или чувство)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recur",
                                "target_lang_sense_clarification": "happen again"
                            },
                            {
                                "target_lang_word": "come back",
                                "target_lang_sense_clarification": "reappear"
                            },
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "reappear"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "повторяться",
                                "target_lang_sense_clarification": "о теме или мотиве"
                            },
                            {
                                "target_lang_word": "возвращаться",
                                "target_lang_sense_clarification": "о болезни или чувстве"
                            },
                            {
                                "target_lang_word": "прослеживаться",
                                "target_lang_sense_clarification": "быть заметным (в творчестве)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e46a7cc1-5f7c-3349-8eee-ff3db3931368",
                    "sense_definition": "O deszczu, wietrze lub śniegu: padać lub wiać gwałtownie, uderzając z dużą siłą, często powodując ból lub dyskomfort.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "weather_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lodowaty wiatr zaczął <siec> nas po twarzach, gdy tylko wyszliśmy z domu.",
                            "target_lang_translations": {
                                "ru": "Ледяной ветер начал <сечь> нас по лицам, как только мы вышли из дома."
                            }
                        },
                        {
                            "text": "Gęsty deszcz będzie <siec> w szyby okienne przez całą nadchodzącą noc.",
                            "target_lang_translations": {
                                "ru": "Густой дождь будет <хлестать> в оконные стекла всю предстоящую ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "chlastać"
                    ],
                    "common_phrases": [
                        "deszcz siecze",
                        "wiatr siecze",
                        "siec po twarzy",
                        "siec po oczach"
                    ],
                    "target_lang_definitions": {
                        "ru": "О дожде, ветре или снеге: падать или дуть с силой, резко ударяя, часто причиняя боль или дискомфорт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сечь",
                                "target_lang_sense_clarification": "о дожде или ветре"
                            },
                            {
                                "target_lang_word": "хлестать",
                                "target_lang_sense_clarification": "бить с силой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "025511f9-f241-3cae-8b2f-8fee52eff67d",
                    "sense_definition": "Uderzać gwałtownie czymś ostrym (np. mieczem, szablą), zadając ciosy lub rany; walczyć bronią białą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cutting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz w srebrnej zbroi zaczął <siec> wrogów swoim ciężkim mieczem.",
                            "target_lang_translations": {
                                "ru": "Рыцарь в серебряных доспехах начал <сечь> врагов своим тяжелым мечом."
                            }
                        },
                        {
                            "text": "Podróżnicy musieli <siec> gęste zarośla maczetami, aby utorować sobie drogę.",
                            "target_lang_translations": {
                                "ru": "Путешественникам пришлось <рубить> густые заросли мачете, чтобы проложить себе путь."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciąć",
                        "rąbać"
                    ],
                    "common_phrases": [
                        "siec mieczem",
                        "siec szablą",
                        "siec wrogów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно ударять чем-то острым (например, мечом, саблей), нанося удары или раны; сражаться холодным оружием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рубить",
                                "target_lang_sense_clarification": "наносить удары оружием"
                            },
                            {
                                "target_lang_word": "сечь",
                                "target_lang_sense_clarification": "устар. сражаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e484bed1-0a62-3ec1-94eb-286bc0c986cd",
                    "sense_definition": "Kroić lub dzielić coś na bardzo drobne kawałki za pomocą ostrego narzędzia (rzadziej używana forma czasownika 'siekać').",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "culinary_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony kucharz potrafi błyskawicznie <siec> warzywa na drobną kostkę.",
                            "target_lang_translations": {
                                "ru": "Опытный повар может молниеносно <рубить> овощи на мелкие кубики."
                            }
                        },
                        {
                            "text": "Nie należy zbyt mocno <siec> mięsa, aby nie straciło swojej struktury.",
                            "target_lang_translations": {
                                "ru": "Не следует слишком сильно <рубить> мясо, чтобы оно не потеряло свою структуру."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciachać",
                        "ciąć"
                    ],
                    "common_phrases": [
                        "siec na kawałki",
                        "drobno siec"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "w tym znaczeniu częściej używa się czasownika 'siekać'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Резать или делить что-либо на очень мелкие кусочки с помощью острого инструмента (реже используемая форма глагола 'siekać')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рубить",
                                "target_lang_sense_clarification": "измельчать ножом"
                            },
                            {
                                "target_lang_word": "сечь",
                                "target_lang_sense_clarification": "шинковать (напр. капусту)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "858c1522-beb1-3a6a-b5fc-f0e878fbedf8",
                    "sense_definition": "Bić kogoś mocno i gwałtownie, zwykle używając do tego bata, rózgi lub innego elastycznego przedmiotu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "punishment_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Okrutny nadzorca kazał <siec> więźniów za każde najmniejsze przewinienie.",
                            "target_lang_translations": {
                                "ru": "Жестокий надзиратель приказал <сечь> заключенных за малейшую провинность."
                            }
                        },
                        {
                            "text": "Woźnica zaczął mocniej <siec> konie batem, aby przyspieszyć bieg wozu.",
                            "target_lang_translations": {
                                "ru": "Кучер начал сильнее <хлестать> лошадей кнутом, чтобы ускорить бег повозки."
                            }
                        }
                    ],
                    "synonyms": [
                        "chlastać"
                    ],
                    "common_phrases": [
                        "siec batem",
                        "siec rózgami"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "słownictwo rzadziej używane we współczesnym języku"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно и резко бить кого-либо, обычно используя кнут, розги или другой гибкий предмет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сечь",
                                "target_lang_sense_clarification": "наказывать розгами"
                            },
                            {
                                "target_lang_word": "пороть",
                                "target_lang_sense_clarification": "бить кнутом"
                            },
                            {
                                "target_lang_word": "хлестать",
                                "target_lang_sense_clarification": "наносить удары"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e741bc36-e3ba-3fbe-b26e-92e5f9ec8e76",
                    "sense_definition": "Ścinać zboże lub trawę przy użyciu kosy lub sierpa.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agricultural_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawniej chłopi wychodzili o świcie, aby ręcznie <siec> dojrzałe zboże na polach.",
                            "target_lang_translations": {
                                "ru": "Раньше крестьяне выходили на рассвете, чтобы вручную <жать> спелое зерно на полях."
                            }
                        },
                        {
                            "text": "Trzeba było <siec> trawę na łące, zanim słońce wzeszło zbyt wysoko.",
                            "target_lang_translations": {
                                "ru": "Нужно было <косить> траву на лугу, пока солнце не поднялось слишком высоко."
                            }
                        }
                    ],
                    "synonyms": [
                        "kosić",
                        "żąć"
                    ],
                    "common_phrases": [
                        "siec zboże",
                        "siec trawę"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie używa się słowa 'kosić'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Срезать зерновые или траву с помощью косы или серпа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "косить",
                                "target_lang_sense_clarification": "срезать траву"
                            },
                            {
                                "target_lang_word": "жать",
                                "target_lang_sense_clarification": "срезать злаки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

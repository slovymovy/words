{
    "word_family": [
        "consistent",
        "consistently",
        "inconsistent",
        "inconsistency"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b45d152b-3740-3751-b0dc-4aa43903a4cd",
                    "sense_definition": "The quality of always behaving, happening, or appearing in the same way; reliability or uniformity.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "uniformity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team needs to improve the <w>consistency</w> of their performance to win the league.",
                            "target_lang_translations": {
                                "ru": "Команде нужно улучшить <w>стабильность</w> своей игры, чтобы выиграть лигу."
                            }
                        },
                        {
                            "text": "Customers appreciate the <w>consistency</w> of quality in this brand's products.",
                            "target_lang_translations": {
                                "ru": "Покупатели ценят <w>постоянство</w> качества продуктов этого бренда."
                            }
                        }
                    ],
                    "synonyms": [
                        "uniformity",
                        "reliability",
                        "stability",
                        "steadiness"
                    ],
                    "antonyms": [
                        "inconsistency"
                    ],
                    "common_phrases": [
                        "lack of consistency",
                        "consistency of quality",
                        "show consistency",
                        "ensure consistency"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство вести себя, происходить или выглядеть одинаково; постоянство или стабильность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постоянство",
                                "target_lang_sense_clarification": "неизменность качества или поведения"
                            },
                            {
                                "target_lang_word": "стабильность",
                                "target_lang_sense_clarification": "надежность и устойчивость результата"
                            },
                            {
                                "target_lang_word": "последовательность",
                                "target_lang_sense_clarification": "логичность и упорядоченность действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29445415-33f0-3a1d-8700-3daee5b1d167",
                    "sense_definition": "The physical quality of a substance, specifically how thick, smooth, or viscous it is.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mix the ingredients until the batter reaches a smooth <w>consistency</w>.",
                            "target_lang_translations": {
                                "ru": "Смешивайте ингредиенты, пока тесто не приобретет однородную <w>консистенцию</w>."
                            }
                        },
                        {
                            "text": "The soup should have a thick <w>consistency</w> before you serve it.",
                            "target_lang_translations": {
                                "ru": "Суп должен иметь густую <w>консистенцию</w> перед подачей."
                            }
                        }
                    ],
                    "synonyms": [
                        "texture",
                        "thickness",
                        "viscosity",
                        "density"
                    ],
                    "common_phrases": [
                        "creamy consistency",
                        "thick consistency",
                        "consistency of paste",
                        "smooth consistency"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое свойство вещества, определяющее степень его густоты, плотности или вязкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "консистенция",
                                "target_lang_sense_clarification": "степень густоты или плотности вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca74df64-f285-3d98-8ede-1ff3e72a2cfc",
                    "sense_definition": "The state of being accurate or free from contradiction; logical coherence between parts.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The editor checked the manuscript for internal <w>consistency</w> regarding the timeline of events.",
                            "target_lang_translations": {
                                "ru": "Редактор проверил рукопись на внутреннюю <w>согласованность</w> хронологии событий."
                            }
                        },
                        {
                            "text": "There is a lack of <w>consistency</w> between the witness's two statements.",
                            "target_lang_translations": {
                                "ru": "Между двумя показаниями свидетеля отсутствует <w>согласованность</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "coherence",
                        "compatibility",
                        "correspondence"
                    ],
                    "antonyms": [
                        "inconsistency"
                    ],
                    "common_phrases": [
                        "internal consistency",
                        "logical consistency",
                        "consistency with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие противоречий; логическая взаимосвязь между частями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласованность",
                                "target_lang_sense_clarification": "гармоничная связь между частями"
                            },
                            {
                                "target_lang_word": "непротиворечивость",
                                "target_lang_sense_clarification": "отсутствие логических противоречий"
                            },
                            {
                                "target_lang_word": "логичность",
                                "target_lang_sense_clarification": "соответствие законам логики"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
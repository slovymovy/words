                    "sense_id": "e5b82d81-fb77-3914-a23b-dbfb723ae876",
                    "sense_definition": "Taki, któremu chce się spać; odczuwający potrzebę snu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_fatigue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po całym dniu pracy jestem bardzo <w>śpiący</w>.",
                            "target_lang_translations": {
                                "ru": "После целого рабочего дня я очень <w>сонный</w>."
                            }
                        },
                        {
                            "text": "Dzieci zrobiły się <w>śpiące</w> już o godzinie ósmej.",
                            "target_lang_translations": {
                                "ru": "Дети стали <w>сонными</w> уже в восемь часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "senny",
                        "ospały",
                        "zaspany"
                    ],
                    "antonyms": [
                        "rozbudzony"
                    ],
                    "common_phrases": [
                        "być śpiącym",
                        "zrobić się śpiącym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто хочет спать; испытывающий потребность во сне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сонный",
                                "target_lang_sense_clarification": "ощущающий сонливость"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [

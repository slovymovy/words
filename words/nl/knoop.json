    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d5009fc6-0585-3d82-868c-7922f108b4e8",
                    "sense_definition": "Een klein, meestal rond voorwerp dat aan kleding is bevestigd en door een gat wordt gehaald om de kleding te sluiten",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing_fastener",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bovenste <w>knoop</w> van mijn overhemd is eraf gevallen.",
                            "target_lang_translations": {
                                "en": "The top <w>button</w> of my shirt has fallen off.",
                                "ru": "Верхняя <w>пуговица</w> на моей рубашке оторвалась."
                            }
                        },
                        {
                            "text": "Kun jij deze lastige <w>knoop</w> voor mij losmaken?",
                            "target_lang_translations": {
                                "en": "Can you undo this difficult <w>button</w> for me?",
                                "ru": "Ты можешь расстегнуть эту тугую <w>пуговицу</w> для меня?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "knoop aanzetten",
                        "knoop losmaken",
                        "knoop dichtdoen"
                    ],
                    "target_lang_definitions": {
                        "en": "A small, usually round object attached to clothing and passed through a hole to fasten the garment",
                        "ru": "Небольшой, обычно круглый предмет, прикрепленный к одежде и продеваемый через петлю для застегивания"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "button",
                                "target_lang_sense_clarification": "fastener on clothes"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пуговица",
                                "target_lang_sense_clarification": "застёжка на одежде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e927d1da-a935-3886-89dc-cb0cc6d6329e",
                    "sense_definition": "Een verbinding of verdikking in touw, draad of garen die ontstaat door het materiaal door elkaar te halen en aan te trekken",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rope_tie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zit een stevige <w>knoop</w> in mijn veters die ik niet los krijg.",
                            "target_lang_translations": {
                                "en": "There is a tight <w>knot</w> in my shoelaces that I cannot untie.",
                                "ru": "На моих шнурках тугой <w>узел</w>, который я не могу развязать."
                            }
                        },
                        {
                            "text": "Hij moest de <w>knoop</w> doorhakken om een beslissing te nemen.",
                            "target_lang_translations": {
                                "en": "He had to cut the <w>knot</w> to make a decision.",
                                "ru": "Ему пришлось разрубить <w>узел</w>, чтобы принять решение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de knoop",
                        "knoop leggen",
                        "de knoop doorhakken"
                    ],
                    "target_lang_definitions": {
                        "en": "A fastening made by looping a piece of string, rope, or something similar on itself and tightening it",
                        "ru": "Соединение или утолщение на веревке, нити или пряже, образованное путем переплетения и затягивания материала"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "knot",
                                "target_lang_sense_clarification": "in rope or string"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "узел",
                                "target_lang_sense_clarification": "на веревке или нитке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a63c4ea8-9277-3b68-a2e4-03ecc5252b6f",
                    "sense_definition": "Een eenheid voor snelheid die wordt gebruikt in de scheepvaart en luchtvaart, gelijk aan één zeemijl per uur",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "measurement_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het schip voer met een snelheid van twintig <w>knopen</w> richting de haven.",
                            "target_lang_translations": {
                                "en": "The ship sailed towards the harbor at a speed of twenty <w>knots</w>.",
                                "ru": "Корабль плыл к гавани со скоростью двадцать <w>узлов</w>."
                            }
                        },
                        {
                            "text": "De wind nam toe tot dertig <w>knopen</w> tijdens de storm.",
                            "target_lang_translations": {
                                "en": "The wind increased to thirty <w>knots</w> during the storm.",
                                "ru": "Во время шторма ветер усилился до тридцати <w>узлов</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "nautical/aviation context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A unit of speed used by ships and aircraft, equivalent to one nautical mile per hour",
                        "ru": "Единица скорости, используемая в мореплавании и авиации, равная одной морской миле в час"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "knot",
                                "target_lang_sense_clarification": "unit of speed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "узел",
                                "target_lang_sense_clarification": "единица скорости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce76621e-1e0d-3977-832f-1b5d4e56d071",
                    "sense_definition": "Een verdikking in de stengel van een plant waar bladeren of zijtakken ontspringen",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "botany_node",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij elke <w>knoop</w> van de bamboestengel groeit een nieuw blad.",
                            "target_lang_translations": {
                                "en": "A new leaf grows at every <w>node</w> of the bamboo stem.",
                                "ru": "У каждого <w>узла</w> бамбукового стебля растет новый лист."
                            }
                        },
                        {
                            "text": "Je kunt de plant het beste net boven een <w>knoop</w> snoeien.",
                            "target_lang_translations": {
                                "en": "It is best to prune the plant just above a <w>node</w>.",
                                "ru": "Лучше всего обрезать растение чуть выше <w>узла</w>."
                            }

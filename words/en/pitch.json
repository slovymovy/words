    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "11327610-7a25-3ac6-a49e-053d3149152c",
                    "sense_definition": "The area of ground marked out for playing a game such as soccer, cricket, or rugby.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sports_field",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The players ran onto the football <w>pitch</w> to start the match.",
                            "target_lang_translations": {
                                "ru": "Игроки выбежали на футбольное <w>поле</w>, чтобы начать матч."
                            }
                        },
                        {
                            "text": "Heavy rain turned the cricket <w>pitch</w> into a muddy swamp.",
                            "target_lang_translations": {
                                "ru": "Проливной дождь превратил <w>площадку</w> для крикета в грязное болото."
                            }
                        }
                    ],
                    "common_phrases": [
                        "football pitch",
                        "cricket pitch",
                        "on the pitch"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок земли, размеченный для игры в футбол, крикет или регби."

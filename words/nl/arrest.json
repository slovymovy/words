    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5d93cced-5265-31d5-ae14-c88385d32ddd",
                    "sense_definition": "Het gevangennemen of aanhouden van een persoon door de politie of justitie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte werd direct na de overval onder <w>arrest</w> geplaatst.",
                            "target_lang_translations": {
                                "en": "The suspect was placed under <w>arrest</w> immediately after the robbery.",
                                "ru": "Подозреваемый был помещен под <w>арест</w> сразу после ограбления."
                            }
                        },
                        {
                            "text": "Je staat onder <w>arrest</w> voor het stelen van een auto.",
                            "target_lang_translations": {
                                "en": "You are under <w>arrest</w> for stealing a car.",
                                "ru": "Вы находитесь под <w>арестом</w> за угон автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanhouding",
                        "arrestatie",
                        "hechtenis"
                    ],
                    "antonyms": [
                        "vrijlating"
                    ],
                    "common_phrases": [
                        "onder arrest staan",
                        "onder arrest plaatsen",
                        "huis van arrest"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of taking a person into custody by the police or justice system.",
                        "ru": "Взятие под стражу или задержание лица полицией или органами правосудия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arrest",
                                "target_lang_sense_clarification": "the act of detaining someone"
                            },
                            {
                                "target_lang_word": "custody",
                                "target_lang_sense_clarification": "state of being kept by police"
                            },
                            {
                                "target_lang_word": "detention",
                                "target_lang_sense_clarification": "confinement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "арест",
                                "target_lang_sense_clarification": "лишение свободы"
                            },
                            {
                                "target_lang_word": "задержание",
                                "target_lang_sense_clarification": "временное лишение свободы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69faa73e-faae-31a6-8e50-c2a9daabae69",
                    "sense_definition": "Een schriftelijke uitspraak of vonnis van een hogere rechter, zoals een gerechtshof of de Hoge Raad.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De Hoge Raad heeft in dit belangrijke <w>arrest</w> duidelijkheid geschept over de nieuwe wet.",
                            "target_lang_translations": {
                                "en": "In this important <w>judgment</w>, the Supreme Court provided clarity regarding the new law.",
                                "ru": "В этом важном <w>постановлении</w> Верховный суд внес ясность касательно нового закона."
                            }
                        },
                        {
                            "text": "Het <w>arrest</w> van het hof bevestigde de eerdere veroordeling van de fraudeur.",
                            "target_lang_translations": {
                                "en": "The court's <w>ruling</w> confirmed the fraudster's earlier conviction.",
                                "ru": "<w>Постановление</w> суда подтвердило предыдущий приговор мошеннику."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitspraak",
                        "vonnis",
                        "rechterlijke beslissing"
                    ],
                    "common_phrases": [
                        "arrest wijzen"
                    ],
                    "target_lang_definitions": {
                        "en": "A written decision or judgment by a higher court, such as a court of appeal or the Supreme Court.",
                        "ru": "Письменное решение или постановление суда высшей инстанции (например, апелляционного или Верховного суда)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "judgment",
                                "target_lang_sense_clarification": "formal court decision"
                            },
                            {
                                "target_lang_word": "ruling",
                                "target_lang_sense_clarification": "authoritative decision"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "постановление",
                                "target_lang_sense_clarification": "судебный акт высшей инстанции"
                            },
                            {
                                "target_lang_word": "судебное решение",
                                "target_lang_sense_clarification": "вердикт суда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c77d320-ec77-306b-9986-b89dfaeae0c7",
                    "sense_definition": "Het gerechtelijk in beslag nemen van goederen of gelden.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schuldeiser liet <w>arrest</w> leggen op de bankrekening van het bedrijf.",
                            "target_lang_translations": {
                                "en": "The creditor had an <w>attachment</w> placed on the company's bank account.",
                                "ru": "Кредитор наложил <w>арест</w> на банковский счет компании."
                            }
                        },
                        {
                            "text": "Er werd conservatoir <w>arrest</w> gelegd op het schip in de haven.",
                            "target_lang_translations": {
                                "en": "A precautionary <w>arrest</w> was placed on the ship in the harbor.",
                                "ru": "На судно в порту был наложен обеспечительный <w>арест</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "beslaglegging",
                        "inbeslagname"
                    ],
                    "common_phrases": [
                        "arrest leggen op"
                    ],
                    "target_lang_definitions": {
                        "en": "The judicial seizure of goods or money.",
                        "ru": "Судебное наложение запрета на распоряжение имуществом или денежными средствами."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seizure",
                                "target_lang_sense_clarification": "taking possession of property"
                            },
                            {
                                "target_lang_word": "attachment",
                                "target_lang_sense_clarification": "legal seizure of assets"
                            },
                            {
                                "target_lang_word": "arrest",
                                "target_lang_sense_clarification": "seizure of a ship"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "арест",
                                "target_lang_sense_clarification": "имущества или счетов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

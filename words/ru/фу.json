    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "63cd4f0c-04cf-36f8-831b-57092a7b1f90",
                    "sense_definition": "Выражает отвращение, брезгливость или сильное неудовольствие по поводу чего-либо неприятного (запаха, вида, поступка).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "disgust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Фу</w>, какая гадость, убери это от меня немедленно!"
                        },
                        {
                            "text": "<w>Фу</w>, как здесь неприятно пахнет сыростью и плесенью."
                        }
                    ],
                    "synonyms": [
                        "фи",
                        "бе"
                    ],
                    "antonyms": [
                        "ах",
                        "ня"
                    ],
                    "common_phrases": [
                        "фу, какая гадость",
                        "фу ты ну ты"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ]
                },
                {
                    "sense_id": "8183a563-3e8d-300a-9cb7-0cfa12be3148",
                    "sense_definition": "Команда собаке прекратить нежелательное действие; используется также в переносном смысле как запрет человеку.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мухтар, <w>фу</w>, нельзя трогать чужие вещи!"
                        },
                        {
                            "text": "Хозяин громко крикнул <w>фу</w>, и пёс сразу отошёл от стола."
                        }
                    ],
                    "common_phrases": [
                        "фу, нельзя",
                        "команда фу"
                    ]
                },
                {
                    "sense_id": "124b418b-3270-313c-8df5-20c6379f5fbd",
                    "sense_definition": "Выражает чувство облегчения, выдох после сильного напряжения или трудной работы.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "relief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Фу</w>, как хорошо, что мы наконец-то закончили этот ремонт."
                        },
                        {
                            "text": "<w>Фу</w>, пронесло, я уже думал, что мы опоздаем на поезд."
                        }
                    ],
                    "synonyms": [
                        "фух"
                    ],
                    "common_phrases": [
                        "фу, слава богу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ]
                }
            ]
        },
        {

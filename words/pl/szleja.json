    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dfde1ffd-1eaf-3518-bb08-0d0a9af08404",
                    "sense_definition": "Rodzaj uprzęży zakładanej na kark i pierś konia, używanej zamiast chomąta, zazwyczaj do lżejszych zaprzęgów.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz musiał naprawić skórzaną <szleję>, która pękła podczas orki.",
                            "target_lang_translations": {
                                "ru": "Хозяину пришлось починить кожаную <шлею>, которая лопнула во время пахоты."
                            }
                        },
                        {
                            "text": "Do lekkiego powozu zaprzęgnięto gniadego konia w nową <szleję>.",
                            "target_lang_translations": {
                                "ru": "В легкую повозку запрягли гнедую лошадь в новой <шлее>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uprząż szorowa"
                    ],
                    "common_phrases": [
                        "koń w szlei"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид упряжи, надеваемой на шею и грудь лошади, используемый вместо хомута, обычно для легких повозок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шлея",
                                "target_lang_sense_clarification": "часть конской упряжи"
                            },
                            {
                                "target_lang_word": "шорка",
                                "target_lang_sense_clarification": "нагрудная упряжь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8269c7e8-5656-3f90-a8f9-4082d5a52689",
                    "sense_definition": "Pas lub szelka zakładane na ramiona, ułatwiające człowiekowi ciągnięcie lub dźwiganie ciężkich przedmiotów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tragarz poprawił <szleję> na ramieniu i z wysiłkiem ruszył z ciężkim ładunkiem.",
                            "target_lang_translations": {
                                "ru": "Носильщик поправил <лямку> на плече и с усилием двинулся с тяжелым грузом."
                            }
                        },
                        {
                            "text": "Mężczyzna użył szerokiej <szlei>, aby łatwiej wciągnąć wózek pod górę.",
                            "target_lang_translations": {
                                "ru": "Мужчина использовал широкую <лямку>, чтобы легче втащить тележку в гору."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремень или лямка, надеваемая на плечи, облегчающая человеку тягу или переноску тяжелых предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лямка",
                                "target_lang_sense_clarification": "широкий ремень через плечо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
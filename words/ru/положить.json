        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c0dc2878-8783-3958-8a1e-7ec73e283c66",
                    "sense_definition": "Поместить что-либо на поверхность в лежачее положение",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <w>положить</w> эту книгу на стол.",
                            "target_lang_translations": {
                                "en": "I want to <w>put</w> this book on the table."
                            }
                        },
                        {
                            "text": "Мама <w>положила</w> ребёнка спать в кроватку.",
                            "target_lang_translations": {
                                "en": "Mom <w>put</w> the baby to sleep in the crib."
                            }
                        },
                        {
                            "text": "Пожалуйста, <w>положи</w> ковёр на пол аккуратно.",
                            "target_lang_translations": {
                                "en": "Please <w>lay</w> the carpet on the floor carefully."
                            }
                        }
                    ],
                    "synonyms": [
                        "поместить"
                    ],
                    "common_phrases": [
                        "положить на стол",
                        "положить на место",
                        "положить спать",
                        "положить голову на подушку"
                    ],
                    "target_lang_definitions": {
                        "en": "To place something on a surface in a lying position"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put",
                                "target_lang_sense_clarification": "general term for placing something"
                            },
                            {
                                "target_lang_word": "lay",
                                "target_lang_sense_clarification": "to place something in a horizontal position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1e07e5b-fad3-3419-9e73-08adadb622d2",
                    "sense_definition": "Поместить внутрь чего-либо",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не забудь <w>положить</w> ключи в карман.",
                            "target_lang_translations": {
                                "en": "Don't forget to <w>put</w> the keys in your pocket."
                            }
                        },
                        {
                            "text": "Повар <w>положил</w> слишком много соли в суп.",
                            "target_lang_translations": {
                                "en": "The cook <w>put</w> too much salt in the soup."
                            }
                        },
                        {
                            "text": "Мы <w>положили</w> все вещи в большой чемодан.",
                            "target_lang_translations": {
                                "en": "We <w>put</w> all the things into a big suitcase."
                            }
                        }
                    ],
                    "synonyms": [
                        "поместить",
                        "сунуть"
                    ],
                    "common_phrases": [
                        "положить в карман",
                        "положить сахар в чай",
                        "положить в ящик",
                        "положить в сумку"
                    ],
                    "target_lang_definitions": {
                        "en": "To place inside something"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put",
                                "target_lang_sense_clarification": "to insert into a container or space"
                            },
                            {
                                "target_lang_word": "place",
                                "target_lang_sense_clarification": "more formal synonym for put"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "becd14c2-b95e-316d-998a-e576ca9305d9",
                    "sense_definition": "Внести деньги в банк на хранение или на счёт",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно срочно <w>положить</w> деньги на телефон.",
                            "target_lang_translations": {
                                "en": "I urgently need to <w>put</w> money on my phone."
                            }
                        },
                        {
                            "text": "Завтра я пойду в банк и <w>положу</w> зарплату на счёт.",
                            "target_lang_translations": {
                                "en": "Tomorrow I will go to the bank and <w>deposit</w> my salary into the account."
                            }
                        }
                    ],
                    "common_phrases": [
                        "положить деньги на счёт",
                        "положить на карту",
                        "положить в банк",
                        "положить под проценты"
                    ],
                    "target_lang_definitions": {
                        "en": "To deposit money in a bank for safekeeping or into an account"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deposit",
                                "target_lang_sense_clarification": "to pay money into a bank account"
                            },
                            {
                                "target_lang_word": "put",
                                "target_lang_sense_clarification": "informal, used with adding money to phones or cards"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ba984a1-5226-34ac-a07a-45e209335940",
                    "sense_definition": "Потратить, отдать (силы, здоровье, жизнь) ради какой-либо цели",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_spending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдаты были готовы <w>положить</w> жизнь за свою родину.",
                            "target_lang_translations": {
                                "en": "The soldiers were ready to <w>lay down</w> their lives for their homeland."
                            }
                        },
                        {
                            "text": "Он <w>положил</w> всё своё здоровье на эту тяжёлую работу.",
                            "target_lang_translations": {
                                "en": "He <w>sacrificed</w> all his health for this hard work."
                            }
                        }
                    ],
                    "common_phrases": [
                        "положить жизнь",
                        "положить здоровье",
                        "положить все силы"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend or give (strength, health, life) for the sake of a goal"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lay down",
                                "target_lang_sense_clarification": "specifically used in 'lay down one's life'"
                            },
                            {
                                "target_lang_word": "sacrifice",
                                "target_lang_sense_clarification": "to give up health or effort for a cause"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95aa0579-4454-33e4-a10f-c6a535202b73",
                    "sense_definition": "Сочинить музыку на какой-либо текст",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Композитор решил <w>положить</w> эти прекрасные стихи на музыку.",
                            "target_lang_translations": {
                                "en": "The composer decided to <w>set</w> these beautiful poems to music."
                            }
                        },
                        {
                            "text": "Эту народную песню <w>положили</w> на ноты только в прошлом веке.",
                            "target_lang_translations": {
                                "en": "This folk song was <w>set</w> to notes only in the last century."
                            }
                        }
                    ],
                    "common_phrases": [
                        "положить на музыку",
                        "положить стихи на музыку",
                        "положить на ноты"
                    ],
                    "target_lang_definitions": {
                        "en": "To compose music for a text"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "used in the phrase 'set to music'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0207962e-d71f-3d64-8c8d-4b77e8e3209a",
                    "sense_definition": "Перестать обращать внимание, проигнорировать, отнестись безразлично",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_ignore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент совсем <w>положил</w> на учёбу и перестал ходить на лекции.",
                            "target_lang_translations": {
                                "en": "The student completely <w>gave up</w> on his studies and stopped going to lectures."
                            }
                        },
                        {
                            "text": "Нельзя просто так <w>положить</w> на мнение окружающих людей.",
                            "target_lang_translations": {
                                "en": "You can't just <w>ignore</w> the opinions of people around you."
                            }
                        }
                    ],
                    "common_phrases": [
                        "положить на всё",
                        "положить на учёбу",
                        "положить на работу"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "rude"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stop paying attention, ignore, or treat with indifference"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give up on",
                                "target_lang_sense_clarification": "to stop trying or caring about something"
                            },
                            {
                                "target_lang_word": "ignore",
                                "target_lang_sense_clarification": "to disregard something intentionally"
                            },
                            {
                                "target_lang_word": "stop caring about",
                                "target_lang_sense_clarification": "to become indifferent towards"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0394ea6e-fa36-3e78-a298-5e97973956dd",
                    "sense_definition": "Сделать компьютерную систему, сервер или сайт недоступным",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computer_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хакеры смогли <w>положить</w> сайт банка на несколько часов.",
                            "target_lang_translations": {
                                "en": "Hackers managed to <w>bring down</w> the bank's website for several hours."
                            }
                        },
                        {
                            "text": "Из-за ошибки в новом коде мы случайно <w>положили</w> сервер.",
                            "target_lang_translations": {
                                "en": "Due to a bug in the new code, we accidentally <w>crashed</w> the server."
                            }
                        }
                    ],
                    "synonyms": [
                        "уронить",
                        "грохнуть"
                    ],
                    "common_phrases": [
                        "положить сервер",
                        "положить сайт",
                        "положить сеть"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "computer jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make a computer system, server, or website unavailable"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bring down",
                                "target_lang_sense_clarification": "to cause a system to stop functioning"
                            },
                            {
                                "target_lang_word": "crash",
                                "target_lang_sense_clarification": "to cause a server or program to fail"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dac249d8-0e3b-3df1-a839-3596ffc42df7",
                    "sense_definition": "Убить кого-либо, обычно в бою или перестрелке",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В том жестоком бою снайпер <w>положил</w> пятерых врагов.",
                            "target_lang_translations": {
                                "en": "In that fierce battle, the sniper <w>took down</w> five enemies."
                            }
                        },
                        {
                            "text": "Бандиты <w>положили</w> всю охрану банка за одну минуту.",
                            "target_lang_translations": {
                                "en": "The bandits <w>killed</w> all the bank's security guards in one minute."
                            }
                        }
                    ],
                    "common_phrases": [
                        "положить наповал",
                        "положить всех"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To kill someone, usually in battle or a shootout"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take down",
                                "target_lang_sense_clarification": "to kill or defeat an enemy"
                            },
                            {
                                "target_lang_word": "kill",
                                "target_lang_sense_clarification": "general term for ending a life"

                {
                    "sense_id": "857b8152-be64-3fed-b640-7026b1cd9f05",
                    "sense_definition": "Poglądy skrajnie lewicowe, często postrzegane jako radykalne.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "ideology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Leninowskie idee są nie do pogodzenia z reformizmem, oportunizmem, <w>lewactwem</w> i innymi przejawami wrogich marksizmowi tendencji.",
                            "target_lang_translations": {
                                "ru": "Ленинские идеи несовместимы с реформизмом, оппортунизмом, <w>левачеством</w> и другими проявлениями враждебных марксизму тенденций."
                            }
                        },
                        {
                            "text": "Współczesne <w>lewactwo</w> jest często krytykowane przez środowiska konserwatywne za swoje postulaty.",
                            "target_lang_translations": {
                                "ru": "Современное <w>левачество</w> часто подвергается критике со стороны консервативных кругов за свои требования."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrajna lewica"
                    ],
                    "antonyms": [
                        "prawactwo"
                    ],
                    "common_phrases": [
                        "skrajne lewactwo",
                        "szerzące się lewactwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайне левые взгляды, часто воспринимаемые как радикальные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левачество",
                                "target_lang_sense_clarification": "политическое течение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6bf0656-6a61-3c78-b05d-47c54d2ceee4",
                    "sense_definition": "Ogół ludzi lub ugrupowań o poglądach skrajnie lewicowych.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu publicystów oskarża <w>lewactwo</w> o niszczenie tradycyjnych wartości.",
                            "target_lang_translations": {
                                "ru": "Многие публицисты обвиняют <w>леваков</w> в разрушении традиционных ценностей."
                            }
                        },
                        {
                            "text": "Lokalne <w>lewactwo</w> zorganizowało głośny protest przeciwko nowej ustawie.",
                            "target_lang_translations": {
                                "ru": "Местные <w>леваки</w> организовали громкий протест против нового закона."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrajna lewica"
                    ],
                    "antonyms": [
                        "prawactwo"
                    ],
                    "common_phrases": [
                        "walka z lewactwem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często pejoratywnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность людей или группировок с крайне левыми взглядами."
                    },

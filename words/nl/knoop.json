                        {
                            "trait_type": "colloquial",
                            "comment": "botany"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A point on a plant stem from which leaves or branches grow",
                        "ru": "Утолщение на стебле растения, откуда растут листья или ветки"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "node",
                                "target_lang_sense_clarification": "botany"
                            },
                            {
                                "target_lang_word": "joint",
                                "target_lang_sense_clarification": "plant stem part"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "узел",
                                "target_lang_sense_clarification": "в ботанике"
                            }

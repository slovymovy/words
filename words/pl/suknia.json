    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7342c3e8-50dd-324c-ad6d-519beb354bbd",
                    "sense_definition": "Elegancki, jednoczęściowy strój kobiecy okrywający tułów i nogi, noszony zazwyczaj na uroczyste okazje.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Panna młoda wyglądała olśniewająco, gdy weszła do kościoła w białej <sukni>.",
                            "target_lang_translations": {
                                "ru": "Невеста выглядела ослепительно, когда вошла в церковь в белом <платье>."
                            }
                        },
                        {
                            "text": "Na balu maturalnym każda dziewczyna chciała mieć najpiękniejszą <suknię> wieczorową.",
                            "target_lang_translations": {
                                "ru": "На выпускном балу каждая девушка хотела быть в самом красивом вечернем <платье>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kreacja",
                        "toaleta"
                    ],
                    "common_phrases": [
                        "suknia ślubna",
                        "suknia wieczorowa",
                        "suknia balowa",
                        "długa suknia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элегантный цельный предмет женской одежды, закрывающий туловище и ноги, который обычно носят в торжественных случаях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платье",
                                "target_lang_sense_clarification": "женская одежда"
                            },
                            {
                                "target_lang_word": "наряд",
                                "target_lang_sense_clarification": "одежда, костюм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9dfadfdf-28a0-3e7e-a55a-17ae8a30b347",
                    "sense_definition": "W języku łowieckim: sierść zwierzyny płowej i dzika lub upierzenie ptaków.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą <suknia> sarny staje się gęstsza i zmienia kolor na szary.",
                            "target_lang_translations": {
                                "ru": "Зимой <шерсть> косули становится гуще и меняет цвет на серый."
                            }
                        },
                        {
                            "text": "Doświadczony myśliwy potrafi ocenić stan zdrowia zwierzęcia, patrząc na jego <suknię>.",
                            "target_lang_translations": {
                                "ru": "Опытный охотник может оценить состояние здоровья животного, глядя на его <шерсть>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sierść",
                        "upierzenie",
                        "szata"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "łowiectwo"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В охотничьем языке: шерсть диких животных или оперение птиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шерсть",
                                "target_lang_sense_clarification": "животного"
                            },
                            {
                                "target_lang_word": "оперение",
                                "target_lang_sense_clarification": "птицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf175833-9acd-3e34-974f-32b907338969",
                    "sense_definition": "Dawne określenie na wierzchni ubiór męski lub kobiecy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bogaty kupiec nosił drogą, jedwabną <suknię>, która świadczyła o jego statusie.",
                            "target_lang_translations": {
                                "ru": "Богатый купец носил дорогое шелковое <платье>, свидетельствовавшее о его статусе."
                            }
                        },
                        {
                            "text": "Jego długa <suknia> szeleściła cicho przy każdym kroku stawianym na posadzce.",
                            "target_lang_translations": {
                                "ru": "Его длинное <одеяние> тихо шелестело при каждом шаге по полу."
                            }
                        }
                    ],
                    "synonyms": [
                        "szata",
                        "ubiór",
                        "odzienie"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название верхней мужской или женской одежды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одеяние",
                                "target_lang_sense_clarification": "торжественная или старинная одежда"
                            },
                            {
                                "target_lang_word": "платье",
                                "target_lang_sense_clarification": "устар. одежда вообще"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        {
                            "text": "The morning was <w>crisp</w> and sunny, perfect for a hike.",
                            "target_lang_translations": {
                                "ru": "Утро было <w>свежим</w> и солнечным, идеальным для похода."
                            }
                        }
                    ],
                    "synonyms": [
                        "chip",
                        "potato chip"
                    ],
                    "common_phrases": [
                        "crisp air",
                        "crisp morning",
                        "crisp autumn day"
                    ],
                    "target_lang_definitions": {
                        "ru": "О воздухе или погоде: приятно прохладный, сухой и бодрящий."

{
    "word_family": [
        "registratie",
        "register",
        "registrator"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f52d386c-8be8-37eb-a86f-6da5c11006b8",
                    "sense_definition": "Officieel vastleggen in een lijst, systeem of administratie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "recording_data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet je bij de gemeente <w>registreren</w> als je gaat verhuizen.",
                            "target_lang_translations": {
                                "en": "You must <w>register</w> with the municipality when you move.",
                                "ru": "Вы должны <w>зарегистрироваться</w> в муниципалитете при переезде."
                            }
                        },
                        {
                            "text": "Alle nieuwe studenten worden in het computersysteem <w>geregistreerd</w>.",
                            "target_lang_translations": {
                                "en": "All new students are <w>registered</w> in the computer system.",
                                "ru": "Все новые студенты <w>регистрируются</w> в компьютерной системе."
                            }
                        }
                    ],
                    "synonyms": [
                        "inschrijven",
                        "vastleggen",
                        "noteren"
                    ],
                    "antonyms": [
                        "uitschrijven",
                        "wissen"
                    ],
                    "common_phrases": [
                        "zich laten registreren",
                        "gegevens registreren",
                        "een klacht registreren"
                    ],
                    "target_lang_definitions": {
                        "en": "To officially record in a list, system, or administration.",
                        "ru": "Официально вносить в список, систему или реестр."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "to enter into an official list"
                            },
                            {
                                "target_lang_word": "sign up",
                                "target_lang_sense_clarification": "to enroll oneself"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "регистрировать",
                                "target_lang_sense_clarification": "вносить в список или систему"
                            },
                            {
                                "target_lang_word": "регистрироваться",
                                "target_lang_sense_clarification": "вставать на учёт (возвратный глагол)"
                            },
                            {
                                "target_lang_word": "записывать",
                                "target_lang_sense_clarification": "вносить данные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f70093bf-a83d-320a-b8ad-5ea663677472",
                    "sense_definition": "Gegevens meten en opslaan met behulp van een apparaat of instrument.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "recording_data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De camera <w>registreerde</w> dat de auto door rood reed.",
                            "target_lang_translations": {
                                "en": "The camera <w>recorded</w> that the car drove through a red light.",
                                "ru": "Камера <w>зафиксировала</w>, что автомобиль проехал на красный свет."
                            }
                        },
                        {
                            "text": "Dit apparaat <w>registreert</w> zelfs de kleinste veranderingen in temperatuur.",
                            "target_lang_translations": {
                                "en": "This device <w>registers</w> even the smallest changes in temperature.",
                                "ru": "Это устройство <w>регистрирует</w> даже малейшие изменения температуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "meten",
                        "opnemen",
                        "detecteren"
                    ],
                    "common_phrases": [
                        "een schok registreren",
                        "beweging registreren",
                        "snelheid registreren"
                    ],
                    "target_lang_definitions": {
                        "en": "To measure and store data using a device or instrument.",
                        "ru": "Измерять и сохранять данные с помощью устройства или инструмента."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "record",
                                "target_lang_sense_clarification": "to capture data or events"
                            },
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "to show a measurement or reading"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "фиксировать",
                                "target_lang_sense_clarification": "запечатлевать событие или данные"
                            },
                            {
                                "target_lang_word": "регистрировать",
                                "target_lang_sense_clarification": "отмечать показания прибора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db7ecc73-70fe-36ae-98a3-196561cb2ab7",
                    "sense_definition": "Iets bewust waarnemen, opmerken of onthouden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was zo moe dat hij nauwelijks <w>registreerde</w> wat er om hem heen gebeurde.",
                            "target_lang_translations": {
                                "en": "He was so tired that he barely <w>registered</w> what was happening around him.",
                                "ru": "Он так устал, что едва <w>воспринимал</w> происходящее вокруг."
                            }
                        },
                        {
                            "text": "Haar ogen <w>registreerden</w> de angst op het gezicht van de man.",
                            "target_lang_translations": {
                                "en": "Her eyes <w>registered</w> the fear on the man's face.",
                                "ru": "Ее глаза <w>заметили</w> страх на лице мужчины."
                            }
                        }
                    ],
                    "synonyms": [
                        "waarnemen",
                        "opmerken",
                        "constateren"
                    ],
                    "antonyms": [
                        "negeren",
                        "missen"
                    ],
                    "common_phrases": [
                        "nauwelijks registreren",
                        "bewust registreren"
                    ],
                    "target_lang_definitions": {
                        "en": "To consciously perceive, notice, or remember something.",
                        "ru": "Осознанно воспринимать, замечать или запоминать что-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "to mentally process or realize"
                            },
                            {
                                "target_lang_word": "notice",
                                "target_lang_sense_clarification": "to become aware of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "воспринимать",
                                "target_lang_sense_clarification": "осознавать происходящее"
                            },
                            {
                                "target_lang_word": "замечать",
                                "target_lang_sense_clarification": "обращать внимание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c018c0e-c92c-3af0-93a3-8abe3d95c55f",
                    "sense_definition": "De registers van een orgel instellen om de klankkleur te bepalen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De organist moet het orgel zorgvuldig <w>registreren</w> voor dit specifieke muziekstuk.",
                            "target_lang_translations": {
                                "en": "The organist must carefully <w>register</w> the organ for this specific piece of music.",
                                "ru": "Органист должен тщательно <w>подобрать регистры</w> органа для этого конкретного произведения."
                            }
                        },
                        {
                            "text": "Het is een kunst om een groot kerkorgel goed te <w>registreren</w>.",
                            "target_lang_translations": {
                                "en": "It is an art to <w>register</w> a large church organ well.",
                                "ru": "Это искусство — правильно <w>выбирать регистры</w> большого церковного органа."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To set the stops of an organ to determine the timbre.",
                        "ru": "Настраивать регистры органа для определения тембра."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "to select organ stops"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выбирать регистры",
                                "target_lang_sense_clarification": "настраивать звучание органа"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
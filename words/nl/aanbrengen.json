{
    "word_family": [
        "aanbrenger",
        "aanbrenging"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "15536719-c899-3d5c-8242-23d1b09db101",
                    "sense_definition": "Een stof, zoals verf, zalf of cosmetica, op een oppervlak smeren of doen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "application",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de zonnebrandcrème gelijkmatig <w>aanbrengen</w> om verbranding te voorkomen.",
                            "target_lang_translations": {
                                "ru": "Ты должен равномерно <w>нанести</w> солнцезащитный крем, чтобы предотвратить ожоги.",
                                "en": "You must <w>apply</w> the sunscreen evenly to prevent burning."
                            }
                        },
                        {
                            "text": "Zij gaat elke ochtend zorgvuldig haar make-up <w>aanbrengen</w> voor de spiegel.",
                            "target_lang_translations": {
                                "ru": "Каждое утро она собирается тщательно <w>наносить</w> макияж перед зеркалом.",
                                "en": "She is going to carefully <w>apply</w> her makeup in front of the mirror every morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "opsmeren",
                        "opdoen"
                    ],
                    "antonyms": [
                        "verwijderen",
                        "weghalen"
                    ],
                    "common_phrases": [
                        "make-up aanbrengen",
                        "een laag aanbrengen",
                        "verf aanbrengen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намазывать или накладывать вещество (например, краску, мазь или косметику) на поверхность.",
                        "en": "To spread or put a substance (like paint, ointment, or cosmetics) on a surface."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наносить",
                                "target_lang_sense_clarification": "мазь, краску, макияж"
                            },
                            {
                                "target_lang_word": "накладывать",
                                "target_lang_sense_clarification": "слой чего-либо"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "paint, cream, cosmetics"
                            },
                            {
                                "target_lang_word": "put on",
                                "target_lang_sense_clarification": "makeup, lotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76c7bc12-7dec-337e-9223-293e191af9d7",
                    "sense_definition": "Iets vastmaken, installeren of tot stand brengen (vaak gebruikt bij wijzigingen, verbeteringen of schade).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "installation_modification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente gaat nieuwe straatverlichting <w>aanbrengen</w> in onze wijk.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет собирается <w>установить</w> новое уличное освещение в нашем районе.",
                                "en": "The municipality is going to <w>install</w> new street lighting in our neighborhood."
                            }
                        },
                        {
                            "text": "We moeten enkele noodzakelijke verbeteringen <w>aanbrengen</w> aan het oude gebouw.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>внести</w> некоторые необходимые улучшения в старое здание.",
                                "en": "We need to <w>make</w> some necessary improvements to the old building."
                            }
                        },
                        {
                            "text": "De storm heeft veel schade <w>aangebracht</w> aan de daken van de huizen.",
                            "target_lang_translations": {
                                "ru": "Шторм <w>нанес</w> большой ущерб крышам домов.",
                                "en": "The storm has <w>caused</w> extensive damage to the roofs of the houses."
                            }
                        }
                    ],
                    "synonyms": [
                        "bevestigen",
                        "installeren",
                        "monteren",
                        "plaatsen"
                    ],
                    "antonyms": [
                        "verwijderen",
                        "demonteren"
                    ],
                    "common_phrases": [
                        "veranderingen aanbrengen",
                        "verbeteringen aanbrengen",
                        "schade aanbrengen",
                        "voorzieningen aanbrengen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устанавливать, прикреплять что-либо или производить изменения/ущерб.",
                        "en": "To attach, install, or bring about (often used with changes, improvements, or damage)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "оборудование, детали"
                            },
                            {
                                "target_lang_word": "вносить",
                                "target_lang_sense_clarification": "изменения, улучшения"
                            },
                            {
                                "target_lang_word": "наносить",
                                "target_lang_sense_clarification": "ущерб, вред"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "install",
                                "target_lang_sense_clarification": "lighting, equipment"
                            },
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "changes, improvements"
                            },
                            {
                                "target_lang_word": "cause",
                                "target_lang_sense_clarification": "damage, harm"
                            },
                            {
                                "target_lang_word": "fit",
                                "target_lang_sense_clarification": "locks, parts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1c470a1-bf43-3ff2-b7ab-b35485569060",
                    "sense_definition": "Nieuwe personen werven of aandragen voor een organisatie of vereniging.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "recruitment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je een nieuwe klant kunt <w>aanbrengen</w>, ontvang je een aantrekkelijke bonus.",
                            "target_lang_translations": {
                                "ru": "Если вы сможете <w>привлечь</w> нового клиента, вы получите привлекательный бонус.",
                                "en": "If you can <w>bring in</w> a new client, you will receive an attractive bonus."
                            }
                        },
                        {
                            "text": "De vereniging vraagt haar leden om actief nieuwe donateurs <w>aan te brengen</w>.",
                            "target_lang_translations": {
                                "ru": "Ассоциация просит своих членов активно <w>привлекать</w> новых доноров.",
                                "en": "The association asks its members to actively <w>recruit</w> new donors."
                            }
                        }
                    ],
                    "synonyms": [
                        "werven",
                        "rekruteren",
                        "aandragen"
                    ],
                    "common_phrases": [
                        "nieuwe leden aanbrengen",
                        "klanten aanbrengen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привлекать или предлагать новых людей для организации.",
                        "en": "To recruit or propose new people for an organization."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привлекать",
                                "target_lang_sense_clarification": "клиентов, участников"
                            },
                            {
                                "target_lang_word": "приводить",
                                "target_lang_sense_clarification": "новых членов"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "bring in",
                                "target_lang_sense_clarification": "clients, members"
                            },
                            {
                                "target_lang_word": "recruit",
                                "target_lang_sense_clarification": "new members, donors"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "453444eb-91e0-3f5f-b6f8-c0be772849d4",
                    "sense_definition": "Iemand aangeven bij een autoriteit omdat diegene iets strafbaars heeft gedaan.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "reporting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij besloot zijn medeplichtige bij de politie <w>aan te brengen</w> in ruil voor strafvermindering.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>сдать</w> своего сообщника полиции в обмен на смягчение наказания.",
                                "en": "He decided to <w>turn in</w> his accomplice to the police in exchange for a reduced sentence."
                            }
                        },
                        {
                            "text": "Niemand in de buurt durfde de dader <w>aan te brengen</w> uit angst voor wraakacties.",
                            "target_lang_translations": {
                                "ru": "Никто в округе не осмелился <w>донести</w> на преступника из страха мести.",
                                "en": "No one in the neighborhood dared to <w>report</w> the perpetrator for fear of retaliation."
                            }
                        }
                    ],
                    "synonyms": [
                        "aangeven",
                        "verklikken",
                        "verlinken"
                    ],
                    "antonyms": [
                        "verzwijgen"
                    ],
                    "common_phrases": [
                        "iemand aanbrengen",
                        "de dader aanbrengen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщать властям о чьем-либо правонарушении.",
                        "en": "To report someone to the authorities for a crime or wrongdoing."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доносить",
                                "target_lang_sense_clarification": "на кого-то властям"
                            },
                            {
                                "target_lang_word": "сдать",
                                "target_lang_sense_clarification": "выдать полиции"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "report",
                                "target_lang_sense_clarification": "someone to authorities"
                            },
                            {
                                "target_lang_word": "inform on",
                                "target_lang_sense_clarification": "an accomplice"
                            },
                            {
                                "target_lang_word": "turn in",
                                "target_lang_sense_clarification": "to the police"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "33be7819-b137-3f95-a5b9-8e850c6844cc",
                    "sense_definition": "Utwór audiowizualny składający się z serii obrazów, które dają wrażenie ruchu, zazwyczaj wyświetlany w kinie lub telewizji.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cinema_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wieczorem obejrzeliśmy bardzo ciekawy <w>film</w> w telewizji.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы посмотрели очень интересный <w>фильм</w> по телевизору."
                            }
                        },
                        {
                            "text": "Ten <w>film</w> opowiada o przygodach grupy przyjaciół w górach.",
                            "target_lang_translations": {
                                "ru": "Этот <w>фильм</w> рассказывает о приключениях группы друзей в горах."
                            }
                        }
                    ],
                    "synonyms": [
                        "obraz",
                        "kino"
                    ],
                    "common_phrases": [
                        "oglądać film",
                        "film akcji",
                        "kręcić film",
                        "film fabularny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аудиовизуальное произведение, состоящее из серии изображений, создающих впечатление движения, обычно показываемое в кинотеатре или по телевидению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фильм",
                                "target_lang_sense_clarification": "произведение"
                            },
                            {
                                "target_lang_word": "кинофильм",
                                "target_lang_sense_clarification": "полная форма"
                            },
                            {
                                "target_lang_word": "кинокартина",
                                "target_lang_sense_clarification": "художественное произведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c43b979-1939-3790-aff9-895a42cb344f",
                    "sense_definition": "Dziedzina sztuki i przemysłu zajmująca się produkcją utworów audiowizualnych; kinematografia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cinema_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat studiuje <w>film</w> i chce zostać sławnym reżyserem.",
                            "target_lang_translations": {
                                "ru": "Мой брат изучает <w>кино</w> и хочет стать известным режиссёром."
                            }
                        },
                        {
                            "text": "Polska szkoła <w>filmu</w> jest ceniona przez krytyków na całym świecie.",
                            "target_lang_translations": {
                                "ru": "Польская школа <w>кино</w> ценится критиками во всём мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "kino",
                        "dziesiąta muza"
                    ],
                    "common_phrases": [
                        "historia filmu",
                        "festiwal filmu",
                        "krytyk filmu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Область искусства и индустрии, занимающаяся производством аудиовизуальных произведений; кинематограф."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кино",
                                "target_lang_sense_clarification": "искусство"
                            },
                            {
                                "target_lang_word": "кинематограф",
                                "target_lang_sense_clarification": "индустрия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ec72a6f-8c9b-32ba-a747-67200f4b5d17",
                    "sense_definition": "Światłoczuła taśma używana w tradycyjnych aparatach fotograficznych i kamerach do zapisu obrazu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "material_medium",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zrobiłem na wakacjach tyle zdjęć, że szybko skończył mi się <w>film</w>.",
                            "target_lang_translations": {
                                "ru": "Я сделал в отпуске столько фотографий, что у меня быстро закончилась <w>плёнка</w>."
                            }
                        },
                        {
                            "text": "Muszę zanieść ten stary <w>film</w> do fotografa, żeby go wywołać.",
                            "target_lang_translations": {
                                "ru": "Мне нужно отнести эту старую <w>плёнку</w> фотографу, чтобы её проявить."
                            }
                        }
                    ],
                    "synonyms": [
                        "klisza",
                        "taśma filmowa"
                    ],
                    "common_phrases": [
                        "wywołać film",
                        "rolka filmu",
                        "założyć film"
                    ],
                    "target_lang_definitions": {
                        "ru": "Светочувствительная лента, используемая в традиционных фотоаппаратах и камерах для записи изображения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плёнка",
                                "target_lang_sense_clarification": "носитель"
                            },
                            {
                                "target_lang_word": "фотоплёнка",
                                "target_lang_sense_clarification": "для фото"
                            },
                            {
                                "target_lang_word": "киноплёнка",
                                "target_lang_sense_clarification": "для кино"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f9032c3-f1d4-31f2-8f4f-bb2828148ee3",
                    "sense_definition": "Cienka, często przejrzysta warstwa jakiejś substancji pokrywająca powierzchnię.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "thin_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na powierzchni wody w porcie unosił się cienki <w>film</w> oleju.",
                            "target_lang_translations": {
                                "ru": "На поверхности воды в порту плавала тонкая масляная <w>плёнка</w>."
                            }
                        },
                        {
                            "text": "Specjalny <w>film</w> ochronny zabezpiecza ekran telefonu przed zarysowaniami.",
                            "target_lang_translations": {
                                "ru": "Специальная защитная <w>плёнка</w> предохраняет экран телефона от царапин."
                            }
                        }
                    ],
                    "synonyms": [
                        "błona",
                        "warstewka"
                    ],
                    "common_phrases": [
                        "film olejowy",
                        "film łzowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкий, часто прозрачный слой какого-либо вещества, покрывающий поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плёнка",
                                "target_lang_sense_clarification": "слой"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "0005f2b6-637b-3fd0-907f-0bcd4ec8be86",
                    "sense_definition": "A group of kittens born to the same mother.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_birth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We found a <w>kindle</w> of kittens sleeping in the barn.",
                            "target_lang_translations": {
                                "ru": "Мы нашли <w>выводок</w> котят, спящих в сарае."
                            }
                        },
                        {
                            "text": "The mother cat protected her <w>kindle</w> from the loud noise.",
                            "target_lang_translations": {
                                "ru": "Кошка-мать защищала свой <w>выводок</w> от громкого шума."
                            }
                        }
                    ],
                    "synonyms": [
                        "litter"
                    ],
                    "common_phrases": [
                        "a kindle of kittens"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа котят, рожденных одной матерью (выводок)."
                    },
                    "translations": {

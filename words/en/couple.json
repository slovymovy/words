{
    "word_family": [
        "couplet",
        "coupling",
        "uncouple",
        "coupler"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6176bd59-7e8b-35cd-9cee-6925c3a48a00",
                    "sense_definition": "Two people who are married or in a romantic relationship.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pair_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The happy <w>couple</w> walked hand in hand along the beach.",
                            "target_lang_translations": {
                                "ru": "Счастливая <w>пара</w> шла по пляжу, держась за руки."
                            }
                        },
                        {
                            "text": "A young <w>couple</w> moved into the house next door.",
                            "target_lang_translations": {
                                "ru": "Молодая <w>пара</w> въехала в соседний дом."
                            }
                        }
                    ],
                    "synonyms": [
                        "pair",
                        "duo",
                        "twosome"
                    ],
                    "common_phrases": [
                        "married couple",
                        "young couple",
                        "happy couple",
                        "elderly couple"
                    ],
                    "target_lang_definitions": {
                        "ru": "Два человека, состоящие в браке или романтических отношениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "па́ра",
                                "target_lang_sense_clarification": "партнеры"
                            },
                            {
                                "target_lang_word": "чета́",
                                "target_lang_sense_clarification": "супружеская (офиц./книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "628cab0d-10d5-3db8-af81-6e2f9a5895e5",
                    "sense_definition": "A small number of things or people; a few (usually followed by 'of').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need a <w>couple</w> of minutes to finish this report.",
                            "target_lang_translations": {
                                "ru": "Мне нужна <w>пара</w> минут, чтобы закончить этот отчет."
                            }
                        },
                        {
                            "text": "We only have a <w>couple</w> of dollars left.",
                            "target_lang_translations": {
                                "ru": "У нас осталась только <w>пара</w> долларов."
                            }
                        }
                    ],
                    "synonyms": [
                        "few",
                        "handful"
                    ],
                    "common_phrases": [
                        "a couple of",
                        "couple of days",
                        "couple of times"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое количество людей или вещей; несколько."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "па́ра",
                                "target_lang_sense_clarification": "небольшое количество"
                            },
                            {
                                "target_lang_word": "не́сколько",
                                "target_lang_sense_clarification": "неопределенное число"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56357a6e-128d-344a-ba87-ca7cf2830941",
                    "sense_definition": "Two items of the same kind that are connected or considered together.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "pair_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>couple</w> of police officers stood at the entrance.",
                            "target_lang_translations": {
                                "ru": "<w>Пара</w> полицейских стояла у входа."
                            }
                        },
                        {
                            "text": "He packed a <w>couple</w> of shirts for the overnight trip.",
                            "target_lang_translations": {
                                "ru": "Он взял с собой <w>пару</w> рубашек для поездки с ночевкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "pair",
                        "brace"
                    ],
                    "common_phrases": [
                        "a couple"
                    ],
                    "target_lang_definitions": {
                        "ru": "Два предмета одного вида, соединенные или рассматриваемые вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "па́ра",
                                "target_lang_sense_clarification": "комплект из двух"
                            },
                            {
                                "target_lang_word": "дво́е",
                                "target_lang_sense_clarification": "собирательное числительное (о людях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86279044-986d-3ec6-a936-2c76e56e7c36",
                    "sense_definition": "A pair of equal and parallel forces acting in opposite directions and producing rotation.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physics_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The forces created a <w>couple</w> that caused the beam to rotate.",
                            "target_lang_translations": {
                                "ru": "Силы создали <w>пару</w>, которая заставила балку вращаться."
                            }
                        },
                        {
                            "text": "In mechanics, a <w>couple</w> produces a turning effect without a net force.",
                            "target_lang_translations": {
                                "ru": "В механике <w>пара</w> создает вращательный эффект без результирующей силы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "physics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пара равных и параллельных сил, действующих в противоположных направлениях и вызывающих вращение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "па́ра сил",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "212e22d1-b19c-3b63-877a-a63345697844",
                    "sense_definition": "To join, link, or connect one thing to another.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "join_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The railway worker will <w>couple</w> the wagons to the engine.",
                            "target_lang_translations": {
                                "ru": "Железнодорожник <w>сцепит</w> вагоны с локомотивом."
                            }
                        },
                        {
                            "text": "High inflation was <w>coupled</w> with low economic growth.",
                            "target_lang_translations": {
                                "ru": "Высокая инфляция <w>сочеталась</w> с низким экономическим ростом."
                            }
                        }
                    ],
                    "synonyms": [
                        "join",
                        "attach",
                        "put together",
                        "affix"
                    ],
                    "common_phrases": [
                        "couple with",
                        "couple together"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединять, связывать или сцеплять одно с другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сцепля́ть",
                                "target_lang_sense_clarification": "вагоны, механизмы"
                            },
                            {
                                "target_lang_word": "соединя́ть",
                                "target_lang_sense_clarification": "связывать вместе"
                            },
                            {
                                "target_lang_word": "сочета́ть",
                                "target_lang_sense_clarification": "комбинировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f17dec18-6d98-3e7b-b190-f73a83d1461b",
                    "sense_definition": "To engage in sexual intercourse.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sex_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many animals <w>couple</w> only during a specific breeding season.",
                            "target_lang_translations": {
                                "ru": "Многие животные <w>спариваются</w> только в определенный сезон размножения."
                            }
                        },
                        {
                            "text": "The birds were observed <w>coupling</w> in the trees.",
                            "target_lang_translations": {
                                "ru": "Наблюдали, как птицы <w>спаривались</w> на деревьях."
                            }
                        }
                    ],
                    "synonyms": [
                        "copulate",
                        "have sex",
                        "make love"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "biological or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступать в половую связь (обычно о животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спа́риваться",
                                "target_lang_sense_clarification": "о животных"
                            },
                            {
                                "target_lang_word": "совокупля́ться",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "63f2dbba-7d3e-31dc-bbbf-b0a9b51b039e",
                    "sense_definition": "Two or a small number of (used directly before a noun without 'of').",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will be there in a <w>couple</w> minutes.",
                            "target_lang_translations": {
                                "ru": "Я буду там через <w>пару</w> минут."
                            }
                        },
                        {
                            "text": "We just need a <w>couple</w> more chairs for the table.",
                            "target_lang_translations": {
                                "ru": "Нам нужно еще <w>пару</w> стульев для стола."
                            }
                        }
                    ],
                    "synonyms": [
                        "few"
                    ],
                    "common_phrases": [
                        "couple minutes",
                        "couple things",
                        "couple times"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informal usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Два или небольшое количество (используется в разговорной речи перед существительным)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "па́ра",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
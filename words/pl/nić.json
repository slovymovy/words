    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "58d6b757-a0a5-300c-a18d-754c70214a63",
                    "sense_definition": "Cienkie, długie i elastyczne włókno wykonane z przędzy, używane głównie do szycia, cerowania lub tkactwa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_thread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krawcowa nawlekła czerwoną <w>nić</w> na igłę, aby zaszyć dziurę.",
                            "target_lang_translations": {
                                "ru": "Портниха вдела красную <w>нитку</w> в иголку, чтобы зашить дырку."
                            }
                        },
                        {
                            "text": "Podczas szycia pękła mi <w>nić</w> i musiałam zaczynać od nowa.",
                            "target_lang_translations": {
                                "ru": "Во время шитья у меня порвалась <w>нитка</w>, и мне пришлось начинать заново."
                            }
                        }
                    ],
                    "synonyms": [
                        "przędza",
                        "włókno"
                    ],
                    "common_phrases": [
                        "nawlec nić",
                        "igła z nicią",
                        "szyć nicią",
                        "kłębek nici"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкое, длинное и гибкое волокно из пряжи, используемое в основном для шитья, штопки или ткачества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нитка",
                                "target_lang_sense_clarification": "для шитья"
                            },
                            {
                                "target_lang_word": "нить",
                                "target_lang_sense_clarification": "текстильная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47ad12d4-7404-3058-aad8-30c9175981d8",
                    "sense_definition": "Długi, cienki obiekt, struktura lub struga przypominająca wyglądem włókno do szycia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_thread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naukowcy badają podwójną <w>nić</w> DNA pod mikroskopem.",
                            "target_lang_translations": {
                                "ru": "Ученые исследуют двойную <w>нить</w> ДНК под микроскопом."
                            }
                        },
                        {
                            "text": "W rogu pokoju wisiała długa, srebrzysta pajęcza <w>nić</w>.",
                            "target_lang_translations": {
                                "ru": "В углу комнаты висела длинная серебристая <w>нить</w> паутины."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasmo",
                        "włókno"
                    ],
                    "common_phrases": [
                        "nić DNA",
                        "nić pajęcza",
                        "nić życia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный тонкий объект, структура или полоса, напоминающая внешним видом швейное волокно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нить",
                                "target_lang_sense_clarification": "структура, ДНК, паутина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a9c4868-3061-31d4-a57a-5eec13f5a00c",
                    "sense_definition": "Potoczne określenie braku rezultatu lub niepowodzenia planów, używane wyłącznie w liczbie mnogiej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "idiomatic_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieliśmy iść na spacer, ale <w>nici</w> z tego, bo zaczęło padać.",
                            "target_lang_translations": {
                                "ru": "Мы собирались пойти на прогулку, но из этого <w>ничего</w> не вышло, так как пошел дождь."
                            }
                        },
                        {
                            "text": "Obiecywał, że naprawi samochód, ale ostatecznie wyszły z tego <w>nici</w>.",
                            "target_lang_translations": {
                                "ru": "Он обещал, что починит машину, но в итоге из этого <w>ничего</w> не вышло."
                            }
                        }
                    ],
                    "synonyms": [
                        "fiasko",
                        "klapa"
                    ],
                    "antonyms": [
                        "sukces",
                        "powodzenie"
                    ],
                    "common_phrases": [
                        "nici z tego",
                        "nici z planów",
                        "nici z wyjazdu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts"
                        },
                        {
                            "trait_type": "form",
                            "comment": "always plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное выражение, означающее отсутствие результата или срыв планов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ничего",
                                "target_lang_sense_clarification": "отсутствие результата"
                            },
                            {
                                "target_lang_word": "провал",
                                "target_lang_sense_clarification": "неудача"
                            }
                        ]
                    }
                }
            ]

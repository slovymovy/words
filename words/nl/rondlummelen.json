{
    "word_family": [
        "lummelen",
        "lummel"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "814746cd-e5fa-32da-bcdc-d334c40e864f",
                    "sense_definition": "Ergens lui rondhangen zonder iets nuttigs te doen",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "idling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet niet de hele dag op de bank blijven <w>rondlummelen</w>.",
                            "target_lang_translations": {
                                "en": "You shouldn't just <w>lounge about</w> on the couch all day.",
                                "ru": "Ты не должен весь день <w>валяться</w> на диване."
                            }
                        },
                        {
                            "text": "De jongeren stonden wat te <w>rondlummelen</w> bij de ingang van de winkel.",
                            "target_lang_translations": {
                                "en": "The youths were <w>loitering</w> a bit at the entrance of the shop.",
                                "ru": "Молодежь немного <w>околачивалась</w> у входа в магазин."
                            }
                        }
                    ],
                    "synonyms": [
                        "rondhangen"
                    ],
                    "common_phrases": [
                        "wat rondlummelen",
                        "blijven rondlummelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To hang around lazily somewhere without doing anything useful",
                        "ru": "Лениво слоняться где-либо, не делая ничего полезного"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "loaf around",
                                "target_lang_sense_clarification": "general laziness"
                            },
                            {
                                "target_lang_word": "lounge about",
                                "target_lang_sense_clarification": "relaxing lazily, often sitting or lying down"
                            },
                            {
                                "target_lang_word": "loiter",
                                "target_lang_sense_clarification": "standing around without apparent purpose"
                            },
                            {
                                "target_lang_word": "hang around",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "слоняться без дела",
                                "target_lang_sense_clarification": "бесцельно бродить"
                            },
                            {
                                "target_lang_word": "околачиваться",
                                "target_lang_sense_clarification": "находиться где-то без дела, разговорное"
                            },
                            {
                                "target_lang_word": "бездельничать",
                                "target_lang_sense_clarification": "ничего не делать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b93baf96-271c-3f67-b5fa-02e2a934096c",
                    "sense_definition": "Tijdelijk niet meespelen bij sommige kaartspelen",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat hij de vorige ronde verloor, moest hij een beurt <w>rondlummelen</w>.",
                            "target_lang_translations": {
                                "en": "Because he lost the previous round, he had to <w>sit out</w> a turn.",
                                "ru": "Так как он проиграл прошлый раунд, ему пришлось один кон <w>пропустить</w>."
                            }
                        },
                        {
                            "text": "Bij deze variant van het spel moet de deler soms <w>rondlummelen</w>.",
                            "target_lang_translations": {
                                "en": "In this variant of the game, the dealer sometimes has to <w>sit out</w>.",
                                "ru": "В этом варианте игры сдающий иногда должен <w>пропускать ход</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To temporarily not participate in certain card games",
                        "ru": "Временно не участвовать в некоторых карточных играх"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sit out",
                                "target_lang_sense_clarification": "miss a turn in a game"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пропускать ход",
                                "target_lang_sense_clarification": "в игре"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
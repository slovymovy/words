{
    "word_family": [
        "gifted",
        "gifter"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5cafdb44-ece2-3198-942f-20d701f48ed3",
                    "sense_definition": "Something given to another voluntarily, without charge.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "gift_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She gave him a cell phone as a birthday <w>gift</w>.",
                            "target_lang_translations": {
                                "ru": "Она преподнесла ему мобильный телефон как <w>подарок</w> на день рождения."
                            }
                        },
                        {
                            "text": "I received a lovely <w>gift</w> from my grandmother yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера я получил чудесный <w>подарок</w> от бабушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "present"
                    ],
                    "common_phrases": [
                        "birthday gift",
                        "Christmas gift",
                        "free gift",
                        "generous gift"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещь, которую дарят кому-либо добровольно и безвозмездно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пода́рок",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "дар",
                                "target_lang_sense_clarification": "высокий стиль"
                            },
                            {
                                "target_lang_word": "гости́нец",
                                "target_lang_sense_clarification": "обычно сладости или сувенир"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb98179a-036e-3541-b988-e14a9131742a",
                    "sense_definition": "A talent or natural ability.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gift_talent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She had a <w>gift</w> for playing the flute.",
                            "target_lang_translations": {
                                "ru": "У неё был <w>дар</w> игры на флейте."
                            }
                        },
                        {
                            "text": "He has a natural <w>gift</w> for making people laugh.",
                            "target_lang_translations": {
                                "ru": "У него природный <w>дар</w> смешить людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "talent",
                        "ability",
                        "aptitude",
                        "knack"
                    ],
                    "common_phrases": [
                        "gift for",
                        "natural gift",
                        "have a gift"
                    ],
                    "target_lang_definitions": {
                        "ru": "Талант или врождённая способность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дар",
                                "target_lang_sense_clarification": "природная способность"
                            },
                            {
                                "target_lang_word": "тала́нт",
                                "target_lang_sense_clarification": "выдающиеся способности"
                            },
                            {
                                "target_lang_word": "одарённость",
                                "target_lang_sense_clarification": "свойство личности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e6b316d-5f81-3168-9703-8784909505db",
                    "sense_definition": "Something gained incidentally, without effort.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gift_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The extra day off was a welcome <w>gift</w> after a hard week.",
                            "target_lang_translations": {
                                "ru": "Дополнительный выходной стал желанным <w>подарком</w> после тяжелой недели."
                            }
                        },
                        {
                            "text": "Finding that money on the street was a real <w>gift</w>.",
                            "target_lang_translations": {
                                "ru": "Найти эти деньги на улице было настоящим <w>подарком</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "boon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то полученное случайно, без усилий; приятная неожиданность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пода́рок",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "дар",
                                "target_lang_sense_clarification": "дар судьбы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b335a2b4-3fdc-3bfa-82c1-e75a4b394045",
                    "sense_definition": "The act, right, or power of giving or bestowing.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "gift_power",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The office is in the <w>gift</w> of the President.",
                            "target_lang_translations": {
                                "ru": "Эта должность находится в <w>распоряжении</w> президента."
                            }
                        },
                        {
                            "text": "This important decision is not within my <w>gift</w>.",
                            "target_lang_translations": {
                                "ru": "Это важное решение не в моей <w>власти</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in the gift of",
                        "within one's gift"
                    ],
                    "target_lang_definitions": {
                        "ru": "Право или возможность дарить, назначать или распределять что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "право дарения",
                                "target_lang_sense_clarification": "юридический/формальный контекст"
                            },
                            {
                                "target_lang_word": "распоряжение",
                                "target_lang_sense_clarification": "власть распределять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "445694a0-50a5-3280-af78-01e3ff71ea8a",
                    "sense_definition": "Acronym of gamete intrafallopian transfer.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "gift_medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor suggested <w>GIFT</w> as a possible fertility treatment.",
                            "target_lang_translations": {
                                "ru": "Врач предложил <w>ГИФТ</w> как возможный метод лечения бесплодия."
                            }
                        },
                        {
                            "text": "They read about the success rates of the <w>GIFT</w> procedure.",
                            "target_lang_translations": {
                                "ru": "Они читали о показателях успеха процедуры <w>ГИФТ</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "acronym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акроним для пересадки гамет в маточную трубу (метод лечения бесплодия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ГИФТ",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "95f893d1-1c99-3716-95c9-2ef23d02fa30",
                    "sense_definition": "To give as a gift or donation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gift_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to <w>gift</w> their old car to a local charity.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>подарить</w> свою старую машину местной благотворительной организации."
                            }
                        },
                        {
                            "text": "She was <w>gifted</w> a beautiful necklace for her anniversary.",
                            "target_lang_translations": {
                                "ru": "На годовщину ей <w>подарили</w> красивое ожерелье."
                            }
                        }
                    ],
                    "synonyms": [
                        "donate",
                        "give",
                        "contribute"
                    ],
                    "common_phrases": [
                        "gift wrap",
                        "gift away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать что-либо в качестве подарка или пожертвования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дари́ть",
                                "target_lang_sense_clarification": "несовершенный вид"
                            },
                            {
                                "target_lang_word": "подари́ть",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db9be403-cca9-3146-8983-387ef4214806",
                    "sense_definition": "To give away, to concede easily.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gift_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The goalkeeper <w>gifted</w> the opposing team an easy goal.",
                            "target_lang_translations": {
                                "ru": "Вратарь <w>подарил</w> команде противника легкий гол."
                            }
                        },
                        {
                            "text": "You should not <w>gift</w> your opponent any advantages in the negotiation.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>дарить</w> оппоненту никаких преимуществ в переговорах."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдать или уступить что-либо легко, без борьбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подари́ть",
                                "target_lang_sense_clarification": "уступить просто так"
                            },
                            {
                                "target_lang_word": "отда́ть",
                                "target_lang_sense_clarification": "уступить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
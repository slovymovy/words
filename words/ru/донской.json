    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "976367dd-776e-3bdd-817b-989b74e4913e",
                    "sense_definition": "Относящийся к реке Дон.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "don_geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы поехали на рыбалку и поймали отличную <w>донскую</w> рыбу для ухи.",
                            "target_lang_translations": {
                                "en": "We went fishing and caught excellent <w>Don</w> fish for the fish soup."
                            }
                        },
                        {
                            "text": "Тихая <w>донская</w> вода медленно текла мимо наших туристических палаток.",
                            "target_lang_translations": {
                                "en": "The quiet <w>Don</w> water flowed slowly past our camping tents."
                            }
                        }
                    ],
                    "common_phrases": [
                        "донская рыба",
                        "донская вода",
                        "донской берег"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the Don River."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Don",
                                "target_lang_sense_clarification": "relating to the river"
                            },
                            {
                                "target_lang_word": "of the Don",
                                "target_lang_sense_clarification": "genitive meaning"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34d40e77-98be-3011-80b3-02c2b199d172",
                    "sense_definition": "Относящийся к региону бассейна реки Дон, а также к казачеству этого края.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "don_geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой прадедушка был настоящий <w>донской</w> казак и отлично ездил верхом.",
                            "target_lang_translations": {
                                "en": "My great-grandfather was a real <w>Don</w> Cossack and rode horses excellently."
                            }
                        },
                        {
                            "text": "Бескрайняя <w>донская</w> степь раскинулась перед путешественниками во всей своей красе.",
                            "target_lang_translations": {
                                "en": "The endless <w>Don</w> steppe spread out before the travelers in all its glory."
                            }
                        }
                    ],
                    "common_phrases": [
                        "донской казак",
                        "донская степь",
                        "донской край",
                        "донская порода"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the Don basin region and the Cossacks of this area."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Don",
                                "target_lang_sense_clarification": "relating to the region or Cossacks"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "eb0b7531-aad0-3e5d-9751-827b98da2236",

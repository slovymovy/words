    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dcc8f260-db7a-3ced-8a68-187b95ef3476",
                    "sense_definition": "Взрослый человек мужского пола (в отличие от женщины или ребёнка).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "biological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В комнату вошёл высокий темноволосый <w>мужчина</w> в пальто.",
                            "target_lang_translations": {
                                "en": "A tall dark-haired <w>man</w> in a coat entered the room."
                            }
                        },
                        {
                            "text": "Этот <w>мужчина</w> помог мне донести тяжёлые сумки до дома.",
                            "target_lang_translations": {
                                "en": "This <w>man</w> helped me carry the heavy bags home."
                            }
                        }
                    ],
                    "synonyms": [
                        "мужик",
                        "сильный пол"
                    ],
                    "antonyms": [
                        "женщина",
                        "мальчик"
                    ],
                    "common_phrases": [
                        "настоящий мужчина",
                        "взрослый мужчина",
                        "красивый мужчина"
                    ],
                    "target_lang_definitions": {
                        "en": "An adult male human (as opposed to a woman or a child)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "man",
                                "target_lang_sense_clarification": "adult male human"
                            },
                            {
                                "target_lang_word": "male",
                                "target_lang_sense_clarification": "biological sex"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b3a0f17-e153-3169-9cb0-0e8ef524c412",
                    "sense_definition": "Обращение к незнакомому взрослому человеку мужского пола (обычно в разговорной речи).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Мужчина</w>, вы забыли свой зонтик на прилавке магазина!",
                            "target_lang_translations": {
                                "en": "<w>Sir</w>, you forgot your umbrella on the shop counter!"
                            }
                        },
                        {
                            "text": "Скажите, <w>мужчина</w>, этот автобус идёт до центра города?",
                            "target_lang_translations": {
                                "en": "Tell me, <w>sir</w>, does this bus go to the city center?"
                            }
                        }
                    ],
                    "synonyms": [
                        "гражданин"
                    ],
                    "antonyms": [
                        "женщина"
                    ],
                    "common_phrases": [
                        "послушайте, мужчина",
                        "извините, мужчина"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A form of address to an unfamiliar adult male (usually in spoken language)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sir",
                                "target_lang_sense_clarification": "polite address to a stranger"
                            },
                            {
                                "target_lang_word": "mister",
                                "target_lang_sense_clarification": "informal address"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b565829d-e53c-361e-bbce-358b1d0be703",
                    "sense_definition": "Человек, обладающий твёрдым характером, смелостью и другими качествами, традиционно приписываемыми мужскому полу.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой сложной ситуации он повёл себя как настоящий <w>мужчина</w>.",
                            "target_lang_translations": {
                                "en": "In this difficult situation, he behaved like a real <w>man</w>."
                            }
                        },
                        {
                            "text": "Перестань жаловаться на трудности и будь <w>мужчиной</w>.",
                            "target_lang_translations": {
                                "en": "Stop complaining about difficulties and be a <w>man</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "баба"
                    ],
                    "common_phrases": [
                        "будь мужчиной",
                        "поступить как мужчина"
                    ],
                    "target_lang_definitions": {
                        "en": "A person possessing strong character, courage, and other qualities traditionally attributed to the male sex."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "man",
                                "target_lang_sense_clarification": "person with masculine qualities"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01adef19-f6b1-338b-a8ef-ad7c33ad2f96",
                    "sense_definition": "Возлюбленный или сексуальный партнёр женщины.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После развода она долго не хотела впускать в свою жизнь нового <w>мужчину</w>.",
                            "target_lang_translations": {
                                "en": "After the divorce, she didn't want to let a new <w>man</w> into her life for a long time."
                            }
                        },
                        {
                            "text": "Для неё он был не просто другом, а единственным любимым <w>мужчиной</w>.",
                            "target_lang_translations": {
                                "en": "For her, he was not just a friend, but the only beloved <w>man</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "её новый мужчина",
                        "любимый мужчина",
                        "мой мужчина"
                    ],
                    "target_lang_definitions": {
                        "en": "A woman's beloved or sexual partner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "man",
                                "target_lang_sense_clarification": "romantic partner"
                            },
                            {
                                "target_lang_word": "boyfriend",
                                "target_lang_sense_clarification": "male partner"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4da7a28-b268-30fc-873d-5a95316ffdda",
                    "sense_definition": "Женщина, обладающая мужским характером или исполняющая мужские обязанности.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за своей твёрдости и решительности она была единственным <w>мужчиной</w> в семье.",
                            "target_lang_translations": {
                                "en": "Because of her firmness and determination, she was the only <w>man</w> in the family."
                            }
                        },
                        {
                            "text": "Коллеги шутили, что наш директор — самый сильный <w>мужчина</w> в коллективе, хотя она женщина.",
                            "target_lang_translations": {
                                "en": "Colleagues joked that our director is the strongest <w>man</w> in the team, even though she is a woman."
                            }
                        }
                    ],
                    "synonyms": [
                        "баба с яйцами"
                    ],
                    "target_lang_definitions": {
                        "en": "A woman possessing a masculine character or performing men's duties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "man",
                                "target_lang_sense_clarification": "metaphorical usage"
                            }
                        ]
                    }
                }
            ]
        }
    ]

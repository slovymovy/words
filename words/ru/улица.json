    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "befc0368-c4ea-392d-8e07-9a4bbe640773",
                    "sense_definition": "Часть населённого пункта, дорога с рядами домов по обеим сторонам.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На нашей <w>улице</w> открылся новый продуктовый магазин.",
                            "target_lang_translations": {
                                "en": "A new grocery store opened on our <w>street</w>."
                            }
                        },
                        {
                            "text": "Мы перешли <w>улицу</w> на зелёный сигнал светофора.",
                            "target_lang_translations": {
                                "en": "We crossed the <w>street</w> at the green traffic light."
                            }
                        }
                    ],
                    "common_phrases": [
                        "перейти улицу",
                        "центральная улица",
                        "узкая улица",
                        "название улицы"
                    ],
                    "target_lang_definitions": {
                        "en": "A public road in a city or town, typically with houses and buildings on one or both sides."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "street",
                                "target_lang_sense_clarification": "public road in a town"
                            },
                            {
                                "target_lang_word": "road",
                                "target_lang_sense_clarification": "thoroughfare for vehicles"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba11a5bc-76ed-373d-ba12-a7793e19d852",
                    "sense_definition": "Пространство вне дома, под открытым небом.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "outdoors",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой на <w>улице</w> часто идёт снег и дует ветер.",
                            "target_lang_translations": {
                                "en": "In winter, it often snows and is windy <w>outside</w>."
                            }
                        },
                        {
                            "text": "Мама позвала детей домой, потому что на <w>улице</w> стало темно.",
                            "target_lang_translations": {
                                "en": "Mom called the children home because it had gotten dark <w>outside</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "дом"
                    ],
                    "common_phrases": [
                        "на улице холодно",
                        "пойти на улицу",
                        "быть на улице",
                        "смотреть на улицу"
                    ],
                    "target_lang_definitions": {
                        "en": "The space outside of buildings; the open air."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "outside",
                                "target_lang_sense_clarification": "external environment"
                            },
                            {
                                "target_lang_word": "outdoors",
                                "target_lang_sense_clarification": "in the open air"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49a484ff-dc3d-30fa-8711-451c282571f8",
                    "sense_definition": "Внешняя среда, лишённая домашнего уюта или культурного влияния; отсутствие жилья.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_environment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После пожара семья осталась на <w>улице</w> без вещей и документов.",
                            "target_lang_translations": {
                                "en": "After the fire, the family was left on the <w>street</w> without belongings or documents."
                            }
                        },
                        {
                            "text": "Родители боялись, что влияние <w>улицы</w> испортит характер сына.",
                            "target_lang_translations": {
                                "en": "The parents were afraid that the influence of the <w>streets</w> would spoil their son's character."
                            }
                        }
                    ],
                    "common_phrases": [
                        "оказаться на улице",
                        "выбросить на улицу",
                        "человек улицы",
                        "законы улицы"
                    ],
                    "target_lang_definitions": {
                        "en": "The external environment characterized by a lack of home comforts or positive cultural influence; the state of being homeless."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "street",
                                "target_lang_sense_clarification": "social environment or state of homelessness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78b180ea-cab4-396f-ac55-ca999e2b1abd",
                    "sense_definition": "Жители домов, расположенных на одной улице.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "people_collective",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся <w>улица</w> вышла встречать знаменитого гостя.",
                            "target_lang_translations": {
                                "en": "The whole <w>street</w> came out to meet the famous guest."
                            }
                        },
                        {
                            "text": "Скоро об этой новости знала уже каждая <w>улица</w> в деревне.",
                            "target_lang_translations": {
                                "en": "Soon every <w>street</w> in the village already knew about this news."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вся улица знает",
                        "собрать всю улицу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The people who live on a particular street considered as a group."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "street",
                                "target_lang_sense_clarification": "residents of a street"
                            },
                            {
                                "target_lang_word": "neighborhood",
                                "target_lang_sense_clarification": "local community"
                            }
                        ]
                    }
                }
            ]
        }
    ]

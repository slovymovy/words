                    "frequency": "High",
                    "semantic_group_id": "artistic_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kunstenaar schilderde een prachtig <w>portret</w> van de koningin.",
                            "target_lang_translations": {
                                "ru": "Художник написал прекрасный <w>портрет</w> королевы."
                            }
                        },
                        {
                            "text": "In de gang hangt een oud <w>portret</w> van mijn grootvader.",
                            "target_lang_translations": {
                                "ru": "В коридоре висит старый <w>портрет</w> моего дедушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "beeltenis",
                        "afbeelding"
                    ],
                    "common_phrases": [

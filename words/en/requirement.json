                            },
                            {
                                "target_lang_word": "необходимость",
                                "target_lang_sense_clarification": "надобность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b98ebd48-2452-3cbf-9bb8-1e29e2edf9b3",
                    "sense_definition": "A specific constraint, standard, or specification that a system, product, or service must meet to be accepted.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "necessity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please check the system <w>requirements</w> before you install the software.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте системные <w>требования</w> перед установкой программного обеспечения."
                            }
                        },
                        {
                            "text": "The engineering team is reviewing the safety <w>requirements</w> for the new bridge.",
                            "target_lang_translations": {
                                "ru": "Инженерная группа пересматривает <w>требования</w> безопасности для нового моста."
                            }
                        }
                    ],
                    "synonyms": [
                        "specification",
                        "spec"
                    ],
                    "common_phrases": [
                        "system requirements",
                        "software requirements",
                        "user requirements"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное ограничение, стандарт или спецификация, которым должны соответствовать система, продукт или услуга."
                    },
                    "translations": {

                },
                {
                    "sense_id": "48dfa39c-21c7-3947-9d1c-00cad046ae2b",
                    "sense_definition": "Een grote, zeldzame loopvogel uit de familie Otididae.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "bird_species",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De grote <w>trap</w> komt in Nederland nauwelijks meer voor.",
                            "target_lang_translations": {
                                "en": "The great <w>bustard</w> is hardly found in the Netherlands anymore.",
                                "ru": "Большая <w>дрофа</w> в Нидерландах почти не встречается."
                            }
                        },
                        {
                            "text": "Vogelaars reizen ver om een wilde <w>trap</w> te kunnen zien.",
                            "target_lang_translations": {
                                "en": "Birdwatchers travel far to be able to see a wild <w>bustard</w>.",
                                "ru": "Орнитологи путешествуют далеко, чтобы увидеть дикую <w>дрофу</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A large, rare walking bird from the family Otididae.",
                        "ru": "Крупная редкая птица из семейства дрофиных."
                    },
                    "translations": {

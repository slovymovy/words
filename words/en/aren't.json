    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "febdcbb7-5b90-3df9-8f0c-9a0c69b83856",
                    "sense_definition": "Contraction of <w>are</w> <w>not</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>aren't</w> going to the cinema with us tonight.",
                            "target_lang_translations": {
                                "ru": "Они сегодня вечером <w>не</w> идут с нами в кино."
                            }
                        },
                        {
                            "text": "You <w>aren't</w> listening to a word I say.",
                            "target_lang_translations": {
                                "ru": "Ты <w>не</w> слушаешь ни слова из того, что я говорю."
                            }
                        }
                    ],
                    "synonyms": [
                        "ain’t"
                    ],
                    "antonyms": [
                        "are"
                    ],
                    "common_phrases": [
                        "aren't you",
                        "they aren't",
                        "we aren't",
                        "you aren't"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"are not\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не",
                                "target_lang_sense_clarification": "отрицательная частица перед глаголом или прилагательным"
                            },
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "отрицание наличия (в значении \"there aren't\")"
                            },
                            {
                                "target_lang_word": "не являются",
                                "target_lang_sense_clarification": "формальное отрицание состояния или свойства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "864eb8c4-6a4c-3b89-97a2-b0e1d606785c",
                    "sense_definition": "Contraction of <w>am</w> <w>not</w>, used primarily in questions.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am helpful, <w>aren't</w> I?",
                            "target_lang_translations": {
                                "ru": "Я полезен, <w>не так ли</w>?"
                            }
                        },
                        {
                            "text": "<w>Aren't</w> I supposed to wait here for the instructions?",
                            "target_lang_translations": {
                                "ru": "<w>Разве</w> я <w>не</w> должен ждать инструкций здесь?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ain't",
                        "amn't"
                    ],
                    "antonyms": [
                        "am"
                    ],
                    "common_phrases": [
                        "aren't I"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "used in questions like 'aren't I?'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"am not\", используемое преимущественно в вопросах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не так ли",
                                "target_lang_sense_clarification": "в разделительных вопросах (tag questions)"
                            },
                            {
                                "target_lang_word": "разве не",
                                "target_lang_sense_clarification": "в начале вопросительного предложения"
                            },
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "разговорный аналог в конце вопроса"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навреди́ть",
                                "target_lang_sense_clarification": "причинить вред или зло"
                            },
                            {
                                "target_lang_word": "ра́нить",
                                "target_lang_sense_clarification": "нанести физическую рану"
                            }
                        ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8314a39e-344b-3edc-beb7-6838718a4dae",
                    "sense_definition": "Wyrób cukierniczy sporządzany z miazgi kakaowej, tłuszczu i cukru, często z dodatkiem mleka lub orzechów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja ulubiona mleczna <w>czekolada</w> rozpływa się w ustach.",
                            "target_lang_translations": {
                                "ru": "Мой любимый молочный <w>шоколад</w> тает во рту."
                            }
                        },
                        {
                            "text": "Do tego przepisu potrzebna jest gorzka <w>czekolada</w> o wysokiej zawartości kakao.",
                            "target_lang_translations": {
                                "ru": "Для этого рецепта нужен горький <w>шоколад</w> с высоким содержанием какао."
                            }
                        }
                    ],
                    "synonyms": [
                        "czekolada twarda",
                        "gw. (Górny Śląsk) szekulada"
                    ],
                    "common_phrases": [
                        "gorzka czekolada",
                        "mleczna czekolada",
                        "biała czekolada",
                        "nadziewana czekolada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кондитерское изделие, приготовленное из какао-массы, жира и сахара, часто с добавлением молока или орехов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоколад",
                                "target_lang_sense_clarification": "кондитерское изделие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b451a244-86d5-3423-817f-aa38b68d08b3",
                    "sense_definition": "Pojedyncza tabliczka tego wyrobu cukierniczego, zazwyczaj w prostokątnym opakowaniu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krzyś jest łakomczuchem i zjadł całą <w>czekoladę</w> na raz.",
                            "target_lang_translations": {
                                "ru": "Кшись — сластёна, он съел целую <w>шоколадку</w> за раз."
                            }
                        },
                        {
                            "text": "Kupiłem dużą <w>czekoladę</w> z orzechami jako prezent dla siostry.",
                            "target_lang_translations": {
                                "ru": "Я купил большую <w>шоколадку</w> с орехами в подарок сестре."
                            }
                        }
                    ],
                    "synonyms": [
                        "tabliczka czekolady"
                    ],
                    "common_phrases": [
                        "kostka czekolady",
                        "cała czekolada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная плитка этого кондитерского изделия, обычно в прямоугольной упаковке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоколадка",
                                "target_lang_sense_clarification": "плитка"
                            },
                            {
                                "target_lang_word": "плитка шоколада",
                                "target_lang_sense_clarification": "форма выпуска"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "541fdae8-2b36-3a6e-8446-a93908ac84a9",
                    "sense_definition": "Słodki, gęsty i gorący napój przygotowany z kakao, mleka lub śmietanki.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W zimowe wieczory najlepiej rozgrzewa nas gorąca <w>czekolada</w>.",
                            "target_lang_translations": {
                                "ru": "Зимними вечерами нас лучше всего согревает горячий <w>шоколад</w>."
                            }
                        },
                        {
                            "text": "Po powrocie ze spaceru wypiliśmy kubek pysznej <w>czekolady</w>.",
                            "target_lang_translations": {
                                "ru": "Вернувшись с прогулки, мы выпили чашку вкусного <w>шоколада</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "czekolada pitna",
                        "czekolada na gorąco"
                    ],
                    "common_phrases": [
                        "gorąca czekolada",
                        "filiżanka czekolady",
                        "automat do czekolady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкий, густой и горячий напиток, приготовленный из какао, молока или сливок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горячий шоколад",
                                "target_lang_sense_clarification": "напиток"
                            },
                            {
                                "target_lang_word": "шоколад",
                                "target_lang_sense_clarification": "напиток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e12f972d-b13c-38c9-a81a-bea8207ba208",
                    "sense_definition": "Ciemnobrązowy kolor przypominający barwę typowej czekolady.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten kolor na tapicerce to bardziej <w>czekolada</w> niż mahoń.",
                            "target_lang_translations": {
                                "ru": "Этот цвет обивки — скорее <w>шоколад</w>, чем красное дерево."
                            }
                        },
                        {
                            "text": "Jej włosy mają piękny odcień ciemnej <w>czekolady</w>.",
                            "target_lang_translations": {
                                "ru": "У её волос красивый оттенок тёмного <w>шоколада</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciepły brąz"
                    ],
                    "common_phrases": [
                        "kolor czekolady",
                        "odcień czekolady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тёмно-коричневый цвет, напоминающий цвет типичного шоколада."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоколадный цвет",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "шоколад",
                                "target_lang_sense_clarification": "цвет"
                            }
                        ]
                    }
                }
            ]
        }

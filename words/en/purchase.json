    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0044a17e-398f-34cc-b232-6b6d1da59889",
                    "sense_definition": "To buy or obtain something by paying money for it.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_buy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to <w>purchase</w> a new car after saving money for two years.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>купить</w> новую машину, копив деньги два года."
                            }
                        },
                        {
                            "text": "Customers can <w>purchase</w> tickets online or at the box office.",
                            "target_lang_translations": {
                                "ru": "Покупатели могут <w>приобрести</w> билеты онлайн или в кассе."
                            }
                        }
                    ],
                    "synonyms": [
                        "procure"
                    ],
                    "common_phrases": [
                        "purchase goods",
                        "purchase online",
                        "purchase a ticket"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покупать или приобретать что-либо, платя за это деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покупать",
                                "target_lang_sense_clarification": "приобретать за деньги"
                            },
                            {
                                "target_lang_word": "приобретать",
                                "target_lang_sense_clarification": "получать в собственность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee6131c3-a731-37a2-9c82-ea81277d46ed",
                    "sense_definition": "To be sufficient to buy something; to provide the financial power to obtain something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her savings <w>purchased</w> her freedom from debt and allowed her to start fresh.",
                            "target_lang_translations": {
                                "ru": "Её сбережения <w>купили</w> ей свободу от долгов и позволили начать всё сначала."
                            }
                        },
                        {
                            "text": "A small amount of money <w>purchased</w> a surprising amount of food at the market.",
                            "target_lang_translations": {
                                "ru": "На небольшую сумму денег на рынке удалось <w>купить</w> удивительно много еды."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть достаточным для покупки чего-либо; обеспечивать финансовую возможность получить что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покупать",
                                "target_lang_sense_clarification": "быть достаточным для оплаты"
                            },
                            {
                                "target_lang_word": "приобретать",
                                "target_lang_sense_clarification": "позволять купить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc105667-4a6f-35d7-9561-95f8cd61ee61",
                    "sense_definition": "To obtain something through significant effort, sacrifice, or difficulty rather than money.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_effort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sought to <w>purchase</w> favor with the king through constant flattery.",
                            "target_lang_translations": {
                                "ru": "Он стремился <w>купить</w> расположение короля постоянной лестью."
                            }
                        },
                        {
                            "text": "The victory was <w>purchased</w> at the cost of many lives.",
                            "target_lang_translations": {
                                "ru": "Эта победа была <w>куплена</w> ценой многих жизней."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить что-либо ценой значительных усилий, жертв или трудностей, а не за деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покупать",
                                "target_lang_sense_clarification": "переносное значение: добывать ценой жертв"
                            },
                            {
                                "target_lang_word": "доставаться",
                                "target_lang_sense_clarification": "приобретаться с трудом"
                            }
                        ]
                    }
                }
            ]
        },
        {

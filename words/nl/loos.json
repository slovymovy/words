                },
                {
                    "sense_id": "7613ccb4-7f9b-3429-a687-ba91b1676551",
                    "sense_definition": "Niet werkelijk; vals; bedoeld om te misleiden of slechts voor de vorm.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "empty_false",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gelukkig bleek de melding slechts <w>loos</w> alarm te zijn.",

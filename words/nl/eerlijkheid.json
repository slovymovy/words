{
    "word_family": [
        "eerlijk",
        "eerlijkheidshalve"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3b28453b-17e0-3f98-b2a9-dd51726e7550",
                    "sense_definition": "De eigenschap van oprecht en betrouwbaar zijn en de waarheid spreken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik waardeer je <w>eerlijkheid</w> over dit moeilijke onderwerp enorm.",
                            "target_lang_translations": {
                                "ru": "Я очень ценю твою <w>честность</w> в этом непростом вопросе.",
                                "en": "I greatly appreciate your <w>honesty</w> about this difficult subject."
                            }
                        },
                        {
                            "text": "De <w>eerlijkheid</w> gebiedt te zeggen dat ik de afspraak was vergeten.",
                            "target_lang_translations": {
                                "ru": "<w>Честность</w> требует признать, что я забыл о встрече.",
                                "en": "<w>Honesty</w> compels me to say that I had forgotten the appointment."
                            }
                        }
                    ],
                    "synonyms": [
                        "oprechtheid"
                    ],
                    "antonyms": [
                        "oneerlijkheid"
                    ],
                    "common_phrases": [
                        "eerlijkheid duurt het langst",
                        "in alle eerlijkheid",
                        "de eerlijkheid gebiedt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество быть искренним, надежным и говорить правду.",
                        "en": "The quality of being sincere and trustworthy and speaking the truth."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "честность",
                                "target_lang_sense_clarification": "свойство говорить правду"
                            },
                            {
                                "target_lang_word": "искренность",
                                "target_lang_sense_clarification": "откровенность, отсутствие притворства"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "honesty",
                                "target_lang_sense_clarification": "the quality of being truthful"
                            },
                            {
                                "target_lang_word": "sincerity",
                                "target_lang_sense_clarification": "absence of pretence or deceit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23f499a3-6cc7-3760-a1c1-999c4d211ddd",
                    "sense_definition": "De eigenschap van rechtvaardig zijn en iedereen gelijk behandelen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De spelers twijfelden aan de <w>eerlijkheid</w> van de scheidsrechter tijdens de wedstrijd.",
                            "target_lang_translations": {
                                "ru": "Игроки сомневались в <w>честности</w> судьи во время матча.",
                                "en": "The players doubted the <w>fairness</w> of the referee during the match."
                            }
                        },
                        {
                            "text": "Voor de <w>eerlijkheid</w> verdelen we de taart in precies even grote stukken.",
                            "target_lang_translations": {
                                "ru": "Ради <w>справедливости</w> мы разделим торт на совершенно равные куски.",
                                "en": "For the sake of <w>fairness</w>, we divide the cake into exactly equal pieces."
                            }
                        }
                    ],
                    "synonyms": [
                        "rechtvaardigheid",
                        "billijkheid"
                    ],
                    "antonyms": [
                        "oneerlijkheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество быть справедливым и относиться ко всем одинаково.",
                        "en": "The quality of being just and treating everyone equally."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "справедливость",
                                "target_lang_sense_clarification": "беспристрастность"
                            },
                            {
                                "target_lang_word": "честность",
                                "target_lang_sense_clarification": "добросовестность, соблюдение правил"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "fairness",
                                "target_lang_sense_clarification": "impartial and just treatment"
                            },
                            {
                                "target_lang_word": "justice",
                                "target_lang_sense_clarification": "fairness in the way people are treated"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
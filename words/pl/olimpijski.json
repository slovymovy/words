                {
                    "sense_id": "b8b4d51f-bfe5-3e88-9385-29931dc0dd1a",
                    "sense_definition": "związany z igrzyskami olimpijskimi, dotyczący międzynarodowych zawodów sportowych organizowanych co cztery lata",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy zawodowy sportowiec marzy o tym, by zdobyć złoty medal <w>olimpijski</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый профессиональный спортсмен мечтает завоевать золотую <w>олимпийскую</w> медаль."
                            }
                        },
                        {
                            "text": "Hymn <w>olimpijski</w> zabrzmiał po raz pierwszy na igrzyskach w Atenach w 1896 roku.",
                            "target_lang_translations": {
                                "ru": "<w>Олимпийский</w> гимн впервые прозвучал на играх в Афинах в 1896 году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "igrzyska olimpijskie",
                        "medal olimpijski",
                        "znicz olimpijski",
                        "wioska olimpijska",
                        "komitet olimpijski"
                    ],
                    "target_lang_definitions": {
                        "ru": "связанный с Олимпийскими играми, касающийся международных спортивных соревнований, проводимых раз в четыре года"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "олимпийский",
                                "target_lang_sense_clarification": "относящийся к спортивным играм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "626313c1-0cd3-3113-a82d-0bc57757b363",
                    "sense_definition": "dotyczący góry Olimp w Grecji lub mitycznej siedziby starożytnych bogów",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zeus był najważniejszym z dwunastu głównych bogów <w>olimpijskich</w>.",
                            "target_lang_translations": {
                                "ru": "Зевс был важнейшим из двенадцати главных <w>олимпийских</w> богов."
                            }
                        },
                        {
                            "text": "W mitologii greckiej nektar i ambrozja były pokarmem bóstw <w>olimpijskich</w>.",
                            "target_lang_translations": {
                                "ru": "В греческой мифологии нектар и амброзия были пищей <w>олимпийских</w> божеств."
                            }
                        }
                    ],
                    "common_phrases": [

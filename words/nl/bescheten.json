                {
                    "sense_id": "22923bd4-2dd7-336e-bbb8-d2264e066dcc",
                    "sense_definition": "Beroerd, in een slechte of beschamende toestand verkerend.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kwam er uiteindelijk maar <w>bescheten</w> van af tijdens de vergadering.",
                            "target_lang_translations": {
                                "en": "He came off looking rather <w>foolish</w> during the meeting.",
                                "ru": "В итоге на собрании он выглядел довольно <w>жалко</w>."
                            }
                        },
                        {
                            "text": "Het is een <w>bescheten</w> situatie waar we nu in verzeild zijn geraakt.",
                            "target_lang_translations": {
                                "en": "It is a <w>lousy</w> situation we have ended up in now.",
                                "ru": "Мы оказались в <w>паршивой</w> ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "beroerd",
                        "ellendig"
                    ],
                    "antonyms": [
                        "geweldig",
                        "succesvol"
                    ],
                    "common_phrases": [
                        "er bescheten van afkomen",
                        "er bescheten op staan"
                    ],
                    "target_lang_definitions": {
                        "en": "Miserable, in a bad or shameful state.",
                        "ru": "Скверный, находящийся в плохом или постыдном состоянии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lousy",

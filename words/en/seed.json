                    ],
                    "common_phrases": [
                        "sow seeds",
                        "sunflower seeds",
                        "plant a seed",
                        "pumpkin seeds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькая твердая часть, производимая растением, из которой может вырасти новое растение того же вида."

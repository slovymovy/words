            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f3876a2b-4979-33e1-9eda-3971331ad7f4",
                    "sense_definition": "Stan odpoczynku, na ogół nocnego, w którym spowolnione są funkcje organizmu i obniżony poziom świadomości",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_sleep_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długim i męczącym dniu szybko zapadł w głęboki <w>sen</w>.",
                            "target_lang_translations": {
                                "ru": "После долгого и утомительного дня он быстро погрузился в глубокий <w>сон</w>."
                            }
                        },
                        {
                            "text": "Zdrowy <w>sen</w> jest niezbędny do regeneracji sił witalnych człowieka.",
                            "target_lang_translations": {
                                "ru": "Здоровый <w>сон</w> необходим для восстановления жизненных сил человека."
                            }
                        }
                    ],
                    "synonyms": [
                        "spanie",
                        "kima"
                    ],
                    "common_phrases": [
                        "głęboki sen",
                        "zapaść w sen",
                        "sen zimowy",
                        "wieczny sen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние отдыха, обычно ночного, при котором замедляются функции организма и снижается уровень сознания"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сон",
                                "target_lang_sense_clarification": "физиологическое состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d2a7eaf-8060-37c2-a5bb-905195b36358",
                    "sense_definition": "Doświadczenie przeżywane w trakcie snu; obrazy, dźwięki i odczucia pojawiające się w umyśle osoby śpiącej",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_dream_vision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tej nocy miałem bardzo dziwny <w>sen</w>, w którym potrafiłem latać.",
                            "target_lang_translations": {
                                "ru": "Этой ночью мне приснился очень странный <w>сон</w>, в котором я умел летать."
                            }
                        },
                        {
                            "text": "Zły <w>sen</w> obudził go w środku nocy z krzykiem na ustach.",
                            "target_lang_translations": {
                                "ru": "Плохой <w>сон</w> разбудил его посреди ночи с криком на губах."
                            }
                        }
                    ],
                    "synonyms": [
                        "marzenie senne",
                        "mara"
                    ],
                    "antonyms": [
                        "jawa"
                    ],
                    "common_phrases": [
                        "zły sen",
                        "proroczy sen",
                        "tłumaczyć sny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переживание во время сна; образы, звуки и ощущения, возникающие в сознании спящего"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сон",
                                "target_lang_sense_clarification": "сновидение"
                            },
                            {
                                "target_lang_word": "сновидение",
                                "target_lang_sense_clarification": "образы во сне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff655bb8-d96b-31ed-bb48-de7eb95343c7",
                    "sense_definition": "Fantazja, marzenie, wizja, coś, co wydaje się nierzeczywiste lub jest przedmiotem wielkich pragnień",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_dream_wish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>sen</w> o podróży dookoła świata w końcu się spełnił.",
                            "target_lang_translations": {
                                "ru": "Его <w>мечта</w> о кругосветном путешествии наконец сбылась."
                            }
                        },
                        {
                            "text": "To był tylko piękny <w>sen</w>, który zderzył się z brutalną rzeczywistością.",
                            "target_lang_translations": {
                                "ru": "Это был всего лишь прекрасный <w>сон</w>, который столкнулся с жестокой реальностью."
                            }
                        }
                    ],
                    "synonyms": [
                        "marzenie",
                        "rojenie"
                    ],
                    "common_phrases": [
                        "sen o potędze",
                        "spełnić sen",
                        "amerykański sen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фантазия, мечта, видение; нечто, кажущееся нереальным или являющееся предметом сильных желаний"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мечта",
                                "target_lang_sense_clarification": "предмет желаний"
                            },
                            {
                                "target_lang_word": "грёза",
                                "target_lang_sense_clarification": "фантазия"
                            },
                            {
                                "target_lang_word": "сон",
                                "target_lang_sense_clarification": "иллюзия, нечто несбыточное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbd87503-2a0b-3dd9-b8dc-1cc2d19fddb2",
                    "sense_definition": "Drobna jednostka monetarna w niektórych krajach Azji, m.in. jedna setna jena japońskiego lub ringgita malezyjskiego",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_currency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W portfelu zostało mu tylko kilka monet, w tym dwa <w>seny</w>.",
                            "target_lang_translations": {
                                "ru": "В кошельке у него осталось только несколько монет, в том числе два <w>сена</w>."
                            }
                        },
                        {
                            "text": "Cena towaru wynosiła pięć ringgitów i pięćdziesiąt <w>senów</w>.",
                            "target_lang_translations": {
                                "ru": "Цена товара составляла пять ринггитов и пятьдесят <w>сенов</w>."
                            }
                        }
                    ],
                    "traits": [
                        {

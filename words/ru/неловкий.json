{
    "word_family": [
        "ловкий",
        "ловкость",
        "неловкость",
        "неловко"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "05e42d6c-ddb4-3bf0-a831-e268e95fe7ce",
                    "sense_definition": "Лишённый ловкости, точности и искусности в движениях.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_clumsiness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он сделал <неловкое> движение и случайно задел чашку.",
                            "target_lang_translations": {
                                "en": "He made an <awkward> movement and accidentally hit the cup."
                            }
                        },
                        {
                            "text": "Этот <неловкий> подросток постоянно натыкается на мебель.",
                            "target_lang_translations": {
                                "en": "This <clumsy> teenager is constantly bumping into furniture."
                            }
                        }
                    ],
                    "synonyms": [
                        "неуклюжий",
                        "неповоротливый"
                    ],
                    "antonyms": [
                        "ловкий"
                    ],
                    "common_phrases": [
                        "неловкое движение",
                        "неловкий жест",
                        "неловкий шаг"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking dexterity, precision, or skill in movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clumsy",
                                "target_lang_sense_clarification": "lacking physical coordination"
                            },
                            {
                                "target_lang_word": "awkward",
                                "target_lang_sense_clarification": "lacking grace or dexterity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89047eec-9168-37c5-a951-88a4d6d64354",
                    "sense_definition": "Вызывающий чувство стеснения, неудобства или стыда (о ситуации, молчании и т. п.).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_awkwardness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В комнате повисла долгая <неловкая> пауза.",
                            "target_lang_translations": {
                                "en": "A long <awkward> pause hung in the room."
                            }
                        },
                        {
                            "text": "Я почувствовал себя в крайне <неловком> положении из-за опоздания.",
                            "target_lang_translations": {
                                "en": "I felt myself in an extremely <awkward> position because of being late."
                            }
                        }
                    ],
                    "common_phrases": [
                        "неловкая пауза",
                        "неловкое молчание",
                        "неловкое положение",
                        "неловкая ситуация",
                        "неловкий момент"
                    ],
                    "target_lang_definitions": {
                        "en": "Causing a feeling of embarrassment, discomfort, or shame (regarding a situation, silence, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "awkward",
                                "target_lang_sense_clarification": "causing embarrassment or social unease"
                            },
                            {
                                "target_lang_word": "uncomfortable",
                                "target_lang_sense_clarification": "causing mental unease"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
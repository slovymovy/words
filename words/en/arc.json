    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "01fbc5d5-fb46-37a0-b8ce-6c896f690343",
                    "sense_definition": "A curved shape or line that resembles part of the circumference of a circle.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "shape_geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The basketball flew in a high <w>arc</w> before dropping through the net.",
                            "target_lang_translations": {
                                "ru": "Баскетбольный мяч пролетел по высокой <w>дуге</w>, прежде чем опуститься в сетку."
                            }
                        },
                        {
                            "text": "She used a compass to draw a perfect <w>arc</w> on the sheet of paper.",
                            "target_lang_translations": {
                                "ru": "Она использовала циркуль, чтобы начертить идеальную <w>дугу</w> на листе бумаги."
                            }
                        }
                    ],
                    "synonyms": [
                        "curve",
                        "circular arc"
                    ],
                    "common_phrases": [
                        "in an arc",
                        "wide arc",
                        "circular arc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изогнутая линия или форма, напоминающая часть окружности"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуга",

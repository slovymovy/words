    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "825cd5a4-0456-3eff-ad1c-14dd06b99704",
                    "sense_definition": "Trzecia osoba liczby pojedynczej czasu teraźniejszego czasownika <w>klikać</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat ciągle <w>klika</w> myszką, grając w gry komputerowe.",
                            "target_lang_translations": {
                                "ru": "Мой брат постоянно <w>щёлкает</w> мышкой, играя в компьютерные игры."
                            }
                        },
                        {
                            "text": "Każdy użytkownik <w>klika</w> w ten link, aby zobaczyć szczegóły oferty.",
                            "target_lang_translations": {
                                "ru": "Каждый пользователь <w>кликает</w> по этой ссылке, чтобы увидеть подробности предложения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "klika myszką",
                        "klika w link",
                        "klika długopisem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czas teraźniejszy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Третье лицо единственного числа настоящего времени глагола \"klikać\" (кликать, нажимать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кликает",
                                "target_lang_sense_clarification": "нажимать на ссылку или кнопку"
                            },
                            {
                                "target_lang_word": "щёлкает",
                                "target_lang_sense_clarification": "издавать звук или нажимать кнопкой мыши"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fe75dc86-7f6a-3771-a466-b77a1c41b99c",
                    "sense_definition": "Grupa ludzi powiązanych wspólnymi interesami i wspierających się wzajemnie, wbrew interesowi szerszej grupy, do której należą.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym mieście rządzi jedna <w>klika</w>, która dzieli między siebie wszystkie stanowiska.",
                            "target_lang_translations": {
                                "ru": "В этом городе правит одна <w>клика</w>, которая делит между собой все должности."
                            }
                        },
                        {
                            "text": "To nie jest otwarta grupa, ale zamknięta <w>klika</w>, do której trudno się dostać.",
                            "target_lang_translations": {
                                "ru": "Это не открытая группа, а закрытая <w>клика</w>, в которую трудно попасть."
                            }
                        }
                    ],
                    "synonyms": [
                        "koteria",
                        "sitwa",
                        "klan",
                        "kasta",
                        "mafia",
                        "kamaryla"
                    ],
                    "common_phrases": [
                        "rządząca klika",
                        "wpływowa klika",
                        "lokalna klika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, связанных общими интересами и поддерживающих друг друга, часто в ущерб интересам более широкого сообщества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клика",
                                "target_lang_sense_clarification": "группировка, сообщество"
                            },
                            {
                                "target_lang_word": "шайка",
                                "target_lang_sense_clarification": "разговорное, неодобрительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90034f1e-5593-346a-ac53-f44e87017385",
                    "sense_definition": "Część grafu, w której każdy wierzchołek jest połączony z każdym różnym od niego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Algorytm szuka największej <w>kliki</w> w podanym grafie połączeń sieciowych.",
                            "target_lang_translations": {
                                "ru": "Алгоритм ищет наибольшую <w>клику</w> в заданном графе сетевых соединений."
                            }
                        },
                        {
                            "text": "Ta <w>klika</w> składa się z pięciu połączonych ze sobą węzłów.",
                            "target_lang_translations": {
                                "ru": "Эта <w>клика</w> состоит из пяти соединённых между собой узлов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "problem kliki",
                        "klika maksymalna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть графа, в которой каждая вершина соединена ребром с каждой другой вершиной этой части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клика",
                                "target_lang_sense_clarification": "теория графов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

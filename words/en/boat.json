                    "frequency": "VeryLow",
                    "semantic_group_id": "water_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers <w>boated</w> their supplies across the flooded river.",
                            "target_lang_translations": {
                                "ru": "Жители деревни <w>перевозили на лодках</w> свои припасы через затопленную реку."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c97a97ae-2bee-3a48-a9de-e9da68e1b3ad",
                    "sense_definition": "A person who is not an active member of the armed forces or the police.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "non_military_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "An army never can be commanded or controlled by <w>civilians</w>.",
                            "target_lang_translations": {
                                "ru": "Армией никогда не могут командовать или управлять <w>гражданские</w>."
                            }
                        },
                        {
                            "text": "Three <w>civilians</w> were apprehended by the soldiers and taken away in a military vehicle.",
                            "target_lang_translations": {
                                "ru": "Трое <w>гражданских</w> были задержаны солдатами и увезены на военной машине."
                            }
                        }
                    ],
                    "synonyms": [
                        "non-combatant",
                        "citizen",
                        "private citizen"
                    ],
                    "antonyms": [
                        "soldier",
                        "military personnel"
                    ],
                    "common_phrases": [
                        "innocent civilians",
                        "civilian population",
                        "civilian casualties"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, не состоящий на действительной военной службе или в полиции"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гражданский",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "мирный житель",
                                "target_lang_sense_clarification": "в контексте военных действий"
                            },
                            {
                                "target_lang_word": "штатский",
                                "target_lang_sense_clarification": "разговорное; человек не в форме"
                            },
                            {
                                "target_lang_word": "гражданское лицо",
                                "target_lang_sense_clarification": "официальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b50d186-7938-3cf5-9a0a-28f9fbc423c9",
                    "sense_definition": "A person who does not belong to a particular group, profession, or activity; an outsider.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have this vision one day that I'll walk amongst you a regular <w>civilian</w>.",
                            "target_lang_translations": {
                                "ru": "У меня есть мечта, что однажды я буду ходить среди вас как обычный <w>гражданский</w>."
                            }
                        },
                        {
                            "text": "The bathroom was for employees only, so <w>civilians</w> weren't allowed to use it.",
                            "target_lang_translations": {
                                "ru": "Туалет был только для сотрудников, поэтому <w>посторонним</w> не разрешалось им пользоваться."
                            }
                        }
                    ],
                    "synonyms": [
                        "outsider",
                        "layperson"
                    ],
                    "antonyms": [
                        "insider",
                        "professional"
                    ],
                    "common_phrases": [
                        "regular civilian",
                        "mere civilian"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to distinguish insiders from outsiders"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, не принадлежащий к определенной группе или профессии; посторонний"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посторонний",
                                "target_lang_sense_clarification": "чужак, не входящий в группу"
                            },
                            {
                                "target_lang_word": "гражданский",
                                "target_lang_sense_clarification": "на профессиональном жаргоне"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7040aad4-08dd-3015-a347-6c617fd64dcc",
                    "sense_definition": "Relating to private citizens and ordinary life rather than the military, police, or religious organizations.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "non_military_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The three detainees were actually army defectors wearing <w>civilian</w> clothing.",
                            "target_lang_translations": {
                                "ru": "Трое задержанных на самом деле были дезертирами, одетыми в <w>гражданскую</w> одежду."
                            }
                        },
                        {
                            "text": "He worked as a <w>civilian</w> journalist for ten years before being employed by the public broadcaster.",
                            "target_lang_translations": {
                                "ru": "Он десять лет работал <w>гражданским</w> журналистом, прежде чем устроиться в общественную телерадиокомпанию."
                            }
                        }
                    ],
                    "synonyms": [
                        "non-military",
                        "civil",
                        "private"
                    ],
                    "antonyms": [
                        "military",
                        "martial"
                    ],
                    "common_phrases": [
                        "civilian clothing",
                        "civilian life",
                        "civilian rule",
                        "civilian infrastructure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к частным гражданам и обычной жизни, а не к военным или полиции"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гражданский",
                                "target_lang_sense_clarification": "не военный"
                            },
                            {
                                "target_lang_word": "штатский",
                                "target_lang_sense_clarification": "об одежде или внешнем виде"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
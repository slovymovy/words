                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past participle of 'trappen' (to kick, step, or pedal).",
                        "ru": "Причастие прошедшего времени от глагола trappen (пинать, наступать)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kicked",
                                "target_lang_sense_clarification": "struck with the foot"
                            },
                            {
                                "target_lang_word": "stepped",
                                "target_lang_sense_clarification": "placed the foot on something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пнуть",
                                "target_lang_sense_clarification": "ударить ногой"
                            },
                            {
                                "target_lang_word": "наступить",
                                "target_lang_sense_clarification": "встать ногой на что-либо"
                            },
                            {
                                "target_lang_word": "попасться",
                                "target_lang_sense_clarification": "в выражении 'in de val getrapt' (попасться в ловушку)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1c702939-8e22-3395-b1c7-c96222f71abd",
                    "sense_definition": "Niet rechtstreeks, maar verlopend via tussenstappen of niveaus.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leden van de Eerste Kamer worden gekozen via <w>getrapte</w> verkiezingen.",
                            "target_lang_translations": {
                                "en": "The members of the Senate are elected via <w>indirect</w> elections.",
                                "ru": "Члены Первой палаты избираются посредством <w>косвенных</w> выборов."
                            }
                        },
                        {
                            "text": "Er is sprake van een <w>getrapte</w> besluitvorming waarbij meerdere lagen betrokken zijn.",
                            "target_lang_translations": {
                                "en": "There is a <w>tiered</w> decision-making process involving multiple layers.",
                                "ru": "Речь идет о <w>многоступенчатом</w> принятии решений, в которое вовлечено несколько уровней."
                            }
                        }
                    ],
                    "synonyms": [
                        "indirect",
                        "onrechtstreeks"
                    ],
                    "antonyms": [
                        "direct",
                        "rechtstreeks"
                    ],
                    "common_phrases": [
                        "getrapte verkiezingen",
                        "getrapte methode"
                    ],
                    "target_lang_definitions": {
                        "en": "Not direct, but proceeding through intermediate steps or levels.",
                        "ru": "Непрямой, происходящий поэтапно или через промежуточные уровни."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "indirect",
                                "target_lang_sense_clarification": "via representatives or intermediate stages"
                            },
                            {
                                "target_lang_word": "tiered",
                                "target_lang_sense_clarification": "arranged in layers or levels"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "косвенный",
                                "target_lang_sense_clarification": "непрямой (например, о выборах)"
                            },
                            {
                                "target_lang_word": "многоступенчатый",
                                "target_lang_sense_clarification": "поэтапный"
                            }
                        ]
                    }
                },
                {

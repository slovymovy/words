        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5ff7c02f-5b1e-3811-b89d-53bc4524cfd3",
                    "sense_definition": "Een ruimte onder de grond in een huis of gebouw, meestal gebruikt voor opslag.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "building_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Staan er nog flessen wijn in de <w>kelder</w>?",
                            "target_lang_translations": {
                                "ru": "Остались ли еще бутылки вина в <w>погребе</w>?",
                                "en": "Are there still bottles of wine in the <w>cellar</w>?"
                            }
                        },
                        {
                            "text": "We hebben onze oude spullen in de <w>kelder</w> opgeslagen.",
                            "target_lang_translations": {
                                "ru": "Мы сложили наши старые вещи в <w>подвал</w>.",
                                "en": "We stored our old stuff in the <w>basement</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "zolder"
                    ],
                    "common_phrases": [
                        "in de kelder",
                        "naar de kelder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещение под землей в доме или здании, обычно используемое для хранения.",
                        "en": "A room underground in a house or building, usually used for storage."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвал",
                                "target_lang_sense_clarification": "общая комната под домом"
                            },
                            {
                                "target_lang_word": "погреб",
                                "target_lang_sense_clarification": "для хранения продуктов или вина"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "cellar",
                                "target_lang_sense_clarification": "underground storage space, often for wine"
                            },
                            {
                                "target_lang_word": "basement",
                                "target_lang_sense_clarification": "floor of a building below ground level"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "393ca1fd-7789-3ebb-9016-91949e6178d0",
                    "sense_definition": "De bodem van de zee, vaak gebruikt in uitdrukkingen over zinken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sea_bottom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het zwaar beschadigde schip ging snel naar de <w>kelder</w>.",
                            "target_lang_translations": {
                                "ru": "Тяжело поврежденный корабль быстро пошел ко <w>дну</w>.",
                                "en": "The heavily damaged ship quickly went to the <w>bottom</w>."
                            }
                        },
                        {
                            "text": "Door de explosie verdween de boot naar de <w>kelder</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за взрыва лодка пошла ко <w>дну</w>.",
                                "en": "Due to the explosion, the boat disappeared to the <w>bottom</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zeebodem"
                    ],
                    "common_phrases": [
                        "naar de kelder gaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Морское дно, часто используется в выражениях о потоплении.",
                        "en": "The bottom of the sea, often used in expressions about sinking."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дно",
                                "target_lang_sense_clarification": "в выражении 'идти ко дну'"

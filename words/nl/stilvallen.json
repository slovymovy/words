    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "df7c49a0-6831-3af5-a38c-d896fc78afdc",
                    "sense_definition": "Plotseling stoppen met praten of geluid maken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "silence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iedereen <w>viel stil</w> toen de directeur de vergaderzaal binnenkwam.",
                            "target_lang_translations": {
                                "en": "Everyone <w>fell silent</w> when the director entered the meeting room."
                            }
                        },
                        {
                            "text": "Het levendige gesprek <w>viel stil</w> na zijn onverwachte opmerking.",
                            "target_lang_translations": {
                                "en": "The lively conversation <w>fell silent</w> after his unexpected remark."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwijgen",
                        "verstommen"
                    ],
                    "antonyms": [
                        "spreken",
                        "praten"
                    ],
                    "common_phrases": [
                        "het gesprek viel stil",
                        "de muziek viel stil"
                    ],
                    "target_lang_definitions": {
                        "en": "To suddenly stop talking or making noise."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall silent",
                                "target_lang_sense_clarification": "of people or conversations"
                            },
                            {
                                "target_lang_word": "go quiet",
                                "target_lang_sense_clarification": "to become silent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3db34b0f-20f8-3ae2-9175-73f24b5bfdec",
                    "sense_definition": "Stoppen met functioneren, bewegen of vooruitgaan (vaak gezegd van motoren, voertuigen of processen).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "stop_functioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn auto <w>viel stil</w> midden op een druk kruispunt.",
                            "target_lang_translations": {
                                "en": "My car <w>stalled</w> in the middle of a busy intersection."
                            }
                        },
                        {
                            "text": "Door een gebrek aan materialen is de productie in de fabriek volledig <w>stilgevallen</w>.",
                            "target_lang_translations": {
                                "en": "Due to a lack of materials, production in the factory has completely <w>come to a halt</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "afslaan",
                        "stoppen",
                        "vastlopen"
                    ],
                    "antonyms": [
                        "starten",
                        "doorgaan"
                    ],
                    "common_phrases": [
                        "de motor viel stil",
                        "het werk is stilgevallen"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop functioning, moving, or progressing (often regarding engines or processes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stall",
                                "target_lang_sense_clarification": "of an engine or vehicle"
                            },
                            {
                                "target_lang_word": "come to a halt",
                                "target_lang_sense_clarification": "of a process or movement"
                            }
                        ]
                    }
                }
            ]
        }
    ]

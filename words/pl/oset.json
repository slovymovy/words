    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2550a4a8-aaf9-37d0-9d0d-1cb46ddfc600",
                    "sense_definition": "Roślina o kolczastych liściach i łodygach oraz fioletowych kwiatach, rosnąca dziko na łąkach i nieużytkach.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "botanical_thistle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na łące rósł wysoki i kłujący <w>oset</w>.",
                            "target_lang_translations": {
                                "ru": "На лугу рос высокий и колючий <w>чертополох</w>."
                            }
                        },
                        {
                            "text": "Musisz uważać, żeby nie nadepnąć na <w>oset</w> bosą stopą.",
                            "target_lang_translations": {
                                "ru": "Нужно быть осторожным, чтобы не наступить на <w>чертополох</w> босой ногой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kłujący oset",
                        "polny oset"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение с колючими листьями и стеблями, а также фиолетовыми цветами, дикорастущее на лугах и пустырях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чертополох",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcee7bae-d513-3e7f-9c6d-373ea512df04",
                    "sense_definition": "Pospolity chwast polny, botanicznie klasyfikowany jako ostrożeń polny, często mylony z właściwym ostem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botanical_thistle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik narzekał, że <w>oset</w> zarasta mu uprawy zboża.",
                            "target_lang_translations": {
                                "ru": "Фермер жаловался, что <w>бодяк</w> зарастает его посевы зерновых."
                            }
                        },
                        {
                            "text": "Ten gatunek to w rzeczywistości ostrożeń, choć potocznie mówi się na niego <w>oset</w>.",
                            "target_lang_translations": {
                                "ru": "Этот вид на самом деле бодяк, хотя в просторечии его называют <w>чертополохом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ostrożeń polny",
                        "bodiak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенный полевой сорняк, ботанически классифицируемый как бодяк полевой, который часто путают с настоящим чертополохом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бодяк",
                                "target_lang_sense_clarification": "сорняк"
                            },
                            {
                                "target_lang_word": "чертополох",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "6b28b6dc-38b6-359f-9e75-5a721208e1d7",
                    "sense_definition": "Polskie nazwisko męskie lub żeńskie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Oset</w> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Осет</w> был избран председателем совета микрорайона."
                            }
                        },
                        {
                            "text": "Nie ma dzisiaj w pracy pani <w>Oset</w>.",
                            "target_lang_translations": {
                                "ru": "Сегодня на работе нет госпожи <w>Осет</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская мужская или женская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Осет",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "3c6aa94b-a1c2-3bbd-a03e-ef8a7e5e6b72",
                    "sense_definition": "Gemakkelijk om doorheen te kijken omdat het niet troebel is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_transparency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water in het bergmeer was ijskoud en volkomen <w>helder</w>.",
                            "target_lang_translations": {
                                "ru": "Вода в горном озере была ледяной и совершенно <w>прозрачной</w>."
                            }
                        },
                        {
                            "text": "Door het <w>heldere</w> glas kon ik de vogels in de tuin goed zien.",
                            "target_lang_translations": {
                                "ru": "Через <w>прозрачное</w> стекло я хорошо видел птиц в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorzichtig",
                        "transparant"
                    ],
                    "antonyms": [
                        "troebel",
                        "ondoorzichtig"
                    ],
                    "common_phrases": [
                        "helder water",
                        "helder glas",
                        "heldere vloeistof"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко просматриваемый насквозь, не мутный; прозрачный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачный",
                                "target_lang_sense_clarification": "о воде, стекле или материале"
                            },
                            {
                                "target_lang_word": "чистый",
                                "target_lang_sense_clarification": "без примесей, не мутный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef81445f-93c9-35a4-b4d9-bd6e5871304a",
                    "sense_definition": "Veel licht uitstralend of weerkaatsend; fel en intens van kleur of licht.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_brightness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zon scheen met een <w>helder</w> licht door de ramen naar binnen.",
                            "target_lang_translations": {
                                "ru": "Солнце светило <w>ярким</w> светом сквозь окна внутрь."
                            }
                        },
                        {
                            "text": "Zij draagt graag kleding met <w>heldere</w> kleuren om op te vallen.",
                            "target_lang_translations": {
                                "ru": "Она любит носить одежду <w>ярких</w> цветов, чтобы выделяться."
                            }
                        }
                    ],
                    "synonyms": [
                        "fel",
                        "stralend",
                        "licht"
                    ],
                    "antonyms": [
                        "dof",
                        "donker"
                    ],
                    "common_phrases": [
                        "helder licht",
                        "heldere kleuren",
                        "helder wit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Излучающий или отражающий много света; интенсивный по цвету или свету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яркий",
                                "target_lang_sense_clarification": "о свете или цвете"

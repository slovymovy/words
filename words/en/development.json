                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "event_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They bought a house in a new residential <w>development</w> outside the city.",
                            "target_lang_translations": {
                                "ru": "Они купили дом в новом жилом <w>комплексе</w> за городом."
                            }
                        },
                        {
                            "text": "The council approved a large commercial <w>development</w> near the river.",
                            "target_lang_translations": {
                                "ru": "Совет одобрил крупный проект коммерческой <w>застройки</w> у реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "complex",
                        "estate",
                        "project"
                    ],
                    "common_phrases": [
                        "housing development",
                        "residential development",
                        "commercial development"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок земли с новыми домами, магазинами или офисами, построенный как единый проект."

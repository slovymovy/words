                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Could you please <w>page</w> Dr. Smith to the emergency room?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы <w>вызвать</w> доктора Смита в отделение неотложной помощи?"
                            }

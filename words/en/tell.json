{
    "word_family": [
        "teller",
        "retelling",
        "foretell",
        "storyteller",
        "untold",
        "telltale"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "52a6eb8c-2057-380a-89b3-a7fd3d6aa910",
                    "sense_definition": "To convey information, thoughts, or feelings by speech; to say or communicate something to someone.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>tell</w> me your name so I can introduce you.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>скажите</w> мне ваше имя, чтобы я мог вас представить."
                            }
                        },
                        {
                            "text": "He <w>told</w> us the truth about what happened yesterday.",
                            "target_lang_translations": {
                                "ru": "Он <w>рассказал</w> нам правду о том, что случилось вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "make known",
                        "disclose"
                    ],
                    "common_phrases": [
                        "tell the truth",
                        "tell a lie",
                        "tell me",
                        "tell someone something"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать информацию, мысли или чувства посредством речи; говорить или сообщать что-то кому-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говорить",
                                "target_lang_sense_clarification": "пользоваться устной речью, сообщать мысли"
                            },
                            {
                                "target_lang_word": "сказать",
                                "target_lang_sense_clarification": "выразить словесно, сообщить (совершенный вид)"
                            },
                            {
                                "target_lang_word": "сообщать",
                                "target_lang_sense_clarification": "передавать сведения, уведомлять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d15dc865-99fc-332a-9dc8-98dd576966a6",
                    "sense_definition": "To narrate or give an account of a story or event.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother likes to <w>tell</w> stories about her childhood.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка любит <w>рассказывать</w> истории о своем детстве."
                            }
                        },
                        {
                            "text": "Can you <w>tell</w> us a funny joke to cheer everyone up?",
                            "target_lang_translations": {
                                "ru": "Можешь <w>рассказать</w> нам смешную шутку, чтобы всех развеселить?"
                            }
                        }
                    ],
                    "synonyms": [
                        "narrate",
                        "recount",
                        "relate"
                    ],
                    "common_phrases": [
                        "tell a story",
                        "tell a joke",
                        "tell a tale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повествовать или описывать историю или событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассказывать",
                                "target_lang_sense_clarification": "подробно излагать, повествовать"
                            },
                            {
                                "target_lang_word": "повествовать",
                                "target_lang_sense_clarification": "вести рассказ о чем-либо (книжное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5ca97e9-5e5a-35b7-bc9a-02a4a3fec562",
                    "sense_definition": "To instruct, inform, or give knowledge to someone about how to do something or about a fact.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Could you <w>tell</w> me how to get to the train station?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы <w>сказать</w> мне, как добраться до вокзала?"
                            }
                        },
                        {
                            "text": "She <w>told</w> him that the meeting had been cancelled.",
                            "target_lang_translations": {
                                "ru": "Она <w>сказала</w> ему, что встреча отменена."
                            }
                        }
                    ],
                    "synonyms": [
                        "advise",
                        "apprise"
                    ],
                    "antonyms": [
                        "ask"
                    ],
                    "common_phrases": [
                        "tell someone how",
                        "tell someone about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструктировать, информировать или сообщать кому-то факты или способы действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сказать",
                                "target_lang_sense_clarification": "дать информацию, указать"
                            },
                            {
                                "target_lang_word": "объяснять",
                                "target_lang_sense_clarification": "растолковывать, делать понятным"
                            },
                            {
                                "target_lang_word": "подсказать",
                                "target_lang_sense_clarification": "дать совет или навести на мысль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7987127-7606-3a87-9ea8-c1e48edabf2a",
                    "sense_definition": "To order or direct someone to do something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher <w>told</w> the students to be quiet and sit down.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>велел</w> ученикам замолчать и сесть."
                            }
                        },
                        {
                            "text": "I did exactly what I was <w>told</w> to do by my boss.",
                            "target_lang_translations": {
                                "ru": "Я сделал именно то, что мне <w>велел</w> сделать начальник."
                            }
                        }
                    ],
                    "synonyms": [
                        "count",
                        "number"
                    ],
                    "common_phrases": [
                        "do as you are told",
                        "tell someone to do"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приказывать или давать указание кому-то сделать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велеть",
                                "target_lang_sense_clarification": "приказывать, распоряжаться"
                            },
                            {
                                "target_lang_word": "приказывать",
                                "target_lang_sense_clarification": "давать официальное распоряжение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d504085a-f9aa-3796-9a7c-afc4e1d02acf",
                    "sense_definition": "To discern, notice, identify, or distinguish something, often by observation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is very hard to <w>tell</w> the twins apart because they look identical.",
                            "target_lang_translations": {
                                "ru": "Очень трудно <w>различить</w> близнецов, потому что они выглядят одинаково."
                            }
                        },
                        {
                            "text": "I can <w>tell</w> that you are upset about the news.",
                            "target_lang_translations": {
                                "ru": "Я <w>вижу</w>, что ты расстроен из-за новостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "disclose"
                    ],
                    "common_phrases": [
                        "tell the difference",
                        "tell apart",
                        "hard to tell",
                        "you can tell",
                        "time will tell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распознавать, замечать, определять или различать что-то, часто путем наблюдения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "различать",
                                "target_lang_sense_clarification": "распознавать органами чувств"
                            },
                            {
                                "target_lang_word": "отличать",
                                "target_lang_sense_clarification": "находить разницу между объектами"
                            },
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "осознавать, делать вывод на основе наблюдений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcd38b90-4c40-3886-958b-71778737816e",
                    "sense_definition": "To count, reckon, or measure; primarily used today in specific phrases regarding time or totals.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "calculation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My young son is just learning how to <w>tell</w> time on a clock.",
                            "target_lang_translations": {
                                "ru": "Мой маленький сын только учится <w>определять</w> время по часам."
                            }
                        },
                        {
                            "text": "All <w>told</w>, there were more than fifty people at the event.",
                            "target_lang_translations": {
                                "ru": "В общей <w>сложности</w> на мероприятии было более пятидесяти человек."
                            }
                        }
                    ],
                    "synonyms": [
                        "count",
                        "number"
                    ],
                    "common_phrases": [
                        "tell time",
                        "all told"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "The general sense of 'counting' is archaic, but it remains standard in 'tell time' and 'all told'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать или измерять; в современном языке используется преимущественно в фразах о времени или общих итогах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "устанавливать (время)"
                            },
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "вести счет (устаревшее значение, кроме идиом)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30f9463d-5c43-3b2d-96cb-2139e115e494",
                    "sense_definition": "To inform someone in authority about a wrongdoing; to report on someone's bad behavior.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He threatened to <w>tell</w> on his brother if he didn't share the candy.",
                            "target_lang_translations": {
                                "ru": "Он угрожал <w>наябедничать</w> на брата, если тот не поделится конфетами."
                            }
                        },
                        {
                            "text": "I'm not going to <w>tell</w>, so your secret is safe with me.",
                            "target_lang_translations": {
                                "ru": "Я никому не <w>расскажу</w>, так что твой секрет со мной в безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "snitch",
                        "tattle",
                        "grass up",
                        "tell on"
                    ],
                    "common_phrases": [
                        "tell on someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщать кому-то, имеющему власть, о проступке; доносить на кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ябедничать",
                                "target_lang_sense_clarification": "доносить по мелочам (разговорное, детское)"
                            },
                            {
                                "target_lang_word": "доносить",
                                "target_lang_sense_clarification": "сообщать о чьей-то вине властям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1f6e844-449b-3ab1-9d73-1d6a7b9bd891",
                    "sense_definition": "To have a noticeable effect or impact, often a negative one.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The long hours of work are beginning to <w>tell</w> on his health.",
                            "target_lang_translations": {
                                "ru": "Долгие часы работы начинают <w>сказываться</w> на его здоровье."
                            }
                        },
                        {
                            "text": "Her lack of sleep finally <w>told</w> during the important exam.",
                            "target_lang_translations": {
                                "ru": "Её недосыпание в конце концов <w>сказалось</w> во время важного экзамена."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beginning to tell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать заметное влияние или воздействие, часто негативное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сказываться",
                                "target_lang_sense_clarification": "проявляться, отражаться на чем-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "82d7c360-e0a1-3fca-a9e5-ecc62d36f38d",
                    "sense_definition": "An unconscious action or behavior that reveals a secret, deception, or inner state, often used in the context of poker or psychology.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Touching his nose is a reliable <w>tell</w> that he is bluffing.",
                            "target_lang_translations": {
                                "ru": "Прикосновение к носу — это верный <w>признак</w> того, что он блефует."
                            }
                        },
                        {
                            "text": "The detective looked for a <w>tell</w> in the suspect's body language.",
                            "target_lang_translations": {
                                "ru": "Детектив искал <w>зацепку</w> в языке тела подозреваемого."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poker tell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неосознанное действие или поведение, выдающее секрет или внутреннее состояние (термин в покере)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признак",
                                "target_lang_sense_clarification": "знак, примета"
                            },
                            {
                                "target_lang_word": "выдающий жест",
                                "target_lang_sense_clarification": "поведение, раскрывающее намерения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bc3f6df-9227-3aa9-83d0-8ec321110454",
                    "sense_definition": "A hill or mound, typically in the Middle East, consisting of the accumulated debris of ancient settlements.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Archaeologists spent years excavating the ancient <w>tell</w> to find artifacts.",
                            "target_lang_translations": {
                                "ru": "Археологи потратили годы на раскопки древнего <w>телля</w>, чтобы найти артефакты."
                            }
                        },
                        {
                            "text": "The city was built upon a <w>tell</w> formed by centuries of habitation.",
                            "target_lang_translations": {
                                "ru": "Город был построен на <w>телле</w>, образованном веками обитания."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Originally and especially in the Middle East."
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "Used in archaeology."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Холм или курган (обычно на Ближнем Востоке), образованный накопившимися остатками древних поселений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телль",
                                "target_lang_sense_clarification": "археологический термин для холма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "85bd5458-4847-394e-a211-83ebdce9d973",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Tell</w> is waiting in the lobby.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Телль</w> ждет в вестибюле."
                            }
                        },
                        {
                            "text": "We visited the <w>Tell</w> family last weekend.",
                            "target_lang_translations": {
                                "ru": "Мы навестили семью <w>Телль</w> в прошлые выходные."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Телль",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
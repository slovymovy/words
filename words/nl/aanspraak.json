                    "sense_id": "a4ba04f6-b5c6-3251-b42c-51183497fc4c",
                    "sense_definition": "Het recht om iets te vorderen of te eisen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De werknemer maakt <w>aanspraak</w> op een hogere reiskostenvergoeding.",
                            "target_lang_translations": {

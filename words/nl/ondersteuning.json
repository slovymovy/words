        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6a3d7452-6e30-3d53-9838-5676fd9982b1",
                    "sense_definition": "Het geven van hulp, middelen of aanmoediging aan iemand of iets om een doel te bereiken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_help",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zonder jouw <w>ondersteuning</w> was het project nooit gelukt.",
                            "target_lang_translations": {
                                "en": "Without your <w>support</w>, the project would never have succeeded.",
                                "ru": "Без твоей <w>поддержки</w> проект никогда бы не удался."
                            }
                        },
                        {
                            "text": "Het bedrijf biedt technische <w>ondersteuning</w> aan klanten met computerproblemen.",
                            "target_lang_translations": {
                                "en": "The company offers technical <w>support</w> to customers with computer problems.",
                                "ru": "Компания предлагает техническую <w>поддержку</w> клиентам с проблемами с компьютером."
                            }
                        }
                    ],
                    "synonyms": [
                        "hulp",
                        "bijstand",
                        "steun"
                    ],
                    "antonyms": [
                        "tegenwerking"
                    ],
                    "common_phrases": [
                        "technische ondersteuning",
                        "financiële ondersteuning",
                        "maatschappelijke ondersteuning"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of providing help, resources, or encouragement to someone or something to achieve a goal.",
                        "ru": "Предоставление помощи, ресурсов или поощрения кому-либо или чему-либо для достижения цели."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "support",
                                "target_lang_sense_clarification": "general assistance or encouragement"
                            },
                            {
                                "target_lang_word": "assistance",
                                "target_lang_sense_clarification": "help provided to achieve a task"
                            },
                            {
                                "target_lang_word": "backing",
                                "target_lang_sense_clarification": "endorsement or financial help"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поддержка",
                                "target_lang_sense_clarification": "общая помощь или одобрение"
                            },
                            {
                                "target_lang_word": "помощь",
                                "target_lang_sense_clarification": "содействие в достижении цели"
                            }
                        ]

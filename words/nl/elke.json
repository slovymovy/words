                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Inflected form of 'elk': used before a plural noun, usually in combination with a numeral or quantifier."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "every",

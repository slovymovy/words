    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "32b41df7-b461-306b-9207-87fa5cc2359f",
                    "sense_definition": "Duży ssak leśny, którego samce (byki) charakteryzują się okazałym, rozgałęzionym porożem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "zwierzęta",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas spaceru po lesie zobaczyliśmy pięknego <w>jelenia</w> stojącego na polanie.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки по лесу мы увидели красивого <w>оленя</w>, стоящего на поляне."
                            }
                        },
                        {
                            "text": "Każdej zimy samiec <w>jelenia</w> zrzuca swoje poroże, które potem odrasta.",
                            "target_lang_translations": {
                                "ru": "Каждую зиму самец <w>оленя</w> сбрасывает свои рога, которые потом отрастают."
                            }
                        }
                    ],
                    "synonyms": [
                        "byk",
                        "jeleniec"
                    ],
                    "common_phrases": [
                        "stado jeleni",
                        "poroże jelenia",
                        "rykowisko jeleni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное лесное млекопитающее, самцы которого (быки) отличаются большими ветвистыми рогами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "олень",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04d1696f-f4f0-3fe5-93a7-37803b1cd806",
                    "sense_definition": "Osoba naiwna, którą łatwo oszukać, naciągnąć na pieniądze lub wykorzystać.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cechy_charakteru",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź takim <w>jeleniem</w> i nie pożyczaj pieniędzy nieznajomym ludziom.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>лохом</w> и не одалживай деньги незнакомым людям."
                            }
                        },
                        {
                            "text": "Oszuści internetowi ciągle próbują znaleźć jakiegoś <w>jelenia</w>, który kliknie w podejrzany link.",
                            "target_lang_translations": {
                                "ru": "Интернет-мошенники постоянно пытаются найти какого-нибудь <w>лоха</w>, который кликнет по подозрительной ссылке."
                            }
                        }
                    ],
                    "synonyms": [
                        "frajer",
                        "naiwniak",
                        "łoś",
                        "ofiara",
                        "durniacha"
                    ],
                    "common_phrases": [
                        "szukać jelenia",
                        "zrobić z kogoś jelenia"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "potocznie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наивный человек, которого легко обмануть, развести на деньги или использовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лох",
                                "target_lang_sense_clarification": "разг., жертва обмана"
                            },
                            {
                                "target_lang_word": "простак",
                                "target_lang_sense_clarification": "наивный человек"
                            },
                            {
                                "target_lang_word": "олень",
                                "target_lang_sense_clarification": "слэнг, глупый человек"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "60213edd-031d-3474-8ac7-4e3ccac7d949",
                    "sense_definition": "Polskie nazwisko, noszone zarówno przez mężczyzn, jak i kobiety.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Jeleń</w> został wybrany na przewodniczącego naszej wspólnoty mieszkaniowej.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Елень</w> был избран председателем нашего жилищного кооператива."
                            }
                        },
                        {
                            "text": "Wczoraj listonosz przyniósł ważną przesyłkę dla państwa <w>Jeleniów</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера почтальон принёс важную посылку для семьи <w>Елень</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "nazwisko"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия, которую носят как мужчины, так и женщины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Елень",
                                "target_lang_sense_clarification": "фамилия"

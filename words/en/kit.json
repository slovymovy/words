    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "96a9083d-16ac-3b03-8653-3e757496648c",
                    "sense_definition": "A set of tools, supplies, or equipment needed for a specific purpose.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "equipment_set",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She keeps a first-aid <w>kit</w> in the trunk of her car.",
                            "target_lang_translations": {
                                "ru": "Она хранит <w>аптечку</w> в багажнике своей машины."
                            }
                        },
                        {
                            "text": "He bought a shaving <w>kit</w> before going on his business trip.",
                            "target_lang_translations": {
                                "ru": "Он купил бритвенный <w>набор</w> перед командировкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "kit out",
                        "kit violin"
                    ],
                    "common_phrases": [
                        "first-aid kit",
                        "tool kit",
                        "survival kit",
                        "travel kit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор инструментов, припасов или оборудования для определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набор",
                                "target_lang_sense_clarification": "инструментов, принадлежностей"
                            },
                            {
                                "target_lang_word": "комплект",
                                "target_lang_sense_clarification": "оборудования"
                            },
                            {
                                "target_lang_word": "аптечка",
                                "target_lang_sense_clarification": "первой помощи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fd79a76-3704-357a-ae63-a3e6f991c71a",
                    "sense_definition": "The standard set of clothing and equipment worn by players of a sports team.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The football team wore their new red <w>kit</w> for the final match.",
                            "target_lang_translations": {
                                "ru": "Футбольная команда надела свою новую красную <w>форму</w> на финальный матч."
                            }
                        },
                        {
                            "text": "You need to wash your gym <w>kit</w> after school.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно постирать свою спортивную <w>форму</w> после школы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "football kit",
                        "home kit",
                        "away kit",
                        "gym kit"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандартный набор одежды и снаряжения для игроков спортивной команды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "форма",
                                "target_lang_sense_clarification": "спортивная одежда"
                            },
                            {
                                "target_lang_word": "экипировка",
                                "target_lang_sense_clarification": "снаряжение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e986e741-24a6-38df-b0d6-59c5a4071e3f",
                    "sense_definition": "A set of separate parts sold together to be assembled by the buyer.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "assembly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I built this model airplane from a <w>kit</w> I bought at the hobby shop.",
                            "target_lang_translations": {
                                "ru": "Я собрал эту модель самолета из <w>набора</w>, который купил в магазине хобби."
                            }
                        },
                        {
                            "text": "The furniture comes in a <w>kit</w> with all the necessary screws.",
                            "target_lang_translations": {
                                "ru": "Мебель поставляется в <w>комплекте</w> со всеми необходимыми винтами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "model kit",
                        "assembly kit",
                        "kit car"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор отдельных деталей, продаваемых вместе для сборки покупателем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набор",

{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "408255b5-89f2-354f-a04c-30a43dda28e1",
                    "sense_definition": "Duidt het beginpunt van een tijdsperiode of een specifiek moment aan waarna iets geldt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je rijbewijs is pas <w>vanaf</w> maandag geldig.",
                            "target_lang_translations": {
                                "en": "Your driver's license is only valid <w>from</w> Monday.",
                                "ru": "Твои водительские права действительны только <w>с</w> понедельника."
                            }
                        },
                        {
                            "text": "We gaan <w>vanaf</w> nu elke dag samen oefenen voor het examen.",
                            "target_lang_translations": {
                                "en": "We are going to practice together for the exam every day <w>from</w> now on.",
                                "ru": "<w>С</w> этого момента мы будем каждый день вместе готовиться к экзамену."
                            }
                        }
                    ],
                    "antonyms": [
                        "tot",
                        "tot en met"
                    ],
                    "common_phrases": [
                        "vanaf nu",
                        "vanaf morgen",
                        "vanaf het begin"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the starting point of a time period or a specific moment from which something applies.",
                        "ru": "Указывает на начальный момент периода времени или конкретный момент, после которого что-либо действует."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "starting point in time"
                            },
                            {
                                "target_lang_word": "as of",
                                "target_lang_sense_clarification": "formal start date"
                            },
                            {
                                "target_lang_word": "starting from",
                                "target_lang_sense_clarification": "emphasis on the beginning"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "предлог времени"
                            },
                            {
                                "target_lang_word": "начиная с",
                                "target_lang_sense_clarification": "для усиления значения начала периода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef9aa9df-8f88-3e0f-bd01-2f48cdcf4219",
                    "sense_definition": "Duidt een fysiek vertrekpunt of een plaats aan waarvandaan een handeling plaatsvindt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er staat iemand <w>vanaf</w> het dak naar ons te roepen.",
                            "target_lang_translations": {
                                "en": "Someone is shouting at us <w>from</w> the roof.",
                                "ru": "Кто-то кричит нам <w>с</w> крыши."
                            }
                        },
                        {
                            "text": "<w>Vanaf</w> hier is het nog ongeveer een uur rijden naar de kust.",
                            "target_lang_translations": {
                                "en": "<w>From</w> here it is about an hour's drive to the coast.",
                                "ru": "<w>От</w>сюда до побережья ехать еще около часа."
                            }
                        }
                    ],
                    "antonyms": [
                        "tot"
                    ],
                    "common_phrases": [
                        "vanaf hier",
                        "vanaf daar",
                        "vanaf de zijkant"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a physical starting point or a place from which an action originates.",
                        "ru": "Указывает на физическую точку отправления или место, откуда происходит действие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "origin or location"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "движение с поверхности или сверху"
                            },
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "движение от объекта или точки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "17e0ddce-b31e-3682-943b-dc844c734605",
                    "sense_definition": "Verlost of bevrijd zijn van een vervelende zaak, last of verplichting.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rid_of",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft dat dure jacht verkocht, dus daar is hij mooi <w>vanaf</w>.",
                            "target_lang_translations": {
                                "en": "He sold that expensive yacht, so he is nicely <w>rid of</w> it.",
                                "ru": "Он продал ту дорогую яхту, так что он от нее удачно <w>избавился</w>."
                            }
                        },
                        {
                            "text": "Ik ben gelukkig eindelijk <w>vanaf</w> van die zware verkoudheid.",
                            "target_lang_translations": {
                                "en": "Fortunately, I am finally <w>rid of</w> that heavy cold.",
                                "ru": "К счастью, я наконец <w>избавился</w> от этой сильной простуды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "er vanaf zijn",
                        "er mooi vanaf komen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be relieved or freed from an annoying matter, burden, or obligation.",
                        "ru": "Быть избавленным или освобожденным от неприятного дела, бремени или обязательства."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rid of",
                                "target_lang_sense_clarification": "freed from something unpleasant"
                            },
                            {
                                "target_lang_word": "done with",
                                "target_lang_sense_clarification": "finished with something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "избавиться",
                                "target_lang_sense_clarification": "освободиться от чего-то (в составе выражения)"
                            },
                            {
                                "target_lang_word": "освободиться",
                                "target_lang_sense_clarification": "стать свободным от обязательств"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "common_phrases": [
                        "instrument pomiarowy",
                        "instrument optyczny",
                        "instrument nawigacyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точное техническое устройство для выполнения измерений, наблюдений или управления процессами."

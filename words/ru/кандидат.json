                    "target_lang_definitions": {
                        "en": "The first postgraduate academic degree in Russia (Candidate of Sciences), or a person holding this degree."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Candidate of Sciences",

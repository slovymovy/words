            "pos": "verb",
            "senses": [
                {
                    "sense_id": "82ed6b7e-e0cf-3c38-89a8-422ff1ca4dca",
                    "sense_definition": "Ustawiać tekst lub obiekt graficzny na środku strony, ekranu lub wyznaczonego obszaru w programie komputerowym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "alignment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Edytor tekstu pozwala automatycznie <w>centrować</w> tytuły rozdziałów.",
                            "target_lang_translations": {
                                "ru": "Текстовый редактор позволяет автоматически <w>центрировать</w> заголовки глав."
                            }
                        },
                        {
                            "text": "Musisz <w>centrować</w> obrazek, aby strona internetowa wyglądała estetycznie.",
                            "target_lang_translations": {
                                "ru": "Нужно <w>выровнять по центру</w> изображение, чтобы веб-страница выглядела эстетично."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyśrodkowywać"
                    ],
                    "common_phrases": [
                        "centrować tekst",
                        "centrować w poziomie",
                        "centrować obraz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размещать текст или графический объект посередине страницы, экрана или выделенной области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "центрировать",
                                "target_lang_sense_clarification": "текст или изображение"
                            },
                            {
                                "target_lang_word": "выравнивать по центру",
                                "target_lang_sense_clarification": "форматирование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cb045c7-1c87-383a-8792-98704424742e",
                    "sense_definition": "Regulować naciąg szprych w kole (zwykle rowerowym lub motocyklowym), aby obracało się równo i nie biło na boki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "alignment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat potrafi samodzielnie <w>centrować</w> koła w swoim rowerze górskim.",
                            "target_lang_translations": {
                                "ru": "Мой брат умеет самостоятельно <w>центровать</w> колеса на своем горном велосипеде."
                            }
                        },
                        {
                            "text": "Po wjechaniu w dużą dziurę musiałem oddać rower do serwisu, by <w>centrować</w> tylną obręcz.",
                            "target_lang_translations": {
                                "ru": "После попадания в большую яму мне пришлось отдать велосипед в сервис, чтобы <w>выправить</w> задний обод."
                            }
                        }
                    ],
                    "antonyms": [
                        "krzywić",
                        "scentrować"
                    ],
                    "common_phrases": [
                        "centrować koło",
                        "centrować szprychy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулировать натяжение спиц в колесе для устранения искривления и обеспечения ровного вращения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "центровать",
                                "target_lang_sense_clarification": "колесо или спицы"
                            },
                            {
                                "target_lang_word": "выправлять",
                                "target_lang_sense_clarification": "устранять дефекты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6a28f52-c5c9-3d15-be7c-2b1a7444fbb0",
                    "sense_definition": "Podawać piłkę ze skrzydła (bocznej części boiska) w pole karne, w kierunku bramki przeciwnika.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_pass",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Skrzydłowy biegł szybko wzdłuż linii bocznej, aby <w>centrować</w> piłkę do napastników.",
                            "target_lang_translations": {
                                "ru": "Вингер быстро бежал вдоль боковой линии, чтобы <w>навесить</w> мяч нападающим."
                            }
                        },
                        {
                            "text": "Zamiast strzelać na bramkę, zawodnik wolał <w>centrować</w> w tłok przed bramkarzem.",
                            "target_lang_translations": {
                                "ru": "Вместо удара по воротам игрок предпочел <w>сделать навес</w> в скопление игроков перед вратарем."
                            }
                        }
                    ],
                    "synonyms": [
                        "dośrodkowywać",
                        "docentrowywać"
                    ],
                    "common_phrases": [
                        "centrować w pole karne",
                        "centrować na głowę",
                        "centrować piłkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать передачу мяча с фланга в штрафную площадь к воротам соперника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навешивать",
                                "target_lang_sense_clarification": "верховая передача"
                            },
                            {
                                "target_lang_word": "делать навес",
                                "target_lang_sense_clarification": "футбол"
                            },
                            {
                                "target_lang_word": "простреливать",
                                "target_lang_sense_clarification": "сильная передача низом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab79ad00-06d8-322c-a1d0-4a9edfb76f36",
                    "sense_definition": "Ustawiać elementy maszyny lub konstrukcji tak, aby ich środki znajdowały się na jednej, wspólnej osi.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "alignment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Inżynier musi precyzyjnie <w>centrować</w> wał napędowy, aby uniknąć wibracji.",
                            "target_lang_translations": {
                                "ru": "Инженер должен точно <w>центровать</w> приводной вал, чтобы избежать вибраций."
                            }

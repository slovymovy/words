                    "sense_id": "fa85b00a-a7b4-3bf6-a631-88a3d1d1be2c",
                    "sense_definition": "Relating to a specific country; internal to a country rather than foreign or international.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The airline operates both international and <w>domestic</w> flights every day.",
                            "target_lang_translations": {
                                "ru": "Авиакомпания выполняет как международные, так и <w>внутренние</w> рейсы каждый день."
                            }

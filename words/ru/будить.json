    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "faf36d1a-4242-3fbe-9c46-2fffb36d2fa9",
                    "sense_definition": "Прерывать чей-либо сон, заставлять кого-либо проснуться.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама обычно приходит <будить> меня в семь часов утра.",
                            "target_lang_translations": {
                                "en": "Mom usually comes to <wake> me at seven in the morning."
                            }
                        },
                        {
                            "text": "Пожалуйста, не надо <будить> собаку, пусть она поспит.",
                            "target_lang_translations": {
                                "en": "Please, don't <wake> the dog, let her sleep."
                            }
                        }
                    ],
                    "synonyms": [
                        "пробуждать"
                    ],
                    "antonyms": [
                        "усыплять"
                    ],
                    "common_phrases": [
                        "будить рано",
                        "будить детей",
                        "будить по утрам"
                    ],
                    "target_lang_definitions": {
                        "en": "To interrupt someone's sleep or cause someone to wake up."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wake up",
                                "target_lang_sense_clarification": "to cause someone to stop sleeping"
                            },
                            {
                                "target_lang_word": "awaken",
                                "target_lang_sense_clarification": "to cause to become awake (formal)"
                            },
                            {
                                "target_lang_word": "rouse",
                                "target_lang_sense_clarification": "to wake someone from sleep (formal or forceful)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4a3e996-de75-3af1-ab8f-29c43bf58b50",
                    "sense_definition": "Вызывать какие-либо чувства, мысли или воспоминания; возбуждать интерес или активность.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта старая мелодия <будит> во мне приятные воспоминания о детстве.",
                            "target_lang_translations": {
                                "en": "This old melody <awakens> pleasant childhood memories in me."
                            }
                        },
                        {
                            "text": "Его рассказы о приключениях <будят> желание путешествовать.",
                            "target_lang_translations": {
                                "en": "His stories about adventures <awaken> the desire to travel."
                            }
                        }
                    ],
                    "synonyms": [
                        "пробуждать",
                        "вызывать",
                        "возбуждать",
                        "вызывать к жизни"
                    ],
                    "antonyms": [
                        "усыплять"
                    ],
                    "common_phrases": [
                        "будить воспоминания",
                        "будить интерес",
                        "будить зверя"
                    ],
                    "target_lang_definitions": {
                        "en": "To evoke feelings, thoughts, or memories; to stimulate interest or activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "awaken",
                                "target_lang_sense_clarification": "to bring feelings or interests into action"
                            },
                            {
                                "target_lang_word": "arouse",
                                "target_lang_sense_clarification": "to stimulate or induce feelings or interest"
                            },
                            {
                                "target_lang_word": "rouse",
                                "target_lang_sense_clarification": "to stir up or excite emotions"
                            }
                        ]
                    }
                }
            ]
        }
    ]

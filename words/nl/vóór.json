    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "818bd234-e789-362f-8c83-e15530cb5cd9",
                    "sense_definition": "Eerder in tijd of plaats dan iets of iemand anders, met nadruk op het contrast met 'na' of 'achter'.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "prep_time_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet <w>vóór</w> het huis parkeren, niet ernaast.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно припарковаться <w>перед</w> домом, а не сбоку."
                            }
                        },
                        {
                            "text": "Maandag pas? U zou mijn tv <w>vóór</w> het weekeind bezorgen!",
                            "target_lang_translations": {
                                "ru": "Только в понедельник? Вы должны были доставить мой телевизор <w>до</w> выходных!"
                            }
                        }
                    ],
                    "synonyms": [
                        "voorafgaand aan"
                    ],
                    "antonyms": [
                        "na",
                        "achter"
                    ],
                    "common_phrases": [
                        "vóór en na"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "benadrukte vorm van 'voor'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Раньше по времени или месту, чем что-то или кто-то другой, с акцентом на контрасте с «после» или «сзади»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перед",
                                "target_lang_sense_clarification": "о месте"
                            },
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "о времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba3582e4-f2f1-3b32-b14c-067ed9daa85e",
                    "sense_definition": "Voorafgaand aan een tijdstip, specifiek benadrukt om verwarring met de betekenis 'ten behoeve van' te voorkomen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "prep_disambiguation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij wou een verlovingsring <w>vóór</w> haar verjaardag hebben, zodat ze hem op de dag zelf kon dragen.",
                            "target_lang_translations": {
                                "ru": "Она хотела получить помолвочное кольцо <w>до</w> своего дня рождения, чтобы надеть его в этот день."
                            }
                        },
                        {
                            "text": "Zorg dat je het rapport <w>vóór</w> de vergadering inlevert.",
                            "target_lang_translations": {
                                "ru": "Убедись, что сдашь отчет <w>до</w> собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorafgaand aan"
                    ],
                    "antonyms": [
                        "na"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "benadrukte vorm van 'voor'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предшествующий определенному моменту времени; подчеркивается, чтобы избежать путаницы со значением «для» (ten behoeve van)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "временной предел"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9e19a3f5-c24f-3c83-9f6c-b4bd4007b0a0",
                    "sense_definition": "In het voordeel (bij een wedstrijd) of instemmend (bij een voorstel).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "adv_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze staan niet 2-0 achter, maar 2-0 <w>vóór</w>!",
                            "target_lang_translations": {
                                "ru": "Они не проигрывают 2-0, они <w>впереди</w> на 2-0!"
                            }
                        },
                        {
                            "text": "Hij houdt niet zo van vliegreizen, maar toen we naar Bali konden, was hij <w>vóór</w>.",
                            "target_lang_translations": {
                                "ru": "Он не очень любит летать, но когда появилась возможность полететь на Бали, он был <w>за</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "eens",
                        "aan de leiding"
                    ],
                    "antonyms": [
                        "tegen",
                        "achter"
                    ],
                    "common_phrases": [
                        "voor of tegen",
                        "sta je voor"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "benadrukte vorm van 'voor'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В выигрышном положении (в соревновании) или выражающий согласие (с предложением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "согласие"
                            },
                            {
                                "target_lang_word": "впереди",
                                "target_lang_sense_clarification": "преимущество в счете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2daa77a-9ee7-3a98-8fcb-27173d6df22c",
                    "sense_definition": "Aan de voorzijde, met nadrukkelijke uitsluiting van andere posities.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "adv_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij ging er <w>vóór</w> liggen om de doorgang te blokkeren.",
                            "target_lang_translations": {
                                "ru": "Он лег <w>спереди</w>, чтобы заблокировать проход."
                            }
                        },
                        {
                            "text": "Kijk niet achter de kast, maar <w>vóór</w> de kast.",
                            "target_lang_translations": {
                                "ru": "Смотри не за шкафом, а <w>перед</w> шкафом."
                            }
                        }
                    ],
                    "synonyms": [
                        "ervoor"
                    ],
                    "antonyms": [
                        "erachter"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "benadrukte vorm van 'voor'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "С передней стороны, с подчеркнутым исключением других позиций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спереди",
                                "target_lang_sense_clarification": "наречие места"
                            },
                            {
                                "target_lang_word": "перед",
                                "target_lang_sense_clarification": "в сочетании с местоимением"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "948858c6-df42-3129-b481-381bff953e3b",
                    "sense_definition": "Eerder in de tijd dan dat; voordat.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "conj_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de tekst controleren <w>vóór</w> je hem publiceert.",
                            "target_lang_translations": {
                                "ru": "Ты должен проверить текст, <w>прежде чем</w> опубликуешь его."
                            }
                        },
                        {
                            "text": "Denk goed na <w>vóór</w> je iets zegt waar je spijt van krijgt.",
                            "target_lang_translations": {
                                "ru": "Хорошенько подумай, <w>прежде чем</w> скажешь то, о чем пожалеешь."
                            }
                        }
                    ],
                    "synonyms": [
                        "voordat",
                        "alvorens"
                    ],
                    "antonyms": [
                        "nadat"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "benadrukte vorm van 'voor'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Раньше по времени, чем что-то произошло; перед тем как."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прежде чем",
                                "target_lang_sense_clarification": "союз"
                            },
                            {
                                "target_lang_word": "перед тем как",
                                "target_lang_sense_clarification": "союз"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
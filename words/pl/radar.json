            "senses": [
                {
                    "sense_id": "e7b83c27-6f42-3eb6-849f-1a4c6d2324e1",
                    "sense_definition": "Urządzenie emitujące fale elektromagnetyczne, służące do wykrywania, lokalizowania i śledzenia obiektów, takich jak samoloty, statki czy pojazdy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "device_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja ustawiła przenośny <w>radar</w> przy wyjeździe z tunelu, aby kontrolować prędkość.",
                            "target_lang_translations": {
                                "ru": "Полиция установила переносной <w>радар</w> на выезде из туннеля, чтобы контролировать скорость."
                            }
                        },
                        {
                            "text": "Gęsta mgła zmusiła kapitana statku do nawigowania wyłącznie w oparciu o <w>radar</w>.",
                            "target_lang_translations": {
                                "ru": "Густой туман заставил капитана корабля вести судно, полагаясь исключительно на <w>радар</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "radiolokator"
                    ],
                    "common_phrases": [
                        "radar policyjny",
                        "radar pogodowy",
                        "namierzyć radarem",
                        "zasięg radaru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, излучающее электромагнитные волны, служащее для обнаружения, локализации и отслеживания объектов, таких как самолеты, корабли или транспортные средства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радар",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "радиолокатор",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0c17eb8-60a9-3d8d-a050-b5ea7bb6679a",
                    "sense_definition": "Ekran lub wyświetlacz urządzenia radiolokacyjnego, na którym prezentowane jest położenie wykrytych obiektów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "device_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kontroler lotów z niepokojem obserwował znikający punkt na <w>radarze</w>.",
                            "target_lang_translations": {
                                "ru": "Авиадиспетчер с тревогой наблюдал за исчезающей точкой на <w>радаре</w>."
                            }
                        },
                        {
                            "text": "Samolot nieprzyjaciela był doskonale widoczny na <w>radarze</w> mimo zachmurzenia.",
                            "target_lang_translations": {
                                "ru": "Самолет противника был отлично виден на <w>радаре</w>, несмотря на облачность."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na radarze",
                        "zniknąć z radaru",
                        "patrzeć w radar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Экран или дисплей радиолокационного устройства, на котором отображается положение обнаруженных объектов."
                    },
                    "translations": {

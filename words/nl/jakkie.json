{
    "word_family": [
        "jak",
        "jakkes"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "a1eb0fb9-7af0-36a0-8930-ffdb0f1d2a69",
                    "sense_definition": "Uitroep van afkeer of walging, meestal als reactie op iets dat vies ruikt, smaakt of eruitziet.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "disgust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jakkie, deze melk is helemaal zuur geworden.",
                            "target_lang_translations": {
                                "ru": "<w>Фу</w>, это молоко совсем скисло."
                            }
                        },
                        {
                            "text": "Hij stapte per ongeluk in een hondendrol, <w>jakkie</w>!",
                            "target_lang_translations": {
                                "ru": "Он случайно наступил в собачью кучу, <w>фу</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "bah",
                        "gadver",
                        "gatsie",
                        "jakkes"
                    ],
                    "antonyms": [
                        "jammie",
                        "mmm",
                        "heerlijk"
                    ],
                    "common_phrases": [
                        "jakkie bah"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас отвращения или брезгливости, обычно как реакция на что-то, что плохо пахнет, неприятно на вкус или выглядит грязно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фу",
                                "target_lang_sense_clarification": "выражение физического отвращения"
                            },
                            {
                                "target_lang_word": "бе",
                                "target_lang_sense_clarification": "разговорное, часто детское выражение отвращения"
                            },
                            {
                                "target_lang_word": "гадость",
                                "target_lang_sense_clarification": "оценка неприятного предмета или субстанции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7e7cedb-de9c-303c-91e0-01071952e358",
                    "sense_definition": "Uitroep van teleurstelling of afkeuring over een vervelende situatie of naar bericht.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "dismay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hè <w>jakkie</w>, nu begint het net te regenen terwijl we naar buiten willen.",
                            "target_lang_translations": {
                                "ru": "Ну <w>фу</w>, дождь начинается как раз когда мы хотели выйти."
                            }
                        },
                        {
                            "text": "Wat een vervelend nieuws is dat zeg, <w>jakkie</w>.",
                            "target_lang_translations": {
                                "ru": "Какие неприятные новости, <w>кошмар</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hè",
                        "getver"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас разочарования или неодобрения по поводу неприятной ситуации или плохой новости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фу",
                                "target_lang_sense_clarification": "выражение недовольства ситуацией"
                            },
                            {
                                "target_lang_word": "гадость",
                                "target_lang_sense_clarification": "о неприятном событии"
                            },
                            {
                                "target_lang_word": "кошмар",
                                "target_lang_sense_clarification": "эмоциональная реакция на плохую новость"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00a6b4e3-8a60-3c09-9f25-38622a245762",
                    "sense_definition": "Verkleinwoord van <w>jak</w>; een kort, nauwsluitend kledingstuk voor het bovenlichaam, vroeger gedragen door vrouwen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het meisje droeg een kleurrijk <w>jakkie</w> tijdens de optocht in klederdracht.",
                            "target_lang_translations": {
                                "ru": "Во время парада в национальных костюмах на девочке был пестрый <w>жакетик</w>."
                            }
                        },
                        {
                            "text": "Mijn oma had vroeger geen geld om een mooi <w>jakkie</w> te kopen.",
                            "target_lang_translations": {
                                "ru": "Раньше у моей бабушки не было денег, чтобы купить красивую <w>кофточку</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "refers to historical clothing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от \"jak\"; короткая, облегающая верхняя одежда для верхней части тела, которую раньше носили женщины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жакетик",
                                "target_lang_sense_clarification": "уменьшительное от жакет"
                            },
                            {
                                "target_lang_word": "кофточка",
                                "target_lang_sense_clarification": "предмет женской одежды"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
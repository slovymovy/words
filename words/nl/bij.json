                },
                {
                    "sense_id": "7bbd9ae1-c1c0-34d1-9f78-d561def3fd27",
                    "sense_definition": "In het huis van of in het gezelschap van (iemand).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locatie_persoon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Bij</w> ons thuis eten we altijd om zes uur.",
                            "target_lang_translations": {
                                "en": "<w>At</w> our place we always eat at six o'clock.",
                                "ru": "<w>У</w> нас дома мы всегда едим в шесть часов."
                            }
                        },
                        {
                            "text": "Heb je toevallig wat geld <w>bij</w> je?",
                            "target_lang_translations": {
                                "en": "Do you happen to have some money <w>on</w> you?",
                                "ru": "У тебя случайно нет <w>с</w> собой денег?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "bij iemand thuis",
                        "bij elkaar"

    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "73ee613e-0e4e-3c29-b519-7c5a232bfac6",
                    "sense_definition": "An archaic or dialectal word for \"you\", used when addressing a group of people.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "pronoun_you",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O come, all <w>ye</w> faithful, joyful and triumphant.",
                            "target_lang_translations": {
                                "ru": "О придите, все <w>вы</w>, верные, радостные и торжествующие."
                            }
                        },
                        {
                            "text": "Seek and <w>ye</w> shall find the truth.",
                            "target_lang_translations": {
                                "ru": "Ищите, и <w>вы</w> найдете истину."
                            }
                        }
                    ],
                    "synonyms": [
                        "y'all"
                    ],
                    "common_phrases": [
                        "hear ye",
                        "thank ye"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "standard in older English texts like the Bible"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "still used in some regional dialects (e.g., Ireland, Newfoundland)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее или диалектное слово для обращения к группе людей («вы»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "устаревшее или торжественное обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "561bdcd8-79d9-3a35-8b6a-060a8ae88267",
                    "sense_definition": "An archaic or dialectal word for \"you\", used when addressing a single person.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "pronoun_you",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Know <w>ye</w> the reason for this meeting?",
                            "target_lang_translations": {
                                "ru": "Знаете ли <w>вы</w> причину этого собрания?"
                            }
                        },
                        {
                            "text": "If <w>ye</w> believe in magic, clap your hands.",
                            "target_lang_translations": {
                                "ru": "Если <w>вы</w> верите в магию, похлопайте в ладоши."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "standard in older English"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее или диалектное слово для обращения к одному человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "уважительное или устаревшее обращение"
                            },
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "диалектное или фамильярное обращение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "2b5a1893-207e-36db-8800-d041b3388204",
                    "sense_definition": "An archaic spelling of the definite article \"the\", often used in modern times to make names of shops or pubs appear old-fashioned.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We stopped for lunch at a pub called <w>Ye</w> Olde Cheshire Cheese.",
                            "target_lang_translations": {
                                "ru": "Мы остановились пообедать в пабе под названием «<w>Старый</w> Чеширский Сыр»."
                            }
                        },
                        {
                            "text": "The sign above the door read \"<w>Ye</w> Olde Sweet Shoppe\".",
                            "target_lang_translations": {
                                "ru": "Вывеска над дверью гласила: «<w>Старинная</w> кондитерская лавка»."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Ye Olde"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "often pseudo-archaic in modern signage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Архаичное написание определенного артикля «the», используемое в названиях для стилизации под старину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старинный",
                                "target_lang_sense_clarification": "в составе стилизованных названий (Ye Olde)"
                            },
                            {
                                "target_lang_word": "старый",
                                "target_lang_sense_clarification": "в составе стилизованных названий"
                            }
                        ]
                    }
                }
            ]
        },
        {

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "держаться",
                                "target_lang_sense_clarification": "следовать определенной стороне пути"
                            },
                            {
                                "target_lang_word": "поворачивать",
                                "target_lang_sense_clarification": "менять направление движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf126598-f320-3c36-87cd-76dc504e4e2b",
                    "sense_definition": "To carry or bring something from one place to another.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "carry_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The messengers arrived <w>bearing</w> gifts for the king.",

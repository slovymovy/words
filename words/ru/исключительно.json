                        "исключительно из-за",
                        "исключительно в целях"
                    ],
                    "target_lang_definitions": {
                        "en": "Only, solely, or exclusively; used to single out a specific item, purpose, or group."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exclusively",
                                "target_lang_sense_clarification": "excluding everything else; solely"
                            },
                            {
                                "target_lang_word": "solely",
                                "target_lang_sense_clarification": "not involving anyone or anything else"
                            },
                            {
                                "target_lang_word": "only",
                                "target_lang_sense_clarification": "no one or nothing more besides"
                            }
                        ]
                    }
                }
            ]
        },
        {

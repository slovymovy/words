    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "566132fb-11c6-3d1c-9ecc-2e2b1c4d12a2",
                    "sense_definition": "Wysunąć część ciała lub przedmiot na zewnątrz czegoś lub w stronę słońca/wiatru.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pasażer nie powinien <w>wystawić</w> głowy przez otwarte okno pociągu.",
                            "target_lang_translations": {
                                "ru": "Пассажир не должен <w>высовывать</w> голову из открытого окна поезда."
                            }
                        },
                        {
                            "text": "Usiadłem na ławeczce pod ścianą, twarz <w>wystawiłem</w> do słońca i rozprostowałem nogi.",
                            "target_lang_translations": {
                                "ru": "Я сел на скамейку у стены, <w>подставил</w> лицо солнцу и вытянул ноги."
                            }
                        },
                        {
                            "text": "Dziecko postanowiło <w>wystawić</w> język do lekarza, co rozbawiło wszystkich w gabinecie.",
                            "target_lang_translations": {
                                "ru": "Ребенок решил <w>показать</w> язык врачу, что рассмешило всех в кабинете."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwiać"
                    ],
                    "antonyms": [
                        "schować"
                    ],
                    "common_phrases": [
                        "wystawić głowę",
                        "wystawić język",
                        "wystawić rękę",
                        "wystawić twarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высунуть часть тела или предмет наружу; подставить что-то солнцу или ветру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высунуть",
                                "target_lang_sense_clarification": "голову, язык"
                            },
                            {
                                "target_lang_word": "подставить",
                                "target_lang_sense_clarification": "лицо солнцу/ветру"
                            },
                            {
                                "target_lang_word": "выставить",
                                "target_lang_sense_clarification": "предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cec94f05-14c0-3bba-af14-9ee2206f1267",
                    "sense_definition": "Nie przyjść na umówione spotkanie lub zawieść czyjeś zaufanie, stawiając kogoś w niezręcznej sytuacji.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czekałem na nią godzinę przed kinem, ale ona postanowiła mnie <w>wystawić</w>.",
                            "target_lang_translations": {
                                "ru": "Я ждал её час перед кинотеатром, но она решила меня <w>продинамить</w>."
                            }
                        },
                        {
                            "text": "Nie możesz tak po prostu <w>wystawić</w> przyjaciół, kiedy na ciebie liczą.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь так просто <w>кинуть</w> друзей, когда они на тебя рассчитывают."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wystawić kogoś",
                        "wystawić do wiatru",
                        "wystawić kumpla"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не прийти на встречу или подвести кого-то, поставив в неловкое положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подставить",
                                "target_lang_sense_clarification": "подвести"
                            },
                            {
                                "target_lang_word": "кинуть",
                                "target_lang_sense_clarification": "сленг: обмануть, не прийти"
                            },
                            {
                                "target_lang_word": "продинамить",
                                "target_lang_sense_clarification": "разг.: не прийти на свидание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bc68a15-381e-3630-b02c-59bc7d5821cb",
                    "sense_definition": "Pokazać coś publicznie, udostępnić przedmioty do oglądania (np. w muzeum, galerii).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "display",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muzeum Narodowe planuje <w>wystawić</w> nieznane dotąd obrazy tego malarza.",
                            "target_lang_translations": {
                                "ru": "Национальный музей планирует <w>выставить</w> неизвестные ранее картины этого художника."
                            }
                        },
                        {
                            "text": "Artysta chce <w>wystawić</w> swoje rzeźby w centrum miasta, aby każdy mógł je zobaczyć.",
                            "target_lang_translations": {
                                "ru": "Художник хочет <w>выставить</w> свои скульптуры в центре города, чтобы каждый мог их увидеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwiać"
                    ],
                    "antonyms": [
                        "ukryć"
                    ],
                    "common_phrases": [
                        "wystawić obrazy",
                        "wystawić na widok",
                        "wystawić eksponaty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показать публике, разместить экспонаты (в музее, галерее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выставить",
                                "target_lang_sense_clarification": "экспонаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa4ee001-baa1-30a1-ad5e-e4c85f13d0ca",
                    "sense_definition": "Zaoferować coś do sprzedaży lub umieścić na liście ofert.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "commerce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <w>wystawić</w> stary samochód na sprzedaż, bo kupiłem już nowy.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>выставить</w> старую машину на продажу, так как уже купил новую."
                            }
                        },
                        {
                            "text": "Zdecydowaliśmy się <w>wystawić</w> nasz dom na aukcję internetową.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>выставить</w> наш дом на интернет-аукцион."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wystawić na sprzedaż",
                        "wystawić na aukcję",
                        "wystawić ofertę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предложить товар для продажи или разместить объявление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выставить",
                                "target_lang_sense_clarification": "на продажу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1884ab37-105c-3d78-ab58-08d4af50c915",
                    "sense_definition": "Przygotować i zaprezentować utwór sceniczny (sztukę, operę) przed publicznością.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "art_performance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Teatr miejski zamierza w tym sezonie <w>wystawić</w> klasyczną komedię.",
                            "target_lang_translations": {
                                "ru": "Городской театр намерен в этом сезоне <w>поставить</w> классическую комедию."
                            }
                        },
                        {
                            "text": "Uczniowie chcą <w>wystawić</w> jasełka dla rodziców przed przerwą świąteczną.",
                            "target_lang_translations": {
                                "ru": "Ученики хотят <w>поставить</w> рождественскую сценку для родителей перед зимними каникулами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wystawić sztukę",
                        "wystawić spektakl",
                        "wystawić operę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подготовить и показать сценическое произведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поставить",
                                "target_lang_sense_clarification": "пьесу, спектакль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efce022f-a1d6-36dd-8be6-59629d0b2d21",
                    "sense_definition": "Sprawić, że ktoś lub coś stanie się obiektem niekorzystnego działania, ryzyka lub oceny.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "risk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Swoim nieodpowiedzialnym zachowaniem możesz <w>wystawić</w> się na pośmiewisko.",
                            "target_lang_translations": {
                                "ru": "Своим безответственным поведением ты можешь <w>выставить</w> себя на посмешище."
                            }
                        },
                        {
                            "text": "Żołnierze nie chcieli <w>wystawić</w> oddziału na bezpośredni atak wroga.",
                            "target_lang_translations": {
                                "ru": "Солдаты не хотели <w>подставлять</w> отряд под прямой удар врага."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwiać"
                    ],
                    "antonyms": [
                        "ochronić"
                    ],
                    "common_phrases": [
                        "wystawić na próbę",
                        "wystawić na ryzyko",
                        "wystawić na pośmiewisko",
                        "wystawić na niebezpieczeństwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергнуть кого-то или что-то риску, критике или опасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подставить",
                                "target_lang_sense_clarification": "под удар, опасности"
                            },
                            {
                                "target_lang_word": "выставить",
                                "target_lang_sense_clarification": "на посмешище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cab32f8c-3b9d-311b-897f-2113fdefdfee",
                    "sense_definition": "Zgłosić czyjąś kandydaturę do pełnienia funkcji lub wyznaczyć kogoś do udziału w rywalizacji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W mediach trwają spekulacje, kogo partia <w>wystawi</w> jako kandydata na prezydenta.",
                            "target_lang_translations": {
                                "ru": "В СМИ идут спекуляции, кого партия <w>выдвинет</w> кандидатом в президенты."
                            }
                        },
                        {
                            "text": "Trener postanowił <w>wystawić</w> najsilniejszy skład na finałowy mecz sezonu.",
                            "target_lang_translations": {
                                "ru": "Тренер решил <w>выставить</w> сильнейший состав на финальный матч сезона."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wystawić kandydata",
                        "wystawić zawodnika",
                        "wystawić reprezentację",
                        "wystawić skład"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдвинуть кандидатуру или назначить состав команды для участия в соревнованиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выставить",
                                "target_lang_sense_clarification": "кандидата, команду"
                            },
                            {
                                "target_lang_word": "выдвинуть",
                                "target_lang_sense_clarification": "кандидатуру"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
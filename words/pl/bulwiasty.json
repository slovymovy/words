                {
                    "sense_id": "e614238a-58f2-3bc1-848e-8e3595bd2509",
                    "sense_definition": "Wytwarzający bulwy lub posiadający części podziemne w formie bulw (dotyczy roślin).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słonecznik <w>bulwiasty</w> jest rośliną jadalną, której bulwy przypominają w smaku karczochy.",
                            "target_lang_translations": {
                                "ru": "Подсолнечник <w>клубненосный</w> — это съедобное растение, клубни которого по вкусу напоминают артишоки."
                            }
                        },
                        {
                            "text": "Wszystkie rośliny <w>bulwiaste</w> gromadzą substancje odżywcze w podziemnych częściach pędu.",
                            "target_lang_translations": {
                                "ru": "Все <w>клубневые</w> растения накапливают питательные вещества в подземных частях побега."
                            }
                        }
                    ],
                    "synonyms": [
                        "bulwowy"
                    ],
                    "common_phrases": [
                        "rośliny bulwiaste",
                        "słonecznik bulwiasty",
                        "seler bulwiasty",
                        "warzywa bulwiaste"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образующий клубни или имеющий подземные части в форме клубней (о растениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клубневой",
                                "target_lang_sense_clarification": "относящийся к клубням; имеющий клубни"
                            },
                            {
                                "target_lang_word": "клубненосный",
                                "target_lang_sense_clarification": "приносящий клубни (часто в названиях видов)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ed8e068-9586-3e3e-87b0-4693cdfc897e",
                    "sense_definition": "Mający kształt przypominający bulwę; pękaty, zaokrąglony i zgrubiały.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna miał duży, <w>bulwiasty</w> nos, który dominował na jego twarzy.",
                            "target_lang_translations": {
                                "ru": "У мужчины был большой <w>шишковатый</w> нос, который доминировал на его лице."
                            }
                        },
                        {
                            "text": "Na stole stał stary, <w>bulwiasty</w> dzban wypełniony wodą.",
                            "target_lang_translations": {
                                "ru": "На столе стоял старый <w>пузатый</w> кувшин, наполненный водой."
                            }
                        }
                    ],
                    "synonyms": [
                        "pękaty",
                        "bulwowaty",
                        "obły"
                    ],
                    "antonyms": [
                        "smukły",
                        "cienki"
                    ],
                    "common_phrases": [
                        "bulwiasty nos",
                        "bulwiasty kształt",
                        "bulwiasty korzeń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий форму, напоминающую клубень; округлый, утолщенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клубневидный",
                                "target_lang_sense_clarification": "по форме напоминающий клубень"
                            },
                            {
                                "target_lang_word": "шишковатый",
                                "target_lang_sense_clarification": "неровный, с утолщениями (например, о носе)"
                            },
                            {
                                "target_lang_word": "пузатый",
                                "target_lang_sense_clarification": "округлый, выпуклый (о сосудах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

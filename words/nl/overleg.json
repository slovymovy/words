                            }
                        },
                        {
                            "text": "Wacht even, ik <w>overleg</w> dit eerst met mijn partner.",
                            "target_lang_translations": {
                                "en": "Wait a moment, I'll <w>discuss</w> this with my partner first."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {

                            {
                                "target_lang_word": "надо же",
                                "target_lang_sense_clarification": "изумление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "539a7c0d-79da-3d30-a060-bf749598ba55",
                    "sense_definition": "Drukt twijfel of aarzeling uit.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Nou>, ik weet het nog niet zo zeker hoor.",
                            "target_lang_translations": {

                        "cel soteryczny",
                        "walor soteryczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий спасительной силой, ведущий к спасению души."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спасительный",

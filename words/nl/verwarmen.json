    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d56583c4-1732-3f8a-835b-3c33a770651e",
                    "sense_definition": "De temperatuur van iets verhogen; iets warm maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "heat_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de winter <w>verwarmen</w> we ons huis met aardgas.",
                            "target_lang_translations": {
                                "en": "In the winter, we <w>heat</w> our house with natural gas.",
                                "ru": "Зимой мы <w>отапливаем</w> наш дом природным газом."
                            }
                        },
                        {
                            "text": "De zon <w>verwarmt</w> de aarde en zorgt voor leven.",
                            "target_lang_translations": {
                                "en": "The sun <w>warms</w> the earth and makes life possible.",
                                "ru": "Солнце <w>согревает</w> землю и дает жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "opwarmen",
                        "verhitten"
                    ],
                    "antonyms": [
                        "afkoelen",
                        "koelen"
                    ],
                    "common_phrases": [
                        "het huis verwarmen",
                        "water verwarmen",
                        "de oven verwarmen"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase the temperature of something; to make something warm.",
                        "ru": "Повышать температуру чего-либо; делать что-либо теплым."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "heat",
                                "target_lang_sense_clarification": "to make hot; often used for buildings, ovens, or substances"
                            },
                            {
                                "target_lang_word": "warm",
                                "target_lang_sense_clarification": "to make warm; often implies a moderate or pleasant temperature"
                            },
                            {
                                "target_lang_word": "heat up",
                                "target_lang_sense_clarification": "common phrasal verb for raising temperature"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нагревать",
                                "target_lang_sense_clarification": "повышать температуру объекта или вещества"
                            },
                            {
                                "target_lang_word": "обогревать",
                                "target_lang_sense_clarification": "снабжать теплом помещение или пространство"
                            },
                            {
                                "target_lang_word": "отапливать",
                                "target_lang_sense_clarification": "поддерживать тепло в здании с помощью системы отопления"
                            },
                            {
                                "target_lang_word": "согревать",
                                "target_lang_sense_clarification": "давать тепло живому существу или части тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "185f9f79-6063-3c3c-bf15-d7e9f2cbf739",
                    "sense_definition": "Zichzelf weer warm maken, vaak door een externe warmtebron op te zoeken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "heat_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wandelaars <w>verwarmden</w> zich aan het grote kampvuur.",
                            "target_lang_translations": {
                                "en": "The hikers <w>warmed</w> themselves by the large campfire.",
                                "ru": "Туристы <w>грелись</w> у большого костра."
                            }
                        },
                        {
                            "text": "Zij <w>verwarmt</w> zich graag met een kop hete thee na een koude dag.",
                            "target_lang_translations": {
                                "en": "She likes to <w>warm</w> herself with a cup of hot tea after a cold day.",
                                "ru": "Она любит <w>согреваться</w> чашкой горячего чая после холодного дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich opwarmen",
                        "zich koesteren"
                    ],
                    "common_phrases": [
                        "zich verwarmen aan"
                    ],
                    "target_lang_definitions": {
                        "en": "To make oneself warm again, often by seeking an external heat source.",
                        "ru": "Согревать себя, часто используя внешний источник тепла."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "warm oneself",
                                "target_lang_sense_clarification": "reflexive; to restore body heat"
                            },
                            {
                                "target_lang_word": "warm up",
                                "target_lang_sense_clarification": "to become warm again"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "греться",
                                "target_lang_sense_clarification": "находиться возле источника тепла"
                            },
                            {
                                "target_lang_word": "согреваться",
                                "target_lang_sense_clarification": "восстанавливать тепло тела"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
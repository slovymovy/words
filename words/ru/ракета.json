    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "39dda2f4-236d-3a95-87a4-890c32f0a036",
                    "sense_definition": "Летательный аппарат с реактивным двигателем, предназначенный для вывода грузов в космос или научных исследований.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "space_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Космическая <w>ракета</w> успешно стартовала с космодрома ранним утром.",
                            "target_lang_translations": {
                                "en": "The space <w>rocket</w> successfully launched from the cosmodrome early in the morning."
                            }
                        },
                        {
                            "text": "Мы наблюдали по телевизору, как происходит запуск <w>ракеты</w>.",
                            "target_lang_translations": {
                                "en": "We watched the <w>rocket</w> launch on TV."
                            }
                        }
                    ],
                    "common_phrases": [
                        "космическая ракета",
                        "запуск ракеты",
                        "двигатель ракеты",
                        "полёт ракеты"
                    ],
                    "target_lang_definitions": {
                        "en": "An aircraft with a jet engine designed to launch payloads into space or for scientific research."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rocket",
                                "target_lang_sense_clarification": "space vehicle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88ff4d9b-9921-3d20-a548-8010c1f29b72",
                    "sense_definition": "Боевой реактивный снаряд, который доставляет заряд к цели по воздуху.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Крылатая <w>ракета</w> поразила учебную цель с высокой точностью.",
                            "target_lang_translations": {
                                "en": "The cruise <w>missile</w> hit the training target with high precision."
                            }
                        },
                        {
                            "text": "Военные провели испытания новой баллистической <w>ракеты</w>.",
                            "target_lang_translations": {
                                "en": "The military conducted tests of a new ballistic <w>missile</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "крылатая ракета",
                        "баллистическая ракета",
                        "удар ракетой",
                        "перехват ракеты"
                    ],
                    "target_lang_definitions": {
                        "en": "A combat jet-propelled projectile that delivers a charge to a target through the air."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "missile",
                                "target_lang_sense_clarification": "guided weapon"
                            },
                            {
                                "target_lang_word": "rocket",
                                "target_lang_sense_clarification": "unguided projectile"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4100d9e8-41f9-34c4-9796-94a515d80bb7",
                    "sense_definition": "Пиротехнический снаряд, который ярко светится в полёте и служит для подачи сигналов или фейерверков.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "signal_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В ночное небо взлетела красная сигнальная <w>ракета</w>, осветив всё вокруг.",
                            "target_lang_translations": {
                                "en": "A red signal <w>flare</w> flew into the night sky, illuminating everything around."
                            }
                        },
                        {
                            "text": "Капитан приказал пустить <w>ракету</w>, чтобы спасатели заметили лодку.",
                            "target_lang_translations": {
                                "en": "The captain ordered to fire a <w>flare</w> so the rescuers would spot the boat."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сигнальная ракета",
                        "пустить ракету",
                        "свет ракеты"
                    ],
                    "target_lang_definitions": {
                        "en": "A pyrotechnic projectile that glows brightly in flight and is used for signaling or fireworks."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flare",
                                "target_lang_sense_clarification": "signal light"
                            },
                            {
                                "target_lang_word": "signal rocket",
                                "target_lang_sense_clarification": "pyrotechnic device"
                            }
                        ]
                    }
                },
                {

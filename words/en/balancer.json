    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3bf3058b-d761-34f6-9343-ec614a0c1db8",
                    "sense_definition": "A person, device, or mechanism that maintains equilibrium, stability, or equal distribution.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The server uses a load <w>balancer</w> to distribute incoming traffic efficiently.",
                            "target_lang_translations": {
                                "ru": "Сервер использует <w>балансировщик</w> нагрузки для эффективного распределения входящего трафика."
                            }
                        },
                        {
                            "text": "He held a long pole as a <w>balancer</w> while walking across the narrow beam.",
                            "target_lang_translations": {
                                "ru": "Он держал длинный шест как <w>балансир</w>, проходя по узкой балке."
                            }
                        }
                    ],
                    "synonyms": [
                        "stabilizer",
                        "equalizer"
                    ],
                    "antonyms": [
                        "destabilizer"
                    ],
                    "common_phrases": [
                        "load balancer",
                        "harmonic balancer",
                        "propeller balancer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, механизм или человек, поддерживающие равновесие, стабильность или равномерное распределение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балансир",
                                "target_lang_sense_clarification": "механизм или приспособление для равновесия"
                            },
                            {
                                "target_lang_word": "балансировщик",
                                "target_lang_sense_clarification": "в информатике: устройство распределения нагрузки"
                            },
                            {
                                "target_lang_word": "стабилизатор",
                                "target_lang_sense_clarification": "устройство для придания устойчивости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb021d1a-dbfa-32b9-b1f9-0c875adfda92",
                    "sense_definition": "An acrobat or performer who specializes in feats of balancing.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>balancer</w> amazed the audience by stacking chairs on his chin.",
                            "target_lang_translations": {
                                "ru": "<w>Эквилибрист</w> поразил зрителей, удерживая стулья на подбородке."
                            }
                        },
                        {
                            "text": "Every circus act included a skilled <w>balancer</w> on the high wire.",
                            "target_lang_translations": {
                                "ru": "Каждое цирковое представление включало искусного <w>эквилибриста</w> на высокой проволоке."
                            }
                        }
                    ],
                    "synonyms": [
                        "acrobat",
                        "equilibrist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акробат или артист, специализирующийся на номерах с балансированием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эквилибрист",
                                "target_lang_sense_clarification": "цирковой артист"
                            },
                            {
                                "target_lang_word": "канатоходец",
                                "target_lang_sense_clarification": "артист, ходящий по канату"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be8ea694-ae14-32e1-a825-1f88257e2352",
                    "sense_definition": "One of the modified hind wings of certain insects, such as flies, used to maintain stability during flight; a haltere.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fly uses its <w>balancer</w> to detect changes in direction while flying.",
                            "target_lang_translations": {
                                "ru": "Муха использует свое <w>жужжальце</w>, чтобы определять изменения направления во время полета."
                            }
                        },
                        {
                            "text": "Under the microscope, the tiny <w>balancer</w> looked like a small club.",
                            "target_lang_translations": {
                                "ru": "Под микроскопом крошечное <w>жужжальце</w> выглядело как маленькая булава."
                            }
                        }
                    ],
                    "synonyms": [
                        "haltere"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из видоизмененных задних крыльев некоторых насекомых (например, мух), служащее для поддержания равновесия в полете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жужжальце",
                                "target_lang_sense_clarification": "энтомология"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
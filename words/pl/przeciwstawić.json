                        "zrównać"
                    ],
                    "common_phrases": [
                        "przeciwstawić fakty mitom",
                        "przeciwstawić zalety wadom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сопоставить людей, предметы или явления для их сравнения и подчеркивания различий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противопоставить",

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полпинты",
                                "target_lang_sense_clarification": "мера объема"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e21cdb9-7485-32be-a121-85d6cde041b4",
                    "sense_definition": "A sibling with whom one shares only one biological parent.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "kinship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has two brothers and one <w>half</w> from her father's second marriage.",
                            "target_lang_translations": {
                                "ru": "У нее два брата и один <w>сводный</w> от второго брака отца."
                            }
                        },
                        {
                            "text": "The <w>halves</w> grew up in different houses but remained close.",
                            "target_lang_translations": {
                                "ru": "<w>Сводные</w> братья и сестры росли в разных домах, но остались близки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Брат или сестра, с которыми у человека только один общий биологический родитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сводный брат",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "сводная сестра",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                }
            ]
        },
        {

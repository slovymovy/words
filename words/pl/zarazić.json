    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d4db29a1-bb58-39a0-b857-9296b6807b38",
                    "sense_definition": "Przenieść na kogoś chorobę wywoływaną przez drobnoustroje, takie jak wirusy lub bakterie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_infection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chory uczeń może niechcący <w>zarazić</w> całą klasę grypą.",
                            "target_lang_translations": {
                                "ru": "Больной ученик может случайно <w>заразить</w> весь класс гриппом."
                            }
                        },
                        {
                            "text": "Lekarze noszą maseczki, aby nie <w>zarazić</w> pacjentów podczas zabiegu.",
                            "target_lang_translations": {
                                "ru": "Врачи носят маски, чтобы не <w>заразить</w> пациентов во время процедуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "zainfekować",
                        "zakazić"
                    ],
                    "antonyms": [
                        "wyleczyć"
                    ],
                    "common_phrases": [
                        "zarazić grypą",
                        "zarazić wirusem",
                        "zarazić kogoś chorobą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать кому-либо болезнь, вызываемую микроорганизмами, такими как вирусы или бактерии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заразить",
                                "target_lang_sense_clarification": "передать инфекцию"
                            },
                            {
                                "target_lang_word": "инфицировать",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "140833bc-6402-307d-9152-2540da9335ae",
                    "sense_definition": "Zachorować na chorobę zakaźną poprzez kontakt z osobą chorą lub nosicielem; przyjąć drobnoustroje do organizmu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_infection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz uważać w autobusie, żeby nie <w>zarazić się</w> od kaszlących pasażerów.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть осторожен в автобусе, чтобы не <w>заразиться</w> от кашляющих пассажиров."
                            }
                        },
                        {
                            "text": "Mój młodszy brat <w>zaraził się</w> ospą w przedszkolu.",
                            "target_lang_translations": {
                                "ru": "Мой младший брат <w>заразился</w> ветрянкой в детском саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakazić się",
                        "zainfekować",
                        "złapać"
                    ],
                    "common_phrases": [
                        "zarazić się od kogoś",
                        "zarazić się grypą",
                        "łatwo się zarazić"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny (wymaga 'się')"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заболеть инфекционным заболеванием через контакт с больным человеком или носителем; принять микроорганизмы в организм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заразиться",
                                "target_lang_sense_clarification": "заболеть от кого-то"
                            },
                            {
                                "target_lang_word": "подхватить",
                                "target_lang_sense_clarification": "разг., получить болезнь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0ecd85b-4b88-3725-bdd6-8e4a1df6b087",
                    "sense_definition": "Sprawić, że ktoś inny zacznie odczuwać te same emocje, interesować się tym samym lub mieć podobne poglądy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel potrafił <w>zarazić</w> swoich uczniów pasją do historii.",
                            "target_lang_translations": {
                                "ru": "Учитель умел <w>заразить</w> своих учеников страстью к истории."
                            }
                        },
                        {
                            "text": "Swoim głośnym śmiechem potrafiła błyskawicznie <w>zarazić</w> wszystkich dookoła.",
                            "target_lang_translations": {
                                "ru": "Своим громким смехом она могла мгновенно <w>заразить</w> всех вокруг."
                            }
                        }
                    ],
                    "antonyms": [
                        "zniechęcić"
                    ],
                    "common_phrases": [
                        "zarazić pasją",
                        "zarazić optymizmem",
                        "zarazić śmiechem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то другого почувствовать те же эмоции, заинтересоваться тем же или перенять взгляды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заразить",
                                "target_lang_sense_clarification": "передать эмоции или увлечение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c668e2f3-fadc-3934-80c1-41acd12f8515",
                    "sense_definition": "Przejąć od kogoś nastrój, zapał do czegoś lub sposób myślenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szybko <w>zaraził się</w> od kolegów entuzjazmem do wspinaczki górskiej.",
                            "target_lang_translations": {
                                "ru": "Он быстро <w>заразился</w> от коллег энтузиазмом к скалолазанию."
                            }
                        },
                        {
                            "text": "Nie warto <w>zarazić się</w> pesymizmem od wiecznie narzekających ludzi.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>заражаться</w> пессимизмом от вечно жалующихся людей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zarazić się entuzjazmem",
                        "zarazić się bakcylem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny (wymaga 'się')"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перенять от кого-то настроение, энтузиазм или образ мыслей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заразиться",
                                "target_lang_sense_clarification": "перенять настроение или увлечение"
                            }
                        ]
                    }
                }
            ]
        }

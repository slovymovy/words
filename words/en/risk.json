{
    "word_family": [
        "risky",
        "riskiness",
        "riskless",
        "riskily"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c9ddceb-2e0f-3f6b-ad0d-8722d7267a20",
                    "sense_definition": "The probability or possibility that something bad or unpleasant will happen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_probability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doctors say that smoking increases the <w>risk</w> of lung cancer.",
                            "target_lang_translations": {
                                "ru": "Врачи говорят, что курение повышает <w>риск</w> развития рака легких."
                            }
                        },
                        {
                            "text": "You have to be willing to take a <w>risk</w> if you want to succeed in business.",
                            "target_lang_translations": {
                                "ru": "Нужно быть готовым пойти на <w>риск</w>, если хочешь преуспеть в бизнесе."
                            }
                        }
                    ],
                    "synonyms": [
                        "danger",
                        "peril",
                        "hazard"
                    ],
                    "common_phrases": [
                        "at risk",
                        "take a risk",
                        "high risk",
                        "risk of",
                        "run the risk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вероятность или возможность того, что произойдет что-то плохое или неприятное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "риск",
                                "target_lang_sense_clarification": "вероятность неудачи или опасности"
                            },
                            {
                                "target_lang_word": "опасность",
                                "target_lang_sense_clarification": "возможность беды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d026d310-1f0f-3cf6-b413-5177bf5817b5",
                    "sense_definition": "A person or thing that is likely to cause problems, danger, or damage.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Leaving that old wiring exposed is a serious fire <w>risk</w>.",
                            "target_lang_translations": {
                                "ru": "Оставлять эту старую проводку оголенной — серьезный пожарный <w>риск</w>."
                            }
                        },
                        {
                            "text": "The security guard identified the unattended bag as a potential <w>risk</w>.",
                            "target_lang_translations": {
                                "ru": "Охранник определил оставленную без присмотра сумку как потенциальную <w>угрозу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hazard",
                        "danger"
                    ],
                    "common_phrases": [
                        "security risk",
                        "health risk",
                        "fire risk",
                        "safety risk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет, который может вызвать проблемы, опасность или ущерб."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угроза",
                                "target_lang_sense_clarification": "источник опасности"
                            },
                            {
                                "target_lang_word": "опасность",
                                "target_lang_sense_clarification": "потенциальная проблема"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0464e002-552d-301e-baff-a8de4f6622a9",
                    "sense_definition": "A person or business considered by a bank or insurance company in terms of the likelihood of financial loss.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bank refused the loan because they considered him a bad credit <w>risk</w>.",
                            "target_lang_translations": {
                                "ru": "Банк отказал в кредите, так как счел его плохим кредитным <w>риском</w>."
                            }
                        },
                        {
                            "text": "Insurance companies calculate the <w>risk</w> before deciding on the premium price.",
                            "target_lang_translations": {
                                "ru": "Страховые компании рассчитывают <w>риск</w>, прежде чем определить размер страхового взноса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "credit risk",
                        "insurance risk",
                        "calculated risk",
                        "financial risk"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "financial"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или компания, рассматриваемые банком или страховой компанией с точки зрения вероятности финансовых потерь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "риск",
                                "target_lang_sense_clarification": "финансовая ответственность или ненадежность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "981f770c-8cd8-3b8e-93d0-6750fa5119e4",
                    "sense_definition": "To do something although there is a chance that a negative result will happen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I did not want to <w>risk</w> being late, so I took a taxi.",
                            "target_lang_translations": {
                                "ru": "Я не хотел <w>рисковать</w> опоздать, поэтому взял такси."
                            }
                        },
                        {
                            "text": "He decided to <w>risk</w> it and ask her out on a date.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>рискнуть</w> и пригласить ее на свидание."
                            }
                        }
                    ],
                    "synonyms": [
                        "court"
                    ],
                    "common_phrases": [
                        "risk it",
                        "risk losing",
                        "risk doing something"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то, хотя есть шанс, что произойдет отрицательный результат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рисковать",
                                "target_lang_sense_clarification": "действовать наудачу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33bd6313-e39d-316f-943f-de42b72f294f",
                    "sense_definition": "To put something valuable in a situation where it could be lost, damaged, or destroyed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_endanger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The brave firefighters <w>risk</w> their lives every day to save others.",
                            "target_lang_translations": {
                                "ru": "Отважные пожарные каждый день <w>рискуют</w> своими жизнями, чтобы спасти других."
                            }
                        },
                        {
                            "text": "She was not willing to <w>risk</w> her reputation on such a doubtful scheme.",
                            "target_lang_translations": {
                                "ru": "Она не хотела <w>рисковать</w> своей репутацией ради такой сомнительной схемы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "risk one's life",
                        "risk everything",
                        "risk one's neck"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ставить что-то ценное в ситуацию, где оно может быть потеряно, повреждено или уничтожено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рисковать",
                                "target_lang_sense_clarification": "подвергать опасности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "63bec0ee-d18c-3a7e-b10b-d1abda299aa8",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We have an appointment with Mr. <w>Risk</w> at three o'clock.",
                            "target_lang_translations": {
                                "ru": "У нас встреча с мистером <w>Риском</w> в три часа."
                            }
                        },
                        {
                            "text": "The famous artist Charles <w>Risk</w> displayed his work in the gallery.",
                            "target_lang_translations": {
                                "ru": "Известный художник Чарльз <w>Риск</w> выставил свои работы в галерее."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Риск",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "da05dce6-e82d-3a25-8431-d0897ab404fd",
                    "sense_definition": "Wyborowe, uprzywilejowane oddziały wojska, często pełniące funkcje reprezentacyjne.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_units",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed pałacem prezydenckim stała honorowa <w>gwardia</w> w odświętnych mundurach.",
                            "target_lang_translations": {
                                "ru": "Перед президентским дворцом стояла почётная <w>гвардия</w> в парадной форме."
                            }
                        },
                        {
                            "text": "Słynna Szwajcarska <w>Gwardia</w> od wieków chroni papieża w Watykanie.",
                            "target_lang_translations": {
                                "ru": "Знаменитая Швейцарская <w>гвардия</w> веками охраняет Папу в Ватикане."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Gwardia Narodowa",
                        "Gwardia Szwajcarska",
                        "stara gwardia",
                        "gwardia honorowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отборные, привилегированные воинские части, часто выполняющие представительские функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гвардия",
                                "target_lang_sense_clarification": "отборные войска"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18bf3596-7cf2-3c5f-9295-1c9c7cfd21fc",
                    "sense_definition": "Straż przyboczna panującego monarchy lub dowódcy, zwłaszcza w dawnych czasach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_units",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Osobista <w>gwardia</w> króla nie odstępowała go ani na krok podczas podróży.",
                            "target_lang_translations": {
                                "ru": "Личная <w>гвардия</w> короля не отходила от него ни на шаг во время путешествия."
                            }
                        },
                        {
                            "text": "Cesarz ufał tylko swojej przybocznej <w>gwardii</w>, która składała się z najlepszych żołnierzy.",
                            "target_lang_translations": {
                                "ru": "Император доверял только своей личной <w>гвардии</w>, которая состояла из лучших солдат."
                            }
                        }
                    ],
                    "synonyms": [
                        "obstawa",
                        "ochrona"
                    ],
                    "common_phrases": [
                        "gwardia przyboczna",
                        "gwardia cesarska",
                        "gwardia królewska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Личная охрана правящего монарха или полководца, особенно в прошлые времена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гвардия",
                                "target_lang_sense_clarification": "личная охрана"
                            },
                            {
                                "target_lang_word": "лейб-гвардия",
                                "target_lang_sense_clarification": "охрана монарха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "642f1065-949b-33b0-b593-dd4f49861c03",
                    "sense_definition": "Oddziały ochotnicze zorganizowane na wzór wojska zawodowego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_units",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miejscowa <w>gwardia</w> została powołana do obrony miasta przed najeźdźcą.",
                            "target_lang_translations": {
                                "ru": "Местная <w>гвардия</w> была созвана для обороны города от захватчиков."
                            }
                        },
                        {
                            "text": "Syn sąsiadów zaciągnął się do <w>gwardii</w>, aby walczyć o wolność kraju.",
                            "target_lang_translations": {
                                "ru": "Сын соседей записался в <w>гвардию</w>, чтобы сражаться за свободу страны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Gwardia Ludowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добровольческие отряды, организованные по образцу профессиональной армии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гвардия",
                                "target_lang_sense_clarification": "военизированная организация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
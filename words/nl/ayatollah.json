            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0042d614-485c-38d1-95a4-47d952e51b13",
                    "sense_definition": "Een hoge geestelijke leider binnen het sjiisme.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>ayatollah</w> sprak zijn volgelingen toe tijdens het vrijdaggebed.",
                            "target_lang_translations": {
                                "ru": "<w>Аятолла</w> обратился к своим последователям во время пятничной молитвы."
                            }
                        },
                        {
                            "text": "Na jaren van ballingschap keerde de <w>ayatollah</w> terug naar zijn vaderland.",
                            "target_lang_translations": {
                                "ru": "После многих лет изгнания <w>аятолла</w> вернулся на родину."
                            }
                        }
                    ],
                    "synonyms": [
                        "geestelijke"
                    ],
                    "common_phrases": [
                        "Iraanse ayatollah",
                        "sjiitische ayatollah"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий духовный лидер в шиизме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аятолла",
                                "target_lang_sense_clarification": "духовный лидер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07796f9b-1a2c-3727-9b60-8a55c39cc793",
                    "sense_definition": "Een leider of sleutelfiguur met zeer strenge, onbuigzame en dogmatische opvattingen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gedraagt zich als een <w>ayatollah</w> als het gaat om de naleving van de kantoorregels.",
                            "target_lang_translations": {
                                "ru": "Он ведет себя как <w>аятолла</w>, когда речь идет о соблюдении офисных правил."
                            }
                        },
                        {
                            "text": "De politicus werd door zijn tegenstanders omschreven als de <w>ayatollah</w> van de vrije markt.",
                            "target_lang_translations": {
                                "ru": "Противники называли политика <w>аятоллой</w> свободного рынка."
                            }
                        }
                    ],
                    "synonyms": [
                        "scherpslijper",
                        "fanaticus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек с крайне строгими, непреклонными и догматичными взглядами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аятолла",
                                "target_lang_sense_clarification": "перен. фанатик, догматик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce93fe11-d2cd-37ad-8ecf-560394a570c5",
                    "sense_definition": "De hoogste graad van theologische geleerdheid binnen het sjiisme.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religious_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het vergt tientallen jaren studie om de rang van <w>ayatollah</w> te bereiken.",
                            "target_lang_translations": {
                                "ru": "Чтобы достичь ранга <w>аятоллы</w>, требуются десятилетия учебы."
                            }
                        },
                        {
                            "text": "Zijn uitgebreide kennis van de theologie leverde hem uiteindelijk de titel van <w>ayatollah</w> op.",
                            "target_lang_translations": {
                                "ru": "Его обширные знания теологии в конечном итоге принесли ему титул <w>аятоллы</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высшая степень богословской учености в шиизме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аятолла",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
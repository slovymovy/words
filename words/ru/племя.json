    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2635fdfc-9c10-3ca2-ba12-3be8c598e841",
                    "sense_definition": "Этническая и социальная общность людей, связанных родовыми отношениями, территорией, культурой, языком и самоназванием.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ethnography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В джунглях Амазонки нашли неизвестное <w>племя</w>.",
                            "target_lang_translations": {
                                "en": "An unknown <w>tribe</w> was found in the Amazon jungle."
                            }
                        },
                        {
                            "text": "Вождь собрал всё <w>племя</w> у костра.",
                            "target_lang_translations": {
                                "en": "The chief gathered the whole <w>tribe</w> by the fire."
                            }
                        },
                        {
                            "text": "Кочевое <w>племя</w> перемещалось со своими стадами.",
                            "target_lang_translations": {
                                "en": "The nomadic <w>tribe</w> moved with its herds."
                            }
                        }
                    ],
                    "common_phrases": [
                        "кочевое племя",
                        "вождь племени",
                        "дикое племя",
                        "союз племён"
                    ],
                    "target_lang_definitions": {
                        "en": "An ethnic and social community of people connected by kinship, territory, culture, language, and a common name."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tribe",
                                "target_lang_sense_clarification": "ethnic group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "daf3bcb6-9137-32ab-9f21-599b87839f94",
                    "sense_definition": "Группа людей, объединённых каким-либо общим признаком (часто с ироничным оттенком), или поколение людей (в высоком стиле).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Здравствуй, <w>племя</w> младое, незнакомое!",
                            "target_lang_translations": {
                                "en": "Hello, young <w>generation</w>, unknown!"
                            }
                        },
                        {
                            "text": "Всё это шумное туристическое <w>племя</w> заполнило пляж.",
                            "target_lang_translations": {
                                "en": "All this noisy tourist <w>tribe</w> filled the beach."
                            }
                        },
                        {
                            "text": "Чиновничье <w>племя</w> любит бумажную работу.",
                            "target_lang_translations": {
                                "en": "The bureaucratic <w>tribe</w> loves paperwork."
                            }
                        }
                    ],
                    "synonyms": [
                        "поколение"
                    ],
                    "common_phrases": [
                        "молодое племя",
                        "чиновничье племя",
                        "людское племя"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "when referring to a category of people"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people united by a common feature (often ironic), or a generation (in high style)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tribe",
                                "target_lang_sense_clarification": "group of people"
                            },
                            {
                                "target_lang_word": "generation",
                                "target_lang_sense_clarification": "age group"
                            },
                            {
                                "target_lang_word": "breed",
                                "target_lang_sense_clarification": "type of people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e7fbcbe-ce60-3482-ad86-85499d63cb01",
                    "sense_definition": "Порода животных, а также животные, оставленные для размножения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лучших коров фермер решил оставить на <w>племя</w>.",
                            "target_lang_translations": {
                                "en": "The farmer decided to keep the best cows for <w>breeding</w>."
                            }
                        },
                        {
                            "text": "Всё птичье <w>племя</w> проснулось с рассветом.",
                            "target_lang_translations": {
                                "en": "The whole bird <w>tribe</w> woke up at dawn."
                            }
                        }
                    ],
                    "synonyms": [
                        "порода"
                    ],
                    "common_phrases": [
                        "оставить на племя",
                        "птичье племя"
                    ],
                    "target_lang_definitions": {
                        "en": "A breed of animals, or animals kept for reproduction."
                    },
                    "translations": {

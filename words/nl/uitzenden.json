            "pos": "verb",
            "senses": [
                {
                    "sense_id": "034edb48-63cd-398e-a78c-26ad241ef2b0",
                    "sense_definition": "Beeld- of geluidssignalen via radio, televisie of internet verspreiden zodat ze door een publiek ontvangen kunnen worden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het journaal wordt elke avond om acht uur <w>uitgezonden</w> op de nationale televisie.",
                            "target_lang_translations": {
                                "en": "The news is <w>broadcast</w> every evening at eight o'clock on national television.",
                                "ru": "Новости <w>транслируются</w> каждый вечер в восемь часов по национальному телевидению."
                            }
                        },
                        {
                            "text": "De belangrijke voetbalwedstrijd werd live <w>uitgezonden</w> in meer dan vijftig landen.",
                            "target_lang_translations": {
                                "en": "The important football match was <w>broadcast</w> live in more than fifty countries.",
                                "ru": "Важный футбольный матч <w>транслировался</w> в прямом эфире более чем в пятидесяти странах."
                            }
                        }
                    ],
                    "synonyms": [
                        "omroepen",
                        "uitstralen"
                    ],
                    "common_phrases": [
                        "live uitzenden",
                        "op tv uitzenden",
                        "een programma uitzenden"
                    ],
                    "target_lang_definitions": {
                        "en": "To distribute visual or audio signals via radio, television, or the internet so that they can be received by an audience.",
                        "ru": "Распространять видео- или аудиосигналы по радио, телевидению или интернету для приема аудиторией."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "broadcast",
                                "target_lang_sense_clarification": "radio or TV programs"
                            },
                            {
                                "target_lang_word": "air",
                                "target_lang_sense_clarification": "a program on TV or radio"
                            },
                            {
                                "target_lang_word": "transmit",
                                "target_lang_sense_clarification": "signals"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "транслировать",
                                "target_lang_sense_clarification": "теле- или радиопрограммы"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "в эфир"
                            },
                            {
                                "target_lang_word": "вещать",
                                "target_lang_sense_clarification": "проводить вещание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2cea3896-774c-3f74-9e9c-7cee1fb2cab4",
                    "sense_definition": "Mensen naar een andere locatie sturen om daar een specifieke taak of missie uit te voeren, vaak in een officiële functie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regering heeft besloten extra militairen <w>uit</w> te <w>zenden</w> naar het conflictgebied.",
                            "target_lang_translations": {
                                "en": "The government has decided to <w>deploy</w> extra soldiers to the conflict zone.",
                                "ru": "Правительство решило <w>направить</w> дополнительных военных в зону конфликта."
                            }
                        },
                        {
                            "text": "Zij werd voor haar werk als ingenieur tijdelijk <w>uitgezonden</w> naar een project in het buitenland.",
                            "target_lang_translations": {
                                "en": "She was temporarily <w>sent</w> to a project abroad for her work as an engineer.",
                                "ru": "Её временно <w>командировали</w> на проект за границу для работы инженером."
                            }
                        }
                    ],
                    "synonyms": [
                        "afvaardigen",
                        "detacheren",
                        "uitsturen"
                    ],
                    "common_phrases": [
                        "militairen uitzenden",
                        "personeel uitzenden",
                        "een team uitzenden"
                    ],
                    "target_lang_definitions": {
                        "en": "To send people to another location to perform a specific task or mission, often in an official capacity.",
                        "ru": "Отправлять людей в другое место для выполнения определенной задачи или миссии, часто в официальном качестве."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dispatch",
                                "target_lang_sense_clarification": "personnel or troops"
                            },
                            {
                                "target_lang_word": "deploy",
                                "target_lang_sense_clarification": "military forces"
                            },
                            {
                                "target_lang_word": "send out",
                                "target_lang_sense_clarification": "general"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "командировать",
                                "target_lang_sense_clarification": "сотрудников по работе"
                            },
                            {
                                "target_lang_word": "направлять",
                                "target_lang_sense_clarification": "войска или официальных лиц"
                            },
                            {
                                "target_lang_word": "отправлять",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "faf9eea0-356e-3754-bb91-c3f4671e58ed",
                    "sense_definition": "Straling, licht, warmte of signalen produceren en naar buiten brengen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zon <w>zendt</w> enorme hoeveelheden licht en warmte <w>uit</w> die essentieel zijn voor leven op aarde.",
                            "target_lang_translations": {
                                "en": "The sun <w>emits</w> enormous amounts of light and heat that are essential for life on earth.",
                                "ru": "Солнце <w>излучает</w> огромное количество света и тепла, необходимых для жизни на Земле."
                            }
                        },
                        {
                            "text": "Deze antenne <w>zendt</w> een krachtig signaal <w>uit</w> om schepen op zee te waarschuwen.",
                            "target_lang_translations": {
                                "en": "This antenna <w>transmits</w> a powerful signal to warn ships at sea.",
                                "ru": "Эта антенна <w>передает</w> мощный сигнал для предупреждения судов в море."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitstralen",
                        "afgeven",
                        "emitteren"
                    ],
                    "antonyms": [
                        "absorberen",
                        "opnemen"
                    ],
                    "common_phrases": [
                        "licht uitzenden",
                        "signalen uitzenden",
                        "straling uitzenden"
                    ],
                    "target_lang_definitions": {
                        "en": "To produce radiation, light, heat, or signals and release them externally.",
                        "ru": "Производить и выделять наружу излучение, свет, тепло или сигналы."

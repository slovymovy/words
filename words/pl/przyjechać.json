    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "019cf436-0b8b-3b59-99c4-99486f16882a",
                    "sense_definition": "Przybyć do jakiegoś miejsca, poruszając się środkiem transportu (np. samochodem, pociągiem, rowerem), a nie pieszo.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_human",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice <przyjechali> wczoraj pociągiem z Warszawy.",
                            "target_lang_translations": {
                                "ru": "Мои родители <приехали> вчера на поезде из Варшавы."
                            }
                        },
                        {
                            "text": "Obiecuję, że <przyjadę> do ciebie w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Обещаю, что <приеду> к тебе на следующей неделе."
                            }
                        },
                        {
                            "text": "Musimy <przyjechać> na czas, żeby nie spóźnić się na film.",
                            "target_lang_translations": {
                                "ru": "Мы должны <приехать> вовремя, чтобы не опоздать на фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "dotrzeć",
                        "przybyć"
                    ],
                    "antonyms": [
                        "odjechać"
                    ],
                    "common_phrases": [
                        "przyjechać pociągiem",
                        "przyjechać samochodem",
                        "przyjechać do domu",
                        "przyjechać na czas",
                        "przyjechać z wizytą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибыть в какое-либо место, передвигаясь на транспортном средстве (например, на машине, поезде, велосипеде), а не пешком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приехать",
                                "target_lang_sense_clarification": "самый распространенный вариант; прибыть на транспорте"
                            },
                            {
                                "target_lang_word": "прибыть",
                                "target_lang_sense_clarification": "более официально; достичь места назначения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2470616a-8b32-3643-835f-c6b0e9fa6f77",
                    "sense_definition": "O pojeździe (np. autobusie, pociągu): dotrzeć do określonego miejsca lub celu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autobus <przyjechał> na przystanek z dużym opóźnieniem.",
                            "target_lang_translations": {
                                "ru": "Автобус <приехал> на остановку с большим опозданием."
                            }
                        },
                        {
                            "text": "Czekaliśmy na peronie, aż <przyjedzie> nasz pociąg.",
                            "target_lang_translations": {
                                "ru": "Мы ждали на перроне, когда <приедет> наш поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "nadjechać",
                        "podjechać"
                    ],
                    "antonyms": [
                        "odjechać"
                    ],
                    "common_phrases": [
                        "pociąg przyjechał",
                        "autobus przyjechał",
                        "przyjechać na stację"
                    ],
                    "target_lang_definitions": {
                        "ru": "О транспортном средстве (например, автобусе, поезде): достичь определенного места или цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прибыть",
                                "target_lang_sense_clarification": "официально или по расписанию (о поезде, автобусе)"
                            },
                            {
                                "target_lang_word": "приехать",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
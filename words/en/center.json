            "senses": [
                {
                    "sense_id": "dd550010-ec02-3fe0-b8ef-1a6fbd795576",
                    "sense_definition": "A place, especially a building or complex, set aside for some specified function or activity.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went to the shopping <w>center</w> to buy new clothes.",
                            "target_lang_translations": {
                                "ru": "Мы пошли в торговый <w>центр</w>, чтобы купить новую одежду."
                            }
                        },
                        {
                            "text": "The community <w>center</w> hosts events for families every weekend.",
                            "target_lang_translations": {
                                "ru": "Общественный <w>центр</w> проводит мероприятия для семей каждые выходные."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shopping center",
                        "city center",
                        "civic center",
                        "call center"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, здание или комплекс, предназначенные для определенной цели или деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "центр",
                                "target_lang_sense_clarification": "учреждение или комплекс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdaf314c-f722-397a-bdfa-9e9c59f0eb10",
                    "sense_definition": "The middle portion of something; the part well away from the edges.",

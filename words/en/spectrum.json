    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab5d893c-642b-3396-99d0-a45362c63f3e",
                    "sense_definition": "The band of colors produced when white light is separated into its components, or the entire range of electromagnetic wave frequencies.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physics_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A prism breaks sunlight into a visible <w>spectrum</w> of colors.",
                            "target_lang_translations": {
                                "ru": "Призма разлагает солнечный свет в видимый <w>спектр</w> цветов."
                            }
                        },
                        {
                            "text": "Radio waves are located at one end of the electromagnetic <w>spectrum</w>.",
                            "target_lang_translations": {
                                "ru": "Радиоволны находятся на одном конце электромагнитного <w>спектра</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "visible spectrum",
                        "electromagnetic spectrum",
                        "color spectrum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полоса цветов, получаемая при разложении белого света, или весь диапазон частот электромагнитных волн."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спектр",
                                "target_lang_sense_clarification": "физика, оптика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85b8d7b3-8099-3f46-bdc6-3da1ecf89c6a",
                    "sense_definition": "The range of neurodevelopmental conditions related to autism, which vary in severity and symptoms.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doctors diagnosed the child as being on the autism <w>spectrum</w>.",
                            "target_lang_translations": {
                                "ru": "Врачи диагностировали у ребенка расстройство аутистического <w>спектра</w>."
                            }
                        },
                        {
                            "text": "Many people on the <w>spectrum</w> have unique skills and perspectives.",
                            "target_lang_translations": {
                                "ru": "Многие люди в <w>спектре</w> обладают уникальными навыками и взглядами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the spectrum",
                        "autism spectrum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Диапазон нарушений нейроразвития, связанных с аутизмом, которые различаются по степени тяжести и симптомам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спектр",
                                "target_lang_sense_clarification": "медицина, аутизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38d49a41-4329-3048-8a10-8c49098459a9",
                    "sense_definition": "A continuous range or extent of positions, opinions, ideas, or values between two extremes.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "general_range",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician appealed to voters across the entire political <w>spectrum</w>.",
                            "target_lang_translations": {
                                "ru": "Политик обратился к избирателям всего политического <w>спектра</w>."
                            }
                        },
                        {
                            "text": "We offer a broad <w>spectrum</w> of services to meet every client's needs.",
                            "target_lang_translations": {
                                "ru": "Мы предлагаем широкий <w>спектр</w> услуг для удовлетворения потребностей каждого клиента."
                            }
                        }
                    ],
                    "synonyms": [
                        "eigenspectrum"
                    ],
                    "common_phrases": [
                        "political spectrum",
                        "broad spectrum",
                        "wide spectrum",
                        "across the spectrum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрерывный ряд или охват позиций, мнений, идей или ценностей между двумя крайностями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спектр",
                                "target_lang_sense_clarification": "переносное значение, многообразие"
                            },
                            {
                                "target_lang_word": "диапазон",
                                "target_lang_sense_clarification": "область, охват"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2929b362-495b-350d-8742-1adfb2813360",
                    "sense_definition": "The set of eigenvalues associated with a matrix.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The student calculated the <w>spectrum</w> of the matrix for the linear algebra assignment.",
                            "target_lang_translations": {
                                "ru": "Студент вычислил <w>спектр</w> матрицы для задания по линейной алгебре."
                            }
                        },
                        {
                            "text": "Analyzing the <w>spectrum</w> helps in understanding the stability of the system.",
                            "target_lang_translations": {
                                "ru": "Анализ <w>спектра</w> помогает понять устойчивость системы."
                            }
                        }
                    ],
                    "synonyms": [
                        "eigenspectrum"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множество собственных значений матрицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спектр",
                                "target_lang_sense_clarification": "математика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fc4b7af-fd7f-3834-8131-3e3690292c55",
                    "sense_definition": "A ghost, phantom, or apparition.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old stories tell of a terrifying <w>spectrum</w> that haunted the tower.",
                            "target_lang_translations": {
                                "ru": "Старые легенды рассказывают об ужасном <w>призраке</w>, который обитал в башне."
                            }
                        },
                        {
                            "text": "He felt as though a <w>spectrum</w> from his past was watching him.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, будто <w>призрак</w> из его прошлого наблюдает за ним."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Original Latin meaning referring to an image or apparition."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Призрак, фантом или видение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "призрак",
                                "target_lang_sense_clarification": "устаревшее"
                            },
                            {
                                "target_lang_word": "видение",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "8b3252a5-77d0-3b30-9ba4-b222867d1d98",
                    "sense_definition": "Not suitable or proper for the specific situation, time, or place.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>inappropriate</w> to answer your phone during a meeting.",
                            "target_lang_translations": {
                                "ru": "Отвечать на телефонный звонок во время совещания <w>неуместно</w>."
                            }
                        },
                        {
                            "text": "Wearing shorts is considered <w>inappropriate</w> for a formal wedding.",
                            "target_lang_translations": {
                                "ru": "Носить шорты на официальной свадьбе считается <w>неуместным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "unsuitable",
                        "out of place",
                        "improper",
                        "unfit",
                        "out of line"
                    ],
                    "antonyms": [
                        "appropriate",
                        "suitable",
                        "fitting"
                    ],
                    "common_phrases": [
                        "inappropriate behavior",
                        "inappropriate comment",
                        "inappropriate response",
                        "deemed inappropriate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не подходящий или неправильный для конкретной ситуации, времени или места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неуместный",
                                "target_lang_sense_clarification": "не соответствующий обстановке, сделанный некстати"
                            },
                            {
                                "target_lang_word": "неподходящий",
                                "target_lang_sense_clarification": "не отвечающий требованиям ситуации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08cbcd25-af92-3c8a-8d29-c71530b12aca",
                    "sense_definition": "Referring to content or behavior that is indecent, offensive, or sexual in nature.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher blocked access to websites with <w>inappropriate</w> content.",
                            "target_lang_translations": {
                                "ru": "Учитель заблокировал доступ к сайтам с <w>неподобающим</w> контентом."
                            }
                        },
                        {
                            "text": "He was fired from his job for making <w>inappropriate</w> advances toward a colleague.",
                            "target_lang_translations": {
                                "ru": "Его уволили с работы за <w>непристойные</w> приставания к коллеге."
                            }
                        }
                    ],
                    "synonyms": [
                        "improper",
                        "unseemly",
                        "out of line"
                    ],
                    "antonyms": [
                        "decent",
                        "wholesome"
                    ],
                    "common_phrases": [
                        "inappropriate content",
                        "inappropriate touching",
                        "inappropriate language"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к контенту или поведению, которые являются неприличными, оскорбительными или имеют сексуальный характер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неподобающий",
                                "target_lang_sense_clarification": "нарушающий принятые нормы приличия"
                            },
                            {
                                "target_lang_word": "непристойный",
                                "target_lang_sense_clarification": "противоречащий нормам морали, часто сексуального характера"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4f7b3aef-f23f-36fd-806c-0c518a0d2ccc",
                    "sense_definition": "An initialism for Clean Air Zone, an area in a city where targeted action is taken to improve air quality, often by charging polluting vehicles.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drivers of older cars may have to pay a daily fee to enter the <w>CAZ</w>.",
                            "target_lang_translations": {
                                "ru": "Водителям старых автомобилей, возможно, придется платить ежедневный сбор за въезд в <w>зону чистого воздуха</w>."
                            }
                        },
                        {
                            "text": "The city council plans to expand the <w>CAZ</w> to reduce pollution in residential areas.",
                            "target_lang_translations": {
                                "ru": "Городской совет планирует расширить <w>зону чистого воздуха</w>, чтобы снизить загрязнение в жилых районах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "enter the CAZ",
                        "CAZ charges"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от Clean Air Zone — зона в городе, где принимаются меры для улучшения качества воздуха, часто путем взимания платы с загрязняющих транспортных средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зона чистого воздуха",
                                "target_lang_sense_clarification": "дословный перевод термина"
                            },
                            {
                                "target_lang_word": "экологическая зона",
                                "target_lang_sense_clarification": "зона с ограничениями для транспорта"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7d41c0f1-9786-3729-af50-e6ec83d82909",
                    "sense_definition": "A diminutive of the female given name <w>Carol</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "I am going to meet <w>Caz</w> for lunch later today.",
                            "target_lang_translations": {
                                "ru": "Я собираюсь встретиться с <w>Кэз</w> за обедом сегодня попозже."
                            }
                        },
                        {
                            "text": "Everyone calls her <w>Caz</w>, even though her full name is Caroline.",
                            "target_lang_translations": {
                                "ru": "Все зовут её <w>Кэз</w>, хотя её полное имя — Кэролайн."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма женского имени Кэрол или Кэролайн."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кэз",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c70dcbb1-99df-3da4-b223-83e7121f3595",
                    "sense_definition": "Informal short form of <w>casual</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dress code for the party is pretty <w>caz</w>, so jeans are fine.",
                            "target_lang_translations": {
                                "ru": "Дресс-код на вечеринке довольно <w>неформальный</w>, так что джинсы подойдут."
                            }
                        },
                        {
                            "text": "He tried to act <w>caz</w> about the news, but I could tell he was excited.",
                            "target_lang_translations": {
                                "ru": "Он пытался вести себя <w>непринужденно</w>, узнав новости, но я видел, что он взволнован."
                            }
                        }
                    ],
                    "synonyms": [
                        "casual",
                        "relaxed"
                    ],
                    "antonyms": [
                        "formal"
                    ],
                    "common_phrases": [
                        "keep it caz"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное сокращение от слова casual (повседневный, неформальный)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неформальный",
                                "target_lang_sense_clarification": "об обстановке или стиле"
                            },
                            {
                                "target_lang_word": "расслабленный",
                                "target_lang_sense_clarification": "о поведении или атмосфере"
                            },
                            {
                                "target_lang_word": "повседневный",
                                "target_lang_sense_clarification": "об одежде"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
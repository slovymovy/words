    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "b749a150-ecc4-37ab-93a6-51f27137e7c8",
                    "sense_definition": "Spójnik wprowadzający zdanie podrzędne, wskazujący na moment zakończenia trwania jakiejś czynności lub stanu; oznacza to samo co „do czasu gdy”.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Będę czekał na stacji, <aż> przyjedzie twój pociąg."
                        },
                        {
                            "text": "Nie wychodź z domu, <aż> przestanie padać deszcz."
                        }
                    ],
                    "synonyms": [
                        "dopóki",
                        "póki nie, dopóki nie"
                    ],
                    "common_phrases": [
                        "czekać aż",
                        "tak długo aż"
                    ]
                },
                {
                    "sense_id": "632226f6-d1e8-3fbc-b5a3-4b2858b266cd",
                    "sense_definition": "Spójnik wprowadzający zdanie skutkowe, wskazujący na rezultat lub intensywność wcześniejszej czynności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Śmialiśmy się tak głośno, <aż> sąsiedzi zapukali w ścianę."
                        },
                        {
                            "text": "Był tak zmęczony, <aż> zasnął przy biurku."
                        }
                    ],
                    "synonyms": [
                        "że"
                    ],
                    "common_phrases": [
                        "tak... aż"
                    ]
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1f37f17d-d19c-3a8f-9e33-6f2281eb2bc1",
                    "sense_definition": "Twisted or bent out of the original shape, usually because of heat or dampness.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_distortion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old wooden door is so <w>warped</w> that it no longer closes properly.",
                            "target_lang_translations": {
                                "ru": "Старая деревянная дверь настолько <w>покоробилась</w>, что больше не закрывается плотно."
                            }
                        },
                        {
                            "text": "Leaving the vinyl record in the sun resulted in a <w>warped</w> disc that cannot be played.",
                            "target_lang_translations": {
                                "ru": "Из-за того, что виниловую пластинку оставили на солнце, получился <w>деформированный</w> диск, который невозможно проиграть."
                            }
                        }
                    ],
                    "synonyms": [
                        "distorted",
                        "twisted",
                        "bent",
                        "deformed",
                        "misshapen"
                    ],
                    "antonyms": [
                        "straight",
                        "flat",
                        "even"
                    ],
                    "common_phrases": [
                        "warped wood",
                        "warped floorboards",
                        "warped surface"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искривленный или деформированный, потерявший первоначальную форму (обычно из-за тепла или влаги)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покоробленный",
                                "target_lang_sense_clarification": "о дереве, бумаге или плоских поверхностях"
                            },
                            {
                                "target_lang_word": "деформированный",
                                "target_lang_sense_clarification": "общая физическая деформация"
                            },
                            {
                                "target_lang_word": "искривленный",
                                "target_lang_sense_clarification": "изогнутый, потерявший прямизну"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0332294e-3a55-3fa8-8417-7d43b393351a",
                    "sense_definition": "Having ideas or attitudes that are strange, unpleasant, or abnormal.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a <w>warped</w> sense of humor that many people find offensive.",
                            "target_lang_translations": {
                                "ru": "У него <w>извращенное</w> чувство юмора, которое многие находят оскорбительным."
                            }
                        },
                        {
                            "text": "The villain in the movie had a <w>warped</w> mind and wanted to destroy the world.",
                            "target_lang_translations": {
                                "ru": "У злодея в фильме был <w>извращенный</w> ум, и он хотел уничтожить мир."
                            }
                        }
                    ],
                    "synonyms": [
                        "twisted",
                        "perverse",
                        "sick",
                        "deviant",
                        "corrupt"
                    ],
                    "antonyms": [
                        "normal",
                        "wholesome",
                        "balanced"
                    ],
                    "common_phrases": [
                        "warped sense of humor",
                        "warped mind",
                        "warped perspective"
                    ],
                    "target_lang_definitions": {
                        "ru": "Странный, ненормальный или неприятный (об идеях, взглядах или характере)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извращенный",
                                "target_lang_sense_clarification": "о чувстве юмора, уме или морали"
                            },
                            {
                                "target_lang_word": "искаженный",
                                "target_lang_sense_clarification": "о восприятии реальности или фактах"
                            },
                            {
                                "target_lang_word": "больной",
                                "target_lang_sense_clarification": "разг. о фантазии или воображении"
                            }
                        ]
                    }
                }
            ]
        },

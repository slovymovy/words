    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "008d785a-e424-3d18-be4a-8b4afba82f3d",
                    "sense_definition": "The state of being ill or having a disease.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He could not go to work today due to <w>sickness</w>.",
                            "target_lang_translations": {
                                "ru": "Он не смог пойти сегодня на работу из-за <w>болезни</w>."
                            }
                        },
                        {
                            "text": "The <w>sickness</w> spread rapidly through the small village.",
                            "target_lang_translations": {
                                "ru": "<w>Болезнь</w> быстро распространилась по маленькой деревне."
                            }
                        }
                    ],
                    "synonyms": [
                        "illness",
                        "disease",
                        "malady"
                    ],
                    "antonyms": [
                        "health",
                        "wellness"
                    ],
                    "common_phrases": [
                        "in sickness and in health",
                        "serious sickness",
                        "sickness benefit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние нездоровья или наличие заболевания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болезнь",
                                "target_lang_sense_clarification": "расстройство здоровья, заболевание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69cab2db-bb77-318a-bc2a-c6c29f8d57b8",
                    "sense_definition": "A feeling of nausea or the urge to vomit.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt a sudden wave of <w>sickness</w> as the boat rocked.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала внезапный приступ <w>тошноты</w>, когда лодка покачнулась."
                            }
                        },
                        {
                            "text": "Motion <w>sickness</w> can make traveling by car very unpleasant.",
                            "target_lang_translations": {
                                "ru": "<w>Укачивание</w> может сделать путешествие на машине очень неприятным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "motion sickness",
                        "morning sickness",
                        "sea sickness",
                        "car sickness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство тошноты или позыв к рвоте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тошнота",
                                "target_lang_sense_clarification": "тягостное ощущение, дурнота"
                            },
                            {
                                "target_lang_word": "рвота",
                                "target_lang_sense_clarification": "извержение содержимого желудка (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eef953dc-49a4-3f4b-b680-9d2119668214",
                    "sense_definition": "The incorrect use of a specific grammatical case instead of the standard one.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Linguists study dative <w>sickness</w> in the evolution of certain languages.",
                            "target_lang_translations": {
                                "ru": "Лингвисты изучают дативную <w>болезнь</w> в эволюции некоторых языков."
                            }
                        },
                        {
                            "text": "The student wrote a paper on the phenomenon of case <w>sickness</w>.",
                            "target_lang_translations": {
                                "ru": "Студент написал работу о феномене падежной <w>болезни</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Некорректное использование определенного грамматического падежа вместо стандартного."
                    },

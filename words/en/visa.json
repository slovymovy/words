                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c1ab428e-9cd7-38a6-b894-a76ee97beba5",
                    "sense_definition": "A multinational financial services corporation known for its credit cards and payment systems.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "companies",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "<w>Visa</w> provides secure payment solutions for millions of customers worldwide.",
                            "target_lang_translations": {
                                "ru": "<w>Visa</w> предоставляет безопасные платежные решения для миллионов клиентов по всему миру."
                            }
                        },
                        {
                            "text": "He works as a software engineer for <w>Visa</w>.",
                            "target_lang_translations": {
                                "ru": "Он работает инженером-программистом в <w>Visa</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Транснациональная финансовая корпорация, известная своими кредитными картами и платежными системами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Visa",
                                "target_lang_sense_clarification": "компания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "873815ee-12c9-30d2-a811-04c1ed6b068c",

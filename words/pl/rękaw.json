    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cb771bec-f520-3ada-8ab9-042f2112c42f",
                    "sense_definition": "Część ubrania okrywająca ramię.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta koszula ma za długi <w>rękaw</w>.",
                            "target_lang_translations": {
                                "ru": "У этой рубашки слишком длинный <w>рукав</w>."
                            }
                        },
                        {
                            "text": "Latem najchętniej noszę bluzki, które mają krótki <w>rękaw</w>.",
                            "target_lang_translations": {
                                "ru": "Летом я охотнее всего ношу блузки с коротким <w>рукавом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "długi rękaw",
                        "krótki rękaw",
                        "podwinąć rękawy",
                        "rękaw koszuli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть одежды, покрывающая руку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рукав",
                                "target_lang_sense_clarification": "часть одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3309477f-e3f6-37a7-af2b-45be355f31a7",
                    "sense_definition": "Ruchomy tunel łączący samolot z budynkiem lotniska, umożliwiający pasażerom bezpośrednie wejście na pokład.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_tunnels",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pasażerowie wchodzili do samolotu przez <w>rękaw</w>.",
                            "target_lang_translations": {
                                "ru": "Пассажиры заходили в самолет через <w>рукав</w>."
                            }
                        },
                        {
                            "text": "Obsługa lotniska podstawiła <w>rękaw</w> pod drzwi maszyny.",
                            "target_lang_translations": {
                                "ru": "Служащие аэропорта подогнали <w>телетрап</w> к двери самолета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wejść rękawem",
                        "rękaw lotniczy",
                        "podłączyć rękaw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижный туннель, соединяющий самолет со зданием аэропорта для посадки пассажиров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телетрап",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "рукав",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7cef85a-0a6b-352a-887c-82020bf6268a",
                    "sense_definition": "Foliowy worek odporny na wysoką temperaturę, służący do pieczenia potraw w piekarniku bez dodatku tłuszczu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "culinary_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kurczak upieczony w <w>rękawie</w> jest bardzo soczysty.",
                            "target_lang_translations": {
                                "ru": "Курица, запеченная в <w>рукаве</w>, получается очень сочной."
                            }
                        },
                        {
                            "text": "Muszę kupić w sklepie nowy <w>rękaw</w> do pieczenia.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить в магазине новый <w>рукав</w> для запекания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rękaw do pieczenia",
                        "piec w rękawie",
                        "rękaw foliowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Термостойкий пленочный мешок для запекания продуктов в духовке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рукав для запекания",
                                "target_lang_sense_clarification": "кулинария"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9503724c-a8ae-39df-9c5e-4226bddbdd9d",
                    "sense_definition": "Narzędzie kuchenne w kształcie stożkowatego worka, służące do wyciskania kremu lub ciasta w celach dekoracyjnych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "culinary_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cukiernik użył <w>rękawa</w>, aby ozdobić tort różami z kremu.",
                            "target_lang_translations": {
                                "ru": "Кондитер использовал <w>кондитерский мешок</w>, чтобы украсить торт розами из крема."
                            }
                        },
                        {
                            "text": "Napełnij <w>rękaw</w> masą i wyciskaj małe ciasteczka na blachę.",
                            "target_lang_translations": {
                                "ru": "Наполни <w>кондитерский мешок</w> массой и выдавливай маленькие печенья на противень."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rękaw cukierniczy",
                        "wyciskać rękawem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кухонный инструмент в виде конусообразного мешка для выдавливания крема или теста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кондитерский мешок",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22d62fb0-85d6-319b-b242-8d747d25d237",
                    "sense_definition": "Wskaźnik kierunku wiatru w postaci stożkowatej rury z tkaniny umieszczonej na maszcie, używany głównie na lotniskach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_tunnels",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pomarańczowy <w>rękaw</w> na lotnisku wskazywał silny wiatr zachodni.",
                            "target_lang_translations": {
                                "ru": "Оранжевый <w>ветроуказатель</w> в аэропорту указывал на сильный западный ветер."
                            }
                        },
                        {
                            "text": "Pilot spojrzał na <w>rękaw</w>, aby ocenić warunki do lądowania.",
                            "target_lang_translations": {
                                "ru": "Пилот посмотрел на <w>ветровой конус</w>, чтобы оценить условия для посадки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rękaw wiatrowskazu",
                        "rękaw lotniczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указатель направления ветра в виде тканевого конуса на мачте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветроуказатель",
                                "target_lang_sense_clarification": "метеорология"
                            },
                            {
                                "target_lang_word": "ветровой конус",
                                "target_lang_sense_clarification": "форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79df761c-d7c8-3bb4-918a-a985f2341132",
                    "sense_definition": "Długa, segmentowa rura z tworzywa lub tkaniny, służąca do bezpiecznego zrzucania gruzu i śmieci z wyższych pięter budowy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_tunnels",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Robotnicy wyrzucali gruz z trzeciego piętra przez plastikowy <w>rękaw</w>.",
                            "target_lang_translations": {
                                "ru": "Рабочие выбрасывали строительный мусор с третьего этажа через пластиковый <w>рукав</w>."
                            }
                        },
                        {
                            "text": "Podczas remontu zamontowano <w>rękaw</w> zsypowy przy oknie.",
                            "target_lang_translations": {
                                "ru": "Во время ремонта у окна установили <w>мусоросброс</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rękaw gruzowy",
                        "rękaw zsypowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная секционная труба для безопасного сброса строительного мусора с высоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строительный рукав",
                                "target_lang_sense_clarification": "оборудование"
                            },
                            {
                                "target_lang_word": "мусоросброс",
                                "target_lang_sense_clarification": "система"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "297e8914-fd3f-3baf-b021-ff3cf468faf6",
                    "sense_definition": "Cel powietrzny w kształcie stożka ciągnięty na linie za samolotem podczas ćwiczeń strzeleckich.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_tunnels",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artyleria przeciwlotnicza trenowała strzelanie do <w>rękawa</w>.",
                            "target_lang_translations": {
                                "ru": "Зенитная артиллерия тренировалась в стрельбе по <w>конусу-мишени</w>."
                            }
                        },
                        {
                            "text": "Samolot holował <w>rękaw</w> nad poligonem wojskowym.",
                            "target_lang_translations": {
                                "ru": "Самолет буксировал <w>мишень</w> над военным полигоном."
                            }
                        }
                    ],
                    "common_phrases": [
                        "holować rękaw"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "military"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздушная мишень в форме конуса, буксируемая самолетом для тренировки стрельбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конус-мишень",
                                "target_lang_sense_clarification": "военное дело"
                            },
                            {
                                "target_lang_word": "буксируемая мишень",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

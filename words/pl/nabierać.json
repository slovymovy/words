                    "sense_id": "3475b608-439a-384e-ae36-b161ddea8af1",
                    "sense_definition": "Napełniać naczynie lub czerpać jakąś substancję, płyn lub powietrze do wnętrza czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fizyczne_gromadzenie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pielęgniarka musi bardzo ostrożnie <nabierać> lek do strzykawki.",
                            "target_lang_translations": {
                                "ru": "Медсестра должна очень осторожно <набирать> лекарство в шприц."
                            }
                        },
                        {
                            "text": "Lubię stać nad brzegiem morza i głęboko <nabierać> świeżego powietrza.",
                            "target_lang_translations": {
                                "ru": "Я люблю стоять на берегу моря и глубоко <вдыхать> свежий воздух."
                            }
                        }
                    ],
                    "synonyms": [
                        "czerpać",
                        "wciągać",
                        "brać"
                    ],
                    "antonyms": [
                        "wylewać",
                        "wypuszczać"
                    ],
                    "common_phrases": [

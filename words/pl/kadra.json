                {
                    "sense_id": "1b82bc07-1755-3644-9dfb-34426a770bca",
                    "sense_definition": "Grupa zawodników reprezentujących kraj lub klub w zawodach sportowych",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sports_team",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza <w>kadra</w> narodowa wygrała wczorajszy mecz eliminacyjny.",
                            "target_lang_translations": {
                                "ru": "Наша национальная <w>сборная</w> выиграла вчерашний отборочный матч."
                            }
                        },
                        {
                            "text": "Trener ogłosił nazwiska piłkarzy powołanych do <w>kadry</w> na mistrzostwa.",
                            "target_lang_translations": {
                                "ru": "Тренер объявил имена футболистов, вызванных в <w>сборную</w> на чемпионат."
                            }
                        }
                    ],
                    "synonyms": [

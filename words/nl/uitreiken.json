                {
                    "sense_id": "4e8a4f94-ab2b-3ebd-bda0-6250b4bc373c",
                    "sense_definition": "Iets officieels of feestelijks overhandigen, zoals een prijs, diploma of medaille.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De burgemeester zal de prijs persoonlijk <w>uitreiken</w>.",
                            "target_lang_translations": {
                                "en": "The mayor will personally <w>present</w> the prize.",
                                "ru": "Мэр лично <w>вручит</w> приз."
                            }
                        },
                        {
                            "text": "Aan het einde van het schooljaar worden de diploma's <w>uitgereikt</w>.",
                            "target_lang_translations": {
                                "en": "At the end of the school year, the diplomas are <w>handed out</w>.",
                                "ru": "В конце учебного года будут <w>вручены</w> дипломы."
                            }
                        }
                    ],
                    "synonyms": [
                        "overhandigen",
                        "toekennen"
                    ],
                    "antonyms": [
                        "innemen"
                    ],
                    "common_phrases": [
                        "een prijs uitreiken",
                        "een diploma uitreiken",
                        "een oorkonde uitreiken"
                    ],
                    "target_lang_definitions": {
                        "en": "To hand over something official or festive, such as a prize, diploma, or medal.",
                        "ru": "Вручать что-либо официальное или торжественное, например, приз, диплом или медаль."
                    },
                    "translations": {
                        "en": [
                            {

                        "lomp"
                    ],
                    "common_phrases": [
                        "goed hanteerbaar",
                        "hanteerbaar formaat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкий в удержании или использовании благодаря удобному размеру или весу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удобный в обращении",

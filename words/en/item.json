            "senses": [
                {
                    "sense_id": "1f02cfc0-46af-33aa-a07a-4c6b18167beb",
                    "sense_definition": "A single distinct physical object or thing.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please place each <w>item</w> in the correct recycling bin.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, положите каждый <w>предмет</w> в соответствующий мусорный бак."
                            }
                        },
                        {
                            "text": "She bought a few small <w>items</w> at the grocery store.",
                            "target_lang_translations": {
                                "ru": "Она купила несколько мелких <w>вещей</w> в продуктовом магазине."
                            }
                        }
                    ],
                    "synonyms": [
                        "object",
                        "thing",
                        "article"
                    ],
                    "common_phrases": [
                        "single item",
                        "collector's item",
                        "household item",
                        "luxury item"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный физический объект или вещь."
                    },
                    "translations": {
                        "ru": [
                            {

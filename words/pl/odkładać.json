                    "common_phrases": [
                        "odkładać pieniądze",
                        "odkładać na czarną godzinę",
                        "odkładać na przyszłość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Копить деньги или другие ресурсы для использования в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "сберегать деньги"
                            },
                            {
                                "target_lang_word": "копить",
                                "target_lang_sense_clarification": "собирать средства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac03013b-5778-343c-82aa-e6a6c7ab6e4c",
                    "sense_definition": "Kończyć rozmowę telefoniczną poprzez położenie słuchawki na aparat lub rozłączenie się.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy dzwonią telemarketerzy, zazwyczaj od razu <w>odkładam</w> słuchawkę.",
                            "target_lang_translations": {
                                "ru": "Когда звонят телемаркетологи, я обычно сразу <w>вешаю</w> трубку."
                            }
                        },
                        {
                            "text": "Jeszcze raz tak powiesz i <w>odkładam</w> słuchawkę!",
                            "target_lang_translations": {
                                "ru": "Скажешь так еще раз, и я <w>повешу</w> трубку!"
                            }
                        }
                    ],
                    "antonyms": [
                        "odbierać",
                        "podnosić"
                    ],
                    "common_phrases": [
                        "odkładać słuchawkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заканчивать телефонный разговор, вешая трубку или отключаясь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вешать трубку",
                                "target_lang_sense_clarification": "заканчивать разговор"
                            },
                            {
                                "target_lang_word": "класть трубку",
                                "target_lang_sense_clarification": "физическое действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da3eea26-9d48-3d48-ada4-6ae58727d8df",
                    "sense_definition": "Gromadzić się warstwami lub osadzać na jakiejś powierzchni (często o tłuszczu, osadzie, kamieniu).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tkanka tłuszczowa chętnie <w>odkłada</w> się na brzuchu i biodrach.",
                            "target_lang_translations": {
                                "ru": "Жировая ткань охотно <w>откладывается</w> на животе и бедрах."
                            }
                        },
                        {
                            "text": "W starych rurach często <w>odkłada</w> się kamień, co utrudnia przepływ wody.",
                            "target_lang_translations": {
                                "ru": "В старых трубах часто <w>откладывается</w> накипь, что затрудняет поток воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "odciskać się",
                        "zdawać się"
                    ],
                    "common_phrases": [
                        "odkładać się w organizmie",
                        "odkładać się na ściankach"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Накапливаться слоями или оседать на какой-либо поверхности (часто о жире, осадке, накипи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откладываться",
                                "target_lang_sense_clarification": "накапливаться"
                            },
                            {
                                "target_lang_word": "оседать",
                                "target_lang_sense_clarification": "о налете или осадке"
                            }
                        ]
                    }
                }

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2dea3445-b2a8-3cf1-a504-20115acefe9b",
                    "sense_definition": "A person who is in charge of the controls of an aircraft.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "navigation_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>pilot</w> announced that we would be landing in twenty minutes.",
                            "target_lang_translations": {
                                "ru": "<w>Пилот</w> объявил, что мы приземлимся через двадцать минут."
                            }
                        },
                        {
                            "text": "She trained for years to become a commercial airline <w>pilot</w>.",
                            "target_lang_translations": {
                                "ru": "Она училась годами, чтобы стать <w>пилотом</w> гражданской авиации."
                            }
                        }
                    ],
                    "synonyms": [
                        "aviator",
                        "flyer"
                    ],
                    "common_phrases": [
                        "airline pilot",
                        "fighter pilot",
                        "helicopter pilot",
                        "commercial pilot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который управляет летательным аппаратом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пилот",
                                "target_lang_sense_clarification": "профессия"
                            },
                            {
                                "target_lang_word": "лётчик",
                                "target_lang_sense_clarification": "авиатор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47a0fd08-75de-3b4d-8a21-562eb2c69d52",
                    "sense_definition": "A sample episode of a proposed TV series produced to decide if it should be made into a full series.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "test_trial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The network ordered a <w>pilot</w> for the new comedy show.",
                            "target_lang_translations": {
                                "ru": "Телеканал заказал <w>пилотный выпуск</w> для нового комедийного шоу."
                            }
                        },
                        {
                            "text": "Although the <w>pilot</w> was successful, the series was cancelled after one season.",
                            "target_lang_translations": {
                                "ru": "Хотя <w>пилот</w> был успешным, сериал закрыли после первого сезона."
                            }
                        }
                    ],
                    "common_phrases": [
                        "TV pilot",
                        "pilot episode",
                        "shoot a pilot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пробный выпуск телесериала, созданный для оценки перспективности проекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пилотный выпуск",
                                "target_lang_sense_clarification": "сериала или шоу"
                            },
                            {
                                "target_lang_word": "пилот",
                                "target_lang_sense_clarification": "профессиональный жаргон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b7dc5e7-851d-3e6b-bf5a-28ed36a2c530",
                    "sense_definition": "A person with local knowledge who is hired to guide a ship through a difficult harbor or coastal area.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "navigation_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A local <w>pilot</w> boarded the vessel to navigate the treacherous river mouth.",
                            "target_lang_translations": {
                                "ru": "Местный <w>лоцман</w> поднялся на борт судна, чтобы провести его через опасное устье реки."
                            }
                        },
                        {
                            "text": "The captain handed over control to the harbor <w>pilot</w> for docking.",
                            "target_lang_translations": {
                                "ru": "Капитан передал управление портовому <w>лоцману</w> для швартовки."
                            }
                        }
                    ],
                    "synonyms": [
                        "helmsman",
                        "navigator"
                    ],
                    "common_phrases": [
                        "harbor pilot",
                        "river pilot",
                        "marine pilot"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист, знающий местный фарватер и проводящий суда через опасные или сложные участки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лоцман",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62c9b18e-ff34-3d84-8974-a7cca4f34c79",
                    "sense_definition": "A small flame kept burning continuously in a gas appliance to light the main burner when needed.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The water heater isn't working because the <w>pilot</w> has gone out.",
                            "target_lang_translations": {
                                "ru": "Водонагреватель не работает, потому что погас <w>запальник</w>."
                            }
                        },
                        {
                            "text": "You need to relight the <w>pilot</w> before the furnace will turn on.",
                            "target_lang_translations": {
                                "ru": "Вам нужно снова зажечь <w>запальник</w>, прежде чем печь включится."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pilot light"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое пламя в газовом приборе, горящее постоянно для розжига основной горелки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запальник",
                                "target_lang_sense_clarification": "устройство розжига"
                            },
                            {
                                "target_lang_word": "фитиль",
                                "target_lang_sense_clarification": "разг. пламя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5a98e6e3-ea56-3f1e-97ad-6c445b9aa1e7",
                    "sense_definition": "To control and steer an aircraft or watercraft.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "navigation_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has <w>piloted</w> small planes for over ten years.",
                            "target_lang_translations": {
                                "ru": "Он <w>пилотирует</w> небольшие самолеты уже более десяти лет."
                            }
                        },
                        {
                            "text": "The captain <w>piloted</w> the ship safely through the storm.",
                            "target_lang_translations": {
                                "ru": "Капитан благополучно <w>провел</w> корабль сквозь шторм."
                            }
                        }
                    ],
                    "synonyms": [
                        "fly",
                        "steer",
                        "navigate",
                        "captain"
                    ],
                    "common_phrases": [
                        "pilot a plane",
                        "pilot a ship",
                        "pilot a boat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Управлять летательным аппаратом или судном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пилотировать",
                                "target_lang_sense_clarification": "самолет"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "судно или самолет"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c909df3-f5cf-3910-911a-2072ef47e640",
                    "sense_definition": "To test or have a preliminary trial of a new idea, product, or program.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "test_trial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school decided to <w>pilot</w> the new curriculum in three classrooms.",
                            "target_lang_translations": {
                                "ru": "Школа решила <w>апробировать</w> новую учебную программу в трех классах."
                            }
                        },
                        {
                            "text": "We are <w>piloting</w> a new payment system to see if it reduces errors.",
                            "target_lang_translations": {
                                "ru": "Мы <w>тестируем</w> новую платежную систему, чтобы проверить, уменьшит ли она количество ошибок."
                            }
                        }
                    ],
                    "synonyms": [
                        "test",
                        "trial"
                    ],
                    "common_phrases": [
                        "pilot a scheme",
                        "pilot a program",
                        "pilot a study"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить предварительные испытания новой идеи, продукта или программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тестировать",
                                "target_lang_sense_clarification": "проверять"
                            },
                            {
                                "target_lang_word": "апробировать",
                                "target_lang_sense_clarification": "официально испытывать"
                            },
                            {
                                "target_lang_word": "испытывать",
                                "target_lang_sense_clarification": "пробовать на практике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee5d237e-2e34-3979-9afc-abead637ad31",
                    "sense_definition": "To guide or conduct a person through a place or situation.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "navigation_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>piloted</w> the guests through the crowded room to the exit.",
                            "target_lang_translations": {
                                "ru": "Она <w>провела</w> гостей через переполненную комнату к выходу."
                            }
                        },
                        {
                            "text": "The manager <w>piloted</w> his team through the difficult restructuring process.",
                            "target_lang_translations": {
                                "ru": "Менеджер <w>провел</w> свою команду через сложный процесс реструктуризации."
                            }
                        }
                    ],
                    "synonyms": [
                        "guide",
                        "escort",
                        "steer",
                        "shepherd"
                    ],
                    "common_phrases": [
                        "pilot someone through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить или направлять кого-либо через место или ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проводить",
                                "target_lang_sense_clarification": "сопровождать"
                            },
                            {
                                "target_lang_word": "вести",
                                "target_lang_sense_clarification": "направлять"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1b71e79b-a2d8-383c-a308-00c076446011",
                    "sense_definition": "Made or used as a test or demonstration of capability before a full launch.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "test_trial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>pilot</w> project was successful enough to justify a nationwide launch.",
                            "target_lang_translations": {
                                "ru": "<w>Пилотный</w> проект был достаточно успешным, чтобы оправдать запуск в масштабах всей страны."
                            }
                        },
                        {
                            "text": "We are conducting a <w>pilot</w> study to gather initial data.",
                            "target_lang_translations": {
                                "ru": "Мы проводим <w>пилотное</w> исследование для сбора первичных данных."
                            }
                        }
                    ],
                    "synonyms": [
                        "experimental",
                        "trial",
                        "test",
                        "probationary"
                    ],
                    "antonyms": [
                        "final",

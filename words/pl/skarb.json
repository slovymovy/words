    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "012199c8-bba3-3c07-8d70-eaf16f4dafba",
                    "sense_definition": "Osoba bardzo kochana i ceniona; często używane jako czuły zwrot do partnera, dziecka lub bliskiej osoby.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person_endearment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <skarbie>, czy możesz zrobić mi herbatę?",
                            "target_lang_translations": {
                                "ru": "Моё <сокровище>, можешь сделать мне чай?"
                            }
                        },
                        {
                            "text": "Ta mała dziewczynka to prawdziwy <skarb> dla swoich rodziców.",
                            "target_lang_translations": {
                                "ru": "Эта маленькая девочка — настоящее <сокровище> для своих родителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "perła"
                    ],
                    "common_phrases": [
                        "mój skarbie",
                        "największy skarb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, которого очень любят и ценят; часто используется как ласковое обращение к партнеру, ребенку или близкому человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сокровище",
                                "target_lang_sense_clarification": "ласковое обращение"
                            },
                            {
                                "target_lang_word": "золотце",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "238e762f-19bb-3d72-ac23-4cb1d19102f9",
                    "sense_definition": "Zbiór bardzo wartościowych rzeczy, takich jak złoto, klejnoty czy pieniądze, często ukryty lub znaleziony.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "valuable_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Piraci zakopali swój zrabowany <skarb> na bezludnej wyspie.",
                            "target_lang_translations": {
                                "ru": "Пираты закопали своё награбленное <сокровище> на необитаемом острове."
                            }
                        },
                        {
                            "text": "Dzieci bawiły się w ogrodzie, udając, że szukają ukrytego <skarbu>.",
                            "target_lang_translations": {
                                "ru": "Дети играли в саду, притворяясь, что ищут спрятанный <клад>."
                            }
                        }
                    ],
                    "synonyms": [
                        "majątek"
                    ],
                    "common_phrases": [
                        "mapa skarbów",
                        "szukać skarbu",
                        "ukryty skarb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собрание очень ценных вещей, таких как золото, драгоценности или деньги, часто спрятанное или найденное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сокровище",
                                "target_lang_sense_clarification": "драгоценности"
                            },
                            {
                                "target_lang_word": "клад",
                                "target_lang_sense_clarification": "спрятанные ценности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d159a85c-1a6a-393c-b85f-055ccbccdc26",
                    "sense_definition": "Coś niezwykle wartościowego, drogocennego lub rzadkiego, co ma wielkie znaczenie niematerialne.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zdrowie jest największym <skarbem>, o który każdy powinien dbać.",
                            "target_lang_translations": {
                                "ru": "Здоровье — это величайшее <сокровище>, которое каждый должен беречь."
                            }
                        },
                        {
                            "text": "Ta stara książka to prawdziwy <skarb> dla każdego historyka.",
                            "target_lang_translations": {
                                "ru": "Эта старая книга — настоящее <сокровище> для каждого историка."
                            }
                        }
                    ],
                    "synonyms": [
                        "perła"
                    ],
                    "common_phrases": [
                        "prawdziwy skarb",
                        "skarb kultury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то невероятно ценное, дорогое или редкое, имеющее большое нематериальное значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сокровище",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "ценность",
                                "target_lang_sense_clarification": "важность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8e529cf-2543-3526-a602-16e6a89e32c5",
                    "sense_definition": "Majątek i środki finansowe należące do państwa; instytucja zarządzająca tymi środkami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie podatki i opłaty zasilają ostatecznie <skarb> państwa.",
                            "target_lang_translations": {
                                "ru": "Все налоги и сборы в конечном итоге пополняют государственную <казну>."
                            }
                        },
                        {
                            "text": "Firma została ukarana wysoką grzywną na rzecz <Skarbu> Państwa.",
                            "target_lang_translations": {
                                "ru": "Компания была оштрафована на крупную сумму в пользу Государственной <казны>."
                            }
                        }
                    ],
                    "synonyms": [
                        "majątek"
                    ],
                    "common_phrases": [
                        "Skarb Państwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имущество и финансовые средства, принадлежащие государству; учреждение, управляющее этими средствами."
                    },
                    "translations": {

{
    "word_family": [
        "stypendialny",
        "stypendysta",
        "stypendystka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c6488b9b-97db-32d9-a71b-12d593aa7b04",
                    "sense_definition": "Pomoc finansowa udzielana osobom kształcącym się lub prowadzącym badania naukowe.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "edu_funding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marta otrzymała <stypendium> za wybitne wyniki w nauce.",
                            "target_lang_translations": {
                                "ru": "Марта получила <стипендию> за выдающиеся успехи в учебе."
                            }
                        },
                        {
                            "text": "Jeśli planujesz nowe projekty ekologiczne, możesz ubiegać się o <stypendium>.",
                            "target_lang_translations": {
                                "ru": "Если ты планируешь новые экологические проекты, ты можешь подать заявку на <грант>."
                            }
                        }
                    ],
                    "synonyms": [
                        "grant"
                    ],
                    "common_phrases": [
                        "stypendium naukowe",
                        "stypendium socjalne",
                        "otrzymać stypendium",
                        "ubiegać się o stypendium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Финансовая помощь, предоставляемая людям, которые учатся или проводят научные исследования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стипендия",
                                "target_lang_sense_clarification": "денежное пособие для учащихся"
                            },
                            {
                                "target_lang_word": "грант",
                                "target_lang_sense_clarification": "финансирование исследований или проектов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3389b23-e361-31eb-af03-6c005489d778",
                    "sense_definition": "Nauka, studia lub badania naukowe finansowane przez daną instytucję, często wiążące się z wyjazdem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "edu_funding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Najwięcej praktycznych umiejętności zdobyłam będąc na <stypendium> w Australii.",
                            "target_lang_translations": {
                                "ru": "Больше всего практических навыков я приобрела, будучи на <стажировке> в Австралии."
                            }
                        },
                        {
                            "text": "Mój brat wyjechał na roczne <stypendium> do Hiszpanii.",
                            "target_lang_translations": {
                                "ru": "Мой брат уехал на годовую <стажировку> в Испанию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyjechać na stypendium",
                        "być na stypendium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обучение, учеба или научные исследования, финансируемые учреждением, часто связанные с выездом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стажировка",
                                "target_lang_sense_clarification": "период обучения или практики, часто за границей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0727aa8e-d4cd-35ab-bd82-aae38ec2c0dc",
                    "sense_definition": "Ofiara pieniężna składana kapłanowi za odprawienie mszy w określonej intencji.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious_offering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za przyjęte <stypendium> mszalne kapłan winien odprawić mszę zgodnie z intencją ofiarodawcy.",
                            "target_lang_translations": {
                                "ru": "За принятую мессовую <стипендию> священник должен отслужить мессу в соответствии с намерением жертвователя."
                            }
                        },
                        {
                            "text": "Wierni złożyli <stypendium> na mszę dziękczynną w intencji zdrowia proboszcza.",
                            "target_lang_translations": {
                                "ru": "Верующие внесли <пожертвование> на благодарственную мессу за здоровье настоятеля."
                            }
                        }
                    ],
                    "synonyms": [
                        "ofiara"
                    ],
                    "common_phrases": [
                        "stypendium mszalne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежное пожертвование священнику за совершение мессы в определенном намерении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожертвование",
                                "target_lang_sense_clarification": "добровольный взнос"
                            },
                            {
                                "target_lang_word": "стипендия",
                                "target_lang_sense_clarification": "церковный термин (мессовая стипендия)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        "разворот"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dd6cf671-3538-3521-93ec-d4db12a3c7e4",
                    "sense_definition": "Изменить направление движения, повернуть в сторону.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_turn",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На перекрёстке машина <свернула> направо и скрылась из виду.",
                            "target_lang_translations": {
                                "en": "At the intersection, the car <turned> right and disappeared from view."
                            }
                        },
                        {
                            "text": "Мы решили <свернуть> с шумной улицы в тихий парк.",
                            "target_lang_translations": {
                                "en": "We decided to <turn off> the noisy street into a quiet park."
                            }
                        }
                    ],
                    "synonyms": [
                        "смотать"
                    ],
                    "common_phrases": [
                        "свернуть направо",
                        "свернуть налево",
                        "свернуть с дороги",
                        "свернуть за угол"
                    ],
                    "target_lang_definitions": {
                        "en": "To change the direction of movement, to turn aside."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "change direction"
                            },
                            {
                                "target_lang_word": "turn off",
                                "target_lang_sense_clarification": "leave a road or path"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b82eaba-1d47-34f5-9859-b30f38c4399c",
                    "sense_definition": "Скатать что-либо в трубку, рулон или сложить, загибая края.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "manipulation_fold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После пикника мы аккуратно <свернули> ковёр и убрали его в машину.",
                            "target_lang_translations": {
                                "en": "After the picnic, we neatly <rolled up> the rug and put it in the car."
                            }
                        },
                        {
                            "text": "Девушка <свернула> записку в трубочку и передала её подруге.",
                            "target_lang_translations": {
                                "en": "The girl <rolled> the note into a tube and passed it to her friend."
                            }
                        }
                    ],
                    "synonyms": [
                        "скрутить",
                        "смотать"
                    ],
                    "antonyms": [
                        "развернуть"
                    ],
                    "common_phrases": [
                        "свернуть ковёр",
                        "свернуть в трубочку",
                        "свернуть в рулон",
                        "свернуть палатку"
                    ],
                    "target_lang_definitions": {
                        "en": "To roll something into a tube or fold it by bending the edges."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "roll up",
                                "target_lang_sense_clarification": "make into a cylinder"
                            },
                            {
                                "target_lang_word": "fold",
                                "target_lang_sense_clarification": "bend over"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7054048-d537-30ae-b94a-1b9acac70cd1",
                    "sense_definition": "Прекратить, закончить или временно остановить какую-либо деятельность или работу.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за нехватки финансирования компании пришлось <свернуть> производство.",
                            "target_lang_translations": {
                                "en": "Due to a lack of funding, the company had to <shut down> production."
                            }
                        },
                        {
                            "text": "Директор приказал <свернуть> проект до лучших времён.",
                            "target_lang_translations": {
                                "en": "The director ordered to <wind up> the project until better times."
                            }
                        }
                    ],
                    "antonyms": [
                        "развернуть"
                    ],
                    "common_phrases": [
                        "свернуть производство",
                        "свернуть деятельность",
                        "свернуть работы",
                        "свернуть программу"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop, finish, or temporarily halt an activity or work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shut down",
                                "target_lang_sense_clarification": "production or business"
                            },
                            {
                                "target_lang_word": "wind up",
                                "target_lang_sense_clarification": "a project or activity"
                            },
                            {
                                "target_lang_word": "curtail",
                                "target_lang_sense_clarification": "reduce or stop"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6db4df7-7926-3abe-adde-f7b3e6e5f4eb",
                    "sense_definition": "Сломать, повредить или сместить резким вращательным движением.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "damage_break",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если крутить гайку слишком сильно, можно <свернуть> резьбу.",
                            "target_lang_translations": {
                                "en": "If you twist the nut too hard, you can <strip> the thread."
                            }
                        },
                        {
                            "text": "Он упал с велосипеда и чуть не <свернул> себе шею.",
                            "target_lang_translations": {
                                "en": "He fell off his bike and almost <broke> his neck."
                            }
                        }
                    ],
                    "synonyms": [
                        "скрутить"
                    ],
                    "common_phrases": [
                        "свернуть шею",
                        "свернуть голову",
                        "свернуть резьбу",
                        "свернуть кран"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в выражении 'свернуть шею'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To break, damage, or displace with a sharp rotating movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strip",
                                "target_lang_sense_clarification": "damage a thread or screw"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "damage by twisting (e.g., neck)"
                            },
                            {
                                "target_lang_word": "wring",
                                "target_lang_sense_clarification": "twist forcibly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72803d22-107e-3012-9b8c-39a8d747d2ad",
                    "sense_definition": "Изготовить что-либо методом скручивания (обычно о самокрутках или кулинарных изделиях).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "manipulation_fold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старик достал кисет и ловко <свернул> самокрутку одной рукой.",
                            "target_lang_translations": {
                                "en": "The old man took out a tobacco pouch and deftly <rolled> a cigarette with one hand."
                            }
                        },
                        {
                            "text": "Повар показал, как правильно <свернуть> рулет с начинкой.",
                            "target_lang_translations": {
                                "en": "The cook showed how to properly <roll> a roulade with filling."
                            }
                        }
                    ],
                    "synonyms": [
                        "скрутить"
                    ],
                    "common_phrases": [
                        "свернуть самокрутку",
                        "свернуть папиросу"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something by rolling (usually hand-rolled cigarettes or food)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "roll",
                                "target_lang_sense_clarification": "make by rolling"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "b541d8f8-d369-37b6-a0e1-e607fbec1455",
                    "sense_definition": "Een lijn van mensen die wachten tot ze aan de beurt zijn",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "queue_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er stond een lange <w>rij</w> voor de ingang van de bioscoop.",
                            "target_lang_translations": {
                                "en": "There was a long <w>queue</w> at the entrance of the cinema.",
                                "ru": "Перед входом в кинотеатр стояла длинная <w>очередь</w>."
                            }
                        },
                        {
                            "text": "Sluit maar achteraan in de <w>rij</w> aan als je een kaartje wilt.",
                            "target_lang_translations": {
                                "en": "Just join the back of the <w>queue</w> if you want a ticket.",
                                "ru": "Вставай в конец <w>очереди</w>, если хочешь билет."
                            }
                        }
                    ],
                    "synonyms": [
                        "reeks"
                    ],
                    "common_phrases": [
                        "in de rij staan",
                        "in de rij",
                        "achteraan in de rij"
                    ],
                    "target_lang_definitions": {
                        "en": "A line of people waiting for their turn",
                        "ru": "Группа людей, ожидающих своей очереди"
                    },
                    "translations": {
                        "en": [

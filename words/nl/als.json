{
    "word_family": [
        "alsof",
        "evenals",
        "zowel",
        "alsmede",
        "alsmaar"
    ],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "cdca12bc-1eb2-3163-952e-df08604fcacf",
                    "sense_definition": "Op voorwaarde dat; in het geval dat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "condition_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Als</w> je goed studeert, zul je slagen voor het examen.",
                            "target_lang_translations": {
                                "en": "<w>If</w> you study well, you will pass the exam.",
                                "ru": "<w>Если</w> ты будешь хорошо учиться, ты сдашь экзамен."
                            }
                        },
                        {
                            "text": "We gaan morgen naar het strand <w>als</w> de zon schijnt.",
                            "target_lang_translations": {
                                "en": "We are going to the beach tomorrow <w>if</w> the sun shines.",
                                "ru": "Мы пойдем завтра на пляж, <w>если</w> будет светить солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "indien"
                    ],
                    "common_phrases": [
                        "als het kan",
                        "als het moet",
                        "als volgt"
                    ],
                    "target_lang_definitions": {
                        "en": "On the condition that; in the case that.",
                        "ru": "При условии, что; в случае, если."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "if",
                                "target_lang_sense_clarification": "introducing a condition"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "если",
                                "target_lang_sense_clarification": "союз условия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0f34a83-bd05-3fde-9a45-c4f5bee4ea2f",
                    "sense_definition": "Op het moment dat; wanneer.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "condition_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Als</w> ik thuiskom, doe ik meteen mijn schoenen uit.",
                            "target_lang_translations": {
                                "en": "<w>When</w> I come home, I immediately take off my shoes.",
                                "ru": "<w>Когда</w> я прихожу домой, я сразу снимаю обувь."
                            }
                        },
                        {
                            "text": "Het wordt meestal snel donker <w>als</w> de avond valt.",
                            "target_lang_translations": {
                                "en": "It usually gets dark quickly <w>when</w> evening falls.",
                                "ru": "Обычно быстро темнеет, <w>когда</w> наступает вечер."
                            }
                        }
                    ],
                    "synonyms": [
                        "wanneer"
                    ],
                    "common_phrases": [
                        "als de dag van gisteren"
                    ],
                    "target_lang_definitions": {
                        "en": "At the moment that; when.",
                        "ru": "В тот момент, когда; когда."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "when",
                                "target_lang_sense_clarification": "at the time that"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "когда",
                                "target_lang_sense_clarification": "союз времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68f16e1d-5198-349f-930f-fdaf5a7b581c",
                    "sense_definition": "Gebruikt als tweede deel in de combinatie 'zowel ... als ...' om aan te geven dat twee dingen gelden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik houd zowel van koffie <w>als</w> van thee.",
                            "target_lang_translations": {
                                "en": "I like both coffee <w>and</w> tea.",
                                "ru": "Я люблю как кофе, <w>так и</w> чай."
                            }
                        },
                        {
                            "text": "Zowel de leraar <w>als</w> de leerlingen waren aanwezig bij de viering.",
                            "target_lang_translations": {
                                "en": "Both the teacher <w>and</w> the students were present at the celebration.",
                                "ru": "Как учитель, <w>так и</w> ученики присутствовали на праздновании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zowel ... als"
                    ],
                    "target_lang_definitions": {
                        "en": "Used as the second part in the combination 'zowel ... als ...' (both ... and) to indicate that two things apply.",
                        "ru": "Используется как вторая часть конструкции 'zowel ... als ...' (как ..., так и ...)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "and",
                                "target_lang_sense_clarification": "in 'both ... and'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "так и",
                                "target_lang_sense_clarification": "часть союза 'как ..., так и ...'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26543dd2-72f5-3a19-993b-a6f6cfdcba29",
                    "sense_definition": "Op een manier die suggereert dat iets het geval is, terwijl dat niet zo is; alsof.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gedroeg zich <w>als</w> was hij de koning van het land.",
                            "target_lang_translations": {
                                "en": "He behaved <w>as if</w> he were the king of the country.",
                                "ru": "Он вел себя так, <w>словно</w> он был королем страны."
                            }
                        },
                        {
                            "text": "Ze keek me aan <w>als</w> zag ze water branden.",
                            "target_lang_translations": {
                                "en": "She looked at me <w>as if</w> she saw water burning.",
                                "ru": "Она смотрела на меня, <w>как будто</w> увидела что-то невероятное."
                            }
                        }
                    ],
                    "common_phrases": [
                        "als het ware"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that suggests something is the case, while it is not; as if.",
                        "ru": "Словно; как будто; указывает на воображаемую ситуацию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "as if",
                                "target_lang_sense_clarification": "hypothetical comparison"
                            },
                            {
                                "target_lang_word": "as though",
                                "target_lang_sense_clarification": "hypothetical comparison"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "словно",
                                "target_lang_sense_clarification": "сравнение с гипотетической ситуацией"
                            },
                            {
                                "target_lang_word": "как будто",
                                "target_lang_sense_clarification": "сравнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0768993b-200f-396e-8d94-44ec2bcb032b",
                    "sense_definition": "Introduceert een indirecte vraag; of.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik weet niet <w>als</w> hij vandaag nog zal komen.",
                            "target_lang_translations": {
                                "en": "I don't know <w>if</w> he will still come today.",
                                "ru": "Я не знаю, придет <w>ли</w> он сегодня."
                            }
                        },
                        {
                            "text": "De directeur vroeg zich af <w>als</w> de cijfers wel klopten.",
                            "target_lang_translations": {
                                "en": "The director wondered <w>if</w> the figures were actually correct.",
                                "ru": "Директор спрашивал себя, верны <w>ли</w> цифры."
                            }
                        }
                    ],
                    "synonyms": [
                        "of"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in Surinaams-Nederlands en Vlaams"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Introduces an indirect question; whether.",
                        "ru": "Вводит косвенный вопрос; соответствует союзу 'ли'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "if",
                                "target_lang_sense_clarification": "whether"
                            },
                            {
                                "target_lang_word": "whether",
                                "target_lang_sense_clarification": "indirect question"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ли",
                                "target_lang_sense_clarification": "частица в косвенном вопросе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "c62c50b1-ba2c-37ac-a3f0-30bf0035d8e3",
                    "sense_definition": "In de hoedanigheid van; in de functie of rol van.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn broer werkt al jaren <w>als</w> bakker in het dorp.",
                            "target_lang_translations": {
                                "en": "My brother has worked <w>as</w> a baker in the village for years.",
                                "ru": "Мой брат уже много лет работает <w>как</w> пекарь в деревне."
                            }
                        },
                        {
                            "text": "<w>Als</w> wijkbewoner wil ik graag mijn mening geven over het plan.",
                            "target_lang_translations": {
                                "en": "<w>As</w> a local resident, I would like to give my opinion on the plan.",
                                "ru": "<w>Как</w> житель района, я хотел бы высказать свое мнение о плане."
                            }
                        }
                    ],
                    "common_phrases": [
                        "werken als",
                        "bekend als",
                        "dienen als"
                    ],
                    "target_lang_definitions": {
                        "en": "In the capacity of; in the function or role of.",
                        "ru": "В качестве; в роли; указывает на функцию или профессию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "as",
                                "target_lang_sense_clarification": "in the role of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "в качестве кого-либо"
                            },
                            {
                                "target_lang_word": "в качестве",
                                "target_lang_sense_clarification": "официальный стиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecfae926-7abf-357c-b970-67a9f105f2c6",
                    "sense_definition": "Op dezelfde manier; zoals; duidt een vergelijking of gelijkheid aan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is precies even groot <w>als</w> ik.",
                            "target_lang_translations": {
                                "en": "He is exactly as tall <w>as</w> I am.",
                                "ru": "Он точно такого же роста, <w>как</w> я."
                            }
                        },
                        {
                            "text": "Ze zingt zo mooi <w>als</w> een nachtegaal.",
                            "target_lang_translations": {
                                "en": "She sings as beautifully <w>as</w> a nightingale.",
                                "ru": "Она поет так красиво, <w>как</w> соловей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "net als",
                        "zoals",
                        "zo goed als"
                    ],
                    "target_lang_definitions": {
                        "en": "In the same way; like; indicates a comparison or equality.",
                        "ru": "Подобно; так же, как; указывает на сходство."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "as",
                                "target_lang_sense_clarification": "indicating equality"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "сравнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "922b3e89-946a-3b9a-8e0b-e2aa064926a8",
                    "sense_definition": "Dan; gebruikt na een vergrotende trap.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is veel groter <w>als</w> zijn jongere broertje.",
                            "target_lang_translations": {
                                "en": "He is much taller <w>than</w> his younger brother.",
                                "ru": "Он намного выше, <w>чем</w> его младший брат."
                            }
                        },
                        {
                            "text": "Dat is nog altijd beter <w>als</w> helemaal niets.",
                            "target_lang_translations": {
                                "en": "That is still better <w>than</w> nothing at all.",
                                "ru": "Это все же лучше, <w>чем</w> совсем ничего."
                            }
                        }
                    ],
                    "synonyms": [
                        "dan"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "in de standaardtaal wordt 'dan' gebruikt na een vergrotende trap"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Than; used after a comparative degree.",
                        "ru": "Чем; используется после сравнительной степени (разговорное употребление)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "than",
                                "target_lang_sense_clarification": "comparison (colloquial)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чем",
                                "target_lang_sense_clarification": "при сравнении"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4776fd4b-fb24-3021-9bf1-1109a42bacb7",
                    "sense_definition": "Een ernstige zenuwziekte waarbij de spieren verzwakken (Amyotrofische Laterale Sclerose).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "De patiënt kreeg de diagnose <w>ALS</w> en begon direct met therapie.",
                            "target_lang_translations": {
                                "en": "The patient received the diagnosis <w>ALS</w> and started therapy immediately.",
                                "ru": "Пациенту поставили диагноз <w>БАС</w> и сразу начали терапию."
                            }
                        },
                        {
                            "text": "Er wordt veel geld ingezameld voor onderzoek naar <w>ALS</w>.",
                            "target_lang_translations": {
                                "en": "A lot of money is being raised for research into <w>ALS</w>.",
                                "ru": "Собирается много денег на исследования <w>БАС</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A serious nervous disease where muscles weaken (Amyotrophic Lateral Sclerosis).",
                        "ru": "Боковой амиотрофический склероз (заболевание нервной системы)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ALS",
                                "target_lang_sense_clarification": "medical acronym"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "БАС",
                                "target_lang_sense_clarification": "аббревиатура болезни"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
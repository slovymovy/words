                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dancers performed a complex <w>hey</w> in the middle of the stage.",
                            "target_lang_translations": {
                                "ru": "Танцоры исполнили сложный <w>хей</w> в центре сцены."
                            }

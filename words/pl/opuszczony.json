    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e44089e4-8e9e-3743-94f2-8b25f05bcb5b",
                    "sense_definition": "Taki, który został pozostawiony przez ludzi, mieszkańców lub opiekunów; pusty lub samotny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "abandonment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie znaleźliśmy stary, <opuszczony> dom, w którym nikt nie mieszkał.",
                            "target_lang_translations": {
                                "ru": "В лесу мы нашли старый <заброшенный> дом, в котором никто не жил."
                            }
                        },
                        {
                            "text": "Mały pies błąkał się po ulicy, wyglądając na głodnego i <opuszczonego>.",
                            "target_lang_translations": {
                                "ru": "Маленький пес бродил по улице и выглядел голодным и <брошенным>."
                            }
                        }
                    ],
                    "synonyms": [
                        "porzucony",
                        "osamotniony",
                        "bez opieki",
                        "pozostawiony",
                        "bezpański",
                        "opustoszały",
                        "pusty"
                    ],
                    "common_phrases": [
                        "opuszczony dom",
                        "opuszczony budynek",
                        "czuć się opuszczonym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставленный людьми, жителями или опекунами; пустой или одинокий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покинутый",
                                "target_lang_sense_clarification": "оставленный кем-то"
                            },
                            {
                                "target_lang_word": "брошенный",
                                "target_lang_sense_clarification": "оставленный без помощи"
                            },
                            {
                                "target_lang_word": "заброшенный",
                                "target_lang_sense_clarification": "о месте, где никто не живет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff16dc82-9427-3d89-b91e-fb104ac4511f",
                    "sense_definition": "Taki, który uległ zniszczeniu lub pogorszeniu stanu technicznego z powodu braku opieki i konserwacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "neglect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <opuszczony> ogród zarósł chwastami i wymaga dużo pracy.",
                            "target_lang_translations": {
                                "ru": "Этот <запущенный> сад зарос сорняками и требует много работы."
                            }
                        },
                        {
                            "text": "Kupili tanio <opuszczony> dworek, aby go wyremontować.",
                            "target_lang_translations": {
                                "ru": "Они дешево купили <заброшенную> усадьбу, чтобы отремонтировать ее."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaniedbany",
                        "zapuszczony",
                        "zdewastowany",
                        "zniszczony"
                    ],
                    "common_phrases": [
                        "opuszczony ogród",
                        "opuszczony cmentarz",
                        "opuszczony teren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пришедший в упадок или разрушенный из-за отсутствия ухода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запущенный",
                                "target_lang_sense_clarification": "неухоженный"
                            },
                            {
                                "target_lang_word": "заброшенный",
                                "target_lang_sense_clarification": "оставленный без присмотра"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "dismissal",
        "dismissive",
        "dismissively"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4f1d2da2-6af9-3f76-9f74-cdb5aa2bd0f6",
                    "sense_definition": "To remove someone from their job or position, usually because they have done something wrong or their services are no longer needed.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "employment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager decided to <w>dismiss</w> the employee for constant lateness.",
                            "target_lang_translations": {
                                "ru": "Менеджер решил <w>уволить</w> сотрудника за постоянные опоздания."
                            }
                        },
                        {
                            "text": "She was <w>dismissed</w> from her post after the company downsized.",
                            "target_lang_translations": {
                                "ru": "Она была <w>уволена</w> со своей должности после сокращения штата компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "lay off"
                    ],
                    "antonyms": [
                        "hire",
                        "employ",
                        "recruit"
                    ],
                    "common_phrases": [
                        "dismiss an employee",
                        "dismiss for misconduct",
                        "unfairly dismiss"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уволить кого-либо с работы или должности, обычно за проступок или из-за того, что услуги больше не требуются."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увольнять",
                                "target_lang_sense_clarification": "с работы"
                            },
                            {
                                "target_lang_word": "уволить",
                                "target_lang_sense_clarification": "с работы (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75dcb42b-da2f-30fc-b22c-2228ce8bc8fc",
                    "sense_definition": "To decide that something or someone is not important and not worth considering seriously.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>dismissed</w> the criticism as mere jealousy.",
                            "target_lang_translations": {
                                "ru": "Он <w>отверг</w> критику, сочтя её простой завистью."
                            }
                        },
                        {
                            "text": "You should not <w>dismiss</w> his ideas without listening to them first.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <w>отвергать</w> его идеи, не выслушав их сначала."
                            }
                        }
                    ],
                    "antonyms": [
                        "consider",
                        "accept",
                        "regard"
                    ],
                    "common_phrases": [
                        "dismiss a suggestion",
                        "dismiss as nonsense",
                        "dismiss out of hand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решить, что что-то или кто-то не важен и не заслуживает серьезного рассмотрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвергать",
                                "target_lang_sense_clarification": "идею, критику"
                            },
                            {
                                "target_lang_word": "отклонять",
                                "target_lang_sense_clarification": "предложение"
                            },
                            {
                                "target_lang_word": "отмахиваться",
                                "target_lang_sense_clarification": "игнорировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6694af44-4807-37f4-992d-9780bf96b09b",
                    "sense_definition": "To officially tell a person or group that they are allowed to leave.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher <w>dismissed</w> the class five minutes early today.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>отпустил</w> класс сегодня на пять минут раньше."
                            }
                        },
                        {
                            "text": "The soldiers were <w>dismissed</w> after the morning inspection.",
                            "target_lang_translations": {
                                "ru": "Солдаты были <w>распущены</w> после утреннего осмотра."
                            }
                        }
                    ],
                    "antonyms": [
                        "detain",
                        "hold",
                        "assemble"
                    ],
                    "common_phrases": [
                        "class dismissed",
                        "dismiss the meeting",
                        "dismiss the troops"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально объявить человеку или группе, что они могут уйти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отпускать",
                                "target_lang_sense_clarification": "разрешать уйти"
                            },
                            {
                                "target_lang_word": "распускать",
                                "target_lang_sense_clarification": "группу, собрание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c3ab7b7-3d57-3a2c-88f9-917b6ae13bc2",
                    "sense_definition": "To officially reject a legal case, claim, or appeal.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge <w>dismissed</w> the case due to a lack of evidence.",
                            "target_lang_translations": {
                                "ru": "Судья <w>отклонил</w> иск из-за недостатка улик."
                            }
                        },
                        {
                            "text": "Her appeal was <w>dismissed</w> by the high court yesterday.",
                            "target_lang_translations": {
                                "ru": "Её апелляция была <w>отклонена</w> верховным судом вчера."
                            }
                        }
                    ],
                    "antonyms": [
                        "accept",
                        "hear"
                    ],
                    "common_phrases": [
                        "dismiss the case",
                        "dismiss the charges",
                        "motion to dismiss"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально отклонить судебное дело, иск или апелляцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отклонять",
                                "target_lang_sense_clarification": "иск, апелляцию"
                            },
                            {
                                "target_lang_word": "прекращать",
                                "target_lang_sense_clarification": "судебное дело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a2a4efe-6346-3f0a-a6aa-9a9ae455cbd1",
                    "sense_definition": "To stop thinking about something; to remove an idea or feeling from one's mind.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She tried to <w>dismiss</w> the worry from her mind and relax.",
                            "target_lang_translations": {
                                "ru": "Она попыталась <w>выбросить</w> беспокойство из головы и расслабиться."
                            }
                        },
                        {
                            "text": "He <w>dismissed</w> all thoughts of failure and focused on the task.",
                            "target_lang_translations": {
                                "ru": "Он <w>отбросил</w> все мысли о неудаче и сосредоточился на задаче."
                            }
                        }
                    ],
                    "antonyms": [
                        "dwell on",
                        "harbor"
                    ],
                    "common_phrases": [
                        "dismiss a thought",
                        "dismiss from mind",
                        "dismiss the idea"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать думать о чем-то; выбросить идею или чувство из головы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбрасывать из головы",
                                "target_lang_sense_clarification": "мысли, заботы"
                            },
                            {
                                "target_lang_word": "гнать от себя",
                                "target_lang_sense_clarification": "мысли"
                            },
                            {
                                "target_lang_word": "отбрасывать",
                                "target_lang_sense_clarification": "сомнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "391ca153-6640-3ba0-9e8c-305a4fd3aa23",
                    "sense_definition": "To get a batsman out in the sport of cricket.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bowler <w>dismissed</w> the batsman for ninety-nine runs.",
                            "target_lang_translations": {
                                "ru": "Боулер <w>выбил</w> бэтсмена на девяноста девяти пробежках."
                            }
                        },
                        {
                            "text": "He was <w>dismissed</w> early in the game by a fast ball.",
                            "target_lang_translations": {
                                "ru": "Он был <w>выбит</w> из игры в начале матча быстрым мячом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dismiss a batsman",
                        "dismiss for a duck"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вывести бэтсмена из игры в крикете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбивать",
                                "target_lang_sense_clarification": "в крикете"
                            },
                            {
                                "target_lang_word": "выводить из игры",
                                "target_lang_sense_clarification": "спорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57a47969-376f-3055-9c74-3dda6bee4837",
                    "sense_definition": "To send a player off the field for breaking the rules in sports like soccer.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The referee <w>dismissed</w> the player for a dangerous tackle.",
                            "target_lang_translations": {
                                "ru": "Судья <w>удалил</w> игрока за опасный подкат."
                            }
                        },
                        {
                            "text": "He was <w>dismissed</w> from the game after receiving a red card.",
                            "target_lang_translations": {
                                "ru": "Он был <w>удален</w> из игры после получения красной карточки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dismiss a player",
                        "dismiss from the field"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалить игрока с поля за нарушение правил в таких видах спорта, как футбол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удалять",
                                "target_lang_sense_clarification": "с поля"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
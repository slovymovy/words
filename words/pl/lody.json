                {
                    "sense_id": "7e620fde-31d6-30db-8d58-6a86fc4fcd31",
                    "sense_definition": "Mrożony deser zrobiony z mleka, śmietany, cukru i różnych dodatków smakowych, takich jak owoce czy czekolada.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_dessert",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W upalny dzień najlepiej smakują zimne <w>lody</w> owocowe.",
                            "target_lang_translations": {
                                "ru": "В жаркий день холодное фруктовое <w>мороженое</w> вкуснее всего."
                            }
                        },
                        {
                            "text": "Moja siostra zawsze zamawia <w>lody</w> czekoladowe z bitą śmietaną.",
                            "target_lang_translations": {
                                "ru": "Моя сестра всегда заказывает шоколадное <w>мороженое</w> со взбитыми сливками."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gałka lodów",
                        "lody na patyku",
                        "lody włoskie",
                        "iść na lody",
                        "lody waniliowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Замороженный десерт из молока, сливок, сахара и различных вкусовых добавок, таких как фрукты или шоколад."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мороженое",
                                "target_lang_sense_clarification": "десерт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4f236ab-1cc1-3aa4-8ada-7b01f15c582a",
                    "sense_definition": "Forma liczby mnogiej rzeczownika <w>lód</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy przyszła wiosenna odwilż, <w>lody</w> na jeziorze zaczęły gwałtownie topnieć.",
                            "target_lang_translations": {
                                "ru": "Когда пришла весенняя оттепель, <w>льды</w> на озере начали стремительно таять."
                            }
                        },
                        {
                            "text": "Musimy przełamać pierwsze <w>lody</w> i zacząć ze sobą szczerze rozmawiać.",
                            "target_lang_translations": {
                                "ru": "Мы должны растопить <w>лёд</w> и начать честно разговаривать друг с другом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przełamać lody",
                        "ruszyły lody"
                    ],

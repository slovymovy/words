    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c3c2f416-a20c-3134-a2fd-22aee414874b",
                    "sense_definition": "Het aanprijzen van producten of diensten om de verkoop te bevorderen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "promotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij storen ons altijd flink aan de onderbrekingen van televisieprogramma's voor de <w>reclame</w>.",
                            "target_lang_translations": {
                                "en": "We are always quite annoyed by the interruptions of television programs for <w>commercials</w>.",
                                "ru": "Нас всегда сильно раздражают перерывы в телепрограммах на <w>рекламу</w>."
                            }
                        },
                        {
                            "text": "Niemand kent uw zaak, u mag wel wat meer <w>reclame</w> maken.",
                            "target_lang_translations": {
                                "en": "No one knows your business, you should do some more <w>advertising</w>.",
                                "ru": "Никто не знает о вашем деле, вам стоит делать больше <w>рекламы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "advertentie",
                        "publiciteit",
                        "aanprijzing"
                    ],
                    "antonyms": [
                        "antireclame"
                    ],
                    "common_phrases": [
                        "reclame maken",
                        "reclame op tv"
                    ],
                    "target_lang_definitions": {
                        "en": "The promotion of products or services to increase sales.",
                        "ru": "Продвижение товаров или услуг с целью стимулирования продаж."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "advertising",
                                "target_lang_sense_clarification": "general activity of promoting products"
                            },
                            {
                                "target_lang_word": "advertisement",
                                "target_lang_sense_clarification": "a specific notice or announcement"
                            },
                            {
                                "target_lang_word": "commercial",
                                "target_lang_sense_clarification": "specifically on radio or television"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "реклама",
                                "target_lang_sense_clarification": "продвижение товаров"
                            },
                            {
                                "target_lang_word": "объявление",
                                "target_lang_sense_clarification": "рекламное сообщение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "066e99ec-a348-3cd3-a1a3-e8a0b297afd6",
                    "sense_definition": "Een aanbieding waarbij producten tijdelijk voor een lagere prijs worden verkocht.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "promotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik koop wasmiddel alleen als het in de <w>reclame</w> is.",
                            "target_lang_translations": {
                                "en": "I only buy detergent when it is on <w>sale</w>.",
                                "ru": "Я покупаю стиральный порошок, только когда он <w>по акции</w>."
                            }
                        },
                        {
                            "text": "Zijn de bananen deze week in de <w>reclame</w>?",
                            "target_lang_translations": {
                                "en": "Are the bananas on <w>sale</w> this week?",
                                "ru": "Бананы на этой неделе <w>по акции</w>?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de reclame"
                    ],
                    "target_lang_definitions": {
                        "en": "An offer where products are temporarily sold at a lower price.",
                        "ru": "Предложение, при котором товары временно продаются по сниженной цене."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sale",
                                "target_lang_sense_clarification": "period of selling goods at reduced prices"
                            },
                            {
                                "target_lang_word": "special offer",
                                "target_lang_sense_clarification": "product sold at a discount"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "акция",
                                "target_lang_sense_clarification": "специальное предложение"
                            },
                            {
                                "target_lang_word": "скидка",
                                "target_lang_sense_clarification": "снижение цены"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a183b54-08fa-3647-9326-c13c74658532",
                    "sense_definition": "Het werven van steun of aandacht voor een maatschappelijk doel of politieke partij.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "promotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mede door de sympathieke <w>reclame</w> hebben velen hun steun aan het project toegezegd.",
                            "target_lang_translations": {
                                "en": "Partly due to the sympathetic <w>publicity</w>, many have pledged their support to the project.",
                                "ru": "Отчасти благодаря приятной <w>рекламе</w> многие пообещали поддержку проекту."
                            }
                        },
                        {
                            "text": "De politieke partij maakt veel <w>reclame</w> voor de komende verkiezingen.",
                            "target_lang_translations": {
                                "en": "The political party is doing a lot of <w>promotion</w> for the upcoming elections.",
                                "ru": "Политическая партия проводит активную <w>агитацию</w> перед предстоящими выборами."
                            }
                        }
                    ],
                    "synonyms": [
                        "werving",
                        "bekendmaking"
                    ],
                    "common_phrases": [
                        "reclame maken voor"
                    ],
                    "target_lang_definitions": {
                        "en": "Gaining support or attention for a social cause or political party.",
                        "ru": "Привлечение поддержки или внимания к общественной цели или политической партии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "publicity",
                                "target_lang_sense_clarification": "public attention or awareness"
                            },
                            {
                                "target_lang_word": "promotion",
                                "target_lang_sense_clarification": "active support or encouragement of a cause"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "агитация",
                                "target_lang_sense_clarification": "политическая"
                            },
                            {
                                "target_lang_word": "пропаганда",
                                "target_lang_sense_clarification": "распространение идей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ee948f4-3e61-3f5b-84e5-4b9d747a1d11",
                    "sense_definition": "Een officiële klacht over een geleverd product of dienst.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "complaint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf accepteerde de <w>reclame</w> over de defecte levering niet.",
                            "target_lang_translations": {
                                "en": "The company did not accept the <w>complaint</w> regarding the defective delivery.",
                                "ru": "Компания не приняла <w>рекламацию</w> по поводу бракованной поставки."
                            }
                        },
                        {
                            "text": "U dient uw <w>reclame</w> binnen acht dagen schriftelijk in te dienen.",
                            "target_lang_translations": {
                                "en": "You must submit your <w>claim</w> in writing within eight days.",
                                "ru": "Вы должны подать свою <w>претензию</w> в письменном виде в течение восьми дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "klacht",
                        "beklag",
                        "terugvordering"
                    ],
                    "target_lang_definitions": {
                        "en": "An official complaint about a delivered product or service.",
                        "ru": "Официальная жалоба на поставленный товар или услугу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "complaint",
                                "target_lang_sense_clarification": "expression of dissatisfaction"
                            },
                            {
                                "target_lang_word": "claim",
                                "target_lang_sense_clarification": "formal demand for compensation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рекламация",
                                "target_lang_sense_clarification": "официальная жалоба"
                            },
                            {
                                "target_lang_word": "претензия",
                                "target_lang_sense_clarification": "требование"
                            }
                        ]
                    }
                }
            ]
        }

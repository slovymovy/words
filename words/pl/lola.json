    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "0ba2de46-a8e7-3669-9b52-52ce3e2c95a3",
                    "sense_definition": "Zdrobnienie od imienia żeńskiego <w>Karolina</w> lub <w>Karola</w>, często funkcjonujące również jako samodzielne imię.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person_name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja ciocia <w>Lola</w> robi najlepsze ciasto z truskawkami.",
                            "target_lang_translations": {
                                "ru": "Моя тётя <w>Лола</w> печёт лучший пирог с клубникой."
                            }
                        },
                        {
                            "text": "Na przyjęciu urodzinowym mała <w>Lola</w> bawiła się z innymi dziećmi.",
                            "target_lang_translations": {
                                "ru": "На дне рождения маленькая <w>Лола</w> играла с другими детьми."
                            }
                        }
                    ],
                    "synonyms": [
                        "Lolita"
                    ],
                    "common_phrases": [
                        "ciocia Lola",
                        "mała Lola"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "diminutive form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма женского имени Каролина или Карола, часто используемая как самостоятельное имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лола",
                                "target_lang_sense_clarification": "женское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "667ca7bd-f6c2-3713-a045-85482b9cb96f",
                    "sense_definition": "Miasto w południowo-wschodniej Gwinei, będące ośrodkiem administracyjnym prefektury.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Podróżnicy zatrzymali się w mieście <w>Lola</w> na krótki odpoczynek.",
                            "target_lang_translations": {
                                "ru": "Путешественники остановились в городе <w>Лола</w> для короткого отдыха."
                            }
                        },
                        {
                            "text": "Region, w którym leży <w>Lola</w>, słynie z gęstych lasów.",
                            "target_lang_translations": {
                                "ru": "Регион, в котором находится <w>Лола</w>, славится густыми лесами."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город на юго-востоке Гвинеи, административный центр префектуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Лола",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "6fe53104-cf55-3433-aa3e-f8d76edcfa5b",
                    "sense_definition": "Постоянно и непрерывно, без колебаний (обычно о развитии, росте или движении).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уровень жизни в стране <неуклонно> повышается с каждым годом.",
                            "target_lang_translations": {
                                "en": "The standard of living in the country is rising <steadily> every year."
                            }
                        },
                        {
                            "text": "Цены на бензин продолжают <неуклонно> расти.",
                            "target_lang_translations": {
                                "en": "Gas prices continue to rise <steadily>."
                            }
                        }
                    ],
                    "synonyms": [
                        "неотступно",
                        "неотступно"
                    ],
                    "common_phrases": [
                        "неуклонно расти",
                        "неуклонно повышаться",
                        "неуклонно двигаться"
                    ],
                    "target_lang_definitions": {
                        "en": "Constantly and continuously, without wavering (usually regarding development, growth, or movement)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steadily",
                                "target_lang_sense_clarification": "regarding progress, increase, or movement"
                            },
                            {
                                "target_lang_word": "consistently",
                                "target_lang_sense_clarification": "without stopping or changing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b53cf2c1-d9b6-3592-bc3e-b9d75f16e5cb",
                    "sense_definition": "Твёрдо и строго, не отступая от намеченной цели, принципов или правил.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы должны <неуклонно> соблюдать законы и правила безопасности.",
                            "target_lang_translations": {
                                "en": "We must <strictly> observe laws and safety regulations."
                            }
                        },
                        {
                            "text": "Он <неуклонно> шёл к своей мечте, несмотря на все трудности.",
                            "target_lang_translations": {
                                "en": "He moved <unswervingly> toward his dream, despite all the difficulties."
                            }
                        }
                    ],
                    "synonyms": [
                        "неотступно"
                    ],
                    "common_phrases": [
                        "неуклонно соблюдать",
                        "неуклонно следовать",
                        "неуклонно выполнять"
                    ],
                    "target_lang_definitions": {
                        "en": "Firmly and strictly, without deviating from an intended goal, principles, or rules."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strictly",
                                "target_lang_sense_clarification": "regarding adherence to rules or obligations"
                            },
                            {
                                "target_lang_word": "unswervingly",
                                "target_lang_sense_clarification": "regarding a path or goal"
                            },
                            {
                                "target_lang_word": "steadfastly",
                                "target_lang_sense_clarification": "with firm determination"
                            }
                        ]
                    }
                }
            ]
        }
    ]

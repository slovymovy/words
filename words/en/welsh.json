                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Welsh</w> teaches history at the local high school.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Уэлш</w> преподает историю в местной средней школе."
                            }
                        },
                        {
                            "text": "I have an appointment with Dr. <w>Welsh</w> tomorrow.",
                            "target_lang_translations": {
                                "ru": "У меня завтра прием у доктора <w>Уэлша</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия английского, шотландского или ирландского происхождения."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0c05b70a-41ae-3e9a-93f4-77108abc49e8",
                    "sense_definition": "One side of a sheet of paper in a book, magazine, newspaper, or other document.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "paper_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Open your textbooks to <w>page</w> ten.",
                            "target_lang_translations": {
                                "ru": "Откройте свои учебники на десятой <w>странице</w>."
                            }
                        },
                        {
                            "text": "The article continues on the next <w>page</w>.",
                            "target_lang_translations": {
                                "ru": "Статья продолжается на следующей <w>странице</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "side"
                    ],
                    "common_phrases": [
                        "front page",
                        "turn the page",
                        "blank page",
                        "on the page"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна сторона листа бумаги в книге, журнале, газете или другом документе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страница",
                                "target_lang_sense_clarification": "сторона листа в книге или документе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68657bb2-74d9-386b-818a-701adb6ad135",

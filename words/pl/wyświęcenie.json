                    "sense_id": "74e90a93-d49f-3b37-aaff-8a38686f0eb4",
                    "sense_definition": "Uroczystość religijna polegająca na udzieleniu sakramentu kapłaństwa i włączeniu kandydata do stanu duchownego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sacralization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Władze kościelne wydały zgodę na <w>wyświęcenie</w> trzech nowych kapłanów w naszej diecezji.",
                            "target_lang_translations": {
                                "ru": "Церковные власти дали согласие на <w>рукоположение</w> трех новых священников в нашей епархии."
                            }
                        },
                        {
                            "text": "Jego <w>wyświęcenie</w> na biskupa było wielkim wydarzeniem dla lokalnej społeczności wiernych.",
                            "target_lang_translations": {
                                "ru": "Его <w>рукоположение</w> в епископы стало большим событием для местной общины верующих."
                            }
                        }
                    ],
                    "synonyms": [
                        "ordynacja",
                        "konsekracja"
                    ],
                    "antonyms": [
                        "laicyzacja",
                        "zeświecczenie"
                    ],
                    "common_phrases": [

{
    "word_family": [
        "u",
        "uw"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "46f8e077-85ae-3287-8dec-d5fb5792eed0",
                    "sense_definition": "Zelfstandige vorm van het bezittelijk voornaamwoord <w>uw</w> (beleefdheidsvorm); dat wat aan u toebehoort.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "possessive_independent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als u uw boek vergeten bent, mag u het <w>uwe</w> van mij wel lenen.",
                            "target_lang_translations": {
                                "en": "If you have forgotten your book, you may borrow <w>yours</w> from me.",
                                "ru": "Если вы забыли свою книгу, можете одолжить <w>вашу</w> у меня."
                            }
                        },
                        {
                            "text": "Dat grote huis op de hoek is het <w>uwe</w>.",
                            "target_lang_translations": {
                                "en": "That large house on the corner is <w>yours</w>.",
                                "ru": "Тот большой дом на углу — <w>ваш</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "van u"
                    ],
                    "antonyms": [
                        "de mijne",
                        "het mijne"
                    ],
                    "common_phrases": [
                        "de uwe",
                        "het uwe"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "polite"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Independent form of the possessive pronoun 'uw' (polite form); that which belongs to you.",
                        "ru": "Самостоятельная форма притяжательного местоимения \"uw\" (вежливая форма); то, что принадлежит вам."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "yours",
                                "target_lang_sense_clarification": "polite form"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ваш",
                                "target_lang_sense_clarification": "вежливая форма, самостоятельное употребление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "523220a5-df96-353b-a3ab-f4bc2d8598f0",
                    "sense_definition": "Verbogen vorm van <w>uw</w>, tegenwoordig voornamelijk gebruikt in vaste, formele titels en aanspreekvormen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "possessive_attributive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Uwe</w> Majesteit, de gasten zijn gearriveerd.",
                            "target_lang_translations": {
                                "en": "<w>Your</w> Majesty, the guests have arrived.",
                                "ru": "<w>Ваше</w> Величество, гости прибыли."
                            }
                        },
                        {
                            "text": "Ik verblijf met de meeste hoogachting, <w>Uwe</w> dienaar.",
                            "target_lang_translations": {
                                "en": "I remain with the highest regard, <w>Your</w> servant.",
                                "ru": "Остаюсь с глубочайшим почтением, <w>Ваш</w> покорный слуга."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Uwe Majesteit",
                        "Uwe Hoogheid",
                        "Uwe Edelachtbare"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "in general usage"
                        },
                        {
                            "trait_type": "form",
                            "comment": "in titles"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Inflected form of 'uw', nowadays mainly used in fixed, formal titles and forms of address.",
                        "ru": "Склоняемая форма \"uw\", используемая в настоящее время в основном в устойчивых, официальных титулах и обращениях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Your",
                                "target_lang_sense_clarification": "in formal titles"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Ваше",
                                "target_lang_sense_clarification": "в титулах (ср. р.)"
                            },
                            {
                                "target_lang_word": "Ваш",
                                "target_lang_sense_clarification": "устаревшая форма обращения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a141c1cf-ac15-3085-b525-ed7b5901979e",
                    "sense_definition": "Iemand die tot uw naaste kring, familie of groep behoort (vaak in het meervoud).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "kinship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik hoop dat het goed gaat met u en de <w>uwen</w>.",
                            "target_lang_translations": {
                                "en": "I hope you and <w>yours</w> are doing well.",
                                "ru": "Надеюсь, у вас и <w>ваших</w> всё хорошо."
                            }
                        },
                        {
                            "text": "Is deze man een van de <w>uwen</w>?",
                            "target_lang_translations": {
                                "en": "Is this man one of <w>yours</w>?",
                                "ru": "Этот человек — один из <w>ваших</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "verwanten",
                        "naasten"
                    ],
                    "common_phrases": [
                        "u en de uwen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "polite"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone belonging to your close circle, family, or group (often in plural).",
                        "ru": "Тот, кто принадлежит к вашему близкому кругу, семье или группе (часто во множественном числе)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "yours",
                                "target_lang_sense_clarification": "referring to family or group"
                            },
                            {
                                "target_lang_word": "your loved ones",
                                "target_lang_sense_clarification": "family members"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ваши",
                                "target_lang_sense_clarification": "о близких или родственниках"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "73ab6630-fe77-3021-b173-1a911d56af24",
                    "sense_definition": "Een jongensnaam.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn Duitse collega heet <w>Uwe</w>.",
                            "target_lang_translations": {
                                "en": "My German colleague is called <w>Uwe</w>.",
                                "ru": "Моего немецкого коллегу зовут <w>Уве</w>."
                            }
                        },
                        {
                            "text": "<w>Uwe</w> is gisteren niet op het werk verschenen.",
                            "target_lang_translations": {
                                "en": "<w>Uwe</w> did not show up at work yesterday.",
                                "ru": "<w>Уве</w> вчера не пришёл на работу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A male given name.",
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Uwe",
                                "target_lang_sense_clarification": "name"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Уве",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
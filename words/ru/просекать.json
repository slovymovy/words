    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17f68047-06b6-34a6-8825-d041cca122f4",
                    "sense_definition": "Понимать суть ситуации, догадываться о чём-либо или хорошо разбираться в каком-либо вопросе.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он сразу <просекает>, когда кто-то пытается его обмануть.",
                            "target_lang_translations": {
                                "en": "He immediately <catches on> when someone tries to deceive him."
                            }
                        },
                        {
                            "text": "Ты вообще <просекаешь> фишку этого нового приложения?",
                            "target_lang_translations": {
                                "en": "Do you even <get> the point of this new app?"
                            }
                        }
                    ],
                    "synonyms": [
                        "понимать",
                        "догонять",
                        "врубаться",
                        "соображать"
                    ],
                    "antonyms": [
                        "тупить"
                    ],
                    "common_phrases": [
                        "просекать фишку",
                        "просекать тему",
                        "просекать ситуацию"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To understand the essence of a situation, guess something, or be well-versed in a matter (slang/informal)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch on",
                                "target_lang_sense_clarification": "to understand a situation quickly"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "to understand an idea or concept"
                            },
                            {
                                "target_lang_word": "figure out",
                                "target_lang_sense_clarification": "to understand the nature of something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adc7bead-77aa-33b8-bca9-89ae4fe6073f",
                    "sense_definition": "Делать отверстие или разрубать что-либо насквозь сильным ударом острого предмета.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_cutting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочий аккуратно <просекал> отверстия в кожаном ремне специальным инструментом.",
                            "target_lang_translations": {
                                "en": "The worker carefully <punched> holes in the leather belt with a special tool."
                            }
                        },
                        {
                            "text": "Чтобы добыть воду зимой, рыбаки <просекали> толстый слой льда.",
                            "target_lang_translations": {
                                "en": "To get water in winter, the fishermen <cut through> the thick layer of ice."
                            }
                        }
                    ],
                    "synonyms": [
                        "пробивать",
                        "прорубать"
                    ],
                    "common_phrases": [
                        "просекать отверстие",
                        "просекать металл"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a hole or cut through something with a strong blow of a sharp object."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "punch",
                                "target_lang_sense_clarification": "to make a hole with a tool"
                            },
                            {
                                "target_lang_word": "cut through",
                                "target_lang_sense_clarification": "to penetrate a layer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75f479a8-c487-30a7-89fe-b66ea3db4c89",
                    "sense_definition": "Прокладывать путь или дорогу в лесу, вырубая деревья и кустарники.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_cutting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Геологи вынуждены были <просекать> тропу сквозь густые заросли тайги.",
                            "target_lang_translations": {
                                "en": "The geologists were forced to <cut> a trail through the thick taiga undergrowth."
                            }
                        },
                        {
                            "text": "Строители начали <просекать> широкую просеку для линии электропередач.",
                            "target_lang_translations": {
                                "en": "The builders started to <cut> a wide clearing for the power line."
                            }
                        }
                    ],
                    "synonyms": [
                        "прорубать",
                        "прокладывать"
                    ],
                    "common_phrases": [
                        "просекать просеку",
                        "просекать дорогу",
                        "просекать путь"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a path or road in a forest by cutting down trees and bushes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cut",
                                "target_lang_sense_clarification": "to make a path by cutting vegetation"
                            },
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "to remove vegetation to make a way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a288eac5-aaca-3a1e-bb91-822923c6557f",
                    "sense_definition": "Пересекать пространство или поверхность, оставляя резкий след (о свете, морщинах и т.п.).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "figurative_lines",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Яркий луч прожектора <просекал> ночную темноту, освещая вход в гавань.",
                            "target_lang_translations": {
                                "en": "A bright searchlight beam <cut through> the night darkness, illuminating the harbor entrance."
                            }
                        },
                        {
                            "text": "Глубокая морщина <просекала> его высокий лоб, придавая лицу суровое выражение.",
                            "target_lang_translations": {
                                "en": "A deep wrinkle <furrowed> his high forehead, giving his face a stern expression."
                            }
                        }
                    ],
                    "synonyms": [
                        "пересекать",
                        "прорезать",
                        "бороздить"
                    ],
                    "target_lang_definitions": {
                        "en": "To cross a space or surface leaving a sharp trace (about light, wrinkles, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cut through",
                                "target_lang_sense_clarification": "to pass through sharply (e.g., light)"
                            },
                            {
                                "target_lang_word": "furrow",
                                "target_lang_sense_clarification": "to mark with a deep line (e.g., wrinkles)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
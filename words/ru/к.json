{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "cda385e0-fc10-33a9-b748-149518938c3d",
                    "sense_definition": "Указывает на направление движения или действия в сторону кого-либо или чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы медленно шли <к> дому.",
                            "target_lang_translations": {
                                "en": "We walked slowly <towards> the house."
                            }
                        },
                        {
                            "text": "Подойди <ко> мне, пожалуйста.",
                            "target_lang_translations": {
                                "en": "Come <to> me, please."
                            }
                        }
                    ],
                    "antonyms": [
                        "от"
                    ],
                    "common_phrases": [
                        "идти к врачу",
                        "подойти к окну",
                        "ехать к морю",
                        "лицом к лицу"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the direction of movement or action towards someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "destination or recipient"
                            },
                            {
                                "target_lang_word": "towards",
                                "target_lang_sense_clarification": "direction of movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1c283e4-6c97-364d-b595-6439ac4a884e",
                    "sense_definition": "Указывает на срок или момент времени, к наступлению которого совершается или завершается действие.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я должен закончить этот отчёт <к> пятнице.",
                            "target_lang_translations": {
                                "en": "I must finish this report <by> Friday."
                            }
                        },
                        {
                            "text": "<К> обеду все гости уже собрались.",
                            "target_lang_translations": {
                                "en": "<By> lunch, all the guests had already gathered."
                            }
                        }
                    ],
                    "synonyms": [
                        "до"
                    ],
                    "common_phrases": [
                        "к утру",
                        "к вечеру",
                        "к тому времени",
                        "к началу"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a deadline or moment in time by which an action is performed or completed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "deadline or time limit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c4da40b-4934-36eb-9dbd-f3131450d737",
                    "sense_definition": "Указывает на назначение предмета или цель действия.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "purpose_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студенты усердно готовятся <к> экзаменам.",
                            "target_lang_translations": {
                                "en": "Students are preparing hard <for> exams."
                            }
                        },
                        {
                            "text": "Это единственный ключ <к> этой двери.",
                            "target_lang_translations": {
                                "en": "This is the only key <to> this door."
                            }
                        }
                    ],
                    "common_phrases": [
                        "готовиться к зиме",
                        "пояснение к тексту",
                        "ключ к успеху"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the purpose of an object or the goal of an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "preparation or purpose"
                            },
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "connection or match (e.g., key to)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d234033-b3ae-3d1b-b880-cc2af9cdad5b",
                    "sense_definition": "Указывает на лицо или предмет, на который направлено чувство, интерес или отношение.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его любовь <к> музыке была известна всем.",
                            "target_lang_translations": {
                                "en": "His love <for> music was known to everyone."
                            }
                        },
                        {
                            "text": "Нужно воспитывать уважение <к> старшим.",
                            "target_lang_translations": {
                                "en": "One must cultivate respect <towards> elders."
                            }
                        }
                    ],
                    "common_phrases": [
                        "интерес к чтению",
                        "любовь к жизни",
                        "отношение к людям",
                        "талант к рисованию"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a person or object towards which a feeling, interest, or attitude is directed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "object of feeling (e.g., love for)"
                            },
                            {
                                "target_lang_word": "towards",
                                "target_lang_sense_clarification": "direction of attitude"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3860b1ec-a0d3-3a39-ba07-ff67c3b1c2fd",
                    "sense_definition": "Указывает на приближение вплотную, прикрепление или присоединение к чему-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "direction_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Шкаф поставили <к> стене, чтобы освободить место.",
                            "target_lang_translations": {
                                "en": "The wardrobe was placed <against> the wall to free up space."
                            }
                        },
                        {
                            "text": "Испуганный ребёнок прижался <к> матери.",
                            "target_lang_translations": {
                                "en": "The frightened child pressed <against> his mother."
                            }
                        }
                    ],
                    "antonyms": [
                        "от"
                    ],
                    "common_phrases": [
                        "плечом к плечу",
                        "прижать к груди",
                        "прилипнуть к рукам"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates close proximity, attachment, or joining to something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "against",
                                "target_lang_sense_clarification": "contact or support (e.g., lean against)"
                            },
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "attachment or pressing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba14cf10-175d-368f-91ad-13651c389dcb",
                    "sense_definition": "Используется в составе вводных слов, выражающих эмоциональную оценку сообщаемого.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "idiom_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<К> счастью, дождь скоро закончился и выглянуло солнце.",
                            "target_lang_translations": {
                                "en": "<Fortunately>, the rain soon stopped and the sun came out."
                            }
                        },
                        {
                            "text": "<К> сожалению, мы не сможем прийти на ваш праздник.",
                            "target_lang_translations": {
                                "en": "<Unfortunately>, we will not be able to come to your party."
                            }
                        }
                    ],
                    "common_phrases": [
                        "к счастью",
                        "к сожалению",
                        "к несчастью",
                        "к примеру"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in introductory phrases expressing an emotional evaluation of the statement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "in phrases like 'to my surprise'"
                            },
                            {
                                "target_lang_word": "fortunately/unfortunately",
                                "target_lang_sense_clarification": "functional translation of the phrase"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e31ed01b-41b9-3ad9-8b25-ef324ea59ce5",
                    "sense_definition": "Сокращённое обозначение шахматной фигуры «конь» в записи партий.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "chess_notation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гроссмейстер записал в бланк ход <К>f3.",
                            "target_lang_translations": {
                                "en": "The grandmaster wrote the move <N>f3 on the scoresheet."
                            }
                        },
                        {
                            "text": "В этой позиции белые сыграли <К>e5 и выиграли.",
                            "target_lang_translations": {
                                "en": "In this position, White played <N>e5 and won."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "шахматы"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for the chess piece 'Knight' in game notation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "N",
                                "target_lang_sense_clarification": "chess notation symbol"
                            },
                            {
                                "target_lang_word": "Knight",
                                "target_lang_sense_clarification": "chess piece name"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
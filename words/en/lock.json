                    "common_phrases": [
                        "head lock",
                        "arm lock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Борцовский захват, при котором часть тела противника крепко сжимается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "захват",
                                "target_lang_sense_clarification": "в борьбе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2125145-12b0-34e2-a3cb-a20d0ee4c1a9",
                    "sense_definition": "A player in a rugby team who pushes in the second row of the scrum.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rugby",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>lock</w> used his height and strength to win the ball.",
                            "target_lang_translations": {
                                "ru": "<w>Замок</w> использовал свой рост и силу, чтобы выиграть мяч."
                            }
                        },

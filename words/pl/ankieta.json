                {
                    "sense_id": "0815ed74-c37f-3e52-9469-ac2ebcc97bca",
                    "sense_definition": "Formularz lub arkusz papieru zawierający zestaw pytań, na które należy udzielić odpowiedzi w celu zebrania informacji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "formularz",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę wypełnić tę krótką <w>ankietę</w> i oddać ją w recepcji.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, заполните эту короткую <w>анкету</w> и отдайте её на стойке регистрации."
                            }
                        },
                        {
                            "text": "W <w>ankiecie</w> nie było pytań o moje wykształcenie.",
                            "target_lang_translations": {
                                "ru": "В <w>анкете</w> не было вопросов о моём образовании."
                            }
                        }
                    ],
                    "synonyms": [
                        "kwestionariusz"
                    ],
                    "common_phrases": [
                        "wypełnić ankietę",
                        "ankieta personalna",
                        "ankieta online",
                        "anonimowa ankieta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бланк или лист бумаги с набором вопросов, на которые необходимо ответить для сбора информации."
                    },
                    "translations": {
                        "ru": [
                            {

                    "sense_id": "45393b9e-2c0f-31b7-bba2-1f2edc054005",
                    "sense_definition": "Bardzo szybko, natychmiast, w krótkim czasie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "speed_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieciaki pomogły i <w>migiem</w> obraliśmy wszystkie owoce.",
                            "target_lang_translations": {
                                "ru": "Дети помогли, и мы <w>мигом</w> почистили все фрукты."
                            }
                        },
                        {
                            "text": "Muszę <w>migiem</w> wyskoczyć do sklepu po drobne zakupy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>мигом</w> выскочить в магазин за мелкими покупками."
                            }
                        }
                    ],
                    "synonyms": [
                        "błyskawicznie",
                        "pędem",
                        "w mgnieniu oka",
                        "w mig",
                        "w okamgnieniu"
                    ],
                    "antonyms": [
                        "powoli",
                        "wolno"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cd2b44e5-bb99-39dd-89f8-f102b9514f3d",
                    "sense_definition": "Een klein knaagdier met een spitse snuit, grote oren en een lange, dunne staart.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kat heeft gisteren een <w>muis</w> gevangen in de tuin.",
                            "target_lang_translations": {
                                "en": "The cat caught a <w>mouse</w> in the garden yesterday.",
                                "ru": "Кошка вчера поймала <w>мышь</w> в саду."
                            }
                        },
                        {
                            "text": "Er loopt een kleine <w>muis</w> door de keuken.",
                            "target_lang_translations": {
                                "en": "There is a small <w>mouse</w> running through the kitchen.",
                                "ru": "По кухне бегает маленькая <w>мышь</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "als een muis",
                        "stil als een muis"
                    ],
                    "target_lang_definitions": {
                        "en": "A small rodent with a pointed snout, large ears, and a long, thin tail.",
                        "ru": "Маленький грызун с острой мордочкой, большими ушами и длинным тонким хвостом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mouse",
                                "target_lang_sense_clarification": "animal"
                            }
                        ],
                        "ru": [

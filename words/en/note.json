                    "target_lang_definitions": {
                        "ru": "Бумажный денежный знак."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банкнота",
                                "target_lang_sense_clarification": "денежный знак"
                            },
                            {
                                "target_lang_word": "купюра",
                                "target_lang_sense_clarification": "денежная единица"
                            }
                        ]

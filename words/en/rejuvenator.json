            "senses": [
                {
                    "sense_id": "55aa3407-06da-3729-999d-95bf783ef9db",
                    "sense_definition": "Something that restores youth, energy, or freshness to a person or thing.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "restorative_agent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many people believe that a long vacation acts as a great <w>rejuvenator</w> for the mind.",
                            "target_lang_translations": {
                                "ru": "Многие считают, что долгий отпуск служит отличным <w>средством для восстановления</w> душевных сил."
                            }
                        },
                        {
                            "text": "This expensive cream is marketed as a powerful skin <w>rejuvenator</w>.",
                            "target_lang_translations": {
                                "ru": "Этот дорогой крем рекламируется как мощное <w>омолаживающее средство</w> для кожи."
                            }
                        }
                    ],
                    "synonyms": [
                        "restorative",
                        "tonic",
                        "refresher"
                    ],
                    "common_phrases": [
                        "skin rejuvenator",
                        "natural rejuvenator",
                        "facial rejuvenator"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что возвращает молодость, энергию или свежесть человеку или предмету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "омолаживающее средство",
                                "target_lang_sense_clarification": "косметика, медицина"
                            },
                            {
                                "target_lang_word": "средство для восстановления сил",

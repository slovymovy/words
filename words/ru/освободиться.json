{
    "word_family": [
        "свобода",
        "свободный",
        "освобождение",
        "освободитель",
        "освободить"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f2de6b9d-ec9b-30ce-8395-cfc670b259a2",
                    "sense_definition": "Перестать быть занятым делами, работой или учебой; получить свободное время.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Позвони мне, когда <освободишься> после собрания.",
                            "target_lang_translations": {
                                "en": "Call me when you <are free> after the meeting."
                            }
                        },
                        {
                            "text": "Я постараюсь <освободиться> пораньше, чтобы мы успели в кино.",
                            "target_lang_translations": {
                                "en": "I will try to <get free> early so we can make it to the cinema."
                            }
                        }
                    ],
                    "antonyms": [
                        "заняться"
                    ],
                    "common_phrases": [
                        "освободиться пораньше",
                        "освободиться вечером",
                        "освободиться от дел",
                        "скоро освобожусь"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop being busy with tasks, work, or study; to get free time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be free",
                                "target_lang_sense_clarification": "state of having time"
                            },
                            {
                                "target_lang_word": "get free",
                                "target_lang_sense_clarification": "process of finishing tasks"
                            },
                            {
                                "target_lang_word": "become available",
                                "target_lang_sense_clarification": "regarding personal schedule"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34422b4e-333d-3b67-90fd-19b8a4c27c94",
                    "sense_definition": "Стать пустым, доступным для использования, никем не занятым (о месте, помещении, предмете).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "space_vacancy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В автобусе <освободилось> место у окна, и я сразу сел.",
                            "target_lang_translations": {
                                "en": "A window seat <opened up> on the bus, and I sat down immediately."
                            }
                        },
                        {
                            "text": "Этот столик в кафе должен скоро <освободиться>.",
                            "target_lang_translations": {
                                "en": "This table in the cafe should <become free> soon."
                            }
                        }
                    ],
                    "synonyms": [
                        "опустеть"
                    ],
                    "antonyms": [
                        "заполниться",
                        "заняться"
                    ],
                    "common_phrases": [
                        "место освободилось",
                        "квартира освободилась",
                        "освободиться для проезда"
                    ],
                    "target_lang_definitions": {
                        "en": "To become empty, available for use, or unoccupied (of a place, room, or object)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "become available",
                                "target_lang_sense_clarification": "of an object or space"
                            },
                            {
                                "target_lang_word": "open up",
                                "target_lang_sense_clarification": "informal, regarding a spot or vacancy"
                            },
                            {
                                "target_lang_word": "become vacant",
                                "target_lang_sense_clarification": "formal, regarding premises or seats"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23191d93-412d-3bbb-9f5a-8e913827ab38",
                    "sense_definition": "Стать свободным, выйти на свободу из заключения, плена или рабства.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "liberty_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Заключённый надеялся <освободиться> досрочно за хорошее поведение.",
                            "target_lang_translations": {
                                "en": "The prisoner hoped to <be released> early for good behavior."
                            }
                        },
                        {
                            "text": "Пленникам удалось <освободиться> и вернуться на родину.",
                            "target_lang_translations": {
                                "en": "The captives managed to <free themselves> and return to their homeland."
                            }
                        }
                    ],
                    "synonyms": [
                        "вырваться",
                        "спастись"
                    ],
                    "antonyms": [
                        "попасть в плен"
                    ],
                    "common_phrases": [
                        "освободиться из тюрьмы",
                        "освободиться из плена",
                        "досрочно освободиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To become free, to be released from imprisonment, captivity, or slavery."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be released",
                                "target_lang_sense_clarification": "from prison or confinement"
                            },
                            {
                                "target_lang_word": "break free",
                                "target_lang_sense_clarification": "escape by force or effort"
                            },
                            {
                                "target_lang_word": "free oneself",
                                "target_lang_sense_clarification": "act of gaining liberty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "975a510c-fe1a-3fce-a9df-32bf495dde96",
                    "sense_definition": "Избавиться от чего-либо мешающего, неприятного, стесняющего или обременяющего.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "liberty_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно <освободиться> от старых вещей перед переездом.",
                            "target_lang_translations": {
                                "en": "I need to <get rid> of old things before moving."
                            }
                        },
                        {
                            "text": "Она хотела <освободиться> от навязчивых мыслей и просто отдохнуть.",
                            "target_lang_translations": {
                                "en": "She wanted to <free herself> from intrusive thoughts and just rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "избавиться",
                        "отделаться"
                    ],
                    "antonyms": [
                        "обременить себя"
                    ],
                    "common_phrases": [
                        "освободиться от долгов",
                        "освободиться от ответственности",
                        "освободиться от иллюзий",
                        "освободиться от захвата"
                    ],
                    "target_lang_definitions": {
                        "en": "To get rid of something hindering, unpleasant, constraining, or burdensome."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get rid of",
                                "target_lang_sense_clarification": "remove something unwanted"
                            },
                            {
                                "target_lang_word": "free oneself",
                                "target_lang_sense_clarification": "from a burden or obligation"
                            },
                            {
                                "target_lang_word": "shake off",
                                "target_lang_sense_clarification": "metaphorical, e.g., a feeling or pursuer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b10412e-2fea-3c36-8a01-57c2e2d17965",
                    "sense_definition": "Выделиться, оказаться в свободном состоянии в результате реакции (об энергии, газе, веществе).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "scientific_release",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При сгорании топлива <освободилась> значительная тепловая энергия.",
                            "target_lang_translations": {
                                "en": "Significant thermal energy <was released> during fuel combustion."
                            }
                        },
                        {
                            "text": "В ходе химической реакции должен <освободиться> водород.",
                            "target_lang_translations": {
                                "en": "Hydrogen should <be released> during the chemical reaction."
                            }
                        }
                    ],
                    "synonyms": [
                        "выделиться"
                    ],
                    "antonyms": [
                        "поглотиться"
                    ],
                    "common_phrases": [
                        "энергия освободилась",
                        "газ освободился"
                    ],
                    "target_lang_definitions": {
                        "en": "To be emitted or released as a result of a reaction (of energy, gas, or substance)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be released",
                                "target_lang_sense_clarification": "scientific context"
                            },
                            {
                                "target_lang_word": "be emitted",
                                "target_lang_sense_clarification": "given off"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
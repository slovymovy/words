    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c044eb1b-1b7c-3283-8c72-a4a2412d73ca",
                    "sense_definition": "Качество или свойство, выгодно отличающее кого-либо или что-либо от других; перевес в чём-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Главное <w>преимущество</w> этого метода заключается в его простоте и доступности для всех.",
                            "target_lang_translations": {
                                "en": "The main <w>advantage</w> of this method lies in its simplicity and accessibility for all."
                            }
                        },
                        {
                            "text": "Наша команда быстро доказала своё игровое <w>преимущество</w> и забила первый гол.",
                            "target_lang_translations": {
                                "en": "Our team quickly proved its <w>superiority</w> in the game and scored the first goal."
                            }
                        }
                    ],
                    "synonyms": [
                        "превосходство",
                        "перевес"
                    ],
                    "antonyms": [
                        "недостаток"
                    ],
                    "common_phrases": [
                        "явное преимущество",
                        "иметь преимущество",
                        "получить преимущество",
                        "численное преимущество"
                    ],
                    "target_lang_definitions": {
                        "en": "A quality or characteristic that puts someone or something in a better position than others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "advantage",
                                "target_lang_sense_clarification": "favorable condition or circumstance"
                            },
                            {
                                "target_lang_word": "superiority",
                                "target_lang_sense_clarification": "state of being better or more powerful"
                            },
                            {
                                "target_lang_word": "edge",
                                "target_lang_sense_clarification": "slight or competitive advantage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dfb9e9e-5af8-3e1d-9f3a-dc8ec551701a",
                    "sense_definition": "Исключительное право на что-либо, привилегия или приоритет по сравнению с другими.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пассажиры с маленькими детьми имеют <w>преимущество</w> при посадке в самолёт.",
                            "target_lang_translations": {
                                "en": "Passengers with small children have <w>priority</w> when boarding the plane."
                            }
                        },
                        {
                            "text": "На этом перекрёстке пешеходы имеют безоговорочное <w>преимущество</w> перед автомобилями.",
                            "target_lang_translations": {
                                "en": "At this intersection, pedestrians have absolute <w>right of way</w> over cars."
                            }
                        }
                    ],
                    "synonyms": [
                        "привилегия",
                        "приоритет"
                    ],
                    "common_phrases": [
                        "преимущество проезда",
                        "пользоваться преимуществом",
                        "никакого преимущества"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclusive right or priority to do something before others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "priority",
                                "target_lang_sense_clarification": "right to proceed or be treated first"
                            },
                            {
                                "target_lang_word": "privilege",
                                "target_lang_sense_clarification": "special right or immunity"
                            },
                            {
                                "target_lang_word": "right of way",
                                "target_lang_sense_clarification": "legal right to proceed before other traffic"
                            }
                        ]
                    }
                }
            ]
        }

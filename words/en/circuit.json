                            {
                                "target_lang_word": "контур",
                                "target_lang_sense_clarification": "замкнутый путь тока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bedaba9-f4bb-395f-88f3-8e17c10f239f",
                    "sense_definition": "A regular series of events, competitions, or venues that people visit or perform at in a specific order.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "events",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has been playing on the professional tennis <w>circuit</w> for three years.",
                            "target_lang_translations": {
                                "ru": "Она играет в профессиональном теннисном <w>туре</w> уже три года."
                            }
                        },
                        {
                            "text": "He became famous after performing on the comedy <w>circuit</w> in London clubs.",
                            "target_lang_translations": {
                                "ru": "Он прославился, выступая с <w>гастролями</w> в комедийных клубах Лондона."
                            }
                        }
                    ],
                    "synonyms": [
                        "route",
                        "dao"
                    ],
                    "common_phrases": [
                        "tennis circuit",
                        "comedy circuit",
                        "lecture circuit",
                        "professional circuit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулярная серия мероприятий, соревнований или мест, которые люди посещают или где выступают в определенном порядке."

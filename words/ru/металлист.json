    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "23edba67-ae2c-349a-8229-b3e61d98f416",
                    "sense_definition": "Поклонник музыки в стиле метал или музыкант, исполняющий такую музыку.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "subculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На городской площади собрались <w>металлисты</w> в чёрных кожаных куртках.",
                            "target_lang_translations": {
                                "en": "<w>Metalheads</w> in black leather jackets gathered in the town square."
                            }
                        },
                        {
                            "text": "Мой сосед — убеждённый <w>металлист</w>, который играет на электрогитаре.",
                            "target_lang_translations": {
                                "en": "My neighbor is a die-hard <w>metalhead</w> who plays the electric guitar."
                            }
                        }
                    ],
                    "synonyms": [
                        "металлюга"
                    ],
                    "common_phrases": [
                        "рок-металлист",
                        "тусовка металлистов",
                        "концерт металлистов"
                    ],
                    "target_lang_definitions": {
                        "en": "A fan of metal music or a musician performing such music."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "metalhead",
                                "target_lang_sense_clarification": "fan of metal music"
                            },
                            {
                                "target_lang_word": "headbanger",
                                "target_lang_sense_clarification": "informal, heavy metal fan"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d710f0d-ab89-3f4f-a02f-2b14d2d51d97",
                    "sense_definition": "Рабочий, работающий в металлообрабатывающей промышленности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый год в сентябре завод отмечает профессиональный праздник — День <w>металлиста</w>.",
                            "target_lang_translations": {
                                "en": "Every year in September, the factory celebrates a professional holiday — <w>Metalworker</w>'s Day."
                            }
                        },
                        {
                            "text": "Опытный <w>металлист</w> быстро выточил необходимую деталь на станке.",
                            "target_lang_translations": {
                                "en": "The experienced <w>metalworker</w> quickly turned the necessary part on the lathe."
                            }
                        }
                    ],
                    "common_phrases": [
                        "день металлиста",
                        "профсоюз металлистов",
                        "рабочий-металлист"
                    ],
                    "target_lang_definitions": {
                        "en": "A worker employed in the metalworking industry."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "metalworker",
                                "target_lang_sense_clarification": "industry worker"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "97c65379-d4b8-3b2b-94bc-a44f36ea2e95",
                    "sense_definition": "Кровяное давление в сосудах; один из главных показателей работы сердца и состояния здоровья.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "medical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач посоветовал бабушке каждый день мерить <w>давление</w>.",
                            "target_lang_translations": {
                                "en": "The doctor advised grandmother to measure her <w>blood pressure</w> every day."
                            }
                        },
                        {
                            "text": "Из-за резкой смены погоды у меня упало <w>давление</w>.",
                            "target_lang_translations": {
                                "en": "My <w>blood pressure</w> dropped due to the sudden change in weather."
                            }
                        }
                    ],
                    "common_phrases": [
                        "высокое давление",
                        "низкое давление",
                        "мерить давление",
                        "артериальное давление"
                    ],
                    "target_lang_definitions": {
                        "en": "Blood pressure in the vessels; one of the main indicators of heart function and health status."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blood pressure",
                                "target_lang_sense_clarification": "medical term"
                            },
                            {
                                "target_lang_word": "pressure",
                                "target_lang_sense_clarification": "short for blood pressure"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f114fc4f-c020-38b2-ac5b-657ec19715f7",
                    "sense_definition": "Физическая величина, показывающая силу, действующую на единицу площади поверхности (в атмосфере, жидкостях, газах).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physics_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Атмосферное <w>давление</w> сегодня значительно выше нормы.",
                            "target_lang_translations": {
                                "en": "Atmospheric <w>pressure</w> is significantly higher than normal today."
                            }
                        },
                        {
                            "text": "В шинах автомобиля необходимо регулярно проверять <w>давление</w>.",
                            "target_lang_translations": {
                                "en": "It is necessary to regularly check the <w>pressure</w> in the car tires."
                            }
                        }
                    ],
                    "antonyms": [
                        "растяжение"
                    ],
                    "common_phrases": [
                        "атмосферное давление",
                        "давление воздуха",
                        "давление в шинах",
                        "высокого давления"
                    ],
                    "target_lang_definitions": {
                        "en": "A physical quantity showing the force acting per unit of surface area (in the atmosphere, liquids, gases)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pressure",
                                "target_lang_sense_clarification": "physics concept"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30b4470e-3616-366c-b78c-70a09e539cf3",
                    "sense_definition": "Принуждение, настойчивое моральное или психологическое воздействие на кого-либо с целью заставить поступить определённым образом.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Свидетель отказался давать показания под <w>давлением</w> следствия.",
                            "target_lang_translations": {
                                "en": "The witness refused to testify under <w>pressure</w> from the investigation."
                            }
                        },
                        {
                            "text": "На сотрудника оказывали психологическое <w>давление</w>, чтобы он уволился по собственному желанию.",
                            "target_lang_translations": {
                                "en": "Psychological <w>pressure</w> was exerted on the employee to make him resign voluntarily."
                            }
                        }
                    ],
                    "synonyms": [
                        "нажим",
                        "принуждение",
                        "напор"
                    ],
                    "common_phrases": [
                        "оказывать давление",
                        "под давлением",
                        "политическое давление"
                    ],
                    "target_lang_definitions": {
                        "en": "Coercion, persistent moral or psychological influence on someone to force them to act in a certain way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pressure",
                                "target_lang_sense_clarification": "psychological influence"
                            },
                            {
                                "target_lang_word": "coercion",
                                "target_lang_sense_clarification": "forcing someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "648e83c9-bd2d-3248-aa1e-63f3c71cadf5",
                    "sense_definition": "Действие по глаголу «давить»; сильное нажатие, приложение физической силы к чему-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От сильного <w>давления</w> лёд на реке начал трескаться.",
                            "target_lang_translations": {
                                "en": "The ice on the river began to crack from the strong <w>pressure</w>."
                            }
                        },
                        {
                            "text": "Не ослабляя <w>давления</w> руки, он удерживал дверь закрытой.",
                            "target_lang_translations": {
                                "en": "Without relaxing the <w>pressure</w> of his hand, he kept the door closed."
                            }
                        }
                    ],
                    "synonyms": [
                        "нажим",
                        "натиск"
                    ],
                    "common_phrases": [
                        "сила давления",
                        "ослабить давление"
                    ],
                    "target_lang_definitions": {
                        "en": "The action of pressing; strong pressing, application of physical force to something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pressure",
                                "target_lang_sense_clarification": "physical force"
                            },
                            {
                                "target_lang_word": "force",
                                "target_lang_sense_clarification": "physical strength"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adf1bfe9-ab55-3ad4-b3f3-87f2836ecd9e",
                    "sense_definition": "Воздействие внешних факторов (экономических, социальных, природных), заставляющее систему или процесс изменяться.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Экономическое <w>давление</w> конкурентов заставило компанию снизить цены на продукцию.",
                            "target_lang_translations": {
                                "en": "The economic <w>pressure</w> from competitors forced the company to lower product prices."
                            }
                        },
                        {
                            "text": "Развитие массового туризма увеличивает <w>давление</w> на хрупкую экосистему озера.",
                            "target_lang_translations": {
                                "en": "The development of mass tourism increases the <w>pressure</w> on the lake's fragile ecosystem."
                            }
                        }
                    ],
                    "common_phrases": [
                        "экономическое давление",
                        "демографическое давление"
                    ],
                    "target_lang_definitions": {
                        "en": "The influence of external factors (economic, social, natural) that forces a system or process to change."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pressure",
                                "target_lang_sense_clarification": "external burden"
                            },
                            {
                                "target_lang_word": "strain",
                                "target_lang_sense_clarification": "stress on a system"
                            }
                        ]
                    }
                }
            ]
        }
    ]

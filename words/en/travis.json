            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "24bf4321-f20b-3f59-bd06-644aa5c3539c",
                    "sense_definition": "A wooden frame used to confine a horse while a blacksmith shoes it.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "structure_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The blacksmith secured the restless horse in the <w>travis</w> before starting work.",
                            "target_lang_translations": {
                                "ru": "Кузнец закрепил беспокойную лошадь в <w>станке</w> перед началом работы."
                            }
                        },
                        {
                            "text": "Old stables sometimes still contain a wooden <w>travis</w> for handling animals.",
                            "target_lang_translations": {
                                "ru": "В старых конюшнях иногда все еще можно найти деревянный <w>станок</w> для обращения с животными."
                            }
                        }
                    ],
                    "synonyms": [
                        "traverse"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревянная рама (станок), используемая для фиксации лошади во время ковки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "станок для ковки",
                                "target_lang_sense_clarification": "устройство для фиксации животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1087ec1d-50df-34c4-987b-92f87f2a2397",
                    "sense_definition": "A screen or partition between stable stalls.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "structure_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The horses kicked at the wooden <w>travis</w> separating their stalls.",
                            "target_lang_translations": {
                                "ru": "Лошади били копытами в деревянную <w>перегородку</w>, разделяющую их стойла."
                            }
                        },
                        {
                            "text": "A high <w>travis</w> ensured the animals could not see each other while eating.",
                            "target_lang_translations": {
                                "ru": "Высокая <w>перегородка</w> гарантировала, что животные не видели друг друга во время еды."
                            }
                        }
                    ],
                    "synonyms": [
                        "partition",
                        "divider"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ширма или перегородка между стойлами в конюшне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перегородка",
                                "target_lang_sense_clarification": "в конюшне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b776241b-6362-30d9-9ea9-29e9b75bb481",
                    "sense_definition": "A beam or structural crosspiece.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "structure_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter replaced the rotten <w>travis</w> that supported the floor.",
                            "target_lang_translations": {
                                "ru": "Плотник заменил сгнившую <w>поперечную балку</w>, поддерживавшую пол."
                            }
                        },
                        {
                            "text": "A heavy <w>travis</w> ran across the ceiling of the old barn.",
                            "target_lang_translations": {
                                "ru": "Тяжелая <w>балка</w> проходила через потолок старого амбара."
                            }
                        }
                    ],
                    "synonyms": [
                        "beam",
                        "traverse"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Балка или поперечная перекладина в конструкции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поперечная балка",
                                "target_lang_sense_clarification": "строительный элемент"
                            },
                            {
                                "target_lang_word": "траверса",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d9b23976-962f-3b06-a817-f38569de2efc",
                    "sense_definition": "kobieta na stałe mieszkająca w jakimś miejscu, np. w mieście, wsi lub regionie; żeńska forma słowa <w>mieszkaniec</w>",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "resident",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każda <mieszkanka> naszej gminy otrzyma zaproszenie na festyn.",
                            "target_lang_translations": {
                                "ru": "Каждая <жительница> нашей общины получит приглашение на праздник."
                            }
                        },
                        {
                            "text": "Ministerstwo zrobi ankietę wśród <mieszkanek> Górnego Śląska.",
                            "target_lang_translations": {
                                "ru": "Министерство проведет опрос среди <жительниц> Верхней Силезии."
                            }
                        }
                    ],
                    "antonyms": [
                        "turystka"
                    ],
                    "common_phrases": [
                        "mieszkanka miasta",
                        "mieszkanka wsi",
                        "stała mieszkanka",
                        "rdzenna mieszkanka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, постоянно проживающая в каком-либо месте, например, в городе, деревне или регионе; женская форма слова \"житель\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жительница",
                                "target_lang_sense_clarification": "женщина, проживающая в определенном месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f44c69a-e3eb-3d96-b9db-ca13cfee3703",
                    "sense_definition": "forma fleksyjna (dopełniacz liczby pojedynczej oraz mianownik, biernik i wołacz liczby mnogiej) rzeczownika <w>mieszkanko</w>",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Okna tego małego <mieszkanka> wychodzą na park.",
                            "target_lang_translations": {
                                "ru": "Окна этой маленькой <квартирки> выходят в парк."
                            }
                        },
                        {
                            "text": "Deweloper buduje przytulne <mieszkanka> dla studentów.",
                            "target_lang_translations": {
                                "ru": "Застройщик строит уютные <квартирки> для студентов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "małego mieszkanka",
                        "nowe mieszkanka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "of mieszkanko"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (родительный падеж единственного числа, а также именительный, винительный и звательный падежи множественного числа) существительного \"mieszkanko\" (квартирка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квартирка",
                                "target_lang_sense_clarification": "уменьшительная форма от \"квартира\""
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "88c63a36-1cb9-3d40-a5f5-e2dd5e0dfe0b",
                    "sense_definition": "Providing physical ease, relaxation, and freedom from pain.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_ease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is the most <w>comfortable</w> bed I have ever slept in.",
                            "target_lang_translations": {
                                "ru": "Это самая <w>удобная</w> кровать, в которой я когда-либо спал."
                            }
                        },
                        {
                            "text": "She wore a pair of <w>comfortable</w> shoes for the long walk.",
                            "target_lang_translations": {
                                "ru": "Она надела пару <w>удобных</w> туфель для долгой прогулки."
                            }
                        }
                    ],
                    "synonyms": [
                        "comfy",
                        "cozy",
                        "snug",
                        "restful",
                        "cushy"
                    ],
                    "antonyms": [
                        "uncomfortable",
                        "comfortless"
                    ],
                    "common_phrases": [
                        "comfortable bed",
                        "comfortable chair",
                        "comfortable shoes",
                        "comfortable clothes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечивающий физическое удобство, расслабление и отсутствие боли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удобный",
                                "target_lang_sense_clarification": "наиболее общий перевод; о мебели, одежде, обуви"
                            },
                            {
                                "target_lang_word": "комфортный",
                                "target_lang_sense_clarification": "об условиях, обстановке или ощущениях"
                            },
                            {
                                "target_lang_word": "уютный",
                                "target_lang_sense_clarification": "располагающий к приятному отдыху"
                            },
                            {
                                "target_lang_word": "комфортабельный",
                                "target_lang_sense_clarification": "обычно о транспорте, отелях или жилье с удобствами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fe18804-1dd6-35c5-a1ee-5001ac29b84c",
                    "sense_definition": "Feeling physically relaxed and free from constraint or pain.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_ease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please sit down and make yourself <w>comfortable</w> while you wait.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, присаживайтесь и устраивайтесь <w>поудобнее</w>, пока ждете."
                            }
                        },
                        {
                            "text": "I do not feel <w>comfortable</w> sitting in this hard chair for too long.",
                            "target_lang_translations": {
                                "ru": "Мне не <w>удобно</w> долго сидеть на этом жестком стуле."
                            }
                        }
                    ],
                    "synonyms": [
                        "comfy"
                    ],
                    "antonyms": [
                        "uncomfortable"
                    ],
                    "common_phrases": [
                        "make yourself comfortable",
                        "feel comfortable",
                        "quite comfortable"
                    ],
                    "target_lang_definitions": {

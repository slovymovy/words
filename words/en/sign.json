{
    "word_family": [
        "signature",
        "signal",
        "signify",
        "significant",
        "signage",
        "design",
        "assign",
        "resign",
        "insignia",
        "countersign"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc042746-9b6e-3e8c-bd59-f3c567307424",
                    "sense_definition": "A clearly visible object, generally flat, bearing a short message in words or pictures to give information or instructions.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sign</w> in the window said the shop was closed.",
                            "target_lang_translations": {
                                "ru": "На <w>табличке</w> в окне было написано, что магазин закрыт."
                            }
                        },
                        {
                            "text": "You must stop completely when you see a red traffic <w>sign</w>.",
                            "target_lang_translations": {
                                "ru": "Вы должны полностью остановиться, когда увидите красный дорожный <w>знак</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "road sign",
                        "traffic sign",
                        "stop sign",
                        "street sign",
                        "neon sign"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо видимый объект, обычно плоский, содержащий короткое сообщение или изображение для передачи информации или инструкций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знак",
                                "target_lang_sense_clarification": "дорожный или информационный"
                            },
                            {
                                "target_lang_word": "вывеска",
                                "target_lang_sense_clarification": "на магазине или здании"
                            },
                            {
                                "target_lang_word": "табличка",
                                "target_lang_sense_clarification": "с надписью (на двери, окне)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2109b64-535b-3c1d-926c-53f9b0312bb0",
                    "sense_definition": "A visible fact, event, or action that shows that something exists, is happening, or may happen in the future.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "indication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Those dark clouds are a sure <w>sign</w> of rain coming soon.",
                            "target_lang_translations": {
                                "ru": "Эти темные тучи — верный <w>признак</w> скорого дождя."
                            }
                        },
                        {
                            "text": "Her smile was a <w>sign</w> that she was finally feeling better.",
                            "target_lang_translations": {
                                "ru": "Ее улыбка была <w>знаком</w> того, что она наконец чувствует себя лучше."
                            }
                        }
                    ],
                    "synonyms": [
                        "indication",
                        "evidence",
                        "signal",
                        "mark"
                    ],
                    "common_phrases": [
                        "sign of",
                        "good sign",
                        "bad sign",
                        "vital signs",
                        "show signs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт, событие или действие, указывающее на то, что что-то существует, происходит или может произойти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признак",
                                "target_lang_sense_clarification": "свидетельство наличия чего-либо"
                            },
                            {
                                "target_lang_word": "знак",
                                "target_lang_sense_clarification": "символ или предзнаменование"
                            },
                            {
                                "target_lang_word": "примета",
                                "target_lang_sense_clarification": "предвестие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6eeaa74b-8590-3bce-8852-f87b69a8e5fc",
                    "sense_definition": "A written mark or symbol used to represent a concept or operation, especially in mathematics or music.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher put a check <w>sign</w> next to the correct answer.",
                            "target_lang_translations": {
                                "ru": "Учитель поставил <w>знак</w> галочки рядом с правильным ответом."
                            }
                        },
                        {
                            "text": "Do you know what this mathematical <w>sign</w> means in the equation?",
                            "target_lang_translations": {
                                "ru": "Ты знаешь, что означает этот математический <w>знак</w> в уравнении?"
                            }
                        }
                    ],
                    "synonyms": [
                        "symbol",
                        "mark"
                    ],
                    "common_phrases": [
                        "dollar sign",
                        "plus sign",
                        "minus sign",
                        "equals sign"
                    ],
                    "target_lang_definitions": {
                        "ru": "Письменный символ, используемый для обозначения понятия или операции, особенно в математике или музыке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знак",
                                "target_lang_sense_clarification": "математический или графический символ"
                            },
                            {
                                "target_lang_word": "символ",
                                "target_lang_sense_clarification": "условное обозначение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2276d70c-da6c-3151-abf9-c70c7288709c",
                    "sense_definition": "A specific gesture or motion made with the hands or body to communicate information or instructions.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gesture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He gave a thumbs-up <w>sign</w> to show he was ready to go.",
                            "target_lang_translations": {
                                "ru": "Он показал <w>знак</w> большим пальцем, чтобы показать, что готов идти."
                            }
                        },
                        {
                            "text": "The police officer made a <w>sign</w> for the car to stop immediately.",
                            "target_lang_translations": {
                                "ru": "Полицейский сделал <w>знак</w> машине немедленно остановиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "signal",
                        "indication"
                    ],
                    "common_phrases": [
                        "give a sign",
                        "make a sign",
                        "hand sign"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жест или движение рукой/телом для передачи информации или команд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знак",
                                "target_lang_sense_clarification": "сигнал рукой"
                            },
                            {
                                "target_lang_word": "жест",
                                "target_lang_sense_clarification": "движение тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "685c1e69-6177-380d-9461-13264fc8f7fa",
                    "sense_definition": "One of the twelve equal divisions of the zodiac, represented by a symbol and associated with a specific period of the year.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "astrology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do you know which star <w>sign</w> you were born under?",
                            "target_lang_translations": {
                                "ru": "Ты знаешь, под каким <w>знаком</w> зодиака ты родился?"
                            }
                        },
                        {
                            "text": "My <w>sign</w> is Taurus, so I am supposedly very stubborn.",
                            "target_lang_translations": {
                                "ru": "Мой <w>знак</w> — Телец, поэтому я, предположительно, очень упрямый."
                            }
                        }
                    ],
                    "common_phrases": [
                        "star sign",
                        "zodiac sign"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из двенадцати секторов зодиака, связанный с определенным периодом года."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знак",
                                "target_lang_sense_clarification": "зодиака"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c52f74aa-5d55-33da-bf23-3fe126288ca0",
                    "sense_definition": "Sign language used by deaf people to communicate.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "language",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sorry, I don't know <w>sign</w> very well, so I cannot understand you.",
                            "target_lang_translations": {
                                "ru": "Извините, я плохо знаю <w>язык жестов</w>, поэтому не могу вас понять."
                            }
                        },
                        {
                            "text": "She is learning <w>sign</w> so she can talk to her deaf cousin.",
                            "target_lang_translations": {
                                "ru": "Она учит <w>язык жестов</w>, чтобы разговаривать со своим глухим двоюродным братом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "speak sign",
                        "learn sign"
                    ],
                    "target_lang_definitions": {
                        "ru": "Язык жестов, используемый глухими людьми для общения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язык жестов",
                                "target_lang_sense_clarification": "система общения"
                            },
                            {
                                "target_lang_word": "жестовый язык",
                                "target_lang_sense_clarification": "лингвистический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "22a53be7-4be1-3493-a1f2-84b1312caa2c",
                    "sense_definition": "To write one's name on a document to authorize, validate, or acknowledge it.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "signature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>sign</w> your name at the bottom of the page.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>подпишите</w> свое имя внизу страницы."
                            }
                        },
                        {
                            "text": "I need to <w>sign</w> for the package when it arrives at the door.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>расписаться</w> за посылку, когда ее доставят к двери."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sign a contract",
                        "sign a document",
                        "sign here",
                        "sign in",
                        "sign out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Написать свое имя на документе для его подтверждения или согласия с ним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписывать",
                                "target_lang_sense_clarification": "ставить подпись (несов. вид)"
                            },
                            {
                                "target_lang_word": "подписать",
                                "target_lang_sense_clarification": "поставить подпись (сов. вид)"
                            },
                            {
                                "target_lang_word": "расписываться",
                                "target_lang_sense_clarification": "ставить роспись"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b55f95f-0588-3345-89f2-5b5f08792d5a",
                    "sense_definition": "To employ a person, especially a sports player or musician, by obtaining their signature on a contract.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "signature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The football club wants to <w>sign</w> a new striker for the next season.",
                            "target_lang_translations": {
                                "ru": "Футбольный клуб хочет <w>подписать</w> нового нападающего на следующий сезон."
                            }
                        },
                        {
                            "text": "She managed to <w>sign</w> with a major record label after her performance.",
                            "target_lang_translations": {
                                "ru": "Ей удалось <w>подписать контракт</w> с крупным звукозаписывающим лейблом после выступления."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sign a player",
                        "sign a deal",
                        "sign with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанять человека (обычно спортсмена или музыканта), заключив с ним контракт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписать",
                                "target_lang_sense_clarification": "контракт с игроком"
                            },
                            {
                                "target_lang_word": "заключить контракт",
                                "target_lang_sense_clarification": "оформить договор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c74da4d-18ff-3d94-a063-f87b7c91db8c",
                    "sense_definition": "To communicate or indicate something using a gesture or motion.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gesture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>signed</w> to the waiter to bring the bill to the table.",
                            "target_lang_translations": {
                                "ru": "Он <w>сделал знак</w> официанту принести счет к столику."
                            }
                        },
                        {
                            "text": "The director <w>signed</w> for the actors to begin the scene.",
                            "target_lang_translations": {
                                "ru": "Режиссер <w>дал знак</w> актерам начинать сцену."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sign to someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщить или указать на что-то с помощью жеста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать знак",
                                "target_lang_sense_clarification": "сигнализировать жестом"
                            },
                            {
                                "target_lang_word": "подавать знак",
                                "target_lang_sense_clarification": "давать сигнал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6bbd9e3e-8b39-3744-b362-38479a920e9c",
                    "sense_definition": "To communicate using sign language.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "gesture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My cousin is deaf, so we <w>sign</w> to communicate with each other.",
                            "target_lang_translations": {
                                "ru": "Мой двоюродный брат глухой, поэтому мы <w>объясняемся жестами</w>, чтобы общаться друг с другом."
                            }
                        },
                        {
                            "text": "She learned to <w>sign</w> fluently in just a few years.",
                            "target_lang_translations": {
                                "ru": "Она научилась бегло <w>говорить на языке жестов</w> всего за несколько лет."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Общаться, используя язык жестов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объясняться жестами",
                                "target_lang_sense_clarification": "использовать жесты"
                            },
                            {
                                "target_lang_word": "говорить на языке жестов",
                                "target_lang_sense_clarification": "владеть жестовым языком"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
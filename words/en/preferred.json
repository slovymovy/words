                            {
                                "target_lang_word": "предпочитаемый",
                                "target_lang_sense_clarification": "пользующийся предпочтением"
                            },
                            {
                                "target_lang_word": "предпочтительный",
                                "target_lang_sense_clarification": "наилучший, желательный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "62021653-12cb-3842-9cd7-66c933d450ad",

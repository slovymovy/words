{
    "word_family": [
        "impeach",
        "impeachment",
        "impeachable",
        "unimpeachably"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7bdd7efb-7298-3b90-a6b6-1828994910f6",
                    "sense_definition": "Impossible to criticize or blame because of being totally honest or correct.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The candidate was chosen for her <w>unimpeachable</w> moral character.",
                            "target_lang_translations": {
                                "ru": "Кандидата выбрали за её <w>безупречный</w> моральный облик."
                            }
                        },
                        {
                            "text": "He is a gentleman of <w>unimpeachable</w> integrity who has served the public for decades.",
                            "target_lang_translations": {
                                "ru": "Он джентльмен <w>безукоризненной</w> порядочности, который десятилетиями служил обществу."
                            }
                        }
                    ],
                    "synonyms": [
                        "blameless",
                        "irreproachable",
                        "impeccable"
                    ],
                    "antonyms": [
                        "blameworthy",
                        "corrupt",
                        "questionable"
                    ],
                    "common_phrases": [
                        "unimpeachable integrity",
                        "unimpeachable character",
                        "unimpeachable reputation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не подлежащий критике или порицанию благодаря исключительной честности или правильности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безупречный",
                                "target_lang_sense_clarification": "о репутации или характере"
                            },
                            {
                                "target_lang_word": "безукоризненный",
                                "target_lang_sense_clarification": "не имеющий недостатков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49c22dc2-7659-3b2b-a1e4-3a69f10f5d5a",
                    "sense_definition": "Impossible to doubt or question; completely reliable and certain.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "evidence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police had to release the suspect because he provided an <w>unimpeachable</w> alibi.",
                            "target_lang_translations": {
                                "ru": "Полиции пришлось отпустить подозреваемого, так как он предоставил <w>неопровержимое</w> алиби."
                            }
                        },
                        {
                            "text": "We received this information from an <w>unimpeachable</w> source within the administration.",
                            "target_lang_translations": {
                                "ru": "Мы получили эту информацию из <w>достоверного</w> источника в администрации."
                            }
                        }
                    ],
                    "synonyms": [
                        "unquestionable",
                        "indisputable",
                        "undeniable"
                    ],
                    "antonyms": [
                        "dubious",
                        "unreliable",
                        "questionable"
                    ],
                    "common_phrases": [
                        "unimpeachable source",
                        "unimpeachable alibi",
                        "unimpeachable evidence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не вызывающий сомнений; полностью надежный и достоверный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неопровержимый",
                                "target_lang_sense_clarification": "об алиби или доказательствах"
                            },
                            {
                                "target_lang_word": "достоверный",
                                "target_lang_sense_clarification": "об источнике информации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22d06b5f-59f5-359a-b46c-35b23d617aba",
                    "sense_definition": "Een plotselinge, gewelddadige aanval om iets te stelen, vaak op een winkel, bank of persoon.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "crime_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie is nog steeds op zoek naar getuigen van de gewapende <w>overval</w>.",
                            "target_lang_translations": {
                                "en": "The police are still looking for witnesses to the armed <w>robbery</w>.",
                                "ru": "Полиция всё ещё ищет свидетелей вооруженного <w>ограбления</w>."
                            }
                        },
                        {
                            "text": "Tijdens de <w>overval</w> op de supermarkt werd er veel contant geld gestolen.",
                            "target_lang_translations": {
                                "en": "During the <w>robbery</w> of the supermarket, a lot of cash was stolen.",
                                "ru": "Во время <w>налёта</w> на супермаркет было украдено много наличных денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "omval",
                        "omval"
                    ],
                    "common_phrases": [
                        "gewapende overval",
                        "overval plegen",
                        "slachtoffer van een overval"
                    ],
                    "target_lang_definitions": {
                        "en": "A sudden, violent attack to steal something, often on a shop, bank, or person.",
                        "ru": "Внезапное насильственное нападение с целью кражи, часто на магазин, банк или человека."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "robbery",

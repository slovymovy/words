    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e4a0be31-4007-3db1-97a8-dd3321d512c9",
                    "sense_definition": "Dokonać wpłaty pieniędzy w zamian za towar, usługę lub w celu uregulowania należności.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "financial_transaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę pamiętać, aby jutro rano <opłacić> rachunek za prąd.",
                            "target_lang_translations": {
                                "ru": "Я должен помнить, что завтра утром нужно <оплатить> счет за электричество."
                            }
                        },
                        {
                            "text": "Czy <opłaciłeś> już czesne za nadchodzący semestr studiów?",
                            "target_lang_translations": {
                                "ru": "Ты уже <оплатил> обучение за предстоящий семестр?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zapłacić",
                        "zapłacić"
                    ],
                    "common_phrases": [
                        "opłacić rachunek",
                        "opłacić czynsz",
                        "opłacić składkę",
                        "opłacić fakturę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внести деньги в обмен на товар, услугу или для погашения долга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оплатить",
                                "target_lang_sense_clarification": "внести плату за что-либо (счет, проезд, услуги)"
                            },
                            {
                                "target_lang_word": "заплатить",
                                "target_lang_sense_clarification": "отдать деньги за что-либо (обычно с предлогом «за»)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1af8e0b5-8dba-3321-b440-080e7071c4f6",
                    "sense_definition": "Przynieść zysk lub korzyść; być wartym wysiłku (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "benefit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta inwestycja na pewno nam się <opłaci> w dłuższej perspektywie.",
                            "target_lang_translations": {
                                "ru": "Эта инвестиция нам точно <окупится> в долгосрочной перспективе."
                            }
                        },
                        {
                            "text": "Naprawa tego starego samochodu po prostu się nie <opłaci>.",
                            "target_lang_translations": {
                                "ru": "Ремонт этой старой машины просто не <окупится>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapłacić"
                    ],
                    "common_phrases": [
                        "to się opłaci",
                        "nie opłaci się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принести прибыль или пользу; стоить затраченных усилий (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окупиться",
                                "target_lang_sense_clarification": "покрыть расходы, принести выгоду"
                            },
                            {
                                "target_lang_word": "стоить",
                                "target_lang_sense_clarification": "быть достойным усилий (в значении «стоит того»)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55648772-440c-3a7b-a663-2ff6a9cc9010",
                    "sense_definition": "Ponieść dotkliwe koszty, straty lub ofiary w zamian za osiągnięcie czegoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze <opłacili> wolność swojego kraju własną krwią.",
                            "target_lang_translations": {
                                "ru": "Солдаты <оплатили> свободу своей страны собственной кровью."
                            }
                        },
                        {
                            "text": "Swój wielki sukces zawodowy <opłaciła> niestety rozpadem małżeństwa.",
                            "target_lang_translations": {
                                "ru": "Свой большой профессиональный успех она, к сожалению, <оплатила> распадом брака."
                            }
                        }
                    ],
                    "common_phrases": [
                        "opłacić życiem",
                        "opłacić krwią",
                        "drogo opłacić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понести тяжелые расходы, потери или жертвы ради достижения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оплатить",
                                "target_lang_sense_clarification": "добиться чего-либо ценой жертв (например, кровью, жизнью)"
                            },
                            {
                                "target_lang_word": "поплатиться",
                                "target_lang_sense_clarification": "понести наказание или ущерб за свои действия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
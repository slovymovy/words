    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a8e8ad4c-3dd2-3cba-9df8-a634e6dc537e",
                    "sense_definition": "Pozbawić życia, spowodować śmierć (słowo o zabarwieniu potocznym, często używane w odniesieniu do zwierząt, owadów lub w kontekście mniej oficjalnym).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "killing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę w końcu <ukatrupić> tego komara, który lata mi nad uchem.",
                            "target_lang_translations": {
                                "ru": "Я должен наконец <прихлопнуть> этого комара, который летает у меня над ухом."
                            }
                        },
                        {
                            "text": "Bohater filmu sensacyjnego musiał <ukatrupić> wielu wrogów, aby przeżyć.",
                            "target_lang_translations": {
                                "ru": "Герою боевика пришлось <прикончить> множество врагов, чтобы выжить."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabić",
                        "zamordować",
                        "ubić",
                        "zatłuc",
                        "zakatrupić"
                    ],
                    "antonyms": [
                        "ocalić",
                        "oszczędzić"
                    ],
                    "common_phrases": [
                        "ukatrupić pająka",
                        "ukatrupić muchę",
                        "chcieć kogoś ukatrupić"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej, rzadziej w oficjalnych tekstach"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишить жизни, убить (разговорное выражение, часто применяется к животным, насекомым или в неофициальном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикончить",
                                "target_lang_sense_clarification": "разговорное, убить или добить кого-то"
                            },
                            {
                                "target_lang_word": "укокошить",
                                "target_lang_sense_clarification": "просторечное, убить"
                            },
                            {
                                "target_lang_word": "прихлопнуть",
                                "target_lang_sense_clarification": "убить ударом (обычно о насекомых)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79ad6bc2-5998-3dc8-856d-4bd7a9dd0ce9",
                    "sense_definition": "Doprowadzić do zniszczenia lub trwałego uszkodzenia przedmiotu, urządzenia lub mechanizmu.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez przypadek udało mi się <ukatrupić> telefon, wrzucając go do wody.",
                            "target_lang_translations": {
                                "ru": "Мне случайно удалось <угробить> телефон, уронив его в воду."
                            }
                        },
                        {
                            "text": "Nagły skok napięcia może trwale <ukatrupić> twój komputer.",
                            "target_lang_translations": {
                                "ru": "Внезапный скачок напряжения может окончательно <убить> твой компьютер."
                            }
                        }
                    ],
                    "synonyms": [
                        "zepsuć",
                        "zniszczyć"
                    ],
                    "antonyms": [
                        "naprawić"
                    ],
                    "common_phrases": [
                        "ukatrupić silnik",
                        "ukatrupić system"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "użycie przenośne w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сломать, испортить или привести в негодность предмет, устройство или механизм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угробить",
                                "target_lang_sense_clarification": "разговорное, испортить вещь"
                            },
                            {
                                "target_lang_word": "убить",
                                "target_lang_sense_clarification": "сленг, сломать технику"

                {
                    "sense_id": "af187a3d-749c-31de-86c8-835cb4e6dd14",
                    "sense_definition": "Sprawić, że coś stanie się zrozumiałe dla kogoś innego; przekazać informacje w sposób jasny i klarowny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tłumaczenie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel cierpliwie <wyjaśnił> uczniom trudne zadanie z matematyki.",
                            "target_lang_translations": {
                                "ru": "Учитель терпеливо <объяснил> ученикам трудную задачу по математике."
                            }
                        },
                        {
                            "text": "Czy możesz mi <wyjaśnić>, jak działa to urządzenie?",
                            "target_lang_translations": {
                                "ru": "Ты можешь мне <объяснить>, как работает это устройство?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wytłumaczyć",
                        "objaśnić",
                        "wyświetlić"
                    ],
                    "common_phrases": [
                        "wyjaśnić sprawę",
                        "wyjaśnić sytuację",
                        "wyjaśnić znaczenie",
                        "wyjaśnić problem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-то понятным для кого-то другого; передать информацию ясно и четко."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объяснить",
                                "target_lang_sense_clarification": "сделать понятным"
                            },
                            {
                                "target_lang_word": "разъяснить",
                                "target_lang_sense_clarification": "внести ясность, растолковать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51c67279-70ee-3c24-b86f-3b20c1cfe076",
                    "sense_definition": "Stać się zrozumiałym, oczywistym lub rozwiązanym (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stawanie_się_jasnym",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że to nieporozumienie wkrótce się <wyjaśni>.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, что это недоразумение вскоре <разрешится>."
                            }
                        },
                        {
                            "text": "Wszystko się <wyjaśniło>, gdy znaleźliśmy zgubione dokumenty.",
                            "target_lang_translations": {
                                "ru": "Всё <выяснилось>, когда мы нашли потерянные документы."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozjaśnić się",
                        "wyświetlić się"
                    ],
                    "common_phrases": [
                        "wszystko się wyjaśniło",
                        "sytuacja się wyjaśniła",
                        "sprawa się wyjaśniła"
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "78745d7f-82d1-37f5-aab5-28f410fb24dd",
                    "sense_definition": "Verklaren of instemmen dat iets waar, juist of vaststaand is",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan dat gerucht op dit moment niet <w>bevestigen</w>.",
                            "target_lang_translations": {
                                "en": "I cannot <w>confirm</w> that rumor at this moment.",
                                "ru": "Я не могу <w>подтвердить</w> этот слух в данный момент."
                            }
                        },
                        {
                            "text": "Kun je onze afspraak voor morgen even <w>bevestigen</w>?",
                            "target_lang_translations": {
                                "en": "Can you just <w>confirm</w> our appointment for tomorrow?",
                                "ru": "Ты можешь <w>подтвердить</w> нашу встречу на завтра?"
                            }
                        },
                        {
                            "text": "De politieagent kon het verhaal van mijn zoontje <w>bevestigen</w>.",
                            "target_lang_translations": {
                                "en": "The police officer could <w>confirm</w> my son's story.",
                                "ru": "Полицейский смог <w>подтвердить</w> рассказ моего сына."
                            }
                        }
                    ],
                    "synonyms": [
                        "beamen",
                        "verifiëren",
                        "affirmeren"
                    ],
                    "antonyms": [
                        "ontkennen",
                        "verwerpen"
                    ],
                    "common_phrases": [
                        "een afspraak bevestigen",
                        "een gerucht bevestigen",
                        "de ontvangst bevestigen",
                        "de regel bevestigen"
                    ],
                    "target_lang_definitions": {
                        "en": "To declare or agree that something is true, correct, or established",
                        "ru": "Заявлять или соглашаться с тем, что что-то является истинным, правильным или установленным"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "confirm",
                                "target_lang_sense_clarification": "to state that something is true or correct"
                            },
                            {
                                "target_lang_word": "affirm",
                                "target_lang_sense_clarification": "to state clearly or publicly that something is true"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подтверждать",
                                "target_lang_sense_clarification": "свидетельствовать о верности чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "197b0673-913e-3348-8907-4e58af917ec3",
                    "sense_definition": "Iets vastmaken aan iets anders zodat het blijft zitten",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt het apparaat eenvoudig aan de muur <w>bevestigen</w>.",
                            "target_lang_translations": {
                                "en": "You can easily <w>attach</w> the device to the wall.",
                                "ru": "Вы можете легко <w>прикрепить</w> устройство к стене."
                            }
                        },
                        {
                            "text": "Het uithangbord werd met een metalen beugel aan de gevel <w>bevestigd</w>.",
                            "target_lang_translations": {
                                "en": "The sign was <w>fastened</w> to the facade with a metal bracket.",
                                "ru": "Вывеска была <w>прикреплена</w> к фасаду с помощью металлической скобы."
                            }
                        }
                    ],
                    "synonyms": [
                        "vastmaken"
                    ],
                    "antonyms": [
                        "losmaken"
                    ],
                    "common_phrases": [
                        "aan de muur bevestigen",
                        "stevig bevestigen"
                    ],
                    "target_lang_definitions": {
                        "en": "To fasten something to something else so it stays in place",
                        "ru": "Прикреплять что-либо к чему-либо другому так, чтобы оно держалось"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attach",
                                "target_lang_sense_clarification": "to join one thing to another"
                            },
                            {
                                "target_lang_word": "fasten",
                                "target_lang_sense_clarification": "to close or join securely"
                            },
                            {
                                "target_lang_word": "fix",
                                "target_lang_sense_clarification": "to put firmly in a specific place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прикреплять",

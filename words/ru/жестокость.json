    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fe47dcfe-6b13-3d3f-8bf5-9bc35f3f88ea",
                    "sense_definition": "Свойство личности или поведения, заключающееся в грубости, отсутствии жалости и сострадания к другим.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <w>жестокость</w> по отношению к животным возмутила всех соседей.",
                            "target_lang_translations": {
                                "en": "His <w>cruelty</w> towards animals outraged all the neighbors."
                            }
                        },
                        {
                            "text": "В этом романе автор показывает бессмысленную <w>жестокость</w> войны.",
                            "target_lang_translations": {
                                "en": "In this novel, the author shows the senseless <w>brutality</w> of war."
                            }
                        }
                    ],
                    "synonyms": [
                        "безжалостность",
                        "беспощадность",
                        "бессердечие",
                        "бесчеловечность",
                        "свире́пость"
                    ],
                    "antonyms": [
                        "милосердие"
                    ],
                    "common_phrases": [
                        "проявить жестокость",
                        "неоправданная жестокость",
                        "с особой жестокостью",
                        "поражать жестокостью"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of being cruel or brutal; a lack of pity or compassion for others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cruelty",
                                "target_lang_sense_clarification": "general term for indifference to suffering"
                            },
                            {
                                "target_lang_word": "brutality",
                                "target_lang_sense_clarification": "implies violence or savage roughness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c718ce6f-696e-3634-be31-28dbbf9d6bdc",
                    "sense_definition": "Крайне грубый, бесчеловечный поступок или действие, причиняющее боль и страдания.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мирные жители часто страдали от <w>жестокостей</w> захватчиков.",
                            "target_lang_translations": {
                                "en": "Civilians often suffered from the <w>atrocities</w> of the invaders."
                            }
                        },
                        {
                            "text": "Судья перечислил все <w>жестокости</w>, которые совершил этот преступник.",
                            "target_lang_translations": {
                                "en": "The judge listed all the <w>cruelties</w> committed by this criminal."
                            }
                        }
                    ],
                    "synonyms": [
                        "зве́рство",
                        "изуверство"
                    ],
                    "antonyms": [
                        "благодеяние"
                    ],
                    "common_phrases": [
                        "совершать жестокости",
                        "ужасы и жестокости",
                        "терпеть жестокости"
                    ],
                    "target_lang_definitions": {
                        "en": "An extremely cruel, violent, or inhumane act causing pain and suffering."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "atrocity",
                                "target_lang_sense_clarification": "an extremely wicked or cruel act, often in war"
                            },
                            {
                                "target_lang_word": "brutality",
                                "target_lang_sense_clarification": "a savage or violent act"
                            },
                            {
                                "target_lang_word": "cruelty",
                                "target_lang_sense_clarification": "a cruel deed or action"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "wchodzić",
        "wejście",
        "wchód"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "06831600-1d1e-3fed-a6cc-10c8e8857351",
                    "sense_definition": "Wstępowanie do wnętrza pomieszczenia, budynku lub na określony teren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciche <w>wchodzenie</w> do sali wykładowej nie zakłóciło zajęć.",
                            "target_lang_translations": {
                                "ru": "Тихий <w>вход</w> в лекционный зал не помешал занятиям."
                            }
                        },
                        {
                            "text": "Zabronione jest <w>wchodzenie</w> do tego budynku osobom nieupoważnionym.",
                            "target_lang_translations": {
                                "ru": "Посторонним <w>вход</w> в это здание воспрещен."
                            }
                        }
                    ],
                    "synonyms": [
                        "wkraczanie",
                        "wstępowanie"
                    ],
                    "antonyms": [
                        "wychodzenie",
                        "opuszczanie"
                    ],
                    "common_phrases": [
                        "wchodzenie do domu",
                        "wchodzenie do środka",
                        "zakaz wchodzenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вход внутрь помещения, здания или на определенную территорию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вход",
                                "target_lang_sense_clarification": "действие или возможность войти"
                            },
                            {
                                "target_lang_word": "вхожде́ние",
                                "target_lang_sense_clarification": "процесс проникновения внутрь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f723c32-4708-35e4-9fb3-0859974b716d",
                    "sense_definition": "Przemieszczanie się w górę, na przykład po schodach lub na szczyt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codzienne <w>wchodzenie</w> po schodach bardzo poprawia kondycję fizyczną.",
                            "target_lang_translations": {
                                "ru": "Ежедневный <w>подъём</w> по лестнице очень улучшает физическую форму."
                            }
                        },
                        {
                            "text": "<w>Wchodzenie</w> na wysoką górę zajęło turystom kilka godzin.",
                            "target_lang_translations": {
                                "ru": "<w>Восхождение</w> на высокую гору заняло у туристов несколько часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "wspinanie się"
                    ],
                    "antonyms": [
                        "schodzenie"
                    ],
                    "common_phrases": [
                        "wchodzenie po schodach",
                        "wchodzenie na górę",
                        "wchodzenie na szczyt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещение вверх, например, по лестнице или на вершину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подъём",
                                "target_lang_sense_clarification": "движение вверх (по лестнице)"
                            },
                            {
                                "target_lang_word": "восхожде́ние",
                                "target_lang_sense_clarification": "на гору или вершину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b8b672e-b3c1-30c2-81af-443a0cfe5d6f",
                    "sense_definition": "Stawianie stopy na czymś, wchodzenie na powierzchnię jakiegoś przedmiotu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ostrożne <w>wchodzenie</w> na drabinę jest kluczowe dla bezpieczeństwa pracy.",
                            "target_lang_translations": {
                                "ru": "Осторожный <w>подъём</w> на лестницу является ключевым для безопасности труда."
                            }
                        },
                        {
                            "text": "Dzieci ćwiczyły <w>wchodzenie</w> na niewysoki murek w parku.",
                            "target_lang_translations": {
                                "ru": "Дети тренировались во <w>влезании</w> на невысокую стенку в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "następowanie"
                    ],
                    "antonyms": [
                        "schodzenie"
                    ],
                    "common_phrases": [
                        "wchodzenie na drabinę",
                        "wchodzenie na krzesło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постановка ноги на что-либо, взбирание на поверхность предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подъём",
                                "target_lang_sense_clarification": "на возвышение или предмет"
                            },
                            {
                                "target_lang_word": "влезание",
                                "target_lang_sense_clarification": "взбирание на объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b284f0b4-af09-3c09-87e0-6a381d1a8aca",
                    "sense_definition": "Stawanie się członkiem jakiejś organizacji, grupy lub zespołu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_joining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wchodzenie</w> w skład zarządu wiąże się z dużą odpowiedzialnością.",
                            "target_lang_translations": {
                                "ru": "<w>Вхождение</w> в состав правления связано с большой ответственностью."
                            }
                        },
                        {
                            "text": "Proces ten obejmuje <w>wchodzenie</w> nowych firm na rynek zagraniczny.",
                            "target_lang_translations": {
                                "ru": "Этот процесс включает <w>выход</w> новых фирм на зарубежный рынок."
                            }
                        }
                    ],
                    "synonyms": [
                        "dołączanie",
                        "przystępowanie"
                    ],
                    "antonyms": [
                        "występowanie",
                        "odchodzenie"
                    ],
                    "common_phrases": [
                        "wchodzenie w skład",
                        "wchodzenie na rynek",
                        "wchodzenie w życie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становление членом какой-либо организации, группы или команды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вхожде́ние",
                                "target_lang_sense_clarification": "в состав группы или структуры"
                            },
                            {
                                "target_lang_word": "вступле́ние",
                                "target_lang_sense_clarification": "присоединение к организации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da878f96-fbc1-3f2f-9e3c-f8fd5f24f261",
                    "sense_definition": "Rozpoczynanie udziału w grze lub wejście na boisko.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trener zadecydował o zmianie i nastąpiło <w>wchodzenie</w> rezerwowego zawodnika.",
                            "target_lang_translations": {
                                "ru": "Тренер принял решение о замене, и произошел <w>выход</w> запасного игрока."
                            }
                        },
                        {
                            "text": "<w>Wchodzenie</w> do gry w połowie meczu jest trudne dla każdego piłkarza.",
                            "target_lang_translations": {
                                "ru": "<w>Вход</w> в игру в середине матча труден для каждого футболиста."
                            }
                        }
                    ],
                    "antonyms": [
                        "schodzenie"
                    ],
                    "common_phrases": [
                        "wchodzenie do gry",
                        "wchodzenie na boisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начало участия в игре или выход на поле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы́ход",
                                "target_lang_sense_clarification": "появление на поле/сцене"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
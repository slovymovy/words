{
    "word_family": [
        "costly",
        "costless",
        "costliness",
        "overcost",
        "undercost"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "24ad0534-9ef3-3b85-baf5-e7d03f137a1b",
                    "sense_definition": "To require the payment of a specified amount of money or other resources to be acquired or done.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new computer will <w>cost</w> at least a thousand dollars.",
                            "target_lang_translations": {
                                "ru": "Новый компьютер будет <w>стоить</w> не меньше тысячи долларов."
                            }
                        },
                        {
                            "text": "It did not <w>cost</w> much to repair the old bicycle.",
                            "target_lang_translations": {
                                "ru": "Ремонт старого велосипеда не <w>стоил</w> дорого."
                            }
                        }
                    ],
                    "synonyms": [
                        "sell for",
                        "go for"
                    ],
                    "common_phrases": [
                        "cost a fortune",
                        "cost money",
                        "cost an arm and a leg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требовать уплаты определенной суммы денег или других ресурсов для приобретения или выполнения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сто́ить",
                                "target_lang_sense_clarification": "иметь цену"
                            },
                            {
                                "target_lang_word": "обходи́ться",
                                "target_lang_sense_clarification": "стоить определенную сумму"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1847e61a-edb6-389c-8e74-a9212a9738de",
                    "sense_definition": "To cause someone to lose something valuable or to suffer an unpleasant consequence.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "loss_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His careless mistake <w>cost</w> the team the championship game.",
                            "target_lang_translations": {
                                "ru": "Его небрежная ошибка <w>стоила</w> команде победы в чемпионате."
                            }
                        },
                        {
                            "text": "Trying to save the equipment <w>cost</w> him his life.",
                            "target_lang_translations": {
                                "ru": "Попытка спасти оборудование <w>стоила</w> ему жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "lose"
                    ],
                    "antonyms": [
                        "gain",
                        "save"
                    ],
                    "common_phrases": [
                        "cost someone their life",
                        "cost someone a job",
                        "cost dear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести к потере чего-либо ценного или к неприятным последствиям для кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сто́ить",
                                "target_lang_sense_clarification": "приводить к утрате чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b71c0bb3-169e-373d-8084-18d50b6cc650",
                    "sense_definition": "To result in a severe penalty, high price, or negative outcome for someone (often used as a warning or threat).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "loss_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I can get you the information you need, but it will <w>cost</w> you.",
                            "target_lang_translations": {
                                "ru": "Я могу достать нужную тебе информацию, но это будет тебе <w>стоить</w>."
                            }
                        },
                        {
                            "text": "If you break the rules again, it is going to <w>cost</w> you.",
                            "target_lang_translations": {
                                "ru": "Если ты снова нарушишь правила, это тебе дорого <w>обойдется</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "it will cost you"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often implies a bribe or retribution"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обернуться тяжелыми последствиями, высокой ценой или наказанием (часто используется как предупреждение или угроза)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до́рого обойти́сь",
                                "target_lang_sense_clarification": "иметь негативные последствия"
                            },
                            {
                                "target_lang_word": "влете́ть в копе́ечку",
                                "target_lang_sense_clarification": "разг. дорого стоить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2acc98c8-eeee-368c-a16e-72cfbccee274",
                    "sense_definition": "To estimate or calculate how much money will be needed for a project or product.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>cost</w> the entire project before presenting it to the board.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>оценить</w> стоимость всего проекта перед представлением совету."
                            }
                        },
                        {
                            "text": "The accountant <w>costed</w> out the proposal to see if it was feasible.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер <w>рассчитал</w> смету предложения, чтобы проверить его целесообразность."
                            }
                        }
                    ],
                    "synonyms": [
                        "price",
                        "estimate",
                        "value"
                    ],
                    "common_phrases": [
                        "cost out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценивать или рассчитывать сумму денег, необходимую для проекта или продукта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оце́нивать",
                                "target_lang_sense_clarification": "определять стоимость"
                            },
                            {
                                "target_lang_word": "рассчи́тывать",
                                "target_lang_sense_clarification": "вычислять затраты"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a861073-6ded-3a5f-bb82-2bd34666a548",
                    "sense_definition": "The amount of money that is required to buy, do, or make something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The total <w>cost</w> of the new house was higher than we expected.",
                            "target_lang_translations": {
                                "ru": "Общая <w>стоимость</w> нового дома оказалась выше, чем мы ожидали."
                            }
                        },
                        {
                            "text": "We are trying to reduce the <w>cost</w> of living in the city.",
                            "target_lang_translations": {
                                "ru": "Мы пытаемся снизить <w>стоимость</w> жизни в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "price",
                        "expense",
                        "charge",
                        "fee"
                    ],
                    "common_phrases": [
                        "at all costs",
                        "cost of living",
                        "low cost",
                        "high cost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумма денег, необходимая для покупки, выполнения или изготовления чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сто́имость",
                                "target_lang_sense_clarification": "денежное выражение ценности"
                            },
                            {
                                "target_lang_word": "цена́",
                                "target_lang_sense_clarification": "плата за товар или услугу"
                            },
                            {
                                "target_lang_word": "расхо́ды",
                                "target_lang_sense_clarification": "затраты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e30ec55-abb1-3f5f-8fca-32cd28ce8878",
                    "sense_definition": "The effort, loss, or sacrifice necessary to achieve or obtain something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He built a successful career at the <w>cost</w> of his personal life.",
                            "target_lang_translations": {
                                "ru": "Он построил успешную карьеру <w>ценой</w> своей личной жизни."
                            }
                        },
                        {
                            "text": "The victory was achieved, but at a terrible <w>cost</w> to the soldiers.",
                            "target_lang_translations": {
                                "ru": "Победа была достигнута, но страшной <w>ценой</w> для солдат."
                            }
                        }
                    ],
                    "synonyms": [
                        "sacrifice",
                        "toll",
                        "penalty",
                        "price"
                    ],
                    "antonyms": [
                        "benefit",
                        "gain"
                    ],
                    "common_phrases": [
                        "at the cost of",
                        "count the cost",
                        "to my cost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Усилия, потери или жертвы, необходимые для достижения или получения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цена́",
                                "target_lang_sense_clarification": "в переносном смысле: усилия, жертвы"
                            },
                            {
                                "target_lang_word": "же́ртва",
                                "target_lang_sense_clarification": "потеря ради цели"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "8b5292ad-40cc-3336-b2db-762db881a7f4",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Cost</w> has been working at the firm for ten years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Кост</w> работает в фирме уже десять лет."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>Cost</w> at the conference.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Сара <w>Кост</w> на конференции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кост",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
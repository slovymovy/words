                    "frequency": "Middle",
                    "semantic_group_id": "ordinal_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was the <w>seventh</w> to finish the race.",
                            "target_lang_translations": {
                                "ru": "Она пришла к финишу <w>седьмой</w>."
                            }
                        },
                        {
                            "text": "My birthday is on the <w>seventh</w> of July.",
                            "target_lang_translations": {
                                "ru": "Мой день рождения <w>седьмого</w> июля."
                            }

            "senses": [
                {
                    "sense_id": "935b0b16-b651-3d2a-9f4b-81c153f0399f",
                    "sense_definition": "Предприятие бытового обслуживания или магазин, предлагающий модные товары и услуги.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "service_establishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя сестра работает парикмахером в <салоне> красоты.",
                            "target_lang_translations": {
                                "en": "My sister works as a hairdresser at a beauty <salon>."
                            }
                        },
                        {
                            "text": "Невеста провела весь день, выбирая платье в свадебном <салоне>.",
                            "target_lang_translations": {
                                "en": "The bride spent the whole day choosing a dress at a bridal <salon>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "салон красоты",
                        "свадебный салон",
                        "массажный салон",
                        "тату-салон"
                    ],
                    "target_lang_definitions": {
                        "en": "A business providing personal services or a shop selling fashionable goods."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "salon",
                                "target_lang_sense_clarification": "beauty or fashion establishment"
                            },
                            {
                                "target_lang_word": "parlor",
                                "target_lang_sense_clarification": "service establishment (e.g., tattoo, massage)"
                            }
                        ]
                    }
                },
                {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9830fe38-51fa-33e3-a838-3d7a4c17cfa3",
                    "sense_definition": "A flat, straight strip of wood, plastic, or metal, marked with units of measurement and used for drawing straight lines or measuring lengths.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "measuring_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please use a <w>ruler</w> to draw a straight line on the paper.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, используй <w>линейку</w>, чтобы начертить прямую линию на бумаге."
                            }
                        },
                        {
                            "text": "The student has a plastic <w>ruler</w> in his pencil case.",
                            "target_lang_translations": {
                                "ru": "У ученика в пенале лежит пластиковая <w>линейка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rule"
                    ],
                    "common_phrases": [
                        "wooden ruler",
                        "plastic ruler",
                        "measure with a ruler"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская прямая планка из дерева, пластика или металла с делениями, используемая для черчения прямых линий или измерения длины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лине́йка",
                                "target_lang_sense_clarification": "инструмент для измерения и черчения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f71e138c-83be-3a46-b6b3-79f053352c3b",
                    "sense_definition": "A person who rules or governs a country, state, or group.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_governing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>ruler</w> of the country decided to change the law.",
                            "target_lang_translations": {
                                "ru": "<w>Правитель</w> страны решил изменить закон."
                            }
                        },
                        {
                            "text": "She was a powerful <w>ruler</w> who was loved by her people.",
                            "target_lang_translations": {
                                "ru": "Она была могущественной <w>правительницей</w>, которую любил её народ."
                            }
                        }
                    ],
                    "common_phrases": [
                        "powerful ruler",
                        "ruler of the country"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который правит или управляет страной, государством или группой людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прави́тель",
                                "target_lang_sense_clarification": "глава государства или территории"
                            },
                            {
                                "target_lang_word": "властели́н",
                                "target_lang_sense_clarification": "обладатель верховной власти"
                            },
                            {
                                "target_lang_word": "госуда́рь",
                                "target_lang_sense_clarification": "монарх"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ca777100-4eb8-3fdd-a176-aa3a975047a3",
                    "sense_definition": "To beat someone with a ruler, usually as a punishment.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the old days, teachers often <w>rulered</w> students who misbehaved.",
                            "target_lang_translations": {
                                "ru": "В прежние времена учителя часто <w>били линейкой</w> учеников, которые плохо себя вели."
                            }
                        },
                        {
                            "text": "He was <w>rulered</w> on the hand for talking during the lesson.",
                            "target_lang_translations": {
                                "ru": "Его <w>ударили линейкой</w> по руке за разговоры во время урока."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "referring to a traditional school punishment"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить кого-либо линейкой, обычно в качестве наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить линейкой",
                                "target_lang_sense_clarification": "телесное наказание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
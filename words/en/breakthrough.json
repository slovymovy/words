                        "advance"
                    ],
                    "antonyms": [
                        "stalemate",
                        "retreat"
                    ],
                    "common_phrases": [
                        "military breakthrough",
                        "enemy breakthrough"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по преодолению линии фронта и продвижению в тыл врага."

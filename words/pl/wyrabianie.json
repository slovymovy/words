            "senses": [
                {
                    "sense_id": "e5fd1c03-6966-3391-a99c-d377edc6f691",
                    "sense_definition": "Czynność polegająca na dopełnianiu formalności w celu uzyskania oficjalnego dokumentu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "administrative_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wyrabianie paszportu w sezonie wakacyjnym może potrwać dłużej niż zwykle.",
                            "target_lang_translations": {
                                "ru": "<w>Оформление</w> паспорта в сезон отпусков может занять больше времени, чем обычно."
                            }
                        },
                        {
                            "text": "Zleciłem w urzędzie wyrabianie nowego dowodu osobistego.",
                            "target_lang_translations": {
                                "ru": "Я заказал в учреждении <w>оформление</w> нового удостоверения личности."
                            }
                        }
                    ],
                    "synonyms": [
                        "załatwianie",
                        "uzyskiwanie"
                    ],
                    "common_phrases": [
                        "wyrabianie paszportu",
                        "wyrabianie dowodu",
                        "wyrabianie wizy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс выполнения формальностей для получения официального документа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оформление",
                                "target_lang_sense_clarification": "документов"
                            },
                            {
                                "target_lang_word": "изготовление",
                                "target_lang_sense_clarification": "производство документа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2213b7d8-80fe-3f14-9a77-922d5d1d1fd4",
                    "sense_definition": "Proces intensywnego ugniatania i łączenia składników w jednolitą masę, zazwyczaj w kontekście przygotowywania jedzenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "culinary_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ręczne wyrabianie ciasta na pizzę wymaga sporo siły fizycznej.",
                            "target_lang_translations": {
                                "ru": "Ручное <w>замешивание</w> теста для пиццы требует немалой физической силы."
                            }
                        },
                        {
                            "text": "Długie wyrabianie masy sprawia, że wypieki są bardziej puszyste.",
                            "target_lang_translations": {
                                "ru": "Долгое <w>вымешивание</w> массы делает выпечку более пышной."
                            }
                        }
                    ],
                    "synonyms": [
                        "ugniatanie",
                        "zagniatanie"
                    ],
                    "common_phrases": [
                        "wyrabianie ciasta",
                        "ręczne wyrabianie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс интенсивного разминания и смешивания ингредиентов до получения однородной массы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замешивание",
                                "target_lang_sense_clarification": "теста"
                            },
                            {
                                "target_lang_word": "вымешивание",
                                "target_lang_sense_clarification": "тщательное"
                            }
                        ]
                    }
                }
            ]

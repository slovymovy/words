    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4ddacfbc-bf5a-3a7b-a21d-fba3769a7c19",
                    "sense_definition": "In a way that involves no difficulty or effort.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She passed the driving test <w>easily</w> on her first try.",
                            "target_lang_translations": {
                                "ru": "Она <w>легко</w> сдала экзамен на права с первой попытки."
                            }
                        },
                        {
                            "text": "Information is <w>easily</w> accessible on the internet.",
                            "target_lang_translations": {
                                "ru": "Информация <w>легко</w> доступна в интернете."
                            }
                        }
                    ],
                    "synonyms": [
                        "readily",
                        "handily"
                    ],
                    "common_phrases": [
                        "easily accessible",
                        "easily available",
                        "win easily"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без затруднений, не прилагая больших усилий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легко",
                                "target_lang_sense_clarification": "без труда"
                            },
                            {
                                "target_lang_word": "без труда",
                                "target_lang_sense_clarification": "свободно, просто"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c7fd022-a5b8-3ae4-92fb-0cc98f388cbd",
                    "sense_definition": "Showing a tendency to happen quickly, frequently, or with little provocation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Small children get bored very <w>easily</w>.",
                            "target_lang_translations": {
                                "ru": "Маленькие дети очень <w>легко</w> начинают скучать."
                            }
                        },
                        {
                            "text": "Glass is fragile and breaks <w>easily</w> if dropped.",
                            "target_lang_translations": {
                                "ru": "Стекло хрупкое и <w>легко</w> бьется, если его уронить."
                            }
                        }
                    ],
                    "synonyms": [
                        "readily"
                    ],
                    "common_phrases": [
                        "easily distracted",
                        "easily offended",
                        "easily damaged"
                    ],
                    "target_lang_definitions": {
                        "ru": "О склонности поддаваться воздействию или меняться быстро и часто."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легко",
                                "target_lang_sense_clarification": "быстро, без сопротивления"
                            },
                            {
                                "target_lang_word": "быстро",
                                "target_lang_sense_clarification": "в скором времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40c6e3f8-c748-3fd9-aee9-7cd81d972a0b",
                    "sense_definition": "Without question or doubt; used to emphasize superlatives.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That was <w>easily</w> the best meal I have ever eaten.",
                            "target_lang_translations": {
                                "ru": "Это была <w>безусловно</w> лучшая еда, которую я когда-либо ел."
                            }
                        },
                        {
                            "text": "She is <w>easily</w> the smartest student in the class.",
                            "target_lang_translations": {
                                "ru": "Она <w>бесспорно</w> самая умная ученица в классе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "easily the best",
                        "easily the worst"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несомненно, очевидно; используется для усиления превосходной степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безусловно",
                                "target_lang_sense_clarification": "несомненно"
                            },
                            {
                                "target_lang_word": "бесспорно",
                                "target_lang_sense_clarification": "очевидно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74f8b005-0de0-34a6-8f13-a303b9ded51e",
                    "sense_definition": "In a comfortable or relaxed manner; without anxiety.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We cannot rest <w>easily</w> until we know they are safe.",
                            "target_lang_translations": {
                                "ru": "Мы не сможем спать <w>спокойно</w>, пока не узнаем, что они в безопасности."
                            }
                        },
                        {
                            "text": "The wealthy couple lived <w>easily</w> in their large estate.",
                            "target_lang_translations": {
                                "ru": "Богатая пара жила <w>беззаботно</w> в своем огромном поместье."

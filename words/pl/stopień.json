                },
                {
                    "sense_id": "9dbe53d1-c048-3427-bdf0-525712632b5d",
                    "sense_definition": "Tytuł określający miejsce i funkcję w hierarchii wojskowej lub służbowej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hierarchy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz został awansowany na <w>stopień</w> kapitana za swoje zasługi.",
                            "target_lang_translations": {
                                "ru": "Солдат был повышен до <w>звания</w> капитана за свои заслуги."
                            }
                        },
                        {
                            "text": "Mój dziadek służył w wojsku i miał wysoki <w>stopień</w> oficerski.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка служил в армии и имел высокое офицерское <w>звание</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ranga",
                        "szarża"
                    ],
                    "common_phrases": [

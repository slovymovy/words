{
    "word_family": [
        "limit",
        "limited",
        "limitless",
        "delimitation",
        "limiter"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "465c01a1-9f0b-358f-8bd8-122fa433b0cc",
                    "sense_definition": "A rule, fact, or condition that restricts what is possible, allowed, or available.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "restriction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Due to budget <w>limitations</w>, the company cannot hire any new staff this year."
                        },
                        {
                            "text": "There is a strict <w>limitation</w> on the amount of luggage you can bring on the plane."
                        }
                    ],
                    "common_phrases": [
                        "strict limitation",
                        "physical limitation",
                        "severe limitation"
                    ]
                },
                {
                    "sense_id": "fdf715ef-cbaf-3869-a062-994adfc14484",
                    "sense_definition": "A weakness, imperfection, or lack of ability that reduces effectiveness or success.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "flaw",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is smart enough to know his own <w>limitations</w> and ask for help when needed."
                        },
                        {
                            "text": "The main <w>limitation</w> of this study is the small number of people who participated."
                        }
                    ],
                    "common_phrases": [
                        "know your limitations",
                        "overcome a limitation",
                        "inherent limitation"
                    ]
                },
                {
                    "sense_id": "99a8e086-0378-3edb-b69c-2850634348b9",
                    "sense_definition": "The act of controlling something to keep it within limits, or the state of being controlled.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "restriction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government proposed a <w>limitation</w> on the use of private cars in the city center."
                        },
                        {
                            "text": "Many citizens protested against the <w>limitation</w> of their personal freedoms."
                        }
                    ],
                    "antonyms": [
                        "limitlessness"
                    ],
                    "common_phrases": [
                        "limitation of liability",
                        "limitation of power"
                    ]
                },
                {
                    "sense_id": "0e218c59-0f62-38bd-9789-ca127dc559d9",
                    "sense_definition": "A specific period of time established by law during which a legal action can be brought.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge dismissed the case because the statute of <w>limitations</w> had already expired."
                        },
                        {
                            "text": "They could not sue for damages because the three-year <w>limitation</w> period had passed."
                        }
                    ],
                    "synonyms": [
                        "prescription"
                    ],
                    "common_phrases": [
                        "statute of limitations"
                    ]
                }
            ]
        }
    ]
}
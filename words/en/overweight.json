                            {
                                "target_lang_word": "перегру́женный",
                                "target_lang_sense_clarification": "о транспортном средстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6983b92f-a93c-33c7-8383-f0e628337646",

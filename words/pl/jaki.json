    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "a3627ba1-82eb-3eeb-980e-fd75f1344f93",
                    "sense_definition": "Służy do zadawania pytań o właściwości, cechy, jakość lub wygląd kogoś lub czegoś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "question_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiem, <w>jaki</w> kolor pasuje do tego pokoju.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, <w>какой</w> цвет подходит к этой комнате."
                            }
                        },
                        {
                            "text": "<w>Jaka</w> jest dzisiaj pogoda na zewnątrz?",
                            "target_lang_translations": {
                                "ru": "<w>Какая</w> сегодня погода на улице?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "w jakim sensie",
                        "w jakim celu",
                        "jaki taki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для вопроса о свойствах, качествах или внешнем виде кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "вопросительное местоимение"
                            },
                            {
                                "target_lang_word": "каков",
                                "target_lang_sense_clarification": "краткая форма, книжное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d731f9d-a5da-3552-a23a-22a18a5dc145",
                    "sense_definition": "Służy do zadawania pytań o wskazanie konkretnego elementu lub osoby z większej grupy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "question_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jaki</w> film chcesz dzisiaj obejrzeć w kinie?",
                            "target_lang_translations": {
                                "ru": "<w>Какой</w> фильм ты хочешь сегодня посмотреть в кино?"
                            }
                        },
                        {
                            "text": "Zastanawiam się, <w>jaki</w> deser wybrać z karty.",
                            "target_lang_translations": {
                                "ru": "Я думаю, <w>какой</w> десерт выбрать из меню."
                            }
                        }
                    ],
                    "synonyms": [
                        "który"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для вопроса о выборе конкретного элемента или лица из группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "выбор из группы"
                            },
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "порядковый выбор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1b0f899-9be7-352a-aded-6f1541574b78",
                    "sense_definition": "Służy do wyrażania zachwytu, zdziwienia lub silnych emocji w zdaniach wykrzyknikowych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "exclamation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Jaki</w> piękny widok rozciąga się z tego okna!",
                            "target_lang_translations": {
                                "ru": "<w>Какой</w> красивый вид открывается из этого окна!"
                            }
                        },
                        {
                            "text": "Spójrz, <w>jaki</w> duży pies biegnie w naszą stronę!",
                            "target_lang_translations": {
                                "ru": "Смотри, <w>какая</w> большая собака бежит в нашу сторону!"
                            }
                        }
                    ],
                    "synonyms": [
                        "ale"
                    ],
                    "common_phrases": [
                        "jaka szkoda",
                        "jaki wstyd",
                        "jaki żal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения восхищения, удивления или сильных эмоций в восклицательных предложениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "в восклицаниях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed41c42a-3581-36be-b35a-bb3da6580b1e",
                    "sense_definition": "Łączy zdanie nadrzędne z podrzędnym, wskazując na cechę lub przedmiot wymieniony wcześniej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relative_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem dokładnie taki samochód, <w>jaki</w> mi poleciłeś.",
                            "target_lang_translations": {
                                "ru": "Я купил именно такую машину, <w>какую</w> ты мне посоветовал."
                            }
                        },
                        {
                            "text": "To jest jedyny dowód, <w>jakim</w> obecnie dysponuje policja.",
                            "target_lang_translations": {
                                "ru": "Это единственное доказательство, <w>каким</w> сейчас располагает полиция."
                            }
                        }
                    ],
                    "synonyms": [
                        "który"
                    ],
                    "common_phrases": [
                        "taki jaki",
                        "w taki sposób w jaki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связывает главное предложение с придаточным, указывая на признак или предмет, упомянутый ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "союзное слово"
                            },
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "относительное местоимение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26545ebf-687f-309c-bf2e-ba49411ecfd5",
                    "sense_definition": "Służy do wyrażenia zdziwienia, oburzenia lub prośby o powtórzenie, często nawiązując do słów rozmówcy.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "exclamation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mówisz o problemach? <w>Jaki</w> znowu problem?",
                            "target_lang_translations": {
                                "ru": "Ты говоришь о проблемах? <w>Какая</w> ещё проблема?"
                            }
                        },
                        {
                            "text": "<w>Jaki</w> mandat? Przecież zaparkowałem prawidłowo!",
                            "target_lang_translations": {
                                "ru": "<w>Какой</w> штраф? Я же припарковался правильно!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "jaki znowu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения удивления, возмущения или просьбы повторить, часто ссылаясь на слова собеседника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "возмущение или переспрос"
                            },
                            {
                                "target_lang_word": "какой ещё",
                                "target_lang_sense_clarification": "усиление возмущения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd749a38-24c0-3692-bb4f-f5b70bbeb459",
                    "sense_definition": "Używany potocznie do określenia przybliżonej ilości, czasu lub miary.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "indefinite_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę poczekać na niego jeszcze <w>jakie</w> piętnaście minut.",
                            "target_lang_translations": {
                                "ru": "Мне нужно подождать его ещё <w>примерно</w> пятнадцать минут."
                            }
                        },
                        {
                            "text": "Byłem w tym mieście <w>jaki</w> rok temu.",
                            "target_lang_translations": {
                                "ru": "Я был в этом городе <w>где-то</w> год назад."
                            }
                        }
                    ],
                    "synonyms": [
                        "gdzieś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to approximate numbers or time"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в разговорной речи для обозначения приблизительного количества, времени или меры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "примерно",
                                "target_lang_sense_clarification": "наречие"
                            },
                            {
                                "target_lang_word": "где-то",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "предлог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb0d24e7-73b3-3f0c-918a-98831518d960",
                    "sense_definition": "Oznacza osobę lub rzecz nieokreśloną, bliżej nieznaną.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "indefinite_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy jest na sali <w>jaki</w> lekarz, który może pomóc?",
                            "target_lang_translations": {
                                "ru": "Есть ли в зале <w>какой-нибудь</w> врач, который может помочь?"
                            }
                        },
                        {
                            "text": "Przydałby nam się tu <w>jaki</w> silny człowiek do pomocy.",
                            "target_lang_translations": {
                                "ru": "Нам бы здесь пригодился <w>какой-нибудь</w> сильный человек в помощь."
                            }
                        }
                    ],
                    "synonyms": [
                        "jakiś"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly replaced by 'jakiś' in modern Polish"
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает неопределённое лицо или предмет, о которых мало известно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какой-то",
                                "target_lang_sense_clarification": "неопределённое"
                            },
                            {
                                "target_lang_word": "какой-нибудь",
                                "target_lang_sense_clarification": "любой"
                            }
                        ]
                    }
                }
            ]
        },
        {

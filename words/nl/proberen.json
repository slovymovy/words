    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5e57b9e0-25cf-351a-9c1b-452ff125acac",
                    "sense_definition": "Moeite doen om iets te bereiken of uit te voeren, zonder zekerheid dat het lukt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attempt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal het morgen opnieuw <w>proberen</w>.",
                            "target_lang_translations": {
                                "en": "I will <w>try</w> it again tomorrow."
                            }
                        },
                        {
                            "text": "Hij <w>probeerde</w> de deur te openen, maar die zat op slot.",
                            "target_lang_translations": {
                                "en": "He <w>tried</w> to open the door, but it was locked."
                            }
                        },
                        {
                            "text": "Zij <w>probeert</w> altijd goed te luisteren naar de leraar.",
                            "target_lang_translations": {
                                "en": "She always <w>tries</w> to listen carefully to the teacher."
                            }
                        }
                    ],
                    "synonyms": [
                        "trachten",
                        "pogen"
                    ],
                    "antonyms": [
                        "opgeven",
                        "stoppen"
                    ],
                    "common_phrases": [
                        "het opnieuw proberen",
                        "alles proberen",
                        "niet geschoten is altijd mis"
                    ],
                    "target_lang_definitions": {
                        "en": "To make an effort to achieve or execute something, without certainty of success."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "try",
                                "target_lang_sense_clarification": "to make an effort"
                            },
                            {
                                "target_lang_word": "attempt",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1947d2c-dfa0-3297-b5c7-22cf324ab178",
                    "sense_definition": "Iets testen, keuren of proeven om te ervaren hoe het is, smaakt of werkt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "test",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil je dit stukje taart even <w>proberen</w>?",
                            "target_lang_translations": {
                                "en": "Do you want to <w>try</w> this piece of cake?"
                            }
                        },
                        {
                            "text": "Je moet die nieuwe app echt eens <w>proberen</w>.",
                            "target_lang_translations": {
                                "en": "You really should <w>try</w> that new app sometime."
                            }
                        },
                        {
                            "text": "In de winkel kun je de schoenen eerst <w>proberen</w>.",
                            "target_lang_translations": {
                                "en": "In the store you can <w>try</w> the shoes first."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitproberen",
                        "uittesten",
                        "proeven",
                        "beproeven"
                    ],
                    "common_phrases": [
                        "iets nieuws proberen",
                        "een hapje proberen",
                        "deze maat proberen"
                    ],
                    "target_lang_definitions": {
                        "en": "To test, inspect, or taste something to experience its quality, taste, or function."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "try",
                                "target_lang_sense_clarification": "to test or taste"
                            },
                            {
                                "target_lang_word": "try out",
                                "target_lang_sense_clarification": "to test something new"
                            },
                            {
                                "target_lang_word": "test",
                                "target_lang_sense_clarification": "to verify function"
                            }
                        ]
                    }
                }
            ]
        }
    ]

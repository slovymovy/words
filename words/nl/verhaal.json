{
    "word_family": [
        "verhaler",
        "verhalend"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "667debe8-2fec-3dd2-b167-5e2549d49957",
                    "sense_definition": "Een mondeling of schriftelijk verslag van echte of verzonnen gebeurtenissen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "narrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn opa vertelt elke avond een spannend <w>verhaal</w> voor het slapengaan.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка каждый вечер перед сном рассказывает захватывающую <w>историю</w>."
                            }
                        },
                        {
                            "text": "Het boek is gebaseerd op een waargebeurd <w>verhaal</w> uit de oorlog.",
                            "target_lang_translations": {
                                "ru": "Книга основана на реальной <w>истории</w> военных лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertelling"
                    ],
                    "common_phrases": [
                        "een spannend verhaal",
                        "verhaal doen",
                        "het hele verhaal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устный или письменный отчет о реальных или вымышленных событиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "история",
                                "target_lang_sense_clarification": "последовательность событий"
                            },
                            {
                                "target_lang_word": "рассказ",
                                "target_lang_sense_clarification": "повествование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9d8715f-c348-3599-b7df-bede0404fcbe",
                    "sense_definition": "Een lange, vaak ingewikkelde uitleg of toespraak.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "narrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij hield een heel <w>verhaal</w> over waarom hij te laat was.",
                            "target_lang_translations": {
                                "ru": "Он выдал целую <w>тираду</w> о том, почему опоздал."
                            }
                        },
                        {
                            "text": "Bespaar me je <w>verhaal</w> en zeg gewoon wat je wilt.",
                            "target_lang_translations": {
                                "ru": "Избавь меня от своих <w>историй</w> и просто скажи, чего ты хочешь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een heel verhaal",
                        "een warrig verhaal",
                        "een lang verhaal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинное, часто сложное объяснение или речь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "история",
                                "target_lang_sense_clarification": "длинное объяснение или оправдание"
                            },
                            {
                                "target_lang_word": "тирада",
                                "target_lang_sense_clarification": "длинная речь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bba7832-9fe7-35e9-a73b-db724e3cec7c",
                    "sense_definition": "Het recht of de daad om schade en kosten op iemand anders te verhalen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verzekering zoekt <w>verhaal</w> op de dader van het ongeluk.",
                            "target_lang_translations": {
                                "ru": "Страховая компания требует <w>возмещения</w> от виновника аварии."
                            }
                        },
                        {
                            "text": "Bij een faillissement is er vaak geen <w>verhaal</w> meer mogelijk.",
                            "target_lang_translations": {
                                "ru": "При банкротстве часто невозможно никакое <w>взыскание</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "recht van verhaal",
                        "verhaal halen",
                        "verhaal zoeken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "juridisch"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Право или действие по взысканию ущерба и расходов с другого лица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взыскание",
                                "target_lang_sense_clarification": "юридическое действие"
                            },
                            {
                                "target_lang_word": "возмещение",
                                "target_lang_sense_clarification": "компенсация убытков"
                            },
                            {
                                "target_lang_word": "регресс",
                                "target_lang_sense_clarification": "право обратного требования"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17878df7-4f65-3e20-a8e4-247d6fe4e354",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>verhalen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>verhaal</w> de gemaakte kosten direct op mijn werkgever.",
                            "target_lang_translations": {
                                "ru": "Я <w>перекладываю</w> понесенные расходы непосредственно на работодателя."
                            }
                        },
                        {
                            "text": "Als ik schade lijd, <w>verhaal</w> ik die op de verantwoordelijke persoon.",
                            "target_lang_translations": {
                                "ru": "Если я несу убытки, я <w>взыскиваю</w> их с ответственного лица."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола 'verhalen' (взыскивать, перекладывать расходы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взыскиваю",
                                "target_lang_sense_clarification": "требую возмещения"
                            },
                            {
                                "target_lang_word": "перекладываю",
                                "target_lang_sense_clarification": "отношу расходы на кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d08c6d6-205c-3b02-be0f-2b7238822b3d",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>verhalen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Verhaal</w> je de schade op je verzekering of betaal je het zelf?",
                            "target_lang_translations": {
                                "ru": "Ты <w>взыскиваешь</w> ущерб со страховки или платишь сам?"
                            }
                        },
                        {
                            "text": "Waarom <w>verhaal</w> jij die oude gebeurtenissen steeds opnieuw?",
                            "target_lang_translations": {
                                "ru": "Почему ты снова и снова <w>пересказываешь</w> эти старые события?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Второе лицо единственного числа настоящего времени от глагола 'verhalen'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взыскиваешь",
                                "target_lang_sense_clarification": "требуешь возмещения"
                            },
                            {
                                "target_lang_word": "пересказываешь",
                                "target_lang_sense_clarification": "повествуешь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94f0ce7e-ca1f-3dcc-8ecd-db8ca154ce49",
                    "sense_definition": "Gebiedende wijs van <w>verhalen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Verhaal</w> die onkosten maar op de klant, dat is de afspraak.",
                            "target_lang_translations": {
                                "ru": "<w>Переложи</w> эти расходы на клиента, таков уговор."
                            }
                        },
                        {
                            "text": "<w>Verhaal</w> niet steeds op het verleden, maar kijk naar de toekomst.",
                            "target_lang_translations": {
                                "ru": "Не <w>оглядывайся</w> постоянно на прошлое, а смотри в будущее."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола 'verhalen'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взыщи",
                                "target_lang_sense_clarification": "потребуй возмещения"
                            },
                            {
                                "target_lang_word": "переложи",
                                "target_lang_sense_clarification": "отнеси расходы на другого"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
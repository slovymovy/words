{
    "word_family": [
        "caroler",
        "caroling"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9b0a0b8d-5608-3ed1-9a81-289a4b9dbbe2",
                    "sense_definition": "A traditional religious or secular song sung at Christmastime.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "christmas_music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The choir sang a beautiful Christmas <w>carol</w> during the evening service.",
                            "target_lang_translations": {
                                "ru": "Хор исполнил красивый <w>рождественский гимн</w> во время вечерней службы."
                            }
                        },
                        {
                            "text": "We learned a new <w>carol</w> to perform at the school holiday concert.",
                            "target_lang_translations": {
                                "ru": "Мы выучили новую <w>рождественскую песню</w>, чтобы выступить на школьном праздничном концерте."
                            }
                        }
                    ],
                    "synonyms": [
                        "noel",
                        "Christmas song"
                    ],
                    "common_phrases": [
                        "Christmas carol",
                        "sing a carol",
                        "favorite carol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционная религиозная или светская песня, исполняемая во время Рождества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рождественский гимн",
                                "target_lang_sense_clarification": "торжественная песня, исполняемая в церкви или на концертах"
                            },
                            {
                                "target_lang_word": "колядка",
                                "target_lang_sense_clarification": "народная обрядовая песня"
                            },
                            {
                                "target_lang_word": "рождественская песня",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d7fe6dc-b896-3bf2-8a75-be54a00abb47",
                    "sense_definition": "A ballad or song of joy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "joyful_song",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The morning <w>carol</w> of the birds woke me up early in the spring.",
                            "target_lang_translations": {
                                "ru": "Утренняя <w>песнь</w> птиц разбудила меня рано весной."
                            }
                        },
                        {
                            "text": "She wrote a sweet <w>carol</w> to celebrate the arrival of the harvest.",
                            "target_lang_translations": {
                                "ru": "Она написала милую <w>радостную песню</w>, чтобы отпраздновать начало сбора урожая."
                            }
                        }
                    ],
                    "synonyms": [
                        "song",
                        "ballad",
                        "lay"
                    ],
                    "antonyms": [
                        "dirge",
                        "lament"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary or poetic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Баллада или радостная песня (устаревшее или поэтическое)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "радостная песнь",
                                "target_lang_sense_clarification": "поэтическое"
                            },
                            {
                                "target_lang_word": "гимн",
                                "target_lang_sense_clarification": "песня восхваления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa45ff0f-59c1-3a2d-b156-e51ae48676ef",
                    "sense_definition": "A round dance accompanied by singing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_dance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The medieval peasants performed a lively <w>carol</w> in the village square.",
                            "target_lang_translations": {
                                "ru": "Средневековые крестьяне исполняли оживлённый <w>хоровод</w> на деревенской площади."
                            }
                        },
                        {
                            "text": "Historians study the <w>carol</w> as an early form of community dance and music.",
                            "target_lang_translations": {
                                "ru": "Историки изучают <w>кэрол</w> как раннюю форму общественных танцев и музыки."
                            }
                        }
                    ],
                    "synonyms": [
                        "round dance"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хороводный танец с пением (историческое)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хороводная песня",
                                "target_lang_sense_clarification": "танец с пением"
                            },
                            {
                                "target_lang_word": "кэрол",
                                "target_lang_sense_clarification": "исторический музыкальный жанр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d151fd16-59fc-3a92-895a-0899d16612c3",
                    "sense_definition": "A small closet or enclosure built against the inner side of a window of a monastery's cloister, used for study.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The monk sat in his wooden <w>carol</w> to read the ancient scriptures.",
                            "target_lang_translations": {
                                "ru": "Монах сидел в своей деревянной <w>кабинке</w>, читая древние писания."
                            }
                        },
                        {
                            "text": "The library preserved an old <w>carol</w> that was once used by scholars for private study.",
                            "target_lang_translations": {
                                "ru": "В библиотеке сохранилась старинная <w>кабинка для занятий</w>, которой когда-то пользовались учёные для уединения."
                            }
                        }
                    ],
                    "synonyms": [
                        "carrel",
                        "cubicle"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical architecture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая ниша или кабинка для занятий, встроенная в окно монастырской галереи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кабинка для занятий",
                                "target_lang_sense_clarification": "в монастыре или библиотеке"
                            },
                            {
                                "target_lang_word": "ниша",
                                "target_lang_sense_clarification": "архитектурный элемент"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1b74a85b-6af9-3324-9e25-cff9f170fbe7",
                    "sense_definition": "To sing carols; especially to sing Christmas carols in a group.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "christmas_music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We plan to go <w>caroling</w> around the neighborhood on Christmas Eve.",
                            "target_lang_translations": {
                                "ru": "Мы планируем пойти <w>колядовать</w> по району в канун Рождества."
                            }
                        },
                        {
                            "text": "The children <w>caroled</w> happily outside the nursing home to cheer up the residents.",
                            "target_lang_translations": {
                                "ru": "Дети радостно <w>пели рождественские песни</w> возле дома престарелых, чтобы подбодрить постояльцев."
                            }
                        }
                    ],
                    "synonyms": [
                        "sing",
                        "chant"
                    ],
                    "common_phrases": [
                        "go caroling",
                        "carol door to door"
                    ],
                    "target_lang_definitions": {
                        "ru": "Петь рождественские гимны или колядки, особенно группой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колядовать",
                                "target_lang_sense_clarification": "ходить по домам с песнями"
                            },
                            {
                                "target_lang_word": "петь рождественские гимны",
                                "target_lang_sense_clarification": "исполнять песни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "137f7f9f-8e29-34ff-ad51-34de7b9b4350",
                    "sense_definition": "To sing in a joyful or cheerful manner.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "joyful_song",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lark began to <w>carol</w> as the sun rose over the green hills.",
                            "target_lang_translations": {
                                "ru": "Жаворонок начал <w>заливаться песней</w>, когда солнце взошло над зелёными холмами."
                            }
                        },
                        {
                            "text": "She <w>caroled</w> a happy tune while working in the garden.",
                            "target_lang_translations": {
                                "ru": "Она <w>напевала</w> весёлую мелодию, работая в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "warble",
                        "trill",
                        "croon"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Петь радостно или весело."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распевать",
                                "target_lang_sense_clarification": "петь громко и весело"
                            },
                            {
                                "target_lang_word": "заливаться песней",
                                "target_lang_sense_clarification": "о птицах или людях"
                            },
                            {
                                "target_lang_word": "напевать",
                                "target_lang_sense_clarification": "тихо петь про себя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9d464123-be88-3fc3-9420-7f2327533629",
                    "sense_definition": "A female given name.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "female_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My aunt <w>Carol</w> is coming to visit us next week.",
                            "target_lang_translations": {
                                "ru": "Моя тётя <w>Кэрол</w> приедет навестить нас на следующей неделе."
                            }
                        },
                        {
                            "text": "<w>Carol</w> decided to change her hairstyle for the party.",
                            "target_lang_translations": {
                                "ru": "<w>Кэрол</w> решила сменить причёску к вечеринке."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кэрол",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93c537e9-045a-356c-ac7e-c2cd97c04438",
                    "sense_definition": "A male given name.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "male_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "King <w>Carol</w> II of Romania reigned during a turbulent period in history.",
                            "target_lang_translations": {
                                "ru": "Король Румынии <w>Кароль</w> II правил в неспокойный исторический период."
                            }
                        },
                        {
                            "text": "We named our son <w>Carol</w> after his great-grandfather.",
                            "target_lang_translations": {
                                "ru": "Мы назвали сына <w>Кароль</w> в честь его прадеда."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кароль",
                                "target_lang_sense_clarification": "историческое или европейское имя"
                            },
                            {
                                "target_lang_word": "Кэрол",
                                "target_lang_sense_clarification": "редкое мужское имя в англоязычных странах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "c356d4d6-65bc-34a3-8338-60e97160655b",
                    "sense_definition": "Увеличить количество, объём или состав чего-либо, добавив новое к уже имеющемуся.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно срочно <w>пополнить</w> баланс на телефоне, чтобы позвонить.",
                            "target_lang_translations": {
                                "en": "I need to urgently <w>top up</w> my phone balance to make a call."
                            }
                        },
                        {
                            "text": "Студент пошёл в библиотеку, чтобы <w>пополнить</w> свои знания перед экзаменом.",
                            "target_lang_translations": {
                                "en": "The student went to the library to <w>add to</w> his knowledge before the exam."
                            }
                        },
                        {
                            "text": "Мы решили <w>пополнить</w> запасы еды перед долгой поездкой.",
                            "target_lang_translations": {
                                "en": "We decided to <w>restock</w> our food supplies before the long trip."
                            }
                        }
                    ],
                    "synonyms": [
                        "увеличить",
                        "дополнить",
                        "приумножить"
                    ],
                    "antonyms": [
                        "уменьшить",
                        "сократить",
                        "растратить"
                    ],
                    "common_phrases": [
                        "пополнить счёт",
                        "пополнить баланс",
                        "пополнить коллекцию",
                        "пополнить бюджет",
                        "пополнить знания"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase the quantity, volume, or composition of something by adding new things to what already exists."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "replenish",
                                "target_lang_sense_clarification": "supplies, stocks, or general quantity"
                            },
                            {
                                "target_lang_word": "top up",
                                "target_lang_sense_clarification": "accounts, balances, or liquids"
                            },
                            {
                                "target_lang_word": "restock",
                                "target_lang_sense_clarification": "inventory or provisions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3800c7c-7801-3909-909c-60d8f2b02967",
                    "sense_definition": "Добавить людей, сотрудников или военные силы в состав группы или организации для усиления или замены выбывших.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир получил приказ <w>пополнить</w> ряды армии новобранцами.",
                            "target_lang_translations": {
                                "en": "The commander received an order to <w>replenish</w> the army's ranks with new recruits."
                            }
                        },
                        {
                            "text": "Клуб планирует <w>пополнить</w> свой состав талантливыми игроками в следующем сезоне.",
                            "target_lang_translations": {
                                "en": "The club plans to <w>reinforce</w> its squad with talented players next season."
                            }
                        }
                    ],
                    "synonyms": [
                        "укомплектовать",
                        "восполнить",
                        "усилить"
                    ],
                    "common_phrases": [
                        "пополнить ряды",
                        "пополнить состав",
                        "пополнить армию",
                        "пополнить команду"
                    ],
                    "target_lang_definitions": {
                        "en": "To add people, employees, or military forces to a group or organization to reinforce or replace those lost."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reinforce",
                                "target_lang_sense_clarification": "a group, army, or team"
                            },
                            {
                                "target_lang_word": "replenish",
                                "target_lang_sense_clarification": "ranks or personnel numbers"
                            }
                        ]
                    }
                }
            ]
        }
    ]

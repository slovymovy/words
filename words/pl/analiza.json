                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zlecił pacjentowi szczegółową <w>analizę</w> krwi.",
                            "target_lang_translations": {
                                "ru": "Врач назначил пациенту подробный <w>анализ</w> крови."
                            }
                        },
                        {
                            "text": "Laboratorium wykonało <w>analizę</w> chemiczną pobranych próbek wody.",
                            "target_lang_translations": {
                                "ru": "Лаборатория выполнила химический <w>анализ</w> взятых проб воды."
                            }
                        }
                    ],
                    "antonyms": [
                        "synteza"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e3ea072-a5d8-39f2-a62d-ee1ae2da4a7f",
                    "sense_definition": "Украшение в виде тонкой гибкой ленты из металлических звеньев, которое обычно носят на шее.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "jewelry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На день рождения ей подарили красивую золотую <w>цепочку</w>.",
                            "target_lang_translations": {
                                "en": "For her birthday, she was given a beautiful gold <w>chain</w>."
                            }
                        },
                        {
                            "text": "У него на шее висела серебряная <w>цепочка</w> с крестиком.",
                            "target_lang_translations": {
                                "en": "A silver <w>chain</w> with a small cross hung around his neck."
                            }
                        }
                    ],
                    "common_phrases": [
                        "золотая цепочка",
                        "серебряная цепочка",
                        "цепочка с кулоном",
                        "надеть цепочку"
                    ],
                    "target_lang_definitions": {
                        "en": "A piece of jewelry in the form of a thin flexible band of metal links, usually worn around the neck."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chain",
                                "target_lang_sense_clarification": "jewelry"
                            },
                            {
                                "target_lang_word": "necklace",
                                "target_lang_sense_clarification": "jewelry worn around the neck"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d2ed98d-090e-3901-86ae-6ef387df30d1",
                    "sense_definition": "Уменьшительная форма слова <w>цепь</w>; ряд соединенных между собой звеньев, предметов или последовательность событий.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Богданка не услышал, а скорее почувствовал, с каким трудом руки Владимира справляются с дверной <w>цепочкой</w>.",
                            "target_lang_translations": {
                                "en": "Bogdanka didn't hear, but rather felt, with what difficulty Vladimir's hands were managing the door <w>chain</w>."
                            }
                        },
                        {
                            "text": "Странная <w>цепочка</w> событий привела нас в этот незнакомый город.",
                            "target_lang_translations": {
                                "en": "A strange <w>chain</w> of events led us to this unfamiliar city."
                            }
                        },
                        {
                            "text": "Вдоль дороги тянулась длинная <w>цепочка</w> следов.",
                            "target_lang_translations": {
                                "en": "A long <w>chain</w> of footprints stretched along the road."
                            }
                        }
                    ],
                    "synonyms": [
                        "ряд",
                        "вереница",
                        "череда",
                        "последовательность"
                    ],
                    "common_phrases": [
                        "цепочка событий",
                        "дверная цепочка",
                        "живая цепочка",
                        "выстроиться в цепочку"
                    ],
                    "target_lang_definitions": {
                        "en": "A diminutive form of 'chain'; a row of interconnected links, objects, or a sequence of events."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chain",
                                "target_lang_sense_clarification": "connected series or physical object"
                            },
                            {
                                "target_lang_word": "series",
                                "target_lang_sense_clarification": "sequence of events"
                            },
                            {
                                "target_lang_word": "string",
                                "target_lang_sense_clarification": "line of objects"
                            }
                        ]
                    }
                }
            ]
        }
    ]

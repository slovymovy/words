                    ],
                    "common_phrases": [
                        "zapaść krążeniowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "termin medyczny, ale używany powszechnie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Острая сосудистая недостаточность, сопровождающаяся падением давления"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коллапс",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                }
            ]
        }

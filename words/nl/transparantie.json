    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cebe1ead-210f-33b3-bbcb-79166bf64ade",
                    "sense_definition": "De mate van openheid en duidelijkheid over handelingen, besluiten of processen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf beloofde volledige <w>transparantie</w> over de gemaakte kosten.",
                            "target_lang_translations": {
                                "en": "The company promised full <w>transparency</w> regarding the costs incurred.",
                                "ru": "Компания пообещала полную <w>прозрачность</w> в отношении понесенных расходов."
                            }
                        },
                        {
                            "text": "De kiezers eisen meer <w>transparantie</w> van de regering.",
                            "target_lang_translations": {
                                "en": "The voters demand more <w>transparency</w> from the government.",
                                "ru": "Избиратели требуют от правительства большей <w>прозрачности</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "volledige transparantie",
                        "gebrek aan transparantie"
                    ],
                    "target_lang_definitions": {
                        "en": "The degree of openness and clarity regarding actions, decisions, or processes.",
                        "ru": "Степень открытости и ясности в отношении действий, решений или процессов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transparency",
                                "target_lang_sense_clarification": "openness/accountability"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "открытость действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80427d99-b25a-3442-9bd4-865501a6cb51",
                    "sense_definition": "De eigenschap van een materiaal om licht door te laten zodat men erdoorheen kan kijken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_transparency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de hoge <w>transparantie</w> van het glas lijkt het alsof er niets tussen zit.",
                            "target_lang_translations": {
                                "en": "Due to the high <w>transparency</w> of the glass, it seems as if there is nothing in between.",
                                "ru": "Из-за высокой <w>прозрачности</w> стекла кажется, что между нами ничего нет."
                            }
                        },
                        {
                            "text": "De <w>transparantie</w> van het water in dit meer is bijzonder groot.",
                            "target_lang_translations": {
                                "en": "The <w>transparency</w> of the water in this lake is particularly high.",
                                "ru": "<w>Прозрачность</w> воды в этом озере особенно высока."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorzichtigheid",
                        "doorzichtbaarheid"
                    ],
                    "common_phrases": [
                        "hoge transparantie"
                    ],
                    "target_lang_definitions": {
                        "en": "The property of a material to allow light to pass through so that one can see through it.",
                        "ru": "Свойство материала пропускать свет, позволяя видеть сквозь него."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transparency",
                                "target_lang_sense_clarification": "physical quality"
                            },
                            {
                                "target_lang_word": "clarity",
                                "target_lang_sense_clarification": "clearness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "физическое свойство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3fc3086-53c7-38b9-be24-4930a4548681",
                    "sense_definition": "De verhouding van de intensiteit van het doorgelaten licht tot die van het opvallende licht.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_transparency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>transparantie</w> van het materiaal werd in het laboratorium gemeten.",
                            "target_lang_translations": {
                                "en": "The <w>transparency</w> of the material was measured in the laboratory.",
                                "ru": "<w>Прозрачность</w> материала была измерена в лаборатории."
                            }
                        },
                        {
                            "text": "Voor deze lens is een optimale <w>transparantie</w> vereist.",
                            "target_lang_translations": {
                                "en": "Optimal <w>transparency</w> is required for this lens.",
                                "ru": "Для этой линзы требуется оптимальная <w>прозрачность</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lichtdoorlaatbaarheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The ratio of the intensity of transmitted light to that of incident light.",
                        "ru": "Отношение интенсивности прошедшего света к интенсивности падающего света."
                    },
                    "translations": {

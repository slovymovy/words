            "senses": [
                {
                    "sense_id": "e1262da8-6860-3c69-badf-56c5daf8156c",
                    "sense_definition": "A city in Alameda County, California, famous for its university and history of political activism.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "places",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They moved to <w>Berkeley</w> to enjoy the vibrant cultural scene.",
                            "target_lang_translations": {
                                "ru": "Они переехали в <w>Беркли</w>, чтобы насладиться яркой культурной жизнью."
                            }
                        },
                        {
                            "text": "The protests in <w>Berkeley</w> during the 1960s are historically significant.",
                            "target_lang_translations": {
                                "ru": "Протесты в <w>Беркли</w> в 1960-х годах имеют историческое значение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "downtown Berkeley",
                        "city of Berkeley"
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в округе Аламида, штат Калифорния, известный своим университетом и историей политического активизма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Беркли",
                                "target_lang_sense_clarification": "город в Калифорнии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f3ed952-fc22-3761-b9a9-2963fef8687a",
                    "sense_definition": "The University of California, Berkeley; a prestigious public research university.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "organizations",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "She received a scholarship to study physics at <w>Berkeley</w>.",
                            "target_lang_translations": {
                                "ru": "Она получила стипендию для изучения физики в <w>Беркли</w>."
                            }
                        },
                        {
                            "text": "Many Nobel Prize winners have taught at <w>Berkeley</w> over the years.",
                            "target_lang_translations": {
                                "ru": "Многие лауреаты Нобелевской премии преподавали в <w>Беркли</w> на протяжении многих лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "Cal",
                        "UC Berkeley"
                    ],
                    "common_phrases": [
                        "at Berkeley",
                        "Berkeley campus"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for University of California, Berkeley"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Калифорнийский университет в Беркли; престижный государственный исследовательский университет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Беркли",
                                "target_lang_sense_clarification": "университет"
                            }
                        ]

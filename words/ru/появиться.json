    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f123b27d-0eb4-32ca-84df-d5643198f379",
                    "sense_definition": "Стать видимым, показаться взгляду или прийти в какое-либо место.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за густых облаков наконец <появилось> солнце.",
                            "target_lang_translations": {
                                "en": "The sun finally <appeared> from behind the thick clouds."
                            }
                        },
                        {
                            "text": "Мой друг неожиданно <появился> в дверях комнаты.",
                            "target_lang_translations": {
                                "en": "My friend unexpectedly <appeared> in the doorway of the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "показаться",
                        "явиться"
                    ],
                    "antonyms": [
                        "скрыться",
                        "исчезнуть",
                        "пропасть"
                    ],
                    "common_phrases": [
                        "неожиданно появиться",
                        "появиться на пороге",
                        "появиться на публике"
                    ],
                    "target_lang_definitions": {
                        "en": "To become visible, come into view, or arrive at a place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to become visible or come into sight"
                            },
                            {
                                "target_lang_word": "show up",
                                "target_lang_sense_clarification": "to arrive, often unexpectedly (informal)"
                            },
                            {
                                "target_lang_word": "emerge",
                                "target_lang_sense_clarification": "to come out from a hidden place or background"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69aa5111-baf6-3887-bd91-2b8a8efdaf54",
                    "sense_definition": "Возникнуть, образоваться, начать существовать (о чувствах, мыслях, проблемах или объектах).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У нас <появилась> отличная возможность поехать в отпуск.",
                            "target_lang_translations": {
                                "en": "An excellent opportunity to go on vacation <appeared> for us."
                            }
                        },
                        {
                            "text": "После долгого разговора у него <появились> сомнения.",
                            "target_lang_translations": {
                                "en": "After a long conversation, doubts <arose> in his mind."
                            }
                        }
                    ],
                    "synonyms": [
                        "возникнуть",
                        "образоваться",
                        "начаться"
                    ],
                    "antonyms": [
                        "исчезнуть",
                        "пропасть"
                    ],
                    "common_phrases": [
                        "появилась идея",
                        "появилась проблема",
                        "появилась надежда",
                        "появился вопрос"
                    ],
                    "target_lang_definitions": {
                        "en": "To come into existence, start to exist, or form (of feelings, thoughts, problems, or objects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to begin to exist or be present"
                            },
                            {
                                "target_lang_word": "arise",
                                "target_lang_sense_clarification": "to happen or occur (especially of problems, questions, or feelings)"
                            },
                            {
                                "target_lang_word": "emerge",
                                "target_lang_sense_clarification": "to become known or develop"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "333334dc-2f93-37c0-9f61-f1a4dca4e199",
                    "sense_definition": "Выйти из печати, быть опубликованным или стать доступным для зрителей.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его новая статья <появилась> в научном журнале.",
                            "target_lang_translations": {
                                "en": "His new article <appeared> in a scientific journal."
                            }
                        },
                        {
                            "text": "Этот фильм скоро <появится> на экранах кинотеатров.",
                            "target_lang_translations": {
                                "en": "This movie will soon <appear> on cinema screens."
                            }
                        }
                    ],
                    "synonyms": [
                        "выйти"
                    ],
                    "common_phrases": [
                        "появиться в печати",
                        "появиться в продаже",
                        "книга появилась"
                    ],
                    "target_lang_definitions": {
                        "en": "To be published, released, or become available to the public."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appear",
                                "target_lang_sense_clarification": "to be published or become available in media"
                            },
                            {
                                "target_lang_word": "come out",
                                "target_lang_sense_clarification": "to be released (of books, movies, or products)"
                            },
                            {
                                "target_lang_word": "be released",
                                "target_lang_sense_clarification": "to be made available for sale or viewing"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
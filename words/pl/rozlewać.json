    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "83afc70e-dfa1-3774-b774-caa982efc258",
                    "sense_definition": "Powodować wylanie się płynu z naczynia lub poza jego obręb, często w sposób przypadkowy i niekontrolowany.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spilling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe dziecko często <rozlewa> sok, gdy próbuje pić z dużego kubka.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок часто <проливает> сок, когда пытается пить из большой чашки."
                            }
                        },
                        {
                            "text": "Uważaj, żebyś nie <rozlewał> wody na podłogę podczas mycia naczyń.",
                            "target_lang_translations": {
                                "ru": "Смотри, не <разливай> воду на пол, когда моешь посуду."
                            }
                        }
                    ],
                    "synonyms": [
                        "wylewać",
                        "chlapać"
                    ],
                    "common_phrases": [
                        "rozlewać mleko",
                        "rozlewać wodę",
                        "rozlewać na stół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случайно выливать жидкость из сосуда или мимо него, часто неконтролируемо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проливать",
                                "target_lang_sense_clarification": "случайно выливать жидкость"
                            },
                            {
                                "target_lang_word": "разливать",
                                "target_lang_sense_clarification": "лить мимо или на поверхность, создавая лужу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1ac2340-85ea-341c-8a75-240c5288ff4c",
                    "sense_definition": "Napełniać płynem wiele naczyń jedno po drugim, dzieląc go na porcje.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "serving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner zaczął sprawnie <rozlewać> czerwone wino do kieliszków gości.",
                            "target_lang_translations": {
                                "ru": "Официант начал ловко <разливать> красное вино по бокалам гостей."
                            }
                        },
                        {
                            "text": "Moja babcia zawsze sprawiedliwie <rozlewa> zupę do talerzy wszystkich wnuków.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка всегда справедливо <разливает> суп по тарелкам всех внуков."
                            }
                        }
                    ],
                    "synonyms": [
                        "nalewać",
                        "serwować"
                    ],
                    "common_phrases": [
                        "rozlewać zupę",
                        "rozlewać wino",
                        "rozlewać do kieliszków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наполнять жидкостью несколько сосудов один за другим, разделяя её на порции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разливать",
                                "target_lang_sense_clarification": "распределять жидкость по ёмкостям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e69c0dc1-7b79-34fa-896c-fdff2eef413c",
                    "sense_definition": "O cieczy: płynąć swobodnie po powierzchni, zajmując coraz większy obszar (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spreading_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gęsty olej powoli <rozlewa się> po asfalcie po wypadku samochodowym.",
                            "target_lang_translations": {
                                "ru": "Густое масло медленно <растекается> по асфальту после автомобильной аварии."
                            }
                        },
                        {
                            "text": "Woda z pękniętej rury szybko <rozlewa się> po całej łazience.",
                            "target_lang_translations": {
                                "ru": "Вода из лопнувшей трубы быстро <разливается> по всей ванной."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpływać się"
                    ],
                    "common_phrases": [
                        "plama się rozlewa",
                        "woda się rozlewa",
                        "rozlewać się po podłodze"
                    ],
                    "target_lang_definitions": {
                        "ru": "О жидкости: свободно течь по поверхности, занимая всё большую площадь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растекаться",
                                "target_lang_sense_clarification": "расплываться по поверхности"
                            },
                            {
                                "target_lang_word": "разливаться",
                                "target_lang_sense_clarification": "течь во все стороны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79b8eca3-31b6-30a7-8c5f-84e093cbdcca",
                    "sense_definition": "O rzece lub zbiorniku wodnym: występować z brzegów i zalewać okoliczne tereny (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "overflowing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiosną ta górska rzeka często <rozlewa się> na pobliskie łąki i pola.",
                            "target_lang_translations": {
                                "ru": "Весной эта горная река часто <разливается> на близлежащие луга и поля."
                            }
                        },
                        {
                            "text": "Wisła w tym miejscu szeroko <rozlewa się>, tworząc malowniczy krajobraz.",
                            "target_lang_translations": {
                                "ru": "Висла в этом месте широко <разливается>, создавая живописный пейзаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "wylewać"
                    ],
                    "common_phrases": [
                        "rzeka się rozlewa",
                        "szeroko się rozlewać"
                    ],
                    "target_lang_definitions": {
                        "ru": "О реке или водоёме: выходить из берегов и затапливать окрестности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разливаться",
                                "target_lang_sense_clarification": "выходить из берегов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b40663f4-be10-3ebc-912e-3dfcf3e772d3",
                    "sense_definition": "Rozprzestrzeniać się w przestrzeni lub obejmować swoim zasięgiem duży obszar (dotyczy np. dźwięku, światła, zapachu, uczuć lub widoków; używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_spread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przyjemne ciepło <rozlewa się> po moim ciele po wypiciu gorącej herbaty.",
                            "target_lang_translations": {
                                "ru": "Приятное тепло <разливается> по моему телу после того, как я выпью горячего чая."
                            }
                        },
                        {
                            "text": "Z tego wzgórza piękny widok <rozlewa się> na całą dolinę i miasto.",
                            "target_lang_translations": {
                                "ru": "С этого холма красивый вид <раскидывается> на всю долину и город."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozprzestrzeniać się",
                        "ogarniać"
                    ],
                    "common_phrases": [
                        "ciepło się rozlewa",
                        "widok się rozlewa",
                        "muzyka się rozlewa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространяться в пространстве или охватывать большую территорию (о звуке, свете, чувствах или видах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разливаться",
                                "target_lang_sense_clarification": "о чувствах, звуках, свете"
                            },
                            {
                                "target_lang_word": "растекаться",
                                "target_lang_sense_clarification": "о мыслях или ощущениях"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
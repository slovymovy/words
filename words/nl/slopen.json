                        "afmatten",
                        "vermoeien",
                        "opbranden"
                    ],
                    "antonyms": [
                        "verkwikken",
                        "opladen"
                    ],
                    "common_phrases": [
                        "iemand slopen",
                        "helemaal gesloopt zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely exhaust someone physically or mentally"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exhaust",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "wear out",
                                "target_lang_sense_clarification": "to make someone very tired"
                            },
                            {
                                "target_lang_word": "wreck",
                                "target_lang_sense_clarification": "colloquial, extreme fatigue"
                            }
                        ]

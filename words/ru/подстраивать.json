            "pos": "verb",
            "senses": [
                {
                    "sense_id": "00543812-0e07-3a44-9498-87399381b717",
                    "sense_definition": "Тайно и намеренно организовывать что-либо, создавать ситуацию (часто неприятную или обманную) для достижения своих целей.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "plotting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Злоумышленники пытались <w>подстраивать</w> несчастные случаи, чтобы получить страховку.",
                            "target_lang_translations": {
                                "en": "The criminals tried to <w>stage</w> accidents to get insurance."
                            }
                        },
                        {
                            "text": "Он любил <w>подстраивать</w> своим друзьям безобидные, но неожиданные розыгрыши.",
                            "target_lang_translations": {
                                "en": "He loved to <w>set up</w> harmless but unexpected pranks for his friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "устраивать",
                        "организовывать",
                        "фальсифицировать"
                    ],
                    "common_phrases": [
                        "подстраивать ловушку",
                        "подстраивать аварию",
                        "подстраивать результаты",
                        "подстраивать козни"
                    ],
                    "target_lang_definitions": {
                        "en": "To secretly arrange or manipulate a situation, often for deceptive purposes or to entrap someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stage",
                                "target_lang_sense_clarification": "an event or accident"
                            },
                            {
                                "target_lang_word": "rig",
                                "target_lang_sense_clarification": "results or outcomes"
                            },
                            {
                                "target_lang_word": "set up",
                                "target_lang_sense_clarification": "a trap or situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2a04717-5d5e-33b3-a0ff-21276d3cc594",
                    "sense_definition": "Регулировать механизм, инструмент или устройство для точного соответствия нужному стандарту, частоте или звучанию.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "adjustment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Музыканту приходилось постоянно <w>подстраивать</w> гитару из-за высокой влажности в зале.",
                            "target_lang_translations": {
                                "en": "The musician had to constantly <w>tune</w> the guitar due to the high humidity in the hall."
                            }
                        },
                        {
                            "text": "Оператор должен внимательно <w>подстраивать</w> оборудование для получения чистого сигнала.",
                            "target_lang_translations": {
                                "en": "The operator must carefully <w>adjust</w> the equipment to receive a clear signal."
                            }
                        }
                    ],
                    "synonyms": [
                        "настраивать",
                        "регулировать",
                        "корректировать"
                    ],
                    "antonyms": [
                        "расстраивать"
                    ],
                    "common_phrases": [
                        "подстраивать инструмент",
                        "подстраивать частоту",
                        "подстраивать звук"
                    ],
                    "target_lang_definitions": {
                        "en": "To make small adjustments to a mechanism or instrument for optimal performance or correct pitch."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "adjust",
                                "target_lang_sense_clarification": "a mechanism or device"
                            },
                            {
                                "target_lang_word": "tune",
                                "target_lang_sense_clarification": "a musical instrument or radio"
                            },
                            {
                                "target_lang_word": "fine-tune",
                                "target_lang_sense_clarification": "make precise adjustments"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c824e369-453c-3ab4-9cad-953647df9f04",
                    "sense_definition": "Возводить сооружение вплотную к другому зданию или добавлять новые части к уже существующему строению.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Владельцы дома решили <w>подстраивать</w> к нему просторную веранду.",
                            "target_lang_translations": {
                                "en": "The house owners decided to <w>build on</w> a spacious veranda to it."
                            }
                        },
                        {
                            "text": "Архитектор запретил <w>подстраивать</w> новые помещения без укрепления фундамента.",
                            "target_lang_translations": {
                                "en": "The architect forbade <w>adding on</w> new rooms without strengthening the foundation."
                            }
                        }
                    ],
                    "synonyms": [
                        "пристраивать",
                        "достраивать"
                    ],
                    "common_phrases": [
                        "подстраивать дом",
                        "подстраивать флигель"
                    ],
                    "target_lang_definitions": {
                        "en": "To construct an extension or addition attached to an existing building."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "build on",
                                "target_lang_sense_clarification": "add a structure to an existing one"
                            },
                            {
                                "target_lang_word": "add on",
                                "target_lang_sense_clarification": "extend a building"
                            }
                        ]
                    }
                }
            ]
        }
    ]

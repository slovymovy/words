    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4f235b3f-9104-3846-9dc2-7e1a108e2995",
                    "sense_definition": "Восторженный почитатель кого-либо или чего-либо, страстный любитель какого-либо вида искусства или деятельности.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "enthusiast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой отец — большой <w>поклонник</w> классической музыки и оперы.",
                            "target_lang_translations": {
                                "en": "My father is a big <w>fan</w> of classical music and opera."
                            }
                        },
                        {
                            "text": "После концерта толпа верных <w>поклонников</w> ждала артиста у выхода.",
                            "target_lang_translations": {
                                "en": "After the concert, a crowd of loyal <w>fans</w> waited for the artist at the exit."
                            }
                        }
                    ],
                    "synonyms": [
                        "ухажёр",
                        "воздыхатель",
                        "хахаль"
                    ],
                    "antonyms": [
                        "ненавистник",
                        "противник"
                    ],
                    "common_phrases": [
                        "большой поклонник",
                        "ярый поклонник",
                        "поклонник таланта",
                        "поклонник театра"
                    ],
                    "target_lang_definitions": {
                        "en": "An enthusiastic admirer of someone or something, or a passionate lover of a specific art form or activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fan",
                                "target_lang_sense_clarification": "enthusiast of a celebrity, sport, or art"
                            },
                            {
                                "target_lang_word": "admirer",
                                "target_lang_sense_clarification": "one who holds someone in high esteem"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4bdbd63-187d-3709-8a0d-ca7278112a8f",
                    "sense_definition": "Мужчина, который ухаживает за женщиной, проявляя к ней симпатию или влюблённость.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "suitor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этой красивой актрисы появился новый богатый <w>поклонник</w>.",
                            "target_lang_translations": {
                                "en": "This beautiful actress has a new rich <w>admirer</w>."
                            }
                        },
                        {
                            "text": "Она получила цветы и записку, но её тайный <w>поклонник</w> не подписался.",
                            "target_lang_translations": {
                                "en": "She received flowers and a note, but her secret <w>admirer</w> did not sign it."
                            }
                        }
                    ],
                    "synonyms": [
                        "ухажёр",
                        "воздыхатель"
                    ],
                    "common_phrases": [
                        "тайный поклонник",
                        "назойливый поклонник",
                        "отвергнуть поклонника"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A man who courts a woman, showing romantic interest or affection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "admirer",
                                "target_lang_sense_clarification": "romantic interest"
                            },
                            {
                                "target_lang_word": "suitor",
                                "target_lang_sense_clarification": "man courting a woman"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44b087ba-0c9d-36c3-9b5d-68337e66bc2b",
                    "sense_definition": "Человек, который поклоняется какому-либо божеству, святыне или высшей силе.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый истинный <w>поклонник</w> этого культа должен совершать молитву на рассвете.",
                            "target_lang_translations": {
                                "en": "Every true <w>worshipper</w> of this cult must perform a prayer at dawn."
                            }
                        },
                        {
                            "text": "Древний храм был разрушен, и <w>поклонники</w> старых богов ушли в леса.",
                            "target_lang_translations": {
                                "en": "The ancient temple was destroyed, and the <w>worshippers</w> of the old gods left for the forests."
                            }
                        }
                    ],
                    "synonyms": [
                        "воздыхатель"
                    ],
                    "common_phrases": [
                        "ревностный поклонник",
                        "поклонник идолов",
                        "поклонник культа"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who worships a deity, shrine, or higher power."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "worshipper",
                                "target_lang_sense_clarification": "religious context"
                            }
                        ]
                    }
                }
            ]
        }
    ]

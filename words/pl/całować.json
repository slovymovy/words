                {
                    "sense_id": "63d1e0f1-7c1c-3b81-a803-e0914ab69488",
                    "sense_definition": "Dotykać kogoś lub czegoś ustami, wyrażając w ten sposób uczucia, przyjaźń lub szacunek.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kissing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna szarmancko <całował> kobietę w dłoń na powitanie.",
                            "target_lang_translations": {
                                "ru": "Мужчина галантно <w>целовал</w> женщине руку в знак приветствия."
                            }
                        },
                        {
                            "text": "Dziecko zawsze <całuje> mamę w policzek na dobranoc.",
                            "target_lang_translations": {
                                "ru": "Ребенок всегда <w>целует</w> маму в щеку перед сном."
                            }
                        }
                    ],
                    "synonyms": [
                        "cmokać"
                    ],
                    "common_phrases": [
                        "całować w rękę",
                        "całować w policzek",
                        "całować po rękach",
                        "całować klamkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикасаться к кому-либо или чему-либо губами, выражая таким образом чувства, дружбу или уважение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целовать",
                                "target_lang_sense_clarification": "кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d1bac53-937e-3a07-aa53-3464dbf427f5",
                    "sense_definition": "Dotykać się wzajemnie ustami (czynność zwrotna, wymaga użycia zaimka „się”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kissing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zakochana para <całowała> się na ławce w parku.",
                            "target_lang_translations": {
                                "ru": "Влюбленная пара <w>целовалась</w> на скамейке в парке."
                            }
                        },
                        {
                            "text": "Na końcu filmu bohaterowie zaczęli się namiętnie <całować>.",
                            "target_lang_translations": {
                                "ru": "В конце фильма герои начали страстно <w>целоваться</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "całować się namiętnie",
                        "całować się z języczkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикасаться друг к другу губами (взаимное действие)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целоваться",
                                "target_lang_sense_clarification": "друг друга"

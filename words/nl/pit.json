    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "568e2749-1cee-30bf-a983-8c39e7547fff",
                    "sense_definition": "De harde kern in het midden van bepaalde vruchten die het zaad bevat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "seed_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op dat je de <w>pit</w> van de kers niet inslikt.",
                            "target_lang_translations": {
                                "ru": "Осторожно, не проглоти вишневую <w>косточку</w>."
                            }
                        },
                        {
                            "text": "In deze olijven zit nog een harde <w>pit</w>.",
                            "target_lang_translations": {
                                "ru": "В этих оливках все еще есть твердая <w>косточка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "steen"
                    ],
                    "common_phrases": [
                        "kersen met pit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердая сердцевина в центре некоторых фруктов, содержащая семя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "косточка",
                                "target_lang_sense_clarification": "фрукта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69c77622-4965-3423-aa42-48ba5fe1d794",
                    "sense_definition": "Een brander op een gasfornuis waarop gekookt wordt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fire_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet de soep maar op de kleinste <w>pit</w> om warm te blijven.",
                            "target_lang_translations": {
                                "ru": "Поставь суп на самую маленькую <w>конфорку</w>, чтобы он оставался теплым."
                            }
                        },
                        {
                            "text": "We hebben een fornuis met vijf <w>pitten</w> in de keuken.",
                            "target_lang_translations": {
                                "ru": "У нас на кухне плита с пятью <w>конфорками</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op een laag pitje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горелка на газовой плите, используемая для приготовления пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конфорка",
                                "target_lang_sense_clarification": "плиты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6b9de60-04ca-3220-b4d0-aaa874fa13b1",
                    "sense_definition": "Energie, levendigheid of een krachtig karakter.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "energy_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is een vrouw met veel <w>pit</w> en doorzettingsvermogen.",
                            "target_lang_translations": {
                                "ru": "Она женщина с <w>характером</w> и упорством."
                            }
                        },
                        {
                            "text": "Er zit helaas weinig <w>pit</w> in deze saaie voorstelling.",
                            "target_lang_translations": {
                                "ru": "К сожалению, в этом скучном представлении мало <w>огонька</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "met pit",
                        "pit hebben"
                    ],
                    "target_lang_definitions": {
                        "ru": "Энергия, живость или сильный характер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огонек",
                                "target_lang_sense_clarification": "живость, энергия"
                            },
                            {
                                "target_lang_word": "характер",
                                "target_lang_sense_clarification": "сила духа"
                            },
                            {
                                "target_lang_word": "изюминка",
                                "target_lang_sense_clarification": "особенность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d84a84f-f11b-35f1-bf36-06fb34ccdc44",
                    "sense_definition": "Een gevlochten katoenen draad in een kaars of olielamp die de vlam voedt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fire_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als de <w>pit</w> te lang is, gaat de kaars walmen.",
                            "target_lang_translations": {
                                "ru": "Если <w>фитиль</w> слишком длинный, свеча начинает коптить."
                            }
                        },
                        {
                            "text": "Je moet de <w>pit</w> aansteken met een lucifer of aansteker.",
                            "target_lang_translations": {
                                "ru": "Нужно поджечь <w>фитиль</w> спичкой или зажигалкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "wiek",
                        "lont"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плетеная хлопковая нить в свече или масляной лампе, поддерживающая пламя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фитиль",
                                "target_lang_sense_clarification": "свечи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7549bf7d-6d8c-324d-9abe-c23133133469",
                    "sense_definition": "De werkplaats langs een racecircuit waar auto's stoppen voor onderhoud of brandstof.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "racing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De coureur reed snel de <w>pit</w> in voor nieuwe banden.",
                            "target_lang_translations": {
                                "ru": "Гонщик быстро заехал в <w>боксы</w> за новыми шинами."
                            }
                        },
                        {
                            "text": "Het team in de <w>pit</w> staat klaar voor een snelle wissel.",
                            "target_lang_translations": {
                                "ru": "Команда в <w>боксах</w> готова к быстрой замене."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место на гоночной трассе, где машины останавливаются для техобслуживания или дозаправки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "боксы",
                                "target_lang_sense_clarification": "в автогонках"
                            },
                            {
                                "target_lang_word": "пит-лейн",
                                "target_lang_sense_clarification": "часть трассы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7c907b0b-8e47-3c63-bfe8-a850e8392a08",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>pitten</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>pit</w> de kersen meestal met een speciaal apparaatje.",
                            "target_lang_translations": {
                                "ru": "Я обычно <w>вынимаю косточки</w> из вишен специальным прибором."
                            }
                        },
                        {
                            "text": "Ik <w>pit</w> vanavond lekker vroeg omdat ik moe ben.",
                            "target_lang_translations": {
                                "ru": "Я сегодня <w>лягу спать</w> пораньше, потому что устал."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола pitten."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вынимать косточки",
                                "target_lang_sense_clarification": "удалять сердцевину"
                            },
                            {
                                "target_lang_word": "спать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "878f1af7-7006-386b-a6eb-8f44133146bf",
                    "sense_definition": "Gebiedende wijs van <w>pitten</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Pit</w> die olijven eerst voordat je ze in de salade doet.",
                            "target_lang_translations": {
                                "ru": "Сначала <w>вынь косточки</w> из оливок, прежде чем класть их в салат."
                            }
                        },
                        {
                            "text": "<w>Pit</w> lekker lang uit in het weekend.",
                            "target_lang_translations": {
                                "ru": "<w>Отоспись</w> как следует в выходные."
                            }
                        }
                    ],
                    "traits": [

                        {
                            "text": "Его <приняли> вчера вечером за нарушение общественного порядка.",
                            "target_lang_translations": {
                                "en": "He was <picked up> last night for disturbing the peace."
                            }
                        }
                    ],
                    "synonyms": [
                        "взять"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be arrested or detained by the police."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arrest",
                                "target_lang_sense_clarification": "a criminal"
                            },
                            {
                                "target_lang_word": "pick up",
                                "target_lang_sense_clarification": "slang for arrest"
                            }
                        ]
                    }
                }
            ]
        }

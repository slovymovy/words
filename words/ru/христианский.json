            "senses": [
                {
                    "sense_id": "ac88628b-7088-3489-a7d7-b2f2d52d8a4c",
                    "sense_definition": "Связанный с христианством как религией, относящийся к христианам, их культуре, обычаям или церкви.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "religion_rel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В центре города стоит старый <w>христианский</w> храм.",
                            "target_lang_translations": {
                                "en": "An old <w>Christian</w> church stands in the city center."
                            }
                        },
                        {
                            "text": "<w>Христианская</w> традиция призывает помогать бедным и нуждающимся.",
                            "target_lang_translations": {
                                "en": "<w>Christian</w> tradition calls for helping the poor and needy."
                            }
                        }
                    ],
                    "antonyms": [
                        "антихристианский",
                        "нехристианский"
                    ],
                    "common_phrases": [
                        "христианская вера",
                        "христианская церковь",
                        "христианский мир",
                        "христианские праздники",
                        "христианская мораль"
                    ],
                    "target_lang_definitions": {
                        "en": "Connected with Christianity as a religion, relating to Christians, their culture, customs, or church."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Christian",
                                "target_lang_sense_clarification": "relating to the religion or its followers"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a516a0f0-1d9c-394b-95cd-ef28171b5810",
                    "sense_definition": "Умеренный, щадящий, справедливый или человечный; такой, который можно терпеть (часто употребляется иронично по отношению к ценам, условиям или требованиям).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом магазине на продукты вполне <w>христианские</w> цены.",
                            "target_lang_translations": {
                                "en": "The prices for groceries in this store are quite <w>reasonable</w>."
                            }
                        },
                        {
                            "text": "Хозяин квартиры сделал нам <w>христианскую</w> скидку, учитывая наши обстоятельства.",
                            "target_lang_translations": {
                                "en": "The landlord gave us a <w>decent</w> discount, considering our circumstances."
                            }
                        }
                    ],
                    "synonyms": [
                        "божеский",
                        "умеренный",
                        "человеческий"
                    ],
                    "common_phrases": [
                        "христианские цены",
                        "христианский поступок"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Moderate, sparing, fair, or humane; tolerable (often used ironically regarding prices, conditions, or requirements)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reasonable",
                                "target_lang_sense_clarification": "regarding prices or demands"
                            },
                            {
                                "target_lang_word": "fair",
                                "target_lang_sense_clarification": "just or appropriate"
                            },
                            {
                                "target_lang_word": "decent",
                                "target_lang_sense_clarification": "acceptable or satisfactory"
                            },
                            {
                                "target_lang_word": "humane",
                                "target_lang_sense_clarification": "showing compassion"
                            }
                        ]
                    }
                }
            ]

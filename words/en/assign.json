{
    "word_family": [
        "assignment",
        "assignee",
        "assignor",
        "reassign",
        "unassigned",
        "assignable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "81c8b667-04d0-3758-a832-656c07ab3323",
                    "sense_definition": "To give someone a particular job, task, or duty to do.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager decided to <w>assign</w> the new project to the most experienced team.",
                            "target_lang_translations": {
                                "ru": "Менеджер решил <w>поручить</w> новый проект самой опытной команде."
                            }
                        },
                        {
                            "text": "Our teacher will <w>assign</w> homework at the end of the lesson.",
                            "target_lang_translations": {
                                "ru": "Наш учитель <w>задаст</w> домашнее задание в конце урока."
                            }
                        }
                    ],
                    "synonyms": [
                        "allocate"
                    ],
                    "common_phrases": [
                        "assign a task",
                        "assign responsibility",
                        "assign a role"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать кому-либо определенную работу, задачу или обязанность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поручать",
                                "target_lang_sense_clarification": "давать задание или работу"
                            },
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "на должность или выполнение задачи"
                            },
                            {
                                "target_lang_word": "задавать",
                                "target_lang_sense_clarification": "домашнее задание или урок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2070a3b3-ef63-3619-a810-0c5aabe0987c",
                    "sense_definition": "To choose or set something apart for a specific purpose or use.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>assign</w> a specific date for the upcoming conference.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>назначить</w> конкретную дату для предстоящей конференции."
                            }
                        },
                        {
                            "text": "A parking space was <w>assigned</w> to each employee near the entrance.",
                            "target_lang_translations": {
                                "ru": "Каждому сотруднику было <w>выделено</w> парковочное место у входа."
                            }
                        }
                    ],
                    "synonyms": [
                        "allocate",
                        "set apart",
                        "earmark"
                    ],
                    "common_phrases": [
                        "assign a date",
                        "assign a room",
                        "assign a place"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбрать или выделить что-либо для конкретной цели или использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "дату, время или встречу"
                            },
                            {
                                "target_lang_word": "выделять",
                                "target_lang_sense_clarification": "место, ресурсы или средства"
                            },
                            {
                                "target_lang_word": "отводить",
                                "target_lang_sense_clarification": "место или роль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a997efdd-6438-31a1-bbb4-3390469fab39",
                    "sense_definition": "To decide that something has a particular value, quality, origin, or cause.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is often difficult to <w>assign</w> a precise cause to such complex economic changes.",
                            "target_lang_translations": {
                                "ru": "Часто трудно <w>приписать</w> точную причину таким сложным экономическим изменениям."
                            }
                        },
                        {
                            "text": "Historians generally <w>assign</w> this ancient manuscript to the twelfth century.",
                            "target_lang_translations": {
                                "ru": "Историки обычно <w>относят</w> эту древнюю рукопись к двенадцатому веку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "assign a value",
                        "assign blame",
                        "assign a cause"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решить, что что-либо имеет определенную ценность, качество, происхождение или причину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приписывать",
                                "target_lang_sense_clarification": "причину, качество или значение"
                            },
                            {
                                "target_lang_word": "относить",
                                "target_lang_sense_clarification": "к определенному периоду или категории"
                            },
                            {
                                "target_lang_word": "устанавливать",
                                "target_lang_sense_clarification": "причину или факт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54683505-7937-36c4-9650-b109aca5eb6c",
                    "sense_definition": "To give a specific value to a variable or memory location in computing or mathematics.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The program will <w>assign</w> a random number to the variable x.",
                            "target_lang_translations": {
                                "ru": "Программа <w>присвоит</w> случайное число переменной x."
                            }
                        },
                        {
                            "text": "You must define the variable before you can <w>assign</w> data to it.",
                            "target_lang_translations": {
                                "ru": "Вы должны определить переменную, прежде чем сможете <w>присвоить</w> ей данные."
                            }
                        }
                    ],
                    "common_phrases": [
                        "assign a value to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Присвоить определенное значение переменной или ячейке памяти в информатике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присваивать",
                                "target_lang_sense_clarification": "значение переменной"
                            },
                            {
                                "target_lang_word": "задавать",
                                "target_lang_sense_clarification": "параметр или значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0833dc99-e74d-3158-9a32-2506b46ebecb",
                    "sense_definition": "To transfer property, rights, or interests to another person legally.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tenant cannot <w>assign</w> the lease to a third party without the landlord's permission.",
                            "target_lang_translations": {
                                "ru": "Арендатор не может <w>переуступить</w> аренду третьему лицу без разрешения арендодателя."
                            }
                        },
                        {
                            "text": "He decided to <w>assign</w> all his intellectual property rights to the company.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>передать</w> все свои права на интеллектуальную собственность компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "transfer",
                        "convey",
                        "consign"
                    ],
                    "common_phrases": [
                        "assign rights",
                        "assign a lease"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридически передать имущество, права или интересы другому лицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "права или собственность"
                            },
                            {
                                "target_lang_word": "переуступать",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "280c8903-f250-3053-8585-e2119117b0dc",
                    "sense_definition": "A person to whom property or a legal right is transferred.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The contract is binding upon the parties and their respective heirs and <w>assigns</w>.",
                            "target_lang_translations": {
                                "ru": "Договор является обязательным для сторон, их соответствующих наследников и <w>правопреемников</w>."
                            }
                        },
                        {
                            "text": "He was named as the sole <w>assign</w> in the legal document regarding the estate.",
                            "target_lang_translations": {
                                "ru": "Он был назван единственным <w>правопреемником</w> в юридическом документе, касающемся поместья."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heirs and assigns",
                        "successors and assigns"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, которому передается имущество или законное право."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правопреемник",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "08d7fc25-bbe8-3992-9312-e0bbf9988149",
                    "sense_definition": "Miasto na prawach powiatu w południowej Polsce, w województwie śląskim.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "city",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Za jedenaście dni spotkamy się w <w>Chorzowie</w> na zimowym zlocie naszego stowarzyszenia.",
                            "target_lang_translations": {
                                "ru": "Через одиннадцать дней мы встретимся в <w>Хожуве</w> на зимнем слете нашей ассоциации."
                            }
                        },
                        {
                            "text": "Słynny Stadion Śląski znajduje się właśnie w <w>Chorzowie</w>.",
                            "target_lang_translations": {
                                "ru": "Знаменитый Силезский стадион находится именно в <w>Хожуве</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Królewska Huta"
                    ],
                    "common_phrases": [
                        "Stadion Śląski w Chorzowie",
                        "Park Śląski w Chorzowie",
                        "mieszkaniec Chorzowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Город на правах повята на юге Польши, в Силезском воеводстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хожув",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc04e19f-6750-3168-ad71-6c0078ee99f3",
                    "sense_definition": "Wieś w Polsce, w województwie podkarpackim, w powiecie jarosławskim.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "village",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Ten mały <w>Chorzów</w> leży w gminie Roźwienica na Podkarpaciu.",
                            "target_lang_translations": {
                                "ru": "Этот маленький <w>Хожув</w> находится в гмине Розьвеница в Подкарпатье."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie słyszałem o <w>Chorzowie</w> w powiecie jarosławskim.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не слышал о <w>Хожуве</w> в Ярославском повяте."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревня в Польше, в Подкарпатском воеводстве, в Ярославском повяте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хожув",
                                "target_lang_sense_clarification": "деревня"
                            }
                        ]
                    }
                }
            ]

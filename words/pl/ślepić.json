{
    "word_family": [
        "ślepnąć",
        "oślepiać",
        "oślepnąć",
        "ślepiec",
        "ślepota",
        "ślepy",
        "wyślepiać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "96b21712-aeb7-367f-982f-047311d36676",
                    "sense_definition": "Razić oczy silnym, jaskrawym światłem, utrudniając widzenie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "light_effect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zachodzące słońce mocno <ślepiło> kierowców jadących na zachód.",
                            "target_lang_translations": {
                                "ru": "Заходящее солнце сильно <слепило> водителей, едущих на запад."
                            }
                        },
                        {
                            "text": "Musisz ustawić światła w samochodzie, żeby nie <ślepić> innych.",
                            "target_lang_translations": {
                                "ru": "Ты должен отрегулировать фары в машине, чтобы не <слепить> других."
                            }
                        }
                    ],
                    "synonyms": [
                        "razić",
                        "oślepiać"
                    ],
                    "common_phrases": [
                        "słońce ślepi",
                        "światła ślepią"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раздражать глаза сильным, ярким светом, затрудняя видение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слепить",
                                "target_lang_sense_clarification": "ярким светом"
                            },
                            {
                                "target_lang_word": "ослеплять",
                                "target_lang_sense_clarification": "мешать видеть светом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0acaa4d-48d9-3a42-8ca8-7fc26b371fc9",
                    "sense_definition": "Wpatrywać się w coś intensywnie, często z wysiłkiem lub bezmyślnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vision_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie ma sensu <ślepić> w ten mały ekran przez całą noc.",
                            "target_lang_translations": {
                                "ru": "Нет смысла <пялиться> в этот маленький экран всю ночь."
                            }
                        },
                        {
                            "text": "Dlaczego tak <ślepiesz> oczami, czyżbyś zapomniał okularów?",
                            "target_lang_translations": {
                                "ru": "Почему ты так <щуришься>, ты что, забыл очки?"
                            }
                        }
                    ],
                    "synonyms": [
                        "gapić się",
                        "wpatrywać się",
                        "wyślepiać"
                    ],
                    "common_phrases": [
                        "ślepić w ekran",
                        "ślepić oczami"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пристально смотреть на что-то, часто с усилием или бессмысленно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пялиться",
                                "target_lang_sense_clarification": "разговорное; пристально смотреть"
                            },
                            {
                                "target_lang_word": "щуриться",
                                "target_lang_sense_clarification": "напрягать зрение"
                            },
                            {
                                "target_lang_word": "таращиться",
                                "target_lang_sense_clarification": "разговорное; смотреть широко открытыми глазами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10a37ef5-5f9b-3d3a-8bc7-69471744bcd3",
                    "sense_definition": "Odbierać zdolność rozsądnego myślenia i oceny sytuacji, zazwyczaj pod wpływem silnych emocji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_blindness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nienawiść <ślepiła> go tak bardzo, że nie widział krzywdy bliskich.",
                            "target_lang_translations": {
                                "ru": "Ненависть так сильно <ослепляла> его, что он не замечал боли близких."
                            }
                        },
                        {
                            "text": "Nie pozwól, by pycha cię <ślepiła> w podejmowaniu decyzji.",
                            "target_lang_translations": {
                                "ru": "Не позволяй гордыне <ослеплять> тебя при принятии решений."
                            }
                        }
                    ],
                    "synonyms": [
                        "oślepiać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишать способности разумно мыслить и оценивать ситуацию, обычно под влиянием сильных эмоций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ослеплять",
                                "target_lang_sense_clarification": "переносное значение; лишать рассудка"
                            },
                            {
                                "target_lang_word": "застилать глаза",
                                "target_lang_sense_clarification": "фразеологизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b781766-1eaf-30e0-8c11-045194038cc3",
                    "sense_definition": "Pozbawiać kogoś wzroku na stałe; czynić niewidomym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_harm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych wiekach okrutni władcy kazali <ślepić> zdrajców.",
                            "target_lang_translations": {
                                "ru": "В давние времена жестокие правители приказывали <ослеплять> предателей."
                            }
                        },
                        {
                            "text": "Legenda opowiada o ptakach, które celowo <ślepiono>, by piękniej śpiewały.",
                            "target_lang_translations": {
                                "ru": "Легенда рассказывает о птицах, которых намеренно <ослепляли>, чтобы они красивее пели."
                            }
                        }
                    ],
                    "synonyms": [
                        "oślepiać"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern Polish uses 'oślepiać' for this meaning"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Навсегда лишать кого-то зрения; делать слепым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ослеплять",
                                "target_lang_sense_clarification": "физически лишать зрения"
                            },
                            {
                                "target_lang_word": "лишать зрения",
                                "target_lang_sense_clarification": "формальное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "447facff-1823-358c-8d52-e7c968fde559",
                    "sense_definition": "Lid of aanhanger van de Nationalsozialistische Deutsche Arbeiterpartei (NSDAP), de politieke partij die in Duitsland regeerde van 1933 tot 1945.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "historical_political",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de Tweede Wereldoorlog bezetten de <w>nazi's</w> grote delen van Europa.",
                            "target_lang_translations": {
                                "en": "During the Second World War, the <w>Nazis</w> occupied large parts of Europe."
                            }
                        },
                        {
                            "text": "Na de oorlog vluchtte de voormalige <w>nazi</w> naar Zuid-Amerika om vervolging te voorkomen.",
                            "target_lang_translations": {
                                "en": "After the war, the former <w>Nazi</w> fled to South America to avoid prosecution."
                            }
                        }
                    ],
                    "synonyms": [
                        "nationaalsocialist"
                    ],
                    "common_phrases": [
                        "Duitse nazi",
                        "fanatieke nazi"
                    ],
                    "target_lang_definitions": {
                        "en": "Member or supporter of the National Socialist German Workers' Party (NSDAP), the political party that ruled Germany from 1933 to 1945."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Nazi",
                                "target_lang_sense_clarification": "historical political party member"
                            },
                            {
                                "target_lang_word": "National Socialist",
                                "target_lang_sense_clarification": "formal term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00f2f89a-7eae-3966-a2be-d0c898f2224c",
                    "sense_definition": "Aanhanger van het nazisme of een soortgelijke fascistische, racistische of extreemrechtse ideologie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "ideology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De demonstranten riepen leuzen tegen iedereen die zich als een <w>nazi</w> gedroeg.",
                            "target_lang_translations": {
                                "en": "The protesters shouted slogans against anyone behaving like a <w>Nazi</w>."
                            }
                        },
                        {
                            "text": "Hij werd door zijn politieke tegenstanders onterecht voor <w>nazi</w> uitgemaakt.",
                            "target_lang_translations": {
                                "en": "He was unjustly called a <w>Nazi</w> by his political opponents."
                            }
                        }
                    ],
                    "common_phrases": [
                        "neo-nazi"
                    ],
                    "target_lang_definitions": {
                        "en": "Supporter of Nazism or a similar fascist, racist, or far-right ideology."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Nazi",
                                "target_lang_sense_clarification": "adherent of fascist ideology"
                            },
                            {
                                "target_lang_word": "neo-Nazi",
                                "target_lang_sense_clarification": "modern adherent"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e11c1ab2-ee7e-3c1a-a9e9-bf9410cb5c07",
                    "sense_definition": "Iemand die zich zeer autoritair gedraagt of overdreven streng is op het naleven van regels.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn collega is een echte <w>nazi</w> als het gaat om het opruimen van het bureau.",
                            "target_lang_translations": {
                                "en": "My colleague is a real <w>Nazi</w> when it comes to tidying up the desk."
                            }
                        },
                        {
                            "text": "Ze noemen hem gekscherend een taal-<w>nazi</w> omdat hij elke spelfout verbetert.",
                            "target_lang_translations": {
                                "en": "They jokingly call him a grammar <w>Nazi</w> because he corrects every spelling error."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used metaphorically to describe strictness"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who behaves very authoritatively or is excessively strict about following rules."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Nazi",
                                "target_lang_sense_clarification": "metaphorical: strict person"
                            },
                            {
                                "target_lang_word": "stickler",
                                "target_lang_sense_clarification": "person insisting on exactness"
                            }
                        ]
                    }
                }
            ]
        }
    ]

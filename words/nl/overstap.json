    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "af36bf21-44f8-34c2-8055-0024f6a5f412",
                    "sense_definition": "Het veranderen van het ene vervoermiddel naar het andere tijdens een reis.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben een korte <w>overstap</w> van twintig minuten op het station van Utrecht.",
                            "target_lang_translations": {
                                "ru": "У нас короткая <w>пересадка</w> в двадцать минут на станции Утрехт."
                            }
                        },
                        {
                            "text": "Door de vertraging miste hij zijn <w>overstap</w> naar de laatste trein.",
                            "target_lang_translations": {
                                "ru": "Из-за задержки он пропустил свою <w>пересадку</w> на последний поезд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een overstap maken",
                        "korte overstap",
                        "directe overstap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смена одного транспортного средства на другое во время путешествия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пересадка",
                                "target_lang_sense_clarification": "транспорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4cf6b65-a06c-366e-aac2-cc7ed8ee41cd",
                    "sense_definition": "De fysieke handeling van ergens overheen stappen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met een grote <w>overstap</w> wist hij de diepe plas water te ontwijken.",
                            "target_lang_translations": {
                                "ru": "Сделав широкий <w>шаг</w>, он сумел избежать глубокой лужи."
                            }
                        },
                        {
                            "text": "De <w>overstap</w> van de wiebelende boot naar de kade vereiste enige voorzichtigheid.",
                            "target_lang_translations": {
                                "ru": "<w>Переход</w> с качающейся лодки на причал требовал некоторой осторожности."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое действие перешагивания через что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "движение"
                            },
                            {
                                "target_lang_word": "переход",
                                "target_lang_sense_clarification": "перемещение"
                            },
                            {
                                "target_lang_word": "перешагивание",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea0fa0ae-500f-3bef-bcb1-0dac7a34b4d3",
                    "sense_definition": "Een houten of stenen trapje om over een hek of muur te klimmen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wandelaars gebruikten de oude <w>overstap</w> om in het volgende weiland te komen.",
                            "target_lang_translations": {
                                "ru": "Туристы использовали старый <w>перелаз</w>, чтобы попасть на следующее пастбище."
                            }
                        },
                        {
                            "text": "Pas op dat je niet uitglijdt op de bemoste treden van de <w>overstap</w>.",
                            "target_lang_translations": {
                                "ru": "Осторожно, не поскользнитесь на замшелых ступенях <w>перелаза</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревянная или каменная лесенка для перелезания через забор или стену."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перелаз",
                                "target_lang_sense_clarification": "ступеньки через ограду"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "149f5fbc-3ff0-3445-ab75-5b294fe40dbc",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd in een bijzin van <w>overstappen</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is belangrijk dat ik op het juiste station <w>overstap</w>.",
                            "target_lang_translations": {
                                "ru": "Важно, чтобы я <w>пересел</w> на правильной станции."
                            }
                        },
                        {
                            "text": "Als ik nu <w>overstap</w> naar een andere bank, bespaar ik veel geld.",
                            "target_lang_translations": {
                                "ru": "Если я сейчас <w>перейду</w> в другой банк, я сэкономлю много денег."
                            }
                        }
                    ],

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eb2c8cbe-41b7-3cb5-8f38-8c513f479be1",
                    "sense_definition": "Umieścić przedmiot na jakiejś powierzchni, zazwyczaj w pozycji poziomej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "placement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <w>położyć</w> te dokumenty na moim biurku, żebym mógł je później przejrzeć.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>положите</w> эти документы мне на стол, чтобы я мог их позже просмотреть."
                            }
                        },
                        {
                            "text": "Gdzie <w>położyłeś</w> klucze od samochodu, bo nie mogę ich nigdzie znaleźć?",
                            "target_lang_translations": {
                                "ru": "Куда ты <w>положил</w> ключи от машины? Я нигде не могу их найти."
                            }
                        }
                    ],
                    "synonyms": [
                        "umieścić"
                    ],
                    "antonyms": [
                        "podnieść",
                        "wziąć",
                        "zdjąć"
                    ],
                    "common_phrases": [
                        "położyć na stole",
                        "położyć na miejscu",
                        "położyć rękę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить предмет на какую-либо поверхность, обычно в горизонтальном положении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "поместить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9d468a7-3d99-3463-af77-a24db610f2f0",
                    "sense_definition": "Przyjąć pozycję leżącą, kładąc się na czymś (wymaga użycia zaimka \"się\").",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "placement_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem tak zmęczony, że muszę natychmiast <w>położyć</w> się do łóżka.",
                            "target_lang_translations": {
                                "ru": "Я так устал, что должен немедленно <w>лечь</w> в постель."
                            }
                        },
                        {
                            "text": "Latem lubimy <w>położyć</w> się na trawie w parku i patrzeć w chmury.",
                            "target_lang_translations": {
                                "ru": "Летом мы любим <w>лечь</w> на траву в парке и смотреть на облака."
                            }
                        }
                    ],
                    "antonyms": [
                        "wstać"
                    ],
                    "common_phrases": [
                        "położyć się spać",
                        "położyć się na łóżku",
                        "położyć się wygodnie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять лежачее положение (используется с возвратной частицей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лечь",
                                "target_lang_sense_clarification": "принять горизонтальное положение"
                            },
                            {
                                "target_lang_word": "прилечь",
                                "target_lang_sense_clarification": "ненадолго"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bea162f-ac9f-3334-a200-d88dc8157238",
                    "sense_definition": "Pomóc komuś, zazwyczaj dziecku lub osobie chorej, znaleźć się w łóżku w celu snu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "placement_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy możesz dzisiaj <w>położyć</w> dzieci spać, kiedy ja będę brać prysznic?",
                            "target_lang_translations": {
                                "ru": "Ты можешь сегодня <w>уложить</w> детей спать, пока я буду принимать душ?"
                            }
                        },
                        {
                            "text": "Matka delikatnie <w>położyła</w> śpiące niemowlę do kołyski.",
                            "target_lang_translations": {
                                "ru": "Мать осторожно <w>положила</w> спящего младенца в колыбель."
                            }
                        }
                    ],
                    "synonyms": [
                        "uśpić"
                    ],
                    "antonyms": [
                        "obudzić"
                    ],
                    "common_phrases": [
                        "położyć dziecko spać",
                        "położyć do łóżka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помочь кому-то (обычно ребенку или больному) лечь в постель для сна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уложить",
                                "target_lang_sense_clarification": "спать"
                            },
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "в постель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "871f127c-474f-3799-ab81-0339efe31248",
                    "sense_definition": "Zamontować lub ułożyć warstwę materiału wykończeniowego na powierzchni konstrukcyjnej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu fachowcy przyjdą, aby <w>położyć</w> nowe kafelki w łazience.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе придут мастера, чтобы <w>положить</w> новую плитку в ванной."
                            }
                        },
                        {
                            "text": "Musimy <w>położyć</w> nową podłogę w salonie, ponieważ stara jest bardzo zniszczona.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>положить</w> новый пол в гостиной, так как старый очень изношен."
                            }
                        }
                    ],
                    "synonyms": [
                        "zamontować",
                        "ułożyć"
                    ],
                    "antonyms": [
                        "zerwać",
                        "skuć"
                    ],
                    "common_phrases": [
                        "położyć kafelki",
                        "położyć parkiet",
                        "położyć dach",
                        "położyć tynk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Установить или настелить слой отделочного материала на поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "плитку, пол"
                            },
                            {
                                "target_lang_word": "уложить",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9194758-60f9-3ed3-a818-abf9c55a0e7c",
                    "sense_definition": "Umieścić kogoś w szpitalu w celu przeprowadzenia leczenia lub badań.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zdecydował, że trzeba go <w>położyć</w> do szpitala na obserwację.",
                            "target_lang_translations": {
                                "ru": "Врач решил, что его нужно <w>положить</w> в больницу на обследование."
                            }
                        },
                        {
                            "text": "Z powodu ciężkiego zapalenia płuc <w>położono</w> ją na oddziale zakaźnym.",
                            "target_lang_translations": {
                                "ru": "Из-за тяжелой пневмонии её <w>положили</w> в инфекционное отделение."
                            }
                        }
                    ],
                    "synonyms": [
                        "hospitalizować",
                        "przyjąć"
                    ],
                    "antonyms": [
                        "wypisać"
                    ],
                    "common_phrases": [
                        "położyć do szpitala",
                        "położyć na oddział"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поместить кого-то в медицинское учреждение для лечения или обследования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "в больницу"
                            },
                            {
                                "target_lang_word": "госпитализировать",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5c4ab35-1d6e-3e2e-beaa-9865382ca9d8",
                    "sense_definition": "Stworzyć podstawy dla czegoś, zapoczątkować coś lub ustanowić fundamenty.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To odkrycie pozwoliło <w>położyć</w> podwaliny pod rozwój nowoczesnej medycyny.",
                            "target_lang_translations": {
                                "ru": "Это открытие позволило <w>заложить</w> основы развития современной медицины."
                            }
                        },
                        {
                            "text": "Musimy <w>położyć</w> kres tym niekończącym się kłótniom i zacząć współpracować.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>положить</w> конец этим бесконечным ссорам и начать сотрудничать."
                            }
                        }
                    ],
                    "synonyms": [

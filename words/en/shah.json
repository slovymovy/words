            "senses": [
                {
                    "sense_id": "910fe192-e3ec-3de9-9f91-399d9779ae76",
                    "sense_definition": "A king or supreme sovereign of Persia (modern Iran).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "ruler_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>shah</w> ruled the country for many decades before the revolution changed the political landscape.",
                            "target_lang_translations": {
                                "ru": "<w>Шах</w> правил страной много десятилетий, прежде чем революция изменила политический ландшафт."
                            }
                        },
                        {
                            "text": "Historians often discuss the reforms introduced by the <w>shah</w> during the twentieth century.",
                            "target_lang_translations": {
                                "ru": "Историки часто обсуждают реформы, введенные <w>шахом</w> в двадцатом веке."
                            }
                        }
                    ],
                    "synonyms": [
                        "monarch",
                        "sovereign",
                        "king"
                    ],
                    "antonyms": [
                        "subject",
                        "commoner"
                    ],
                    "common_phrases": [
                        "the Shah of Iran",
                        "the last Shah"
                    ],
                    "target_lang_definitions": {
                        "ru": "Король или верховный правитель Персии (современный Иран)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шах",
                                "target_lang_sense_clarification": "титул правителя Ирана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a892fa0-ad3b-3a2e-9269-68f712050e44",
                    "sense_definition": "A supreme ruler or monarch in certain West, Central, or South Asian nations.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "ruler_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local <w>shah</w> commanded a vast army that protected the borders of the kingdom.",
                            "target_lang_translations": {
                                "ru": "Местный <w>шах</w> командовал огромной армией, защищавшей границы королевства."
                            }
                        },
                        {
                            "text": "Travelers needed official permission from the <w>shah</w> to cross the territory safely.",
                            "target_lang_translations": {
                                "ru": "Путешественникам требовалось официальное разрешение от <w>шаха</w>, чтобы безопасно пересечь территорию."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruler",
                        "monarch"
                    ],
                    "target_lang_definitions": {

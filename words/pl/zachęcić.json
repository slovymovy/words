                    "sense_id": "6513e646-70d5-3e2a-b1ea-345891e8afc1",
                    "sense_definition": "Sprawić, że ktoś nabierze chęci, odwagi lub motywacji do zrobienia czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "motivation_external",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel starał się <w>zachęcić</w> uczniów do częstszego czytania książek.",
                            "target_lang_translations": {
                                "ru": "Учитель старался <w>побудить</w> учеников чаще читать книги."
                            }
                        },
                        {
                            "text": "Nowa oferta promocyjna ma <w>zachęcić</w> klientów do odwiedzenia naszego sklepu.",
                            "target_lang_translations": {
                                "ru": "Новое рекламное предложение призвано <w>привлечь</w> клиентов посетить наш магазин."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmotywować",
                        "namówić",
                        "skłonić",
                        "zmobilizować"
                    ],
                    "antonyms": [
                        "zniechęcić"
                    ],
                    "common_phrases": [
                        "zachęcić do działania",
                        "zachęcić do kupna",
                        "zachęcić do współpracy",
                        "skutecznie zachęcić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Побудить кого-то к действию, вызвав желание, смелость или мотивацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побудить",

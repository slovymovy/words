    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9d9b5de9-0395-3e47-86b2-e5ae389db950",
                    "sense_definition": "Сообщение или раздел в прессе, на радио и телевидении, посвящённый текущим событиям.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "record_of_events",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В утренней газете была опубликована светская <хроника>.",
                            "target_lang_translations": {
                                "en": "A society <column> was published in the morning newspaper."
                            }
                        },
                        {
                            "text": "Криминальная <хроника> по телевизору часто пугает зрителей.",
                            "target_lang_translations": {
                                "en": "The crime <news> on TV often scares viewers."
                            }
                        }
                    ],
                    "common_phrases": [
                        "светская хроника",
                        "криминальная хроника",
                        "отдел хроники"
                    ],
                    "target_lang_definitions": {
                        "en": "A section in the press or media dedicated to current events or specific topics."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "column",
                                "target_lang_sense_clarification": "newspaper section"
                            },
                            {
                                "target_lang_word": "news",
                                "target_lang_sense_clarification": "reports of events"
                            },
                            {
                                "target_lang_word": "chronicle",
                                "target_lang_sense_clarification": "record of events"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14621bb1-3279-36af-9871-461a08ba6bb5",
                    "sense_definition": "Запись исторических событий в хронологической последовательности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "record_of_events",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Монах старательно вёл <хронику> жизни своего монастыря.",
                            "target_lang_translations": {
                                "en": "The monk diligently kept a <chronicle> of his monastery's life."
                            }
                        },
                        {
                            "text": "Эта древняя <хроника> сохранилась до наших дней.",
                            "target_lang_translations": {
                                "en": "This ancient <chronicle> has survived to this day."
                            }
                        }
                    ],
                    "synonyms": [
                        "ле́топись"
                    ],
                    "common_phrases": [
                        "историческая хроника",
                        "вести хронику"
                    ],
                    "target_lang_definitions": {
                        "en": "A written record of historical events in the order in which they happened."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chronicle",
                                "target_lang_sense_clarification": "historical account"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d82fe58-5192-31c3-b9ed-8f076b56b904",
                    "sense_definition": "Документальный фильм или кадры, запечатлевшие реальные события.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "record_of_events",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Режиссёр использовал в фильме редкую военную <хронику>.",
                            "target_lang_translations": {
                                "en": "The director used rare war <footage> in the film."
                            }
                        },
                        {
                            "text": "Мы смотрели старую <хронику> о строительстве города.",
                            "target_lang_translations": {
                                "en": "We watched an old <newsreel> about the construction of the city."
                            }
                        }
                    ],
                    "common_phrases": [
                        "военная хроника",
                        "кадры хроники",
                        "документальная хроника"
                    ],
                    "target_lang_definitions": {

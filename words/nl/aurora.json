                {
                    "sense_id": "33691ae9-ca91-37d7-8b2b-9fde0ced9095",
                    "sense_definition": "Het eerste licht van de dag voordat de zon opkomt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "time_of_day",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dichter beschreef de prachtige <w>aurora</w> die boven de heuvels verscheen.",
                            "target_lang_translations": {
                                "en": "The poet described the beautiful <w>dawn</w> that appeared above the hills.",
                                "ru": "Поэт описал прекрасную <w>зарю</w>, которая появилась над холмами."
                            }
                        },
                        {
                            "text": "Bij het zien van de <w>aurora</w> wisten we dat de nieuwe dag was aangebroken.",
                            "target_lang_translations": {
                                "en": "Upon seeing the <w>dawn</w>, we knew that the new day had broken.",
                                "ru": "Увидев <w>рассвет</w>, мы поняли, что наступил новый день."
                            }
                        }
                    ],
                    "synonyms": [
                        "dageraad",
                        "ochtendgloren"
                    ],
                    "antonyms": [
                        "avondschemering"
                    ],
                    "target_lang_definitions": {
                        "en": "The first light of day before the sun rises.",
                        "ru": "Первый свет дня перед восходом солнца."
                    },

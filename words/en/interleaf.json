{
    "word_family": [
        "leaf",
        "interleave",
        "leaflet"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f35f6fa4-f03d-3fac-8c99-90c75fe49503",
                    "sense_definition": "A leaf, often of tissue paper or other thin paper, inserted between the pages of a book to protect illustrations.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "book_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antique book contains a tissue <w>interleaf</w> before every engraving.",
                            "target_lang_translations": {
                                "ru": "В этой старинной книге перед каждой гравюрой есть <w>прокладка</w> из папиросной бумаги."
                            }
                        },
                        {
                            "text": "Please handle the protective <w>interleaf</w> gently to avoid tearing the paper.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, обращайтесь с защитной <w>прокладкой</w> осторожно, чтобы не порвать бумагу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tissue interleaf",
                        "protective interleaf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лист, часто из папиросной или другой тонкой бумаги, вставленный между страницами книги для защиты иллюстраций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокладка",
                                "target_lang_sense_clarification": "лист бумаги между страницами"
                            },
                            {
                                "target_lang_word": "прокладочный лист",
                                "target_lang_sense_clarification": "полиграфический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "469d3ec7-49ef-392d-8160-988ea9bb7109",
                    "sense_definition": "A sheet of paper or cardboard, placed between layers on a pallet to create a cohesive structure.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "logistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need a cardboard <w>interleaf</w> to stabilize the pallet load.",
                            "target_lang_translations": {
                                "ru": "Нам нужна картонная <w>прокладка</w> для стабилизации груза на поддоне."
                            }
                        },
                        {
                            "text": "The warehouse uses a plastic <w>interleaf</w> between heavy metal sheets.",
                            "target_lang_translations": {
                                "ru": "На складе используют пластиковую <w>прокладку</w> между тяжелыми металлическими листами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cardboard interleaf",
                        "pallet interleaf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лист бумаги или картона, размещаемый между слоями груза на поддоне для создания устойчивой конструкции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокладка",
                                "target_lang_sense_clarification": "между слоями груза"
                            },
                            {
                                "target_lang_word": "прокладочный лист",
                                "target_lang_sense_clarification": "в логистике"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a4af9ecc-4a77-30c1-9b7a-6c39c8dd0ed2",
                    "sense_definition": "To insert blank leaves or sheets between the pages of a book or layers of a stack.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>interleaf</w> the document with blank sheets for comments.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>проложить</w> документ чистыми листами для комментариев."
                            }
                        },
                        {
                            "text": "The printer can <w>interleaf</w> colored paper between the white pages.",
                            "target_lang_translations": {
                                "ru": "Принтер может <w>прокладывать</w> цветную бумагу между белыми страницами."
                            }
                        }
                    ],
                    "synonyms": [
                        "interleave"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вставлять пустые листы между страницами книги или слоями стопки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокладывать",
                                "target_lang_sense_clarification": "вставлять листы между чем-либо"
                            },
                            {
                                "target_lang_word": "перекладывать",
                                "target_lang_sense_clarification": "разделять слои листами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
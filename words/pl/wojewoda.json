    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "01883cc5-ca13-3ee7-9160-6ad6f68496a7",
                    "sense_definition": "Przedstawiciel rządu sprawujący władzę administracyjną na terenie województwa.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "admin_office",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obecny <wojewoda> ogłosił stan alarmowy z powodu nadchodzącej powodzi.",
                            "target_lang_translations": {
                                "ru": "Нынешний <воевода> объявил чрезвычайное положение из-за надвигающегося наводнения."
                            }
                        },
                        {
                            "text": "Mieszkańcy wysłali oficjalny list, który wkrótce otrzyma <wojewoda>.",
                            "target_lang_translations": {
                                "ru": "Жители отправили официальное письмо, которое вскоре получит <воевода>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "urząd wojewody",
                        "decyzja wojewody",
                        "powołanie wojewody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель правительства, осуществляющий административную власть на территории воеводства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воевода",
                                "target_lang_sense_clarification": "глава воеводства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84af670a-ebed-3938-88d4-f1a1110168a0",
                    "sense_definition": "Kobieta sprawująca urząd przedstawiciela rządu w województwie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "admin_office",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa <wojewoda> spotkała się z dziennikarzami na porannej konferencji prasowej.",
                            "target_lang_translations": {
                                "ru": "Новая <воевода> встретилась с журналистами на утренней пресс-конференции."
                            }
                        },
                        {
                            "text": "Pani <wojewoda> podpisała ważne dokumenty dotyczące budowy nowej drogi.",
                            "target_lang_translations": {
                                "ru": "Госпожа <воевода> подписала важные документы, касающиеся строительства новой дороги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pani wojewoda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, занимающая должность представителя правительства в воеводстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воевода",
                                "target_lang_sense_clarification": "женщина-чиновник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87db6f8a-3a2d-30e7-bc66-0af10512fe8a",
                    "sense_definition": "W dawnej Polsce: wysoki urzędnik królewski zarządzający prowincją i dowodzący wojskiem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słynny <wojewoda> przybył na zamek królewski wraz ze swoją liczną świtą.",
                            "target_lang_translations": {
                                "ru": "Знаменитый <воевода> прибыл в королевский замок вместе со своей многочисленной свитой."
                            }
                        },
                        {
                            "text": "W powieści historycznej opisano, jak <wojewoda> zbierał rycerstwo na wielką bitwę.",
                            "target_lang_translations": {
                                "ru": "В историческом романе описано, как <воевода> собирал рыцарство на великую битву."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wojewoda ruski",
                        "wojewoda wileński"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy funkcji historycznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В древней Польше: высокий королевский чиновник, управляющий провинцией и командующий войском."

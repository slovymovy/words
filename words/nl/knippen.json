                },
                {
                    "sense_id": "08568d21-353a-39df-a370-8c0b9dd2625b",
                    "sense_definition": "Geselecteerde digitale gegevens verwijderen en tijdelijk in het geheugen opslaan om ze elders te plaatsen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag die tabel wel uit mijn presentatie <w>knippen</w>.",

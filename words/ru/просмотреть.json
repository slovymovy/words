    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5f179014-9ba8-39a6-880c-a35f6f7be3fd",
                    "sense_definition": "Ознакомиться с содержанием чего-либо, читая или разглядывая (обычно бегло или быстро).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я успел <просмотреть> утренние газеты за завтраком, пока пил кофе.",
                            "target_lang_translations": {
                                "en": "I managed to <look through> the morning newspapers at breakfast while drinking coffee."
                            }
                        },
                        {
                            "text": "Много позже мне пришлось <просмотреть> сотню словарей, чтобы найти это редкое слово.",
                            "target_lang_translations": {
                                "en": "Much later I had to <look through> a hundred dictionaries to find this rare word."
                            }
                        }
                    ],
                    "synonyms": [
                        "проглядеть"
                    ],
                    "common_phrases": [
                        "просмотреть документы",
                        "просмотреть статью",
                        "бегло просмотреть",
                        "просмотреть список"
                    ],
                    "target_lang_definitions": {
                        "en": "To familiarize oneself with the content of something by reading or viewing it (usually briefly or quickly)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look through",
                                "target_lang_sense_clarification": "examine briefly or quickly"
                            },
                            {
                                "target_lang_word": "scan",
                                "target_lang_sense_clarification": "read quickly to find information"
                            },
                            {
                                "target_lang_word": "skim",
                                "target_lang_sense_clarification": "read superficially"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "748457f7-84f1-3bad-8c28-560c51cf7fbd",
                    "sense_definition": "Посмотреть что-либо от начала до конца или с целью проверки (фильм, передачу, запись).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <просмотреть> этот фильм всей семьёй в выходные.",
                            "target_lang_translations": {
                                "en": "We decided to <watch> this movie with the whole family on the weekend."
                            }
                        },
                        {
                            "text": "Следователь потребовал <просмотреть> видеозапись с камер наблюдения.",
                            "target_lang_translations": {
                                "en": "The investigator demanded to <review> the video recording from the surveillance cameras."
                            }
                        }
                    ],
                    "common_phrases": [
                        "просмотреть фильм",
                        "просмотреть запись",
                        "просмотреть видео"
                    ],
                    "target_lang_definitions": {
                        "en": "To watch something from beginning to end or for the purpose of checking (a movie, broadcast, recording)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "watch",
                                "target_lang_sense_clarification": "view a movie or show"
                            },
                            {
                                "target_lang_word": "review",
                                "target_lang_sense_clarification": "watch to check or analyze"
                            },
                            {
                                "target_lang_word": "view",
                                "target_lang_sense_clarification": "formal way of saying watch"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1b2116f-9bc2-34a6-bbc1-cf7dcb5af027",
                    "sense_definition": "Не заметить чего-либо, упустить из виду по невнимательности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Редактор случайно <просмотрел> досадную опечатку в заголовке статьи.",
                            "target_lang_translations": {
                                "en": "The editor accidentally <overlooked> an annoying typo in the article headline."
                            }
                        },
                        {
                            "text": "Непонятно, как опытные юристы умудрились <просмотреть> такую ошибку в договоре.",
                            "target_lang_translations": {
                                "en": "It is unclear how experienced lawyers managed to <miss> such an error in the contract."
                            }
                        }
                    ],
                    "synonyms": [
                        "пропустить",
                        "упустить",
                        "проглядеть",
                        "проморгать",
                        "прошляпить"
                    ],
                    "antonyms": [
                        "высмотреть"
                    ],
                    "common_phrases": [
                        "просмотреть ошибку",
                        "просмотреть опасность"
                    ],
                    "target_lang_definitions": {
                        "en": "To fail to notice something, to lose sight of it due to inattention."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overlook",
                                "target_lang_sense_clarification": "fail to notice"
                            },
                            {
                                "target_lang_word": "miss",
                                "target_lang_sense_clarification": "fail to see or catch"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a431b3d-2fc9-3db2-8909-ad6dd71e8a0a",
                    "sense_definition": "Провести определённое время, глядя на что-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она <просмотрела> в окно целый час, ожидая его возвращения домой.",
                            "target_lang_translations": {
                                "en": "She <looked> out the window for a whole hour, waiting for his return home."
                            }
                        },
                        {
                            "text": "Я столько <просмотрел> на эту картину, пытаясь понять замысел художника.",
                            "target_lang_translations": {
                                "en": "I <looked> at this painting for so long, trying to understand the artist's intent."
                            }
                        }
                    ],
                    "common_phrases": [
                        "просмотреть все глаза"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend a certain amount of time looking at something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look",
                                "target_lang_sense_clarification": "spend time looking"
                            },
                            {
                                "target_lang_word": "gaze",
                                "target_lang_sense_clarification": "look steadily for a duration"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
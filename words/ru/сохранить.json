                        {
                            "text": "Я решил <сохранить> за собой старый номер телефона при переезде.",
                            "target_lang_translations": {
                                "en": "I decided to <keep> my old phone number when moving."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сохранить право",
                        "сохранить за собой"
                    ],
                    "target_lang_definitions": {

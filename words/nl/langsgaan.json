            "pos": "verb",
            "senses": [
                {
                    "sense_id": "24967326-f561-37bc-b47f-5bfe9ba96c99",
                    "sense_definition": "Een kort, vaak informeel bezoek brengen aan iemand.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>ga</w> vanavond even bij mijn ouders <w>langs</w>.",
                            "target_lang_translations": {
                                "en": "I'm <w>dropping by</w> my parents' place tonight.",
                                "ru": "Я сегодня вечером ненадолго <w>загляну</w> к родителям."
                            }
                        },
                        {
                            "text": "Je mag altijd <w>langsgaan</w> als je in de buurt bent.",
                            "target_lang_translations": {
                                "en": "You can always <w>stop by</w> if you are in the neighborhood.",
                                "ru": "Ты всегда можешь <w>зайти</w>, если будешь поблизости."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezoeken",
                        "langskomen",
                        "aanwippen",
                        "binnenwippen"
                    ],
                    "common_phrases": [
                        "even langsgaan",
                        "bij iemand langsgaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To pay a short, often informal visit to someone.",
                        "ru": "Нанести короткий, часто неофициальный визит кому-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drop by",
                                "target_lang_sense_clarification": "informal visit"
                            },

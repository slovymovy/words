            "senses": [
                {
                    "sense_id": "aecbf2ff-7802-3560-962a-48d6d58b8a19",
                    "sense_definition": "The quality, standard, or level of ability of a person or thing.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is looking to hire an engineer of the highest <w>caliber</w>.",
                            "target_lang_translations": {
                                "ru": "Компания ищет инженера высочайшего <w>класса</w>."
                            }
                        },
                        {
                            "text": "Her performance was of such a high <w>caliber</w> that she won the competition easily.",
                            "target_lang_translations": {
                                "ru": "Её выступление было столь высокого <w>уровня</w>, что она легко выиграла конкурс."
                            }
                        }
                    ],
                    "synonyms": [
                        "quality",
                        "standard",
                        "stature",
                        "capability",
                        "merit"
                    ],
                    "antonyms": [
                        "inadequacy",
                        "inferiority"
                    ],
                    "common_phrases": [
                        "high caliber",
                        "of this caliber",
                        "highest caliber"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество, уровень способностей или значимость человека или предмета"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "у́ровень",
                                "target_lang_sense_clarification": "качество, степень мастерства"
                            },
                            {
                                "target_lang_word": "класс",
                                "target_lang_sense_clarification": "разряд, категория качества"
                            },
                            {
                                "target_lang_word": "кали́бр",
                                "target_lang_sense_clarification": "масштаб личности, значимость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3239a6de-bd69-3ddf-9ccf-c089bf203bc8",
                    "sense_definition": "The internal diameter of a gun barrel or the size of a bullet.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective identified the weapon by checking the <w>caliber</w> of the bullet.",
                            "target_lang_translations": {
                                "ru": "Детектив определил оружие, проверив <w>калибр</w> пули."
                            }
                        },
                        {
                            "text": "Hunters often choose a large <w>caliber</w> rifle for big game.",
                            "target_lang_translations": {
                                "ru": "Охотники часто выбирают винтовку крупного <w>калибра</w> для крупной дичи."
                            }
                        }
                    ],
                    "synonyms": [
                        "bore",
                        "gauge",
                        "diameter"
                    ],
                    "common_phrases": [
                        "small caliber",
                        "large caliber",
                        "50 caliber"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренний диаметр ствола оружия или размер пули"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кали́бр",
                                "target_lang_sense_clarification": "диаметр канала ствола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a30733dd-dcff-3dd8-8e65-c2bb1a49329f",
                    "sense_definition": "The specific internal mechanism or movement of a watch or clock.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This luxury watch features a complex automatic <w>caliber</w> designed in Switzerland.",
                            "target_lang_translations": {
                                "ru": "Эти роскошные часы оснащены сложным автоматическим <w>калибром</w>, разработанным в Швейцарии."
                            }
                        },
                        {
                            "text": "Collectors admire the precision of this rare mechanical <w>caliber</w>.",
                            "target_lang_translations": {
                                "ru": "Коллекционеры восхищаются точностью этого редкого механического <w>калибра</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "movement"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical (horology)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип или модель часового механизма"
                    },
                    "translations": {
                        "ru": [

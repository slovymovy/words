                        "ru": "Втягивать жидкость или воздух с силой, обычно ртом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suck",
                                "target_lang_sense_clarification": "to pull into the mouth"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сосать",
                                "target_lang_sense_clarification": "втягивать ртом"
                            },
                            {
                                "target_lang_word": "всасывать",
                                "target_lang_sense_clarification": "втягивать внутрь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c79576f-656f-3b52-9ef6-d7703fa184b4",
                    "sense_definition": "Stof en vuil van de vloer verwijderen met een apparaat.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet de woonkamer nog even <w>zuigen</w> voor het bezoek komt.",
                            "target_lang_translations": {

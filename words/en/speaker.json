{
    "word_family": [
        "speak",
        "speech",
        "spokesperson",
        "loudspeaker",
        "unspeakable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3d846129-1116-3bfe-af70-4be9bdccea18",
                    "sense_definition": "An electronic device that produces sound from an audio signal.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "audio_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The music coming from the <w>speaker</w> was too loud.",
                            "target_lang_translations": {
                                "ru": "Музыка, доносившаяся из <w>колонки</w>, была слишком громкой."
                            }
                        },
                        {
                            "text": "I connected my phone to a portable bluetooth <w>speaker</w>.",
                            "target_lang_translations": {
                                "ru": "Я подключил телефон к портативной блютуз-<w>колонке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "talker"
                    ],
                    "common_phrases": [
                        "bluetooth speaker",
                        "smart speaker",
                        "stereo speakers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Электронное устройство, которое воспроизводит звук из аудиосигнала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колонка",
                                "target_lang_sense_clarification": "акустическая система"
                            },
                            {
                                "target_lang_word": "динамик",
                                "target_lang_sense_clarification": "компонент устройства"
                            },
                            {
                                "target_lang_word": "громкоговоритель",
                                "target_lang_sense_clarification": "устройство оповещения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e475abc-2d54-3376-8f0f-51cad5df5937",
                    "sense_definition": "A person who speaks a particular language or is speaking.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_speaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a native <w>speaker</w> of Spanish.",
                            "target_lang_translations": {
                                "ru": "Она — <w>носитель</w> испанского языка."
                            }
                        },
                        {
                            "text": "The course is designed for advanced English <w>speakers</w>.",
                            "target_lang_translations": {
                                "ru": "Курс разработан для людей, свободно <w>говорящих</w> по-английски."
                            }
                        }
                    ],
                    "synonyms": [
                        "talker"
                    ],
                    "common_phrases": [
                        "native speaker",
                        "fluent speaker",
                        "non-native speaker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, говорящий на определенном языке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носитель",
                                "target_lang_sense_clarification": "языка"
                            },
                            {
                                "target_lang_word": "говорящий",
                                "target_lang_sense_clarification": "владеющий языком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33998d6f-9369-36be-9c3b-a584a13c7c13",
                    "sense_definition": "A person who gives a formal speech or presentation to an audience.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_speaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The guest <w>speaker</w> gave an inspiring talk about climate change.",
                            "target_lang_translations": {
                                "ru": "Приглашенный <w>докладчик</w> выступил с вдохновляющей речью об изменении климата."
                            }
                        },
                        {
                            "text": "He is a very confident public <w>speaker</w>.",
                            "target_lang_translations": {
                                "ru": "Он очень уверенный <w>оратор</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "speechmaker",
                        "talker",
                        "speaker"
                    ],
                    "common_phrases": [
                        "guest speaker",
                        "keynote speaker",
                        "public speaker",
                        "motivational speaker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, выступающий с официальной речью или презентацией перед аудиторией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "докладчик",
                                "target_lang_sense_clarification": "выступающий с докладом"
                            },
                            {
                                "target_lang_word": "оратор",
                                "target_lang_sense_clarification": "выступающий публично"
                            },
                            {
                                "target_lang_word": "спикер",
                                "target_lang_sense_clarification": "на конференции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "479d9d94-0dcf-3f7f-87dd-6b89127e2575",
                    "sense_definition": "A setting on a telephone that allows sound to be heard without holding the receiver to the ear.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "audio_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will put you on <w>speaker</w> so everyone in the room can hear you.",
                            "target_lang_translations": {
                                "ru": "Я поставлю тебя на <w>громкую связь</w>, чтобы все в комнате могли тебя слышать."
                            }
                        },
                        {
                            "text": "It is hard to hear what you are saying when you are on <w>speaker</w>.",
                            "target_lang_translations": {
                                "ru": "Трудно разобрать, что ты говоришь, когда ты на <w>громкой связи</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on speaker",
                        "put on speaker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Режим телефона, позволяющий слышать звук без прикладывания трубки к уху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "громкая связь",
                                "target_lang_sense_clarification": "режим телефона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43531dd1-0ac0-3955-a88f-628d5913f00b",
                    "sense_definition": "The official who leads the discussions in a legislative body, such as a parliament or congress.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Speaker</w> called the meeting to order and asked for silence.",
                            "target_lang_translations": {
                                "ru": "<w>Спикер</w> призвал собрание к порядку и попросил тишины."
                            }
                        },
                        {
                            "text": "Members of the house must address their comments to the <w>Speaker</w>.",
                            "target_lang_translations": {
                                "ru": "Члены палаты должны адресовать свои комментарии <w>Спикеру</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Speaker of the House",
                        "Mr Speaker",
                        "Madam Speaker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Должностное лицо, руководящее обсуждениями в законодательном органе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спикер",
                                "target_lang_sense_clarification": "председатель парламента"
                            },
                            {
                                "target_lang_word": "председатель",
                                "target_lang_sense_clarification": "собрания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6595afce-46a5-36fe-a723-624b92c0c78b",
                    "sense_definition": "The voice or character that narrates a poem or story, distinct from the author.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>speaker</w> in the poem expresses deep regret about his past choices.",
                            "target_lang_translations": {
                                "ru": "<w>Лирический герой</w> стихотворения выражает глубокое сожаление о своем прошлом выборе."
                            }
                        },
                        {
                            "text": "We must distinguish the feelings of the <w>speaker</w> from those of the poet.",
                            "target_lang_translations": {
                                "ru": "Мы должны отличать чувства <w>лирического героя</w> от чувств поэта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "speaker of the poem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary analysis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Голос или персонаж, ведущий повествование в стихотворении или рассказе, отличный от автора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лирический герой",
                                "target_lang_sense_clarification": "в поэзии"
                            },
                            {
                                "target_lang_word": "рассказчик",
                                "target_lang_sense_clarification": "повествователь"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f305cf9f-3fc0-333a-881e-3b34d5a3c2e5",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Speaker</w> was known for his contributions to the local community.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Спикер</w> был известен своим вкладом в местное сообщество."
                            }
                        },
                        {
                            "text": "I went to school with a girl named Sarah <w>Speaker</w>.",
                            "target_lang_translations": {
                                "ru": "Я учился в школе с девочкой по имени Сара <w>Спикер</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Спикер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

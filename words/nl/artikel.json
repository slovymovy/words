                            }
                        }
                    ],
                    "common_phrases": [
                        "artikel één",
                        "volgens het artikel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пронумерованная часть или параграф в законе, контракте или регламенте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "статья",
                                "target_lang_sense_clarification": "пункт закона или договора"
                            },
                            {
                                "target_lang_word": "пункт",
                                "target_lang_sense_clarification": "параграф"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f54eeffc-8629-3100-ac78-88c910c3480d",
                    "sense_definition": "Een woordsoort die de bepaaldheid van een zelfstandig naamwoord aangeeft.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het woord 'de' noemen we een bepaald <w>artikel</w>.",
                            "target_lang_translations": {

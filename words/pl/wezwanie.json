    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5a2950c0-e89f-35ee-a3ef-35e1f39ed2fe",
                    "sense_definition": "Oficjalne pismo lub polecenie nakazujące stawienie się w urzędzie, sądzie lub innym miejscu, albo nakazujące wykonanie określonej czynności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "official_order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dostałem urzędowe <w>wezwanie</w> do zapłaty zaległego mandatu.",
                            "target_lang_translations": {
                                "ru": "Я получил официальное <w>требование</w> об уплате просроченного штрафа."
                            }
                        },
                        {
                            "text": "Listonosz przyniósł mi <w>wezwanie</w> do sądu w charakterze świadka.",
                            "target_lang_translations": {
                                "ru": "Почтальон принес мне <w>повестку</w> в суд в качестве свидетеля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wezwanie do zapłaty",
                        "wezwanie do sądu",
                        "ostateczne wezwanie",
                        "otrzymać wezwanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное письмо или предписание, обязывающее явиться в учреждение, суд или иное место, либо предписывающее выполнить определенное действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повестка",
                                "target_lang_sense_clarification": "в суд, полицию или военкомат"
                            },
                            {
                                "target_lang_word": "требование",
                                "target_lang_sense_clarification": "об уплате долга или выполнении обязательств"
                            },
                            {
                                "target_lang_word": "вызов",
                                "target_lang_sense_clarification": "официальное приглашение явиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e66b7cf-a4b8-3d6e-b557-480af3eb33df",
                    "sense_definition": "Publiczna lub dramatyczna prośba o pomoc, interwencję lub podjęcie określonego działania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "appeal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapitan tonącego statku nadał przez radio ostatnie <w>wezwanie</w> o pomoc.",
                            "target_lang_translations": {
                                "ru": "Капитан тонущего корабля передал по радио последний <w>зов</w> о помощи."
                            }
                        },
                        {
                            "text": "Prezydent wystosował do obywateli <w>wezwanie</w> do zachowania spokoju w trudnych chwilach.",
                            "target_lang_translations": {
                                "ru": "Президент обратился к гражданам с <w>призывом</w> сохранять спокойствие в трудные времена."
                            }
                        }
                    ],
                    "synonyms": [
                        "apel",
                        "odezwa",
                        "orędzie"
                    ],
                    "common_phrases": [
                        "wezwanie o pomoc",
                        "wezwanie do działania",
                        "wezwanie do pokoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичная или драматичная просьба о помощи, вмешательстве или принятии определенных мер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "призыв",
                                "target_lang_sense_clarification": "обращение к общественности или группе людей"
                            },
                            {
                                "target_lang_word": "зов",
                                "target_lang_sense_clarification": "крик о помощи, эмоциональная просьба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a10980e5-0506-338f-9bb7-eecc3bc2e4bb",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>wezwać</w>; czynność wołania kogoś, przywoływania lub nakazywania przybycia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na głośne <w>wezwanie</w> właściciela pies natychmiast przybiegł z ogrodu.",
                            "target_lang_translations": {
                                "ru": "На громкий <w>зов</w> хозяина собака немедленно прибежала из сада."
                            }
                        },
                        {
                            "text": "Lekarz przyjechał w środku nocy na pilne <w>wezwanie</w> do chorego pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врач приехал посреди ночи на срочный <w>вызов</w> к больному пациенту."
                            }
                        }
                    ],
                    "common_phrases": [
                        "na wezwanie",
                        "przybyć na wezwanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное от «wezwać»; действие, заключающееся в оклике кого-либо, приглашении или приказе прибыть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызов",
                                "target_lang_sense_clarification": "врача, такси или специалиста"
                            },
                            {
                                "target_lang_word": "зов",
                                "target_lang_sense_clarification": "оклик голосом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a1436ec-89a8-3011-b669-03945d7c9f53",
                    "sense_definition": "Dawne określenie na nazwę, imię lub tytuł.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starych księgach to miasto nosiło zupełnie inne <w>wezwanie</w>.",
                            "target_lang_translations": {
                                "ru": "В старых книгах этот город носил совершенно иное <w>название</w>."
                            }

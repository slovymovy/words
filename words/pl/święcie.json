                {
                    "sense_id": "373c9cbd-b524-383e-8d47-0388c5d5f682",
                    "sense_definition": "W sposób całkowity, pewny i bezdyskusyjny; używane głównie do podkreślenia głębokiego przekonania lub wiary.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem <w>święcie</w> przekonany, że to on wygrał ten konkurs.",
                            "target_lang_translations": {
                                "ru": "Я <w>твердо</w> убежден, что именно он выиграл этот конкурс."
                            }
                        },
                        {
                            "text": "Ona <w>święcie</w> wierzy w każde słowo, które przeczyta w gazecie.",
                            "target_lang_translations": {
                                "ru": "Она <w>свято</w> верит каждому слову, которое прочитает в газете."
                            }
                        }
                    ],
                    "synonyms": [
                        "całkowicie",
                        "w zupełności",
                        "zupełnie",
                        "stuprocentowo"
                    ],
                    "common_phrases": [
                        "święcie przekonany",
                        "święcie wierzyć",
                        "święcie obiecać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью, несомненно и безоговорочно; используется в основном для подчеркивания глубокой убежденности или веры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свято",
                                "target_lang_sense_clarification": "в устойчивых выражениях, напр. 'свято верить'"
                            },
                            {
                                "target_lang_word": "твердо",
                                "target_lang_sense_clarification": "в значении полной уверенности"
                            },
                            {
                                "target_lang_word": "абсолютно",
                                "target_lang_sense_clarification": "полностью, совершенно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01de436a-d60a-38ec-8930-ae83a95125b0",

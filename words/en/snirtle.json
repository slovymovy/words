{
    "word_family": [],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d21f41c3-d40f-308c-864f-259477cf4d71",
                    "sense_definition": "To laugh quietly or in a suppressed way, often in a manner that suggests trying to hide the amusement.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "laugh_suppressed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The students began to <w>snirtle</w> when the teacher accidentally dropped his papers.",
                            "target_lang_translations": {
                                "ru": "Студенты начали <w>хихикать</w>, когда учитель случайно уронил свои бумаги."
                            }
                        },
                        {
                            "text": "She tried not to <w>snirtle</w> at the silly joke during the formal dinner.",
                            "target_lang_translations": {
                                "ru": "Она старалась не <w>прыснуть</w> от глупой шутки во время официального ужина."
                            }
                        }
                    ],
                    "synonyms": [
                        "snigger",
                        "snicker",
                        "giggle",
                        "chortle"
                    ],
                    "common_phrases": [
                        "snirtle at",
                        "began to snirtle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смеяться тихо или сдавленно, часто пытаясь скрыть веселье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хихикать",
                                "target_lang_sense_clarification": "тихо смеяться, часто исподтишка"
                            },
                            {
                                "target_lang_word": "фыркать",
                                "target_lang_sense_clarification": "издавать звук носом, сдерживая смех"
                            },
                            {
                                "target_lang_word": "прыснуть",
                                "target_lang_sense_clarification": "внезапно и коротко рассмеяться, пытаясь сдержаться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4c9c469f-ea9b-3411-801c-cd968ed31407",
                    "sense_definition": "A short, suppressed laugh or snigger.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "laugh_suppressed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He let out a quiet <w>snirtle</w> of amusement while reading the comic.",
                            "target_lang_translations": {
                                "ru": "Он издал тихий довольный <w>смешок</w>, читая комикс."
                            }
                        },
                        {
                            "text": "A sudden <w>snirtle</w> from the back of the room interrupted the speech.",
                            "target_lang_translations": {
                                "ru": "Внезапное <w>хихиканье</w> из задней части комнаты прервало речь."
                            }
                        }
                    ],
                    "synonyms": [
                        "snigger",
                        "snicker",
                        "giggle"
                    ],
                    "common_phrases": [
                        "gave a snirtle",
                        "little snirtle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий, сдавленный смех или хихиканье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смешок",
                                "target_lang_sense_clarification": "короткий, негромкий смех"
                            },
                            {
                                "target_lang_word": "хихиканье",
                                "target_lang_sense_clarification": "процесс тихого смеха"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
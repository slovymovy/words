                    ],
                    "common_phrases": [
                        "на седьмом небе",
                        "манна с небес",
                        "вознестись на небеса",
                        "силы небесные"
                    ],
                    "target_lang_definitions": {
                        "en": "The abode of God, angels, and saints in religious beliefs; the divine realm."
                    },
                    "translations": {

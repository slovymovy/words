    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7e2128c8-3419-360d-a00c-7ae2558a0734",
                    "sense_definition": "Relating to the highest heaven, the sky, or the celestial sphere.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heavenly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poet described the <w>empyreal</w> beauty of the stars shining above the world.",
                            "target_lang_translations": {
                                "ru": "Поэт описал <w>небесную</w> красоту звезд, сияющих над миром."
                            }
                        },
                        {
                            "text": "In the painting, angels descend from the <w>empyreal</w> heights to visit the earth.",
                            "target_lang_translations": {
                                "ru": "На картине ангелы спускаются с <w>эмпирейских</w> высот, чтобы посетить землю."
                            }
                        }
                    ],
                    "synonyms": [
                        "celestial",
                        "empyrean",
                        "sublime",
                        "exalted"
                    ],
                    "antonyms": [
                        "terrestrial",
                        "earthly"
                    ],
                    "common_phrases": [
                        "empyreal realm",
                        "empyreal light",
                        "empyreal sphere"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к высшим небесам, небу или небесной сфере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "небесный",
                                "target_lang_sense_clarification": "относящийся к небу"
                            },
                            {
                                "target_lang_word": "эмпирейский",
                                "target_lang_sense_clarification": "относящийся к эмпирею, высшей части небес"
                            },
                            {
                                "target_lang_word": "горний",
                                "target_lang_sense_clarification": "находящийся в вышине, небесный (поэтич.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da08cb40-7750-3710-ab5a-4534f57ceaba",
                    "sense_definition": "Formed of pure fire or light, often in the context of ancient cosmology.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fiery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ancient philosophers believed the sun was made of an <w>empyreal</w> substance.",
                            "target_lang_translations": {
                                "ru": "Древние философы считали, что солнце состоит из <w>огненной</w> субстанции."
                            }
                        },
                        {
                            "text": "The creature was said to breathe an <w>empyreal</w> fire that could not be extinguished.",
                            "target_lang_translations": {
                                "ru": "Говорили, что существо выдыхает <w>небесный</w> огонь, который невозможно погасить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "empyreal fire",
                        "empyreal substance"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "referring to ancient science or cosmology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий из чистого огня или света, часто в контексте древней космологии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огненный",
                                "target_lang_sense_clarification": "состоящий из огня"
                            },
                            {
                                "target_lang_word": "лучезарный",
                                "target_lang_sense_clarification": "излучающий яркий свет"
                            }
                        ]
                    }
                }
            ]
        }
    ]

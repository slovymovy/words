                {
                    "sense_id": "ef5ff68c-6166-34b0-9c41-1d91f3f6e885",
                    "sense_definition": "Urządzenie komputerowe służące do odczytu lub zapisu danych na nośnikach pamięci, takich jak płyty lub dyskietki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy laptop jest tak cienki, że nie ma wbudowanego <w>napędu</w> optycznego.",
                            "target_lang_translations": {
                                "ru": "Мой новый ноутбук такой тонкий, что в нем нет встроенного оптического <w>привода</w>."
                            }
                        },
                        {
                            "text": "Włóż płytę instalacyjną do <w>napędu</w> i postępuj zgodnie z instrukcjami na ekranie.",
                            "target_lang_translations": {
                                "ru": "Вставьте установочный диск в <w>дисковод</w> и следуйте инструкциям на экране."
                            }
                        }
                    ],
                    "synonyms": [
                        "stacja dysków",
                        "czytnik"
                    ],
                    "common_phrases": [
                        "napęd optyczny",
                        "napęd CD",
                        "napęd DVD",
                        "napęd zewnętrzny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерное устройство для чтения или записи данных на носители памяти, такие как диски."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дисковод",

                {
                    "sense_id": "6604bf0b-4585-3b5b-bff5-6cd9a8243fab",
                    "sense_definition": "Деталь одежды, сумки или рюкзака в виде вшитого или нашитого мешочка для ношения мелких предметов.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я положил ключи в правый <карман> брюк.",
                            "target_lang_translations": {
                                "en": "I put the keys in the right trouser <pocket>."
                            }
                        },
                        {
                            "text": "В нагрудном <кармане> рубашки лежали очки.",
                            "target_lang_translations": {
                                "en": "Glasses were lying in the breast <pocket> of the shirt."
                            }
                        }
                    ],
                    "common_phrases": [
                        "внутренний карман",
                        "задний карман",
                        "пустой карман",
                        "положить в карман"
                    ],
                    "target_lang_definitions": {
                        "en": "A part of clothing, a bag, or a backpack in the form of a sewn-in or sewn-on pouch for carrying small items."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pocket",
                                "target_lang_sense_clarification": "part of clothing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37ebf694-b2dc-3da4-9f64-e73901e09cdd",

                            {
                                "target_lang_word": "указывать",
                                "target_lang_sense_clarification": "в документах, данные"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01d8e99b-427c-3d43-8576-a0fc56e9563d",
                    "sense_definition": "Aplikować choremu lekarstwo; dawać lek do zażycia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transfer_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pielęgniarka przychodzi codziennie, aby <podawać> pacjentowi insulinę.",
                            "target_lang_translations": {
                                "ru": "Медсестра приходит ежедневно, чтобы <вводить> пациенту инсулин."
                            }
                        },
                        {
                            "text": "Lekarz zalecił, by <podawać> ten syrop tylko po jedzeniu.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал <давать> этот сироп только после еды."
                            }

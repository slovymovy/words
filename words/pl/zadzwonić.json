    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e7f2f314-92af-302e-b64c-8d7dfcde0b52",
                    "sense_definition": "Nawiązać połączenie telefoniczne z kimś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę pilnie <zadzwonić> do mamy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно срочно <позвонить> маме."
                            }
                        },
                        {
                            "text": "Proszę <zadzwonić> do mnie jutro rano.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <позвоните> мне завтра утром."
                            }
                        },
                        {
                            "text": "Zanim przyjdziesz, powinieneś <zadzwonić>.",
                            "target_lang_translations": {
                                "ru": "Прежде чем прийти, тебе следует <позвонить>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatelefonować"
                    ],
                    "common_phrases": [
                        "zadzwonić do kogoś",
                        "zadzwonić po pomoc",
                        "zadzwonić na policję",
                        "zadzwonić po taksówkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связаться с кем-либо по телефону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позвонить",
                                "target_lang_sense_clarification": "по телефону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "066412e6-9eef-3a99-9ff8-edcccf9d39df",
                    "sense_definition": "Wydać dźwięk (o telefonie, dzwonku, budziku lub dzwonie).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagle w kieszeni <zadzwonił> mój telefon.",
                            "target_lang_translations": {
                                "ru": "Вдруг у меня в кармане <зазвонил> телефон."
                            }
                        },
                        {
                            "text": "Budzik <zadzwonił> o szóstej, ale nie chciałem wstawać.",
                            "target_lang_translations": {
                                "ru": "Будильник <прозвенел> в шесть, но я не хотел вставать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "telefon zadzwonił",
                        "budzik zadzwonił"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издать звук (о телефоне, звонке или будильнике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зазвонить",
                                "target_lang_sense_clarification": "начать издавать звук"
                            },
                            {
                                "target_lang_word": "прозвенеть",
                                "target_lang_sense_clarification": "издать звон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04c39755-30e3-3e7c-a148-63f2e115542b",
                    "sense_definition": "Użyć dzwonka (zazwyczaj przy wejściu), aby zasygnalizować swoją obecność.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "signaling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Listonosz <zadzwonił> do drzwi, aby doręczyć paczkę.",
                            "target_lang_translations": {
                                "ru": "Почтальон <позвонил> в дверь, чтобы вручить посылку."
                            }
                        },
                        {
                            "text": "Nie mam kluczy, więc muszę <zadzwonić> domofonem.",
                            "target_lang_translations": {
                                "ru": "У меня нет ключей, поэтому я должен <позвонить> в домофон."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zadzwonić do drzwi",
                        "zadzwonić dzwonkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нажать на кнопку звонка (обычно у двери), чтобы сообщить о своём приходе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позвонить",
                                "target_lang_sense_clarification": "в дверь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
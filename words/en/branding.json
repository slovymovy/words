                {
                    "sense_id": "1a0809bd-1152-39a7-b3ed-717e9777c217",
                    "sense_definition": "The promotion of a commercial brand of product or service in order to give it greater public awareness.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_marketing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Effective <w>branding</w> helps a company stand out in a crowded market.",
                            "target_lang_translations": {
                                "ru": "Эффективный <w>брендинг</w> помогает компании выделиться на перенасыщенном рынке."
                            }
                        },
                        {
                            "text": "They hired an expert to improve their corporate <w>branding</w> strategy.",
                            "target_lang_translations": {
                                "ru": "Они наняли эксперта, чтобы улучшить стратегию корпоративного <w>брендинга</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "marketing",
                        "promotion"
                    ],
                    "common_phrases": [
                        "corporate branding",
                        "personal branding",
                        "branding strategy",
                        "branding campaign"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продвижение коммерческого бренда продукта или услуги с целью повышения их узнаваемости среди общественности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брендинг",
                                "target_lang_sense_clarification": "процесс создания и развития бренда"
                            },
                            {
                                "target_lang_word": "создание бренда",
                                "target_lang_sense_clarification": "разработка имиджа компании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "847b9a63-8954-39f6-bbdd-a839d0045c6c",
                    "sense_definition": "The visual design elements, such as logos, colors, and packaging, used to represent a product or company.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "business_marketing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store removed all original <w>branding</w> from the boxes before selling the items.",
                            "target_lang_translations": {
                                "ru": "Магазин удалил всю оригинальную <w>маркировку</w> с коробок перед продажей товаров."
                            }
                        },
                        {
                            "text": "We need to update our <w>branding</w> to look more modern and appealing.",
                            "target_lang_translations": {
                                "ru": "Нам нужно обновить наш <w>фирменный стиль</w>, чтобы выглядеть более современно и привлекательно."
                            }
                        }
                    ],
                    "synonyms": [
                        "trade dress",
                        "labeling"
                    ],
                    "common_phrases": [
                        "remove branding",
                        "branding guidelines",
                        "consistent branding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Визуальные элементы дизайна, такие как логотипы, цвета и упаковка, используемые для представления продукта или компании."
                    },
                    "translations": {
                        "ru": [
                            {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0796c787-3fbb-305c-a8e5-27c6800eac57",
                    "sense_definition": "Ход развития, изменение состояния какого-либо явления или процесса.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач отметил, что у пациента наблюдается положительная <w>динамика</w>.",
                            "target_lang_translations": {
                                "en": "The doctor noted that the patient is showing a positive <w>trend</w>."
                            }
                        },
                        {
                            "text": "Экономическая <w>динамика</w> страны за последние годы значительно улучшилась.",
                            "target_lang_translations": {
                                "en": "The country's economic <w>dynamics</w> have improved significantly in recent years."
                            }
                        }
                    ],
                    "synonyms": [
                        "развитие",
                        "ход",
                        "изменение"
                    ],
                    "antonyms": [
                        "стагнация",
                        "застой"
                    ],
                    "common_phrases": [
                        "положительная динамика",
                        "отрицательная динамика",
                        "динамика цен",
                        "динамика роста"
                    ],
                    "target_lang_definitions": {
                        "en": "The course of development or change in the state of a phenomenon or process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dynamics",
                                "target_lang_sense_clarification": "pattern of change or growth"
                            },
                            {
                                "target_lang_word": "trend",
                                "target_lang_sense_clarification": "general direction of change"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b66a308-0793-32ad-b268-374c776ee72e",
                    "sense_definition": "Раздел механики, изучающий движение тел под действием приложенных к ним сил.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На уроке физики мы начали изучать <w>динамику</w> твердого тела.",
                            "target_lang_translations": {
                                "en": "In physics class, we started studying rigid body <w>dynamics</w>."
                            }
                        },
                        {
                            "text": "Законы <w>динамики</w> позволяют рассчитать траекторию движения космического корабля.",
                            "target_lang_translations": {
                                "en": "The laws of <w>dynamics</w> allow calculating the trajectory of a spaceship."
                            }
                        }
                    ],
                    "antonyms": [
                        "статика"
                    ],
                    "common_phrases": [
                        "законы динамики",
                        "газовая динамика"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "physics"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A branch of mechanics studying the motion of bodies under the action of forces."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dynamics",
                                "target_lang_sense_clarification": "branch of physics"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4af94b2-987f-31e5-9439-4aa8a2922e9a",
                    "sense_definition": "Насыщенность действием, движением или активностью (в сюжете, музыке и т.п.).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этому фильму явно не хватает <w>динамики</w>, сюжет развивается слишком медленно.",
                            "target_lang_translations": {
                                "en": "This movie clearly lacks <w>pacing</w>, the plot develops too slowly."
                            }
                        },
                        {
                            "text": "В музыке чувствовалась бешеная <w>динамика</w>, заставляющая всех танцевать.",
                            "target_lang_translations": {
                                "en": "There was a wild <w>energy</w> in the music that made everyone dance."
                            }
                        }
                    ],
                    "synonyms": [
                        "активность",
                        "темп",
                        "движение"
                    ],
                    "antonyms": [
                        "статика",
                        "покой"
                    ],
                    "common_phrases": [
                        "сюжетная динамика",
                        "терять динамику"
                    ],
                    "target_lang_definitions": {
                        "en": "Saturation with action, movement, or activity (in a plot, music, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pacing",
                                "target_lang_sense_clarification": "speed of plot development"
                            },
                            {
                                "target_lang_word": "dynamism",
                                "target_lang_sense_clarification": "quality of being energetic"
                            },
                            {
                                "target_lang_word": "energy",
                                "target_lang_sense_clarification": "liveliness or intensity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec321b9f-2cf9-31c5-8c9c-ac1aed78ea55",
                    "sense_definition": "Форма родительного падежа единственного числа существительного <w>динамик</w>.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не слышу никакого звука из левого <w>динамика</w>.",
                            "target_lang_translations": {
                                "en": "I don't hear any sound from the left <w>speaker</w>."
                            }
                        },
                        {
                            "text": "У этого старого <w>динамика</w> порвалась защитная сетка.",
                            "target_lang_translations": {
                                "en": "The protective mesh on this old <w>loudspeaker</w> has torn."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive singular form of the noun 'динамик' (speaker)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speaker",

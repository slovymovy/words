            "senses": [
                {
                    "sense_id": "510c2eb8-98f3-392e-baab-aace77fb8439",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>gelukken</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

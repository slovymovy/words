{
    "word_family": [
        "releasable",
        "releaser"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6a4b3d12-1ac5-3ff2-acfd-fd92698d12c2",
                    "sense_definition": "To stop holding or gripping something physically.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "liberation_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>release</w> the handle once the door is fully open."
                        },
                        {
                            "text": "He refused to <w>release</w> his grip on the railing despite the wind."
                        }
                    ],
                    "antonyms": [
                        "hold"
                    ],
                    "common_phrases": [
                        "release the brake",
                        "release the button",
                        "release the handle"
                    ]
                },
                {
                    "sense_id": "e1eec2ef-51fd-3827-9796-32d177ae5bbd",
                    "sense_definition": "To set a person or animal free from confinement or imprisonment.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "liberation_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The authorities agreed to <w>release</w> the prisoners after the negotiations."
                        },
                        {
                            "text": "They plan to <w>release</w> the recovered bird back into the wild tomorrow."
                        }
                    ],
                    "antonyms": [
                        "hold"
                    ],
                    "common_phrases": [
                        "release from prison",
                        "release a hostage",
                        "release on bail"
                    ]
                },
                {
                    "sense_id": "2dc9ef4f-fc0e-3022-a9ad-326325ecee62",
                    "sense_definition": "To make information, a product, or entertainment available to the public.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "publication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company plans to <w>release</w> the new smartphone model next month."
                        },
                        {
                            "text": "The police decided to <w>release</w> a statement regarding the accident."
                        }
                    ],
                    "common_phrases": [
                        "release a movie",
                        "release an album",
                        "release a statement",
                        "release a version"
                    ]
                },
                {
                    "sense_id": "b89916a4-e91a-3cb0-8bea-d2db608748ce",
                    "sense_definition": "To allow a substance, gas, or energy to escape or flow out.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chemical reaction will <w>release</w> a significant amount of heat."
                        },
                        {
                            "text": "Factories must not <w>release</w> toxic waste into the local river systems."
                        }
                    ],
                    "common_phrases": [
                        "release energy",
                        "release gas",
                        "release into the atmosphere"
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3b963735-e5f3-320e-8795-ffd7ba95686a",
                    "sense_definition": "Something that has recently been made available for the public to buy or see, such as a movie or record.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "publication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am looking forward to listening to the band's latest <w>release</w>."
                        },
                        {
                            "text": "The video store advertised that it had all the new <w>releases</w> in stock."
                        }
                    ],
                    "common_phrases": [
                        "new release",
                        "latest release",
                        "press release"
                    ]
                },
                {
                    "sense_id": "0212a747-a96b-3ec5-b133-4588508f38f4",
                    "sense_definition": "The act of setting a person, animal, or thing free from confinement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "liberation_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>release</w> of the hostages was celebrated by the entire nation."
                        },
                        {
                            "text": "We watched the <w>release</w> of the butterflies into the garden."
                        }
                    ],
                    "common_phrases": [
                        "release of prisoners",
                        "early release",
                        "work release"
                    ]
                },
                {
                    "sense_id": "ea88e66a-8933-31d0-bc85-212816f1a4b0",
                    "sense_definition": "The feeling of being freed from stress, pain, or emotional tension.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relief_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Crying often provides a sense of emotional <w>release</w> after a hard day."
                        },
                        {
                            "text": "The massage gave him a great <w>release</w> from the tension in his back."
                        }
                    ],
                    "synonyms": [
                        "relief",
                        "comfort",
                        "solace",
                        "alleviation",
                        "ease"
                    ],
                    "common_phrases": [
                        "sense of release",
                        "emotional release",
                        "find release"
                    ]
                },
                {
                    "sense_id": "087c5843-e750-3732-94ac-471c7815c1f4",
                    "sense_definition": "A mechanical device, such as a button or catch, that releases a mechanism.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to press the shutter <w>release</w> gently to take a clear photo."
                        },
                        {
                            "text": "The emergency <w>release</w> on the door jammed during the safety inspection."
                        }
                    ],
                    "common_phrases": [
                        "shutter release",
                        "safety release",
                        "emergency release"
                    ]
                }
            ]
        }
    ]
}
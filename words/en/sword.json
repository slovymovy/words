    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "604dae9f-8225-3fc4-af32-dab1b23db717",
                    "sense_definition": "A long bladed weapon with a grip and typically a pommel and crossguard, designed to cut, stab, slash, or hack.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knight drew his <w>sword</w> to defend the castle.",
                            "target_lang_translations": {
                                "ru": "Рыцарь обнажил свой <w>меч</w>, чтобы защитить замок."
                            }
                        },
                        {
                            "text": "Be careful when handling the weapon, as the <w>sword</w> is very sharp.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен при обращении с оружием, так как <w>меч</w> очень острый."
                            }
                        }
                    ],
                    "common_phrases": [
                        "draw a sword",
                        "put to the sword",
                        "cross swords",
                        "double-edged sword"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинное клинковое оружие с рукоятью и гардой, предназначенное для нанесения колющих и рубящих ударов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меч",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "шпага",
                                "target_lang_sense_clarification": "в контексте фехтования или более поздних эпох"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5630201-0065-3104-96a4-704ac073fb86",
                    "sense_definition": "A suit in certain playing card decks, particularly those used in Spain and Italy, or a card of this suit.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "card_games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Queen of <w>Swords</w> represents a sharp intellect in this tarot deck.",
                            "target_lang_translations": {
                                "ru": "Королева <w>Мечей</w> олицетворяет острый ум в этой колоде Таро."
                            }
                        },
                        {
                            "text": "He needed a card from the suit of <w>swords</w> to complete his winning hand.",
                            "target_lang_translations": {
                                "ru": "Ему нужна была карта масти <w>мечей</w>, чтобы собрать выигрышную комбинацию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "suit of swords",
                        "king of swords"
                    ],
                    "target_lang_definitions": {
                        "ru": "Масть в некоторых колодах игральных карт (особенно в Таро или итало-испанских колодах), изображаемая в виде мечей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мечи",
                                "target_lang_sense_clarification": "карточная масть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "480b6ab4-77b4-37c4-b610-bcf990f44df1",
                    "sense_definition": "One of the end bars by which the lay of a hand loom is suspended.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "weaving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The weaver carefully adjusted the <w>sword</w> to ensure the fabric was tight.",
                            "target_lang_translations": {
                                "ru": "Ткач осторожно отрегулировал <w>меч</w> станка, чтобы ткань была плотной."
                            }
                        },
                        {
                            "text": "This antique loom cannot be used because it has a broken <w>sword</w>.",
                            "target_lang_translations": {
                                "ru": "Этот старинный ткацкий станок нельзя использовать, так как у него сломан <w>меч</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "weaving term"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из вертикальных стоек, на которых подвешен батан ручного ткацкого станка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меч",
                                "target_lang_sense_clarification": "деталь ткацкого станка"
                            },
                            {
                                "target_lang_word": "стойка батана",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {

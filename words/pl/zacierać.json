    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "727115e6-096e-3aee-ad99-a665f3ce5f48",
                    "sense_definition": "Czynić coś niewyraźnym, usuwać lub niszczyć ślady czegoś poprzez pocieranie lub inne działanie fizyczne.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "blurring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprawca próbował nieudolnie <w>zacierać</w> ślady pozostawione na miejscu przestępstwa.",
                            "target_lang_translations": {
                                "ru": "Преступник пытался неумело <w>заметать</w> следы, оставленные на месте преступления."
                            }
                        },
                        {
                            "text": "Padający gęsty śnieg zaczął szybko <w>zacierać</w> nasze tropy na leśnej ścieżce.",
                            "target_lang_translations": {
                                "ru": "Падающий густой снег начал быстро <w>заметать</w> наши следы на лесной тропинке."
                            }
                        }
                    ],
                    "synonyms": [
                        "zamazywać",
                        "usuwać",
                        "niwelować"
                    ],
                    "antonyms": [
                        "utrwalać",
                        "uwydatniać"
                    ],
                    "common_phrases": [
                        "zacierać ślady",
                        "zacierać granice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то неясным, удалять или уничтожать следы чего-либо трением или другим физическим действием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заметать",
                                "target_lang_sense_clarification": "следы"
                            },
                            {
                                "target_lang_word": "стирать",
                                "target_lang_sense_clarification": "удалять написанное или следы"
                            },
                            {
                                "target_lang_word": "затирать",
                                "target_lang_sense_clarification": "делать невидным посредством трения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e93072c-c262-379a-8eee-3e2ac31848c9",
                    "sense_definition": "Stawać się niewyraźnym, tracić ostrość konturów lub wyrazistość (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "blurring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z biegiem lat wspomnienia z wczesnego dzieciństwa zaczynają <w>zacierać</w> się w mojej pamięci.",
                            "target_lang_translations": {
                                "ru": "С годами воспоминания о раннем детстве начинают <w>стираться</w> в моей памяти."
                            }
                        },
                        {
                            "text": "Różnice społeczne w tym regionie powoli <w>zacierają</w> się dzięki szybkiemu rozwojowi gospodarczemu.",
                            "target_lang_translations": {
                                "ru": "Социальные различия в этом регионе медленно <w>сглаживаются</w> благодаря быстрому экономическому развитию."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozmywać się",
                        "zanikać",
                        "blednąć"
                    ],
                    "antonyms": [
                        "wyostrzać się"
                    ],
                    "common_phrases": [
                        "obraz się zaciera",
                        "pamięć się zaciera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться неясным, терять четкость контуров или выразительность (о воспоминаниях, границах, различиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стираться",
                                "target_lang_sense_clarification": "исчезать из памяти или терять очертания"
                            },
                            {
                                "target_lang_word": "сглаживаться",
                                "target_lang_sense_clarification": "о различиях или противоречиях"
                            },
                            {
                                "target_lang_word": "тускнеть",
                                "target_lang_sense_clarification": "становиться менее ярким (о воспоминаниях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46577f6d-0ad2-3705-ab88-2b41e388e036",
                    "sense_definition": "Sprawiać, że pewne fakty, zdarzenia lub wrażenia stają się mniej widoczne, zapomniane lub trudne do wykrycia.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "blurring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polityk starał się za wszelką cenę <w>zacierać</w> złe wrażenie po swojej niefortunnej wypowiedzi.",
                            "target_lang_translations": {
                                "ru": "Политик старался любой ценой <w>сгладить</w> плохое впечатление после своего неудачного высказывания."
                            }
                        },
                        {
                            "text": "Czas potrafi skutecznie <w>zacierać</w> w ludzkiej pamięci przykre doświadczenia z przeszłości.",
                            "target_lang_translations": {
                                "ru": "Время умеет эффективно <w>стирать</w> из человеческой памяти неприятные переживания прошлого."
                            }
                        }
                    ],
                    "synonyms": [
                        "tuszować",
                        "ukrywać",
                        "maskować"
                    ],
                    "antonyms": [
                        "eksponować",
                        "ujawniać"
                    ],
                    "common_phrases": [
                        "zacierać złe wrażenie",
                        "zacierać różnice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать факты, события или впечатления менее заметными, забытыми или трудными для обнаружения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сглаживать",
                                "target_lang_sense_clarification": "впечатление или конфликт"
                            },
                            {
                                "target_lang_word": "затушевывать",
                                "target_lang_sense_clarification": "скрывать недостатки или факты"
                            },
                            {
                                "target_lang_word": "стирать",
                                "target_lang_sense_clarification": "из памяти или истории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "874c4247-0503-3709-92c8-9f92351715ca",
                    "sense_definition": "Przyrządzać potrawę mączną, mieszając i rozcierając składniki palcami lub narzędziem.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia uczyła mnie, jak ręcznie <w>zacierać</w> ciasto na domowe kluski.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка учила меня, как вручную <w>перетирать</w> тесто для домашних клецок."
                            }
                        },
                        {
                            "text": "Kucharz zaczął energicznie <w>zacierać</w> mąkę z jajkami, aby przygotować tradycyjną zupę.",
                            "target_lang_translations": {
                                "ru": "Повар начал энергично <w>перетирать</w> муку с яйцами, чтобы приготовить традиционный суп."
                            }
                        }
                    ],
                    "synonyms": [
                        "mieszać",
                        "ucierać"
                    ],
                    "common_phrases": [
                        "zacierać ciasto",
                        "zacierać kluski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовить мучное блюдо, смешивая и растирая ингредиенты пальцами или инструментом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перетирать",
                                "target_lang_sense_clarification": "кулинарный термин"
                            },
                            {
                                "target_lang_word": "растирать",
                                "target_lang_sense_clarification": "смешивать до однородности"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "precision",
        "precisely",
        "preciseness",
        "imprecision",
        "imprecise"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c94e522b-36e5-378b-9124-5c1be71f69a1",
                    "sense_definition": "Marked by exactness and accuracy of expression or detail; clearly expressed or delineated.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please provide the <w>precise</w> time of your arrival so we can meet you.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, сообщите <w>точное</w> время вашего прибытия, чтобы мы могли вас встретить."
                            }
                        },
                        {
                            "text": "At that <w>precise</w> moment, the lights went out and the room fell silent.",
                            "target_lang_translations": {
                                "ru": "В тот <w>самый</w> момент свет погас, и в комнате воцарилась тишина."
                            }
                        }
                    ],
                    "synonyms": [
                        "exact",
                        "accurate",
                        "specific",
                        "detailed"
                    ],
                    "antonyms": [
                        "inexact",
                        "imprecise"
                    ],
                    "common_phrases": [
                        "precise moment",
                        "precise details",
                        "precise location",
                        "precise nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличающийся точностью и правильностью выражения или деталей; ясно выраженный или очерченный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точный",
                                "target_lang_sense_clarification": "об информации, времени или деталях"
                            },
                            {
                                "target_lang_word": "конкретный",
                                "target_lang_sense_clarification": "определенный, специфический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7eb57d31-8228-339a-98df-f950a1459ad1",
                    "sense_definition": "Characterized by consistent results or measurements that are clustered closely together, regardless of whether they are accurate to the true value.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The instrument is very <w>precise</w>, giving the same reading every time it is used.",
                            "target_lang_translations": {
                                "ru": "Этот прибор очень <w>точный</w>, он дает одни и те же показания при каждом использовании."
                            }
                        },
                        {
                            "text": "Scientific experiments require <w>precise</w> measurements to ensure the validity of the data.",
                            "target_lang_translations": {
                                "ru": "Научные эксперименты требуют <w>точных</w> измерений для обеспечения достоверности данных."
                            }
                        }
                    ],
                    "synonyms": [
                        "consistent"
                    ],
                    "antonyms": [
                        "inconsistent",
                        "varying"
                    ],
                    "common_phrases": [
                        "precise measurement",
                        "precise calculation",
                        "precise instrument"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся стабильными результатами или измерениями, сгруппированными близко друг к другу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точный",
                                "target_lang_sense_clarification": "об инструментах или измерениях"
                            },
                            {
                                "target_lang_word": "прецизионный",
                                "target_lang_sense_clarification": "высокоточный (технический термин)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe5b0ce8-d2a3-31fd-9a6c-ce396e7db219",
                    "sense_definition": "Strictly adhering to rules, forms, or details; very careful and particular, sometimes to a fault.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a very <w>precise</w> man who organizes his books by color and size.",
                            "target_lang_translations": {
                                "ru": "Он очень <w>педантичный</w> человек, который расставляет свои книги по цвету и размеру."
                            }
                        },
                        {
                            "text": "Her <w>precise</w> manner of speaking can sometimes seem cold and distant to others.",
                            "target_lang_translations": {
                                "ru": "Ее <w>подчеркнуто точная</w> манера говорить иногда может казаться холодной и отстраненной."
                            }
                        }
                    ],
                    "synonyms": [
                        "punctilious",
                        "meticulous",
                        "fastidious"
                    ],
                    "common_phrases": [
                        "precise manner",
                        "precise habits"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строго придерживающийся правил или деталей; очень тщательный, иногда чрезмерно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "педантичный",
                                "target_lang_sense_clarification": "чрезмерно строгий к мелочам"
                            },
                            {
                                "target_lang_word": "скрупулезный",
                                "target_lang_sense_clarification": "очень тщательный"
                            },
                            {
                                "target_lang_word": "аккуратный",
                                "target_lang_sense_clarification": "любящий порядок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6de424d6-fea2-39a4-9064-5213819229fe",
                    "sense_definition": "To make something precise or to specify details.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee needs to <w>precise</w> the terms of the agreement before signing.",
                            "target_lang_translations": {
                                "ru": "Комитету необходимо <w>уточнить</w> условия соглашения перед подписанием."
                            }
                        },
                        {
                            "text": "He tried to <w>precise</w> his meaning, but the explanation remained unclear.",
                            "target_lang_translations": {
                                "ru": "Он попытался <w>конкретизировать</w> свою мысль, но объяснение осталось неясным."
                            }
                        }
                    ],
                    "synonyms": [
                        "specify",
                        "define"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо точным или указывать детали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уточнять",
                                "target_lang_sense_clarification": "делать более точным"
                            },
                            {
                                "target_lang_word": "конкретизировать",
                                "target_lang_sense_clarification": "приводить детали"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a0998a5b-35d9-3687-b93f-4a49f3d9a76c",
                    "sense_definition": "Een officieel document waarin feiten, afspraken of rechtshandelingen zijn vastgelegd om als bewijs te dienen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De notaris heeft de <w>akte</w> voor de aankoop van het huis gisteren getekend.",
                            "target_lang_translations": {
                                "en": "The notary signed the <w>deed</w> for the purchase of the house yesterday.",
                                "ru": "Вчера нотариус подписал <w>акт</w> о покупке дома."
                            }
                        },
                        {
                            "text": "Je hebt een officiële <w>akte</w> van overlijden nodig om de erfenis te regelen.",
                            "target_lang_translations": {
                                "en": "You need an official death <w>certificate</w> to settle the inheritance.",
                                "ru": "Вам нужно официальное <w>свидетельство</w> о смерти, чтобы оформить наследство."
                            }
                        }
                    ],
                    "synonyms": [
                        "oorkonde",
                        "bewijsstuk",
                        "document"
                    ],
                    "common_phrases": [
                        "akte van overlijden",
                        "notariële akte",
                        "authentieke akte",
                        "akte van oprichting"
                    ],
                    "target_lang_definitions": {
                        "en": "An official document in which facts, agreements, or legal acts are recorded to serve as evidence.",
                        "ru": "Официальный документ, в котором зафиксированы факты, соглашения или юридические действия, служащий доказательством."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deed",
                                "target_lang_sense_clarification": "legal document regarding ownership or rights"
                            },
                            {
                                "target_lang_word": "certificate",
                                "target_lang_sense_clarification": "official record of an event like birth or death"
                            },
                            {
                                "target_lang_word": "legal instrument",
                                "target_lang_sense_clarification": "formal legal document"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "акт",
                                "target_lang_sense_clarification": "юридический документ"
                            },
                            {
                                "target_lang_word": "свидетельство",
                                "target_lang_sense_clarification": "например, о рождении или смерти"
                            },
                            {
                                "target_lang_word": "документ",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df0f8551-9866-31ca-8f9d-a66ab8eaea53",
                    "sense_definition": "Een hoofdeel van een toneelstuk, opera of film.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de pauze na de eerste <w>akte</w> dronken we een kopje koffie.",
                            "target_lang_translations": {
                                "en": "During the intermission after the first <w>act</w>, we drank a cup of coffee.",
                                "ru": "Во время антракта после первого <w>акта</w> мы выпили чашку кофе."
                            }
                        },
                        {
                            "text": "Het verhaal wordt in de laatste <w>akte</w> van de voorstelling erg spannend.",
                            "target_lang_translations": {
                                "en": "The story becomes very exciting in the last <w>act</w> of the performance.",
                                "ru": "В последнем <w>акте</w> представления сюжет становится очень захватывающим."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedrijf"
                    ],
                    "common_phrases": [
                        "eerste akte",
                        "tweede akte",
                        "laatste akte"
                    ],
                    "target_lang_definitions": {
                        "en": "A main division of a play, opera, or film.",
                        "ru": "Основная часть пьесы, оперы или фильма."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "act",
                                "target_lang_sense_clarification": "part of a performance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "акт",
                                "target_lang_sense_clarification": "часть произведения"
                            },
                            {
                                "target_lang_word": "действие",
                                "target_lang_sense_clarification": "театральный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "kitten",
        "afkitten",
        "kitspuit",
        "kitrand",
        "kitwerk"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aff8abdc-b95b-372a-8765-76e58c5081ea",
                    "sense_definition": "Een dikvloeibaar materiaal dat hard wordt, gebruikt om naden en kieren waterdicht te maken of om materialen aan elkaar te lijmen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sealant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de randen van het bad opnieuw met <w>kit</w> afwerken.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно заново обработать края ванны <w>герметиком</w>."
                            }
                        },
                        {
                            "text": "Gebruik een goede watervaste <w>kit</w> in de badkamer.",
                            "target_lang_translations": {
                                "ru": "Используй хороший водостойкий <w>герметик</w> в ванной."
                            }
                        }
                    ],
                    "synonyms": [
                        "afdichtmiddel"
                    ],
                    "common_phrases": [
                        "naden en kieren",
                        "kit aanbrengen",
                        "oude kit verwijderen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Густой материал, который затвердевает и используется для герметизации швов или склеивания материалов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герметик",
                                "target_lang_sense_clarification": "строительный материал"
                            },
                            {
                                "target_lang_word": "замазка",
                                "target_lang_sense_clarification": "паста для щелей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4e5dd41-7620-39ec-b4fc-03b18acd2f1f",
                    "sense_definition": "Een verzameling gereedschappen, onderdelen of benodigdheden die samen een geheel vormen voor een specifiek doel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "set",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kocht een handige <w>kit</w> om zijn fietsband te plakken.",
                            "target_lang_translations": {
                                "ru": "Он купил удобный <w>набор</w> для ремонта велосипедных шин."
                            }
                        },
                        {
                            "text": "In de <w>kit</w> zaten alle onderdelen die nodig waren voor de montage.",
                            "target_lang_translations": {
                                "ru": "В <w>наборе</w> были все детали, необходимые для сборки."
                            }
                        }
                    ],
                    "synonyms": [
                        "set",
                        "uitrusting",
                        "pakket"
                    ],
                    "common_phrases": [
                        "doe-het-zelf-kit",
                        "hands-free kit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор инструментов, деталей или принадлежностей, собранных вместе для определённой цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набор",
                                "target_lang_sense_clarification": "комплект инструментов"
                            },
                            {
                                "target_lang_word": "комплект",
                                "target_lang_sense_clarification": "совокупность предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9109a48d-834f-3d6f-907f-0baeaa7fe437",
                    "sense_definition": "Een metalen bak of kan die traditioneel gebruikt wordt om kolen in te bewaren en in de kachel te scheppen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn opa vulde elke avond de <w>kit</w> met kolen uit de schuur.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка каждый вечер наполнял <w>ведро для угля</w> углем из сарая."
                            }
                        },
                        {
                            "text": "De oude koperen <w>kit</w> staat nu als decoratie naast de haard.",
                            "target_lang_translations": {
                                "ru": "Старое медное <w>ведро для угля</w> теперь стоит как украшение у камина."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolenkit"
                    ],
                    "common_phrases": [
                        "een kit kolen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to an object used for coal heating"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический контейнер или ведро, традиционно используемое для хранения угля и засыпания его в печь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ведро для угля",
                                "target_lang_sense_clarification": "ёмкость для топлива"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "87003afd-6fdf-3401-a84e-4a24ce444d75",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>kitten</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kit</w> de naden van de douchecabine zorgvuldig dicht.",
                            "target_lang_translations": {
                                "ru": "Я тщательно <w>герметизирую</w> швы душевой кабины."
                            }
                        },
                        {
                            "text": "Vandaag <w>kit</w> ik de ramen zodat ze niet meer lekken.",
                            "target_lang_translations": {
                                "ru": "Сегодня я <w>замазываю</w> окна, чтобы они больше не текли."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола 'kitten' (наносить герметик)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герметизирую",
                                "target_lang_sense_clarification": "заделываю швы"
                            },
                            {
                                "target_lang_word": "замазываю",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0149ad53-93d6-30b2-a42f-9588c0a55a01",
                    "sense_definition": "Gebiedende wijs van <w>kitten</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Kit</w> die randen even goed dicht voordat je gaat schilderen.",
                            "target_lang_translations": {
                                "ru": "<w>Замажь</w> эти края хорошенько, прежде чем красить."
                            }
                        },
                        {
                            "text": "<w>Kit</w> voorzichtig, anders wordt het een kliederboel.",
                            "target_lang_translations": {
                                "ru": "<w>Наноси герметик</w> осторожно, иначе будет мазня."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола 'kitten' (наносить герметик)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загерметизируй",
                                "target_lang_sense_clarification": "приказ/просьба"
                            },
                            {
                                "target_lang_word": "замажь",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "927ec22f-1430-3f00-842a-5bb9a491e0d6",
                    "sense_definition": "Zich ontspannen na vermoeiende of langdurige bezigheden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "rest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zijn bijna bij het bivak waar we kunnen <w>uitrusten</w>.",
                            "target_lang_translations": {
                                "en": "We are almost at the bivouac where we can <w>rest</w>.",
                                "ru": "Мы почти добрались до бивака, где сможем <w>отдохнуть</w>."
                            }
                        },
                        {
                            "text": "Na het harde werken wil ik even lekker <w>uitrusten</w> op de bank.",
                            "target_lang_translations": {
                                "en": "After the hard work, I want to <w>rest</w> comfortably on the couch.",
                                "ru": "После тяжелой работы я хочу немного <w>отдохнуть</w> на диване."
                            }
                        }
                    ],
                    "synonyms": [
                        "rusten",
                        "ontspannen"
                    ],
                    "common_phrases": [
                        "even uitrusten",
                        "lekker uitrusten"
                    ],
                    "target_lang_definitions": {
                        "en": "To cease work or movement in order to relax, refresh oneself, or recover strength.",
                        "ru": "Прекратить работу или движение, чтобы расслабиться и восстановить силы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rest",
                                "target_lang_sense_clarification": "to stop activity to recover strength"
                            },
                            {
                                "target_lang_word": "relax",
                                "target_lang_sense_clarification": "to become less tense or anxious"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отдыхать",
                                "target_lang_sense_clarification": "восстанавливать силы"
                            },
                            {
                                "target_lang_word": "отдохнуть",
                                "target_lang_sense_clarification": "совершенный вид глагола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ed98dc8-891e-327e-ae45-0d8c83b4e989",
                    "sense_definition": "Personen, vaartuigen of voertuigen voorzien van de benodigdheden voor een taak, expeditie of reis.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "equip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vloot werd <w>uitgerust</w> met een nieuw radarsysteem.",
                            "target_lang_translations": {
                                "en": "The fleet was <w>equipped</w> with a new radar system.",
                                "ru": "Флот был <w>оснащён</w> новой радиолокационной системой."
                            }
                        },
                        {
                            "text": "Alle nieuwe auto's zijn standaard <w>uitgerust</w> met airconditioning en airbags.",
                            "target_lang_translations": {
                                "en": "All new cars are standardly <w>equipped</w> with air conditioning and airbags.",
                                "ru": "Все новые автомобили в стандартной комплектации <w>оснащены</w> кондиционером и подушками безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "toerusten"
                    ],
                    "antonyms": [
                        "ontmantelen",
                        "onttakelen"
                    ],
                    "common_phrases": [
                        "uitrusten met"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide someone or something with the necessary items for a particular purpose, task, or journey.",
                        "ru": "Снабжать кого-либо или что-либо необходимыми предметами для выполнения задачи или путешествия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "equip",
                                "target_lang_sense_clarification": "to supply with necessary items"
                            },
                            {
                                "target_lang_word": "fit out",
                                "target_lang_sense_clarification": "to supply with necessaries, especially a ship or vehicle"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оснащать",
                                "target_lang_sense_clarification": "снабжать техникой или оборудованием"
                            },
                            {
                                "target_lang_word": "оборудовать",
                                "target_lang_sense_clarification": "устанавливать необходимое оборудование"
                            },
                            {
                                "target_lang_word": "снаряжать",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f198308b-3350-34a7-a24c-f417c4b79454",
                    "sense_definition": "To consider someone or something unpleasant, disagreeable, or offensive.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_aversion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>dislike</w> having to wake up early on weekends.",
                            "target_lang_translations": {
                                "ru": "Я <w>не люблю</w> просыпаться рано по выходным."
                            }
                        },
                        {
                            "text": "She seems to <w>dislike</w> the new manager for some reason.",
                            "target_lang_translations": {
                                "ru": "Кажется, она <w>недолюбливает</w> нового менеджера по какой-то причине."
                            }
                        }
                    ],
                    "synonyms": [
                        "object to",
                        "mind",
                        "stomach",
                        "mislike",
                        "disrelish",
                        "disrecommend"
                    ],
                    "antonyms": [
                        "like"
                    ],
                    "common_phrases": [
                        "dislike intensely",
                        "strongly dislike",
                        "really dislike"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать кого-то или что-то неприятным, неприемлемым или оскорбительным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не любить",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "испытывать неприязнь",
                                "target_lang_sense_clarification": "более формально"
                            },
                            {
                                "target_lang_word": "невзлюбить",
                                "target_lang_sense_clarification": "начать испытывать неприязнь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1aec5133-51c2-3d63-91aa-c84e88492715",
                    "sense_definition": "To register disapproval of online content by clicking a specific button.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "internet_vote",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thousands of users <w>disliked</w> the controversial video within an hour.",
                            "target_lang_translations": {
                                "ru": "Тысячи пользователей <w>поставили дизлайк</w> скандальному видео в течение часа."
                            }
                        },
                        {
                            "text": "Please do not <w>dislike</w> the post just because you disagree with the opinion.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>ставьте дизлайк</w> посту только потому, что вы не согласны с мнением."
                            }
                        }
                    ],
                    "antonyms": [
                        "like"
                    ],
                    "common_phrases": [
                        "dislike a video",
                        "dislike a comment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать неодобрение онлайн-контенту, нажимая специальную кнопку."
                    },
                    "translations": {
                        "ru": [

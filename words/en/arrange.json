                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She likes to <w>arrange</w> fresh flowers in a vase on the table."
                        },
                        {
                            "text": "Please <w>arrange</w> the books on the shelf in alphabetical order."
                        }
                    ],
                    "synonyms": [
                        "order",
                        "organize",
                        "array",
                        "set up",
                        "range",
                        "classify"
                    ],
                    "common_phrases": [

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "df4988f8-6c0a-31fe-8727-6631a4e268f6",
                    "sense_definition": "Место для торговли, обычно на открытой площади или в крытых рядах, где продают продукты и вещи.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "trade_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы пошли на <w>базар</w>, чтобы купить свежие овощи и фрукты.",
                            "target_lang_translations": {
                                "en": "We went to the <w>market</w> to buy fresh vegetables and fruits."
                            }
                        },
                        {
                            "text": "Этот шумный восточный <w>базар</w> привлекает много туристов своими красками.",
                            "target_lang_translations": {
                                "en": "This noisy oriental <w>bazaar</w> attracts many tourists with its colors."
                            }
                        }
                    ],
                    "synonyms": [
                        "рынок"
                    ],
                    "common_phrases": [
                        "восточный базар",
                        "ходить на базар",
                        "городской базар"
                    ],
                    "target_lang_definitions": {
                        "en": "A place for trade, usually outdoors or in covered rows, where food and goods are sold."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "market",
                                "target_lang_sense_clarification": "general place for buying and selling goods"
                            },
                            {
                                "target_lang_word": "bazaar",
                                "target_lang_sense_clarification": "oriental market or street of shops"
                            },
                            {
                                "target_lang_word": "marketplace",
                                "target_lang_sense_clarification": "open space where a market is held"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7073a64a-ba25-3b0b-8213-64ecefc5c04d",
                    "sense_definition": "Организованная сезонная или предпраздничная торговля определёнными видами товаров.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "trade_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед первым сентября в центре города открылся школьный <w>базар</w>.",
                            "target_lang_translations": {
                                "en": "Before the first of September, a school <w>fair</w> opened in the city center."
                            }
                        },
                        {
                            "text": "На площади устроили весёлый новогодний <w>базар</w> с ёлками и игрушками.",
                            "target_lang_translations": {
                                "en": "They organized a cheerful Christmas <w>market</w> with trees and toys on the square."
                            }
                        }
                    ],
                    "synonyms": [
                        "ярмарка"
                    ],
                    "common_phrases": [
                        "новогодний базар",
                        "книжный базар",
                        "школьный базар"
                    ],
                    "target_lang_definitions": {
                        "en": "Organized seasonal or pre-holiday trade of specific types of goods."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fair",
                                "target_lang_sense_clarification": "gathering for buying and selling, often seasonal"
                            },
                            {
                                "target_lang_word": "market",
                                "target_lang_sense_clarification": "seasonal event, e.g., Christmas market"
                            },
                            {
                                "target_lang_word": "bazaar",
                                "target_lang_sense_clarification": "sale of goods, often for charity or holidays"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1336dd1-9998-34d1-a7fb-e668b2996c27",
                    "sense_definition": "Беспорядочный шум, громкие крики многих людей, суматоха.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noise_chaos",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В классе начался настоящий <w>базар</w>, когда учитель вышел из кабинета.",
                            "target_lang_translations": {
                                "en": "A real <w>racket</w> started in the classroom when the teacher left the room."
                            }
                        },
                        {
                            "text": "Прекратите этот <w>базар</w> и успокойтесь, иначе мы ничего не решим.",
                            "target_lang_translations": {
                                "en": "Stop this <w>hubbub</w> and calm down, otherwise we won't decide anything."
                            }
                        }
                    ],
                    "synonyms": [
                        "гвалт",
                        "суматоха",
                        "беспорядок",
                        "неразбериха"
                    ],
                    "common_phrases": [
                        "устроить базар",
                        "птичий базар"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Disorderly noise, loud shouting by many people, commotion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "racket",
                                "target_lang_sense_clarification": "loud, unpleasant noise"
                            },
                            {
                                "target_lang_word": "hubbub",
                                "target_lang_sense_clarification": "confused noise of many voices"
                            },
                            {
                                "target_lang_word": "bedlam",
                                "target_lang_sense_clarification": "scene of uproar and confusion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90f92b89-c8bb-30d9-b232-147b1c584470",
                    "sense_definition": "Разговор, обсуждение или слова (часто пустые, агрессивные или безответственные).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_talk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тебе придётся ответить за свой <w>базар</w> перед ребятами.",
                            "target_lang_translations": {
                                "en": "You will have to answer for your <w>words</w> in front of the guys."
                            }
                        },
                        {
                            "text": "Между ними шёл какой-то странный <w>базар</w> о деньгах и долгах.",
                            "target_lang_translations": {
                                "en": "There was some strange <w>talk</w> going on between them about money and debts."
                            }
                        }
                    ],
                    "common_phrases": [
                        "фильтруй базар",
                        "гнилой базар"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

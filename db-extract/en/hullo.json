                "senses": [
                    {
                        "sense_id": "e394b245-0b55-30f6-ba5a-9b56b0cdc127",
                        "entry_id": "9c1175ec-4d78-31f2-a556-667e0620797a",
                        "glosses": [
                            "Alternative form of hello (Greeting.)"
                        ],
                        "tags": [
                            "New-Zealand",

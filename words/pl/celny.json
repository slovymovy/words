    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a7ce0d5a-20a3-3541-84fd-a06cce2c9751",
                    "sense_definition": "Trafiający dokładnie w cel lub wyznaczone miejsce.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "accuracy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnik oddał silny i <w>celny</w> strzał na bramkę przeciwnika.",
                            "target_lang_translations": {
                                "ru": "Нападающий нанес сильный и <w>точный</w> удар по воротам противника."
                            }
                        },
                        {
                            "text": "Tylko <w>celny</w> rzut mógł uratować drużynę przed przegraną w ostatnich sekundach.",
                            "target_lang_translations": {
                                "ru": "Только <w>точный</w> бросок мог спасти команду от поражения на последних секундах."
                            }
                        }
                    ],
                    "synonyms": [
                        "trafiony"
                    ],
                    "antonyms": [
                        "niecelny",
                        "chybiony"
                    ],
                    "common_phrases": [
                        "celny strzał",
                        "celny rzut",
                        "celne oko",
                        "celny ogień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Попадающий точно в цель или в назначенное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меткий",
                                "target_lang_sense_clarification": "попадающий в цель (о выстреле, ударе или стрелке)"
                            },
                            {
                                "target_lang_word": "точный",
                                "target_lang_sense_clarification": "попадающий в определенное место, безошибочный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65ce5454-af36-3d53-bdaf-4512ab69f98f",
                    "sense_definition": "Trafny, błyskotliwy i idealnie pasujący do danej sytuacji (np. o wypowiedzi).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "accuracy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To była bardzo <w>celna</w> uwaga, która zmieniła przebieg całej dyskusji.",
                            "target_lang_translations": {
                                "ru": "Это было очень <w>меткое</w> замечание, которое изменило ход всей дискуссии."
                            }
                        },
                        {
                            "text": "Jego <w>celna</w> riposta rozbawiła wszystkich zgromadzonych przy stole gości.",
                            "target_lang_translations": {
                                "ru": "Его <w>меткий</w> ответ рассмешил всех гостей, собравшихся за столом."
                            }
                        }
                    ],
                    "synonyms": [
                        "trafny",
                        "właściwy",
                        "słuszny"
                    ],
                    "antonyms": [
                        "chybiony"
                    ],
                    "common_phrases": [
                        "celna uwaga",
                        "celna riposta",
                        "celne spostrzeżenie",
                        "celny argument"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уместный, остроумный и идеально подходящий к ситуации (например, о высказывании)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меткий",
                                "target_lang_sense_clarification": "выразительный, точно подмечающий суть (о словах)"
                            },
                            {
                                "target_lang_word": "точный",
                                "target_lang_sense_clarification": "верный, правильный по смыслу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "921ab7f1-0ce9-3db3-86c0-d54e416e8e3c",
                    "sense_definition": "Związany z cłem, dotyczący opłat i kontroli towarów przewożonych przez granicę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "customs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na granicy czekała nas szczegółowa kontrola <w>celna</w>, która trwała godzinę.",
                            "target_lang_translations": {
                                "ru": "На границе нас ждал тщательный <w>таможенный</w> досмотр, который длился час."
                            }
                        },
                        {
                            "text": "Musisz udać się w to miejsce, gdzie znajduje się najbliższy urząd <w>celny</w>.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно отправиться туда, где находится ближайший <w>таможенный</w> пост."
                            }
                        }
                    ],
                    "synonyms": [
                        "cłowy"
                    ],
                    "common_phrases": [
                        "urząd celny",
                        "kontrola celna",
                        "służba celna",
                        "deklaracja celna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с таможней, касающийся пошлин и контроля товаров на границе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таможенный",
                                "target_lang_sense_clarification": "относящийся к таможне"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
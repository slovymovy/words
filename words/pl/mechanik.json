    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a9920f04-c2da-3d6d-a34d-d79ac2c26182",
                    "sense_definition": "Osoba zajmująca się zawodowo naprawą, konserwacją i obsługą maszyn, urządzeń technicznych lub pojazdów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój samochód się zepsuł, więc muszę szybko znaleźć dobrego <mechanika>.",
                            "target_lang_translations": {
                                "ru": "У меня сломалась машина, поэтому мне нужно быстро найти хорошего <w>механика</w>."
                            }
                        },
                        {
                            "text": "Doświadczony <mechanik> samochodowy od razu wiedział, co stuka w silniku.",
                            "target_lang_translations": {
                                "ru": "Опытный автомобильный <w>механик</w> сразу понял, что стучит в двигателе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mechanik samochodowy",
                        "dobry mechanik",
                        "warsztat mechanika",
                        "pomoc mechanika"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, профессионально занимающееся ремонтом, обслуживанием и эксплуатацией машин, технических устройств или транспортных средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "механик",
                                "target_lang_sense_clarification": "специалист по ремонту"
                            },
                            {
                                "target_lang_word": "автомеханик",
                                "target_lang_sense_clarification": "специалист по ремонту автомобилей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1b35899-f6ad-3c28-85a4-6b556ca392c2",
                    "sense_definition": "Potoczne określenie szkoły zawodowej lub technikum o profilu technicznym.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat zdecydował, że po podstawówce pójdzie do <mechanika>.",
                            "target_lang_translations": {
                                "ru": "Мой брат решил, что после начальной школы пойдет в <w>техникум</w>."
                            }
                        },
                        {
                            "text": "Uczniowie z pobliskiego <mechanika> organizują dzień otwarty dla kandydatów.",
                            "target_lang_translations": {
                                "ru": "Ученики из соседнего <w>техникума</w> организуют день открытых дверей для абитуриентов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chodzić do mechanika",
                        "skończyć mechanika",
                        "uczyć się w mechaniku"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название профессионального училища или техникума технического профиля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "техникум",
                                "target_lang_sense_clarification": "учебное заведение"
                            },
                            {
                                "target_lang_word": "колледж",
                                "target_lang_sense_clarification": "учебное заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6fbcf25-03af-3966-90d2-727a76d4108b",
                    "sense_definition": "Specjalista posiadający wiedzę teoretyczną i praktyczną z dziedziny mechaniki (działu fizyki).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako inżynier <mechanik>, brał udział w projektowaniu nowych mostów.",
                            "target_lang_translations": {
                                "ru": "Как инженер-<w>механик</w>, он участвовал в проектировании новых мостов."
                            }
                        },
                        {
                            "text": "Wybitny <mechanik> teoretyk wygłosił wykład na temat dynamiki płynów.",
                            "target_lang_translations": {
                                "ru": "Выдающийся <w>механик</w>-теоретик прочитал лекцию по динамике жидкостей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "inżynier mechanik",
                        "mechanik precyzyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист, обладающий теоретическими и практическими знаниями в области механики (раздела физики)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "механик",
                                "target_lang_sense_clarification": "учёный или инженер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "420921db-b46a-37b9-b6a7-32649b1bea57",
                    "sense_definition": "Oficer na statku wodnym, który nadzoruje pracę maszynowni i urządzeń technicznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starszy <mechanik> zszedł pod pokład, aby sprawdzić stan silników okrętowych.",
                            "target_lang_translations": {
                                "ru": "Старший <w>механик</w> спустился под палубу, чтобы проверить состояние корабельных двигателей."
                            }
                        },
                        {
                            "text": "Kapitan konsultował się z <mechanikiem> w sprawie zapasów paliwa na rejs.",
                            "target_lang_translations": {
                                "ru": "Капитан советовался с <w>механиком</w> по поводу запасов топлива на рейс."
                            }
                        }
                    ],
                    "antonyms": [
                        "nawigator"
                    ],
                    "common_phrases": [
                        "starszy mechanik",
                        "mechanik okrętowy",
                        "mechanik wachtowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Офицер на судне, который контролирует работу машинного отделения и технических устройств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "механик",
                                "target_lang_sense_clarification": "судовой офицер"
                            },
                            {
                                "target_lang_word": "судовой механик",
                                "target_lang_sense_clarification": "должность на флоте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4055b332-206e-3263-b198-30a37a5694a7",
                    "sense_definition": "Forma słowa <w>mechanika</w> (dopełniacz liczby mnogiej).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej grze komputerowej nie rozumiem skomplikowanych <mechanik> walki.",
                            "target_lang_translations": {
                                "ru": "В этой компьютерной игре я не понимаю сложных боевых <w>механик</w>."
                            }
                        },
                        {
                            "text": "Student nie zaliczył egzaminu z podstaw <mechanik> płynów i gruntów.",
                            "target_lang_translations": {
                                "ru": "Студент не сдал экзамен по основам <w>механики</w> жидкостей и грунтов."
                            }
                        }
                    ],

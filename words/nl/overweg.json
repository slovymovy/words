            "senses": [
                {
                    "sense_id": "a973ca13-0f90-383f-bc96-21d102bbdb02",
                    "sense_definition": "Een plaats waar een weg en een spoorbaan elkaar op gelijke hoogte kruisen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>overweg</w> was gesloten omdat er een lange goederentrein aankwam.",
                            "target_lang_translations": {
                                "en": "The <w>level crossing</w> was closed because a long freight train was approaching.",
                                "ru": "<w>Железнодорожный переезд</w> был закрыт, так как приближался длинный товарный поезд."
                            }
                        },
                        {
                            "text": "Je moet altijd wachten voor de <w>overweg</w> als de rode lichten knipperen.",
                            "target_lang_translations": {
                                "en": "You must always wait before the <w>level crossing</w> when the red lights are flashing.",
                                "ru": "Нужно всегда ждать перед <w>переездом</w>, если мигают красные огни."
                            }
                        }
                    ],
                    "synonyms": [
                        "spoorwegovergang"
                    ],
                    "common_phrases": [
                        "bewaakte overweg",
                        "onbewaakte overweg"
                    ],
                    "target_lang_definitions": {
                        "en": "A place where a road and a railway track cross each other at the same level.",
                        "ru": "Место, где автомобильная дорога и железнодорожные пути пересекаются на одном уровне."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "level crossing",
                                "target_lang_sense_clarification": "common in UK/Europe"
                            },
                            {
                                "target_lang_word": "railroad crossing",
                                "target_lang_sense_clarification": "common in US"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "железнодорожный переезд",
                                "target_lang_sense_clarification": "полное название"
                            },
                            {
                                "target_lang_word": "переезд",
                                "target_lang_sense_clarification": "общеупотребительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3faac3d7-9027-3c57-8b9e-32dbdc9bed7a",
                    "sense_definition": "In staat zijn om goed met iemand op te schieten of iets te beheersen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nadien konden zij een stuk beter met elkaar <w>overweg</w>.",
                            "target_lang_translations": {
                                "en": "Afterwards they could <w>get along</w> much better with each other.",
                                "ru": "Впоследствии они стали гораздо лучше <w>ладить</w> друг с другом."
                            }
                        },
                        {
                            "text": "Mijn opa kan nog steeds niet goed <w>overweg</w> met zijn nieuwe computer.",
                            "target_lang_translations": {
                                "en": "My grandpa still can't <w>handle</w> his new computer well.",
                                "ru": "Мой дедушка до сих пор не умеет хорошо <w>обращаться</w> со своим новым компьютером."
                            }
                        }
                    ],
                    "common_phrases": [
                        "overweg kunnen met"
                    ],
                    "target_lang_definitions": {
                        "en": "To be able to get along well with someone or to manage/control something.",
                        "ru": "Быть способным ладить с кем-либо или уметь пользоваться чем-либо (обычно в выражении 'overweg kunnen met')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get along",
                                "target_lang_sense_clarification": "social interaction (with people)"
                            },
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "managing something (e.g., a device or situation)"
                            },
                            {
                                "target_lang_word": "manage",
                                "target_lang_sense_clarification": "to be able to use or control"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ладить",
                                "target_lang_sense_clarification": "об отношениях с людьми"
                            },
                            {
                                "target_lang_word": "справляться",
                                "target_lang_sense_clarification": "о работе или технике"
                            },
                            {
                                "target_lang_word": "уметь обращаться",

                        "в соответствующей графе",
                        "пустая графа",
                        "пятая графа"
                    ],
                    "target_lang_definitions": {
                        "en": "A column, section, or separate cell in a table, form, or document, intended for entering specific information."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "column",
                                "target_lang_sense_clarification": "vertical section in a table"
                            },
                            {
                                "target_lang_word": "field",
                                "target_lang_sense_clarification": "space for data entry in a form"
                            },
                            {
                                "target_lang_word": "box",
                                "target_lang_sense_clarification": "specific space to fill in"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ce8387c-fd52-3ec7-8203-e3b5b6c8cdd0",

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "99e88fbe-090a-3091-ad8a-123613277571",
                    "sense_definition": "Ongeveer; niet precies, maar in de buurt van een bepaald aantal of een bepaalde hoeveelheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dek de schaal af met folie en zet <w>circa</w> één uur in de oven.",
                            "target_lang_translations": {
                                "en": "Cover the dish with foil and put it in the oven for <w>about</w> one hour."
                            }
                        },
                        {
                            "text": "Er waren <w>circa</w> vijfhonderd mensen aanwezig bij het concert.",
                            "target_lang_translations": {
                                "en": "There were <w>approximately</w> five hundred people present at the concert."
                            }
                        }
                    ],
                    "synonyms": [
                        "ongeveer",
                        "plusminus",
                        "omstreeks",
                        "pakweg",
                        "ruwweg",
                        "grofweg"
                    ],
                    "antonyms": [
                        "precies"
                    ],
                    "common_phrases": [
                        "circa tien",
                        "circa een uur",
                        "circa twintig procent"
                    ],
                    "target_lang_definitions": {
                        "en": "Approximately; close to a specific number or amount without being exact."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "approximately",
                                "target_lang_sense_clarification": "formal, used for quantities"
                            },
                            {
                                "target_lang_word": "about",
                                "target_lang_sense_clarification": "common, general use"
                            },
                            {
                                "target_lang_word": "around",
                                "target_lang_sense_clarification": "informal, used for numbers"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "ee124dc1-a2f0-33f1-bf49-419675f9f560",
                    "sense_definition": "Rond; omstreeks een bepaald tijdstip of jaartal.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kasteel is gebouwd <w>circa</w> veertienhonderd en is nog steeds in goede staat.",
                            "target_lang_translations": {
                                "en": "The castle was built <w>circa</w> fourteen hundred and is still in good condition."
                            }
                        },
                        {
                            "text": "Deze schilderijen stammen uit <w>circa</w> de zeventiende eeuw.",
                            "target_lang_translations": {
                                "en": "These paintings date from <w>around</w> the seventeenth century."
                            }
                        }
                    ],
                    "synonyms": [
                        "omstreeks",
                        "plusminus"
                    ],
                    "antonyms": [
                        "precies"
                    ],
                    "common_phrases": [
                        "circa 1900",
                        "geboren circa"
                    ],
                    "target_lang_definitions": {
                        "en": "Around; used to indicate an approximate date or year."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "circa",
                                "target_lang_sense_clarification": "used for dates and years"
                            },
                            {

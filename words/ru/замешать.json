    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b47ba49c-5e8c-3888-b8a3-5369ae9e7397",
                    "sense_definition": "Приготовить однородную густую массу (тесто, раствор, бетон), соединяя и перемешивая жидкость с чем-либо сыпучим.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mixing_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие успели <w>замешать</w> цемент до начала дождя.",
                            "target_lang_translations": {
                                "en": "The workers managed to <w>mix</w> the cement before the rain started."
                            }
                        },
                        {
                            "text": "Бабушка попросила меня <w>замешать</w> тесто для пирожков.",
                            "target_lang_translations": {
                                "en": "Grandma asked me to <w>mix</w> the dough for the pies."
                            }
                        }
                    ],
                    "synonyms": [
                        "приготовить",
                        "сделать"
                    ],
                    "common_phrases": [
                        "замешать тесто",
                        "замешать раствор",
                        "замешать бетон",
                        "замешать краску"
                    ],
                    "target_lang_definitions": {
                        "en": "To prepare a homogeneous thick mass (dough, mortar, concrete) by combining and mixing liquid with dry ingredients."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mix",
                                "target_lang_sense_clarification": "ingredients into a mass"
                            },
                            {
                                "target_lang_word": "knead",
                                "target_lang_sense_clarification": "dough"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3170e60d-35db-3950-83fa-f5fab4d545c0",
                    "sense_definition": "Вовлечь кого-либо в неприятное, предосудительное или преступное дело, сделав соучастником.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "involving_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Они пытались <w>замешать</w> его в свои финансовые махинации.",
                            "target_lang_translations": {
                                "en": "They tried to <w>implicate</w> him in their financial schemes."
                            }
                        },
                        {
                            "text": "Он боялся, что враги могут <w>замешать</w> его в это грязное дело.",
                            "target_lang_translations": {
                                "en": "He was afraid that his enemies might <w>drag</w> him <w>into</w> this dirty business."
                            }
                        }
                    ],
                    "synonyms": [
                        "вовлечь",
                        "впутать"
                    ],
                    "common_phrases": [
                        "замешать в преступление",
                        "замешать в скандал",
                        "замешать в историю"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To involve someone in an unpleasant, reprehensible, or criminal affair, making them an accomplice."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "implicate",
                                "target_lang_sense_clarification": "in a crime or bad deed"
                            },
                            {
                                "target_lang_word": "involve",
                                "target_lang_sense_clarification": "in a difficult situation"
                            },
                            {
                                "target_lang_word": "drag into",
                                "target_lang_sense_clarification": "force into a situation"
                            }
                        ]
                    }
                }
            ]

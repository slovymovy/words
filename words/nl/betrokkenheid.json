{
    "word_family": [
        "betrekken",
        "betrokken"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4fdab479-7fd9-35fa-bd53-5fc923ae7285",
                    "sense_definition": "Het zich nauw verbonden voelen met iets of iemand en zich daarvoor inzetten.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij toont altijd veel <w>betrokkenheid</w> bij het welzijn van de dieren.",
                            "target_lang_translations": {
                                "en": "She always shows great <w>commitment</w> to the welfare of the animals.",
                                "ru": "Она всегда проявляет большое <w>неравнодушие</w> к благополучию животных."
                            }
                        },
                        {
                            "text": "Dankzij de grote <w>betrokkenheid</w> van de vrijwilligers was het evenement een succes.",
                            "target_lang_translations": {
                                "en": "Thanks to the great <w>commitment</w> of the volunteers, the event was a success.",
                                "ru": "Благодаря большой <w>вовлечённости</w> волонтёров мероприятие прошло успешно."
                            }
                        }
                    ],
                    "synonyms": [
                        "toewijding"
                    ],
                    "common_phrases": [
                        "grote betrokkenheid",
                        "sociale betrokkenheid",
                        "betrokkenheid tonen"
                    ],
                    "target_lang_definitions": {
                        "en": "The feeling of being closely connected to something or someone and dedicating oneself to it.",
                        "ru": "Чувство тесной связи с чем-либо или кем-либо и готовность действовать ради этого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commitment",
                                "target_lang_sense_clarification": "dedication to a cause or person"
                            },
                            {
                                "target_lang_word": "involvement",
                                "target_lang_sense_clarification": "active interest or concern"
                            },
                            {
                                "target_lang_word": "engagement",
                                "target_lang_sense_clarification": "emotional investment"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вовлечённость",
                                "target_lang_sense_clarification": "активное участие и интерес"
                            },
                            {
                                "target_lang_word": "неравнодушие",
                                "target_lang_sense_clarification": "эмоциональная отзывчивость"
                            },
                            {
                                "target_lang_word": "участие",
                                "target_lang_sense_clarification": "проявление заботы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "453ef6ed-5274-31e6-8d1f-ed32565de2a2",
                    "sense_definition": "Het feit dat iemand ergens mee te maken heeft of ergens deel van uitmaakt.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>betrokkenheid</w> van de overheid bij deze beslissing is onduidelijk.",
                            "target_lang_translations": {
                                "en": "The government's <w>involvement</w> in this decision is unclear.",
                                "ru": "<w>Участие</w> правительства в этом решении неясно."
                            }
                        },
                        {
                            "text": "Hij heeft geen enkele <w>betrokkenheid</w> bij de organisatie van het feest.",
                            "target_lang_translations": {
                                "en": "He has absolutely no <w>involvement</w> in the organization of the party.",
                                "ru": "Он не имеет никакого <w>отношения</w> к организации праздника."
                            }
                        }
                    ],
                    "common_phrases": [
                        "maatschappelijke betrokkenheid",
                        "politieke betrokkenheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The fact of having a role in something or being part of it.",
                        "ru": "Факт участия в чём-либо или наличие отношения к чему-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "involvement",
                                "target_lang_sense_clarification": "state of being included or associated"
                            },
                            {
                                "target_lang_word": "participation",
                                "target_lang_sense_clarification": "act of taking part"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "участие",
                                "target_lang_sense_clarification": "роль в процессе"
                            },
                            {
                                "target_lang_word": "отношение",
                                "target_lang_sense_clarification": "связь с делом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3ee5783-7635-31ea-8aed-4a9ae118eb3e",
                    "sense_definition": "Het medeplichtig zijn aan een strafbaar feit of een negatieve gebeurtenis.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter oordeelde over zijn <w>betrokkenheid</w> bij de diefstal.",
                            "target_lang_translations": {
                                "en": "The judge ruled on his <w>involvement</w> in the theft.",
                                "ru": "Судья вынес решение о его <w>причастности</w> к краже."
                            }
                        },
                        {
                            "text": "Er zijn aanwijzingen voor zijn <w>betrokkenheid</w> bij de illegale handel.",
                            "target_lang_translations": {
                                "en": "There are indications of his <w>involvement</w> in the illegal trade.",
                                "ru": "Есть указания на его <w>причастность</w> к незаконной торговле."
                            }
                        }
                    ],
                    "synonyms": [
                        "medeplichtigheid"
                    ],
                    "common_phrases": [
                        "vermeende betrokkenheid",
                        "strafbare betrokkenheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of being implicated in a criminal act or negative event.",
                        "ru": "Соучастие в преступлении или негативном событии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "involvement",
                                "target_lang_sense_clarification": "connection to an incident"
                            },
                            {
                                "target_lang_word": "complicity",
                                "target_lang_sense_clarification": "participation in wrongdoing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "причастность",
                                "target_lang_sense_clarification": "связь с событием"
                            },
                            {
                                "target_lang_word": "соучастие",
                                "target_lang_sense_clarification": "совместное совершение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
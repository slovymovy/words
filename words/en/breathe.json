{
    "word_family": [
        "breath",
        "breathable",
        "breathless",
        "breather",
        "unbreathable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "753c5b3a-2875-3072-9180-046c6096b821",
                    "sense_definition": "To take air into the lungs and let it out again to stay alive.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "biological_respiration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is difficult to <w>breathe</w> at very high altitudes.",
                            "target_lang_translations": {
                                "ru": "На очень больших высотах трудно <w>дышать</w>."
                            }
                        },
                        {
                            "text": "The doctor asked the patient to <w>breathe</w> in deeply.",
                            "target_lang_translations": {
                                "ru": "Врач попросил пациента глубоко <w>вдохнуть</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "respire",
                        "draw breath"
                    ],
                    "antonyms": [
                        "suffocate",
                        "choke",
                        "hold one's breath"
                    ],
                    "common_phrases": [
                        "breathe in",
                        "breathe out",
                        "breathe deeply",
                        "breathe easily"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вдыхать воздух в легкие и выдыхать его обратно для поддержания жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дышать",
                                "target_lang_sense_clarification": "процесс газообмена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14ce70b9-0eb9-3aaa-a1b4-37e723592809",
                    "sense_definition": "To take a substance, such as smoke, gas, or a scent, into the lungs.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_respiration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Try not to <w>breathe</w> the fumes while using this cleaning product.",
                            "target_lang_translations": {
                                "ru": "Старайтесь не <w>вдыхать</w> испарения при использовании этого чистящего средства."
                            }
                        },
                        {
                            "text": "He stepped outside to <w>breathe</w> the fresh morning air.",
                            "target_lang_translations": {
                                "ru": "Он вышел на улицу <w>подышать</w> свежим утренним воздухом."
                            }
                        }
                    ],
                    "synonyms": [
                        "inbreathe"
                    ],
                    "common_phrases": [
                        "breathe smoke",
                        "breathe fumes",
                        "breathe air"
                    ],
                    "target_lang_definitions": {
                        "ru": "Втягивать в легкие вещество, такое как дым, газ или запах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдыхать",
                                "target_lang_sense_clarification": "втягивать внутрь"
                            },
                            {
                                "target_lang_word": "дышать",
                                "target_lang_sense_clarification": "чувствовать запах или воздействие газа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "284e77eb-440f-3a25-9525-9dc7b39746fc",
                    "sense_definition": "To say something very quietly; to whisper.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>breathed</w> a secret into her friend's ear.",
                            "target_lang_translations": {
                                "ru": "Она <w>прошептала</w> секрет на ухо своей подруге."
                            }
                        },
                        {
                            "text": "Don't <w>breathe</w> a word of our plan to anyone else.",
                            "target_lang_translations": {
                                "ru": "Не <w>говори</w> никому ни слова о нашем плане."
                            }
                        }
                    ],
                    "synonyms": [
                        "suspire"
                    ],
                    "common_phrases": [
                        "breathe a word",
                        "breathe a sigh"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить что-то очень тихо; шептать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шептать",
                                "target_lang_sense_clarification": "говорить шепотом"
                            },
                            {
                                "target_lang_word": "прошептать",
                                "target_lang_sense_clarification": "сказать тихо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c6144e0-d4e9-3e07-9cb4-022c2d7bfccd",
                    "sense_definition": "To allow air and moisture to pass through freely (used for materials or fabrics).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "material_property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cotton is a fabric that <w>breathes</w> well in hot weather.",
                            "target_lang_translations": {
                                "ru": "Хлопок — это ткань, которая хорошо <w>дышит</w> в жаркую погоду."
                            }
                        },
                        {
                            "text": "You should wear loose clothing to let your skin <w>breathe</w>.",
                            "target_lang_translations": {
                                "ru": "Вам следует носить свободную одежду, чтобы дать коже <w>дышать</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fabric breathes",
                        "skin breathes",
                        "room to breathe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободно пропускать воздух и влагу (о материалах или тканях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дышать",
                                "target_lang_sense_clarification": "пропускать воздух"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74e6e74f-c9c9-3a8a-9b47-770d421fc08f",
                    "sense_definition": "To send out something, such as fire or vapor, from the mouth.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "expulsion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the fairy tale, the dragon could <w>breathe</w> fire.",
                            "target_lang_translations": {
                                "ru": "В сказке дракон мог <w>дышать</w> огнём."
                            }
                        },
                        {
                            "text": "The furious manager looked ready to <w>breathe</w> fire at the staff.",
                            "target_lang_translations": {
                                "ru": "Разъяренный менеджер выглядел так, словно готов был <w>изрыгать</w> пламя на сотрудников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "breathe fire"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпускать изо рта что-то, например огонь или пар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изрыгать",
                                "target_lang_sense_clarification": "выбрасывать с силой"
                            },
                            {
                                "target_lang_word": "дышать",
                                "target_lang_sense_clarification": "выпускать струю (обычно с 'огнем')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86cf3401-2c73-30e9-8790-fe6eb2d8a19e",
                    "sense_definition": "To be alive or exist.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will defend my country as long as I <w>breathe</w>.",
                            "target_lang_translations": {
                                "ru": "Я буду защищать свою страну, пока я <w>дышу</w>."
                            }
                        },
                        {
                            "text": "He is the kindest man that ever <w>breathed</w>.",
                            "target_lang_translations": {
                                "ru": "Он самый добрый человек, который когда-либо <w>жил</w> на свете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "as long as I breathe",
                        "scarcely breathe"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or dramatic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть живым или существовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "существовать"
                            },
                            {
                                "target_lang_word": "дышать",
                                "target_lang_sense_clarification": "быть живым (поэтич.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5c89f00-e6c7-35a3-92d7-bb48e9df0379",
                    "sense_definition": "To display or express a quality, feeling, or atmosphere strongly.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient castle <w>breathes</w> history and mystery.",
                            "target_lang_translations": {
                                "ru": "Древний замок <w>дышит</w> историей и тайной."
                            }
                        },
                        {
                            "text": "Her energetic performance <w>breathed</w> new life into the old song.",
                            "target_lang_translations": {
                                "ru": "Ее энергичное выступление <w>вдохнуло</w> новую жизнь в старую песню."
                            }
                        }
                    ],
                    "common_phrases": [
                        "breathe new life",
                        "breathe confidence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно проявлять или выражать качество, чувство или атмосферу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дышать",
                                "target_lang_sense_clarification": "излучать атмосферу"
                            },
                            {
                                "target_lang_word": "вдохнуть",
                                "target_lang_sense_clarification": "вселить (жизнь, надежду)"
                            },
                            {
                                "target_lang_word": "веять",
                                "target_lang_sense_clarification": "исходить (об атмосфере)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd97f8f8-c1bb-3c47-8ef1-fae89fb28524",
                    "sense_definition": "To be passionately dedicated to an activity or interest.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "passion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He lives and <w>breathes</w> football every single day.",
                            "target_lang_translations": {
                                "ru": "Он живет и <w>дышит</w> футболом каждый божий день."
                            }
                        },
                        {
                            "text": "She <w>breathes</w> art and spends all her free time painting.",
                            "target_lang_translations": {
                                "ru": "Она <w>дышит</w> искусством и проводит все свободное время за рисованием."
                            }
                        }
                    ],
                    "synonyms": [
                        "live and breathe"
                    ],
                    "common_phrases": [
                        "live and breathe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть страстно преданным какому-либо занятию или интересу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "быть поглощенным чем-то"
                            },
                            {
                                "target_lang_word": "дышать",
                                "target_lang_sense_clarification": "не мыслить себя без чего-то"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "5c1e00bb-1837-3286-819e-ec87e1d00396",
                    "sense_definition": "Een bromfiets of motorfiets, vaak informeel zo genoemd vanwege het ploffende geluid van de motor.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurman rijdt elke ochtend op zijn oude <w>ploffer</w> naar het werk.",
                            "target_lang_translations": {
                                "en": "My neighbor rides his old <w>moped</w> to work every morning.",
                                "ru": "Мой сосед каждое утро ездит на работу на своем старом <w>мопеде</w>."
                            }
                        },
                        {
                            "text": "We hoorden de <w>ploffer</w> al van ver aankomen door het lawaai dat hij maakte.",
                            "target_lang_translations": {
                                "en": "We could hear the <w>motorbike</w> coming from afar because of the noise it made.",
                                "ru": "Мы слышали этот <w>мотоцикл</w> уже издалека из-за шума, который он производил."
                            }
                        }
                    ],
                    "synonyms": [
                        "brommer",
                        "motor"
                    ],
                    "common_phrases": [
                        "oude ploffer"
                    ],
                    "target_lang_definitions": {
                        "en": "A moped or motorcycle, often informally called this because of the popping sound of the engine.",
                        "ru": "Мопед или мотоцикл, часто называемый так в разговорной речи из-за хлопающего звука двигателя."
                    },
                    "translations": {

{
    "word_family": [
        "kąpać",
        "wykąpać",
        "kąpielowy",
        "kąpielisko",
        "kąpielówki"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea7ebc0e-cf8d-3559-982b-bf15b7d28e74",
                    "sense_definition": "Czynność mycia całego ciała w wodzie, zwykle w wannie, lub zanurzenia się w zbiorniku wodnym w celach higienicznych, leczniczych lub rekreacyjnych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorna <w>kąpiel</w> w wannie to mój ulubiony sposób na relaks po pracy.",
                            "target_lang_translations": {
                                "ru": "Вечерняя <w>ванна</w> — мой любимый способ расслабиться после работы."
                            }
                        },
                        {
                            "text": "W tym miejscu <w>kąpiel</w> w rzece jest zabroniona z powodu silnych prądów.",
                            "target_lang_translations": {
                                "ru": "В этом месте <w>купание</w> в реке запрещено из-за сильных течений."
                            }
                        }
                    ],
                    "synonyms": [
                        "kąpanie"
                    ],
                    "common_phrases": [
                        "brać kąpiel",
                        "gorąca kąpiel",
                        "kąpiel słoneczna",
                        "kąpiel w morzu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в мытье всего тела в воде или погружении в водоем для отдыха или лечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "купание",
                                "target_lang_sense_clarification": "погружение в воду, плавание"
                            },
                            {
                                "target_lang_word": "ванна",
                                "target_lang_sense_clarification": "процесс мытья в ванне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cc4b6cd-776c-35b9-acf9-c6644206cfa1",
                    "sense_definition": "Woda lub inny płyn w wannie albo naczyniu, przygotowany do mycia się lub zanurzenia ciała.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dla odprężenia przygotował aromatyczną <w>kąpiel</w> z dodatkiem ziół i olejków.",
                            "target_lang_translations": {
                                "ru": "Для расслабления он приготовил ароматную <w>ванну</w> с добавлением трав и масел."
                            }
                        },
                        {
                            "text": "Woda na <w>kąpiel</w> jest już gotowa i przyjemnie ciepła.",
                            "target_lang_translations": {
                                "ru": "Вода для <w>ванны</w> уже готова и приятно теплая."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przygotować kąpiel",
                        "sól do kąpieli",
                        "płyn do kąpieli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вода или другая жидкость, подготовленная для мытья или лечебных процедур."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ванна",
                                "target_lang_sense_clarification": "вода, подготовленная для мытья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c627939-6575-398b-a2e4-d7d0d2419eb9",
                    "sense_definition": "Specjalny roztwór chemiczny lub proces zanurzania w nim przedmiotów w celu wywołania określonej reakcji (np. w fotografii, laboratoriach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Probówkę z substancją należy ogrzewać przez kilka minut, stosując <w>kąpiel</w> wodną.",
                            "target_lang_translations": {
                                "ru": "Пробирку с веществом следует нагревать несколько минут, используя водяную <w>баню</w>."
                            }
                        },
                        {
                            "text": "Metalowe elementy poddano <w>kąpieli</w> w kwasie, aby dokładnie usunąć rdzę.",
                            "target_lang_translations": {
                                "ru": "Металлические элементы подвергли кислотной <w>ванне</w>, чтобы тщательно удалить ржавчину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kąpiel wodna",
                        "kąpiel chemiczna",
                        "kąpiel utrwalająca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химический раствор или технический процесс обработки предметов погружением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баня",
                                "target_lang_sense_clarification": "лабораторный метод (напр. водяная баня)"
                            },
                            {
                                "target_lang_word": "ванна",
                                "target_lang_sense_clarification": "емкость с раствором"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
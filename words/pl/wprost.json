                {
                    "sense_id": "e2d38920-4746-3dd7-a314-94d7c09c2fe8",
                    "sense_definition": "Bezpośrednio w jakimś kierunku, w linii prostej do celu, bez zbaczania z drogi lub bez pośredników.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spatial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta ścieżka prowadzi przez las <w>wprost</w> nad jezioro.",
                            "target_lang_translations": {
                                "ru": "Эта тропинка ведет через лес <w>прямо</w> к озеру."
                            }
                        },
                        {
                            "text": "Spojrzał mi <w>wprost</w> w oczy i uśmiechnął się tajemniczo.",
                            "target_lang_translations": {
                                "ru": "Он посмотрел мне <w>прямо</w> в глаза и загадочно улыбнулся."
                            }
                        }
                    ],
                    "synonyms": [
                        "prosto",
                        "bezpośrednio"
                    ],
                    "common_phrases": [
                        "iść wprost",
                        "patrzeć wprost",
                        "wprost do celu",
                        "wprost przed siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямо в каком-либо направлении, по прямой линии к цели, не сворачивая с пути или без посредников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямо",
                                "target_lang_sense_clarification": "в прямом направлении"
                            },
                            {
                                "target_lang_word": "непосредственно",
                                "target_lang_sense_clarification": "без промежуточных этапов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a76a4db-65ad-3f21-a5a3-6bf11673db12",
                    "sense_definition": "W sposób szczery i otwarty, mówiąc dokładnie to, co się myśli, bez ukrywania prawdy czy intencji.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli coś ci się nie podoba, powiedz mi to <w>wprost</w>.",
                            "target_lang_translations": {
                                "ru": "Если тебе что-то не нравится, скажи мне это <w>прямо</w>."
                            }
                        },
                        {
                            "text": "Zapytałem go <w>wprost</w> o powody jego dziwnego zachowania.",
                            "target_lang_translations": {
                                "ru": "Я спросил его <w>напрямую</w> о причинах его странного поведения."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczerze",
                        "bez ogródek",
                        "prosto z mostu",
                        "bez owijania w bawełnę"
                    ],
                    "common_phrases": [
                        "powiedzieć wprost",
                        "zapytać wprost",
                        "mówić wprost",
                        "wyznać wprost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искренне и открыто, говоря именно то, что думаешь, не скрывая правды или намерений."

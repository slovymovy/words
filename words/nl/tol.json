                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>tol</w> op mijn benen van de duizeligheid.",
                            "target_lang_translations": {

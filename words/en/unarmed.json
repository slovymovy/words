    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "93f78c1d-5c30-3346-95c4-29719caa0523",
                    "sense_definition": "Not carrying or equipped with weapons.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police managed to arrest the <w>unarmed</w> suspect without any violence.",
                            "target_lang_translations": {
                                "ru": "Полиции удалось арестовать <w>безоружного</w> подозреваемого без применения насилия."
                            }
                        },
                        {
                            "text": "Soldiers are trained in <w>unarmed</w> combat in case they lose their weapons.",
                            "target_lang_translations": {
                                "ru": "Солдат обучают <w>рукопашному</w> бою на случай потери оружия."
                            }
                        }
                    ],
                    "synonyms": [
                        "weaponless",
                        "defenseless"
                    ],
                    "antonyms": [
                        "armed"
                    ],
                    "common_phrases": [
                        "unarmed combat",
                        "unarmed civilian",
                        "unarmed robbery",
                        "completely unarmed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий при себе оружия или не оснащённый им."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безоружный",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "невооружённый",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86f6b561-f244-33e4-a013-25c5f54ae40f",
                    "sense_definition": "Not having thorns, claws, or other sharp physical parts used for defense or attack.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Since this species of cactus is <w>unarmed</w>, it is safe to handle without gloves.",
                            "target_lang_translations": {
                                "ru": "Поскольку этот вид кактуса <w>бесшипный</w>, его можно безопасно брать без перчаток."
                            }
                        },
                        {
                            "text": "An <w>unarmed</w> jellyfish would struggle to capture prey in the open ocean.",
                            "target_lang_translations": {
                                "ru": "<w>Невооружённой</w> медузе было бы трудно поймать добычу в открытом океане."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "biological context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий шипов, когтей или других острых частей для защиты (в биологии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невооружённый",
                                "target_lang_sense_clarification": "биологический термин"
                            },
                            {
                                "target_lang_word": "бесшипный",
                                "target_lang_sense_clarification": "о растениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aab700c4-32a7-3a67-8e48-2c333fca7028",
                    "sense_definition": "Done without the help of optical instruments like a telescope or microscope; unaided.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The distant star is too faint to be seen by the <w>unarmed</w> eye.",
                            "target_lang_translations": {
                                "ru": "Далёкая звезда слишком тусклая, чтобы её можно было увидеть <w>невооружённым</w> глазом."
                            }
                        },
                        {
                            "text": "Early astronomers mapped the sky using only their <w>unarmed</w> vision.",
                            "target_lang_translations": {
                                "ru": "Ранние астрономы составляли карты неба, используя только своё <w>невооружённое</w> зрение."
                            }
                        }
                    ],
                    "synonyms": [
                        "unaided",
                        "naked"
                    ],
                    "common_phrases": [
                        "unarmed eye"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "usually replaced by 'naked eye' or 'unaided'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Осуществляемый без помощи оптических приборов (например, телескопа)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невооружённый",
                                "target_lang_sense_clarification": "обычно в выражении 'невооружённый глаз'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0bdc1140-58f0-304e-8f09-e86f785e4d31",
                    "sense_definition": "Раскрыть, расправить то, что было свернуто, скатано или сложено; снять обертку.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_unfold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <развернуть> этот подарок прямо сейчас.",
                            "target_lang_translations": {
                                "en": "I want to <unwrap> this gift right now."
                            }
                        },
                        {
                            "text": "Пассажир решил <развернуть> газету, чтобы прочитать новости.",
                            "target_lang_translations": {
                                "en": "The passenger decided to <unfold> the newspaper to read the news."
                            }
                        },
                        {
                            "text": "Помоги мне <развернуть> ковёр в гостиной.",
                            "target_lang_translations": {
                                "en": "Help me <unroll> the carpet in the living room."
                            }
                        }
                    ],
                    "synonyms": [
                        "расположить"
                    ],
                    "antonyms": [
                        "свернуть",
                        "завернуть"
                    ],
                    "common_phrases": [
                        "развернуть подарок",
                        "развернуть газету",
                        "развернуть карту",
                        "развернуть знамя"
                    ],
                    "target_lang_definitions": {
                        "en": "To open, straighten out something that was rolled, rolled up, or folded; to remove the wrapper."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unwrap",
                                "target_lang_sense_clarification": "a gift, candy, package"
                            },
                            {
                                "target_lang_word": "unfold",
                                "target_lang_sense_clarification": "a newspaper, map, cloth"
                            },
                            {
                                "target_lang_word": "unroll",
                                "target_lang_sense_clarification": "a carpet, scroll"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c9c3bb9-dab9-30da-87dd-ae3f10dd8261",
                    "sense_definition": "Изменить направление движения или положение объекта (обычно транспортного средства) в другую сторону.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "turn_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель успел вовремя <развернуть> машину и избежать аварии.",
                            "target_lang_translations": {
                                "en": "The driver managed to <turn> the car <around> in time and avoid an accident."
                            }
                        },
                        {
                            "text": "Нам нужно <развернуть> диван к стене, чтобы освободить место.",
                            "target_lang_translations": {
                                "en": "We need to <turn> the sofa toward the wall to free up space."
                            }
                        }
                    ],
                    "synonyms": [
                        "обратить"
                    ],
                    "common_phrases": [
                        "развернуть машину",
                        "развернуть плечи",
                        "развернуть корпус"
                    ],
                    "target_lang_definitions": {
                        "en": "To change the direction of movement or position of an object (usually a vehicle) to the opposite side."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn around",
                                "target_lang_sense_clarification": "a vehicle, person"
                            },
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "an object's orientation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a50ba5fc-0e91-335e-b245-e643de9338be",
                    "sense_definition": "Организовать, наладить или расширить какую-либо деятельность, производство или работу в больших масштабах.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "organize_expand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания планирует <развернуть> широкую рекламную кампанию в следующем месяце.",
                            "target_lang_translations": {
                                "en": "The company plans to <launch> a broad advertising campaign next month."
                            }
                        },
                        {
                            "text": "Им удалось быстро <развернуть> производство новых товаров.",
                            "target_lang_translations": {
                                "en": "They managed to quickly <set up> production of the new goods."
                            }
                        }
                    ],
                    "synonyms": [
                        "организовать",
                        "учредить",
                        "распространить",
                        "предпринять",
                        "устроить"
                    ],
                    "antonyms": [
                        "свернуть"
                    ],
                    "common_phrases": [
                        "развернуть деятельность",
                        "развернуть строительство",
                        "развернуть кампанию",
                        "развернуть бизнес"
                    ],
                    "target_lang_definitions": {
                        "en": "To organize, establish, or expand an activity, production, or work on a large scale."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "launch",
                                "target_lang_sense_clarification": "a campaign, project"
                            },
                            {
                                "target_lang_word": "roll out",
                                "target_lang_sense_clarification": "production, business operations"
                            },
                            {
                                "target_lang_word": "set up",
                                "target_lang_sense_clarification": "infrastructure, organization"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41ec6f87-08b1-3e52-9330-3a1871d7ae47",
                    "sense_definition": "Привести в состояние готовности к действию (войска, оборудование, госпиталь).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "deploy_military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир приказал <развернуть> войска вдоль границы.",
                            "target_lang_translations": {
                                "en": "The commander ordered to <deploy> troops along the border."
                            }
                        },
                        {
                            "text": "Спасатели смогли быстро <развернуть> полевой госпиталь для помощи пострадавшим.",
                            "target_lang_translations": {
                                "en": "Rescuers were able to quickly <set up> a field hospital to help the victims."
                            }
                        }
                    ],
                    "synonyms": [
                        "разместить",
                        "расположить",
                        "установить"
                    ],
                    "antonyms": [
                        "свернуть"
                    ],
                    "common_phrases": [
                        "развернуть войска",
                        "развернуть госпиталь",
                        "развернуть рацию"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring into a state of readiness for action (troops, equipment, hospital)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deploy",
                                "target_lang_sense_clarification": "troops, military equipment"
                            },
                            {
                                "target_lang_word": "set up",
                                "target_lang_sense_clarification": "a field hospital, station"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10677e33-c8d3-34a9-85fd-454675413d0d",
                    "sense_definition": "Подробно изложить, развить (мысль, тему) или проявить в полной мере (способности, талант).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "develop_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автор не смог полностью <развернуть> сюжет в этом коротком рассказе.",
                            "target_lang_translations": {
                                "en": "The author couldn't fully <develop> the plot in this short story."
                            }
                        },
                        {
                            "text": "Попробуйте <развернуть> вашу мысль подробнее, чтобы мы поняли суть.",
                            "target_lang_translations": {
                                "en": "Try to <elaborate on> your thought in more detail so we understand the essence."
                            }
                        }
                    ],
                    "synonyms": [
                        "проявить"
                    ],
                    "common_phrases": [
                        "развернуть мысль",
                        "развернуть аргументацию",
                        "развернуть талант"
                    ],
                    "target_lang_definitions": {
                        "en": "To present in detail, develop (a thought, theme) or manifest fully (abilities, talent)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "a plot, idea"
                            },
                            {
                                "target_lang_word": "elaborate on",
                                "target_lang_sense_clarification": "a thought, argument"
                            },
                            {
                                "target_lang_word": "expand on",
                                "target_lang_sense_clarification": "a topic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "896e12ea-9856-3039-a6c1-c1c24d527a92",
                    "sense_definition": "Вращая, вывинтить или ослабить (гайку, винт).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "unscrew",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Без инструментов я не смогу <развернуть> этот старый винт.",
                            "target_lang_translations": {
                                "en": "Without tools, I won't be able to <unscrew> this old screw."
                            }
                        },
                        {
                            "text": "Попробуй <развернуть> гайку руками, если она не затянута туго.",
                            "target_lang_translations": {
                                "en": "Try to <unscrew> the nut with your hands if it's not tightened too much."
                            }
                        }
                    ],
                    "synonyms": [
                        "развинтить",
                        "вывинтить"
                    ],
                    "common_phrases": [
                        "развернуть гайку",
                        "развернуть винт"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech instead of 'развинтить'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To unscrew or loosen by rotating (a nut, screw)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unscrew",
                                "target_lang_sense_clarification": "hardware"
                            },
                            {
                                "target_lang_word": "loosen",
                                "target_lang_sense_clarification": "a nut, bolt"
                            }
                        ]
                    }
                }
            ]
        }

                    ],
                    "antonyms": [
                        "nieśmiertelny",
                        "wieczny"
                    ],
                    "common_phrases": [
                        "istota śmiertelna",
                        "człowiek jest śmiertelny"
                    ],
                    "target_lang_definitions": {

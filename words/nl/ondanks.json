    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "7f36fd91-924b-3610-84de-f6a4cb72a01f",
                    "sense_definition": "In weerwil van; zonder dat een bepaalde omstandigheid het gevolg belet.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gingen wandelen <w>ondanks</w> de zware regen.",
                            "target_lang_translations": {
                                "ru": "Мы пошли гулять <w>несмотря на</w> сильный дождь.",
                                "en": "We went for a walk <w>despite</w> the heavy rain."
                            }
                        },
                        {
                            "text": "<w>Ondanks</w> zijn ziekte kwam hij toch naar school.",
                            "target_lang_translations": {
                                "ru": "<w>Несмотря на</w> свою болезнь, он всё же пришёл в школу.",
                                "en": "<w>In spite of</w> his illness, he still came to school."
                            }
                        }
                    ],
                    "antonyms": [
                        "dankzij"
                    ],
                    "common_phrases": [
                        "ondanks alles",
                        "ondanks het feit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопреки чему-либо; невзирая на обстоятельства, которые могли бы помешать результату.",
                        "en": "In spite of; without a certain circumstance preventing the result."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несмотря на",
                                "target_lang_sense_clarification": "предлог; наиболее общее значение"
                            },
                            {
                                "target_lang_word": "вопреки",
                                "target_lang_sense_clarification": "предлог; указывает на действие наперекор чему-то"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "despite",
                                "target_lang_sense_clarification": "preposition; most common usage"
                            },
                            {
                                "target_lang_word": "in spite of",
                                "target_lang_sense_clarification": "preposition; synonymous with despite"
                            },
                            {
                                "target_lang_word": "notwithstanding",
                                "target_lang_sense_clarification": "preposition; formal"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "ac59da59-2345-336a-aff0-94d8d0d32858",
                    "sense_definition": "Drukt een tegenstelling uit met het voorafgaande; hoewel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij slaagde voor het examen, <w>ondanks</w> dat hij niet had geleerd.",
                            "target_lang_translations": {
                                "ru": "Он сдал экзамен, <w>несмотря на то, что</w> не учился.",
                                "en": "He passed the exam, <w>despite</w> the fact that he hadn't studied."
                            }
                        },
                        {
                            "text": "<w>Ondanks</w> dat ze moe was, bleef ze de hele avond lachen.",
                            "target_lang_translations": {
                                "ru": "<w>Несмотря на то, что</w> она устала, она продолжала смеяться весь вечер.",
                                "en": "<w>Although</w> she was tired, she kept laughing all evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoewel"
                    ],
                    "common_phrases": [
                        "ondanks dat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражает противопоставление предыдущему; хотя.",
                        "en": "Expresses a contrast with the preceding; although."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несмотря на то, что",
                                "target_lang_sense_clarification": "союз; используется перед придаточным предложением"
                            },
                            {
                                "target_lang_word": "хотя",
                                "target_lang_sense_clarification": "союз; выражает уступку"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "although",
                                "target_lang_sense_clarification": "conjunction; introduces a concessive clause"
                            },
                            {
                                "target_lang_word": "even though",
                                "target_lang_sense_clarification": "conjunction; emphatic contrast"
                            },
                            {
                                "target_lang_word": "despite the fact that",
                                "target_lang_sense_clarification": "phrase; explicit equivalent used with a clause"

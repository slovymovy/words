                    "sense_id": "ae16174b-a11e-3993-b820-60ebd3af88d0",
                    "sense_definition": "Polskie nazwisko męskie lub żeńskie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Głowa</w> został poproszony o podpisanie dokumentów w recepcji.",
                            "target_lang_translations": {
                                "ru": "Господина <w>Глову</w> попросили подписать документы на стойке регистрации."
                            }
                        },
                        {
                            "text": "Nie znam nikogo, kto nosiłby nazwisko <w>Głowa</w> w tej okolicy.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого, кто носил бы фамилию <w>Глова</w> в этой округе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская мужская или женская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Глова",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "common only in the idiom 'pig in a poke'"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "Southern US or dialectal UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мешок; используется почти исключительно в идиоме 'pig in a poke' (кот в мешке)."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1e9f4637-cf88-322b-a753-de3afb6a2c21",
                    "sense_definition": "Produced by a musical instrument without electrical amplification.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "music_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She prefers playing an <w>acoustic</w> guitar because of its natural sound.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает играть на <w>акустической</w> гитаре из-за её естественного звучания."
                            }
                        },
                        {
                            "text": "The band performed an <w>acoustic</w> version of their most popular song.",
                            "target_lang_translations": {
                                "ru": "Группа исполнила <w>акустическую</w> версию своей самой популярной песни."
                            }
                        }
                    ],
                    "antonyms": [
                        "electric"
                    ],
                    "common_phrases": [
                        "acoustic guitar",
                        "acoustic version",
                        "acoustic piano",
                        "acoustic set"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создаваемый музыкальным инструментом без электрического усиления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акустический",
                                "target_lang_sense_clarification": "о музыкальном инструменте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4459eb58-283b-3de5-9bc4-610cab463492",
                    "sense_definition": "Relating to sound, the sense of hearing, or the science of sounds.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dolphins use <w>acoustic</w> signals to communicate with each other underwater.",
                            "target_lang_translations": {
                                "ru": "Дельфины используют <w>акустические</w> сигналы для общения друг с другом под водой."
                            }
                        },
                        {
                            "text": "The architect considered the <w>acoustic</w> properties of the materials before building the hall.",
                            "target_lang_translations": {
                                "ru": "Архитектор учел <w>акустические</w> свойства материалов перед строительством зала."
                            }
                        }
                    ],
                    "synonyms": [
                        "auditory"
                    ],
                    "common_phrases": [
                        "acoustic properties",
                        "acoustic energy",
                        "acoustic waves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к звуку, слуху или науке о звуках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акустический",
                                "target_lang_sense_clarification": "научный термин"
                            },
                            {
                                "target_lang_word": "звуковой",
                                "target_lang_sense_clarification": "относящийся к звуку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cff98271-901d-3628-93c2-255fa20fde00",
                    "sense_definition": "Used as a euphemism or slang substitute for the word <w>autistic</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slang_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Some people on the internet use <w>acoustic</w> as a rude joke to describe behavior they find strange.",
                            "target_lang_translations": {
                                "ru": "Некоторые люди в интернете используют слово <w>acoustic</w> как грубую шутку для описания поведения, которое они находят странным."
                            }
                        },
                        {
                            "text": "He was banned from the forum for calling another user <w>acoustic</w>.",
                            "target_lang_translations": {
                                "ru": "Его забанили на форуме за то, что он назвал другого пользователя <w>acoustic</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "artistic"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang, often offensive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как эвфемизм или сленговая замена слова \"аутист\"."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e2a15b8-15db-3cae-bffe-e84d845ca297",

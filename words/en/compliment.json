    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1540a966-07eb-3e8d-939b-1ecb6885ad14",
                    "sense_definition": "An expression of praise, congratulation, or respect.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She paid him a nice <w>compliment</w> regarding his presentation.",
                            "target_lang_translations": {
                                "ru": "Она сделала ему приятный <w>комплимент</w> по поводу его презентации."
                            }
                        },
                        {
                            "text": "I decided to take his remark as a <w>compliment</w> rather than an insult.",
                            "target_lang_translations": {
                                "ru": "Я решил принять его замечание как <w>комплимент</w>, а не как оскорбление."
                            }
                        }
                    ],
                    "synonyms": [
                        "praise"
                    ],
                    "antonyms": [
                        "insult"
                    ],
                    "common_phrases": [
                        "pay a compliment",
                        "take as a compliment",
                        "return the compliment",
                        "fish for compliments"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение похвалы, поздравления или уважения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комплимент",
                                "target_lang_sense_clarification": "приятные слова"
                            },
                            {
                                "target_lang_word": "похвала",
                                "target_lang_sense_clarification": "одобрение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "147c62db-e873-32f0-b756-e4333a52063b",
                    "sense_definition": "To pay a compliment to someone; to express a favourable opinion of someone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>complimented</w> his colleague on her excellent work.",
                            "target_lang_translations": {
                                "ru": "Он <w>похвалил</w> свою коллегу за её отличную работу."
                            }
                        },
                        {
                            "text": "The guests <w>complimented</w> the chef on the delicious meal.",
                            "target_lang_translations": {
                                "ru": "Гости <w>похвалили</w> шеф-повара за восхитительный ужин."
                            }
                        }
                    ],
                    "synonyms": [
                        "praise"
                    ],
                    "antonyms": [
                        "insult"
                    ],
                    "common_phrases": [

                    }
                },
                {
                    "sense_id": "034b153c-a0cd-3fbc-891d-8a2f25e6f92b",
                    "sense_definition": "Een van de vier kleuren (kaartsoorten) in het Duitse kaartspel.",
                    "learner_level": "C2",

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8a67e511-9fbb-367b-85b5-be1ca8b43be2",
                    "sense_definition": "Słowo podkreślające małą ilość, liczbę lub krótki czas; zaledwie, tylko.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_limitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do końca wakacji zostało nam <w>raptem</w> kilka dni.",
                            "target_lang_translations": {
                                "ru": "До конца каникул нам осталось <w>всего</w> несколько дней."
                            }
                        },
                        {
                            "text": "W portfelu mam <w>raptem</w> dziesięć złotych, więc nie pójdę do kina.",
                            "target_lang_translations": {
                                "ru": "В кошельке у меня <w>всего лишь</w> десять злотых, так что в кино я не пойду."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaledwie",
                        "ledwie",
                        "tylko",
                        "wszystkiego"
                    ],
                    "antonyms": [
                        "aż",
                        "dużo",
                        "wiele"
                    ],
                    "common_phrases": [
                        "raptem kilka",
                        "raptem parę",
                        "raptem chwila"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, подчеркивающее малое количество или короткое время; всего лишь, только."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всего",
                                "target_lang_sense_clarification": "ограничение количества"
                            },
                            {
                                "target_lang_word": "всего лишь",
                                "target_lang_sense_clarification": "усиление ограничения"
                            },
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "исключительность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d525e78c-2853-3a5a-84b9-598aeb394276",
                    "sense_definition": "W sposób nagły, niespodziewany, dziejący się w jednej chwili.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "suddenness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spacerowaliśmy spokojnie po parku, gdy <w>raptem</w> zaczęło mocno padać.",
                            "target_lang_translations": {
                                "ru": "Мы спокойно гуляли по парку, когда <w>вдруг</w> пошёл сильный дождь."
                            }
                        },
                        {
                            "text": "Wszystko szło dobrze i <w>raptem</w> światło zgasło w całym domu.",
                            "target_lang_translations": {
                                "ru": "Всё шло хорошо, и <w>вдруг</w> во всём доме погас свет."
                            }
                        }
                    ],
                    "synonyms": [
                        "nagle",
                        "znienacka",
                        "raptownie",
                        "z nagła"
                    ],
                    "antonyms": [
                        "powoli",
                        "stopniowo"
                    ],
                    "common_phrases": [
                        "i raptem",
                        "gdy raptem"
                    ],
                    "target_lang_definitions": {

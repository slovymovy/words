            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7b74c953-0a0a-383b-8563-e2d9072e3f2c",
                    "sense_definition": "Разговорное выражение согласия, подтверждения договорённости; соответствует словам «ладно», «хорошо».",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Мы встречаемся в семь? — <Лады>, я буду вовремя.",
                            "target_lang_translations": {
                                "en": "— Are we meeting at seven? — <Deal>, I'll be on time."
                            }
                        },
                        {
                            "text": "Ну, <лады>, давай обсудим это позже.",
                            "target_lang_translations": {
                                "en": "Well, <alright>, let's discuss this later."
                            }
                        }
                    ],
                    "synonyms": [
                        "ладно"
                    ],
                    "common_phrases": [
                        "ну лады"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informal agreement"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial expression of agreement, confirmation of an arrangement; corresponds to \"okay\", \"alright\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "okay",
                                "target_lang_sense_clarification": "colloquial agreement"
                            },
                            {

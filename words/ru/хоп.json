    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "61c2a0f8-b0a4-37e3-86c3-9dbba1583d2d",
                    "sense_definition": "Обозначение быстрого, внезапного, часто хватательного движения или неожиданного изменения ситуации.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я только потянулся за последним пирожком, а он <хоп> — и съел его.",
                            "target_lang_translations": {
                                "en": "I just reached for the last patty, and <w>snap</w> — he ate it."
                            }
                        },
                        {
                            "text": "Мы спокойно сидели, и вдруг <хоп> — свет погас во всём доме.",
                            "target_lang_translations": {
                                "en": "We were sitting quietly, and suddenly <w>bam</w> — the lights went out in the whole house."
                            }
                        }
                    ],
                    "synonyms": [
                        "хвать",
                        "прыг"
                    ],
                    "common_phrases": [
                        "хоп и всё",
                        "хоп и готово"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate a quick, sudden movement or an unexpected change in a situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bam",
                                "target_lang_sense_clarification": "sudden occurrence"
                            },
                            {
                                "target_lang_word": "snap",
                                "target_lang_sense_clarification": "sudden quick action"
                            },
                            {
                                "target_lang_word": "just like that",
                                "target_lang_sense_clarification": "unexpected change"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0baf1eaa-02d1-3241-a35c-a9bd4b54202e",
                    "sense_definition": "Возглас, используемый как команда или поощрение к прыжку, рывку или началу действия.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дрессировщик громко скомандовал «алле <хоп>», и тигр прыгнул на тумбу.",
                            "target_lang_translations": {
                                "en": "The trainer loudly commanded \"alley-<w>oop</w>\", and the tiger jumped onto the pedestal."
                            }
                        },
                        {
                            "text": "Давай, разбегайся и <хоп> — перепрыгивай через лужу!",
                            "target_lang_translations": {
                                "en": "Come on, take a running start and <w>hup</w> — jump over the puddle!"
                            }
                        }
                    ],
                    "synonyms": [
                        "гоп",
                        "давай"
                    ],
                    "common_phrases": [
                        "алле хоп",
                        "раз и хоп"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation used as a command or encouragement to jump or begin an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hup",
                                "target_lang_sense_clarification": "signal to exert effort or jump"
                            },
                            {
                                "target_lang_word": "hop",
                                "target_lang_sense_clarification": "command to jump"
                            },
                            {
                                "target_lang_word": "oop",
                                "target_lang_sense_clarification": "as in 'alley-oop'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
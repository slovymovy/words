{
    "word_family": [
        "inclusie",
        "includeren"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "39fd4071-5c5f-3908-8bba-cd19c0b3753d",
                    "sense_definition": "Met inbegrip van; waarbij iets of iemand wordt meegerekend.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De prijs voor de hotelkamer is honderd euro, <w>inclusief</w> ontbijt.",
                            "target_lang_translations": {
                                "ru": "Цена за номер в отеле составляет сто евро, <w>включая</w> завтрак."
                            }
                        },
                        {
                            "text": "Iedereen is welkom op het feest, <w>inclusief</w> kinderen en huisdieren.",
                            "target_lang_translations": {
                                "ru": "На вечеринке рады всем, <w>включая</w> детей и домашних животных."
                            }
                        }
                    ],
                    "synonyms": [
                        "met",
                        "inbegrepen",
                        "incluis"
                    ],
                    "antonyms": [
                        "exclusief",
                        "zonder"
                    ],
                    "common_phrases": [
                        "inclusief btw",
                        "inclusief verzendkosten",
                        "alles inclusief"
                    ],
                    "target_lang_definitions": {
                        "ru": "Включая кого-либо или что-либо; с учетом чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включая",
                                "target_lang_sense_clarification": "предлог"
                            },
                            {
                                "target_lang_word": "в том числе",
                                "target_lang_sense_clarification": "фраза"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "aa689d52-3fb7-3858-87fc-44bbaa93265e",
                    "sense_definition": "Gericht op het betrekken van alle mensen, ongeacht hun achtergrond, aard of beperking; niet uitsluitend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De school streeft naar <w>inclusief</w> onderwijs waar elke leerling zich welkom voelt.",
                            "target_lang_translations": {
                                "ru": "Школа стремится к <w>инклюзивному</w> образованию, где каждый ученик чувствует себя желанным."
                            }
                        },
                        {
                            "text": "Het bedrijf wil een <w>inclusieve</w> werkvloer creëren met kansen voor iedereen.",
                            "target_lang_translations": {
                                "ru": "Компания хочет создать <w>инклюзивную</w> рабочую среду с возможностями для всех."
                            }
                        }
                    ],
                    "antonyms": [
                        "exclusief"
                    ],
                    "common_phrases": [
                        "inclusieve samenleving",
                        "inclusief onderwijs",
                        "divers en inclusief"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направленный на вовлечение всех людей, независимо от их происхождения или особенностей; инклюзивный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инклюзивный",
                                "target_lang_sense_clarification": "социальный контекст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c0e9cbc-5568-34f3-a31c-e244a093265a",
                    "sense_definition": "Allesomvattend; die of dat veel of alles in zich opneemt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auteur schreef een zeer <w>inclusief</w> verslag waarin alle details aan bod kwamen.",
                            "target_lang_translations": {
                                "ru": "Автор написал очень <w>всеобъемлющий</w> отчет, в котором были затронуты все детали."
                            }
                        },
                        {
                            "text": "Zijn benadering van het probleem was <w>inclusief</w> en liet geen enkel aspect onbesproken.",
                            "target_lang_translations": {
                                "ru": "Его подход к проблеме был <w>всеобъемлющим</w> и не оставил без внимания ни одного аспекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "incluis"
                    ],
                    "antonyms": [
                        "exclusief"
                    ],
                    "target_lang_definitions": {
                        "ru": "Всеобъемлющий; охватывающий всё или многое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всеобъемлющий",
                                "target_lang_sense_clarification": "полный охват"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d317bf6f-0f43-3a1e-93bb-7e6e59c5b2d7",
                    "sense_definition": "Verwijzend naar een vorm van 'wij' waarbij de aangesprokene is inbegrepen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In sommige talen bestaat er een onderscheid tussen het <w>inclusieve</w> en het exclusieve wij.",
                            "target_lang_translations": {
                                "ru": "В некоторых языках существует различие между <w>инклюзивным</w> и эксклюзивным «мы»."
                            }
                        },
                        {
                            "text": "Bij <w>inclusieve</w> referentie wordt de luisteraar ook bedoeld met het voornaamwoord.",
                            "target_lang_translations": {
                                "ru": "При <w>инклюзивной</w> референции слушающий также подразумевается под местоимением."
                            }
                        }
                    ],
                    "antonyms": [
                        "exclusief"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "taalkunde"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к форме местоимения «мы», которая включает собеседника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инклюзивный",
                                "target_lang_sense_clarification": "лингвистический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "17e77111-666b-37d3-bce6-f7ef70b602ce",
                    "sense_definition": "Een kledingstuk voor het bovenlichaam met mouwen, vaak gebreid, dat aan de voorkant open is en gesloten kan worden met een rits of knopen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing_upper",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is fris buiten, dus ik trek een warm <w>vest</w> aan.",
                            "target_lang_translations": {
                                "en": "It is chilly outside, so I am putting on a warm <w>cardigan</w>."
                            }
                        },
                        {
                            "text": "Zij draagt vaak een grijs <w>vest</w> over haar blouse als ze aan het werk is.",
                            "target_lang_translations": {
                                "en": "She often wears a gray <w>cardigan</w> over her blouse when she is working."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gebreid vest",
                        "warm vest",
                        "vest met rits"
                    ],
                    "target_lang_definitions": {
                        "en": "A garment for the upper body with sleeves, often knitted, which is open at the front and can be closed with a zipper or buttons."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cardigan",
                                "target_lang_sense_clarification": "knitted jacket with sleeves"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a822778-eb0b-3ced-9b0a-ad9331273073",
                    "sense_definition": "Een mouwloos kledingstuk dat netjes over een overhemd en onder een colbert wordt gedragen, vaak als onderdeel van een pak.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing_upper",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bruidegom droeg een driedelig pak met een bijpassend <w>vest</w>.",
                            "target_lang_translations": {
                                "en": "The groom wore a three-piece suit with a matching <w>waistcoat</w>."
                            }
                        },
                        {
                            "text": "Hij deed zijn colbert uit, maar hield zijn <w>vest</w> aan tijdens het diner.",
                            "target_lang_translations": {
                                "en": "He took off his jacket, but kept his <w>waistcoat</w> on during dinner."
                            }
                        }
                    ],
                    "synonyms": [
                        "gilet"
                    ],
                    "common_phrases": [
                        "kogelvrij vest",
                        "veiligheidsvest"
                    ],
                    "target_lang_definitions": {
                        "en": "A sleeveless garment worn neatly over a shirt and under a suit jacket."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "waistcoat",
                                "target_lang_sense_clarification": "sleeveless garment worn with a suit"
                            },
                            {
                                "target_lang_word": "vest",
                                "target_lang_sense_clarification": "sleeveless garment (US English)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31f2409d-e6cd-3336-abeb-00b69df10df7",
                    "sense_definition": "Een verdedigingsmuur of aarden wal rondom een oude stad of vesting.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fortification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude <w>vest</w> is tegenwoordig een mooie plek om te wandelen.",
                            "target_lang_translations": {
                                "en": "The old <w>rampart</w> is nowadays a nice place to walk."
                            }
                        },
                        {
                            "text": "In de middeleeuwen beschermde de <w>vest</w> de inwoners tegen aanvallen.",
                            "target_lang_translations": {
                                "en": "In the Middle Ages, the <w>city wall</w> protected the inhabitants against attacks."
                            }
                        }
                    ],
                    "synonyms": [
                        "stadswal",
                        "vestingmuur"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "refers to historical city walls"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A defensive wall or earthen rampart surrounding an old city or fortress."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rampart",
                                "target_lang_sense_clarification": "defensive wall"
                            },
                            {
                                "target_lang_word": "city wall",
                                "target_lang_sense_clarification": "fortification"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29a0ae53-fa62-3d17-8c72-3114bb2fb2dc",
                    "sense_definition": "Een brede gracht met water rondom een vesting of stad.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fortification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water in de <w>vest</w> stond vroeger in verbinding met de rivier.",
                            "target_lang_translations": {
                                "en": "The water in the <w>moat</w> used to be connected to the river."
                            }
                        },
                        {
                            "text": "We liepen langs de <w>vest</w> en keken naar de eenden in het water.",
                            "target_lang_translations": {
                                "en": "We walked along the <w>moat</w> and looked at the ducks in the water."
                            }
                        }
                    ],
                    "synonyms": [
                        "stadsgracht",
                        "slotgracht"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "refers to historical moats"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A broad, water-filled ditch surrounding a fortress or city."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "moat",
                                "target_lang_sense_clarification": "water-filled ditch"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {

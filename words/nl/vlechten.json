                    "sense_id": "0b796df2-5ebd-33c3-aa71-c8b39a48b297",
                    "sense_definition": "Strengen (zoals haar, touw of riet) in een regelmatig patroon in elkaar draaien of strengelen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt je haar op verschillende manieren <w>vlechten</w> voor een feestje.",
                            "target_lang_translations": {
                                "en": "You can <w>braid</w> your hair in different ways for a party.",
                                "ru": "Ты можешь по-разному <w>заплести</w> волосы для вечеринки."
                            }
                        },
                        {
                            "text": "Zij leerde van haar grootmoeder hoe ze manden moest <w>vlechten</w>.",
                            "target_lang_translations": {
                                "en": "She learned from her grandmother how to <w>weave</w> baskets.",
                                "ru": "Она научилась у бабушки <w>плести</w> корзины."
                            }
                        }
                    ],
                    "synonyms": [
                        "strengelen",
                        "ineenstrengelen"
                    ],
                    "antonyms": [
                        "losmaken",
                        "ontwarren"
                    ],
                    "common_phrases": [
                        "haar vlechten",
                        "manden vlechten",
                        "krans vlechten"
                    ],
                    "target_lang_definitions": {
                        "en": "To twist or interweave strands (such as hair, rope, or reed) into a regular pattern.",
                        "ru": "Скручивать или переплетать пряди (такие как волосы, веревка или тростник) в регулярный узор."
                    },
                    "translations": {

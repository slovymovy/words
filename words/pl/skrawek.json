{
    "word_family": [
        "skrawać",
        "skrawanie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a535bea3-c4f1-3ef6-b2d9-45824b507fd1",
                    "sense_definition": "Mała cząstka odcięta lub oderwana od większej całości, na przykład materiału lub papieru.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "part_of_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znalazłem na podłodze <w>skrawek</w> papieru z zapisanym numerem telefonu.",
                            "target_lang_translations": {
                                "ru": "Я нашел на полу <w>клочок</w> бумаги с записанным номером телефона."
                            }
                        },
                        {
                            "text": "Krawcowa wykorzystała każdy <w>skrawek</w> materiału, aby uszyć tę sukienkę.",
                            "target_lang_translations": {
                                "ru": "Портниха использовала каждый <w>лоскуток</w> ткани, чтобы сшить это платье."
                            }
                        }
                    ],
                    "synonyms": [
                        "ścinek",
                        "okrawek",
                        "obrzynek",
                        "zrzynek"
                    ],
                    "common_phrases": [
                        "skrawek papieru",
                        "skrawek materiału"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькая часть, отрезанная или оторванная от большего целого, например, материала или бумаги"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клочок",
                                "target_lang_sense_clarification": "оторванный кусок бумаги или ткани"
                            },
                            {
                                "target_lang_word": "обрезок",
                                "target_lang_sense_clarification": "отрезанный кусок, остаток материала"
                            },
                            {
                                "target_lang_word": "лоскут",
                                "target_lang_sense_clarification": "обрывок или остаток ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4fde48e-fc9f-3b61-a966-0fd52c0d5ac5",
                    "sense_definition": "Niewielka powierzchnia lub przestrzeń, często w odniesieniu do terenu lub nieba.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "small_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marzę o własnym domu i małym <w>skrawku</w> ziemi pod miastem.",
                            "target_lang_translations": {
                                "ru": "Я мечтаю о собственном доме и маленьком <w>клочке</w> земли за городом."
                            }
                        },
                        {
                            "text": "Przez gęste chmury przebijał się tylko niewielki <w>skrawek</w> błękitnego nieba.",
                            "target_lang_translations": {
                                "ru": "Сквозь густые облака пробивался лишь небольшой <w>клочок</w> голубого неба."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skrawek nieba",
                        "skrawek ziemi",
                        "skrawek lądu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая поверхность или пространство, часто в отношении земли или неба"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клочок",
                                "target_lang_sense_clarification": "небольшое пространство (земли, неба)"
                            },
                            {
                                "target_lang_word": "кусочек",
                                "target_lang_sense_clarification": "малая часть чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f615415d-8c16-3f63-b611-788f0da69fa5",
                    "sense_definition": "Część małżowiny usznej będąca małą, chrzęstną wypustką znajdującą się przed otworem słuchowym.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz dokładnie obejrzał <w>skrawek</w> ucha pacjenta, szukając oznak infekcji.",
                            "target_lang_translations": {
                                "ru": "Врач внимательно осмотрел <w>козелок</w> уха пациента в поисках признаков инфекции."
                            }
                        },
                        {
                            "text": "Ból promieniował od <w>skrawka</w> aż do wnętrza ucha.",
                            "target_lang_translations": {
                                "ru": "Боль отдавала от <w>козелка</w> внутрь уха."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skrawek ucha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть ушной раковины, представляющая собой небольшой хрящевой выступ перед слуховым проходом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "козелок",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
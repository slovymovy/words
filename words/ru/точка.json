                    ],
                    "common_phrases": [
                        "и точка"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to end a dispute firmly"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation used to emphasize that a decision is final and no further discussion is allowed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "period",
                                "target_lang_sense_clarification": "US English, emphatic end of discussion"
                            },
                            {
                                "target_lang_word": "full stop",
                                "target_lang_sense_clarification": "UK English, emphatic end of discussion"
                            }
                        ]

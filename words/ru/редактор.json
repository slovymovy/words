            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea6831b3-9c52-32b1-ba0f-114ddc25fad6",
                    "sense_definition": "Компьютерная программа для создания, изменения и сохранения текстов, изображений или других данных.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "software_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для написания кода программисты используют специальный текстовый <w>редактор</w>.",
                            "target_lang_translations": {
                                "en": "Programmers use a special text <w>editor</w> to write code."
                            }
                        },
                        {
                            "text": "Этот графический <w>редактор</w> позволяет легко обрабатывать фотографии.",
                            "target_lang_translations": {
                                "en": "This graphic <w>editor</w> allows you to easily process photos."
                            }
                        }
                    ],
                    "common_phrases": [
                        "текстовый редактор",
                        "графический редактор",
                        "редактор кода",
                        "бесплатный редактор"
                    ],
                    "target_lang_definitions": {
                        "en": "A computer program for creating, modifying, and saving texts, images, or other data."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "editor",
                                "target_lang_sense_clarification": "software"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bc9137f-48e8-3c50-a50f-4df519fc205d",
                    "sense_definition": "Специалист, который проверяет, исправляет и готовит тексты к публикации.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Литературный <w>редактор</w> внимательно прочитал рукопись и внес правки.",
                            "target_lang_translations": {
                                "en": "The literary <w>editor</w> carefully read the manuscript and made corrections."
                            }
                        },
                        {
                            "text": "Мой брат работает <w>редактором</w> в крупном издательстве.",
                            "target_lang_translations": {
                                "en": "My brother works as an <w>editor</w> at a large publishing house."
                            }
                        }
                    ],
                    "antonyms": [
                        "автор"
                    ],
                    "common_phrases": [
                        "литературный редактор",
                        "технический редактор",
                        "работать редактором",
                        "мнение редактора"
                    ],
                    "target_lang_definitions": {
                        "en": "A specialist who checks, corrects, and prepares texts for publication."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "editor",
                                "target_lang_sense_clarification": "person who corrects texts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff5c1fa3-4a04-3c9b-b40a-989a13106735",
                    "sense_definition": "Руководитель издания (газеты, журнала, сайта), который определяет его содержание и политику.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Главный <w>редактор</w> утвердил новый номер журнала в печать.",
                            "target_lang_translations": {
                                "en": "The chief <w>editor</w> approved the new issue of the magazine for printing."
                            }
                        },
                        {
                            "text": "Журналисты собрались на совещание, которое проводил <w>редактор</w>.",
                            "target_lang_translations": {
                                "en": "The journalists gathered for a meeting conducted by the <w>editor</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "главный редактор",
                        "заместитель редактора",
                        "письмо редактору",
                        "выпускающий редактор"
                    ],
                    "target_lang_definitions": {
                        "en": "The head of a publication (newspaper, magazine, website) who determines its content and policy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "editor",
                                "target_lang_sense_clarification": "head of a publication"
                            },
                            {
                                "target_lang_word": "editor-in-chief",
                                "target_lang_sense_clarification": "senior role"
                            }
                        ]

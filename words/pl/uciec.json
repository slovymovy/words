{
    "word_family": [
        "ucieczka",
        "uciekinier",
        "uciekać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "083e4e9f-5364-3727-8fc5-b2243700763b",
                    "sense_definition": "Szybko oddalić się z jakiegoś miejsca, zwykle w celu uniknięcia zagrożenia, kary lub nieprzyjemnej sytuacji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej zdołał <uciec> policji, przeskakując przez wysoki płot.",
                            "target_lang_translations": {
                                "ru": "Вор сумел <убежать> от полиции, перепрыгнув через высокий забор."
                            }
                        },
                        {
                            "text": "Musimy szybko <uciec> do domu, zanim rozpęta się burza.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро <убежать> домой, пока не разразилась гроза."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbiec",
                        "zwiać",
                        "prysnąć",
                        "umknąć",
                        "czmychnąć"
                    ],
                    "antonyms": [
                        "zostać",
                        "powrócić"
                    ],
                    "common_phrases": [
                        "uciec z domu",
                        "uciec z więzienia",
                        "uciec przed odpowiedzialnością",
                        "uciec gdzie pieprz rośnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро удалиться откуда-либо, обычно чтобы избежать опасности, наказания или неприятной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убежать",
                                "target_lang_sense_clarification": "быстро удалиться бегом"
                            },
                            {
                                "target_lang_word": "сбежать",
                                "target_lang_sense_clarification": "совершить побег, тайком уйти"
                            },
                            {
                                "target_lang_word": "скрыться",
                                "target_lang_sense_clarification": "исчезнуть из виду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26c9f9d9-31cc-3703-9d1e-44ba60581e2f",
                    "sense_definition": "Zniknąć z pamięci, stać się zapomnianym lub minąć bardzo szybko (o czasie, myślach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałem ci coś powiedzieć, ale ta myśl nagle mi <uciekła>.",
                            "target_lang_translations": {
                                "ru": "Я хотел тебе кое-что сказать, но эта мысль вдруг <ускользнула> от меня."
                            }
                        },
                        {
                            "text": "Nawet nie zauważyłem, kiedy <uciekł> nam cały tydzień wakacji.",
                            "target_lang_translations": {
                                "ru": "Я даже не заметил, как <пролетела> у нас целая неделя каникул."
                            }
                        }
                    ],
                    "synonyms": [
                        "wylecieć",
                        "umknąć",
                        "przeminąć"
                    ],
                    "common_phrases": [
                        "uciec z pamięci",
                        "uciec z głowy",
                        "czas uciekł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исчезнуть из памяти, забыться или пройти очень быстро (о времени, мыслях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вылететь из головы",
                                "target_lang_sense_clarification": "забыться (о мыслях)"
                            },
                            {
                                "target_lang_word": "пролететь",
                                "target_lang_sense_clarification": "быстро пройти (о времени)"
                            },
                            {
                                "target_lang_word": "ускользнуть",
                                "target_lang_sense_clarification": "стать неуловимым (о мысли, смысле)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62465838-6029-3a56-8c4b-230de1160b82",
                    "sense_definition": "Wydostać się na zewnątrz z nieszczelnego zbiornika lub pomieszczenia (o gazach, cieczach, cieple).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_leak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zauważyłem rano, że powietrze <uciekło> z tylnej opony roweru.",
                            "target_lang_translations": {
                                "ru": "Я заметил утром, что воздух <вышел> из задней шины велосипеда."
                            }
                        },
                        {
                            "text": "Zamknij okno, żeby ciepło nie <uciekło> z nagrzanego pokoju.",
                            "target_lang_translations": {
                                "ru": "Закрой окно, чтобы тепло не <ушло> из нагретой комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyciec",
                        "ulotnić się"
                    ],
                    "common_phrases": [
                        "gaz uciekł",
                        "powietrze uciekło",
                        "ciepło ucieka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выйти наружу из негерметичной емкости или помещения (о газах, жидкостях, тепле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "о газе или воздухе"
                            },
                            {
                                "target_lang_word": "улетучиться",
                                "target_lang_sense_clarification": "испариться, исчезнуть"
                            },
                            {
                                "target_lang_word": "уйти",
                                "target_lang_sense_clarification": "о тепле"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "efeb7d23-0046-380f-a27b-fa82832a9a95",
                    "sense_definition": "Kobiecy strój przeznaczony do pływania lub opalania się na plaży.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "swimwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem zawsze pakuję do walizki mój ulubiony <kostium>.",
                            "target_lang_translations": {
                                "ru": "Летом я всегда кладу в чемодан свой любимый <купальник>."
                            }
                        },
                        {
                            "text": "Muszę przymierzyć ten jednoczęściowy <kostium> kąpielowy, zanim go kupię.",
                            "target_lang_translations": {
                                "ru": "Я должна примерить этот слитный <купальник>, прежде чем купить его."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jednoczęściowy kostium",
                        "kostium kąpielowy",
                        "dwuczęściowy kostium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская одежда, предназначенная для плавания или загорания на пляже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "купальник",
                                "target_lang_sense_clarification": "женская одежда для плавания"
                            },
                            {
                                "target_lang_word": "купальный костюм",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "692200ff-cf64-3385-bf6a-ca51532814b3",
                    "sense_definition": "Nietypowy strój zakładany dla zabawy, aby udawać kogoś innego, często noszony na balach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "disguise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na imprezę urodzinową brat założył śmieszny <kostium> klauna.",
                            "target_lang_translations": {
                                "ru": "На вечеринку в честь дня рождения брат надел смешной <костюм> клоуна."
                            }
                        },
                        {
                            "text": "Każdy uczestnik zabawy musi mieć własny <kostium> karnawałowy.",
                            "target_lang_translations": {
                                "ru": "У каждого участника праздника должен быть собственный карнавальный <костюм>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przebranie"
                    ],
                    "common_phrases": [
                        "kostium karnawałowy",
                        "bal kostiumowy",
                        "wypożyczalnia kostiumów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Необычный наряд, надеваемый для развлечения или изображения кого-то другого, часто на маскарадах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "костюм",
                                "target_lang_sense_clarification": "маскарадный или карнавальный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03aac72c-9d05-30a5-86fd-85f9fe174bef",
                    "sense_definition": "Elegancki komplet damski składający się z żakietu i spódnicy, noszony przy oficjalnych okazjach.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fashion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do biura zazwyczaj zakładam szary, elegancki <kostium>.",
                            "target_lang_translations": {
                                "ru": "В офис я обычно надеваю серый элегантный <костюм>."
                            }
                        },
                        {
                            "text": "Jej nowy wełniany <kostium> idealnie pasuje na spotkanie biznesowe.",
                            "target_lang_translations": {
                                "ru": "Её новый шерстяной <костюм> идеально подходит для деловой встречи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "elegancki kostium",
                        "wełniany kostium",
                        "kostium ze spódnicą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элегантный женский комплект из жакета и юбки, который носят по официальным поводам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "костюм",
                                "target_lang_sense_clarification": "женский деловой комплект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea862149-76b4-34f8-ad52-38d312c3e542",
                    "sense_definition": "Ubranie noszone przez aktorów w teatrze lub filmie, dopasowane do granej roli i epoki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aktorzy chwalili wygodne <kostiumy> przygotowane na spektakl.",
                            "target_lang_translations": {
                                "ru": "Актеры хвалили удобные <костюмы>, подготовленные для спектакля."
                            }
                        },
                        {
                            "text": "Ten film słynie z tego, że ma piękne <kostiumy> historyczne.",
                            "target_lang_translations": {
                                "ru": "Этот фильм славится тем, что в нём красивые исторические <костюмы>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kostiumy historyczne",
                        "projektant kostiumów",
                        "kostium sceniczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда актеров в театре или кино, соответствующая исполняемой роли и эпохе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "костюм",
                                "target_lang_sense_clarification": "сценический или исторический"
                            }
                        ]
                    }
                }
            ]
        }
    ]

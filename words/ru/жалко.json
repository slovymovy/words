                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Diminutive form of the noun <w>жало</w> (sharp defense organ in some insects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "little stinger",
                                "target_lang_sense_clarification": "diminutive for the sharp organ of an insect"
                            },
                            {
                                "target_lang_word": "sting",
                                "target_lang_sense_clarification": "anatomical part of an insect"
                            }
                        ]
                    }
                }
            ]
        }
    ]

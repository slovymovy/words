    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "711b58ef-53ba-31d2-86c6-508378e513bd",
                    "sense_definition": "Przestać się palić, świecić lub emitować światło",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "light_fire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wieczorem nagle <zgasło> światło w całej dzielnicy.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером внезапно <погас> свет во всём районе."
                            }
                        },
                        {
                            "text": "Ognisko powoli <zgasło>, więc zrobiło się bardzo zimno.",
                            "target_lang_translations": {
                                "ru": "Костёр медленно <потух>, поэтому стало очень холодно."
                            }
                        }
                    ],
                    "synonyms": [
                        "wygasnąć"
                    ],
                    "antonyms": [
                        "zapalić się",
                        "rozbłysnąć",
                        "zaświecić się"
                    ],
                    "common_phrases": [
                        "światło zgasło",
                        "ognisko zgasło",
                        "zgasnąć nagle",
                        "ekran zgasł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать гореть, светить или излучать свет"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погаснуть",
                                "target_lang_sense_clarification": "о свете, лампе, огне"
                            },
                            {
                                "target_lang_word": "потухнуть",
                                "target_lang_sense_clarification": "об огне, костре, свече"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d61b35e6-f64a-39c4-b9a5-7a0238f33100",
                    "sense_definition": "Przestać pracować (o silniku pojazdu lub maszyny)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mechanical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód <zgasł> mi na środku skrzyżowania i nie chciał ruszyć.",
                            "target_lang_translations": {
                                "ru": "Машина <заглохла> у меня посреди перекрёстка и не хотела трогаться."
                            }
                        },
                        {
                            "text": "Silnik kosiarki <zgasł> z powodu braku paliwa.",
                            "target_lang_translations": {
                                "ru": "Двигатель газонокосилки <заглох> из-за нехватки топлива."
                            }
                        }
                    ],
                    "synonyms": [
                        "stanąć"
                    ],
                    "antonyms": [
                        "zapalić",
                        "odpalić"
                    ],
                    "common_phrases": [
                        "silnik zgasł",
                        "auto zgasło",
                        "samochód zgasł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать работать (о двигателе транспортного средства или машины)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заглохнуть",
                                "target_lang_sense_clarification": "о двигателе, автомобиле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa37160e-0dd0-39d9-bd47-b4dd610de36b",
                    "sense_definition": "Stracić intensywność, wyrazistość lub przestać być widocznym (o uczuciach, kolorach, nadziejach)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uśmiech <zgasł> na jej twarzy, gdy usłyszała smutną wiadomość.",
                            "target_lang_translations": {
                                "ru": "Улыбка <погасла> на её лице, когда она услышала печальную новость."
                            }
                        },
                        {
                            "text": "Ostatnia iskierka nadziei <zgasła> wraz z nadejściem nocy.",
                            "target_lang_translations": {
                                "ru": "Последняя искра надежды <угасла> с наступлением ночи."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeminąć",
                        "osłabnąć",
                        "wyblaknąć"
                    ],
                    "antonyms": [
                        "ożyć",
                        "nasilić się"
                    ],
                    "common_phrases": [
                        "nadzieja zgasła",
                        "uśmiech zgasł",
                        "zapał zgasł",
                        "spojrzenie zgasło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерять интенсивность, выразительность или исчезнуть (о чувствах, цветах, надеждах)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угаснуть",
                                "target_lang_sense_clarification": "о надежде, чувствах, энтузиазме"
                            },
                            {
                                "target_lang_word": "погаснуть",
                                "target_lang_sense_clarification": "об улыбке, взгляде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25954c9e-30a4-308e-b813-306dab265f20",
                    "sense_definition": "Umrzeć (zwykle używane w sposób delikatny, o śmierci spokojnej lub po chorobie)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany pisarz <zgasł> wczoraj w wieku dziewięćdziesięciu lat.",
                            "target_lang_translations": {
                                "ru": "Известный писатель <угас> вчера в возрасте девяноста лет."
                            }
                        },
                        {
                            "text": "Pacjent <zgasł> cicho we śnie, nie odzyskawszy przytomności.",
                            "target_lang_translations": {
                                "ru": "Пациент тихо <угас> во сне, не приходя в сознание."
                            }
                        }
                    ],
                    "synonyms": [
                        "umrzeć",
                        "odejść",
                        "skonać"
                    ],
                    "antonyms": [
                        "narodzić się"
                    ],
                    "common_phrases": [
                        "zgasnąć w spokoju",
                        "zgasnąć młodo",
                        "zgasnąć bezpotomnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умереть (обычно о спокойной смерти или после болезни)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угаснуть",
                                "target_lang_sense_clarification": "о человеке (перен.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "afb1b23f-782c-32c6-82e9-a03b94e919ad",
                    "sense_definition": "Een sterke innerlijke neiging of behoefte om iets te doen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "urge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij voelde een sterke <w>drang</w> om zich met de discussie te bemoeien.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал сильный <w>порыв</w> вмешаться в дискуссию."
                            }
                        },
                        {
                            "text": "Ze heeft een onweerstaanbare <w>drang</w> om verre reizen te maken.",
                            "target_lang_translations": {
                                "ru": "У неё непреодолимая <w>тяга</w> к дальним путешествиям."
                            }
                        }
                    ],
                    "synonyms": [
                        "aandrang",
                        "neiging",
                        "impuls",
                        "opwelling",
                        "aandrift"
                    ],
                    "common_phrases": [
                        "onweerstaanbare drang",
                        "drang naar",
                        "innerlijke drang"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное внутреннее желание или потребность совершить какое-либо действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порыв",
                                "target_lang_sense_clarification": "внезапное сильное побуждение"
                            },
                            {
                                "target_lang_word": "тяга",
                                "target_lang_sense_clarification": "сильное влечение или потребность"
                            },
                            {
                                "target_lang_word": "стремление",
                                "target_lang_sense_clarification": "настойчивое желание чего-либо достичь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5a0ed73-598a-3ba3-ae06-daa5952aaa8a",
                    "sense_definition": "Druk of dwang die van buitenaf op iemand wordt uitgeoefend.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "pressure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij nam de beslissing onder zware <w>drang</w> van zijn omgeving.",
                            "target_lang_translations": {
                                "ru": "Он принял решение под сильным <w>давлением</w> своего окружения."
                            }
                        },
                        {
                            "text": "Er ontstond politieke <w>drang</w> om de wetgeving snel aan te passen.",
                            "target_lang_translations": {
                                "ru": "Возникло политическое <w>давление</w> с целью быстро изменить законодательство."
                            }
                        }
                    ],
                    "common_phrases": [
                        "onder drang"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давление или принуждение, оказываемое на кого-либо извне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давление",
                                "target_lang_sense_clarification": "воздействие силой или авторитетом"
                            },
                            {
                                "target_lang_word": "принуждение",
                                "target_lang_sense_clarification": "насилие над волей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c684f08-27d5-3f59-9594-39d565d4e639",
                    "sense_definition": "Het tegen elkaar aan duwen van een menigte mensen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "crowd",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de <w>drang</w> van de menigte werd het kind bijna platgedrukt.",
                            "target_lang_translations": {
                                "ru": "Из-за <w>напора</w> толпы ребёнка чуть не раздавили."
                            }
                        },
                        {
                            "text": "De politie probeerde de <w>drang</w> bij de ingang van het stadion te verminderen.",
                            "target_lang_translations": {
                                "ru": "Полиция пыталась уменьшить <w>давку</w> у входа на стадион."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое давление, создаваемое тесно стоящими людьми в толпе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напор",
                                "target_lang_sense_clarification": "сильное давление массы людей"
                            },
                            {
                                "target_lang_word": "давка",
                                "target_lang_sense_clarification": "теснота и толкотня"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
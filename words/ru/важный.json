                        "маловажный",
                        "пустяковый"
                    ],
                    "common_phrases": [
                        "важное дело",
                        "важный вопрос",
                        "важная роль",
                        "важное событие",
                        "самое важное"
                    ],
                    "target_lang_definitions": {
                        "en": "Having great significance, substantial, weighty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "important",
                                "target_lang_sense_clarification": "having relevant and crucial value"
                            },
                            {

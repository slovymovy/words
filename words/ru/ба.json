    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1b27b53f-f5a7-3fe6-8bec-5de84aac5d87",
                    "sense_definition": "Восклицание, выражающее внезапное узнавание и удивление при неожиданной встрече.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "intj_surprise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ба! Кого я вижу в нашем скромном жилище!",
                            "target_lang_translations": {
                                "en": "Why! Look who I see in our humble abode!"
                            }
                        },
                        {
                            "text": "Ба! Какие люди к нам пожаловали, и без охраны!",
                            "target_lang_translations": {
                                "en": "Well! Look who has honored us with a visit, and without security!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "Ба, знакомые все лица",
                        "Ба, кого я вижу"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation expressing sudden recognition and surprise at an unexpected meeting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "why",
                                "target_lang_sense_clarification": "exclamation of surprise or recognition"
                            },
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "exclamation of surprise"
                            },
                            {
                                "target_lang_word": "wow",
                                "target_lang_sense_clarification": "expression of amazement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5028bde2-ec1d-330a-807b-870db267b7b4",
                    "sense_definition": "Возглас, выражающий сильное изумление или удивление по поводу какого-либо факта или открытия.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "intj_surprise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ба! Да ты, оказывается, ещё и полиглот!",
                            "target_lang_translations": {
                                "en": "Wow! It turns out you're a polyglot too!"
                            }
                        },
                        {
                            "text": "Ба! Да ведь это тот самый писательский дом, о котором все говорят.",
                            "target_lang_translations": {
                                "en": "Wow! This is that very writer's house everyone talks about."
                            }
                        }
                    ],
                    "synonyms": [
                        "ух ты",
                        "опа"
                    ],
                    "antonyms": [
                        "подумаешь"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation expressing strong amazement or surprise regarding a fact or discovery."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wow",
                                "target_lang_sense_clarification": "expression of amazement"
                            },
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "expression of surprise"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "769580a7-288f-3119-871f-750cf4a90105",
                    "sense_definition": "Сокращённая форма слова <w>бабушка</w>, обычно используемая как фамильярное или ласковое обращение.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",

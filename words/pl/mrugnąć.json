{
    "word_family": [
        "mrugać",
        "mrugnięcie",
        "zamrugać",
        "przymrużyć"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d5fa6f45-0c88-338f-a063-09e53cd1e5ae",
                    "sense_definition": "Szybko zamknąć i otworzyć jedno oko lub dwoje oczu, często jako znak porozumienia lub odruch fizjologiczny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_eyes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna <mrugnął> do mnie dyskretnie, dając znać, że to tylko żart.",
                            "target_lang_translations": {
                                "ru": "Мужчина незаметно <подмигнул> мне, давая понять, что это всего лишь шутка."
                            }
                        },
                        {
                            "text": "Nawet nie zdążyłem <mrugnąć>, a samochód już zniknął za rogiem.",
                            "target_lang_translations": {
                                "ru": "Я даже не успел <моргнуть>, как машина уже исчезла за углом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mrugnąć okiem",
                        "mrugnąć do kogoś",
                        "mrugnąć porozumiewawczo",
                        "ani mrugnąć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро закрыть и открыть один или оба глаза, часто как знак понимания или физиологический рефлекс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моргнуть",
                                "target_lang_sense_clarification": "сделать движение веками"
                            },
                            {
                                "target_lang_word": "подмигнуть",
                                "target_lang_sense_clarification": "сделать знак одним глазом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "635a1e85-746f-334a-ac52-dc1da63e4d51",
                    "sense_definition": "O świetle: zaświecić krótko, błysnąć lub zmienić jasność na moment.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stara lampa w korytarzu <mrugnęła> kilka razy i nagle zgasła.",
                            "target_lang_translations": {
                                "ru": "Старая лампа в коридоре несколько раз <мигнула> и внезапно погасла."
                            }
                        },
                        {
                            "text": "W oddali <mrugnęło> czerwone światło ostrzegawcze na wysokiej wieży.",
                            "target_lang_translations": {
                                "ru": "Вдали <мигнул> красный предупреждающий свет на высокой башне."
                            }
                        }
                    ],
                    "synonyms": [
                        "błysnąć",
                        "zamigotać"
                    ],
                    "common_phrases": [
                        "światło mrugnęło",
                        "ekran mrugnął"
                    ],
                    "target_lang_definitions": {
                        "ru": "О свете: коротко вспыхнуть, сверкнуть или на мгновение изменить яркость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мигнуть",
                                "target_lang_sense_clarification": "о свете"
                            },
                            {
                                "target_lang_word": "моргнуть",
                                "target_lang_sense_clarification": "о лампе или электричестве"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
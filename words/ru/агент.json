            "senses": [
                {
                    "sense_id": "33f96b14-b2ca-310d-b777-a5d02183b770",
                    "sense_definition": "Лицо, действующее по поручению учреждения, организации или другого лица; уполномоченный представитель.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Страховой <w>агент</w> подробно объяснил нам условия нового договора.",
                            "target_lang_translations": {
                                "en": "The insurance <w>agent</w> explained the terms of the new contract to us in detail."
                            }
                        },
                        {
                            "text": "Мой литературный <w>агент</w> сейчас ведёт переговоры с крупным издательством.",
                            "target_lang_translations": {
                                "en": "My literary <w>agent</w> is currently negotiating with a major publishing house."
                            }
                        }
                    ],
                    "synonyms": [
                        "представитель",
                        "уполномоченный"
                    ],
                    "common_phrases": [
                        "страховой агент",
                        "торговый агент",
                        "агент по недвижимости",
                        "рекламный агент"
                    ],
                    "target_lang_definitions": {
                        "en": "A person acting on instructions from an institution, organization, or another person; an authorized representative."
                    },
                    "translations": {
                        "en": [

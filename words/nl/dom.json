        "domweg",
        "bisdom"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bafabcc0-d5d8-3a6c-a24c-626ef38b7ffe",
                    "sense_definition": "Getuigend van weinig verstand of inzicht; niet slim.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "stupidity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was niet erg handig om die <domme> fout te maken tijdens het examen.",
                            "target_lang_translations": {
                                "en": "It wasn't very handy to make that <stupid> mistake during the exam."
                            }
                        },
                        {
                            "text": "Hij voelde zich een beetje <dom> toen hij de grap niet begreep.",
                            "target_lang_translations": {
                                "en": "He felt a bit <dumb> when he didn't understand the joke."
                            }
                        }
                    ],
                    "synonyms": [
                        "stom",
                        "achterlijk",
                        "hersenloos",
                        "dwaas",
                        "gek"
                    ],
                    "antonyms": [
                        "slim",
                        "intelligent"
                    ],
                    "common_phrases": [
                        "doe niet zo dom",
                        "een domme fout",
                        "te dom voor woorden"
                    ],
                    "target_lang_definitions": {
                        "en": "Showing little understanding or insight; not smart."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stupid",
                                "target_lang_sense_clarification": "lacking intelligence or common sense"
                            },
                            {
                                "target_lang_word": "dumb",
                                "target_lang_sense_clarification": "informal: unintelligent or foolish"
                            },
                            {
                                "target_lang_word": "foolish",
                                "target_lang_sense_clarification": "lacking good judgment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b918fb6a-b46f-3fcb-861b-8945ad5e7758",
                    "sense_definition": "Min of meer toevallig; zonder duidelijke reden of oorzaak.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "chance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door <dom> toeval kwamen we elkaar tegen in de supermarkt.",
                            "target_lang_translations": {
                                "en": "By <sheer> coincidence we ran into each other at the supermarket."
                            }
                        },
                        {
                            "text": "Het was <domme> pech dat de trein net voor onze neus vertrok.",
                            "target_lang_translations": {
                                "en": "It was <rotten> luck that the train left right under our noses."
                            }
                        }
                    ],
                    "synonyms": [
                        "stom",
                        "dwaas"
                    ],
                    "common_phrases": [
                        "domme pech",
                        "dom geluk",
                        "dom toeval"
                    ],
                    "target_lang_definitions": {
                        "en": "More or less accidental; without clear reason or cause."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sheer",
                                "target_lang_sense_clarification": "used to emphasize the randomness of luck or coincidence"
                            },
                            {
                                "target_lang_word": "rotten",
                                "target_lang_sense_clarification": "used specifically with 'luck' (bad luck)"
                            },
                            {
                                "target_lang_word": "blind",
                                "target_lang_sense_clarification": "occurring by chance without foresight"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "610b6349-d7ff-3ce9-b7d9-0da3f0c385b0",
                    "sense_definition": "Routinematig en saai; werk waar weinig geestelijke inspanning voor nodig is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "routine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij moest de hele dag <dom> werk doen in de fabriek.",
                            "target_lang_translations": {
                                "en": "He had to do <mindless> work in the factory all day."
                            }
                        },
                        {
                            "text": "Ik heb geen zin in <domme> klusjes waar ik niet bij hoef na te denken.",
                            "target_lang_translations": {
                                "en": "I don't feel like doing <menial> chores where I don't have to think."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dom werk",
                        "domme kracht"
                    ],
                    "target_lang_definitions": {
                        "en": "Routine and boring; work that requires little mental effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mindless",
                                "target_lang_sense_clarification": "requiring no intellectual effort"
                            },
                            {
                                "target_lang_word": "menial",
                                "target_lang_sense_clarification": "boring, unskilled work"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "36d62e55-c665-3bfb-a0b4-8b6337c067a1",
                    "sense_definition": "Pora dnia między zachodem słońca a nocą, gdy światło dzienne zanika i robi się ciemno.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time_of_day",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wracaliśmy do domu o <w>zmierzchu</w>, gdy zapalały się pierwsze latarnie.",
                            "target_lang_translations": {
                                "ru": "Мы возвращались домой в <w>сумерках</w>, когда зажигались первые фонари."
                            }
                        },
                        {
                            "text": "Latem <w>zmierzch</w> zapada bardzo późno, co pozwala na długie spacery.",
                            "target_lang_translations": {
                                "ru": "Летом <w>сумерки</w> наступают очень поздно, что позволяет долго гулять."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmrok"
                    ],
                    "antonyms": [
                        "świt"
                    ],
                    "common_phrases": [
                        "o zmierzchu",
                        "zapada zmierzch",
                        "szary zmierzch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время суток между заходом солнца и ночью, когда дневной свет угасает и становится темно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сумерки",
                                "target_lang_sense_clarification": "время суток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "008477f2-a10c-3894-a252-9e5f35ab068a",
                    "sense_definition": "Końcowy etap istnienia, działania lub popularności czegoś; powolny upadek.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "end_stage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Historycy opisują ten okres jako powolny <w>zmierzch</w> wielkiego imperium.",
                            "target_lang_translations": {
                                "ru": "Историки описывают этот период как медленный <w>закат</w> великой империи."
                            }
                        },
                        {
                            "text": "To wydarzenie zapoczątkowało <w>zmierzch</w> jego długiej kariery politycznej.",
                            "target_lang_translations": {
                                "ru": "Это событие положило начало <w>закату</w> его долгой политической карьеры."
                            }
                        }
                    ],
                    "synonyms": [
                        "koniec",
                        "kres"
                    ],
                    "common_phrases": [
                        "zmierzch życia",
                        "zmierzch epoki",
                        "zmierzch bogów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конечный этап существования, деятельности или популярности чего-либо; медленный упадок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закат",
                                "target_lang_sense_clarification": "перен. конец, упадок"
                            },
                            {
                                "target_lang_word": "упадок",
                                "target_lang_sense_clarification": "ухудшение состояния"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "bab732ab-8470-3c43-b076-2e14cbd77691",
                    "sense_definition": "Ter aanduiding van verzet, bestrijding of onenigheid met iets of iemand.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "opposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij nam medicijnen <tegen> zijn zware hoofdpijn.",
                            "target_lang_translations": {
                                "en": "He took medication <for> his severe headache.",
                                "ru": "Он принял лекарство <от> сильной головной боли."
                            }
                        },
                        {
                            "text": "De demonstranten protesteerden fel <tegen> het nieuwe beleid.",
                            "target_lang_translations": {
                                "en": "The demonstrators protested fiercely <against> the new policy.",
                                "ru": "Демонстранты яростно протестовали <против> новой политики."
                            }
                        }
                    ],
                    "synonyms": [
                        "anti",
                        "contra"
                    ],
                    "antonyms": [
                        "voor"
                    ],
                    "common_phrases": [
                        "ergens tegen zijn",
                        "tegen de regels",
                        "tegen de stroom in"
                    ],
                    "target_lang_definitions": {
                        "en": "To indicate resistance, fighting, or disagreement with something or someone.",
                        "ru": "Для обозначения сопротивления, борьбы или несогласия с чем-либо или кем-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "against",
                                "target_lang_sense_clarification": "opposition/resistance"
                            },
                            {
                                "target_lang_word": "anti",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "против",
                                "target_lang_sense_clarification": "общее значение сопротивления или оппозиции"
                            },
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "используется с лекарствами или средствами защиты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a92ae9b5-ac19-3d1e-b43e-c5aec8969572",
                    "sense_definition": "In fysieke aanraking met, botsend op of steunend tegen iets.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fiets staat veilig <tegen> de muur geparkeerd.",
                            "target_lang_translations": {
                                "en": "The bicycle is parked safely <against> the wall.",
                                "ru": "Велосипед надежно припаркован <у> стены."
                            }
                        },
                        {
                            "text": "Ze liep per ongeluk hard <tegen> de dichte deur aan.",
                            "target_lang_translations": {
                                "en": "She accidentally walked hard <into> the closed door.",
                                "ru": "Она случайно сильно ударилась <о> закрытую дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "aan"

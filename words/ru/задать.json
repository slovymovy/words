    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c6964587-fe3c-3439-9a8a-980998e435f0",
                    "sense_definition": "Обратиться к кому-либо с вопросом, чтобы узнать что-то или выразить сомнение.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <задать> тебе один простой вопрос."
                        },
                        {
                            "text": "Студенты не побоялись <задать> лектору неудобные вопросы."
                        }
                    ],
                    "common_phrases": [
                        "задать вопрос",
                        "задать жару"
                    ]
                },
                {
                    "sense_id": "fa96a433-445d-3864-914c-73e14300d113",
                    "sense_definition": "Поручить выполнить какую-либо работу, урок или упражнение.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель <задал> нам прочитать две главы из учебника."
                        },
                        {
                            "text": "Нам ничего не <задали> на завтра по математике."
                        }
                    ],
                    "synonyms": [
                        "поручить",
                        "дать задание"
                    ],
                    "common_phrases": [
                        "задать урок",
                        "задать домашнее задание",
                        "задать работу"
                    ]
                },
                {
                    "sense_id": "197834f1-e314-34e4-9e24-8430e8341486",
                    "sense_definition": "Установить или определить заранее какие-либо параметры, величины, ритм или направление.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Своим выступлением он <задал> тон всей конференции."
                        },
                        {
                            "text": "Вам нужно <задать> правильные параметры в настройках программы."
                        }
                    ],
                    "synonyms": [
                        "определить",
                        "указать",
                        "назначить",
                        "устроить"
                    ],
                    "common_phrases": [
                        "задать тон",
                        "задать темп",
                        "задать ритм",
                        "задать параметры"
                    ]
                },
                {
                    "sense_id": "ab4dc6b9-5ebc-39c3-84e8-3357a21558a1",
                    "sense_definition": "Сильно отругать, наказать или причинить неприятности (обычно в составе устойчивых выражений).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец обещал <задать> мне трёпку, если я снова опоздаю."
                        },
                        {
                            "text": "Мы им ещё <зададим>, пусть только попробуют вернуться!"
                        }
                    ],
                    "synonyms": [
                        "наказать"
                    ],
                    "common_phrases": [
                        "задать трёпку",
                        "задать жару",
                        "задать головомойку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "870d583f-0b31-36d2-a250-3720ca7b4f5a",
                    "sense_definition": "Устроить или организовать торжественное мероприятие, пир или банкет.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В честь победы генерал <задал> грандиозный пир для офицеров."
                        },
                        {
                            "text": "Они решили <задать> бал, о котором будет говорить весь город."
                        }
                    ],
                    "synonyms": [
                        "устроить",
                        "закатить"
                    ],
                    "common_phrases": [
                        "задать пир",
                        "задать бал",
                        "задать банкет"
                    ]
                }
            ]
        }
    ]
}
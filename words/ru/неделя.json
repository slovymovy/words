            "senses": [
                {
                    "sense_id": "deb6fcf6-6f7f-3c3b-85ba-7af9038959a4",
                    "sense_definition": "Единица исчисления времени, равная семи дням; период от понедельника до воскресенья.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На этой <w>неделе</w> у нас будет много работы в офисе.",
                            "target_lang_translations": {
                                "en": "We will have a lot of work at the office this <w>week</w>."
                            }
                        },
                        {
                            "text": "Мы собираемся поехать к бабушке через одну <w>неделю</w>.",
                            "target_lang_translations": {
                                "en": "We are going to visit grandma in one <w>week</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "седмица"
                    ],
                    "common_phrases": [
                        "на этой неделе",
                        "на прошлой неделе",
                        "на следующей неделе",
                        "всю неделю",
                        "рабочая неделя"
                    ],
                    "target_lang_definitions": {
                        "en": "A unit of time equal to seven days; the period from Monday to Sunday."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "week",
                                "target_lang_sense_clarification": "period of seven days"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b050c92-a122-302b-96a9-8004273e0b3a",
                    "sense_definition": "Семидневный промежуток времени, посвящённый какому-либо общественному событию или мероприятию.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем городе сейчас проходит <w>неделя</w> французского кино.",
                            "target_lang_translations": {
                                "en": "A French cinema <w>week</w> is currently taking place in our city."
                            }
                        },
                        {
                            "text": "Знаменитая <w>неделя</w> высокой моды привлекает тысячи туристов.",
                            "target_lang_translations": {
                                "en": "The famous high fashion <w>week</w> attracts thousands of tourists."
                            }
                        }
                    ],
                    "common_phrases": [
                        "неделя моды",
                        "неделя книги"
                    ],
                    "target_lang_definitions": {
                        "en": "A seven-day period dedicated to a specific public event or activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "week",
                                "target_lang_sense_clarification": "event duration"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "415db44b-e076-3d98-9375-59e604ff1456",
                    "sense_definition": "Воскресенье (день, когда «не делают» дел).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старину крестьяне называли этот день <w>неделей</w> и отдыхали от работы.",
                            "target_lang_translations": {
                                "en": "In the old days, peasants called this day <w>Sunday</w> and rested from work."
                            }
                        },
                        {
                            "text": "Приходи ко мне в <w>неделю</w> после утренней службы.",
                            "target_lang_translations": {
                                "en": "Come to see me on <w>Sunday</w> after the morning service."
                            }
                        }
                    ],

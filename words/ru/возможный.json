            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2f045cca-0466-3246-af06-cbdaf5e09c0a",
                    "sense_definition": "Такой, который может случиться, наступить или существовать.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "potentiality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы рассмотрели каждый <возможный> вариант решения задачи.",
                            "target_lang_translations": {
                                "en": "We considered every <w>possible</w> option for solving the problem."
                            }
                        },
                        {
                            "text": "Завтра <возможен> сильный дождь и ветер.",
                            "target_lang_translations": {
                                "en": "Heavy rain and wind are <w>possible</w> tomorrow."
                            }
                        }
                    ],
                    "synonyms": [
                        "вероятный",
                        "мыслимый"
                    ],
                    "antonyms": [
                        "невозможный",
                        "невероятный",
                        "немыслимый"
                    ],
                    "common_phrases": [
                        "возможный вариант",
                        "возможный результат",
                        "возможный риск",
                        "любой возможный"
                    ],
                    "target_lang_definitions": {
                        "en": "That which may happen, occur, or exist."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "possible",
                                "target_lang_sense_clarification": "able to happen or exist"
                            },
                            {
                                "target_lang_word": "probable",
                                "target_lang_sense_clarification": "likely to happen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a5ddd9a-b811-3b31-b6f6-ad7128f809b3",
                    "sense_definition": "Такой, который можно осуществить, сделать или выполнить.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "potentiality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обещаю сделать всё <возможное>, чтобы помочь вам.",
                            "target_lang_translations": {
                                "en": "I promise to do everything <w>possible</w> to help you."
                            }
                        },
                        {
                            "text": "Ремонт компьютера всё ещё <возможен>, не выбрасывайте его.",
                            "target_lang_translations": {
                                "en": "Repairing the computer is still <w>possible</w>, do not throw it away."
                            }
                        }
                    ],
                    "synonyms": [
                        "мыслимый"
                    ],
                    "antonyms": [
                        "невозможный"
                    ],
                    "common_phrases": [
                        "сделать всё возможное",
                        "насколько это возможно"
                    ],
                    "target_lang_definitions": {
                        "en": "That which can be realized, done, or executed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "possible",
                                "target_lang_sense_clarification": "capable of being done"
                            },
                            {
                                "target_lang_word": "feasible",
                                "target_lang_sense_clarification": "able to be done in practice"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b199195-d839-33ef-ba79-4a5a1f8100d0",
                    "sense_definition": "Допустимый, позволительный; такой, который не противоречит нормам.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "permissibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он ответил на сложный вопрос со всей <возможной> вежливостью.",
                            "target_lang_translations": {
                                "en": "He answered the difficult question with all <w>possible</w> politeness."
                            }
                        },
                        {
                            "text": "В данной ситуации компромисс вряд ли <возможен>.",
                            "target_lang_translations": {
                                "en": "In this situation, a compromise is hardly <w>possible</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "допустимый"
                    ],
                    "antonyms": [
                        "невозможный"
                    ],
                    "common_phrases": [
                        "со всей возможной"
                    ],
                    "target_lang_definitions": {
                        "en": "Admissible, permissible; that which does not contradict norms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "possible",
                                "target_lang_sense_clarification": "achievable within limits or norms"
                            }
                        ]
                    }
                }
            ]
        }
    ]

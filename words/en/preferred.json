                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Conservative investors often choose <w>preferreds</w> for their steady income.",
                            "target_lang_translations": {

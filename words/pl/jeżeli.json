    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "4630df89-64bc-3b8b-8bea-2001bcaf7ba0",
                    "sense_definition": "Spójnik wprowadzający zdanie podrzędne warunkowe; oznacza, że realizacja czynności lub stanu w zdaniu nadrzędnym zależy od spełnienia określonego warunku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "conditional_marker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie pójdziemy na spacer, <w>jeżeli</w> będzie padał deszcz.",
                            "target_lang_translations": {
                                "ru": "Мы не пойдем гулять, <w>если</w> будет идти дождь."
                            }
                        },
                        {
                            "text": "<w>Jeżeli</w> masz chwilę czasu, proszę, pomóż mi z tym zadaniem.",
                            "target_lang_translations": {
                                "ru": "<w>Если</w> у тебя есть минутка, пожалуйста, помоги мне с этим заданием."
                            }
                        },
                        {
                            "text": "<w>Jeżeli</w> chodzi o twoją propozycję, muszę ją jeszcze przemyśleć.",
                            "target_lang_translations": {
                                "ru": "<w>Если</w> говорить о твоём предложении, я должен его еще обдумать."
                            }
                        }
                    ],
                    "synonyms": [
                        "jeśli",
                        "gdy",
                        "jak"
                    ],
                    "common_phrases": [
                        "jeżeli chodzi o",
                        "nawet jeżeli",
                        "jeżeli tak",
                        "jeżeli nie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, вводящий условное придаточное предложение; указывает, что действие в главном предложении зависит от выполнения определённого условия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "если",
                                "target_lang_sense_clarification": "союз"
                            },
                            {
                                "target_lang_word": "ежели",
                                "target_lang_sense_clarification": "устар. и разг."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d1ac9427-eba6-36cd-b77e-0663f6ae1742",
                    "sense_definition": "Dawne słowo używane w porównaniach, oznaczające podobieństwo do kogoś lub czegoś.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "comparison_marker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stał przed nami potężny i niewzruszony, <w>jeżeli</w> dąb w starym lesie.",
                            "target_lang_translations": {
                                "ru": "Он стоял перед нами, могучий и невозмутимый, <w>словно</w> дуб в старом лесу."
                            }
                        },
                        {
                            "text": "Jej twarz była blada <w>jeżeli</w> płótno.",
                            "target_lang_translations": {
                                "ru": "Её лицо было бледным, <w>как</w> полотно."
                            }
                        }
                    ],
                    "synonyms": [
                        "jak"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane w dawnym stylu literackim"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее слово, используемое в сравнениях, означающее сходство с кем-либо или чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "сравнение"
                            },
                            {
                                "target_lang_word": "словно",
                                "target_lang_sense_clarification": "сравнение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

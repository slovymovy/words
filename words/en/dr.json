                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от слова \"drive\" (проезд); часто используется в названиях улиц и почтовых адресах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "драйв",

{
    "word_family": [
        "spontaniteit"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "77b6be4b-e604-3c9e-88ae-842793edd02d",
                    "sense_definition": "Geneigd om direct te handelen vanuit een gevoel, open en natuurlijk in de omgang.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is een heel <w>spontane</w> vrouw die makkelijk nieuwe vrienden maakt.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>непосредственная</w> женщина, которая легко заводит новых друзей."
                            }
                        },
                        {
                            "text": "De jury gaf haar een hoog cijfer vanwege haar <w>spontane</w> uitstraling.",
                            "target_lang_translations": {
                                "ru": "Жюри поставило ей высокую оценку за её <w>естественное</w> обаяние."
                            }
                        }
                    ],
                    "synonyms": [
                        "openhartig",
                        "natuurlijk",
                        "impulsief"
                    ],
                    "antonyms": [
                        "gereserveerd",
                        "verlegen",
                        "gemaakt"
                    ],
                    "common_phrases": [
                        "een spontane reactie",
                        "een spontane meid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Склонный действовать под влиянием чувств, открытый и естественный в общении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непосредственный",
                                "target_lang_sense_clarification": "открытый, искренний и естественный в поведении"
                            },
                            {
                                "target_lang_word": "естественный",
                                "target_lang_sense_clarification": "натуральный, без наигранности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "828b4d37-00d4-3022-a336-3afb849e7771",
                    "sense_definition": "Ontstaan vanuit een plotselinge ingeving, niet van tevoren gepland of door anderen afgedwongen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de toespraak klonk er een <w>spontaan</w> applaus vanuit de zaal.",
                            "target_lang_translations": {
                                "ru": "После речи в зале раздались <w>спонтанные</w> аплодисменты."
                            }
                        },
                        {
                            "text": "Het was een <w>spontane</w> actie om dit weekend naar zee te gaan.",
                            "target_lang_translations": {
                                "ru": "Это был <w>спонтанный</w> поступок — поехать на море в эти выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "ongedwongen",
                        "onverwacht",
                        "vrijwillig"
                    ],
                    "antonyms": [
                        "gepland",
                        "gedwongen",
                        "geforceerd"
                    ],
                    "common_phrases": [
                        "spontaan applaus",
                        "een spontaan idee"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возникающий внезапно, не запланированный заранее и не навязанный извне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спонтанный",
                                "target_lang_sense_clarification": "возникающий сам собой, без подготовки"
                            },
                            {
                                "target_lang_word": "незапланированный",
                                "target_lang_sense_clarification": "случайный, не намеченный заранее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76e6a5c2-8ca1-30c1-bf5a-b27a018d217c",
                    "sense_definition": "Vanzelf optredend zonder duidelijke oorzaak of invloed van buitenaf (vaak medisch of wetenschappelijk).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts stond versteld van de <w>spontane</w> genezing van de patiënt.",
                            "target_lang_translations": {
                                "ru": "Врач был поражен <w>спонтанным</w> выздоровлением пациента."
                            }
                        },
                        {
                            "text": "In de droge bossen is er in de zomer een groot risico op <w>spontane</w> branden.",
                            "target_lang_translations": {
                                "ru": "В сухих лесах летом высок риск <w>самопроизвольных</w> возгораний."
                            }
                        }
                    ],
                    "synonyms": [
                        "vanzelf"
                    ],
                    "antonyms": [
                        "geïnduceerd",
                        "kunstmatig"
                    ],
                    "common_phrases": [
                        "spontane genezing",
                        "spontane ontbranding",
                        "spontane mutatie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий сам по себе, без видимой причины или внешнего воздействия (часто в медицине или науке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самопроизвольный",
                                "target_lang_sense_clarification": "происходящий без внешнего стимула (научный термин)"
                            },
                            {
                                "target_lang_word": "спонтанный",
                                "target_lang_sense_clarification": "медицинский или научный контекст"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "80419586-aaa5-31ef-87a1-b9796bf8bca0",
                    "sense_definition": "Op een ongedwongen manier, zonder er lang over na te denken of te plannen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen hij de grap hoorde, begon hij <w>spontaan</w> hard te lachen.",
                            "target_lang_translations": {
                                "ru": "Услышав шутку, он <w>спонтанно</w> громко рассмеялся."
                            }
                        },
                        {
                            "text": "We besloten <w>spontaan</w> om vanavond uit eten te gaan.",
                            "target_lang_translations": {
                                "ru": "Мы <w>спонтанно</w> решили поужинать сегодня в ресторане."
                            }
                        }
                    ],
                    "synonyms": [
                        "onverwachts",
                        "zomaar",
                        "plotseling"
                    ],
                    "antonyms": [
                        "gepland"
                    ],
                    "common_phrases": [
                        "iets spontaan doen",
                        "spontaan beslissen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непринужденно, не раздумывая долго и не планируя заранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спонтанно",
                                "target_lang_sense_clarification": "по внезапному порыву"
                            },
                            {
                                "target_lang_word": "экспромтом",
                                "target_lang_sense_clarification": "без предварительной подготовки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
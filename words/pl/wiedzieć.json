    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "465e2784-b185-3977-bcef-459a5406e47b",
                    "sense_definition": "Posiadać informacje na dany temat, być świadomym faktów lub sytuacji",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "knowledge_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>wiedziałem</w> o twoich problemach w pracy.",
                            "target_lang_translations": {
                                "ru": "Я не <w>знал</w> о твоих проблемах на работе."
                            }
                        },
                        {
                            "text": "Skąd <w>wiesz</w>, że on mówi prawdę?",
                            "target_lang_translations": {
                                "ru": "Откуда ты <w>знаешь</w>, что он говорит правду?"
                            }
                        }
                    ],
                    "synonyms": [
                        "orientować się",
                        "zdawać sobie sprawę",
                        "być świadomym"
                    ],
                    "antonyms": [
                        "nie mieć zielonego pojęcia"
                    ],
                    "common_phrases": [
                        "wiedzieć o czymś",
                        "skąd wiesz",
                        "nie wiem",
                        "dobrze wiedzieć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь информацию или сведения о чём-либо, быть в курсе событий"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "обладать информацией"
                            },
                            {
                                "target_lang_word": "быть в курсе",
                                "target_lang_sense_clarification": "быть осведомленным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f858a0ae-b10c-3650-ba82-4c5de559e256",
                    "sense_definition": "Znać sposób wykonania czegoś lub rozumieć, jak coś zrobić",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "knowledge_skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wiem</w>, jak naprawić ten stary zegar.",
                            "target_lang_translations": {
                                "ru": "Я <w>знаю</w>, как починить эти старые часы."
                            }
                        },
                        {
                            "text": "Ona nie <w>wie</w>, jak trafić na dworzec kolejowy.",
                            "target_lang_translations": {
                                "ru": "Она не <w>знает</w>, как добраться до вокзала."
                            }
                        }
                    ],
                    "synonyms": [
                        "umieć",
                        "potrafić"
                    ],
                    "common_phrases": [
                        "wiedzieć jak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Владеть способом выполнения чего-либо, понимать, как действовать"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "как сделать что-либо"
                            },
                            {
                                "target_lang_word": "уметь",
                                "target_lang_sense_clarification": "обладать навыком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0da42ca-37ce-3cec-ad88-1f09891b3b8f",
                    "sense_definition": "Być całkowicie przekonanym o prawdziwości czegoś, nie mieć wątpliwości",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "knowledge_certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wiem</w> na pewno, że to była dobra decyzja.",
                            "target_lang_translations": {
                                "ru": "Я <w>знаю</w> наверняка, что это было правильное решение."
                            }
                        },
                        {
                            "text": "On <w>wie</w>, co jest dla niego najlepsze w tej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Он <w>знает</w>, что для него лучше всего в этой ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "być pewnym",
                        "mieć pewność"
                    ],
                    "antonyms": [
                        "przypuszczać",
                        "domyślać się",
                        "spekulować"
                    ],
                    "common_phrases": [
                        "wiedzieć na pewno",
                        "wiedzieć swoje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть абсолютно уверенным в чём-либо, не сомневаться"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знать",
                                "target_lang_sense_clarification": "быть уверенным"
                            },
                            {
                                "target_lang_word": "быть уверенным",
                                "target_lang_sense_clarification": "не сомневаться"
                            }
                        ]
                    }
                }
            ]
        }
    ]

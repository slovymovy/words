    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8781072d-03e6-3847-9b81-857212833526",
                    "sense_definition": "One who offers advice.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My academic <w>advisor</w> helped me choose the right courses for the semester.",
                            "target_lang_translations": {
                                "ru": "Мой научный <w>руководитель</w> помог мне выбрать подходящие курсы на семестр."
                            }
                        },
                        {
                            "text": "She works as a financial <w>advisor</w> for a large international bank.",
                            "target_lang_translations": {
                                "ru": "Она работает финансовым <w>консультантом</w> в крупном международном банке."
                            }
                        }
                    ],
                    "synonyms": [
                        "counselor",
                        "consultant"
                    ],
                    "common_phrases": [
                        "financial advisor",
                        "legal advisor",
                        "academic advisor",
                        "senior advisor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто дает советы или предоставляет профессиональные консультации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "советник",
                                "target_lang_sense_clarification": "должностное лицо или человек, дающий советы"
                            },
                            {
                                "target_lang_word": "консультант",
                                "target_lang_sense_clarification": "специалист, дающий советы в определенной области"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6393e274-1832-33d4-854d-7da799a2c542",
                    "sense_definition": "A xiangqi piece that is moved one point diagonally and confined within the palace.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player moved the <w>advisor</w> to protect the general from the attack.",
                            "target_lang_translations": {
                                "ru": "Игрок сделал ход <w>советником</w>, чтобы защитить генерала от атаки."
                            }
                        },
                        {
                            "text": "In Chinese chess, an <w>advisor</w> cannot leave the palace area.",
                            "target_lang_translations": {
                                "ru": "В китайских шахматах <w>советник</w> не может покидать зону дворца."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фигура в сянци (китайских шахматах), которая ходит на одну точку по диагонали и ограничена пределами дворца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "советник",
                                "target_lang_sense_clarification": "фигура в китайских шахматах"
                            }
                        ]
                    }
                }
            ]
        }
    ]

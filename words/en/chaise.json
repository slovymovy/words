            "senses": [
                {
                    "sense_id": "76157e6d-517e-342f-9f5c-85cc01fe0bd4",
                    "sense_definition": "A long reclining chair with a seat extended enough to support the legs; a short form of \"chaise longue\".",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She relaxed on the <w>chaise</w> by the swimming pool while reading a magazine.",
                            "target_lang_translations": {
                                "ru": "Она отдыхала на <w>шезлонге</w> у бассейна, читая журнал."
                            }
                        },
                        {
                            "text": "The interior designer placed a velvet <w>chaise</w> near the window for comfortable seating.",
                            "target_lang_translations": {
                                "ru": "Дизайнер интерьера поставил у окна бархатную <w>кушетку</w>, чтобы на ней было удобно сидеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "daybed",
                        "lounger",
                        "chaise longue"
                    ],
                    "common_phrases": [
                        "chaise lounge",
                        "pool chaise",
                        "beach chaise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинное кресло с удлиненным сиденьем для поддержки ног; сокращение от \"chaise longue\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шезлонг",
                                "target_lang_sense_clarification": "кресло для отдыха полулежа (обычно на улице)"
                            },
                            {
                                "target_lang_word": "кушетка",
                                "target_lang_sense_clarification": "предмет мебели для интерьера"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "071c434d-57a9-3257-a465-7c56337e5def",
                    "sense_definition": "An open carriage pulled by a horse, typically having two wheels and space for one or two passengers.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor arrived at the estate in a small horse and <w>chaise</w>.",
                            "target_lang_translations": {
                                "ru": "Доктор прибыл в поместье в небольшой <w>двуколке</w>."
                            }
                        },
                        {
                            "text": "They enjoyed a pleasant ride through the countryside in an open <w>chaise</w>.",
                            "target_lang_translations": {
                                "ru": "Они наслаждались приятной поездкой за городом в открытой <w>коляске</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "carriage",
                        "buggy"
                    ],
                    "common_phrases": [
                        "horse and chaise",
                        "pony chaise"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to a historical mode of transport"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытый конный экипаж, обычно двухколесный, с местом для одного или двух пассажиров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двуколка",
                                "target_lang_sense_clarification": "легкий двухколесный экипаж"
                            },
                            {
                                "target_lang_word": "коляска",
                                "target_lang_sense_clarification": "конный экипаж"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "weggang"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "12909155-c4d0-3c32-a5c4-3217a6b30588",
                    "sense_definition": "Een plek verlaten om ergens anders heen te gaan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verplaatsing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten nu echt <w>weggaan</w>, anders komen we niet meer op tijd.",
                            "target_lang_translations": {
                                "ru": "Нам нужно сейчас же <w>уходить</w>, иначе мы не успеем вовремя."
                            }
                        },
                        {
                            "text": "Ik wil hier niet <w>weggaan</w> zonder eerst afscheid te nemen.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <w>уходить</w> отсюда, не попрощавшись."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertrekken"
                    ],
                    "antonyms": [
                        "blijven",
                        "aankomen"
                    ],
                    "common_phrases": [
                        "nu weggaan",
                        "moeten weggaan",
                        "snel weggaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покидать место, чтобы отправиться куда-то еще."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "покидать",
                                "target_lang_sense_clarification": "оставлять место"
                            },
                            {
                                "target_lang_word": "отправляться",
                                "target_lang_sense_clarification": "начинать путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14eeccc8-6a50-3ae3-bced-1451facc152d",
                    "sense_definition": "Het huis uitgaan om iets leuks te doen, zoals feesten of op vakantie gaan.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "verplaatsing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wilde jij vanavond nog <w>weggaan</w> met je vrienden?",
                            "target_lang_translations": {
                                "ru": "Ты хотел сегодня вечером куда-нибудь <w>выбраться</w> с друзьями?"
                            }
                        },
                        {
                            "text": "We hebben besloten dat we dit weekend lekker gaan <w>weggaan</w>.",
                            "target_lang_translations": {
                                "ru": "Мы решили, что в эти выходные <w>уедем</w> куда-нибудь отдохнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertrekken",
                        "vertrekken"
                    ],
                    "antonyms": [
                        "thuisblijven"
                    ],
                    "common_phrases": [
                        "een weekendje weggaan",
                        "nog even weggaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выходить из дома ради развлечения или уезжать в отпуск."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уезжать",
                                "target_lang_sense_clarification": "в отпуск или поездку"
                            },
                            {
                                "target_lang_word": "выбираться",
                                "target_lang_sense_clarification": "выходить куда-то развлечься"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80409954-fc9d-31ae-890f-51f6202c77b5",
                    "sense_definition": "Een liefdesrelatie beëindigen en de partner verlaten.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relatie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vriendin van de buurman is gisteren bij hem <w>weggegaan</w>.",
                            "target_lang_translations": {
                                "ru": "Девушка соседа вчера от него <w>ушла</w>."
                            }
                        },
                        {
                            "text": "Ze heeft lang nagedacht voordat ze besloot bij haar man te <w>weggaan</w>.",
                            "target_lang_translations": {
                                "ru": "Она долго думала, прежде чем решила <w>уйти</w> от мужа."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertrekken",
                        "vertrekken"
                    ],
                    "antonyms": [
                        "blijven"
                    ],
                    "common_phrases": [
                        "bij iemand weggaan",
                        "bij hem weggaan",
                        "bij haar weggaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заканчивать романтические отношения и покидать партнера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "от партнера"
                            },
                            {
                                "target_lang_word": "бросать",
                                "target_lang_sense_clarification": "разрывать отношения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
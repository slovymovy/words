    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc920059-0bb2-36ce-9e91-7707e4c14b66",
                    "sense_definition": "Jednostka długości stosowana w krajach anglosaskich oraz w żegludze i lotnictwie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "measure_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ograniczenie prędkości na tej drodze wynosi pięćdziesiąt <w>mil</w> na godzinę.",
                            "target_lang_translations": {
                                "ru": "Ограничение скорости на этой дороге составляет пятьдесят <w>миль</w> в час."
                            }
                        },
                        {
                            "text": "Statek musi przepłynąć jeszcze jedną <w>milę</w> morską, aby dotrzeć do portu.",
                            "target_lang_translations": {
                                "ru": "Корабль должен пройти еще одну морскую <w>милю</w>, чтобы добраться до порта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mila morska",
                        "mila lądowa",
                        "na milę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения длины, используемая в англосаксонских странах, а также в судоходстве и авиации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миля",
                                "target_lang_sense_clarification": "единица измерения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "973ba6b5-0449-3422-ade5-1559253e45d4",
                    "sense_definition": "Żeńskie imię, często funkcjonujące jako zdrobnienie od imienia <w>Emilia</w>.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "person_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Moja przyjaciółka <w>Mila</w> mieszka w sąsiednim bloku.",
                            "target_lang_translations": {
                                "ru": "Моя подруга <w>Мила</w> живет в соседнем доме."
                            }
                        },
                        {
                            "text": "Wczoraj <w>Mila</w> pomogła mi w nauce matematyki.",
                            "target_lang_translations": {
                                "ru": "Вчера <w>Мила</w> помогла мне с математикой."
                            }
                        }
                    ],
                    "synonyms": [
                        "Emilia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя, часто используемое как уменьшительная форма имени Эмилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мила",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]

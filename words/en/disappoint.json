                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расстраивать",
                                "target_lang_sense_clarification": "нарушать планы"
                            },
                            {
                                "target_lang_word": "срывать",
                                "target_lang_sense_clarification": "прекращать действие чего-либо, мешать успеху"
                            }
                        ]
                    }
                }
            ]
        }

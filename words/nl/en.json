        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7a73cac4-67b1-3e9a-b179-9a3ea93159b9",
                    "sense_definition": "Wordt gebruikt aan het begin van een zin om een gesprek te openen, te hervatten of om een reactie te vragen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>En</w>, heb je al iets kunnen ontdekken?",
                            "target_lang_translations": {
                                "ru": "<w>Ну</w>, тебе уже удалось что-то выяснить?",
                                "en": "<w>Well</w>, have you been able to discover anything yet?"
                            }
                        },
                        {
                            "text": "<w>En</w>? Ga je mee naar de film vanavond?",
                            "target_lang_translations": {
                                "ru": "<w>Ну</w>? Ты пойдешь сегодня вечером в кино?",
                                "en": "<w>So</w>? Are you coming to the movies tonight?"
                            }

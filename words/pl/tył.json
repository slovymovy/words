            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2ae28b78-bf9c-3fbf-a8c0-651e13713108",
                    "sense_definition": "Tylna część przedmiotu, pomieszczenia lub pojazdu; strona przeciwna do przedniej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_back",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pasażerowie usiedli z <tyłu> samochodu, aby mieć więcej miejsca.",
                            "target_lang_translations": {
                                "ru": "Пассажиры сели <w>сзади</w> в машине, чтобы было больше места."
                            }
                        },
                        {
                            "text": "Na <tyłach> tego budynku znajduje się piękny ogród.",
                            "target_lang_translations": {
                                "ru": "На <w>задворках</w> этого здания находится красивый сад."
                            }
                        }
                    ],
                    "antonyms": [
                        "przód",
                        "front"
                    ],
                    "common_phrases": [
                        "z tyłu",
                        "do tyłu",
                        "w tyle",
                        "na tyłach"
                    ],
                    "target_lang_definitions": {

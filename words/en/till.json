                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Farmers <w>till</w> the land before planting seeds in the spring.",
                            "target_lang_translations": {
                                "ru": "Фермеры <w>пашут</w> землю весной перед посевом семян."
                            }

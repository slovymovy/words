                    "sense_id": "ae4901ca-0cc8-3f18-894e-32e9bb2b0ba6",
                    "sense_definition": "De handeling van het kopen; het verwerven van iets tegen betaling.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>aankoop</w> van het nieuwe huis duurde langer dan verwacht.",
                            "target_lang_translations": {
                                "ru": "<w>Покупка</w> нового дома заняла больше времени, чем ожидалось."
                            }
                        },
                        {
                            "text": "De <w>aankoop</w> kon niet doorgaan omdat ik mijn geld was vergeten.",
                            "target_lang_translations": {
                                "ru": "<w>Покупка</w> не могла состояться, так как я забыл деньги."
                            }
                        }
                    ],

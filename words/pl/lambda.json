                {
                    "sense_id": "a2d62721-806c-3ede-a7da-94025d3fedb5",
                    "sense_definition": "Jedenasta litera alfabetu greckiego (Λ, λ), odpowiadająca w języku polskim literze L.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "greek_alphabet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W matematyce często używa się litery <w>lambda</w> do oznaczania długości fali.",
                            "target_lang_translations": {
                                "ru": "В математике буква <w>лямбда</w> часто используется для обозначения длины волны."
                            }
                        },
                        {
                            "text": "Grecka <w>lambda</w> ma kształt przypominający odwróconą literę V.",
                            "target_lang_translations": {
                                "ru": "Греческая <w>лямбда</w> имеет форму, напоминающую перевернутую букву V."
                            }
                        }
                    ],
                    "common_phrases": [
                        "litera lambda",
                        "punkt lambda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одиннадцатая буква греческого алфавита (Λ, λ), соответствующая букве L в польском языке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лямбда",
                                "target_lang_sense_clarification": "греческая буква"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37b36ebb-6e02-3666-b751-9d7a9d987e69",
                    "sense_definition": "Cząstka elementarna z grupy barionów, oznaczana grecką literą Λ.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physics_particles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Barion <w>lambda</w> jest cząstką obojętną elektrycznie i rozpada się bardzo szybko.",
                            "target_lang_translations": {
                                "ru": "Барион <w>лямбда</w> является электрически нейтральной частицей и очень быстро распадается."
                            }
                        },
                        {
                            "text": "Fizycy badają właściwości cząstki <w>lambda</w> w akceleratorach.",
                            "target_lang_translations": {
                                "ru": "Физики исследуют свойства частицы <w>лямбда</w> в ускорителях."
                            }
                        }
                    ],
                    "synonyms": [
                        "barion lambda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элементарная частица из группы барионов, обозначаемая греческой буквой Λ."
                    },

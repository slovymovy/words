                        "wyjawianie",
                        "sypnięcie"
                    ],
                    "antonyms": [
                        "dochowanie tajemnicy",
                        "trzymanie gęby na kłódkę",
                        "milczenie jak głaz"
                    ],
                    "common_phrases": [

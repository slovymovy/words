{
    "word_family": [
        "decorate",
        "decoration",
        "decorative",
        "decorator"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ae2885d7-3807-3edf-a971-2ebcfce4f4ea",
                    "sense_definition": "Simple past and past participle of <w>decorate</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>decorated</w> the living room with balloons for the birthday party.",
                            "target_lang_translations": {
                                "ru": "Мы <w>украсили</w> гостиную воздушными шарами к вечеринке в честь дня рождения."
                            }
                        },
                        {
                            "text": "The entire street was <w>decorated</w> with lights for the festival.",
                            "target_lang_translations": {
                                "ru": "Вся улица была <w>украшена</w> огнями к фестивалю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "decorated the house",
                        "decorated with flowers",
                        "fully decorated"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past of decorate"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола \"украшать\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украсил",
                                "target_lang_sense_clarification": "глагол (прошедшее время)"
                            },
                            {
                                "target_lang_word": "украшенный",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "80f2d7b3-155d-35fe-90cc-5b3f95e644cb",
                    "sense_definition": "Having had decorations applied; adorned with attractive items.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>decorated</w> tree looked magical with all the ornaments and lights.",
                            "target_lang_translations": {
                                "ru": "<w>Украшенная</w> елка выглядела волшебно со всеми игрушками и огнями."
                            }
                        },
                        {
                            "text": "She placed a beautifully <w>decorated</w> cake in the center of the table.",
                            "target_lang_translations": {
                                "ru": "Она поставила красиво <w>украшенный</w> торт в центр стола."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beautifully decorated",
                        "decorated room",
                        "richly decorated"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшенный декоративными элементами; нарядный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украшенный",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "декорированный",
                                "target_lang_sense_clarification": "более формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b219b8ab-1880-39d5-bcac-d74acd19ab2c",
                    "sense_definition": "Having received prizes, awards, laurels etc.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is the most <w>decorated</w> soldier in the history of the regiment.",
                            "target_lang_translations": {
                                "ru": "Он самый <w>титулованный</w> солдат в истории полка."
                            }
                        },
                        {
                            "text": "As a highly <w>decorated</w> athlete, she was a role model for many young runners.",
                            "target_lang_translations": {
                                "ru": "Будучи высоко <w>титулованной</w> спортсменкой, она была примером для многих юных бегунов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "highly decorated",
                        "decorated veteran",
                        "decorated officer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получивший множество наград, орденов или знаков отличия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "титулованный",
                                "target_lang_sense_clarification": "о спортсменах или военных"
                            },
                            {
                                "target_lang_word": "награждённый",
                                "target_lang_sense_clarification": "имеющий награды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "873e7e63-5ab6-3f6a-a5ca-3375afdb1ae1",
                    "sense_definition": "In one of the styles of English Gothic architecture.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cathedral features a magnificent window in the <w>decorated</w> style.",
                            "target_lang_translations": {
                                "ru": "В соборе есть великолепное окно в <w>украшенном</w> стиле."
                            }
                        },
                        {
                            "text": "Historians studied the <w>decorated</w> tracery found in the old church ruins.",
                            "target_lang_translations": {
                                "ru": "Историки изучали ажурную каменную кладку <w>украшенного</w> стиля, найденную в руинах старой церкви."
                            }
                        }
                    ],
                    "common_phrases": [
                        "decorated style",
                        "decorated period"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к \"украшенному\" стилю английской готической архитектуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украшенный",
                                "target_lang_sense_clarification": "архитектурный стиль"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
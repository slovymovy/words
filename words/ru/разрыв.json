    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e92563e-4728-387f-8567-e12230bd3548",
                    "sense_definition": "Полное прекращение отношений между людьми, обычно сопровождающееся ссорой или конфликтом.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она очень тяжело переживала <разрыв> с любимым человеком.",
                            "target_lang_translations": {
                                "en": "She took the <breakup> with her beloved very hard."
                            }
                        },
                        {
                            "text": "Их постоянные споры неизбежно вели к полному <разрыву> отношений.",
                            "target_lang_translations": {
                                "en": "Their constant arguments were inevitably leading to a complete <rupture> of relations."
                            }
                        }
                    ],
                    "synonyms": [
                        "ссора",
                        "разлад",
                        "раскол"
                    ],
                    "antonyms": [
                        "соединение"
                    ],
                    "common_phrases": [
                        "разрыв отношений",
                        "полный разрыв",
                        "разрыв с прошлым",
                        "инициатор разрыва"
                    ],
                    "target_lang_definitions": {
                        "en": "A complete end to a relationship or connection between people, usually accompanied by a quarrel or conflict."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breakup",
                                "target_lang_sense_clarification": "of a romantic or personal relationship"
                            },
                            {
                                "target_lang_word": "rupture",
                                "target_lang_sense_clarification": "formal; a breach of relations"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "severing of ties"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cfef30e-a435-3827-a91f-eaff01a28f4d",
                    "sense_definition": "Свободное пространство, промежуток или отверстие между частями чего-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_gap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сквозь <разрыв> в облаках выглянуло яркое солнце.",
                            "target_lang_translations": {
                                "en": "Bright sun peeked through a <break> in the clouds."
                            }
                        },
                        {
                            "text": "Туристы заметили глубокий <разрыв> в скальной породе.",
                            "target_lang_translations": {
                                "en": "The tourists noticed a deep <gap> in the rock formation."
                            }
                        }
                    ],
                    "synonyms": [
                        "промежуток",
                        "просвет"
                    ],
                    "antonyms": [
                        "соединение"
                    ],
                    "common_phrases": [
                        "разрыв в облаках",
                        "разрыв цепи",
                        "разрыв кольца"
                    ],
                    "target_lang_definitions": {
                        "en": "A free space, opening, or hole between parts of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gap",
                                "target_lang_sense_clarification": "space or opening between things"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "opening in clouds or continuity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bcda076-66dd-3b09-90fc-e44d8c9413b8",
                    "sense_definition": "Существенная разница, несоответствие или дистанция между какими-либо показателями, уровнями или явлениями.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_gap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Экономисты отмечают растущий <разрыв> между богатыми и бедными слоями населения.",
                            "target_lang_translations": {
                                "en": "Economists note the growing <gap> between the rich and poor segments of the population."
                            }
                        },
                        {
                            "text": "Нам необходимо сократить <разрыв> в уровне знаний учеников.",
                            "target_lang_translations": {
                                "en": "We need to close the <gap> in the students' level of knowledge."
                            }
                        }
                    ],
                    "synonyms": [
                        "несоответствие"
                    ],
                    "common_phrases": [
                        "разрыв в доходах",
                        "технологический разрыв",
                        "разрыв цен",
                        "сократить разрыв"
                    ],
                    "target_lang_definitions": {
                        "en": "A significant difference, discrepancy, or distance between indicators, levels, or phenomena."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gap",
                                "target_lang_sense_clarification": "disparity between groups, levels, or amounts"
                            },
                            {
                                "target_lang_word": "disparity",
                                "target_lang_sense_clarification": "inequality or difference"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49d35b36-3732-3b18-b82a-2a979053fa97",
                    "sense_definition": "Нарушение целостности предмета, материала или тканей организма под действием силы; повреждение.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_rupture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач диагностировал у спортсмена <разрыв> связок голеностопного сустава.",
                            "target_lang_translations": {
                                "en": "The doctor diagnosed the athlete with a <tear> of the ankle ligaments."
                            }
                        },
                        {
                            "text": "Сильное давление воды вызвало <разрыв> старой трубы в подвале.",
                            "target_lang_translations": {
                                "en": "High water pressure caused the <rupture> of the old pipe in the basement."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрывание",
                        "разлом"
                    ],
                    "antonyms": [
                        "срастание",
                        "соединение"
                    ],
                    "common_phrases": [
                        "разрыв связок",
                        "разрыв сердца",
                        "разрыв линии",
                        "разрыв шаблона"
                    ],
                    "target_lang_definitions": {
                        "en": "A violation of the integrity of an object, material, or body tissue under force; damage."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rupture",
                                "target_lang_sense_clarification": "of organs, pipes, or vessels"
                            },
                            {
                                "target_lang_word": "tear",
                                "target_lang_sense_clarification": "of ligaments, muscles, or tissue"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a77f118-c623-3161-87a2-72b8d85d7644",
                    "sense_definition": "Взрыв боеприпаса (снаряда, мины, бомбы) и звук, сопровождающий это действие.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "explosion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдаты залегли в окопы, услышав громкий <разрыв> снаряда неподалёку.",
                            "target_lang_translations": {
                                "en": "The soldiers lay down in the trenches upon hearing the loud <burst> of a shell nearby."
                            }
                        },
                        {
                            "text": "На месте <разрыва> мины образовалась глубокая воронка.",
                            "target_lang_translations": {
                                "en": "A deep crater formed at the site of the mine <explosion>."
                            }
                        }
                    ],
                    "synonyms": [
                        "взрыв"
                    ],
                    "common_phrases": [
                        "разрыв снаряда",
                        "точка разрыва",
                        "глухой разрыв"
                    ],
                    "target_lang_definitions": {
                        "en": "The explosion of a projectile (shell, mine, bomb) and the sound accompanying this action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "explosion",
                                "target_lang_sense_clarification": "violent release of energy"
                            },
                            {
                                "target_lang_word": "burst",
                                "target_lang_sense_clarification": "of a shell or projectile"
                            }
                        ]
                    }
                }
            ]
        },

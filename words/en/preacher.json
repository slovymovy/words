    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "83aa2d26-7d55-3e16-b53b-b48c6a9f41bd",
                    "sense_definition": "A person who gives religious speeches and leads worship services, often a Christian minister.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>preacher</w> delivered a moving sermon about hope and forgiveness.",
                            "target_lang_translations": {
                                "ru": "<w>Проповедник</w> произнес трогательную проповедь о надежде и прощении."
                            }
                        },
                        {
                            "text": "My grandfather was a traveling <w>preacher</w> who visited many small towns.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был странствующим <w>проповедником</w>, который посещал множество маленьких городков."
                            }
                        }
                    ],
                    "synonyms": [
                        "minister",
                        "pastor",
                        "clergyman",
                        "evangelist"
                    ],
                    "common_phrases": [
                        "street preacher",
                        "gospel preacher",
                        "lay preacher",
                        "itinerant preacher"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который произносит религиозные речи и ведет богослужения, часто христианский священнослужитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проповедник",
                                "target_lang_sense_clarification": "религиозный оратор"
                            }
                        ]
                    }
                }
            ]
        },
        {

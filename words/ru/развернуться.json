      "pos": "verb",
      "senses": [
        {
          "sense_id": "bd6f497c-58b0-376a-ab0c-761c084bddf4",
          "sense_definition": "Изменить направление движения или положение тела на противоположное или иное (о человеке, машине, судне).",
          "learner_level": "A2",
          "frequency": "High",
          "semantic_group_id": "movement_turn",
          "name_type": "no",
          "examples": [
            {
              "text": "Водитель понял ошибку и решил <развернуться> на перекрёстке."
            },
            {
              "text": "Он резко <развернулся> и пошёл в другую сторону."
            }
          ],
          "synonyms": [
            "повернуться",
            "повернуть"
          ],
          "common_phrases": [
            "резко развернуться",
            "развернуться на месте",
            "развернуться к окну"
          ]
        },
        {
          "sense_id": "f4fc36a7-a68d-3a23-9e0e-c20fd51e2250",
          "sense_definition": "Раскрыться, стать плоским или расправленным (о чём-то свёрнутом, сложенном или скатанном).",
          "learner_level": "B1",
          "frequency": "Middle",
          "semantic_group_id": "physical_unfolding",
          "name_type": "no",
          "examples": [
            {
              "text": "Старый ковёр <развернулся> на полу во всю длину."
            },
            {
              "text": "Парашют успешно <развернулся> в небе."
            }
          ],
          "synonyms": [
            "раскрыться",
            "открыться",
            "распаковаться"
          ],
          "common_phrases": [
            "знамя развернулось",
            "ковёр развернулся",
            "парашют развернулся"
          ]
        },
        {
          "sense_id": "a4d244e4-b685-3856-a7cc-875878adbe4a",
          "sense_definition": "Начать происходить широко, активно развиваться или увеличиваться в масштабах (о событиях, деятельности, строительстве).",
          "learner_level": "B2",
          "frequency": "Middle",
          "semantic_group_id": "abstract_expansion",
          "name_type": "no",
          "examples": [
            {
              "text": "В городе <развернулось> масштабное строительство новых домов."
            },
            {
              "text": "Между коллегами <развернулась> жаркая дискуссия по этому вопросу."
            }
          ],
          "synonyms": [
            "расшириться",
            "разрастись"
          ],
          "common_phrases": [
            "строительство развернулось",
            "дискуссия развернулась",
            "события развернулись"
          ],
          "traits": []
        },
        {
          "sense_id": "e8ac1433-4201-343b-830d-e95e1a943f30",
          "sense_definition": "Ослабиться от вращения, перестать быть плотно завинченным (о гайке, винте).",
          "learner_level": "B2",
          "frequency": "Low",
          "semantic_group_id": "mechanical",
          "name_type": "no",
          "examples": [
            {
              "text": "От сильной вибрации винт <развернулся> и упал на пол."
            },
            {
              "text": "Гайка <развернулась>, и колесо начало шататься."
            }

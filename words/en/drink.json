                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "smoking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In old texts, you might read that men <w>drank</w> tobacco in their pipes.",
                            "target_lang_translations": {
                                "ru": "В старых текстах можно прочитать, что мужчины <w>курили</w> табак в трубках."
                            }
                        },
                        {
                            "text": "He sat by the fire and <w>drank</w> smoke from his clay pipe.",
                            "target_lang_translations": {
                                "ru": "Он сидел у огня и <w>втягивал</w> дым из своей глиняной трубки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Курить табак (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курить",
                                "target_lang_sense_clarification": "вдыхать дым"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "46c147a8-61a3-304b-918f-4e1d3c8f7045",
                    "sense_definition": "The direction or area located halfway between south and east.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "compass_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy rain is moving in from the <w>southeast</w>.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь надвигается с <w>юго-востока</w>."
                            }
                        },
                        {
                            "text": "They live in a small town in the <w>southeast</w>.",
                            "target_lang_translations": {
                                "ru": "Они живут в маленьком городке на <w>юго-востоке</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "northwest"
                    ],
                    "common_phrases": [
                        "in the southeast",
                        "to the southeast",
                        "from the southeast"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направление или область, расположенная посередине между югом и востоком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юго-восток",
                                "target_lang_sense_clarification": "сторона света или регион"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1b01d421-e4b7-34ca-bbf0-ac00b95153d2",
                    "sense_definition": "Situated in, directed toward, or relating to the southeast.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "direction_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We explored the <w>southeast</w> coast of the island during our holiday.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска мы исследовали <w>юго-восточное</w> побережье острова."
                            }
                        },
                        {
                            "text": "The <w>southeast</w> corner of the garden gets the most sunlight.",
                            "target_lang_translations": {
                                "ru": "<w>Юго-восточный</w> угол сада получает больше всего солнечного света."
                            }
                        }
                    ],
                    "synonyms": [
                        "southeastern"
                    ],
                    "antonyms": [
                        "northwest"
                    ],
                    "common_phrases": [
                        "southeast coast",
                        "southeast corner",
                        "southeast Asia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный на юго-востоке, направленный на юго-восток или относящийся к нему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юго-восточный",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf13c7aa-5e13-3aaa-8a84-c3ed1c7776e4",
                    "sense_definition": "Coming from the southeast, usually describing wind.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "wind_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A cold <w>southeast</w> wind blew across the fields all day.",
                            "target_lang_translations": {
                                "ru": "Весь день над полями дул холодный <w>юго-восточный</w> ветер."
                            }
                        },
                        {
                            "text": "The sailors waited for a favorable <w>southeast</w> breeze.",
                            "target_lang_translations": {
                                "ru": "Моряки ждали попутного <w>юго-восточного</w> бриза."
                            }
                        }
                    ],
                    "synonyms": [
                        "southeasterly"
                    ],
                    "antonyms": [
                        "northwest"
                    ],
                    "common_phrases": [
                        "southeast wind",
                        "southeast breeze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дующий с юго-востока (обычно о ветре)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юго-восточный",
                                "target_lang_sense_clarification": "о ветре"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "55a15075-9189-3148-9d3a-ac6f9b8f7359",
                    "sense_definition": "Towards or in the direction of the southeast.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "direction_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to drive <w>southeast</w> for about two hours to reach the coast.",
                            "target_lang_translations": {
                                "ru": "Нам нужно ехать <w>на юго-восток</w> около двух часов, чтобы добраться до побережья."
                            }
                        },
                        {
                            "text": "The storm is currently moving <w>southeast</w> across the state.",
                            "target_lang_translations": {
                                "ru": "Шторм сейчас движется <w>на юго-восток</w> через штат."
                            }
                        }
                    ],
                    "synonyms": [
                        "southeastward"
                    ],
                    "antonyms": [
                        "northwest"
                    ],
                    "common_phrases": [
                        "travel southeast",
                        "move southeast",
                        "head southeast"
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении юго-востока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на юго-восток",
                                "target_lang_sense_clarification": "направление движения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

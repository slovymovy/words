    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a3a4188d-88d3-3b24-8a2c-14d55246261c",
                    "sense_definition": "Достать, извлечь что-либо или кого-либо откуда-нибудь, часто прилагая физическое усилие.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я сунул руку в карман и <w>вытащил</w> ключи от машины.",
                            "target_lang_translations": {
                                "en": "I put my hand in my pocket and <w>pulled out</w> the car keys."
                            }
                        },
                        {
                            "text": "Спасателям удалось быстро <w>вытащить</w> пострадавшего из воды.",
                            "target_lang_translations": {
                                "en": "Rescuers managed to quickly <w>pull</w> the victim <w>out</w> of the water."
                            }
                        },
                        {
                            "text": "Отец попросил меня спуститься в погреб и <w>вытащить</w> банку с огурцами.",
                            "target_lang_translations": {
                                "en": "Father asked me to go down to the cellar and <w>get out</w> a jar of pickles."
                            }
                        }
                    ],
                    "synonyms": [
                        "вытянуть"
                    ],
                    "antonyms": [
                        "вставить",
                        "втащить",
                        "втянуть"
                    ],
                    "common_phrases": [
                        "вытащить из кармана",
                        "вытащить из воды",
                        "вытащить занозу",
                        "вытащить наружу"
                    ],
                    "target_lang_definitions": {
                        "en": "To get or extract something or someone from somewhere, often applying physical effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pull out",
                                "target_lang_sense_clarification": "general physical extraction"
                            },
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "general removal from a container or place"
                            },
                            {
                                "target_lang_word": "drag out",
                                "target_lang_sense_clarification": "extraction requiring significant effort or force"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db04cdce-a3ca-3ecf-a66a-32acc78678ec",
                    "sense_definition": "Уговорить или заставить кого-либо пойти куда-нибудь, покинуть помещение (часто против желания).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В выходные друзья с трудом <w>вытащили</w> меня из дома на прогулку.",
                            "target_lang_translations": {
                                "en": "Over the weekend, friends barely managed to <w>drag</w> me <w>out</w> of the house for a walk."
                            }
                        },
                        {
                            "text": "Его невозможно <w>вытащить</w> в театр, он предпочитает сидеть дома.",
                            "target_lang_translations": {
                                "en": "It is impossible to <w>drag</w> him <w>out</w> to the theater; he prefers to stay at home."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вытащить на улицу",
                        "вытащить гулять",
                        "вытащить в кино",
                        "вытащить из дома"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies effort in persuasion"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To persuade or force someone to go somewhere or leave a place (often against their will)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag out",
                                "target_lang_sense_clarification": "persuade someone to go out or leave home"
                            },
                            {
                                "target_lang_word": "get out",
                                "target_lang_sense_clarification": "convince someone to leave a place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52ee5efe-aa48-3e7b-be3f-bff8ef71df26",
                    "sense_definition": "Помочь кому-либо избавиться от неприятностей, спасти из трудного положения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_help",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачам чудом удалось <w>вытащить</w> пациента с того света.",
                            "target_lang_translations": {
                                "en": "Doctors miraculously managed to <w>pull</w> the patient <w>back</w> from the brink of death."
                            }
                        },
                        {
                            "text": "Брат помог мне деньгами и <w>вытащил</w> из долговой ямы.",
                            "target_lang_translations": {
                                "en": "My brother helped me with money and <w>pulled</w> me <w>out</w> of the debt hole."
                            }
                        }
                    ],
                    "synonyms": [
                        "вытянуть"
                    ],
                    "antonyms": [
                        "втянуть"
                    ],
                    "common_phrases": [
                        "вытащить из беды",
                        "вытащить с того света",
                        "вытащить из долгов",
                        "вытащить из кризиса"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To help someone get rid of troubles, to save from a difficult situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pull out",
                                "target_lang_sense_clarification": "save from a difficult situation or trouble"
                            },
                            {
                                "target_lang_word": "bail out",
                                "target_lang_sense_clarification": "rescue from financial trouble or difficulty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ebcbdb1-2423-35dd-abb3-c3d24cdd0262",
                    "sense_definition": "Совершить кражу, незаметно украсть что-либо (обычно из кармана или сумки).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "theft",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В переполненном автобусе у туриста <w>вытащили</w> кошелёк.",
                            "target_lang_translations": {
                                "en": "In a crowded bus, someone <w>stole</w> a tourist's wallet."
                            }
                        },
                        {
                            "text": "Карманник ловко <w>вытащил</w> телефон из куртки прохожего.",
                            "target_lang_translations": {
                                "en": "The pickpocket deftly <w>lifted</w> the phone from the passerby's jacket."
                            }
                        }
                    ],
                    "synonyms": [
                        "украсть",
                        "стащить",
                        "стянуть"
                    ],
                    "common_phrases": [
                        "вытащить кошелёк",
                        "вытащить деньги",
                        "вытащить паспорт"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To commit theft, to steal something unnoticed (usually from a pocket or bag)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "steal",
                                "target_lang_sense_clarification": "general term for taking something illegally"
                            },
                            {
                                "target_lang_word": "lift",
                                "target_lang_sense_clarification": "slang, specifically stealing from pockets or shoplifting"
                            },
                            {
                                "target_lang_word": "pinch",
                                "target_lang_sense_clarification": "informal/slang for stealing"
                            }
                        ]
                    }
                }
            ]
        }
    ]

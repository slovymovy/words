    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5ac7eb99-57ac-3312-a480-0ed3d9112a89",
                    "sense_definition": "Een vloeistof die een dunne, harde en vaak glanzende beschermlaag vormt na aanbrenging op materialen zoals hout of metaal.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schilder bracht een nieuwe laag <w>lak</w> aan op de houten kozijnen.",
                            "target_lang_translations": {
                                "ru": "Маляр нанес новый слой <w>лака</w> на деревянные рамы."
                            }
                        },
                        {
                            "text": "Zonder goede <w>lak</w> kan het metaal door de regen gaan roesten.",
                            "target_lang_translations": {
                                "ru": "Без хорошего <w>лака</w> металл может заржаветь от дождя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de lak",
                        "een laag lak",
                        "strak in de lak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкость, образующая тонкий, твердый и часто блестящий защитный слой после нанесения на такие материалы, как дерево или металл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лак",
                                "target_lang_sense_clarification": "покрытие для дерева, металла, ногтей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4aa9045a-a815-3676-a763-29eb4a19c022",
                    "sense_definition": "Een gevoel van totale onverschilligheid; het zich ergens niets van aantrekken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft overal <w>lak</w> aan en doet gewoon wat hij zelf wil.",
                            "target_lang_translations": {
                                "ru": "Ему на всё <w>наплевать</w>, и он просто делает то, что хочет."
                            }
                        },
                        {
                            "text": "Ik heb er <w>lak</w> aan wat de buren van mijn nieuwe auto vinden.",
                            "target_lang_translations": {
                                "ru": "Мне <w>плевать</w> на то, что соседи думают о моей новой машине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lak hebben aan"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "vooral in de vaste uitdrukking"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство полного безразличия; игнорирование чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наплевать",
                                "target_lang_sense_clarification": "разговорное, в выражении 'lak hebben aan'"
                            },
                            {
                                "target_lang_word": "безразличие",
                                "target_lang_sense_clarification": "чувство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10d9634e-ba74-3e3a-9b15-1eb0e6f4e74b",
                    "sense_definition": "Een tekortkoming, fout, smet of iets wat onwaar is.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "defect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat hele verhaal is klinkklare <w>lak</w> en dient niet serieus genomen te worden.",
                            "target_lang_translations": {
                                "ru": "Вся эта история — чистый <w>вздор</w>, и её не следует воспринимать всерьез."
                            }
                        },
                        {
                            "text": "In zijn betoog zat geen enkele <w>lak</w>; het was volkomen waar.",
                            "target_lang_translations": {
                                "ru": "В его рассуждениях не было ни единого <w>изъяна</w>; всё было совершенно верно."
                            }
                        }
                    ],
                    "synonyms": [
                        "gebrek"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "wordt in het modern Nederlands zelden gebruikt"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Недостаток, ошибка, пятно или что-то не соответствующее истине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изъян",
                                "target_lang_sense_clarification": "недостаток"
                            },
                            {
                                "target_lang_word": "вздор",
                                "target_lang_sense_clarification": "чепуха, ложь"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5535e9ac-b24c-353f-98a6-59e8493ea439",
                    "sense_definition": "Vervoeging van <w>lakken</w>: eerste persoon enkelvoud, gebiedende wijs of tweede persoon enkelvoud (bij inversie).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>lak</w> mijn nagels rood voor het feestje vanavond.",
                            "target_lang_translations": {
                                "ru": "Я <w>крашу</w> ногти в красный цвет для сегодняшней вечеринки."
                            }
                        },
                        {
                            "text": "<w>Lak</w> de tafel goed, zodat er geen kringen op komen.",
                            "target_lang_translations": {
                                "ru": "Хорошо <w>покройте лаком</w> стол, чтобы на нем не оставалось кругов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола <w>lakken</w>: первое лицо единственного числа, повелительное наклонение или второе лицо единственного числа (при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красить",
                                "target_lang_sense_clarification": "ногти"
                            },
                            {
                                "target_lang_word": "покрывать лаком",
                                "target_lang_sense_clarification": "поверхность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9b54f2ea-d877-3ca9-8f92-b511e80f20ef",
                    "sense_definition": "Een Noord-Oost-Kaukasische taal die gesproken wordt in de Russische republiek Dagestan.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Het <w>Lak</w> heeft een ingewikkelde grammatica en wordt door duizenden mensen gesproken.",
                            "target_lang_translations": {
                                "ru": "<w>Лакский язык</w> имеет сложную грамматику, и на нем говорят тысячи людей."
                            }
                        },
                        {
                            "text": "In sommige dorpen in de bergen is het <w>Lak</w> de voertaal.",
                            "target_lang_translations": {
                                "ru": "В некоторых горных селах <w>лакский язык</w> является языком общения."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Северо-восточный кавказский язык, на котором говорят в российской республике Дагестан."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лакский язык",
                                "target_lang_sense_clarification": "язык"
                            }
                        ]
                    }
                }
            ]
        }
    ]

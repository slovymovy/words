                    "common_phrases": [
                        "река Дон",
                        "на Дону",
                        "Тихий Дон"
                    ],
                    "target_lang_definitions": {
                        "en": "A major river in the European part of Russia that flows into the Sea of Azov."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Don",
                                "target_lang_sense_clarification": "river name"
                            }
                        ]

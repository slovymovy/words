                {
                    "sense_id": "54cab21e-f161-3134-b3bf-1340a878daf2",
                    "sense_definition": "Dzieci w stosunku do swoich rodziców; ogół synów i córek danej pary.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "offspring_human",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młode małżeństwo planuje wkrótce powiększyć rodzinę i mieć <w>potomstwo</w>.",
                            "target_lang_translations": {
                                "ru": "Молодая супружеская пара планирует вскоре увеличить семью и завести <w>потомство</w>."
                            }
                        },
                        {
                            "text": "Jego liczne <w>potomstwo</w> zjechało się na uroczystość z całego kraju.",
                            "target_lang_translations": {
                                "ru": "Его многочисленное <w>потомство</w> съехалось на торжество со всей страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "dzieci",
                        "progenitura",
                        "dziatki"
                    ],
                    "antonyms": [
                        "rodzice",
                        "przodkowie"
                    ],
                    "common_phrases": [
                        "liczne potomstwo",
                        "doczekać się potomstwa",
                        "mieć potomstwo",
                        "bez potomstwa"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "collective noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дети по отношению к своим родителям; сыновья и дочери одной пары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потомство",
                                "target_lang_sense_clarification": "дети, молодое поколение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b2cf1fc-804c-3edb-ac54-7f2eeba1bb4f",
                    "sense_definition": "Młode osobniki u zwierząt, które przyszły na świat od jednej matki lub pary rodzicielskiej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "offspring_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samica niedźwiedzia zaciekle broni swojego <w>potomstwa</w> przed drapieżnikami.",
                            "target_lang_translations": {
                                "ru": "Медведица яростно защищает своё <w>потомство</w> от хищников."
                            }
                        },
                        {
                            "text": "Czy zebra i osioł mogą się sparować i uzyskać <w>potomstwo</w>?",
                            "target_lang_translations": {
                                "ru": "Могут ли зебра и осёл спариться и дать <w>потомство</w>?"
                            }
                        }
                    ],
                    "synonyms": [

                {
                    "sense_id": "006e5c44-e30b-3329-8aca-32e74002c027",
                    "sense_definition": "De opening in een molen of gemaal waardoor het water naar buiten wordt gestuwd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij hoogwater stroomt het water met veel geweld door het <w>uitschot</w>.",
                            "target_lang_translations": {
                                "en": "At high water, the water flows with great force through the <w>outlet</w>.",
                                "ru": "В половодье вода с огромной силой течет через <w>водоспуск</w>."
                            }

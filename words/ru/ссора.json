    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aee7f5a9-266e-3c89-9a70-223f03e2b379",
                    "sense_definition": "Словесный конфликт, перебранка, выяснение отношений на повышенных тонах.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "conflict_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за глупой <w>ссоры</w> мы опоздали на последний автобус.",
                            "target_lang_translations": {
                                "en": "Because of a silly <w>quarrel</w> we missed the last bus."
                            }
                        },
                        {
                            "text": "Громкая семейная <w>ссора</w> разбудила соседей посреди ночи.",
                            "target_lang_translations": {
                                "en": "A loud family <w>argument</w> woke the neighbors in the middle of the night."
                            }
                        }
                    ],
                    "synonyms": [
                        "склока",
                        "свара",
                        "ругачка"
                    ],
                    "antonyms": [
                        "мир"
                    ],
                    "common_phrases": [
                        "семейная ссора",
                        "крупная ссора",
                        "затеять ссору",
                        "ссора из-за пустяков"
                    ],
                    "target_lang_definitions": {
                        "en": "A verbal conflict, heated argument, or squabble between people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quarrel",
                                "target_lang_sense_clarification": "A heated argument or disagreement, typically about a trivial issue"
                            },
                            {
                                "target_lang_word": "argument",
                                "target_lang_sense_clarification": "A verbal exchange of diverging or opposite views"
                            },
                            {
                                "target_lang_word": "fight",
                                "target_lang_sense_clarification": "A verbal disagreement or dispute (colloquial)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "666b989c-2f55-3384-84f6-4bd96a6d00cc",
                    "sense_definition": "Состояние вражды, прекращение дружеских отношений между людьми в результате конфликта.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "conflict_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша долгая <w>ссора</w> наконец закончилась примирением.",
                            "target_lang_translations": {
                                "en": "Our long <w>quarrel</w> finally ended in reconciliation."
                            }
                        },
                        {
                            "text": "Я не хочу быть в <w>ссоре</w> с лучшим другом.",
                            "target_lang_translations": {
                                "en": "I don't want to have a <w>falling-out</w> with my best friend."
                            }
                        }
                    ],
                    "synonyms": [
                        "раздор",
                        "распря"
                    ],
                    "antonyms": [
                        "мир",
                        "дружба"
                    ],
                    "common_phrases": [
                        "быть в ссоре",
                        "забыть ссору",
                        "причина ссоры"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of hostility or a break in friendly relations resulting from a conflict."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "falling-out",
                                "target_lang_sense_clarification": "A disagreement that results in people no longer speaking to each other"
                            },
                            {
                                "target_lang_word": "strife",
                                "target_lang_sense_clarification": "Bitter conflict or discord between people or groups"
                            },
                            {
                                "target_lang_word": "discord",
                                "target_lang_sense_clarification": "Disagreement and lack of harmony"
                            }
                        ]
                    }
                }
            ]
        }
    ]

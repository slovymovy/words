                },
                {
                    "sense_id": "129d34cb-a207-3130-a790-0cc469e8a18b",
                    "sense_definition": "Brak regularności lub zakłócenie rytmu w przebiegu zjawisk, procesów lub w sztuce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "general_irregularity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiersz ten cechuje wyraźna <w>arytmia</w>, która ma na celu wywołanie niepokoju u czytelnika.",
                            "target_lang_translations": {
                                "ru": "Этому стихотворению свойственна явная <w>аритмия</w>, цель которой — вызвать беспокойство у читателя."
                            }
                        },
                        {
                            "text": "Nagła <w>arytmia</w> oddechu sprawiła, że biegacz musiał się na chwilę zatrzymać.",
                            "target_lang_translations": {
                                "ru": "Внезапная <w>аритмия</w> дыхания заставила бегуна на мгновение остановиться."
                            }
                        }
                    ],

                },
                {
                    "sense_id": "74354005-2236-36df-a8b7-205717a06804",
                    "sense_definition": "De geografische positie van een plaats ten oosten of westen van de nulmeridiaan.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kapitein berekende de exacte <w>lengte</w> en breedte van het schip.",
                            "target_lang_translations": {
                                "en": "The captain calculated the exact <w>longitude</w> and latitude of the ship.",
                                "ru": "Капитан вычислил точную <w>долготу</w> и широту корабля."
                            }

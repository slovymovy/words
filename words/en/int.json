    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b534ec3-c9ec-3e31-9b10-a0c65f7f1915",
                    "sense_definition": "A data type representing a whole number; short for <w>integer</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The function requires an <w>int</w> as a parameter.",
                            "target_lang_translations": {
                                "ru": "Функция требует <w>целое число</w> в качестве параметра."
                            }
                        },
                        {
                            "text": "You cannot store a decimal value in an <w>int</w> variable.",
                            "target_lang_translations": {
                                "ru": "Вы не можете сохранить десятичное значение в <w>целочисленной</w> переменной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "int variable",
                        "return an int"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "clipping of integer, used in computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип данных, представляющий целое число; сокращение от integer."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целое число",
                                "target_lang_sense_clarification": "математика / программирование"
                            },
                            {
                                "target_lang_word": "целочисленный тип",
                                "target_lang_sense_clarification": "программирование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ba57f44-7db6-39b6-b99a-dea3d914339c",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e3337431-0f53-31ae-96de-40e30bd92d4d",
                    "sense_definition": "Выдавать денежные средства в качестве платы за что-либо (зарплату, пенсию, пособие, гонорар).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "payment_giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Работодатель обязан вовремя <w>выплачивать</w> заработную плату всем сотрудникам.",
                            "target_lang_translations": {
                                "en": "The employer is obliged to <w>pay</w> wages to all employees on time."
                            }
                        },
                        {
                            "text": "Ежемесячно ему <w>выплачивали</w> небольшую премию за отличные результаты работы.",
                            "target_lang_translations": {
                                "en": "Every month they <w>paid</w> him a small bonus for excellent work results."
                            }
                        }
                    ],
                    "synonyms": [
                        "платить"
                    ],
                    "common_phrases": [
                        "выплачивать зарплату",
                        "выплачивать пенсию",
                        "выплачивать пособие",
                        "выплачивать компенсацию"
                    ],
                    "target_lang_definitions": {
                        "en": "To give money as payment for work, services, or benefits (salary, pension, allowance)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "give money for work or services"
                            },
                            {
                                "target_lang_word": "pay out",
                                "target_lang_sense_clarification": "disburse funds, especially benefits or large sums"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2d69ca0-5ad9-38ff-a689-6959b46f8c34",
                    "sense_definition": "Возвращать долг или вносить плату за что-либо частями до полного погашения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "payment_repaying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодая семья будет <w>выплачивать</w> ипотеку за квартиру в течение двадцати лет.",
                            "target_lang_translations": {
                                "en": "The young family will be <w>paying off</w> the mortgage for the apartment for twenty years."
                            }
                        },
                        {
                            "text": "Магазин предложил нам <w>выплачивать</w> стоимость новой мебели в рассрочку.",
                            "target_lang_translations": {
                                "en": "The store offered to let us <w>pay off</w> the cost of the new furniture in installments."
                            }
                        }
                    ],
                    "synonyms": [
                        "платить",
                        "платить"
                    ],
                    "common_phrases": [
                        "выплачивать кредит",
                        "выплачивать долг",
                        "выплачивать ипотеку",
                        "выплачивать в рассрочку"
                    ],
                    "target_lang_definitions": {
                        "en": "To return a debt or pay for something in installments until fully settled."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay off",
                                "target_lang_sense_clarification": "complete the payment of a debt or loan"
                            },
                            {
                                "target_lang_word": "repay",
                                "target_lang_sense_clarification": "return borrowed money"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            {
                                "target_lang_word": "как и",
                                "target_lang_sense_clarification": "сравнение"
                            },
                            {
                                "target_lang_word": "так же, как и",
                                "target_lang_sense_clarification": "усиленное сравнение"
                            },
                            {

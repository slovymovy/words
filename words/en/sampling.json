    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "79962116-44bb-3c83-8e1b-e031d6b3650f",
                    "sense_definition": "The activity of selecting a representative part of a population or group for analysis.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Random <w>sampling</w> is necessary to ensure the survey results are accurate.",
                            "target_lang_translations": {
                                "ru": "Случайная <w>выборка</w> необходима для обеспечения точности результатов опроса."
                            }
                        },
                        {
                            "text": "The scientists conducted extensive water <w>sampling</w> in the river to check for pollution.",
                            "target_lang_translations": {
                                "ru": "Ученые провели обширный <w>отбор проб</w> воды в реке, чтобы проверить ее на наличие загрязнений."
                            }
                        }
                    ],
                    "synonyms": [
                        "selection",
                        "testing"
                    ],
                    "common_phrases": [
                        "random sampling",
                        "data sampling",
                        "soil sampling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс отбора представительной части популяции или группы для анализа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выборка",
                                "target_lang_sense_clarification": "статистика"
                            },
                            {
                                "target_lang_word": "отбор проб",
                                "target_lang_sense_clarification": "взятие образцов вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59293136-5aad-3fbf-9f9c-d38c6ebb933a",
                    "sense_definition": "The technique of using a portion of a previously recorded sound or music in a new musical composition.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hip-hop producers revolutionized music production through the creative <w>sampling</w> of old funk records.",
                            "target_lang_translations": {
                                "ru": "Хип-хоп продюсеры произвели революцию в создании музыки благодаря творческому <w>семплированию</w> старых фанк-пластинок."
                            }
                        },
                        {
                            "text": "There are strict copyright laws that regulate digital <w>sampling</w> in the music industry.",
                            "target_lang_translations": {
                                "ru": "Существуют строгие законы об авторском праве, регулирующие цифровой <w>сэмплинг</w> в музыкальной индустрии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "digital sampling"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "music"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Техника использования фрагмента ранее записанного звука или музыки в новой музыкальной композиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "семплирование",
                                "target_lang_sense_clarification": "создание музыки"
                            },
                            {
                                "target_lang_word": "сэмплинг",
                                "target_lang_sense_clarification": "заимствование звука"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd19164c-8152-3048-86b3-c876b8c55c8f",
                    "sense_definition": "A collection of examples or a small amount of something chosen to show what the rest is like.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gallery displayed a <w>sampling</w> of the artist's early sketches.",
                            "target_lang_translations": {
                                "ru": "В галерее была представлена <w>подборка</w> ранних эскизов художника."
                            }
                        },
                        {
                            "text": "Guests were offered a delicious <w>sampling</w> of local cheeses and wines.",
                            "target_lang_translations": {
                                "ru": "Гостям предложили вкусную <w>дегустацию</w> местных сыров и вин."
                            }
                        }
                    ],
                    "synonyms": [
                        "selection",
                        "assortment",
                        "cross-section"
                    ],
                    "common_phrases": [
                        "a sampling of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коллекция примеров или небольшое количество чего-либо, выбранное для демонстрации качества остального."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подборка",
                                "target_lang_sense_clarification": "коллекция примеров"
                            },
                            {
                                "target_lang_word": "образцы",
                                "target_lang_sense_clarification": "экземпляры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbec6cc4-203e-33d3-b539-d3a939d107b1",
                    "sense_definition": "The process of measuring the strength of a signal at regular intervals to convert it into digital form.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Higher rates of <w>sampling</w> allow for better audio quality in digital recordings.",
                            "target_lang_translations": {
                                "ru": "Более высокая частота <w>дискретизации</w> обеспечивает лучшее качество звука в цифровых записях."
                            }
                        },
                        {
                            "text": "The engineer explained how signal <w>sampling</w> works in modern telecommunications.",
                            "target_lang_translations": {
                                "ru": "Инженер объяснил, как работает <w>дискретизация</w> сигналов в современных телекоммуникациях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sampling rate",
                        "sampling frequency"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс измерения силы сигнала через равные промежутки времени для преобразования его в цифровую форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дискретизация",
                                "target_lang_sense_clarification": "обработка сигналов"
                            }
                        ]
                    }
                }
            ]
        },

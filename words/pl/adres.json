                },
                {
                    "sense_id": "69afeca8-61c2-30c8-93a2-58ec8742554e",
                    "sense_definition": "Dawne określenie na spryt, zręczność lub umiejętność radzenia sobie w życiu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trzeba mieć nie lada <w>adres</w>, aby wyjść cało z takiej opresji.",
                            "target_lang_translations": {
                                "ru": "Нужно иметь немалую <w>смекалку</w>, чтобы выбраться целым из такой переделки."
                            }
                        },
                        {
                            "text": "Jego towarzyski <w>adres</w> pozwalał mu zjednywać sobie wpływowych ludzi.",
                            "target_lang_translations": {
                                "ru": "Его светская <w>ловкость</w> позволяла ему располагать к себе влиятельных людей."
                            }

                    "sense_id": "6ef186a2-7722-3c80-835a-860c559b657f",
                    "sense_definition": "An informal word for thanks.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_thanks",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Here is your coffee, love. — <w>Ta</w>!",
                            "target_lang_translations": {
                                "ru": "Вот твой кофе, милая. — <w>Спасибо</w>!"
                            }
                        },
                        {
                            "text": "<w>Ta</w> for helping me carry these heavy bags.",
                            "target_lang_translations": {
                                "ru": "<w>Спасибо</w>, что помог мне донести эти тяжелые сумки."
                            }
                        }
                    ],
                    "synonyms": [
                        "te",
                        "B-flat"
                    ],
                    "common_phrases": [

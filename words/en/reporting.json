            "pos": "noun",
            "senses": [
                {
                    "sense_id": "55402962-fa1f-3a47-aa39-6b0a90de4a39",
                    "sense_definition": "The act or process of creating and presenting formal statements or documents, especially in business or administration.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_reporting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new software simplifies the <w>reporting</w> of monthly sales figures.",
                            "target_lang_translations": {
                                "ru": "Новое программное обеспечение упрощает <w>составление отчётов</w> о ежемесячных показателях продаж."
                            }
                        },
                        {
                            "text": "Strict financial <w>reporting</w> is required for all public companies to ensure transparency.",
                            "target_lang_translations": {
                                "ru": "Для обеспечения прозрачности от всех публичных компаний требуется строгая финансовая <w>отчётность</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "documentation",
                        "accounting"
                    ],
                    "common_phrases": [
                        "financial reporting",
                        "reporting system",
                        "reporting period",
                        "reporting requirements"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс создания и представления официальных заявлений или документов, особенно в бизнесе или администрации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отчётность",
                                "target_lang_sense_clarification": "финансовая или деловая документация"
                            },
                            {
                                "target_lang_word": "составление отчётов",
                                "target_lang_sense_clarification": "процесс подготовки документов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92e2596d-041b-38f6-9dfa-eb1c638809ff",
                    "sense_definition": "The activity of gathering and presenting news for newspapers, radio, or television.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "journalism_reporting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her investigative <w>reporting</w> exposed corruption in the local government.",
                            "target_lang_translations": {
                                "ru": "Её расследовательские <w>репортажи</w> разоблачили коррупцию в местном правительстве."
                            }
                        },
                        {
                            "text": "We rely on accurate news <w>reporting</w> to understand complex current events.",
                            "target_lang_translations": {
                                "ru": "Мы полагаемся на точное новостное <w>освещение</w>, чтобы понимать сложные текущие события."
                            }
                        }
                    ],
                    "synonyms": [
                        "journalism",
                        "coverage",
                        "newsgathering"
                    ],
                    "common_phrases": [
                        "investigative reporting",
                        "news reporting",
                        "factual reporting",
                        "live reporting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность по сбору и представлению новостей для газет, радио или телевидения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репортаж",
                                "target_lang_sense_clarification": "журналистский материал"
                            },
                            {
                                "target_lang_word": "освещение",
                                "target_lang_sense_clarification": "подача новостей или событий"
                            }
                        ]
                    }
                }
            ]
        },

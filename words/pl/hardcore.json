                },
                {
                    "sense_id": "a21e8411-957a-31a5-b1b8-71eb82b88091",
                    "sense_definition": "Nielegalne tworzenie graffiti w miejscach trudno dostępnych, strzeżonych lub niebezpiecznych.",
                    "learner_level": "C2",

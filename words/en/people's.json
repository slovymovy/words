{
    "word_family": [
        "people",
        "person",
        "peopled"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "df6f06d2-65ea-3ff1-9552-7ea862f7591f",
                    "sense_definition": "The possessive form of <w>people</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must always respect other <people's> feelings and opinions.",
                            "target_lang_translations": {
                                "ru": "Мы всегда должны уважать чувства и мнения других <людей>."
                            }
                        },
                        {
                            "text": "The <people's> decision to elect a new leader was final.",
                            "target_lang_translations": {
                                "ru": "Решение <народа> избрать нового лидера было окончательным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "other people's money",
                        "the people's choice",
                        "people's rights"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "possessive case"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Притяжательная форма слова \"people\" (люди или народ)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "людей",
                                "target_lang_sense_clarification": "родительный падеж от \"люди\""
                            },
                            {
                                "target_lang_word": "народа",
                                "target_lang_sense_clarification": "родительный падеж от \"народ\""
                            },
                            {
                                "target_lang_word": "чужой",
                                "target_lang_sense_clarification": "принадлежащий другим людям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03a4c5d4-7f53-3e91-9d09-454a526d9c8b",
                    "sense_definition": "Considered to be of, from, or in tune with the common people, rather than from a ruling elite.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was widely loved and often referred to as the <people's> princess.",
                            "target_lang_translations": {
                                "ru": "Её очень любили и часто называли <народной> принцессой."
                            }
                        },
                        {
                            "text": "This new policy is a <people's> initiative to improve local parks.",
                            "target_lang_translations": {
                                "ru": "Эта новая политика — <народная> инициатива по улучшению местных парков."
                            }
                        }
                    ],
                    "synonyms": [
                        "popular",
                        "populist",
                        "grassroots",
                        "democratic"
                    ],
                    "antonyms": [
                        "elite",
                        "aristocratic"
                    ],
                    "common_phrases": [
                        "the people's princess",
                        "a people's vote",
                        "people's champion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считающийся исходящим от простого народа или близким ему, в отличие от правящей элиты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "народный",
                                "target_lang_sense_clarification": "относящийся к простому народу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b51420a3-abd7-3ba1-8a20-f44605e0a09e",
                    "sense_definition": "Relating to political systems, especially in socialist states, that claim to be governed by the ordinary population.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <People's> Republic was established after the revolution ended.",
                            "target_lang_translations": {
                                "ru": "<Народная> Республика была провозглашена после окончания революции."
                            }
                        },
                        {
                            "text": "He was awarded the title of <People's> Artist for his contributions to culture.",
                            "target_lang_translations": {
                                "ru": "Ему было присвоено звание <Народного> артиста за вклад в культуру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "People's Republic",
                        "People's Army",
                        "People's Party"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к политическим системам, особенно в социалистических странах, провозглашающих власть народа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "народный",
                                "target_lang_sense_clarification": "в официальных названиях и титулах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1ec8a19-cd6b-35b6-97ed-f654515aa104",
                    "sense_definition": "Used ironically to label a state or organization as authoritarian or totalitarian despite its claims.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Employees jokingly called their strict workplace the <People's> Republic of the Office.",
                            "target_lang_translations": {
                                "ru": "Сотрудники в шутку называли своё строгое рабочее место <Народной> Республикой Офиса."
                            }
                        },
                        {
                            "text": "He mocked the heavy regulations by referring to the town as the <People's> United Kingdom.",
                            "target_lang_translations": {
                                "ru": "Он высмеивал жесткие правила, называя город <Народным> Соединенным Королевством."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется иронически для обозначения государства или организации как авторитарных, вопреки их заявлениям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "народный",
                                "target_lang_sense_clarification": "ироническое использование"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
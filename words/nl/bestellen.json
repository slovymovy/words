                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заказывать",
                                "target_lang_sense_clarification": "делать заказ (товаров, еды, услуг)"
                            },
                            {
                                "target_lang_word": "выписывать",
                                "target_lang_sense_clarification": "заказывать доставку (газет, товаров по почте)"
                            }
                        ]
                    }
                },
                {

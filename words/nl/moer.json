    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aa19e603-6b3d-371e-8b1d-00fcb115e57f",
                    "sense_definition": "Een metalen blokje, meestal zeshoekig, met een gat dat van schroefdraad is voorzien zodat het op een bout past.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fastener",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Draai de <w>moer</w> stevig aan met een passende sleutel.",
                            "target_lang_translations": {
                                "en": "Tighten the <w>nut</w> securely with a suitable wrench.",
                                "ru": "Крепко затяни <w>гайку</w> подходящим ключом."
                            }
                        },
                        {
                            "text": "Zonder de juiste <w>moer</w> blijft de bout niet goed zitten.",
                            "target_lang_translations": {
                                "en": "Without the right <w>nut</w>, the bolt won't stay in place.",
                                "ru": "Без подходящей <w>гайки</w> болт не будет держаться как следует."
                            }
                        }
                    ],
                    "antonyms": [
                        "bout"
                    ],
                    "common_phrases": [
                        "bouten en moeren"
                    ],
                    "target_lang_definitions": {
                        "en": "A small metal block, usually hexagonal, with a threaded hole that fits onto a bolt.",
                        "ru": "Металлическая деталь, обычно шестиугольная, с резьбовым отверстием, навинчиваемая на болт."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nut",
                                "target_lang_sense_clarification": "hardware fastener"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гайка",
                                "target_lang_sense_clarification": "крепежная деталь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50ba5897-8f2f-37b6-a4a2-4c2d59b7a61c",
                    "sense_definition": "Iets kleins of onbeduidends, uitsluitend gebruikt in ontkennende zinnen om aan te geven dat er helemaal niets van iets is.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "idiom_nothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kan me echt geen <w>moer</w> schelen wat anderen daarvan denken.",
                            "target_lang_translations": {
                                "en": "I really don't give a <w>damn</w> what others think about that.",
                                "ru": "Меня совершенно <w>ни черта</w> не волнует, что об этом думают другие."
                            }
                        },
                        {
                            "text": "Ik heb de hele dag gewerkt, maar ik ben er geen <w>moer</w> mee opgeschoten.",
                            "target_lang_translations": {
                                "en": "I worked all day, but I didn't make a <w>damn</w> bit of progress.",
                                "ru": "Я работал весь день, но <w>ни черта</w> не продвинулся."
                            }
                        }
                    ],
                    "common_phrases": [
                        "geen moer"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Something small or insignificant, used exclusively in negative sentences to indicate that there is absolutely nothing of something.",
                        "ru": "Что-то незначительное; используется в отрицательных предложениях для усиления значения «абсолютно ничего»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "damn",
                                "target_lang_sense_clarification": "colloquial, used in negatives"
                            },
                            {
                                "target_lang_word": "shit",
                                "target_lang_sense_clarification": "vulgar, used in negatives"
                            },
                            {
                                "target_lang_word": "thing",
                                "target_lang_sense_clarification": "used in 'not a thing'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ни черта",
                                "target_lang_sense_clarification": "разговорное выражение"
                            },
                            {
                                "target_lang_word": "ни фига",
                                "target_lang_sense_clarification": "просторечное выражение"
                            },
                            {
                                "target_lang_word": "ничего",
                                "target_lang_sense_clarification": "нейтральное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cda482d-e263-3bbd-a292-ddc1d37fa5b4",
                    "sense_definition": "Een vrouwelijke ouder (tegenwoordig vaak als plat, ruw of ouderwets ervaren).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mother",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij schold de jongen uit en beledigde zijn <w>moer</w>.",
                            "target_lang_translations": {
                                "en": "He scolded the boy and insulted his <w>mother</w>.",
                                "ru": "Он обругал парня и оскорбил его <w>мамашу</w>."
                            }
                        },
                        {
                            "text": "In sommige dialecten wordt <w>moer</w> nog gewoon voor moeder gebruikt.",
                            "target_lang_translations": {
                                "en": "In some dialects, <w>moer</w> is still simply used for mother.",
                                "ru": "В некоторых диалектах слово <w>мать</w> всё ещё используется в обычном значении."
                            }
                        }
                    ],
                    "synonyms": [
                        "moeder",
                        "ma",
                        "mama"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often pejorative or rough in modern standard Dutch"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A female parent (nowadays often perceived as vulgar, rough, or old-fashioned).",
                        "ru": "Мать (в современном языке часто звучит грубо, просторечно или устарело)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mother",
                                "target_lang_sense_clarification": "pejorative or dialectal"
                            },
                            {
                                "target_lang_word": "old lady",
                                "target_lang_sense_clarification": "slang/informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мамаша",
                                "target_lang_sense_clarification": "грубое или фамильярное"
                            },
                            {
                                "target_lang_word": "мать",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4deae3c8-8457-33c7-b0e7-28abb0c8682c",
                    "sense_definition": "Een vrouwelijk dier, in het bijzonder bij konijnen, hazen, fretten of bijen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "female_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jager zag een <w>moer</w> met haar jongen in het veld.",
                            "target_lang_translations": {
                                "en": "The hunter saw a <w>doe</w> with her young in the field.",
                                "ru": "Охотник увидел <w>самку</w> с детёнышами в поле."
                            }
                        },
                        {
                            "text": "Als de <w>moer</w> sterft, is het bijenvolk ten dode opgeschreven.",
                            "target_lang_translations": {
                                "en": "If the <w>queen</w> dies, the bee colony is doomed.",
                                "ru": "Если <w>матка</w> погибает, пчелиная семья обречена."
                            }
                        }
                    ],
                    "synonyms": [
                        "bijenkoningin",
                        "moershaas",
                        "moerkonijn"
                    ],
                    "target_lang_definitions": {
                        "en": "A female animal, particularly of rabbits, hares, ferrets, or bees.",
                        "ru": "Самка животного (особенно кролика, зайца, хорька) или пчелиная матка."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "doe",
                                "target_lang_sense_clarification": "rabbit/hare"
                            },
                            {
                                "target_lang_word": "queen",
                                "target_lang_sense_clarification": "bee"
                            },
                            {
                                "target_lang_word": "dam",
                                "target_lang_sense_clarification": "general female parent of animal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "самка",
                                "target_lang_sense_clarification": "животного"
                            },
                            {
                                "target_lang_word": "матка",
                                "target_lang_sense_clarification": "пчелиная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d0c6302-5bad-32f8-8629-08ff6a62257a",
                    "sense_definition": "Het bezinksel dat ontstaat in vloeistoffen zoals wijn of azijn.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sediment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de wijn voorzichtig schenken om de <w>moer</w> in de fles te laten.",
                            "target_lang_translations": {
                                "en": "You must pour the wine carefully to leave the <w>dregs</w> in the bottle.",
                                "ru": "Нужно наливать вино осторожно, чтобы оставить <w>осадок</w> в бутылке."
                            }
                        },
                        {
                            "text": "Onderin het vat had zich een dikke laag <w>moer</w> gevormd.",
                            "target_lang_translations": {
                                "en": "A thick layer of <w>lees</w> had formed at the bottom of the barrel.",
                                "ru": "На дне бочки образовался толстый слой <w>осадка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezinksel",
                        "droesem"
                    ],
                    "target_lang_definitions": {
                        "en": "The sediment that forms in liquids such as wine or vinegar.",
                        "ru": "Осадок, образующийся в жидкостях, таких как вино или уксус."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dregs",
                                "target_lang_sense_clarification": "sediment"
                            },
                            {
                                "target_lang_word": "lees",
                                "target_lang_sense_clarification": "wine sediment"
                            },
                            {
                                "target_lang_word": "mother",
                                "target_lang_sense_clarification": "vinegar fermentation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "осадок",
                                "target_lang_sense_clarification": "в жидкости"
                            },
                            {
                                "target_lang_word": "гуща",
                                "target_lang_sense_clarification": "плотный осадок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28e81ab1-f073-3ecf-b14b-0e682552c0b3",
                    "sense_definition": "Een drassig stuk land of veengebied.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "marsh",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het oude dorp was gebouwd op een gedempte <w>moer</w>.",
                            "target_lang_translations": {
                                "en": "The old village was built on a filled-in <w>marsh</w>.",
                                "ru": "Старая деревня была построена на осушенном <w>болоте</w>."
                            }
                        },
                        {
                            "text": "In de middeleeuwen werd er veel turf gestoken in de <w>moer</w>.",
                            "target_lang_translations": {
                                "en": "In the Middle Ages, much peat was cut in the <w>bog</w>.",
                                "ru": "В Средние века на <w>торфянике</w> добывали много торфа."
                            }
                        }
                    ],
                    "synonyms": [
                        "moeras",
                        "veen"
                    ],
                    "traits": [

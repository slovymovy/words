            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "eb1391c5-e6a8-3d10-bdb0-01516e6d4e2e",
                    "sense_definition": "W taki sam sposób, w jednakowym stopniu lub w takiej samej ilości dla wszystkich stron.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "manner_distribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice starają się kochać i traktować swoje dzieci <w>równo</w>.",
                            "target_lang_translations": {
                                "ru": "Родители стараются любить и относиться к своим детям <w>одинаково</w>."
                            }
                        },
                        {
                            "text": "Zysk ze sprzedaży domu podzieliliśmy <w>równo</w> na trzy części.",
                            "target_lang_translations": {
                                "ru": "Прибыль от продажи дома мы разделили <w>поровну</w> на три части."
                            }
                        }
                    ],
                    "synonyms": [
                        "tak samo"
                    ],
                    "antonyms": [
                        "nierówno"
                    ],
                    "common_phrases": [
                        "dzielić równo",
                        "traktować równo",
                        "wszystkim równo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одинаковым образом, в равной степени или в равном количестве для всех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поровну",
                                "target_lang_sense_clarification": "о делении на части"
                            },
                            {
                                "target_lang_word": "одинаково",
                                "target_lang_sense_clarification": "об отношении или действии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e3b0437-7c7a-37ee-bf70-7b6503e6d04f",
                    "sense_definition": "W sposób tworzący gładką, płaską powierzchnię, bez wystających elementów czy zagłębień.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "manner_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żywopłot wokół ogrodu był bardzo <w>równo</w> przycięty.",
                            "target_lang_translations": {
                                "ru": "Живая изгородь вокруг сада была очень <w>ровно</w> подстрижена."
                            }
                        },
                        {
                            "text": "Książki na półce muszą stać <w>równo</w>, żeby zachować porządek.",
                            "target_lang_translations": {
                                "ru": "Книги на полке должны стоять <w>ровно</w>, чтобы сохранялся порядок."
                            }
                        }
                    ],
                    "antonyms": [
                        "nierówno"
                    ],
                    "common_phrases": [
                        "równo przycięty",
                        "równo ułożony",
                        "równo z ziemią"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образуя гладкую, плоскую поверхность, без выступов и неровностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ровно",
                                "target_lang_sense_clarification": "гладко, плоско"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d90c92d-26a2-3b36-86b2-90e3c5a1a6eb",
                    "sense_definition": "W jednej linii, rzędzie lub szeregu; prosto.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "manner_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczniowie ustawili się <w>równo</w> w pary przed wejściem do szkoły.",
                            "target_lang_translations": {
                                "ru": "Ученики <w>ровно</w> построились парами перед входом в школу."
                            }
                        },
                        {
                            "text": "Samochody na parkingu stały <w>równo</w> obok siebie.",
                            "target_lang_translations": {
                                "ru": "Машины на парковке стояли <w>ровно</w> рядом друг с другом."
                            }
                        }
                    ],
                    "antonyms": [
                        "nierówno"
                    ],
                    "common_phrases": [
                        "stać równo",
                        "iść równo",
                        "ustawić się równo"
                    ],
                    "target_lang_definitions": {
                        "ru": "По прямой линии, в ряд или шеренгу; прямо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ровно",
                                "target_lang_sense_clarification": "прямо, в линию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "431a8754-1efe-3a17-9a04-006aecbcd630",
                    "sense_definition": "W stałym, regularnym tempie lub rytmie; bez zakłóceń.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manner_rhythm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silnik samochodu pracował cicho i <w>równo</w> przez całą drogę.",
                            "target_lang_translations": {
                                "ru": "Двигатель машины работал тихо и <w>ровно</w> всю дорогу."
                            }
                        },
                        {
                            "text": "Pacjent oddycha <w>równo</w>, co jest dobrym znakiem po operacji.",
                            "target_lang_translations": {
                                "ru": "Пациент дышит <w>ровно</w>, что является хорошим знаком после операции."
                            }
                        }
                    ],
                    "antonyms": [
                        "nierówno"
                    ],
                    "common_phrases": [
                        "oddychać równo",
                        "bić równo",
                        "pracować równo"
                    ],
                    "target_lang_definitions": {
                        "ru": "В постоянном, регулярном темпе или ритме; без перебоев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ровно",
                                "target_lang_sense_clarification": "ритмично, размеренно"
                            }
                        ]
                    }
                }
            ]
        },

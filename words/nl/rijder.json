                {
                    "sense_id": "72add75d-0387-3800-a268-82b47562de2f",
                    "sense_definition": "Iemand die een voertuig bestuurt of op een dier rijdt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "driver_rider",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>rijder</w> verloor de controle over zijn motor in de bocht.",
                            "target_lang_translations": {
                                "en": "The <w>rider</w> lost control of his motorcycle in the turn.",
                                "ru": "<w>Водитель</w> потерял управление своим мотоциклом на повороте."
                            }
                        },
                        {
                            "text": "Als zakelijke <w>rijder</w> maak ik jaarlijks veel kilometers voor mijn werk.",
                            "target_lang_translations": {
                                "en": "As a business <w>driver</w>, I cover many kilometers a year for work.",
                                "ru": "Как корпоративный <w>водитель</w>, я ежегодно проезжаю много километров по работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestuurder",
                        "berijder"
                    ],
                    "antonyms": [
                        "passagier",
                        "bijrijder"
                    ],
                    "common_phrases": [
                        "zakelijke rijder",
                        "ervaren rijder",
                        "snelle rijder"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who drives a vehicle or rides an animal.",
                        "ru": "Тот, кто управляет транспортным средством или едет верхом на животном."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rider",
                                "target_lang_sense_clarification": "of a motorcycle, horse, or bicycle"
                            },
                            {
                                "target_lang_word": "driver",
                                "target_lang_sense_clarification": "of a car, especially in professional contexts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "водитель",
                                "target_lang_sense_clarification": "транспортного средства"
                            },
                            {
                                "target_lang_word": "ездок",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "всадник",
                                "target_lang_sense_clarification": "на животном"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4aa9701b-845b-323e-8d0f-8eeae2af16fa",
                    "sense_definition": "Een soldaat met de rang van soldaat bij het Korps Rijdende Artillerie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>rijder</w> poetste zijn laarzen voor de inspectie.",
                            "target_lang_translations": {
                                "en": "The <w>private</w> polished his boots for the inspection.",
                                "ru": "<w>Рядовой</w> чистил свои сапоги перед проверкой."
                            }
                        },
                        {
                            "text": "Tijdens de ceremonie stond de <w>rijder</w> strak in de houding naast het kanon.",
                            "target_lang_translations": {
                                "en": "During the ceremony, the <w>gunner</w> stood at attention next to the cannon.",
                                "ru": "Во время церемонии <w>артиллерист</w> стоял по стойке смирно рядом с орудием."
                            }

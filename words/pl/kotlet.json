    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b57ed86a-acf5-3db8-9524-b348cc1f9193",
                    "sense_definition": "Kawałek mięsa, masy mięsnej, warzywnej lub jajecznej, uformowany w plaster i usmażony na tłuszczu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na niedzielny obiad mama usmażyła pyszne <kotlety> mielone.",
                            "target_lang_translations": {
                                "ru": "На воскресный обед мама пожарила вкусные <котлеты>."
                            }
                        },
                        {
                            "text": "Wolisz zjeść <kotlet> z ziemniakami czy z ryżem?",
                            "target_lang_translations": {
                                "ru": "Ты предпочитаешь съесть <котлету> с картошкой или с рисом?"
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) karminadel",
                        "gw. lwow. karmenadel"
                    ],
                    "common_phrases": [
                        "kotlet schabowy",
                        "kotlet mielony",
                        "kotlet de volaille",
                        "rozbić kotlety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок мяса или изделие из мясного, овощного или яичного фарша, сформированное в лепешку и обжаренное в жире."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "котлета",
                                "target_lang_sense_clarification": "изделие из фарша"
                            },
                            {
                                "target_lang_word": "отбивная",
                                "target_lang_sense_clarification": "цельный кусок мяса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6798c851-681a-35c2-8d6a-0d0e3b5faf03",
                    "sense_definition": "Potoczne określenie na kotlet schabowy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zamówiłem w barze <kotlet> z kapustą zasmażaną.",
                            "target_lang_translations": {
                                "ru": "Я заказал в баре <отбивную> с тушёной капустой."
                            }
                        },
                        {
                            "text": "Ten <kotlet> jest tak duży, że nie mieści się na talerzu.",
                            "target_lang_translations": {
                                "ru": "Эта <отбивная> такая большая, что не помещается на тарелке."
                            }
                        }
                    ],
                    "synonyms": [
                        "schabowy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название свиной отбивной (kotlet schabowy)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отбивная",
                                "target_lang_sense_clarification": "свиная"
                            },
                            {
                                "target_lang_word": "шницель",
                                "target_lang_sense_clarification": "кулинарное блюдо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5bde541-56d2-3b4e-906b-2814b5027969",
                    "sense_definition": "Bokobrody; zarost na policzkach.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starszy pan w kapeluszu miał gęste, siwe <kotlety>.",
                            "target_lang_translations": {
                                "ru": "У пожилого господина в шляпе были густые седые <бакенбарды>."
                            }
                        },
                        {
                            "text": "W dawnych czasach modne były długie <kotlety> na twarzy.",
                            "target_lang_translations": {
                                "ru": "В прежние времена были модны длинные <бакенбарды> на лице."
                            }
                        }
                    ],
                    "traits": [
                        {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3dd20b7d-53bf-345f-8f6f-cc5872c26cc8",
                    "sense_definition": "Взяв в руки, переместить и доставить кого-либо или что-либо в другое место.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обещал маме <отнести> эти книги в библиотеку завтра утром.",
                            "target_lang_translations": {
                                "en": "I promised Mom to <take> these books to the library tomorrow morning."
                            }
                        },
                        {
                            "text": "Нужно срочно <отнести> документы на подпись директору.",
                            "target_lang_translations": {
                                "en": "We need to urgently <take> the documents to the director for signature."
                            }
                        }
                    ],
                    "synonyms": [
                        "доставить",
                        "передать"
                    ],
                    "antonyms": [
                        "принести",
                        "забрать"
                    ],
                    "common_phrases": [
                        "отнести вещи",
                        "отнести письмо",
                        "отнести в ремонт",
                        "отнести домой"
                    ],
                    "target_lang_definitions": {
                        "en": "To take or carry something or someone to a different place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "deliver to a destination"
                            },
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "transport physically"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99200bd3-5946-3719-a25b-357befbf87e3",
                    "sense_definition": "Причислить к какой-либо категории, разряду, группе или времени; связать с чем-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учёные предлагают <отнести> эту находку к первому веку нашей эры.",
                            "target_lang_translations": {
                                "en": "Scientists propose to <date> this find to the first century AD."
                            }
                        },
                        {
                            "text": "Мы не можем <отнести> этот случай к числу простых задач.",
                            "target_lang_translations": {
                                "en": "We cannot <classify> this case as one of the simple tasks."
                            }
                        }
                    ],
                    "synonyms": [
                        "причислить",
                        "приписать",
                        "классифицировать"
                    ],
                    "antonyms": [
                        "исключить"
                    ],
                    "common_phrases": [
                        "отнести к категории",
                        "отнести к числу",
                        "отнести на счёт"
                    ],
                    "target_lang_definitions": {
                        "en": "To assign to a category, group, or time period; to attribute to a cause."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attribute",
                                "target_lang_sense_clarification": "ascribe to a cause or source"
                            },
                            {
                                "target_lang_word": "classify",
                                "target_lang_sense_clarification": "assign to a category"
                            },
                            {
                                "target_lang_word": "date",
                                "target_lang_sense_clarification": "assign to a specific time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e93aaed-7253-3dd3-ba8c-ba27a9da20e7",
                    "sense_definition": "Переместить на некоторое расстояние в сторону от кого-либо или чего-либо (часто о ветре, воде или физическом усилии).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный ветер мог <отнести> воздушный шар далеко на север.",
                            "target_lang_translations": {
                                "en": "A strong wind could <carry> the hot air balloon far to the north."
                            }
                        },
                        {
                            "text": "Рабочие решили <отнести> тяжёлый шкаф от стены, чтобы покрасить её.",
                            "target_lang_translations": {
                                "en": "The workers decided to <move> the heavy wardrobe <away> from the wall to paint it."
                            }
                        }
                    ],
                    "synonyms": [
                        "отодвинуть",
                        "переместить",
                        "оттащить"
                    ],
                    "antonyms": [
                        "придвинуть",
                        "приблизить"
                    ],
                    "common_phrases": [
                        "отнести в сторону",
                        "отнести от берега",
                        "отнести ветром"
                    ],
                    "target_lang_definitions": {
                        "en": "To move something a distance away from a position, or to be carried off by elements like wind or water."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move away",
                                "target_lang_sense_clarification": "shift position from something"
                            },
                            {
                                "target_lang_word": "carry away",
                                "target_lang_sense_clarification": "transport by wind or water"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
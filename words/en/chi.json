                },
                {
                    "sense_id": "a4c7414c-b218-3f8b-b567-ae96245ccf66",
                    "sense_definition": "A surname of Chinese origin.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Dr. <w>Chi</w> has been teaching mathematics at the university for twenty years.",
                            "target_lang_translations": {
                                "ru": "Доктор <w>Чи</w> преподает математику в университете уже двадцать лет."
                            }
                        },
                        {
                            "text": "We invited the <w>Chi</w> family to join us for dinner next Saturday.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили семью <w>Чи</w> поужинать с нами в следующую субботу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия китайского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Чи",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9778efc-3904-32fd-85af-52b7c73866e6",
                    "sense_definition": "An ancient Chinese duchy and later kingdom during the Zhou dynasty.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "history_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Historians study the political strategies used by the state of <w>Chi</w>.",
                            "target_lang_translations": {
                                "ru": "Историки изучают политические стратегии, использовавшиеся царством <w>Ци</w>."
                            }
                        },
                        {
                            "text": "The army of <w>Chi</w> was a powerful force in the ancient wars.",
                            "target_lang_translations": {
                                "ru": "Армия <w>Ци</w> была мощной силой в древних войнах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "Wade-Giles romanization, now usually Qi"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Древнее китайское княжество, а затем царство эпохи династии Чжоу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ци",
                                "target_lang_sense_clarification": "древнее царство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

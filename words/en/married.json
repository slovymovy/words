    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "073fe2e3-2745-379f-a306-e2d4a78b9a08",
                    "sense_definition": "In a state of marriage; having a wife or a husband.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Are you <w>married</w> or single?",
                            "target_lang_translations": {
                                "ru": "Вы <w>женаты</w> или холосты?"
                            }
                        },
                        {
                            "text": "My sister got <w>married</w> to her childhood sweetheart last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году моя сестра <w>вышла замуж</w> за свою школьную любовь."
                            }
                        }
                    ],
                    "synonyms": [
                        "wedded",
                        "wed"
                    ],
                    "antonyms": [
                        "single",
                        "unmarried"
                    ],
                    "common_phrases": [
                        "get married",
                        "happily married",
                        "married couple"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состоящий в браке; имеющий мужа или жену."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "женатый",
                                "target_lang_sense_clarification": "о мужчине"
                            },
                            {
                                "target_lang_word": "замужняя",
                                "target_lang_sense_clarification": "о женщине"
                            },
                            {
                                "target_lang_word": "замужем",
                                "target_lang_sense_clarification": "о женщине (состояние)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d012f930-56ea-39b4-9bba-d0c41e674b0e",
                    "sense_definition": "Showing commitment or devotion normally reserved for a spouse; strongly committed or attached to something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is completely <w>married</w> to his work and rarely takes a vacation.",
                            "target_lang_translations": {
                                "ru": "Он полностью <w>женат</w> на своей работе и редко берет отпуск."
                            }
                        },
                        {
                            "text": "We are not <w>married</w> to this idea, so feel free to suggest changes.",
                            "target_lang_translations": {
                                "ru": "Мы не <w>привязаны</w> к этой идее, так что смело предлагайте изменения."
                            }
                        }
                    ],
                    "synonyms": [
                        "wedded"
                    ],
                    "common_phrases": [
                        "married to the job",
                        "married to an idea"
                    ],
                    "target_lang_definitions": {
                        "ru": "Всецело преданный делу или идее; неразрывно связанный с чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преданный",
                                "target_lang_sense_clarification": "сильно увлечённый"
                            },
                            {
                                "target_lang_word": "поглощённый",
                                "target_lang_sense_clarification": "полностью занятый чем-то"
                            },
                            {
                                "target_lang_word": "привязанный",
                                "target_lang_sense_clarification": "не желающий менять что-то"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c3dfc531-5c74-3a14-ba1c-a00693435e33",
                    "sense_definition": "Simple past and past participle of <w>marry</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>married</w> in a small ceremony on the beach.",
                            "target_lang_translations": {
                                "ru": "Они <w>поженились</w> на скромной церемонии на пляже."
                            }
                        },
                        {
                            "text": "She <w>married</w> into a very wealthy and influential family.",
                            "target_lang_translations": {
                                "ru": "Она <w>вышла замуж</w> за человека из очень богатой и влиятельной семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "wed"
                    ],
                    "common_phrases": [
                        "just married"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола marry (жениться, выходить замуж)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поженились",
                                "target_lang_sense_clarification": "о паре"
                            },
                            {
                                "target_lang_word": "женился",
                                "target_lang_sense_clarification": "о мужчине"
                            },
                            {
                                "target_lang_word": "вышла замуж",
                                "target_lang_sense_clarification": "о женщине"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "84202b40-9007-36e4-a702-e2f2c2bc27e2",
                    "sense_definition": "A married person.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The resort offers special vacation packages designed specifically for young <w>marrieds</w>.",
                            "target_lang_translations": {
                                "ru": "Курорт предлагает специальные путевки, разработанные специально для молодых <w>супругов</w>."
                            }
                        },
                        {
                            "text": "Many <w>marrieds</w> find that buying a home is their first major joint financial decision.",
                            "target_lang_translations": {
                                "ru": "Многие <w>семейные пары</w> считают покупку жилья своим первым крупным совместным финансовым решением."
                            }
                        }
                    ],
                    "antonyms": [
                        "single"
                    ],
                    "common_phrases": [
                        "young marrieds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, состоящий в браке (обычно во множественном числе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "супруги",
                                "target_lang_sense_clarification": "муж и жена"
                            },
                            {
                                "target_lang_word": "семейные пары",
                                "target_lang_sense_clarification": "люди в браке"
                            }
                        ]
                    }
                }
            ]
        }

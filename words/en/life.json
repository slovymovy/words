                            {
                                "target_lang_word": "образ жизни",
                                "target_lang_sense_clarification": "уклад, привычки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "133cfada-dced-346c-a857-c9caac4626e0",
                    "sense_definition": "A living person; an individual human existence.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "biological_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The terrible storm claimed many <w>lives</w> along the coast.",
                            "target_lang_translations": {
                                "ru": "Ужасный шторм унес много <w>жизней</w> на побережье."
                            }
                        },
                        {
                            "text": "Seat belts save thousands of <w>lives</w> on the road every year.",
                            "target_lang_translations": {
                                "ru": "Ремни безопасности спасают тысячи <w>жизней</w> на дорогах каждый год."
                            }
                        }
                    ],
                    "common_phrases": [
                        "save a life",
                        "loss of life",
                        "claim lives"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живой человек; отдельное человеческое существование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жизнь",
                                "target_lang_sense_clarification": "человеческая жизнь (исчисляемое)"

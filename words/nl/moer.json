                        },
                        {
                            "trait_type": "regional",
                            "comment": "often found in place names"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A boggy piece of land or peat area.",
                        "ru": "Топкая местность, болото или торфяник."
                    },

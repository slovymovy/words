{
    "word_family": [
        "parel",
        "parelmoer",
        "bepareld"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "65a13006-c9f6-3dd4-9777-2ee8c2e198b1",
                    "sense_definition": "Meervoud van <w>parel</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij droeg een prachtige ketting van echte <w>parelen</w> om haar hals.",
                            "target_lang_translations": {
                                "en": "She wore a beautiful necklace of real <w>pearls</w> around her neck.",
                                "ru": "Она носила красивое ожерелье из настоящего <w>жемчуга</w> на шее."
                            }
                        },
                        {
                            "text": "Je moet geen <w>parelen</w> voor de zwijnen gooien.",
                            "target_lang_translations": {
                                "en": "You shouldn't cast <w>pearls</w> before swine.",
                                "ru": "Не стоит метать <w>бисер</w> перед свиньями."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of *parel*.",
                        "ru": "Множественное число от слова *parel* (жемчужина)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pearls",
                                "target_lang_sense_clarification": "plural noun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "жемчужины",
                                "target_lang_sense_clarification": "отдельные бусины"
                            },
                            {
                                "target_lang_word": "жемчуг",
                                "target_lang_sense_clarification": "собирательное существительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "558bbdb6-bad1-3524-8026-5ebba6b96cda",
                    "sense_definition": "Zich vormen als kleine, ronde druppels op een oppervlak, vergelijkbaar met parels.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het zweet <w>parelde</w> op zijn voorhoofd na de zware inspanning.",
                            "target_lang_translations": {
                                "en": "Sweat <w>beaded</w> on his forehead after the heavy exertion.",
                                "ru": "Пот <w>выступил каплями</w> на его лбу после тяжелой нагрузки."
                            }
                        },
                        {
                            "text": "In de vroege ochtend <w>parelde</w> de dauw op de groene bladeren.",
                            "target_lang_translations": {
                                "en": "In the early morning, the dew <w>beaded</w> on the green leaves.",
                                "ru": "Ранним утром роса <w>сверкала каплями</w> на зеленых листьях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zweet parelde",
                        "tranen parelden"
                    ],
                    "target_lang_definitions": {
                        "en": "To form like small, round drops on a surface, similar to pearls.",
                        "ru": "Образовываться в виде маленьких круглых капель на поверхности, подобно жемчугу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to bead",
                                "target_lang_sense_clarification": "of liquid forming drops"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выступать каплями",
                                "target_lang_sense_clarification": "о жидкости (пот, роса)"
                            },
                            {
                                "target_lang_word": "покрываться каплями",
                                "target_lang_sense_clarification": "о поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4c21898-3f7c-3829-99f2-cf580f207c66",
                    "sense_definition": "Schitteren of glanzen met kleine lichtpuntjes, vaak gezegd van vloeistoffen (zoals wijn) of ogen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_visual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De champagne <w>parelde</w> feestelijk in de hoge glazen.",
                            "target_lang_translations": {
                                "en": "The champagne <w>sparkled</w> festively in the tall glasses.",
                                "ru": "Шампанское празднично <w>искрилось</w> в высоких бокалах."
                            }
                        },
                        {
                            "text": "Haar ogen <w>parelden</w> van geluk toen ze het goede nieuws hoorde.",
                            "target_lang_translations": {
                                "en": "Her eyes <w>sparkled</w> with happiness when she heard the good news.",
                                "ru": "Её глаза <w>сияли</w> от счастья, когда она услышала хорошие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "schitteren",
                        "fonkelen"
                    ],
                    "common_phrases": [
                        "parelende wijn"
                    ],
                    "target_lang_definitions": {
                        "en": "To shine or glisten with small points of light, often said of liquids (like wine) or eyes.",
                        "ru": "Сверкать или блестеть маленькими точками света, часто говорится о жидкостях (например, вине) или глазах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to sparkle",
                                "target_lang_sense_clarification": "visual effect or carbonation"
                            },
                            {
                                "target_lang_word": "to bubble",
                                "target_lang_sense_clarification": "of carbonated drinks"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "искриться",
                                "target_lang_sense_clarification": "визуальный эффект"
                            },
                            {
                                "target_lang_word": "играть",
                                "target_lang_sense_clarification": "о вине или свете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4f49795-4665-3ad9-b937-7dea916922cf",
                    "sense_definition": "Klinken met een reeks heldere, zuivere en snel opeenvolgende tonen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_auditory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De pianist liet de noten prachtig <w>parelen</w> tijdens het concert.",
                            "target_lang_translations": {
                                "en": "The pianist made the notes <w>ripple</w> beautifully during the concert.",
                                "ru": "Пианист заставил ноты прекрасно <w>переливаться</w> во время концерта."
                            }
                        },
                        {
                            "text": "Haar heldere lach <w>parelde</w> door de stille kamer.",
                            "target_lang_translations": {
                                "en": "Her bright laughter <w>rippled</w> through the quiet room.",
                                "ru": "Её звонкий смех <w>рассыпался</w> по тихой комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprankelen"
                    ],
                    "common_phrases": [
                        "parelende lach"
                    ],
                    "target_lang_definitions": {
                        "en": "To sound with a series of clear, pure, and rapidly successive tones.",
                        "ru": "Звучать серией чистых, ясных и быстро сменяющих друг друга тонов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to ripple",
                                "target_lang_sense_clarification": "of sound or laughter"
                            },
                            {
                                "target_lang_word": "to tinkle",
                                "target_lang_sense_clarification": "light, ringing sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "переливаться",
                                "target_lang_sense_clarification": "о звуке"
                            },
                            {
                                "target_lang_word": "рассыпаться",
                                "target_lang_sense_clarification": "о смехе или быстрых нотах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
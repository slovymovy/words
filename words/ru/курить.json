    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a342ce3e-95ad-3683-9a1d-30fab40c4d38",
                    "sense_definition": "Вдыхать и выдыхать дым тлеющего табака или других курительных смесей, а также иметь такую привычку.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "smoking_tobacco",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дедушка много <курил> в молодости, но потом бросил.",
                            "target_lang_translations": {
                                "en": "My grandfather <smoked> a lot in his youth, but then he quit."
                            }
                        },
                        {
                            "text": "В этом ресторане запрещено <курить> сигареты.",
                            "target_lang_translations": {
                                "en": "It is forbidden to <smoke> cigarettes in this restaurant."
                            }
                        },
                        {
                            "text": "Он вышел на балкон, чтобы <курить> в тишине.",
                            "target_lang_translations": {
                                "en": "He went out onto the balcony to <smoke> in silence."
                            }
                        }
                    ],
                    "synonyms": [
                        "дымить",
                        "смолить"
                    ],
                    "common_phrases": [
                        "бросить курить",
                        "курить сигареты",
                        "курить кальян",
                        "запрещено курить",
                        "вредно курить"
                    ],
                    "target_lang_definitions": {
                        "en": "To inhale and exhale the smoke of smoldering tobacco or other smoking mixtures, or to have such a habit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smoke",
                                "target_lang_sense_clarification": "tobacco, cigarettes, etc."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0d2439c-1bb4-3c4b-a65a-8252398da76f",
                    "sense_definition": "Внимательно и дотошно изучать какую-либо документацию, инструкцию или текст.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "study_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Программист весь вечер <курил> документацию к новой библиотеке.",
                            "target_lang_translations": {
                                "en": "The programmer spent the whole evening <poring over> the documentation for the new library."
                            }
                        },
                        {
                            "text": "Прежде чем задавать вопросы на форуме, нужно <курить> мануалы.",
                            "target_lang_translations": {
                                "en": "Before asking questions on the forum, you need to <study> the manuals."
                            }
                        }
                    ],
                    "synonyms": [
                        "смолить",
                        "дымить"
                    ],
                    "common_phrases": [
                        "курить мануалы",
                        "курить документацию",
                        "курить гайды"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "computer jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To carefully and meticulously study some documentation, instructions, or text."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pore over",
                                "target_lang_sense_clarification": "study intensely"
                            },
                            {
                                "target_lang_word": "study",
                                "target_lang_sense_clarification": "examine closely"
                            },
                            {
                                "target_lang_word": "digest",
                                "target_lang_sense_clarification": "understand information"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ead97f5-d462-372a-9935-32db202ff60c",
                    "sense_definition": "Сжигать ароматические вещества для получения дыма (обычно в религиозных обрядах) или выделять дым, дымиться.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "burn_incense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Священник начал <курить> ладан перед иконами.",
                            "target_lang_translations": {
                                "en": "The priest began to <burn> incense before the icons."
                            }
                        },
                        {
                            "text": "В древних храмах жрецы <курили> благовония богам.",
                            "target_lang_translations": {
                                "en": "In ancient temples, priests <burned> incense to the gods."
                            }
                        }
                    ],
                    "synonyms": [
                        "дымить",
                        "смолить"
                    ],
                    "common_phrases": [
                        "курить ладан",
                        "курить фимиам"

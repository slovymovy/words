    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cc731df1-a7e2-30d3-8a6f-373346208fc5",
                    "sense_definition": "Komend na de tweede in een rangorde; nummer drie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is de <w>derde</w> keer dat ik je probeer te bellen.",
                            "target_lang_translations": {
                                "en": "This is the <w>third</w> time I am trying to call you.",
                                "ru": "Это уже <w>третий</w> раз, когда я пытаюсь тебе дозвониться."
                            }
                        },
                        {
                            "text": "De <w>derde</w> wereldoorlog bleef gelukkig uit.",
                            "target_lang_translations": {
                                "en": "Fortunately, the <w>third</w> world war did not occur.",
                                "ru": "К счастью, <w>третья</w> мировая война не случилась."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ten derde",
                        "derde wereld",
                        "derde rang"
                    ],
                    "target_lang_definitions": {
                        "en": "Coming after the second in a sequence; number three.",
                        "ru": "Следующий за вторым по порядку; номер три."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "third",
                                "target_lang_sense_clarification": "ordinal number"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "третий",
                                "target_lang_sense_clarification": "порядковое числительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "413f796e-38af-326f-ab7b-32b953ceb836",
                    "sense_definition": "Een van de drie gelijke delen van een geheel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een <w>derde</w> van de stedelijke wereldbevolking woont in sloppenwijken.",
                            "target_lang_translations": {
                                "en": "A <w>third</w> of the urban world population lives in slums.",
                                "ru": "<w>Треть</w> городского населения мира живет в трущобах."
                            }
                        },
                        {
                            "text": "Hij heeft ongeveer een <w>derde</w> van het werk afgemaakt.",
                            "target_lang_translations": {
                                "en": "He has finished about a <w>third</w> of the work.",
                                "ru": "Он выполнил примерно <w>треть</w> работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een derde deel"
                    ],
                    "target_lang_definitions": {
                        "en": "One of three equal parts of a whole.",
                        "ru": "Одна из трёх равных частей целого."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "third",
                                "target_lang_sense_clarification": "fraction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "треть",
                                "target_lang_sense_clarification": "дробь"
                            },
                            {
                                "target_lang_word": "третья часть",
                                "target_lang_sense_clarification": "доля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02be1447-1fe2-3ac6-aacc-e07bcf263166",
                    "sense_definition": "Degene die of datgene dat de derde plaats inneemt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>derde</w> die belt maakt kans op 200 euro, waag je kans!",
                            "target_lang_translations": {
                                "en": "The <w>third</w> person to call stands a chance to win 200 euros, take your chance!",
                                "ru": "<w>Третий</w>, кто позвонит, получит шанс выиграть 200 евро, испытай удачу!"
                            }
                        },
                        {
                            "text": "Hij eindigde als <w>derde</w> in de spannende competitie.",
                            "target_lang_translations": {
                                "en": "He finished <w>third</w> in the exciting competition.",
                                "ru": "Он финишировал <w>третьим</w> в этом захватывающем соревновании."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The person or thing occupying the third place.",
                        "ru": "Человек или предмет, занимающий третье место."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "third",
                                "target_lang_sense_clarification": "position or rank"
                            },
                            {
                                "target_lang_word": "third one",
                                "target_lang_sense_clarification": "specific person or item"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "третий",
                                "target_lang_sense_clarification": "о человеке или предмете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52310a43-a86c-38a7-aa67-47eac56bd813",
                    "sense_definition": "Iemand die niet direct bij een zaak of overeenkomst betrokken is; een buitenstaander.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "party",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De mening van <w>derden</w> moet worden ingeroepen om volledig objectief te zijn.",
                            "target_lang_translations": {
                                "en": "The opinion of <w>third parties</w> must be called upon to be completely objective.",
                                "ru": "Для полной объективности необходимо узнать мнение <w>третьих лиц</w>."
                            }
                        },
                        {
                            "text": "Deze vertrouwelijke informatie mag niet aan <w>derden</w> worden verstrekt.",
                            "target_lang_translations": {
                                "en": "This confidential information may not be disclosed to <w>third parties</w>.",
                                "ru": "Эту конфиденциальную информацию нельзя передавать <w>третьим лицам</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "buitenstaander"
                    ],
                    "common_phrases": [
                        "aan derden",
                        "voor rekening van derden"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone not directly involved in a matter or agreement; an outsider.",
                        "ru": "Лицо, не участвующее непосредственно в деле или договоре; посторонний."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "third party",
                                "target_lang_sense_clarification": "uninvolved party"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "третье лицо",
                                "target_lang_sense_clarification": "юридический термин"
                            },
                            {
                                "target_lang_word": "посторонний",
                                "target_lang_sense_clarification": "непричастный человек"
                            }
                        ]
                    }
                }
            ]
        }

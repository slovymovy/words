                {
                    "sense_id": "4557b5f9-6dd1-342d-b532-2bf6f35b913f",
                    "sense_definition": "A place where two or more things, such as roads or rivers, meet.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The historic city was built at the <w>conjunction</w> of two major trade routes.",
                            "target_lang_translations": {
                                "ru": "Исторический город был построен на <w>пересечении</w> двух основных торговых путей."
                            }
                        },
                        {
                            "text": "We agreed to meet at the <w>conjunction</w> of the river and the canal.",
                            "target_lang_translations": {
                                "ru": "Мы договорились встретиться у <w>слияния</w> реки и канала."
                            }
                        }
                    ],
                    "synonyms": [
                        "joining"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где встречаются две или более вещи, например, дороги или реки."

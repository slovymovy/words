                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взломать",
                                "target_lang_sense_clarification": "получить несанкционированный доступ"
                            },
                            {
                                "target_lang_word": "хакнуть",
                                "target_lang_sense_clarification": "сленг: совершить хакерскую атаку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2312143e-b3b0-33cb-9e33-b35cad7c3041",
                    "sense_definition": "Een leegstaand gebouw in gebruik nemen om er te wonen zonder toestemming van de eigenaar.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "squat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De studenten besloten het leegstaande kantoorpand te <w>kraken</w>.",
                            "target_lang_translations": {
                                "ru": "Студенты решили <w>захватить</w> пустующее офисное здание."
                            }
                        },
                        {
                            "text": "Als dit pand niet snel verhuurd wordt, <w>kraken</w> wij het.",
                            "target_lang_translations": {
                                "ru": "Если это здание скоро не сдадут, мы его <w>захватим</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "occuperen"
                    ],
                    "common_phrases": [
                        "een pand kraken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занять пустующее здание для проживания без разрешения владельца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "самовольно занимать здание"
                            },
                            {
                                "target_lang_word": "оккупировать",
                                "target_lang_sense_clarification": "заселяться без разрешения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2601d658-4b2d-308b-8892-42835bc5587c",
                    "sense_definition": "Zeer negatieve kritiek geven op iets of iemand; iets volledig afkeuren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "criticize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn nieuwe boek werd in de krant volledig <w>gekraakt</w>.",
                            "target_lang_translations": {
                                "ru": "Его новую книгу в газете полностью <w>раскритиковали</w>."
                            }
                        },
                        {
                            "text": "De plannen van de architect werden door de buurtbewoners <w>gekraakt</w>.",
                            "target_lang_translations": {
                                "ru": "Планы архитектора были <w>раскритикованы</w> местными жителями."
                            }
                        }

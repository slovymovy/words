    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a07058e4-aa95-3bec-b9a8-5e876dc70482",
                    "sense_definition": "Выемка, щербина или неровность на краю, ребре или лезвии какого-либо предмета.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "notch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На лезвии старого топора была видна глубокая <w>зазубрина</w>.",
                            "target_lang_translations": {
                                "en": "A deep <w>notch</w> was visible on the blade of the old axe."
                            }
                        },
                        {
                            "text": "Он провёл пальцем по краю стола и почувствовал острую <w>зазубрину</w>.",
                            "target_lang_translations": {
                                "en": "He ran his finger along the edge of the table and felt a sharp <w>nick</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "щербина"
                    ],
                    "common_phrases": [
                        "зазубрина на ноже",
                        "зазубрина на лезвии",
                        "мелкая зазубрина"
                    ],
                    "target_lang_definitions": {
                        "en": "A small notch, nick, or irregularity on the edge of an object, especially on a blade or rim."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "notch",
                                "target_lang_sense_clarification": "V-shaped cut or indentation"
                            },
                            {
                                "target_lang_word": "nick",
                                "target_lang_sense_clarification": "small chip on an edge"
                            },
                            {
                                "target_lang_word": "jag",
                                "target_lang_sense_clarification": "sharp, rough projection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "487dd4b5-6204-3c78-a9f0-b6760ebaafee",
                    "sense_definition": "Острый зубец или выступ на острие рыболовного крючка, стрелы или гарпуна, препятствующий их извлечению.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "barb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыболовный крючок имеет специальную <w>зазубрину</w>, чтобы рыба не сорвалась.",
                            "target_lang_translations": {
                                "en": "The fishhook has a special <w>barb</w> so the fish does not break loose."
                            }
                        },
                        {
                            "text": "Наконечник стрелы был сделан с опасными <w>зазубринами</w>, наносящими рваные раны.",
                            "target_lang_translations": {
                                "en": "The arrowhead was made with dangerous <w>barbs</w> that inflict jagged wounds."
                            }
                        }
                    ],
                    "common_phrases": [
                        "зазубрина на крючке",
                        "стрела с зазубринами",
                        "гарпун с зазубриной"
                    ],
                    "target_lang_definitions": {
                        "en": "A sharp backward-pointing projection on a fishhook, arrow, or harpoon that prevents it from being easily pulled out."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "barb",
                                "target_lang_sense_clarification": "on a hook or arrow"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
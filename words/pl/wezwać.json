            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fa2cc6fd-45ad-3c7f-b34d-73a1ababf6df",
                    "sense_definition": "Poprosić lub nakazać komuś przybycie w określone miejsce; zawiadomić służby (np. policję, pogotowie) o konieczności przyjazdu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_summon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy natychmiast <w>wezwać</w> karetkę pogotowia do rannego pieszego.",
                            "target_lang_translations": {
                                "ru": "Мы должны немедленно <w>вызвать</w> скорую помощь для раненого пешехода."
                            }
                        },
                        {
                            "text": "Wczoraj szef <w>wezwał</w> mnie do swojego gabinetu na pilną rozmowę.",
                            "target_lang_translations": {
                                "ru": "Вчера начальник <w>вызвал</w> меня в свой кабинет для срочного разговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywołać",
                        "zawołać"
                    ],
                    "antonyms": [
                        "odesłać",
                        "wyprosić"
                    ],
                    "common_phrases": [
                        "wezwać policję",
                        "wezwać karetkę",
                        "wezwać pomoc",
                        "wezwać taksówkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Попросить или приказать кому-то прибыть в определенное место; вызвать экстренные службы (например, полицию, скорую помощь)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызвать",
                                "target_lang_sense_clarification": "пригласить официальное лицо или службу"
                            },
                            {
                                "target_lang_word": "позвать",
                                "target_lang_sense_clarification": "попросить прийти (менее официально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0b65afd-a456-35f5-bf55-760c0662aea2",
                    "sense_definition": "Skierować do kogoś oficjalne żądanie, nakaz lub apel o wykonanie jakiejś czynności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_urge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bank postanowił ostatecznie <w>wezwać</w> dłużnika do zapłaty zaległych rat.",
                            "target_lang_translations": {
                                "ru": "Банк решил окончательно <w>потребовать</w> от должника уплаты просроченных взносов."
                            }
                        },
                        {
                            "text": "Policja przez megafony musiała <w>wezwać</w> tłum do zachowania spokoju.",
                            "target_lang_translations": {
                                "ru": "Полиция через мегафоны была вынуждена <w>призвать</w> толпу к соблюдению спокойствия."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaapelować",
                        "nakazać"
                    ],
                    "common_phrases": [

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "78e9c03a-7abb-325b-9ef2-9c7b05d9c161",
                    "sense_definition": "Het bedrag dat periodiek betaald moet worden aan een verzekeringsmaatschappij om verzekerd te zijn.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>premie</w> voor de zorgverzekering is dit jaar weer iets gestegen.",
                            "target_lang_translations": {
                                "ru": "<w>Страховой взнос</w> на медицинское страхование в этом году снова немного вырос."
                            }
                        },
                        {
                            "text": "Als je de maandelijkse <w>premie</w> niet betaalt, stopt de dekking van je verzekering.",
                            "target_lang_translations": {
                                "ru": "Если вы не заплатите ежемесячный <w>взнос</w>, действие вашей страховки прекратится."
                            }
                        }
                    ],
                    "synonyms": [
                        "verzekeringsbijdrage"
                    ],
                    "common_phrases": [
                        "maandelijkse premie",
                        "premie betalen",
                        "premie berekenen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумма, которую необходимо периодически выплачивать страховой компании для получения страховки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страховой взнос",
                                "target_lang_sense_clarification": "периодический платеж за страховку"
                            },
                            {
                                "target_lang_word": "страховая премия",
                                "target_lang_sense_clarification": "технический термин в страховании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13b2f858-eace-3c61-92d2-1b291e4088d5",
                    "sense_definition": "Een financiële tegemoetkoming of beloning, vaak verstrekt door de overheid of een werkgever voor specifiek gedrag of een prestatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt bij de gemeente een <w>premie</w> aanvragen als je je huis energiezuiniger maakt.",
                            "target_lang_translations": {
                                "ru": "Вы можете запросить в муниципалитете <w>субсидию</w>, если сделаете свой дом более энергоэффективным."
                            }
                        },
                        {
                            "text": "De werknemers kregen een extra <w>premie</w> bovenop hun salaris vanwege de goede resultaten.",
                            "target_lang_translations": {
                                "ru": "Сотрудники получили дополнительную <w>премию</w> к зарплате за хорошие результаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "subsidie",
                        "toelage",
                        "bonus"
                    ],
                    "common_phrases": [
                        "premie ontvangen",
                        "premie aanvragen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Финансовая помощь или вознаграждение, часто предоставляемые государством или работодателем за определенные действия или достижения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "премия",
                                "target_lang_sense_clarification": "денежное поощрение от работодателя"
                            },
                            {
                                "target_lang_word": "субсидия",
                                "target_lang_sense_clarification": "финансовая помощь от государства"
                            },
                            {
                                "target_lang_word": "бонус",
                                "target_lang_sense_clarification": "дополнительное вознаграждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ecf13f6-379b-3da6-b8cb-e02de2a0808f",
                    "sense_definition": "Een geldbedrag dat wordt uitgeloofd als beloning voor het opsporen of vangen van een gezocht persoon of dier.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er stond een flinke <w>premie</w> op het hoofd van de ontsnapte gevangene.",
                            "target_lang_translations": {
                                "ru": "За голову сбежавшего заключенного была назначена крупная <w>награда</w>."
                            }
                        },
                        {
                            "text": "De politie loofde een <w>premie</w> uit voor de tip die leidde naar de dader.",
                            "target_lang_translations": {
                                "ru": "Полиция пообещала <w>вознаграждение</w> за наводку, которая привела к преступнику."
                            }
                        }
                    ],
                    "synonyms": [
                        "beloning",
                        "prijs"
                    ],
                    "common_phrases": [
                        "premie op iemands hoofd",
                        "premie uitloven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная сумма, обещанная в качестве награды за поимку разыскиваемого лица или животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "награда",
                                "target_lang_sense_clarification": "за поимку или находку"
                            },
                            {
                                "target_lang_word": "вознаграждение",
                                "target_lang_sense_clarification": "денежная выплата за услугу"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d2081e32-e736-3ce7-8591-128ffe2b296f",
                    "sense_definition": "Narzędzie ręczne składające się z trzonka i obucha, służące do uderzania, najczęściej używane do wbijania gwoździ.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "narzedzia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę poszukać <w>młotka</w>, żeby powiesić ten nowy obraz na ścianie w salonie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно поискать <w>молоток</w>, чтобы повесить эту новую картину на стену в гостиной."
                            }
                        },
                        {
                            "text": "Podczas naprawy płotu ojciec przypadkowo uderzył się <w>młotkiem</w> w palec.",
                            "target_lang_translations": {
                                "ru": "Во время ремонта забора отец случайно ударил себя <w>молотком</w> по пальцу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wbijać gwoździe młotkiem",
                        "uderzyć młotkiem",
                        "trzonek młotka",
                        "ciężki młotek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ручной инструмент, состоящий из рукоятки и бойка, служащий для нанесения ударов, чаще всего используемый для забивания гвоздей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молоток",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d8862d2-e1bf-3849-b59a-46f2b6b06373",
                    "sense_definition": "Potoczne, lekceważące określenie osoby, którą uważa się za głupią, nierozgarniętą lub naiwną.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "wyzwiska",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź takim <w>młotkiem</w> i spróbuj wreszcie zrozumieć, o co mi chodzi.",
                            "target_lang_translations": {
                                "ru": "Не будь такой <w>дубиной</w> и попытайся наконец понять, о чём я."
                            }
                        },
                        {
                            "text": "Tylko skończony <w>młotek</w> mógłby zapomnieć kluczyków do własnego samochodu.",
                            "target_lang_translations": {
                                "ru": "Только законченный <w>болван</w> мог бы забыть ключи от собственной машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "głupek",
                        "idiota",
                        "tępak",
                        "baran",
                        "bałwan",
                        "cymbał"
                    ],
                    "common_phrases": [
                        "ty młotku",
                        "kompletny młotek",
                        "skończony młotek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, пренебрежительное название человека, которого считают глупым, недалеким или наивным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болван",
                                "target_lang_sense_clarification": "глупый человек"
                            },
                            {
                                "target_lang_word": "дубина",
                                "target_lang_sense_clarification": "глупый человек (разг.)"
                            },
                            {
                                "target_lang_word": "тупица",
                                "target_lang_sense_clarification": "несообразительный человек"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
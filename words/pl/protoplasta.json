                    "sense_id": "2af3402d-a148-3802-a0f2-136f7bd5b665",
                    "sense_definition": "Najstarszy znany przodek rodu lub założyciel dynastii.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ancestor_person",
                    "name_type": "no",
                    "examples": [

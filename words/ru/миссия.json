{
    "word_family": [
        "миссионер",
        "миссионерский",
        "миссионерство"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f6244499-a170-3ee4-a840-a5a871a7e30a",
                    "sense_definition": "Ответственное задание, поручение или операция, которую необходимо выполнить.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "task_purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У специального агента была секретная <w>миссия</w> за границей.",
                            "target_lang_translations": {
                                "en": "The special agent had a secret <w>mission</w> abroad."
                            }
                        },
                        {
                            "text": "Наша главная <w>миссия</w> — помочь всем пострадавшим в этой катастрофе.",
                            "target_lang_translations": {
                                "en": "Our main <w>mission</w> is to help all the victims of this disaster."
                            }
                        }
                    ],
                    "synonyms": [
                        "задание",
                        "поручение",
                        "задача"
                    ],
                    "common_phrases": [
                        "секретная миссия",
                        "спасательная миссия",
                        "невыполнимая миссия",
                        "боевая миссия"
                    ],
                    "target_lang_definitions": {
                        "en": "An important task, assignment, or operation that must be carried out."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mission",
                                "target_lang_sense_clarification": "specific task or operation"
                            },
                            {
                                "target_lang_word": "assignment",
                                "target_lang_sense_clarification": "task given by authority"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb4a6cd9-9472-3b32-adbb-e59c28e64ca0",
                    "sense_definition": "Высокое предназначение, важная жизненная цель или ответственная роль в истории или обществе.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "task_purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач считал лечение людей своей главной <w>миссией</w> в жизни.",
                            "target_lang_translations": {
                                "en": "The doctor considered treating people his main <w>mission</w> in life."
                            }
                        },
                        {
                            "text": "У каждого поколения есть своя историческая <w>миссия</w>.",
                            "target_lang_translations": {
                                "en": "Every generation has its own historical <w>mission</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "предназначение",
                        "цель",
                        "роль"
                    ],
                    "common_phrases": [
                        "историческая миссия",
                        "великая миссия",
                        "выполнить свою миссию"
                    ],
                    "target_lang_definitions": {
                        "en": "A high purpose, important life goal, or responsible role in history or society."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mission",
                                "target_lang_sense_clarification": "life purpose"
                            },
                            {
                                "target_lang_word": "calling",
                                "target_lang_sense_clarification": "destiny or vocation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "454756b8-a0bc-3257-94d5-1f33cdf0af74",
                    "sense_definition": "Группа дипломатов или представителей, отправленная в другую страну с официальной целью.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "diplomacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В столицу прибыла иностранная торговая <w>миссия</w> для переговоров.",
                            "target_lang_translations": {
                                "en": "A foreign trade <w>mission</w> arrived in the capital for negotiations."
                            }
                        },
                        {
                            "text": "Члены <w>миссии</w> встретились с президентом для обсуждения договора.",
                            "target_lang_translations": {
                                "en": "Members of the <w>mission</w> met with the president to discuss the treaty."
                            }
                        }
                    ],
                    "synonyms": [
                        "поручение"
                    ],
                    "common_phrases": [
                        "торговая миссия",
                        "дипломатическая миссия",
                        "глава миссии"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of diplomats or representatives sent to another country for an official purpose."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mission",
                                "target_lang_sense_clarification": "group of representatives"
                            },
                            {
                                "target_lang_word": "delegation",
                                "target_lang_sense_clarification": "official group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de77780e-56c7-3659-9ca8-d54662c42d66",
                    "sense_definition": "Постоянное дипломатическое представительство в другой стране, а также здание, которое оно занимает.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "diplomacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он работает советником в российской <w>миссии</w> при ООН.",
                            "target_lang_translations": {
                                "en": "He works as an advisor at the Russian <w>mission</w> to the UN."
                            }
                        },
                        {
                            "text": "Протестующие собрались у здания <w>миссии</w> в центре города.",
                            "target_lang_translations": {
                                "en": "Protesters gathered near the <w>mission</w> building in the city center."
                            }
                        }
                    ],
                    "common_phrases": [
                        "здание миссии",
                        "сотрудник миссии"
                    ],
                    "target_lang_definitions": {
                        "en": "A permanent diplomatic representation in another country, as well as the building it occupies."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mission",
                                "target_lang_sense_clarification": "diplomatic office"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4801b9db-3383-33b3-95d7-6f97638d4adf",
                    "sense_definition": "Религиозная организация, занимающаяся распространением веры и благотворительностью.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Католическая <w>миссия</w> открыла школу для местных детей.",
                            "target_lang_translations": {
                                "en": "The Catholic <w>mission</w> opened a school for local children."
                            }
                        },
                        {
                            "text": "В отдаленной деревне работает небольшая гуманитарная <w>миссия</w>.",
                            "target_lang_translations": {
                                "en": "A small humanitarian <w>mission</w> is working in the remote village."
                            }
                        }
                    ],
                    "common_phrases": [
                        "христианская миссия",
                        "благотворительная миссия"
                    ],
                    "target_lang_definitions": {
                        "en": "A religious organization engaged in spreading faith and charity work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mission",
                                "target_lang_sense_clarification": "religious organization"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "55199f46-7c6a-3609-810e-1a4f89321d66",
                    "sense_definition": "Een vieze plek of verkleuring op een oppervlak die daar niet hoort.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "stain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zat een grote <vlek> op zijn nieuwe witte broek.",
                            "target_lang_translations": {
                                "en": "There was a big <w>stain</w> on his new white trousers.",
                                "ru": "На его новых белых брюках было большое <w>пятно</w>."
                            }
                        },
                        {
                            "text": "Mijn moeder probeerde de <vlek> uit mijn shirt te wassen.",
                            "target_lang_translations": {
                                "en": "My mother tried to wash the <w>stain</w> out of my shirt.",
                                "ru": "Моя мама пыталась отстирать <w>пятно</w> с моей рубашки."
                            }
                        }
                    ],
                    "synonyms": [
                        "smet"
                    ],
                    "common_phrases": [
                        "een vlek maken",
                        "vlekken verwijderen",
                        "rode vlek"
                    ],
                    "target_lang_definitions": {
                        "en": "A dirty spot or discoloration on a surface where it doesn't belong.",
                        "ru": "Грязное место или изменение цвета на поверхности, которого там быть не должно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stain",

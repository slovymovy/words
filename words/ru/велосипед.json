                    ],
                    "common_phrases": [
                        "делать велосипед",
                        "упражнение велосипед"
                    ],
                    "target_lang_definitions": {
                        "en": "An abdominal exercise performed lying on the back, mimicking the motion of pedaling."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bicycle crunch",
                                "target_lang_sense_clarification": "specific exercise name"
                            },
                            {

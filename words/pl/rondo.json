{
    "word_family": [
        "rondko",
        "rondowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "523abf3f-cc33-3427-8983-b6f1e3e27f06",
                    "sense_definition": "skrzyżowanie z wyspą środkową i jednokierunkową jezdnią wokół wyspy",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ronda</w> są bezpieczniejsze od zwykłych skrzyżowań.",
                            "target_lang_translations": {
                                "ru": "<w>Круговые перекрёстки</w> безопаснее обычных."
                            }
                        },
                        {
                            "text": "Musisz zjechać z <w>ronda</w> drugim zjazdem w prawo.",
                            "target_lang_translations": {
                                "ru": "Ты должен съехать с <w>кольца</w> на втором съезде направо."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrzyżowanie o ruchu okrężnym"
                    ],
                    "common_phrases": [
                        "wjechać na rondo",
                        "zjechać z ronda",
                        "ruch na rondzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перекресток с круговым движением вокруг центрального острова"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круговое движение",
                                "target_lang_sense_clarification": "тип перекрестка"
                            },
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "разговорное название перекрестка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e0b8443-b794-3ce8-88a5-42bbd56e594b",
                    "sense_definition": "brzeg kapelusza otaczający jego główkę",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiła elegancki kapelusz z bardzo szerokim <w>rondem</w>.",
                            "target_lang_translations": {
                                "ru": "Она купила элегантную шляпу с очень широкими <w>полями</w>."
                            }
                        },
                        {
                            "text": "Deszcz spływał po <w>rondzie</w> jego starego kapelusza.",
                            "target_lang_translations": {
                                "ru": "Дождь стекал по <w>полям</w> его старой шляпы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kapelusz z szerokim rondem",
                        "chwycić za rondo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступающий край шляпы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поля",
                                "target_lang_sense_clarification": "часть шляпы (обычно мн.ч.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88ae8341-1969-31da-95a9-6b2886e3a7fa",
                    "sense_definition": "forma muzyczna z powracającym refrenem i kupletami",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pierwotnie <w>rondo</w> było formą taneczno-wokalną, dopiero w baroku upowszechniło się jako forma instrumentalna.",
                            "target_lang_translations": {
                                "ru": "Изначально <w>рондо</w> было танцевально-вокальной формой, и только в эпоху барокко распространилось как инструментальная."
                            }
                        },
                        {
                            "text": "Mozart skomponował słynne <w>rondo</w> alla turca.",
                            "target_lang_translations": {
                                "ru": "Моцарт сочинил знаменитое <w>рондо</w> в турецком стиле."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rondo na fortepian",
                        "klasyczne rondo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальная форма с повторяющимся рефреном"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рондо",
                                "target_lang_sense_clarification": "музыка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f04da25-6561-3b75-b0bd-5cfe846902b6",
                    "sense_definition": "układ stroficzny posiadający od 8 do 15 wersów, dwa rymy i refren",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poeta napisał kunsztowne <w>rondo</w> dedykowane ukochanej.",
                            "target_lang_translations": {
                                "ru": "Поэт написал искусное <w>рондо</w>, посвященное возлюбленной."
                            }
                        },
                        {
                            "text": "Analiza tego <w>ronda</w> wymaga znajomości dawnych form poetyckich.",
                            "target_lang_translations": {
                                "ru": "Анализ этого <w>рондо</w> требует знания старинных поэтических форм."
                            }
                        }
                    ],
                    "common_phrases": [
                        "średniowieczne rondo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стихотворная форма с определенным чередованием рифм и рефреном"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рондо",
                                "target_lang_sense_clarification": "поэзия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d848cc5-b112-39a7-9f10-3b9a7d8d3ffe",
                    "sense_definition": "A powerful motor vehicle with large rear wheels, used chiefly on farms for hauling equipment and preparing fields.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer drove his <w>tractor</w> across the muddy field to harvest the crops.",
                            "target_lang_translations": {
                                "ru": "Фермер вел свой <w>трактор</w> по грязному полю, чтобы собрать урожай."
                            }
                        },
                        {
                            "text": "We saw a large green <w>tractor</w> pulling a wagon full of hay.",
                            "target_lang_translations": {
                                "ru": "Мы видели большой зеленый <w>трактор</w>, тянущий телегу с сеном."
                            }
                        }
                    ],
                    "synonyms": [
                        "farm tractor"
                    ],
                    "common_phrases": [
                        "drive a tractor",
                        "farm tractor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мощное транспортное средство с большими задними колесами, используемое главным образом на фермах для буксировки оборудования и обработки полей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трактор",
                                "target_lang_sense_clarification": "сельскохозяйственная машина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "658fe845-a479-3d02-8b25-1f6eac004c58",
                    "sense_definition": "A powerful truck with a driver's cab but no body, designed to pull a large trailer or semi-trailer.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>tractor</w> detached from the trailer at the loading dock.",
                            "target_lang_translations": {
                                "ru": "<w>Тягач</w> отцепился от прицепа на погрузочной платформе."
                            }
                        },
                        {
                            "text": "He drives a <w>tractor</w> for a logistics company that ships goods nationwide.",
                            "target_lang_translations": {
                                "ru": "Он водит <w>тягач</w> в логистической компании, которая перевозит товары по всей стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "tractor unit",
                        "semi-tractor",
                        "prime mover"
                    ],
                    "common_phrases": [
                        "tractor-trailer",
                        "tractor unit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мощный грузовик с кабиной, но без кузова, предназначенный для буксировки больших прицепов или полуприцепов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тягач",
                                "target_lang_sense_clarification": "грузовой автомобиль"
                            },
                            {
                                "target_lang_word": "седельный тягач",
                                "target_lang_sense_clarification": "тип тягача"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92e243ea-972e-33e9-aad7-05019906f51a",
                    "sense_definition": "Any piece of machinery or device that pulls something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This heavy-duty winch acts as a <w>tractor</w> to haul logs up the steep slope.",
                            "target_lang_translations": {
                                "ru": "Эта мощная лебедка действует как <w>тягач</w>, затаскивая бревна на крутой склон."
                            }
                        },
                        {
                            "text": "The mechanism works as a <w>tractor</w>, drawing the paper through the printer.",
                            "target_lang_translations": {
                                "ru": "Этот механизм работает как <w>трактор</w>, протягивая бумагу через принтер."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой механизм или устройство, которое тянет что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тяговый механизм",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "трактор",
                                "target_lang_sense_clarification": "механизм подачи бумаги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "329873db-f1cb-3285-9610-015517dbe623",
                    "sense_definition": "A movable coop without a floor, used to allow poultry to range freely on fresh grass while being protected.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We move the chicken <w>tractor</w> every morning so the birds have fresh ground to scratch.",
                            "target_lang_translations": {
                                "ru": "Мы передвигаем <w>передвижной курятник</w> каждое утро, чтобы у птиц была свежая земля для копания."
                            }
                        },
                        {
                            "text": "Building a <w>tractor</w> for the hens allows them to fertilize the lawn naturally.",
                            "target_lang_translations": {
                                "ru": "Постройка <w>передвижного курятника</w> для кур позволяет им удобрять газон естественным образом."
                            }
                        }
                    ],
                    "synonyms": [
                        "chicken tractor",
                        "ark"
                    ],
                    "common_phrases": [
                        "chicken tractor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передвижной курятник без пола, позволяющий домашней птице свободно пастись на свежей траве, находясь под защитой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передвижной курятник",
                                "target_lang_sense_clarification": "для птицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c07f0c7-a6f8-3c75-8c0d-fba961aa0edf",
                    "sense_definition": "An aircraft having the propeller situated in front of the engine or fuselage so that it pulls the aircraft.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most modern propeller planes are designed as a <w>tractor</w> rather than a pusher configuration.",
                            "target_lang_translations": {
                                "ru": "Большинство современных винтовых самолетов спроектированы как самолеты с <w>тянущим винтом</w>, а не с толкающим."
                            }
                        },
                        {
                            "text": "The pilot preferred the visibility offered by the <w>tractor</w> biplane.",
                            "target_lang_translations": {
                                "ru": "Пилот предпочитал обзор, который обеспечивал биплан с <w>тянущим винтом</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "pusher"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "aviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Самолет, у которого воздушный винт расположен перед двигателем или фюзеляжем и тянет самолет за собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самолет с тянущим винтом",
                                "target_lang_sense_clarification": "авиация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db390b15-9585-3de3-8559-2a0c4fed2386",
                    "sense_definition": "A nickname for the British Rail Class 37 locomotive.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trainspotters were excited to see a vintage <w>tractor</w> pulling the freight train.",
                            "target_lang_translations": {
                                "ru": "Любители поездов были в восторге, увидев старинный <w>трактор</w>, тянущий товарный поезд."
                            }
                        },
                        {
                            "text": "The old <w>tractor</w> was known for its distinctive engine sound.",
                            "target_lang_translations": {
                                "ru": "Старый <w>трактор</w> был известен своим характерным звуком двигателя."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "rail transport"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прозвище локомотива British Rail Class 37."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трактор",
                                "target_lang_sense_clarification": "сленговое название локомотива"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bad43786-1d63-32d4-a0b9-f6bc402692f0",
                    "sense_definition": "A metal rod used in the historical medical practice of Perkinism to treat ailments.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The quack doctor claimed he could cure pain using a metallic <w>tractor</w>.",
                            "target_lang_translations": {
                                "ru": "Врач-шарлатан утверждал, что может вылечить боль с помощью металлического <w>трактора</w>."
                            }
                        },
                        {
                            "text": "Museums sometimes display a set of <w>tractors</w> used in 18th-century therapies.",
                            "target_lang_translations": {
                                "ru": "Музеи иногда выставляют наборы <w>тракторов</w>, использовавшихся в терапии 18-го века."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический стержень, использовавшийся в исторической медицинской практике перкинизма для лечения недугов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трактор",
                                "target_lang_sense_clarification": "медицинский инструмент (ист.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "97e2db9b-cef1-35ca-a273-75b14ad3456c",
                    "sense_definition": "To plow or prepare land using a tractor.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>tractored</w> the field early in the morning to get ready for planting.",
                            "target_lang_translations": {
                                "ru": "Они <w>обрабатывали трактором</w> поле рано утром, чтобы подготовиться к посадке."
                            }
                        },
                        {
                            "text": "After the heavy rain, the farmer <w>tractored</w> the soil to improve drainage.",
                            "target_lang_translations": {
                                "ru": "После сильного дождя фермер <w>прошел трактором</w> почву, чтобы улучшить дренаж."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пахать или обрабатывать землю с помощью трактора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрабатывать трактором",
                                "target_lang_sense_clarification": "землю"
                            },
                            {
                                "target_lang_word": "пахать",
                                "target_lang_sense_clarification": "землю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d912a7c-7a59-315e-a02c-1b840d356a5f",
                    "sense_definition": "To drive or operate a tractor.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent the whole afternoon <w>tractoring</w> around the farm checking the fences.",
                            "target_lang_translations": {
                                "ru": "Он провел весь день, <w>разъезжая на тракторе</w> по ферме и проверяя заборы."
                            }
                        },
                        {
                            "text": "She enjoys <w>tractoring</w> during the harvest season.",
                            "target_lang_translations": {
                                "ru": "Ей нравится <w>работать на тракторе</w> во время сбора урожая."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Водить или управлять трактором."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работать на тракторе",
                                "target_lang_sense_clarification": "управлять машиной"
                            },
                            {
                                "target_lang_word": "водить трактор",
                                "target_lang_sense_clarification": "управлять машиной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "155f0295-c065-3931-91d7-61acd95317b2",
                    "sense_definition": "To move or pull an object using a tractor beam.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The alien ship <w>tractored</w> our shuttle into its docking bay.",
                            "target_lang_translations": {
                                "ru": "Корабль пришельцев <w>затянул</w> наш шаттл в свой стыковочный отсек."
                            }
                        },
                        {
                            "text": "In the movie, the space station <w>tractored</w> the debris away from the flight path.",
                            "target_lang_translations": {
                                "ru": "В фильме космическая станция <w>отбуксировала</w> обломки с траектории полета."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "science fiction"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать или притягивать объект с помощью притягивающего луча."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "захватывать лучом",
                                "target_lang_sense_clarification": "фантастика"
                            },
                            {
                                "target_lang_word": "буксировать",
                                "target_lang_sense_clarification": "лучом захвата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a575a1f5-10f6-3c93-8676-2a811268a1f4",
                    "sense_definition": "To treat a patient using metal tractors (Perkinism).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The physician <w>tractored</w> the patient's sore arm in hopes of relieving the pain.",
                            "target_lang_translations": {
                                "ru": "Врач <w>лечил трактором</w> больную руку пациента в надежде облегчить боль."
                            }
                        },
                        {
                            "text": "He was <w>tractored</w> for several weeks without any improvement in his condition.",
                            "target_lang_translations": {
                                "ru": "Его <w>лечили тракторами</w> несколько недель без какого-либо улучшения состояния."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лечить пациента с помощью металлических тракторов (перкинизм)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лечить трактором",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
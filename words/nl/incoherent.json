{
    "word_family": [
        "coherent",
        "coherentie",
        "incoherentie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "61888c5d-dc9c-34f4-85ff-3fa44b69b890",
                    "sense_definition": "Niet logisch samenhangend; bestaande uit delen die geen duidelijk geheel vormen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn betoog was zo <w>incoherent</w> dat niemand in de zaal de boodschap begreep.",
                            "target_lang_translations": {
                                "ru": "Его выступление было настолько <w>бессвязным</w>, что никто в зале не понял сути.",
                                "en": "His argument was so <w>incoherent</w> that no one in the room understood the message."
                            }
                        },
                        {
                            "text": "Dit zijn volledig <w>incoherente</w> samenvattingen waar geen touw aan vast te knopen is.",
                            "target_lang_translations": {
                                "ru": "Это совершенно <w>несвязные</w> конспекты, в которых невозможно разобраться.",
                                "en": "These are completely <w>incoherent</w> summaries that are impossible to make head or tail of."
                            }
                        }
                    ],
                    "synonyms": [
                        "onsamenhangend"
                    ],
                    "antonyms": [
                        "coherent",
                        "samenhangend"
                    ],
                    "common_phrases": [
                        "incoherente tekst",
                        "incoherent verhaal",
                        "incoherente spraak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий логической связи; состоящий из частей, не образующих единого целого.",
                        "en": "Not logically connected; consisting of parts that do not form a clear whole."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бессвязный",
                                "target_lang_sense_clarification": "лишенный логической связи (о речи, мыслях)"
                            },
                            {
                                "target_lang_word": "несвязный",
                                "target_lang_sense_clarification": "разрозненный, не образующий целого"
                            },
                            {
                                "target_lang_word": "непоследовательный",
                                "target_lang_sense_clarification": "лишенный логической последовательности"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "incoherent",
                                "target_lang_sense_clarification": "lacking logical connection or clarity"
                            },
                            {
                                "target_lang_word": "disjointed",
                                "target_lang_sense_clarification": "lacking a coherent sequence"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1228b333-028b-3da6-a501-08a8a949ef1e",
                    "sense_definition": "Op een manier die geen logisch verband vertoont of moeilijk te volgen is.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De patiënt antwoordde <w>incoherent</w> op de vragen van de arts door de hoge koorts.",
                            "target_lang_translations": {
                                "ru": "Пациент отвечал <w>бессвязно</w> на вопросы врача из-за высокой температуры.",
                                "en": "The patient answered the doctor's questions <w>incoherently</w> due to the high fever."
                            }
                        },
                        {
                            "text": "Licht <w>incoherent</w> praten kan een teken zijn van extreme vermoeidheid of verwarring.",
                            "target_lang_translations": {
                                "ru": "Слегка <w>бессвязная</w> речь может быть признаком сильной усталости или спутанности сознания.",
                                "en": "Speaking slightly <w>incoherently</w> can be a sign of extreme fatigue or confusion."
                            }
                        }
                    ],
                    "synonyms": [
                        "onsamenhangend"
                    ],
                    "common_phrases": [
                        "incoherent praten",
                        "incoherent reageren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, не имеющим логической связи или трудным для понимания.",
                        "en": "In a manner that lacks logical connection or is difficult to follow."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бессвязно",
                                "target_lang_sense_clarification": "непонятно, без логики"
                            },
                            {
                                "target_lang_word": "несвязно",
                                "target_lang_sense_clarification": "отрывочно"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "incoherently",
                                "target_lang_sense_clarification": "without logical connection"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
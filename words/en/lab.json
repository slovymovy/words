        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "94f931a7-1b30-385f-a480-a9e306bb8aac",
                    "sense_definition": "A short form of <w>laboratory</w>; a room or building equipped for scientific experiments, research, or teaching.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "science_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The students are conducting an experiment in the science <w>lab</w>.",
                            "target_lang_translations": {
                                "ru": "Студенты проводят эксперимент в научной <w>лаборатории</w>."
                            }
                        },
                        {
                            "text": "She wears a white coat when she works in the <w>lab</w>.",
                            "target_lang_translations": {
                                "ru": "Она носит белый халат, когда работает в <w>лаборатории</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "science lab",
                        "computer lab",
                        "in the lab",
                        "lab coat",
                        "lab rat"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short for laboratory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"laboratory\"; комната или здание, оборудованное для научных экспериментов, исследований или обучения."

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b62caf57-4ef7-310e-b1d8-1a8399507028",
                    "sense_definition": "Self-referential; showing an awareness of itself or the conventions of its genre, often in a clever or ironic way.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "self_ref",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie was very <w>meta</w> because the characters constantly joked about being in a film.",
                            "target_lang_translations": {
                                "ru": "Фильм был очень <w>мета</w>, потому что персонажи постоянно шутили о том, что находятся в кино."
                            }
                        },
                        {
                            "text": "Writing a story about a writer trying to write a story is a classic <w>meta</w> concept.",
                            "target_lang_translations": {
                                "ru": "Написание рассказа о писателе, пытающемся написать рассказ, — это классическая <w>мета</w>-концепция."
                            }
                        }
                    ],
                    "synonyms": [
                        "self-referential",
                        "recursive"
                    ],
                    "common_phrases": [
                        "so meta",
                        "very meta",
                        "meta joke"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to describe media or humor"
                        }
                    ],
                    "target_lang_definitions": {

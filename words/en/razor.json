    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cb3e1ef3-8575-301a-962e-b4f5e8777e38",
                    "sense_definition": "Any tool or instrument designed for shaving hair from the body.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He needs to buy a new <w>razor</w> because his old one is dull.",
                            "target_lang_translations": {
                                "ru": "Ему нужно купить новую <w>бритву</w>, так как старая затупилась."
                            }
                        },
                        {
                            "text": "Use an electric <w>razor</w> if you want a quick shave in the morning.",
                            "target_lang_translations": {
                                "ru": "Используйте электрическую <w>бритву</w>, если хотите быстро побриться утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "shaver"
                    ],
                    "common_phrases": [
                        "electric razor",
                        "safety razor",
                        "straight razor",
                        "disposable razor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Инструмент или прибор, предназначенный для сбривания волос с тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бритва",
                                "target_lang_sense_clarification": "инструмент для бритья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02f2dad5-0de7-3fb6-8771-b5bd0371eed2",
                    "sense_definition": "A conceptual principle or rule of thumb used to eliminate unlikely explanations for a phenomenon.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist applied Occam's <w>razor</w> to choose the simplest theory.",
                            "target_lang_translations": {
                                "ru": "Учёный применил <w>бритву</w> Оккама, чтобы выбрать простейшую теорию."
                            }
                        },
                        {
                            "text": "Hanlon's <w>razor</w> suggests that we should not assume malice when stupidity is a sufficient explanation.",
                            "target_lang_translations": {
                                "ru": "<w>Бритва</w> Хэнлона гласит: не следует приписывать злому умыслу то, что можно объяснить глупостью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Occam's razor",
                        "Hanlon's razor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Концептуальный принцип или правило, используемое для отсечения маловероятных объяснений феномена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бритва",
                                "target_lang_sense_clarification": "философский принцип (например, бритва Оккама)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c92c7ea5-f38c-3bd6-976e-39ee6efa9382",
                    "sense_definition": "The sharp tusk of a wild boar.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hunter was wary of the wild boar's dangerous <w>razor</w>.",
                            "target_lang_translations": {
                                "ru": "Охотник остерегался опасного <w>клыка</w> дикого кабана."
                            }
                        },
                        {
                            "text": "A large <w>razor</w> protruded from the animal's jaw.",
                            "target_lang_translations": {
                                "ru": "Из челюсти животного торчал огромный <w>клык</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tusk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Острый клык дикого кабана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клык",
                                "target_lang_sense_clarification": "зуб кабана"
                            },
                            {
                                "target_lang_word": "бивень",
                                "target_lang_sense_clarification": "крупный зуб животного"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b326038c-ce42-3a19-b4fe-85463f8d9ef0",
                    "sense_definition": "To shave or cut hair using a razor.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The barber <w>razored</w> the hair on the back of his neck for a clean look.",
                            "target_lang_translations": {
                                "ru": "Парикмахер <w>подбрил</w> волосы у него на шее для аккуратного вида."
                            }
                        },
                        {
                            "text": "He <w>razored</w> off his beard to surprise his family.",
                            "target_lang_translations": {
                                "ru": "Он <w>сбрил</w> бороду, чтобы удивить семью."
                            }
                        }
                    ],
                    "synonyms": [

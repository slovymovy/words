                {
                    "sense_id": "d644606b-93ef-370c-9e4d-fbd17ac59cb7",
                    "sense_definition": "In bezit hebben; eigenaar zijn van iets.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <heb> een nieuwe fiets gekregen voor mijn verjaardag.",
                            "target_lang_translations": {
                                "en": "I <have> received a new bike for my birthday.",
                                "ru": "Я <получил> новый велосипед на день рождения."
                            }
                        },
                        {
                            "text": "Hebt u even tijd om mij te helpen?",
                            "target_lang_translations": {
                                "en": "Do you <have> a moment to help me?",
                                "ru": "У вас <есть> минутка, чтобы помочь мне?"
                            }

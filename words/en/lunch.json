                    "common_phrases": [
                        "lunch out",
                        "lunch with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Есть обед; принимать пищу в середине дня"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обедать",
                                "target_lang_sense_clarification": "принимать пищу днем"
                            },
                            {
                                "target_lang_word": "пообедать",
                                "target_lang_sense_clarification": "совершить прием пищи"
                            }
                        ]

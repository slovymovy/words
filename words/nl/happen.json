    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c98654cc-9887-3524-974b-89f92be32037",
                    "sense_definition": "Een snelle beweging met de mond maken om iets te pakken of te bijten, of om lucht binnen te krijgen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond <w>hapte</w> agressief naar de postbode.",
                            "target_lang_translations": {
                                "en": "The dog <w>snapped</w> aggressively at the mailman."
                            }
                        },
                        {
                            "text": "Na de lange sprint stond de atleet naar adem te <w>happen</w>.",
                            "target_lang_translations": {
                                "en": "After the long sprint, the athlete was <w>gasping</w> for breath."
                            }
                        }
                    ],
                    "synonyms": [
                        "bijten",
                        "snappen"
                    ],
                    "common_phrases": [
                        "naar adem happen",
                        "in het stof happen",
                        "naar iets happen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a quick movement with the mouth to catch or bite something, or to inhale air."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "snap",
                                "target_lang_sense_clarification": "movement to bite"
                            },
                            {
                                "target_lang_word": "gasp",
                                "target_lang_sense_clarification": "for air"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a2554d1-e9c5-3eca-a947-c45ecc9f67b3",
                    "sense_definition": "Iets eten, vaak op een snelle of informele manier.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zullen we vanavond in de stad wat gaan <w>happen</w>?",
                            "target_lang_translations": {
                                "en": "Shall we go <w>grab a bite</w> in the city tonight?"
                            }
                        },
                        {
                            "text": "Hij <w>hapte</w> snel een broodje weg voordat de trein vertrok.",
                            "target_lang_translations": {
                                "en": "He quickly <w>ate</w> a sandwich before the train left."
                            }
                        }
                    ],
                    "synonyms": [

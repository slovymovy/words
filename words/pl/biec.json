                            {
                                "target_lang_word": "бежать",
                                "target_lang_sense_clarification": "о тропинке или реке"
                            },
                            {
                                "target_lang_word": "тянуться",
                                "target_lang_sense_clarification": "простираться на расстояние"
                            }

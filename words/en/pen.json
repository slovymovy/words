                {
                    "sense_id": "12f75f7e-eff9-32aa-a843-799e8abed42b",
                    "sense_definition": "A tool, usually a small tubular instrument containing ink, used to write or make marks.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "writing_related",
                    "name_type": "no",

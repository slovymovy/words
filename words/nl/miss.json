                },
                {
                    "sense_id": "d08bf620-aae3-34f8-b498-48db20bb89f7",
                    "sense_definition": "Een meisje of jonge vrouw met veel eigendunk en een arrogante houding.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doe niet zo uit de hoogte, wat een <w>miss</w> ben jij zeg.",
                            "target_lang_translations": {
                                "en": "Don't be so high and mighty, what a <w>madam</w> you are.",
                                "ru": "Не будь такой высокомерной, ну и <w>фифа</w> же ты."
                            }
                        },
                        {
                            "text": "Ze gedraagt zich als een echte <w>miss</w> en kijkt op iedereen neer.",
                            "target_lang_translations": {
                                "en": "She behaves like a real <w>princess</w> and looks down on everyone.",
                                "ru": "Она ведет себя как настоящая <w>цаца</w> и смотрит на всех свысока."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wat een miss",
                        "een echte miss"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A girl or young woman with a lot of conceit and an arrogant attitude.",
                        "ru": "Девушка или молодая женщина с большим самомнением и высокомерным поведением."
                    },
                    "translations": {

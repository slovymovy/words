{
    "word_family": [
        "odstawanie",
        "odstający"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0aac0c6c-59b9-3f96-9e0e-e8b0eb960dc4",
                    "sense_definition": "Nie przylegać ściśle do powierzchni; być odchylonym lub oddzielonym od całości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy płaszcz źle leży i brzydko <w>odstaje</w> na plecach.",
                            "target_lang_translations": {
                                "ru": "Моё новое пальто плохо сидит и некрасиво <w>оттопыривается</w> на спине."
                            }
                        },
                        {
                            "text": "Zauważyłem, że listwa podłogowa zaczyna <w>odstawać</w> w kilku miejscach od ściany.",
                            "target_lang_translations": {
                                "ru": "Я заметил, что плинтус начинает <w>отходить</w> от стены в нескольких местах."
                            }
                        }
                    ],
                    "synonyms": [
                        "sterczeć",
                        "odchodzić"
                    ],
                    "antonyms": [
                        "przylegać"
                    ],
                    "common_phrases": [
                        "odstające uszy",
                        "odstająca kieszeń",
                        "odstawać od ściany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не прилегать плотно к поверхности; оттопыриваться или отходить в сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оттопыриваться",
                                "target_lang_sense_clarification": "об одежде, ушах или карманах"
                            },
                            {
                                "target_lang_word": "отходить",
                                "target_lang_sense_clarification": "отделяться от поверхности"
                            },
                            {
                                "target_lang_word": "торчать",
                                "target_lang_sense_clarification": "выдаваться вперед или в сторону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14613ed2-f3a6-37ac-bbe3-a5a3703cca98",
                    "sense_definition": "Wyraźnie różnić się od innych osób, rzeczy lub przyjętych norm.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego staromodny ubiór sprawiał, że mocno <w>odstawał</w> od reszty towarzystwa.",
                            "target_lang_translations": {
                                "ru": "Из-за старомодной одежды он сильно <w>выделялся</w> на фоне остальной компании."
                            }
                        },
                        {
                            "text": "Ten nowoczesny biurowiec będzie <w>odstawać</w> stylem od zabytkowej zabudowy rynku.",
                            "target_lang_translations": {
                                "ru": "Это современное офисное здание будет <w>выбиваться</w> по стилю из исторической застройки рыночной площади."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyróżniać się",
                        "różnić się",
                        "odbiegać"
                    ],
                    "antonyms": [
                        "pasować"
                    ],
                    "common_phrases": [
                        "odstawać od reszty",
                        "odstawać od normy",
                        "odstawać od rzeczywistości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заметно отличаться от других людей, предметов или принятых норм; выделяться на общем фоне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выделяться",
                                "target_lang_sense_clarification": "быть заметным на фоне других"
                            },
                            {
                                "target_lang_word": "выбиваться",
                                "target_lang_sense_clarification": "не соответствовать общему стилю или ряду"
                            },
                            {
                                "target_lang_word": "отличаться",
                                "target_lang_sense_clarification": "иметь различия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20c777d7-3997-3fd7-9ff0-e514d1046f05",
                    "sense_definition": "Nie nadążać za innymi w rozwoju, pracy lub rywalizacji; mieć gorsze wyniki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polski przemysł nie chce <w>odstawać</w> od światowych liderów innowacji.",
                            "target_lang_translations": {
                                "ru": "Польская промышленность не хочет <w>отставать</w> от мировых лидеров инноваций."
                            }
                        },
                        {
                            "text": "Mimo zmęczenia zawodnik starał się nie <w>odstawać</w> od głównej grupy biegaczy.",
                            "target_lang_translations": {
                                "ru": "Несмотря на усталость, спортсмен старался не <w>отставать</w> от основной группы бегунов."
                            }
                        }
                    ],
                    "synonyms": [
                        "zostawać w tyle",
                        "nie nadążać"
                    ],
                    "antonyms": [
                        "dorównywać",
                        "przodować"
                    ],
                    "common_phrases": [
                        "odstawać poziomem",
                        "odstawać od czołówki",
                        "odstawać od konkurencji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не поспевать за другими в развитии, работе или соревновании; показывать худшие результаты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отставать",
                                "target_lang_sense_clarification": "быть позади в развитии или движении"
                            },
                            {
                                "target_lang_word": "уступать",
                                "target_lang_sense_clarification": "быть хуже по уровню или качеству"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
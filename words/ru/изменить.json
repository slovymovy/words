                    ],
                    "common_phrases": [
                        "память изменила",
                        "чутьё изменило",
                        "вкус изменил",
                        "силы изменили"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in literary or expressive contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stop functioning or serving reliably (regarding memory, feelings, intuition, or self-control)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fail",
                                "target_lang_sense_clarification": "when a faculty, quality, or sense stops functioning properly"

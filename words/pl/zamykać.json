    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c3ab0396-3b21-3b98-85a5-fd823ae8db54",
                    "sense_definition": "Powodować, że otwarta przestrzeń, otwór lub przedmiot przestaje być otwarty; łączyć krawędzie czegoś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <w>zamykam</w> okna przed wyjściem z domu, żeby nie naleciały owady.",
                            "target_lang_translations": {
                                "ru": "Я всегда <w>закрываю</w> окна перед выходом из дома, чтобы не налетели насекомые."
                            }
                        },
                        {
                            "text": "Proszę, <w>zamykaj</w> drzwi cicho, bo dziecko śpi w drugim pokoju.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>закрывай</w> дверь тихо, потому что ребенок спит в другой комнате."
                            }
                        }
                    ],
                    "antonyms": [
                        "otwierać"
                    ],
                    "common_phrases": [
                        "zamykać drzwi",
                        "zamykać oczy",
                        "zamykać okno",
                        "zamykać książkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать так, чтобы открытое пространство, отверстие или предмет перестали быть открытыми; соединять края чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрывать",
                                "target_lang_sense_clarification": "общее значение (окна, двери, книги)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0de3e5f3-eacb-3c90-bbae-e098d7e5e96f",
                    "sense_definition": "Zabezpieczać drzwi, bramę lub pojemnik za pomocą zamka i klucza, aby uniemożliwić ich otwarcie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorem zawsze <w>zamykamy</w> drzwi wejściowe na dwa spusty dla bezpieczeństwa.",
                            "target_lang_translations": {
                                "ru": "Вечером мы всегда <w>запираем</w> входную дверь на два оборота для безопасности."
                            }
                        },
                        {
                            "text": "Musisz <w>zamykać</w> szafkę na klucz, jeśli trzymasz tam ważne dokumenty.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>запирать</w> шкафчик на ключ, если хранишь там важные документы."
                            }
                        }
                    ],
                    "synonyms": [
                        "ryglować"
                    ],
                    "antonyms": [
                        "otwierać"
                    ],
                    "common_phrases": [
                        "zamykać na klucz",
                        "zamykać na kłódkę",
                        "zamykać dom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запирать дверь, ворота или контейнер с помощью замка и ключа, чтобы предотвратить их открытие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запирать",
                                "target_lang_sense_clarification": "на замок"
                            },
                            {
                                "target_lang_word": "закрывать",
                                "target_lang_sense_clarification": "на ключ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65d826c6-7347-3111-a54a-1b32974c1663",
                    "sense_definition": "<w>zamykać się</w> – Przestawać być otwartym (o drzwiach, oknie, sklepie itp.); ulegać zamknięciu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Te nowoczesne drzwi w biurze <w>zamykają się</w> automatycznie po kilku sekundach.",
                            "target_lang_translations": {
                                "ru": "Эти современные двери в офисе <w>закрываются</w> автоматически через несколько секунд."
                            }
                        },
                        {
                            "text": "Sklep <w>zamyka się</w> za piętnaście minut, więc musimy się pospieszyć z zakupami.",
                            "target_lang_translations": {
                                "ru": "Магазин <w>закрывается</w> через пятнадцать минут, так что нам нужно поторопиться с покупками."
                            }
                        }
                    ],
                    "antonyms": [
                        "otwierać się"
                    ],
                    "common_phrases": [
                        "zamykać się automatycznie",
                        "zamykać się z trzaskiem",
                        "zamykać się samoczynnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать быть открытым (о дверях, окне, магазине и т.д.); поддаваться закрытию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрываться",
                                "target_lang_sense_clarification": "возвратный глагол"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24436832-a555-337d-b5f6-5e605152d58c",
                    "sense_definition": "Kończyć działalność, proces, spotkanie lub funkcjonowanie czegoś (np. firmy, konta, dyskusji).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ending_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właściciele <w>zamykają</w> ten stary sklep, ponieważ nie przynosi już zysków.",
                            "target_lang_translations": {
                                "ru": "Владельцы <w>закрывают</w> этот старый магазин, так как он больше не приносит прибыли."
                            }
                        },
                        {
                            "text": "Chciałbym już <w>zamykać</w> ten temat i przejść do kolejnego punktu zebrania.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы уже <w>закрывать</w> эту тему и переходить к следующему пункту собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "kończyć",
                        "likwidować"
                    ],
                    "antonyms": [
                        "otwierać",
                        "uruchamiać"
                    ],
                    "common_phrases": [
                        "zamykać sklep",
                        "zamykać konto",
                        "zamykać dyskusję",
                        "zamykać sprawę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекращать деятельность, процесс, встречу или функционирование чего-либо (например, фирмы, счета, дискуссии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрывать",
                                "target_lang_sense_clarification": "прекращать деятельность или обсуждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0adf29c9-a9e5-3500-b38a-8fed3225808c",
                    "sense_definition": "Wyłączać program komputerowy, aplikację lub okno w systemie operacyjnym.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ending_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze <w>zamykaj</w> nieużywane aplikacje, aby oszczędzać baterię w telefonie.",
                            "target_lang_translations": {
                                "ru": "Всегда <w>закрывай</w> неиспользуемые приложения, чтобы экономить заряд батареи в телефоне."
                            }
                        },
                        {
                            "text": "System operacyjny <w>zamyka</w> programy, które nie odpowiadają przez dłuższy czas.",
                            "target_lang_translations": {
                                "ru": "Операционная система <w>закрывает</w> программы, которые не отвечают в течение долгого времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyłączać"
                    ],
                    "antonyms": [
                        "uruchamiać",
                        "otwierać"
                    ],
                    "common_phrases": [
                        "zamykać okno",
                        "zamykać program",
                        "zamykać aplikację",
                        "zamykać plik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выключать компьютерную программу, приложение или окно в операционной системе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрывать",
                                "target_lang_sense_clarification": "программы, окна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d5d0917-d80f-3255-ba41-7982eade612f",
                    "sense_definition": "<w>zamykać się</w> – Stawać się niedostępnym dla otoczenia, unikać kontaktów z ludźmi, ukrywać swoje uczucia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po trudnych przeżyciach Marek często <w>zamyka się</w> w sobie i z nikim nie rozmawia.",
                            "target_lang_translations": {
                                "ru": "После тяжелых переживаний Марек часто <w>замыкается</w> в себе и ни с кем не разговаривает."
                            }
                        },
                        {
                            "text": "Nie warto <w>zamykać się</w> na nowe znajomości, bo można przegapić coś wartościowego.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>закрываться</w> от новых знакомств, так как можно упустить что-то ценное."
                            }
                        }
                    ],
                    "synonyms": [
                        "izolować się"
                    ],
                    "antonyms": [
                        "otwierać się"
                    ],
                    "common_phrases": [
                        "zamykać się w sobie",
                        "zamykać się na świat",
                        "zamykać się w pokoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться недоступным для окружения, избегать контактов с людьми, скрывать свои чувства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замыкаться",
                                "target_lang_sense_clarification": "в себе"
                            },
                            {
                                "target_lang_word": "закрываться",
                                "target_lang_sense_clarification": "эмоционально отгораживаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a18e45fb-6740-3717-a899-8a577ad2f255",
                    "sense_definition": "Pozbawiać kogoś wolności, umieszczając go w więzieniu lub areszcie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "confinement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja coraz częściej <w>zamyka</w> kierowców prowadzących samochód pod wpływem alkoholu.",
                            "target_lang_translations": {
                                "ru": "Полиция всё чаще <w>сажает</w> водителей, управляющих автомобилем в нетрезвом виде."
                            }
                        },
                        {
                            "text": "W tym kraju rzadko <w>zamykają</w> ludzi za tak drobne wykroczenia.",
                            "target_lang_translations": {
                                "ru": "В этой стране редко <w>сажают</w> людей за такие мелкие правонарушения."
                            }
                        }
                    ],
                    "synonyms": [
                        "aresztować"
                    ],
                    "antonyms": [
                        "wypuszczać"
                    ],
                    "common_phrases": [
                        "zamykać w więzieniu",
                        "zamykać do aresztu",
                        "zamykać za kratkami"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech instead of 'aresztować' or 'więzić'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишать кого-либо свободы, помещая его в тюрьму или под арест."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сажать",
                                "target_lang_sense_clarification": "разговорное (в тюрьму)"
                            },
                            {
                                "target_lang_word": "закрывать",
                                "target_lang_sense_clarification": "жаргонное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "191fb224-5fe5-3fd1-853c-b7e5dd6471be",
                    "sense_definition": "Znajdować się na samym końcu szeregu, listy lub grupy; stanowić ostatni element.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten zawodnik niestety <w>zamyka</w> stawkę i ma małe szanse na medal.",
                            "target_lang_translations": {
                                "ru": "Этот спортсмен, к сожалению, <w>замыкает</w> группу и имеет мало шансов на медаль."
                            }
                        },
                        {
                            "text": "Nasza drużyna <w>zamyka</w> tabelę ligową po serii przegranych meczów.",
                            "target_lang_translations": {
                                "ru": "Наша команда <w>замыкает</w> турнирную таблицу после серии проигранных матчей."
                            }
                        }
                    ],
                    "antonyms": [
                        "otwierać"
                    ],
                    "common_phrases": [
                        "zamykać listę",
                        "zamykać stawkę",
                        "zamykać pochód",
                        "zamykać tabelę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в самом конце ряда, списка или группы; составлять последний элемент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замыкать",
                                "target_lang_sense_clarification": "шествие, список, таблицу"
                            }
                        ]
                    }
                }
            ]
        }
    ]

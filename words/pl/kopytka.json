    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ca5c395b-5708-3839-b6c1-8b2372f4f295",
                    "sense_definition": "Kluski z gotowanych ziemniaków i mąki pszennej w kształcie rombów, uzyskiwane przez ukośne cięcie wałków ciasta.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "culinary_dish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na dzisiejszy obiad mama przygotowała pyszne <w>kopytka</w> z sosem pieczarkowym.",
                            "target_lang_translations": {
                                "ru": "На сегодняшний обед мама приготовила вкусные <w>копытка</w> с грибным соусом."
                            }
                        },
                        {
                            "text": "Najbardziej lubię odsmażane <w>kopytka</w> z bułką tartą i cukrem.",
                            "target_lang_translations": {
                                "ru": "Больше всего я люблю поджаренные <w>копытка</w> с панировочными сухарями и сахаром."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Poznań) szagówki"
                    ],
                    "common_phrases": [
                        "kopytka z sosem",
                        "kopytka z okrasą",
                        "domowe kopytka",
                        "kopytka ziemniaczane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Клецки в форме ромбов из вареного картофеля и пшеничной муки, которые делают, нарезая раскатанное тесто по диагонали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копытка",
                                "target_lang_sense_clarification": "польское национальное блюдо"
                            },
                            {
                                "target_lang_word": "картофельные клёцки",
                                "target_lang_sense_clarification": "кулинарное изделие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "871709bb-1fbd-3de8-9d2c-5a1e1989a9e7",
                    "sense_definition": "Forma mianownika, biernika lub wołacza liczby mnogiej od słowa <w>kopytko</w> (małe kopyto lub narzędzie szewskie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "form_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe koźlę miało delikatne <w>kopytka</w>, którymi niepewnie stukało o podłogę.",
                            "target_lang_translations": {
                                "ru": "У маленького козленка были нежные <w>копытца</w>, которыми он неуверенно стучал по полу."
                            }
                        },
                        {
                            "text": "Na świeżym śniegu myśliwy zauważył drobne <w>kopytka</w> sarny.",
                            "target_lang_translations": {
                                "ru": "На свежем снегу охотник заметил мелкие <w>копытца</w> косули."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма множественного числа слова kopytko (маленькое копыто)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копытца",
                                "target_lang_sense_clarification": "уменьшительное от копыта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4df1a368-c2b2-3c9a-ac64-ab1b24501d23",
                    "sense_definition": "Forma dopełniacza liczby pojedynczej od słowa <w>kopytko</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "form_genitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szewc nie mógł znaleźć swojego ulubionego <w>kopytka</w> do formowania butów.",
                            "target_lang_translations": {
                                "ru": "Сапожник не мог найти своего любимого <w>копытца</w> для формовки обуви."
                            }
                        },
                        {
                            "text": "Bez tego małego <w>kopytka</w> naprawa obcasa będzie znacznie trudniejsza.",
                            "target_lang_translations": {
                                "ru": "Без этого маленького <w>копытца</w> ремонт каблука будет значительно сложнее."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive singular form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа единственного числа слова kopytko (часто в значении сапожного инструмента)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копытца",
                                "target_lang_sense_clarification": "форма слова или инструмент"
                            },
                            {
                                "target_lang_word": "сапожной лапы",
                                "target_lang_sense_clarification": "инструмент сапожника"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
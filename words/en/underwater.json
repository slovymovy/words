    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "acd1ebb0-647c-3bb3-bbea-2aa2f30d1446",
                    "sense_definition": "Located, occurring, or used beneath the surface of the water.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The diver discovered an ancient city while exploring the <w>underwater</w> ruins.",
                            "target_lang_translations": {
                                "ru": "Дайвер обнаружил древний город, исследуя <w>подводные</w> руины."
                            }
                        },
                        {
                            "text": "We used a special <w>underwater</w> camera to take pictures of the turtles.",
                            "target_lang_translations": {
                                "ru": "Мы использовали специальную <w>подводную</w> камеру, чтобы сфотографировать черепах."
                            }
                        }
                    ],
                    "synonyms": [
                        "subaquatic",
                        "submarine"
                    ],
                    "antonyms": [
                        "above-water"
                    ],
                    "common_phrases": [
                        "underwater world",
                        "underwater camera",
                        "underwater exploration",
                        "underwater swimming"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся, происходящий или используемый под поверхностью воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подводный",
                                "target_lang_sense_clarification": "основное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bccc6c4a-cd55-3484-83d6-685281cd3413",
                    "sense_definition": "Having a loan balance that is higher than the current market value of the property or asset securing the loan.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "financial_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many families were left <w>underwater</w> on their mortgages after the economic crisis.",
                            "target_lang_translations": {
                                "ru": "Многие семьи остались <w>с отрицательным капиталом</w> по ипотеке после экономического кризиса."
                            }
                        },
                        {
                            "text": "It is stressful to be <w>underwater</w> and owe the bank more than the house is worth.",
                            "target_lang_translations": {
                                "ru": "Стрессово быть <w>в минусе</w> и быть должным банку больше, чем стоит дом."
                            }
                        }
                    ],
                    "synonyms": [
                        "upside down"
                    ],
                    "antonyms": [
                        "solvent#Adjective",
                        "in the black"
                    ],
                    "common_phrases": [
                        "underwater mortgage",
                        "underwater on a loan"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий задолженность по кредиту, превышающую текущую рыночную стоимость актива (обычно недвижимости)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с отрицательным капиталом",
                                "target_lang_sense_clarification": "финансовый термин"
                            },
                            {
                                "target_lang_word": "в минусе",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6fcdb85-502d-3e51-8331-a9e9422db1c7",
                    "sense_definition": "Of a financial option, having a strike price that makes it unprofitable to exercise at the current market price.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "financial_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The stock options are currently <w>underwater</w> because the share price has fallen significantly.",
                            "target_lang_translations": {
                                "ru": "Опционы на акции сейчас находятся <w>вне денег</w>, так как цена акций значительно упала."
                            }
                        },
                        {
                            "text": "Investors often worry when their call options become <w>underwater</w> before the expiration date.",
                            "target_lang_translations": {
                                "ru": "Инвесторы часто беспокоятся, когда их колл-опционы становятся <w>нерентабельными</w> до даты истечения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "underwater option"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "finance jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О финансовом опционе: имеющий цену исполнения, которая делает его реализацию невыгодной при текущей рыночной цене."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вне денег",
                                "target_lang_sense_clarification": "биржевой термин (out of the money)"
                            },
                            {
                                "target_lang_word": "нерентабельный",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9b6e46ee-4af8-3752-b654-658b8361e200",
                    "sense_definition": "In a way that goes or stays beneath the surface of the water.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She took a deep breath and began to swim <w>underwater</w> across the pool.",
                            "target_lang_translations": {
                                "ru": "Она глубоко вздохнула и поплыла <w>под водой</w> через бассейн."
                            }
                        },
                        {
                            "text": "The submarine can stay <w>underwater</w> for weeks without needing to surface.",
                            "target_lang_translations": {
                                "ru": "Подводная лодка может оставаться <w>под водой</w> неделями, не всплывая на поверхность."
                            }
                        }
                    ],
                    "common_phrases": [
                        "swim underwater",
                        "go underwater",
                        "stay underwater"
                    ],
                    "target_lang_definitions": {
                        "ru": "Таким образом, что действие происходит или состояние сохраняется под поверхностью воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "под водой",
                                "target_lang_sense_clarification": "наречие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {

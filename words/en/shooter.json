    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "916149fa-64b7-3ef2-a4cb-558e1e42e2fa",
                    "sense_definition": "Someone who shoots a gun or other weapon, such as a gunner or archer.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police are currently searching the area for the active <w>shooter</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция в данный момент прочесывает район в поисках активного <w>стрелка</w>."
                            }
                        },
                        {
                            "text": "He is a skilled <w>shooter</w> who rarely misses his target at the range.",
                            "target_lang_translations": {
                                "ru": "Он искусный <w>стрелок</w>, который редко промахивается по мишени на стрельбище."
                            }
                        }
                    ],
                    "synonyms": [
                        "marksman"
                    ],
                    "common_phrases": [
                        "active shooter",
                        "sharp shooter",
                        "mass shooter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто стреляет из ружья или другого оружия, например, артиллерист или лучник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стрелок",
                                "target_lang_sense_clarification": "человек, стреляющий из оружия"
                            },
                            {
                                "target_lang_word": "снайпер",
                                "target_lang_sense_clarification": "меткий стрелок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecc97b69-9b3d-3be6-9d85-91eb10b83608",
                    "sense_definition": "A video game in which the main objective is shooting enemies or targets.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My favorite type of video game is a first-person <w>shooter</w> set in space.",
                            "target_lang_translations": {
                                "ru": "Мой любимый жанр видеоигр — <w>шутер</w> от первого лица, действие которого происходит в космосе."
                            }
                        },
                        {
                            "text": "We stayed up all night playing a multiplayer <w>shooter</w> on the console.",
                            "target_lang_translations": {
                                "ru": "Мы не спали всю ночь, играя в многопользовательский <w>шутер</w> на приставке."
                            }
                        }
                    ],
                    "synonyms": [
                        "shoot-'em-up"
                    ],
                    "common_phrases": [
                        "first-person shooter",
                        "tactical shooter",
                        "space shooter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Видеоигра, в которой главная цель — стрелять во врагов или мишени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шутер",
                                "target_lang_sense_clarification": "жанр видеоигр"
                            },
                            {
                                "target_lang_word": "стрелялка",
                                "target_lang_sense_clarification": "разговорное название жанра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b776d89f-b558-3063-ac8b-d57810db87e7",
                    "sense_definition": "An alcoholic beverage typically served in a small glass and drunk in one swallow.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bartender lined up a tequila <w>shooter</w> for each of the guests.",
                            "target_lang_translations": {
                                "ru": "Бармен выставил в ряд по <w>шоту</w> текилы для каждого из гостей."
                            }
                        },
                        {
                            "text": "She ordered a sweet <w>shooter</w> to celebrate the start of her vacation.",
                            "target_lang_translations": {
                                "ru": "Она заказала сладкий <w>шот</w>, чтобы отпраздновать начало отпуска."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tequila shooter",
                        "oyster shooter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Алкогольный напиток, который обычно подают в маленькой рюмке и выпивают одним глотком."
                    },
                    "translations": {

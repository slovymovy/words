    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "31cab4e0-fc3a-39e4-af2c-eb7121ea8a2a",
                    "sense_definition": "Biec lub szybko poruszać się za kimś lub za czymś, aby to złapać, doścignąć lub zatrzymać.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pies zerwał się ze smyczy i zaczął <gonić> kota sąsiadów.",
                            "target_lang_translations": {
                                "ru": "Собака сорвалась с поводка и начала <гнаться> за соседским котом."
                            }
                        },
                        {
                            "text": "Policjant musiał długo <gonić> złodzieja, zanim udało mu się go aresztować.",
                            "target_lang_translations": {
                                "ru": "Полицейскому пришлось долго <преследовать> вора, прежде чем удалось его арестовать."
                            }
                        }
                    ],
                    "synonyms": [
                        "ścigać"
                    ],
                    "antonyms": [
                        "uciekać"
                    ],
                    "common_phrases": [
                        "gonić złodzieja",
                        "gonić uciekiniera",
                        "gonić wiatru w polu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бежать за кем-то или чем-то, пытаясь поймать, настичь или остановить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнаться",
                                "target_lang_sense_clarification": "бежать следом с целью поймать"
                            },
                            {
                                "target_lang_word": "преследовать",
                                "target_lang_sense_clarification": "неотступно следовать за кем-то"
                            },
                            {
                                "target_lang_word": "догонять",
                                "target_lang_sense_clarification": "приближаться к убегающему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d2b595e-3d6d-3a1a-a524-50b67984c3a4",
                    "sense_definition": "Bawić się w bieganie za sobą nawzajem (zwykle z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "play",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci wybiegły na podwórko, żeby <gonić> się w berka.",
                            "target_lang_translations": {
                                "ru": "Дети выбежали во двор, чтобы <играть> в догонялки."
                            }
                        },
                        {
                            "text": "Dwa małe szczeniaki lubią <gonić> się po całej łące.",
                            "target_lang_translations": {
                                "ru": "Два маленьких щенка любят <гоняться> друг за другом по всему лугу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gonić się w berka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Играть, бегая друг за другом (обычно используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гоняться",
                                "target_lang_sense_clarification": "бегать друг за другом"
                            },
                            {
                                "target_lang_word": "бегать",
                                "target_lang_sense_clarification": "быстро перемещаться (в игре)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a5e2f50-f2cc-33d6-a323-95e09c36791a",
                    "sense_definition": "Spieszyć się, aby zdążyć z czymś na czas, lub usilnie starać się coś osiągnąć.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_pursuit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <gonić> terminy, jeśli chcemy oddać ten projekt przed końcem miesiąca.",
                            "target_lang_translations": {
                                "ru": "Мы должны <нагонять> сроки, если хотим сдать этот проект до конца месяца."
                            }
                        },
                        {
                            "text": "Przez całe życie próbował <gonić> swoje niespełnione marzenia.",
                            "target_lang_translations": {
                                "ru": "Всю жизнь он пытался <гнаться> за своими несбывшимися мечтами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czas goni",
                        "gonić marzenia",
                        "gonić terminy",
                        "gonić resztkami sił"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спешить, чтобы успеть вовремя, или настойчиво стремиться к достижению цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнаться",
                                "target_lang_sense_clarification": "перен. стремиться достичь чего-то (мечты)"
                            },
                            {
                                "target_lang_word": "нагонять",
                                "target_lang_sense_clarification": "стараться успеть, наверстать (сроки)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27df3530-1e14-39bd-954c-1fa31c37f381",
                    "sense_definition": "Zmuszać zwierzęta do przemieszczania się w określonym kierunku; pędzić stado.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "herding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz codziennie rano <goni> krowy na pastwisko pod lasem.",
                            "target_lang_translations": {
                                "ru": "Хозяин каждое утро <гонит> коров на пастбище у леса."
                            }
                        },
                        {
                            "text": "Pasterze zaczęli <gonić> owce do zagrody przed nadchodzącą burzą.",
                            "target_lang_translations": {
                                "ru": "Пастухи начали <гнать> овец в загон перед надвигающейся грозой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gonić bydło",
                        "gonić krowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять животных двигаться в нужном направлении; пасти стадо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнать",
                                "target_lang_sense_clarification": "заставлять идти или бежать (скот)"
                            },
                            {
                                "target_lang_word": "погонять",
                                "target_lang_sense_clarification": "понукать к движению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2fee57f-d00b-32d5-9e48-a34531a02dde",
                    "sense_definition": "Sprzedawać coś, często w sposób nielegalny lub pokątny.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "trade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja zatrzymała grupę, która próbowała <gonić> fałszywe bilety pod stadionem.",
                            "target_lang_translations": {
                                "ru": "Полиция задержала группу, которая пыталась <толкать> фальшивые билеты у стадиона."
                            }
                        },
                        {
                            "text": "Niektórzy próbują <gonić> trefny towar na lokalnym bazarze.",
                            "target_lang_translations": {
                                "ru": "Некоторые пытаются <загонять> паленый товар на местном рынке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gonić towar"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Продавать что-либо, часто нелегально или с рук (слэнг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толкать",
                                "target_lang_sense_clarification": "слэнг: продавать, сбывать"
                            },
                            {
                                "target_lang_word": "загонять",
                                "target_lang_sense_clarification": "слэнг: продавать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efac48a0-0793-3e94-b5c5-1ba4033c536c",
                    "sense_definition": "Chodzić bez celu, wałęsać się lub spędzać czas na bezczynności.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zamiast siedzieć w domu i się uczyć, wolisz <gonić> po mieście z kolegami.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы сидеть дома и учиться, ты предпочитаешь <носиться> по городу с друзьями."
                            }
                        },
                        {
                            "text": "Cały dzień <gonił> po ulicach, nie wiedząc, co ze sobą zrobić.",
                            "target_lang_translations": {
                                "ru": "Весь день он <бегал> по улицам, не зная, чем себя занять."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Kraków"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бесцельно ходить, слоняться или проводить время в беготне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носиться",
                                "target_lang_sense_clarification": "разг. бегать без дела"
                            },
                            {
                                "target_lang_word": "бегать",
                                "target_lang_sense_clarification": "суетиться, ходить туда-сюда"
                            },
                            {
                                "target_lang_word": "мотаться",
                                "target_lang_sense_clarification": "разг. ходить или ездить без передышки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
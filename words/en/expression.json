                    ],
                    "synonyms": [
                        "phrase",
                        "idiom",
                        "saying",
                        "term"
                    ],
                    "common_phrases": [
                        "common expression",
                        "idiomatic expression",
                        "use an expression"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово или группа слов, используемые определенным образом, часто имеющие особое значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выраже́ние",
                                "target_lang_sense_clarification": "фраза"
                            },
                            {
                                "target_lang_word": "оборо́т",
                                "target_lang_sense_clarification": "речевой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb2d3eef-82d3-3f7d-aac7-558b02d53b21",
                    "sense_definition": "The act of showing or communicating thoughts, feelings, or ideas.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "act_of_expressing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dance is a powerful form of self-<expression>.",
                            "target_lang_translations": {
                                "ru": "Танец — это мощная форма <w>самовыражения</w>."
                            }
                        },
                        {
                            "text": "Please accept this gift as an <expression> of my gratitude.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, примите этот подарок как <w>выражение</w> моей благодарности."
                            }
                        }

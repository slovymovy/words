{
    "word_family": [
        "quantify",
        "quantitative",
        "quantification",
        "quantifier"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b98acc8d-9f17-3512-95ca-c6ec1cbdb3f7",
                    "sense_definition": "An indefinite or specific amount or number of something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "amount_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to order a large <w>quantity</w> of paper for the office.",
                            "target_lang_translations": {
                                "ru": "Нам нужно заказать большое <w>количество</w> бумаги для офиса."
                            }
                        },
                        {
                            "text": "A small <w>quantity</w> of oil is sufficient for this recipe.",
                            "target_lang_translations": {
                                "ru": "Для этого рецепта достаточно небольшого <w>количества</w> масла."
                            }
                        }
                    ],
                    "synonyms": [
                        "amount"
                    ],
                    "common_phrases": [
                        "large quantity",
                        "small quantity",
                        "quantity of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопределенное или конкретное число, объем или размер чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "количество",
                                "target_lang_sense_clarification": "объем или число"
                            },
                            {
                                "target_lang_word": "объём",
                                "target_lang_sense_clarification": "величина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8c2f185-b6cf-36d3-a35e-19d55958be0a",
                    "sense_definition": "The aspect of something that deals with how much of it there is, often contrasted with how good it is.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "amount_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You have to choose between <w>quantity</w> and quality when buying clothes.",
                            "target_lang_translations": {
                                "ru": "При покупке одежды приходится выбирать между <w>количеством</w> и качеством."
                            }
                        },
                        {
                            "text": "The factory focuses on <w>quantity</w> rather than the standard of the product.",
                            "target_lang_translations": {
                                "ru": "Фабрика делает упор на <w>количество</w>, а не на стандарты продукции."
                            }
                        }
                    ],
                    "synonyms": [
                        "magnitude"
                    ],
                    "common_phrases": [
                        "quality over quantity",
                        "quality and quantity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеристика, указывающая на численность или объем, часто в противопоставлении качеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "количество",
                                "target_lang_sense_clarification": "в противопоставлении качеству"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1834efe3-1cde-37bc-9ce9-a6cf94d547cb",
                    "sense_definition": "A large or considerable amount of something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "amount_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new aircraft model was the first to be produced in <w>quantity</w>.",
                            "target_lang_translations": {
                                "ru": "Новая модель самолета стала первой, которую начали производить в больших <w>количествах</w>."
                            }
                        },
                        {
                            "text": "They bought the materials in <w>quantity</w> to secure a better price.",
                            "target_lang_translations": {
                                "ru": "Они закупили материалы в большом <w>количестве</w>, чтобы получить лучшую цену."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in quantity",
                        "produced in quantity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой или значительный объем чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "большое количество",
                                "target_lang_sense_clarification": "значительный объем"
                            },
                            {
                                "target_lang_word": "опт",
                                "target_lang_sense_clarification": "при покупке большой партии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7d99586-6a5a-3c51-8a40-1f3ba762205d",
                    "sense_definition": "A word used in spoken mathematics to indicate that the preceding terms should be treated as a group, usually enclosed in parentheses.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher read the formula as x plus y <w>quantity</w> squared.",
                            "target_lang_translations": {
                                "ru": "Учитель прочитал формулу как икс плюс игрек, <w>всё выражение</w> в квадрате."
                            }
                        },
                        {
                            "text": "To solve the equation, you must calculate a minus b <w>quantity</w> cubed first.",
                            "target_lang_translations": {
                                "ru": "Чтобы решить уравнение, сначала нужно вычислить а минус б, <w>всё выражение</w> в кубе."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в устной математике для обозначения того, что предыдущие члены следует рассматривать как группу (в скобках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всё выражение",
                                "target_lang_sense_clarification": "математическая группировка"
                            },
                            {
                                "target_lang_word": "в скобках",
                                "target_lang_sense_clarification": "указание на порядок действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "457c2c34-a21b-3bb1-ae90-f1ba6cdd3084",
                    "sense_definition": "The relative duration or length of a sound or syllable in speech or verse.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Latin poetry, the rhythm depends heavily on syllable <w>quantity</w>.",
                            "target_lang_translations": {
                                "ru": "В латинской поэзии ритм сильно зависит от <w>долготы</w> слога."
                            }
                        },
                        {
                            "text": "Linguists study vowel <w>quantity</w> to understand the phonology of the language.",
                            "target_lang_translations": {
                                "ru": "Лингвисты изучают <w>долготу</w> гласных, чтобы понять фонологию языка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vowel quantity",
                        "syllable quantity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относительная длительность или протяженность звука или слога в речи или стихе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "долгота",
                                "target_lang_sense_clarification": "звука или слога"
                            },
                            {
                                "target_lang_word": "квантитет",
                                "target_lang_sense_clarification": "лингвистический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "72576188-2f20-3137-a51a-74a8a4720d9a",
                    "sense_definition": "3. osoba liczby mnogiej (rodzaj niemęskoosobowy) czasu przeszłego od: <w>przejść</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ciemne chmury szybko <w>przeszły</w> i znów zaświeciło słońce.",
                            "target_lang_translations": {
                                "ru": "Тёмные тучи быстро <w>прошли</w>, и снова засветило солнце."
                            }
                        },
                        {
                            "text": "Dzieci ostrożnie <w>przeszły</w> na drugą stronę ulicy.",
                            "target_lang_translations": {
                                "ru": "Дети осторожно <w>перешли</w> на другую сторону улицы."
                            }
                        }
                    ],
                    "traits": [

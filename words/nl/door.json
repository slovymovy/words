    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "126ffb2c-609b-39ed-8ec8-233ddc8f4564",
                    "sense_definition": "Via de binnenkant van iets naar de andere kant; geeft een beweging aan die een ruimte doorkruist.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij keek even snel <w>door</w> het raam naar buiten.",
                            "target_lang_translations": {
                                "en": "He took a quick look outside <w>through</w> the window."
                            }
                        },
                        {
                            "text": "Om bij de tuin te komen, moet je <w>door</w> de gang lopen.",
                            "target_lang_translations": {
                                "en": "To get to the garden, you have to walk <w>through</w> the hallway."
                            }
                        }
                    ],
                    "synonyms": [
                        "in",
                        "doorheen"
                    ],
                    "common_phrases": [
                        "door de deur",
                        "door het raam",
                        "door de stad"
                    ],
                    "target_lang_definitions": {
                        "en": "Via the inside of something to the other side; indicates a movement crossing a space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "through",
                                "target_lang_sense_clarification": "moving from one side to the other"
                            },
                            {
                                "target_lang_word": "via",
                                "target_lang_sense_clarification": "by way of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93dbf170-6b1a-3c91-8afe-8b6d84e6086b",
                    "sense_definition": "Vanwege; als gevolg van een bepaalde oorzaak of reden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "causality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We kwamen helaas te laat <w>door</w> de drukke file.",
                            "target_lang_translations": {
                                "en": "Unfortunately, we arrived too late <w>because of</w> the heavy traffic."
                            }
                        },
                        {
                            "text": "<w>Door</w> het slechte weer is de wedstrijd afgelast.",
                            "target_lang_translations": {
                                "en": "<w>Due to</w> the bad weather, the match was cancelled."
                            }
                        }
                    ],
                    "synonyms": [
                        "vanwege",
                        "ten gevolge van"
                    ],
                    "common_phrases": [
                        "door omstandigheden",
                        "door toedoen van",
                        "door gebrek aan"
                    ],
                    "target_lang_definitions": {
                        "en": "Because of; as a result of a certain cause or reason."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "because of",
                                "target_lang_sense_clarification": "on account of"
                            },
                            {
                                "target_lang_word": "due to",
                                "target_lang_sense_clarification": "attributable to"
                            },
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "indicating source or cause (e.g., weak from hunger)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be72f19d-6b57-37d7-814c-8352b800affe",
                    "sense_definition": "Geeft aan wie of wat de handeling verricht in een zin die in de lijdende vorm staat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "passive_agent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit prachtige schilderij is gemaakt <w>door</w> Rembrandt.",
                            "target_lang_translations": {
                                "en": "This beautiful painting was made <w>by</w> Rembrandt."
                            }
                        },
                        {
                            "text": "Het pakketje werd gisteren <w>door</w> de postbode bezorgd.",
                            "target_lang_translations": {
                                "en": "The package was delivered yesterday <w>by</w> the mail carrier."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gemaakt door",
                        "geschreven door",
                        "georganiseerd door"

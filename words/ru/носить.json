    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "afdc7a21-8b14-3ee2-b5d4-53452d4204a8",
                    "sense_definition": "Быть одетым во что-либо или иметь что-либо на себе (одежду, обувь, аксессуары, прическу)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "wear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой люди обычно <носят> тёплые шапки и шарфы."
                        },
                        {
                            "text": "Мой брат любит <носить> джинсы и футболки каждый день."
                        },
                        {
                            "text": "Она начала <носить> очки, когда пошла в школу."
                        }
                    ],
                    "common_phrases": [
                        "носить очки",
                        "носить бороду",
                        "носить кольцо",
                        "носить форму",
                        "носить траур"
                    ]
                },
                {
                    "sense_id": "81f4ffc3-38b8-3272-8e80-1bcd42573fea",
                    "sense_definition": "Перемещать кого-либо или что-либо, держа в руках или на себе (обычно неоднократно, туда-сюда или в разных направлениях)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Официант должен быстро <носить> еду клиентам в зале."
                        },
                        {
                            "text": "Нам пришлось <носить> воду из колодца весь день."
                        },
                        {
                            "text": "Мать любит <носить> своего ребёнка на руках."
                        }
                    ],
                    "synonyms": [
                        "таскать"
                    ],
                    "common_phrases": [
                        "носить на руках",
                        "носить воду",
                        "носить тяжести",
                        "носить с собой"
                    ]
                },
                {
                    "sense_id": "c1a4a595-72ea-3960-ac0d-2a73bacba625",
                    "sense_definition": "Иметь, обладать каким-либо признаком, именем или свойством",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "possess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта центральная улица будет <носить> имя известного поэта."
                        },
                        {
                            "text": "Их встреча <носила> чисто деловой характер."
                        },
                        {
                            "text": "Многие женщины после свадьбы начинают <носить> фамилию мужа."
                        }
                    ],
                    "common_phrases": [
                        "носить имя",
                        "носить фамилию",
                        "носить характер",
                        "носить название",
                        "носить звание"
                    ]
                },
                {
                    "sense_id": "827fb0f4-81ab-30d0-ae8c-c2bd740b69f9",
                    "sense_definition": "Быть беременной, вынашивать ребёнка",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "pregnancy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она <носит> под сердцем долгожданного сына."
                        },
                        {
                            "text": "Женщинам бывает трудно <носить> ребёнка на последних месяцах."
                        }
                    ],
                    "common_phrases": [
                        "носить ребёнка",
                        "носить под сердцем",
                        "носить плод"
                    ]
                },
                {
                    "sense_id": "5cf9a486-1784-3703-8ac1-e7bc3afffa12",
                    "sense_definition": "Быстро перемещать или гнать силой потока (о ветре, воде)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drive_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный ветер <носит> сухие листья по всему парку."
                        },
                        {
                            "text": "Лодку <носило> по волнам во время страшного шторма."
                        }
                    ],
                    "common_phrases": [

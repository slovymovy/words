{
    "word_family": [],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f494d754-dee5-3a3a-980a-bb2b51500649",
                    "sense_definition": "Słowo oznaczające, że dana rzecz, osoba lub czynność jest jedyną, o której mowa; wyklucza inne możliwości.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "exclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W portfelu mam <tylko> pięć złotych.",
                            "target_lang_translations": {
                                "ru": "В кошельке у меня <только> пять злотых."
                            }
                        },
                        {
                            "text": "Na spotkanie przyszła <tylko> jedna osoba.",
                            "target_lang_translations": {
                                "ru": "На встречу пришел <только> один человек."
                            }
                        }
                    ],
                    "synonyms": [
                        "jedynie",
                        "wyłącznie"
                    ],
                    "common_phrases": [
                        "tylko i wyłącznie",
                        "tylko jeden",
                        "tylko raz",
                        "tylko ty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, указывающее на то, что данный предмет, лицо или действие является единственным; исключает другие возможности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "ограничение, исключение других вариантов"
                            },
                            {
                                "target_lang_word": "лишь",
                                "target_lang_sense_clarification": "более книжный вариант"
                            },
                            {
                                "target_lang_word": "исключительно",
                                "target_lang_sense_clarification": "только и ничто иное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79a9d3bf-19f1-3aec-951c-4baeb0ff84b2",
                    "sense_definition": "Słowo podkreślające małą ilość, krótki czas lub niedawne wystąpienie zdarzenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Minęła <tylko> godzina od naszego wyjścia.",
                            "target_lang_translations": {
                                "ru": "Прошел <всего> час с момента нашего выхода."
                            }
                        },
                        {
                            "text": "To kosztuje <tylko> kilka groszy.",
                            "target_lang_translations": {
                                "ru": "Это стоит <всего лишь> несколько грошей."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaledwie",
                        "ledwie",
                        "raptem"
                    ],
                    "common_phrases": [
                        "tylko chwila",
                        "tylko trochę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, подчеркивающее малое количество, короткое время или недавность события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всего",
                                "target_lang_sense_clarification": "при указании на малое количество"
                            },
                            {
                                "target_lang_word": "всего лишь",
                                "target_lang_sense_clarification": "усиление малого количества"
                            },
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00a3f291-aa86-397b-b655-4dea9233b8d6",
                    "sense_definition": "Słowo używane w trybie rozkazującym do wyrażenia groźby lub ostrzeżenia.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "threat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Tylko> spróbuj to zrobić, a będziesz miał kłopoty!",
                            "target_lang_translations": {
                                "ru": "<Только> попробуй это сделать, и у тебя будут неприятности!"
                            }
                        },
                        {
                            "text": "Niech on <tylko> tu przyjdzie, to zobaczy.",
                            "target_lang_translations": {
                                "ru": "Пусть он <только> сюда придет, и он увидит."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tylko spróbuj",
                        "niech tylko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в повелительном наклонении для выражения угрозы или предупреждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "частица для усиления угрозы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "198ca874-96b5-3071-9fcc-fc12a7edc044",
                    "sense_definition": "Spójnik łączący przeciwstawne części zdania, używany po zaprzeczeniu, aby wskazać właściwą informację.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "contrast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To nie jest samochód, <tylko> rower.",
                            "target_lang_translations": {
                                "ru": "Это не машина, <а> велосипед."
                            }
                        },
                        {
                            "text": "Nie chcę herbaty, <tylko> kawę.",
                            "target_lang_translations": {
                                "ru": "Я не хочу чая, <а> кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "ale",
                        "lecz"
                    ],
                    "common_phrases": [
                        "nie tylko",
                        "nie teraz tylko potem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, соединяющий противопоставленные части предложения; используется после отрицания для указания правильной информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "противопоставление (не..., а...)"
                            },
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "в значении противопоставления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0dc9723-c2c7-3baa-8574-763a069a54d0",
                    "sense_definition": "Spójnik wprowadzający zdanie, które ogranicza lub koryguje treść zdania poprzedniego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "restriction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym pójść do kina, <tylko> nie mam pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы пойти в кино, <только> у меня нет денег."
                            }
                        },
                        {
                            "text": "Wszystko jest gotowe, <tylko> musimy chwilę poczekać.",
                            "target_lang_translations": {
                                "ru": "Всё готово, <только> нам нужно немного подождать."
                            }
                        }
                    ],
                    "synonyms": [
                        "ale",
                        "tyle że"
                    ],
                    "common_phrases": [
                        "tylko że"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, вводящий предложение, которое ограничивает или корректирует содержание предыдущего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "ограничительный союз"
                            },
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "противительный союз"
                            },
                            {
                                "target_lang_word": "вот только",
                                "target_lang_sense_clarification": "разговорное, с оттенком сожаления"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "24b9db92-04ca-3fa7-b056-43eb7596f764",
                    "sense_definition": "Sukces osiągnięty w walce, wojnie, zawodach sportowych lub innej rywalizacji; pokonanie przeciwnika",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_victory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza drużyna odniosła wczoraj wielkie <w>zwycięstwo</w> w meczu.",
                            "target_lang_translations": {
                                "ru": "Вчера наша команда одержала большую <w>победу</w> в матче."
                            }
                        },
                        {
                            "text": "To <w>zwycięstwo</w> zapewniło sportowcom awans do finału.",
                            "target_lang_translations": {
                                "ru": "Эта <w>победа</w> обеспечила спортсменам выход в финал."
                            }
                        }
                    ],
                    "synonyms": [
                        "wygrana",
                        "triumf",
                        "wiktoria"
                    ],
                    "antonyms": [
                        "przegrana",
                        "porażka",
                        "klęska"
                    ],
                    "common_phrases": [

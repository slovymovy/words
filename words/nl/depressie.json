            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab90f60d-2c4f-3d8f-9410-e51ba8beecb6",
                    "sense_definition": "Een toestand van langdurige en ziekelijke neerslachtigheid waarbij men het plezier in het leven verliest.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "psychology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het verlies van haar baan raakte ze in een diepe <w>depressie</w>.",
                            "target_lang_translations": {
                                "ru": "После потери работы она впала в глубокую <w>депрессию</w>."
                            }
                        },
                        {
                            "text": "Hij slikt medicijnen en volgt therapie om zijn <w>depressie</w> te behandelen.",
                            "target_lang_translations": {
                                "ru": "Он принимает лекарства и проходит терапию, чтобы вылечить свою <w>депрессию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lagedrukgebied"
                    ],
                    "common_phrases": [
                        "in een depressie raken",
                        "zware depressie",
                        "postnatale depressie",
                        "klinische depressie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние длительной и болезненной подавленности, при котором человек теряет радость жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "депрессия",
                                "target_lang_sense_clarification": "психологическое состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fe203fc-c330-38e5-9689-98e389201a28",
                    "sense_definition": "Een gebied in de atmosfeer met relatief lage luchtdruk dat vaak bewolking en neerslag brengt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "meteorology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De weerman voorspelde dat er morgen een actieve <w>depressie</w> over het land trekt.",
                            "target_lang_translations": {
                                "ru": "Синоптик предсказал, что завтра над страной пройдет активный <w>циклон</w>."
                            }
                        },
                        {
                            "text": "Door de naderende <w>depressie</w> gaat het waarschijnlijk hard waaien aan de kust.",
                            "target_lang_translations": {
                                "ru": "Из-за приближающейся <w>депрессии</w> на побережье, вероятно, будет сильный ветер."
                            }
                        }
                    ],
                    "synonyms": [
                        "lagedrukgebied"

                },
                {
                    "sense_id": "49950562-baf7-3e40-924c-ed0a2a0d8d58",
                    "sense_definition": "In hoge mate; erg.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hulp is in dit gebied <w>hard</w> nodig.",
                            "target_lang_translations": {
                                "ru": "Помощь в этом районе <w>очень</w> нужна."
                            }
                        },
                        {
                            "text": "Het regent <w>hard</w> buiten.",
                            "target_lang_translations": {
                                "ru": "На улице <w>сильно</w> идёт дождь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hard nodig"
                    ],
                    "target_lang_definitions": {
                        "ru": "В высокой степени; очень."

            "senses": [
                {
                    "sense_id": "d95f187b-0668-3bb5-81fd-dcbd728655a8",
                    "sense_definition": "Dorosły człowiek płci żeńskiej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "human_female",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta młoda <kobieta> pracuje w pobliskim sklepie.",
                            "target_lang_translations": {
                                "ru": "Эта молодая <женщина> работает в соседнем магазине."
                            }
                        },
                        {
                            "text": "Każda <kobieta> powinna regularnie dbać o swoje zdrowie.",
                            "target_lang_translations": {
                                "ru": "Каждая <женщина> должна регулярно заботиться о своём здоровье."
                            }
                        },
                        {
                            "text": "Na spotkanie przyszła elegancka <kobieta> w czerwonym płaszczu.",
                            "target_lang_translations": {
                                "ru": "На встречу пришла элегантная <женщина> в красном пальто."
                            }
                        }
                    ],
                    "synonyms": [
                        "pani",
                        "dama",
                        "niewiasta",
                        "babka",
                        "facetka"
                    ],
                    "antonyms": [
                        "mężczyzna",
                        "facet"
                    ],
                    "common_phrases": [
                        "piękna kobieta",
                        "prawa kobiet",
                        "niezależna kobieta",
                        "dojrzała kobieta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрослый человек женского пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "женщина",
                                "target_lang_sense_clarification": "взрослый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "456e494d-960c-3deb-9a08-e3361aeeedc0",
                    "sense_definition": "Partnerka życiowa, żona lub dziewczyna, z którą jest się w związku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "partner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest moja <kobieta>, z którą planuję wspólną przyszłość.",
                            "target_lang_translations": {
                                "ru": "Это моя <женщина>, с которой я планирую совместное будущее."
                            }
                        },
                        {
                            "text": "On zawsze liczy się ze zdaniem swojej <kobiety>.",
                            "target_lang_translations": {
                                "ru": "Он всегда считается с мнением своей <женщины>."
                            }
                        },
                        {
                            "text": "Budzę się rano z moją <kobietą> u boku i jestem szczęśliwy.",
                            "target_lang_translations": {
                                "ru": "Я просыпаюсь утром с моей <женщиной> под боком и счастлив."
                            }
                        }
                    ],
                    "synonyms": [
                        "żona",
                        "moja"
                    ],
                    "antonyms": [
                        "chłop",
                        "facet"
                    ],
                    "common_phrases": [
                        "moja kobieta",
                        "jego kobieta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спутница жизни, жена или девушка, с которой состоят в отношениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "женщина",
                                "target_lang_sense_clarification": "партнёрша, возлюбленная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ff48856-42c3-3d23-940f-d5d629de7aa5",
                    "sense_definition": "Służąca lub pracownica najemna wykonująca prace domowe (znaczenie historyczne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "worker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz najął <kobietę> do pomocy przy żniwach i w kuchni.",
                            "target_lang_translations": {
                                "ru": "Хозяин нанял <работницу> в помощь на жатве и на кухне."
                            }
                        },
                        {
                            "text": "Bogata rodzina zatrudniała <kobietę> ze wsi do sprzątania dworu.",
                            "target_lang_translations": {
                                "ru": "Богатая семья нанимала <женщину> из деревни для уборки усадьбы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nająć kobietę"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie używa się określeń: pomoc domowa, sprzątaczka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Служанка или наёмная работница, выполняющая работу по дому (историческое значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работница",

                    "synonyms": [
                        "nowotwór",
                        "rak",
                        "guz nowotworowy",
                        "zmiana nowotworowa"
                    ],
                    "common_phrases": [
                        "guz mózgu",
                        "złośliwy guz",
                        "łagodny guz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Патологическое разрастание ткани, которое может быть доброкачественным или злокачественным новообразованием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опухоль",
                                "target_lang_sense_clarification": "медицинское"
                            },
                            {
                                "target_lang_word": "новообразование",
                                "target_lang_sense_clarification": "формальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36913845-dca4-371b-a8a1-f3b53a107dbe",
                    "sense_definition": "Ozdobna gałka lub duży, wypukły guzik, używany dawniej jako element stroju szlacheckiego.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego staropolski kontusz był zapinany na srebrne, bogato zdobione <guzy>.",
                            "target_lang_translations": {

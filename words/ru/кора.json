    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "262a1403-8a6f-35f6-94c2-f10a10be9fdf",
                    "sense_definition": "Наружный защитный слой ствола и ветвей деревьев.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "outer_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дуб растёт небольшой, с тёмной и потрескавшейся <корой>.",
                            "target_lang_translations": {
                                "en": "The oak grows small, with dark and cracked <bark>."
                            }
                        },
                        {
                            "text": "Для различных поделок часто используется <кора> берёзы.",
                            "target_lang_translations": {
                                "en": "Birch <bark> is often used for various crafts."
                            }
                        }
                    ],
                    "common_phrases": [
                        "кора дуба",
                        "кора берёзы",
                        "древесная кора"
                    ],
                    "target_lang_definitions": {
                        "en": "The tough outer covering of the woody stems and roots of trees and other woody plants."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bark",
                                "target_lang_sense_clarification": "tree covering"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52b1e652-964e-3964-867c-47f21a5f2062",
                    "sense_definition": "Внешняя твёрдая оболочка Земли или другой планеты, расположенная поверх мантии.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "outer_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Толщина земной <коры> варьируется в зависимости от региона.",
                            "target_lang_translations": {
                                "en": "The thickness of the Earth's <crust> varies depending on the region."
                            }
                        },
                        {
                            "text": "Земная <кора> состоит из различных горных пород и минералов.",
                            "target_lang_translations": {
                                "en": "The Earth's <crust> consists of various rocks and minerals."
                            }
                        }
                    ],
                    "antonyms": [
                        "ядро"
                    ],
                    "common_phrases": [
                        "земная кора"
                    ],
                    "target_lang_definitions": {
                        "en": "The outermost solid shell of a rocky planet or natural satellite."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crust",
                                "target_lang_sense_clarification": "geology"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49fbfe58-ef10-3225-a2d5-789e6bae5ad3",
                    "sense_definition": "Слой серого вещества, покрывающий полушария головного мозга и отвечающий за высшую нервную деятельность.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "outer_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Префронтальная <кора> мозга отвечает за принятие сложных решений.",
                            "target_lang_translations": {
                                "en": "The prefrontal <cortex> of the brain is responsible for making complex decisions."
                            }
                        },
                        {
                            "text": "Зрительная информация обрабатывается в затылочной <коре>.",
                            "target_lang_translations": {
                                "en": "Visual information is processed in the occipital <cortex>."
                            }
                        }
                    ],
                    "synonyms": [
                        "кора больших полушарий головного мозга"
                    ],
                    "antonyms": [
                        "подкорка"
                    ],
                    "common_phrases": [
                        "кора головного мозга",
                        "зрительная кора"
                    ],
                    "target_lang_definitions": {
                        "en": "The outer layer of the cerebrum, composed of folded gray matter, playing a key role in consciousness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cortex",
                                "target_lang_sense_clarification": "anatomy"
                            },
                            {
                                "target_lang_word": "cerebral cortex",
                                "target_lang_sense_clarification": "anatomy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1440967c-69a4-3f2f-8551-d99e6f8ab39e",
                    "sense_definition": "Смешная ситуация, шутка или что-то нелепое и забавное.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_joke",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он постоянно рассказывает какие-то <коры>, и мы все смеёмся.",
                            "target_lang_translations": {
                                "en": "He's constantly telling some <jokes>, and we all laugh."
                            }
                        },
                        {
                            "text": "Вчера на вечеринке была полная <кора>, такого я давно не видел.",
                            "target_lang_translations": {
                                "en": "Yesterday at the party was a total <riot>, I haven't seen anything like that in a long time."
                            }
                        }
                    ],
                    "synonyms": [
                        "прикол"
                    ],
                    "common_phrases": [
                        "выдавать коры"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "youth slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A very funny person, thing, or situation (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "riot",
                                "target_lang_sense_clarification": "funny situation"
                            },
                            {
                                "target_lang_word": "hoot",
                                "target_lang_sense_clarification": "funny thing/person"
                            },
                            {
                                "target_lang_word": "joke",
                                "target_lang_sense_clarification": "funny story"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "586c833a-2a3c-3212-ab73-d9dbc422cd79",
                    "sense_definition": "Затвердевший плотный слой на поверхности чего-либо мягкого или жидкого.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "outer_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зимой на снегу часто образуется твёрдая ледяная <кора>.",
                            "target_lang_translations": {
                                "en": "In winter, a hard icy <crust> often forms on the snow."
                            }
                        },
                        {
                            "text": "На старой ране появилась сухая и жёсткая <кора>.",
                            "target_lang_translations": {
                                "en": "A dry and hard <crust> appeared on the old wound."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ледяная кора"
                    ],
                    "target_lang_definitions": {
                        "en": "A hardened layer, coating, or deposit on the surface of something soft or liquid."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crust",
                                "target_lang_sense_clarification": "hard layer"
                            }

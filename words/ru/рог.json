            "senses": [
                {
                    "sense_id": "b94af896-b70e-3404-bf74-44f4571c097a",
                    "sense_definition": "Твёрдый вырост (одиночный или парный) на голове некоторых животных",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромный лось гордо нёс свои ветвистые <w>рога</w> сквозь густую лесную чащу.",
                            "target_lang_translations": {
                                "en": "The huge moose proudly carried its branching <w>antlers</w> through the thick forest thicket."
                            }
                        },
                        {
                            "text": "Пастух предупредил туристов, что у этого быка очень острые и опасные <w>рога</w>.",
                            "target_lang_translations": {
                                "en": "The shepherd warned the tourists that this bull has very sharp and dangerous <w>horns</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "оленьи рога",
                        "рога быка",
                        "ветвистые рога",
                        "бодаться рогами"
                    ],
                    "target_lang_definitions": {
                        "en": "A hard growth (single or paired) on the head of certain animals"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "horn",
                                "target_lang_sense_clarification": "general term for cattle, goats, rhinos, etc."
                            },
                            {
                                "target_lang_word": "antler",
                                "target_lang_sense_clarification": "branched horn of deer, moose, etc."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0460559-ac42-3151-b4fb-0fcb9809e91e",
                    "sense_definition": "Духовой музыкальный или сигнальный инструмент",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вдали послышался охотничий <w>рог</w>, сзывающий собак и людей в одно место.",
                            "target_lang_translations": {
                                "en": "In the distance, a hunting <w>horn</w> was heard, gathering dogs and people to one place."
                            }
                        },
                        {
                            "text": "Музыкант поднял свой <w>рог</w> и извлёк из него глубокий, протяжный звук.",
                            "target_lang_translations": {
                                "en": "The musician raised his <w>horn</w> and extracted a deep, drawn-out sound from it."
                            }
                        }
                    ],
                    "synonyms": [
                        "горн"
                    ],
                    "common_phrases": [
                        "охотничий рог",
                        "трубить в рог",
                        "звук рога"
                    ],
                    "target_lang_definitions": {
                        "en": "A wind musical or signaling instrument"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "horn",
                                "target_lang_sense_clarification": "musical or hunting instrument"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76b845f0-44da-37a3-aa4b-d0baa32d5b3e",
                    "sense_definition": "Предмет или часть чего-либо, имеющие изогнутую, заострённую форму",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На ночном небе ярко сиял золотой <w>рог</w> молодого месяца.",
                            "target_lang_translations": {
                                "en": "The golden <w>horn</w> of the new moon shone brightly in the night sky."
                            }
                        },
                        {
                            "text": "Улитка медленно ползла по листу, выставив свои маленькие чувствительные <w>рога</w>.",
                            "target_lang_translations": {
                                "en": "The snail crawled slowly along the leaf, sticking out its small sensitive <w>horns</w>."
                            }

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "61008a64-c189-3f00-bfb3-a6102632353a",
                    "sense_definition": "The male sexual organ; the penis.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He made a crude joke about his <w>dong</w> in the locker room.",
                            "target_lang_translations": {
                                "ru": "Он отпустил грубую шутку про свой <w>член</w> в раздевалке."
                            }
                        },
                        {
                            "text": "The comedian's routine relied heavily on immature humor about his <w>dong</w>.",
                            "target_lang_translations": {
                                "ru": "Номер комика строился в основном на незрелом юморе о его <w>члене</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "penis"
                    ],
                    "common_phrases": [
                        "my dong",
                        "huge dong"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужской половой орган; пенис (сленг)."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec74ef8f-6e1c-38c0-be58-c0f75ca41eec",
                    "sense_definition": "Something extra that is good; an added benefit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "benefit_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sunny weather was an added <w>bonus</w> for our picnic.",
                            "target_lang_translations": {
                                "ru": "Солнечная погода стала приятным <w>бонусом</w> для нашего пикника."
                            }
                        },
                        {
                            "text": "Free parking is a nice <w>bonus</w> of living in this apartment.",
                            "target_lang_translations": {
                                "ru": "Бесплатная парковка — это приятный <w>бонус</w> проживания в этой квартире."
                            }
                        }
                    ],
                    "synonyms": [
                        "benefit",
                        "plus",
                        "perk",
                        "extra",
                        "advantage"
                    ],
                    "antonyms": [
                        "drawback",
                        "disadvantage"
                    ],
                    "common_phrases": [
                        "added bonus",
                        "unexpected bonus",
                        "big bonus",
                        "real bonus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то хорошее сверх ожидаемого; дополнительное преимущество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бонус",
                                "target_lang_sense_clarification": "дополнительное преимущество"
                            },
                            {
                                "target_lang_word": "приятное дополнение",
                                "target_lang_sense_clarification": "что-то хорошее сверх ожидаемого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82216ea5-29e8-3118-8846-3aebdaf7c081",
                    "sense_definition": "An extra sum of money given to an employee or shareholder as a reward.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "benefit_financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She received a large <w>bonus</w> at the end of the year for her hard work.",
                            "target_lang_translations": {
                                "ru": "В конце года она получила крупную <w>премию</w> за усердную работу."
                            }
                        },
                        {
                            "text": "The company pays a performance <w>bonus</w> if sales targets are met.",
                            "target_lang_translations": {
                                "ru": "Компания выплачивает <w>бонус</w> по результатам работы, если цели по продажам достигнуты."
                            }
                        }
                    ],
                    "synonyms": [
                        "premium",
                        "reward",
                        "gratuity"
                    ],
                    "antonyms": [
                        "penalty",
                        "fine"
                    ],
                    "common_phrases": [
                        "annual bonus",
                        "Christmas bonus",
                        "cash bonus",
                        "performance bonus",
                        "sign-on bonus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дополнительная сумма денег, выплачиваемая работнику или акционеру в качестве вознаграждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "премия",
                                "target_lang_sense_clarification": "денежное вознаграждение"
                            },
                            {
                                "target_lang_word": "бонус",
                                "target_lang_sense_clarification": "дополнительная выплата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "645d6151-d4a5-36d2-b106-9c38e1e8baee",
                    "sense_definition": "An addition to a player's score or status based on performance in a game.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "benefit_game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You get a <w>bonus</w> for collecting all the gold coins in the level.",
                            "target_lang_translations": {
                                "ru": "Вы получаете <w>бонус</w> за сбор всех золотых монет на уровне."
                            }
                        },
                        {
                            "text": "The <w>bonus</w> round gives you a chance to win extra lives.",
                            "target_lang_translations": {
                                "ru": "<w>Бонусный</w> раунд дает вам шанс выиграть дополнительные жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "boost"
                    ],
                    "common_phrases": [
                        "bonus points",
                        "bonus level",
                        "bonus round"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличение очков или статуса игрока за определенные достижения в игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бонус",
                                "target_lang_sense_clarification": "в играх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86bcdff9-c117-3250-928b-e80638232f2b",
                    "sense_definition": "One or more free throws awarded to a team when the opposing team has accumulated enough fouls.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "benefit_sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team is now in the <w>bonus</w> and will shoot free throws for every foul.",
                            "target_lang_translations": {
                                "ru": "Команда теперь в <w>бонусе</w> и будет пробивать штрафные за каждый фол."
                            }
                        },
                        {
                            "text": "A foul at this stage would put the opponents in the <w>bonus</w>.",
                            "target_lang_translations": {
                                "ru": "Фол на этой стадии отправит соперников в <w>бонус</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in the bonus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, когда команда получает право на штрафные броски из-за перебора фолов соперником."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бонус",
                                "target_lang_sense_clarification": "спортивный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "прощение",
        "непростительный",
        "всепрощение"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5a4919fe-5e81-37e4-bf2c-8d5cc492b89d",
                    "sense_definition": "Перестать винить кого-либо за что-либо, не держать зла за обиду или проступок.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "forgiveness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не могу <w>простить</w> тебе эту ложь.",
                            "target_lang_translations": {
                                "en": "I cannot <w>forgive</w> you for this lie."
                            }
                        },
                        {
                            "text": "Пожалуйста, <w>прости</w> меня за опоздание.",
                            "target_lang_translations": {
                                "en": "Please <w>forgive</w> me for being late."
                            }
                        }
                    ],
                    "synonyms": [
                        "извинить"
                    ],
                    "antonyms": [
                        "обвинить",
                        "осудить"
                    ],
                    "common_phrases": [
                        "простить обиду",
                        "простить ошибку",
                        "простить измену",
                        "простить грехи"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop blaming someone for something, not to hold a grudge for an offense or misconduct."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forgive",
                                "target_lang_sense_clarification": "to stop feeling angry or resentful towards someone"
                            },
                            {
                                "target_lang_word": "pardon",
                                "target_lang_sense_clarification": "formal; to officially forgive an offense"
                            },
                            {
                                "target_lang_word": "excuse",
                                "target_lang_sense_clarification": "to overlook a minor fault or accept an apology"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdb0ea01-020b-3838-9653-590974ebe3a2",
                    "sense_definition": "Освободить от какого-либо обязательства, не требовать возврата взятого.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "debt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Друг решил <w>простить</w> мне старый долг.",
                            "target_lang_translations": {
                                "en": "My friend decided to <w>forgive</w> my old debt."
                            }
                        },
                        {
                            "text": "Банк согласился <w>простить</w> часть кредита.",
                            "target_lang_translations": {
                                "en": "The bank agreed to <w>forgive</w> part of the loan."
                            }
                        }
                    ],
                    "antonyms": [
                        "взыскать"
                    ],
                    "common_phrases": [
                        "простить долг",
                        "простить кредит"
                    ],
                    "target_lang_definitions": {
                        "en": "To release from an obligation, not to demand the return of what was taken."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forgive",
                                "target_lang_sense_clarification": "to cancel a debt or obligation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
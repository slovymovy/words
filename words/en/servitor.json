    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f379bee6-48af-3493-8586-0be8f16d3c84",
                    "sense_definition": "A person who performs the duties of a servant or attendant.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "service_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loyal <w>servitor</w> stood silently by the door, waiting for his master's command.",
                            "target_lang_translations": {
                                "ru": "Верный <w>слуга</w> молча стоял у двери, ожидая приказа своего господина."
                            }
                        },
                        {
                            "text": "In the fantasy novel, the dark wizard summoned a magical <w>servitor</w> to carry his message.",
                            "target_lang_translations": {
                                "ru": "В фэнтезийном романе темный волшебник призвал магического <w>прислужника</w>, чтобы передать свое послание."
                            }
                        }
                    ],
                    "synonyms": [
                        "servant",
                        "attendant",
                        "retainer",
                        "menial"
                    ],
                    "antonyms": [
                        "master",
                        "lord"
                    ],
                    "common_phrases": [
                        "loyal servitor",
                        "humble servitor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, выполняющий обязанности слуги или сопровождающего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слуга",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "служитель",
                                "target_lang_sense_clarification": "официальное или возвышенное"
                            },
                            {
                                "target_lang_word": "прислужник",
                                "target_lang_sense_clarification": "часто в фэнтези или с негативным оттенком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58870abc-ff50-3d06-aa63-7d0fcd9b31fd",
                    "sense_definition": "An undergraduate student, historically at Oxford University, who performed menial duties in exchange for financial support.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a poor <w>servitor</w>, he struggled to balance his studies with his duties to the wealthier students.",
                            "target_lang_translations": {
                                "ru": "Будучи бедным <w>сервитором</w>, он с трудом совмещал учебу с обязанностями перед более богатыми студентами."
                            }
                        },
                        {
                            "text": "The university eventually abolished the rank of <w>servitor</w> in the nineteenth century.",
                            "target_lang_translations": {
                                "ru": "В девятнадцатом веке университет в конце концов упразднил звание <w>сервитора</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sizar"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to a historical position"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Студент Оксфордского университета (в прошлом), выполнявший работу слуги в обмен на финансовую поддержку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сервитор",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7a117ed-8610-356a-8204-cd7953532b92",
                    "sense_definition": "One who serves in an army; a soldier.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "service_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old <w>servitor</w> recounted tales of the battles he had fought in his youth.",
                            "target_lang_translations": {
                                "ru": "Старый <w>служивый</w> рассказывал истории о битвах, в которых он сражался в молодости."
                            }
                        },
                        {
                            "text": "Every <w>servitor</w> in the regiment was prepared to defend the castle.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>солдат</w> полка был готов защищать замок."
                            }
                        }
                    ],
                    "synonyms": [
                        "soldier"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rarely used in modern English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто служит в армии; солдат (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "служивый",
                                "target_lang_sense_clarification": "устаревшее, разговорное"
                            },
                            {
                                "target_lang_word": "солдат",
                                "target_lang_sense_clarification": "военный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "szykowny",
        "szykownie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa18fd1b-8b34-36cf-ad18-b0dcfce4acc0",
                    "sense_definition": "Elegancja, wytworność i dobry styl, widoczne zwłaszcza w ubiorze i zachowaniu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej wieczorowa kreacja miała prawdziwy <w>szyk</w> i zachwyciła wszystkich gości.",
                            "target_lang_translations": {
                                "ru": "Ее вечернее платье отличалось настоящим <w>шиком</w> и восхитило всех гостей."
                            }
                        },
                        {
                            "text": "On zawsze lubi zadawać <w>szyku</w> drogimi dodatkami i modnym ubraniem.",
                            "target_lang_translations": {
                                "ru": "Он всегда любит блеснуть <w>шиком</w> с помощью дорогих аксессуаров и модной одежды."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytworność",
                        "dystynkcja"
                    ],
                    "common_phrases": [
                        "zadawać szyku",
                        "mieć szyk",
                        "paryski szyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элегантность, изысканность и хороший стиль, заметные особенно в одежде и поведении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шик",
                                "target_lang_sense_clarification": "изысканность, блеск, показная роскошь"
                            },
                            {
                                "target_lang_word": "элегантность",
                                "target_lang_sense_clarification": "утонченность и хороший вкус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d09c8cb-641f-336b-a92c-f6d6fe4ab133",
                    "sense_definition": "Uporządkowane ustawienie ludzi, oddziałów wojskowych lub przedmiotów według określonego planu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze sformowali <w>szyk</w> bojowy i ruszyli do ataku.",
                            "target_lang_translations": {
                                "ru": "Солдаты сформировали боевой <w>строй</w> и двинулись в атаку."
                            }
                        },
                        {
                            "text": "Niespodziewana choroba pomieszała nam <w>szyki</w> i musieliśmy odwołać wakacje.",
                            "target_lang_translations": {
                                "ru": "Неожиданная болезнь спутала нам <w>планы</w>, и нам пришлось отменить отпуск."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szyk bojowy",
                        "pomieszać szyki",
                        "zwierać szyki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упорядоченное построение людей, военных отрядов или предметов по определенному плану."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строй",
                                "target_lang_sense_clarification": "военное построение, порядок размещения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c572eb1-0d2b-379d-b7b2-a5dd647b1be9",
                    "sense_definition": "Gramatyczny układ wyrazów w zdaniu regulowany zasadami składni.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "order",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W języku polskim <w>szyk</w> wyrazów jest zazwyczaj swobodny, ale wpływa na znaczenie.",
                            "target_lang_translations": {
                                "ru": "В польском языке <w>порядок</w> слов обычно свободный, но влияет на значение."
                            }
                        },
                        {
                            "text": "Nauczyciel zwrócił uwagę na niepoprawny <w>szyk</w> zdania w moim wypracowaniu.",
                            "target_lang_translations": {
                                "ru": "Учитель обратил внимание на неправильный <w>порядок</w> слов в предложении в моем сочинении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szyk zdania",
                        "szyk przestawny",
                        "szyk prosty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматический порядок слов в предложении, регулируемый правилами синтаксиса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порядок",
                                "target_lang_sense_clarification": "последовательность расположения (слов)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "2fa65295-cb37-3208-be28-d84762877630",
                    "sense_definition": "Sztuka organizacji dźwięków w czasie, tworząca kompozycje; utwory muzyczne.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja ulubiona <w>muzyka</w> to jazz i rock.",
                            "target_lang_translations": {
                                "ru": "Моя любимая <w>музыка</w> — это джаз и рок."
                            }
                        },
                        {
                            "text": "Często słucham <w>muzyki</w> w drodze do pracy.",
                            "target_lang_translations": {
                                "ru": "Я часто слушаю <w>музыку</w> по дороге на работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "słuchać muzyki",
                        "muzyka poważna",
                        "muzyka rozrywkowa",
                        "pisać muzykę"
                    ],
                    "target_lang_definitions": {

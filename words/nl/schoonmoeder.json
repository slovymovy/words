                    "sense_id": "a6a51966-54a7-35d9-98ed-21e39e0b0b25",
                    "sense_definition": "De moeder van iemands echtgenoot of echtgenote.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "family_in_law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn <w>schoonmoeder</w> komt dit weekend gezellig bij ons op bezoek.",
                            "target_lang_translations": {
                                "ru": "Моя <w>свекровь</w> приедет к нам в гости в эти выходные."
                            }
                        },
                        {
                            "text": "Hij kan gelukkig heel goed opschieten met zijn nieuwe <w>schoonmoeder</w>.",
                            "target_lang_translations": {
                                "ru": "К счастью, он очень хорошо ладит со своей новой <w>тёщей</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "schoonmama"
                    ],
                    "antonyms": [
                        "schoonvader"
                    ],
                    "common_phrases": [
                        "toekomstige schoonmoeder",
                        "lieve schoonmoeder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мать мужа или жены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свекровь",
                                "target_lang_sense_clarification": "мать мужа"
                            },
                            {
                                "target_lang_word": "тёща",
                                "target_lang_sense_clarification": "мать жены"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6139e8d9-3ebb-307d-af28-1f1918b19fe9",
                    "sense_definition": "De moeder van iemands vriend of vriendin (partner) met wie men niet getrouwd is.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "family_in_law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hoewel we niet getrouwd zijn, noem ik haar toch gewoon mijn <w>schoonmoeder</w>.",
                            "target_lang_translations": {
                                "ru": "Хотя мы не женаты, я всё равно называю её просто своей <w>свекровью</w>."
                            }
                        },
                        {
                            "text": "Ik ga morgen voor het eerst eten bij mijn <w>schoonmoeder</w> en schoonvader.",
                            "target_lang_translations": {
                                "ru": "Завтра я впервые иду ужинать к моей <w>свекрови</w> и свёкру."
                            }
                        }
                    ],
                    "synonyms": [
                        "schoonmama"
                    ],
                    "antonyms": [
                        "schoonvader"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мать партнера (парня или девушки), с которым вы не состоите в официальном браке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свекровь",
                                "target_lang_sense_clarification": "мать партнера (разг.)"
                            },
                            {
                                "target_lang_word": "тёща",
                                "target_lang_sense_clarification": "мать партнерши (разг.)"
                            }
                        ]
                    }
                }
            ]
        }

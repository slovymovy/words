            "senses": [
                {
                    "sense_id": "74ec3a46-71b0-3ccd-b0e2-1a8bbb76922a",
                    "sense_definition": "gwałtowne uderzenie zadane ręką, nogą lub narzędziem",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_hit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bokser zadał przeciwnikowi decydujący <w>cios</w> w ostatniej rundzie.",
                            "target_lang_translations": {
                                "ru": "Боксер нанес противнику решающий <w>удар</w> в последнем раунде."
                            }
                        },
                        {
                            "text": "Mężczyzna otrzymał silny <w>cios</w> w głowę i stracił przytomność.",
                            "target_lang_translations": {
                                "ru": "Мужчина получил сильный <w>удар</w> по голове и потерял сознание."
                            }
                        }
                    ],
                    "synonyms": [
                        "uderzenie",
                        "walnięcie",
                        "reg. śl. szlag"
                    ],
                    "common_phrases": [
                        "zadać cios",
                        "otrzymać cios",
                        "cios pięścią",
                        "wymiana ciosów",
                        "cios poniżej pasa"
                    ],
                    "target_lang_definitions": {
                        "ru": "резкий удар, нанесенный рукой, ногой или предметом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "физическое воздействие"

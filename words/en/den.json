            "senses": [
                {
                    "sense_id": "e73b7358-f339-346e-a9e9-e121153854d0",
                    "sense_definition": "A small cavern or hollow place in the side of a hill or among rocks, especially one used by a wild animal for shelter.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "animal_home",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bear returned to its <w>den</w> to sleep through the winter.",
                            "target_lang_translations": {
                                "ru": "Медведь вернулся в свою <w>берлогу</w>, чтобы проспать зиму."
                            }
                        },
                        {
                            "text": "We found a fox's <w>den</w> hidden deep in the forest.",
                            "target_lang_translations": {
                                "ru": "Мы нашли лисью <w>нору</w>, спрятанную глубоко в лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "lair"
                    ],
                    "common_phrases": [
                        "lions' den",
                        "bear den",
                        "wolf den"
                    ],

                {
                    "sense_id": "e5ae7a87-6297-3979-a91b-28d4571fe6eb",
                    "sense_definition": "A shortened form of <w>magazine</w>, a periodical publication containing articles and illustrations.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "publication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought a fashion <w>mag</w> to read on the train.",
                            "target_lang_translations": {
                                "ru": "Я купил модный <w>журнал</w>, чтобы почитать в поезде."
                            }
                        },
                        {
                            "text": "He keeps a stack of old car <w>mags</w> in the garage.",
                            "target_lang_translations": {
                                "ru": "Он хранит стопку старых автомобильных <w>журналов</w> в гараже."
                            }
                        }
                    ],
                    "synonyms": [
                        "journal",
                        "periodical"
                    ],
                    "common_phrases": [
                        "fashion mag",
                        "gossip mag",
                        "monthly mag"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "clipping of magazine"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «magazine», периодическое издание со статьями и иллюстрациями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "журнал",
                                "target_lang_sense_clarification": "периодическое издание"

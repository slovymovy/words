    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a4faa28-02a8-3633-8468-4f12ea199a2a",
                    "sense_definition": "Iemand die leiding geeft aan werknemers of aan het hoofd staat van een organisatie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "authority_figures",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn <w>baas</w> heeft me gisteren een compliment gegeven over het project.",
                            "target_lang_translations": {
                                "en": "My <w>boss</w> gave me a compliment yesterday about the project.",
                                "ru": "Мой <w>начальник</w> вчера похвалил меня за проект."
                            }
                        },
                        {
                            "text": "Hij wil graag zijn eigen <w>baas</w> zijn en een bedrijf starten.",
                            "target_lang_translations": {
                                "en": "He would like to be his own <w>boss</w> and start a company.",
                                "ru": "Он хочет быть сам себе <w>начальником</w> и открыть бизнес."
                            }
                        }
                    ],
                    "synonyms": [
                        "chef",
                        "leider",
                        "bestuurder"
                    ],
                    "common_phrases": [
                        "de grote baas",
                        "eigen baas zijn",
                        "baas boven baas"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who manages employees or heads an organization.",
                        "ru": "Человек, который руководит сотрудниками или возглавляет организацию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boss",
                                "target_lang_sense_clarification": "employer or manager"
                            },
                            {
                                "target_lang_word": "manager",
                                "target_lang_sense_clarification": "supervisor"
                            },
                            {
                                "target_lang_word": "chief",
                                "target_lang_sense_clarification": "leader of a group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "начальник",
                                "target_lang_sense_clarification": "руководитель"
                            },
                            {
                                "target_lang_word": "босс",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "шеф",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db8aa98c-6746-39dc-b56a-b82f4979afae",
                    "sense_definition": "De eigenaar en verzorger van een huisdier, meestal een hond.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ownership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond wachtte geduldig bij de deur op zijn <w>baas</w>.",
                            "target_lang_translations": {
                                "en": "The dog waited patiently at the door for its <w>owner</w>.",
                                "ru": "Собака терпеливо ждала своего <w>хозяина</w> у двери."
                            }
                        },
                        {
                            "text": "Het <w>baasje</w> liep elke ochtend een rondje door het park.",
                            "target_lang_translations": {
                                "en": "The <w>owner</w> walked a lap through the park every morning.",
                                "ru": "<w>Хозяин</w> каждое утро совершал прогулку по парку."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestuurder"
                    ],
                    "common_phrases": [
                        "het baasje van",
                        "een trouwe baas"
                    ],
                    "target_lang_definitions": {
                        "en": "The owner and caretaker of a pet, usually a dog.",
                        "ru": "Владелец и опекун домашнего животного, обычно собаки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "owner",
                                "target_lang_sense_clarification": "of a pet"
                            },
                            {
                                "target_lang_word": "master",
                                "target_lang_sense_clarification": "of a dog"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хозяин",
                                "target_lang_sense_clarification": "владелец животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3f01caf-6ece-3926-bf9d-842272d3797a",
                    "sense_definition": "Degene die de macht heeft of de controle behoudt over een situatie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "authority_figures",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet proberen je zenuwen de <w>baas</w> te blijven tijdens het examen.",
                            "target_lang_translations": {
                                "en": "You must try to remain the <w>master</w> of your nerves during the exam.",
                                "ru": "Ты должен стараться оставаться <w>хозяином</w> своих нервов во время экзамена."
                            }
                        },
                        {
                            "text": "Zij speelt graag de <w>baas</w> over haar jongere broertje.",
                            "target_lang_translations": {
                                "en": "She likes to play the <w>boss</w> over her younger brother.",
                                "ru": "Она любит строить из себя <w>начальницу</w> перед младшим братом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de baas spelen",
                        "de baas blijven",
                        "de baas zijn over"
                    ],
                    "target_lang_definitions": {
                        "en": "The person who has power or maintains control over a situation.",
                        "ru": "Тот, кто обладает властью или сохраняет контроль над ситуацией."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "master",
                                "target_lang_sense_clarification": "one who controls or dominates"
                            },
                            {
                                "target_lang_word": "boss",
                                "target_lang_sense_clarification": "person in charge"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "хозяин",
                                "target_lang_sense_clarification": "положения"
                            },
                            {
                                "target_lang_word": "главный",
                                "target_lang_sense_clarification": "лидер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c42ba84c-f336-3c3a-80a8-aad0621fc75d",
                    "sense_definition": "Iemand die ergens uitzonderlijk goed in is of iets indrukwekkends heeft gedaan.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "skill_expert",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is echt een <w>baas</w> in het oplossen van moeilijke puzzels.",
                            "target_lang_translations": {
                                "en": "He is truly a <w>boss</w> at solving difficult puzzles.",
                                "ru": "Он настоящий <w>профи</w> в решении сложных головоломок."
                            }
                        },
                        {
                            "text": "Wat een <w>baas</w> dat hij die wedstrijd toch nog gewonnen heeft.",
                            "target_lang_translations": {
                                "en": "What a <w>boss</w> that he still won that match.",
                                "ru": "Какой <w>красавчик</w>, что он всё-таки выиграл этот матч."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wat een baas",
                        "een echte baas"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used mostly by younger generations"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who is exceptionally good at something or has done something impressive.",
                        "ru": "Тот, кто исключительно хорош в чем-то или сделал что-то впечатляющее."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boss",
                                "target_lang_sense_clarification": "slang for someone excellent or cool"
                            },
                            {
                                "target_lang_word": "legend",
                                "target_lang_sense_clarification": "slang for an impressive person"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мастер",
                                "target_lang_sense_clarification": "знаток"
                            },
                            {
                                "target_lang_word": "красавчик",
                                "target_lang_sense_clarification": "сленг, одобрение"
                            },
                            {
                                "target_lang_word": "профи",
                                "target_lang_sense_clarification": "профессионал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "406d8c09-a2df-3bd7-bd57-cacac1e64c8e",
                    "sense_definition": "Een zeer groot of indrukwekkend exemplaar van iets.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "emphasis_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft een <w>baas</w> van een auto gekocht met alles erop en eraan.",
                            "target_lang_translations": {
                                "en": "He bought a <w>beast</w> of a car with all the trimmings.",
                                "ru": "Он купил настоящую <w>махину</w>, а не машину, со всеми наворотами."
                            }
                        },
                        {
                            "text": "Er stond een <w>baas</w> van een huis aan de rand van het dorp.",
                            "target_lang_translations": {
                                "en": "There stood a <w>monster</w> of a house at the edge of the village.",
                                "ru": "На краю деревни стояла <w>громадина</w>, а не дом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een baas van een"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A very large or impressive specimen of something.",
                        "ru": "Очень большой или впечатляющий экземпляр чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beast",
                                "target_lang_sense_clarification": "impressive or large specimen"
                            },
                            {
                                "target_lang_word": "whopper",
                                "target_lang_sense_clarification": "large specimen"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "махина",
                                "target_lang_sense_clarification": "о большом предмете"
                            },
                            {
                                "target_lang_word": "громадина",
                                "target_lang_sense_clarification": "что-то огромное"
                            }
                        ]
                    }
                }
            ]
        },
        {

                    "sense_id": "c200519e-3731-33d7-ad9e-bff7180b4ab5",
                    "sense_definition": "Stan, w którym ponosi się odpowiedzialność za zrobienie czegoś złego, popełnienie błędu lub spowodowanie szkody.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "guilt_responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To nie jest moja <w>wina</w>, że spóźniliśmy się na autobus.",
                            "target_lang_translations": {
                                "ru": "Это не моя <w>вина</w>, что мы опоздали на автобус."
                            }
                        },
                        {
                            "text": "Mężczyzna przyznał się do <w>winy</w> przed sądem.",
                            "target_lang_translations": {
                                "ru": "Мужчина признал свою <w>вину</w> перед судом."
                            }
                        }

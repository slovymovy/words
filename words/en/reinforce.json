{
    "word_family": [
        "reinforcement",
        "reinforcer",
        "unreinforced"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3ef4bead-a006-3d1a-b7d1-2a537a14616c",
                    "sense_definition": "To make something stronger, especially by adding more material or support.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "strengthen_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>reinforced</w> the broken handle with a metal rod and some tape.",
                            "target_lang_translations": {
                                "ru": "Он <w>укрепил</w> сломанную ручку металлическим стержнем и изолентой."
                            }
                        },
                        {
                            "text": "The army sent more soldiers to <w>reinforce</w> the border defenses.",
                            "target_lang_translations": {
                                "ru": "Армия направила больше солдат, чтобы <w>усилить</w> оборону границы."
                            }
                        }
                    ],
                    "synonyms": [
                        "strengthen",
                        "augment",
                        "fortify",
                        "buttress",
                        "bolster"
                    ],
                    "antonyms": [
                        "weaken"
                    ],
                    "common_phrases": [
                        "reinforce the structure",
                        "reinforce the wall",
                        "reinforce the troops"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо более прочным, особенно путем добавления материала или поддержки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укреплять",
                                "target_lang_sense_clarification": "делать физически прочнее, надежнее"
                            },
                            {
                                "target_lang_word": "усиливать",
                                "target_lang_sense_clarification": "делать мощнее, добавлять ресурсы (например, войска)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2f6803b-41a2-3c15-a421-bbb86370c19b",
                    "sense_definition": "To make an idea, belief, or feeling stronger and more effective.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "strengthen_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The right homework will <w>reinforce</w> the lesson you learned in class.",
                            "target_lang_translations": {
                                "ru": "Правильное домашнее задание <w>укрепит</w> знания, полученные на уроке."
                            }
                        },
                        {
                            "text": "Recent events <w>reinforce</w> the need for better safety regulations.",
                            "target_lang_translations": {
                                "ru": "Недавние события <w>усиливают</w> необходимость в улучшении правил безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "emphasize",
                        "strengthen"
                    ],
                    "antonyms": [
                        "undermine"
                    ],
                    "common_phrases": [
                        "reinforce the message",
                        "reinforce the idea",
                        "reinforce a belief"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать идею, убеждение или чувство сильнее и эффективнее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подкреплять",
                                "target_lang_sense_clarification": "обосновывать доводами, фактами или доказательствами"
                            },
                            {
                                "target_lang_word": "усиливать",
                                "target_lang_sense_clarification": "повышать значимость, интенсивность чувства или потребности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b86681d6-72ce-3df0-9619-3473239778cf",
                    "sense_definition": "To encourage a specific behavior or response so that it happens more often.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "strengthen_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Advertising for fast food can <w>reinforce</w> unhealthy dietary tendencies.",
                            "target_lang_translations": {
                                "ru": "Реклама фастфуда может <w>усиливать</w> нездоровые пищевые пристрастия."
                            }
                        },
                        {
                            "text": "We should <w>reinforce</w> good behavior in children with praise and rewards.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>подкреплять</w> хорошее поведение детей похвалой и наградами."
                            }
                        }
                    ],
                    "synonyms": [
                        "encourage",
                        "reward"
                    ],
                    "antonyms": [
                        "discourage"
                    ],
                    "common_phrases": [
                        "reinforce behavior",
                        "reinforce stereotypes",
                        "positive reinforcement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поощрять определенное поведение или реакцию для их более частого повторения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подкреплять",
                                "target_lang_sense_clarification": "фиксировать рефлекс или поведение (психологический термин)"
                            },
                            {
                                "target_lang_word": "усиливать",
                                "target_lang_sense_clarification": "делать склонность или привычку более выраженной"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "adc5d42b-c9b9-36c6-9534-bc7c4f9dc534",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>lopen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>loop</w> elke dag naar mijn werk.",
                            "target_lang_translations": {
                                "ru": "Я <w>хожу</w> на работу каждый день."
                            }
                        },
                        {
                            "text": "Als het mooi weer is, <w>loop</w> ik graag buiten.",
                            "target_lang_translations": {
                                "ru": "Когда погода хорошая, я люблю <w>гулять</w> на улице."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "conjugation"
                        }
                    ],
                    "target_lang_definitions": {

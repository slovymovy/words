            "senses": [
                {
                    "sense_id": "24e06800-0e39-3d99-af98-2cb94f40967a",
                    "sense_definition": "Zacząć mieszkać w jakimś miejscu; wprowadzić się do domu, mieszkania lub miejscowości w celu stałego lub tymczasowego pobytu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "living_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Postanowiliśmy <zamieszkać> razem zaraz po ślubie.",
                            "target_lang_translations": {
                                "ru": "Мы решили <поселиться> вместе сразу после свадьбы."
                            }
                        },
                        {
                            "text": "Mój brat planuje <zamieszkać> w Warszawie, aby znaleźć lepszą pracę.",
                            "target_lang_translations": {
                                "ru": "Мой брат планирует <поселиться> в Варшаве, чтобы найти работу получше."
                            }
                        },
                        {
                            "text": "Podczas wakacji możecie <zamieszkać> w naszym domku letniskowym.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска вы можете <поселиться> в нашем летнем домике."
                            }
                        }
                    ],
                    "synonyms": [
                        "opóźnić się",
                        "opóźnić się"
                    ],
                    "antonyms": [
                        "wyprowadzić się"
                    ],
                    "common_phrases": [
                        "zamieszkać na stałe",
                        "zamieszkać razem",
                        "zamieszkać w hotelu",
                        "zamieszkać u rodziny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать жить в каком-либо месте; въехать в дом, квартиру или населенный пункт для постоянного или временного проживания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поселиться",
                                "target_lang_sense_clarification": "начать жить где-либо"
                            },
                            {
                                "target_lang_word": "обосноваться",
                                "target_lang_sense_clarification": "устроиться на жительство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd10e6d5-e008-3f38-8b5f-8abf70f406f9",
                    "sense_definition": "O uczuciach, nastrojach lub zjawiskach: pojawić się, zapanować gdzieś i trwać przez pewien czas.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy ojciec stracił pracę, w domu <zamieszkała> bieda.",
                            "target_lang_translations": {
                                "ru": "Когда отец потерял работу, в доме <поселилась> бедность."
                            }
                        },
                        {
                            "text": "W jej oczach <zamieszkał> lęk, gdy usłyszała dziwny hałas.",
                            "target_lang_translations": {
                                "ru": "В её глазах <поселился> страх, когда она услышала странный шум."
                            }
                        }
                    ],
                    "synonyms": [
                        "opóźnić się",
                        "opóźnić się"
                    ],
                    "common_phrases": [

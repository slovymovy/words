    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9dcc3c56-3942-3497-a813-012822e487c5",
                    "sense_definition": "Simple past and past participle of <w>wave</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She smiled and <waved> at us from across the street.",
                            "target_lang_translations": {
                                "ru": "Она улыбнулась и <помахала> нам с другой стороны улицы."
                            }
                        },
                        {
                            "text": "The large flag <waved> proudly in the strong wind.",
                            "target_lang_translations": {
                                "ru": "Большой флаг гордо <развевался> на сильном ветру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "waved goodbye",
                        "waved back",
                        "waved down",
                        "waved a hand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола \"wave\" (махать, развеваться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помахал",
                                "target_lang_sense_clarification": "сделал движение рукой в знак приветствия или прощания"
                            },
                            {
                                "target_lang_word": "развевался",
                                "target_lang_sense_clarification": "колебался на ветру (о флаге, ткани)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "529b91ed-25cf-33de-bd6f-08b86f214d3a",
                    "sense_definition": "Having a wave-like form or outline; undulating.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <waved> surface of the metal panel created interesting light reflections.",
                            "target_lang_translations": {
                                "ru": "<Волнистая> поверхность металлической панели создавала интересные световые отражения."
                            }
                        },
                        {
                            "text": "She preferred the <waved> texture of the fabric over the smooth one.",
                            "target_lang_translations": {
                                "ru": "Она предпочла <волнистую> фактуру ткани гладкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "wavy",
                        "undulating",
                        "rippled"
                    ],
                    "antonyms": [
                        "straight",
                        "flat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий волнообразную форму или очертания; неровный, изогнутый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волнистый",
                                "target_lang_sense_clarification": "имеющий форму волн"
                            },
                            {
                                "target_lang_word": "гофрированный",
                                "target_lang_sense_clarification": "с ребристой, волнообразной поверхностью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49e7fc9a-51ff-3e53-8271-b3394b15320c",
                    "sense_definition": "Having on the margin a succession of curved segments or incisions.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The botanist identified the plant by its distinctively <waved> leaves.",
                            "target_lang_translations": {
                                "ru": "Ботаник определил растение по его характерным <волнистым> листьям."
                            }
                        },
                        {
                            "text": "This species features a <waved> margin along the petals.",
                            "target_lang_translations": {
                                "ru": "Этот вид отличается <волнистым> краем лепестков."
                            }
                        }
                    ],
                    "synonyms": [
                        "sinuate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий по краю ряд изогнутых сегментов или вырезов (обычно о листьях в ботанике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волнистый",
                                "target_lang_sense_clarification": "о крае листа или лепестка"
                            },
                            {
                                "target_lang_word": "выемчатый",
                                "target_lang_sense_clarification": "ботанический термин для описания края листа"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
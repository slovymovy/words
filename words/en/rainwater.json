        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b85ff13d-8031-32e9-98a9-dd04ab4bb80e",
                    "sense_definition": "Water that has fallen as rain, especially when collected for use rather than coming from a spring, river, or public supply.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "water_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We use a large barrel to collect <w>rainwater</w> for the garden.",
                            "target_lang_translations": {
                                "ru": "Мы используем большую бочку, чтобы собирать <w>дождевую воду</w> для сада."
                            }
                        },
                        {
                            "text": "The <w>rainwater</w> is soft and excellent for washing clothes.",
                            "target_lang_translations": {
                                "ru": "<w>Дождевая вода</w> мягкая и отлично подходит для стирки одежды."
                            }
                        }
                    ],
                    "antonyms": [
                        "tap water",
                        "groundwater"
                    ],
                    "common_phrases": [
                        "collect rainwater",
                        "rainwater tank",
                        "rainwater harvesting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вода, выпавшая в виде дождя, особенно когда она собирается для использования, а не поступает из источника, реки или водопровода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дождевая вода",
                                "target_lang_sense_clarification": "вода от осадков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bd022ed-0aae-3c94-a585-13bf84b26330",
                    "sense_definition": "Water that accumulates or flows on the ground resulting from rainfall.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "water_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy <w>rainwaters</w> flooded the streets within minutes.",
                            "target_lang_translations": {
                                "ru": "Сильные <w>дождевые потоки</w> затопили улицы за считанные минуты."
                            }
                        },
                        {
                            "text": "Engineers designed a canal to divert excess <w>rainwater</w> away from the city center.",
                            "target_lang_translations": {
                                "ru": "Инженеры спроектировали канал для отвода излишков <w>дождевой воды</w> от центра города."
                            }
                        }
                    ],
                    "synonyms": [
                        "runoff"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вода, которая скапливается или течет по земле в результате дождя; поверхностный сток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дождевая вода",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "ливневые воды",
                                "target_lang_sense_clarification": "сточные воды после дождя"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a92a40e7-a9f1-3dd2-a60e-e559a64aceb9",
                    "sense_definition": "Перемещать кого-либо или что-либо изнутри помещения или закрытого пространства наружу.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое утро мне приходится <w>выносить</w> мусор на улицу.",
                            "target_lang_translations": {
                                "en": "Every morning I have to <w>take out</w> the trash."
                            }
                        },
                        {
                            "text": "Рабочие начали <w>выносить</w> старую мебель из квартиры перед ремонтом.",
                            "target_lang_translations": {
                                "en": "The workers started to <w>carry out</w> the old furniture from the apartment before the renovation."
                            }
                        }
                    ],
                    "synonyms": [
                        "уносить"
                    ],
                    "antonyms": [
                        "вносить"
                    ],
                    "common_phrases": [
                        "выносить мусор",
                        "выносить из дома",
                        "выносить на руках"
                    ],
                    "target_lang_definitions": {
                        "en": "To move someone or something from inside a place to the outside."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "general movement of objects outside"
                            },
                            {
                                "target_lang_word": "carry out",
                                "target_lang_sense_clarification": "physically lifting and moving something out"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f58f252-b59f-34c2-ba91-d116bcf0dc8d",
                    "sense_definition": "Быть в состоянии терпеть что-либо трудное, неприятное, болезненное или тяжелое; выдерживать испытания.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "endurance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я больше не могу <w>выносить</w> этот ужасный шум за стеной.",
                            "target_lang_translations": {
                                "en": "I can no longer <w>stand</w> this terrible noise behind the wall."
                            }
                        },
                        {
                            "text": "Этот человек умеет стойко <w>выносить</w> любые жизненные трудности.",
                            "target_lang_translations": {
                                "en": "This person knows how to steadfastly <w>endure</w> any of life's hardships."
                            }
                        }
                    ],
                    "synonyms": [
                        "уносить",
                        "уносить"
                    ],
                    "common_phrases": [
                        "не выносить",
                        "с трудом выносить",
                        "выносить боль"
                    ],
                    "target_lang_definitions": {
                        "en": "To be able to tolerate or withstand something difficult, unpleasant, or painful."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand",
                                "target_lang_sense_clarification": "often used in negative contexts (can't stand)"
                            },
                            {
                                "target_lang_word": "endure",
                                "target_lang_sense_clarification": "to suffer through hardships or pain patiently"
                            },
                            {
                                "target_lang_word": "bear",
                                "target_lang_sense_clarification": "formal or literary; to sustain a burden"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf9a9c91-3a58-382d-8b3b-24435d06f7fd",
                    "sense_definition": "Официально принимать, объявлять или оглашать (решение, приговор, вердикт).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "official_decision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Судья готовится <w>выносить</w> окончательный приговор по этому громкому делу.",
                            "target_lang_translations": {
                                "en": "The judge is preparing to <w>pass</w> the final sentence in this high-profile case."
                            }
                        },
                        {
                            "text": "Комиссия будет <w>выносить</w> решение о финансировании проекта на следующей неделе.",
                            "target_lang_translations": {
                                "en": "The commission will <w>render</w> a decision on the project funding next week."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выносить решение",
                        "выносить приговор",
                        "выносить вердикт",
                        "выносить благодарность"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "official"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To officially announce or deliver a legal or formal decision."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "used with sentences or judgments"
                            },
                            {
                                "target_lang_word": "render",
                                "target_lang_sense_clarification": "formal; used with verdicts or decisions"
                            },
                            {
                                "target_lang_word": "deliver",
                                "target_lang_sense_clarification": "to formally state a judgment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2580895-d6a7-38d3-88d0-279a0741166d",
                    "sense_definition": "Беременной, проносить плод в утробе до момента родов (обычно совершенный вид: вы́носить).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pregnancy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач сказал, что она сможет благополучно <w>выносить</w> и родить здорового ребёнка.",
                            "target_lang_translations": {
                                "en": "The doctor said that she would be able to safely <w>carry</w> and give birth to a healthy baby."
                            }
                        },
                        {
                            "text": "Ей было нелегко <w>выносить</w> двойню, но всё закончилось хорошо.",
                            "target_lang_translations": {
                                "en": "It was not easy for her to <w>carry</w> twins, but everything ended well."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выносить ребёнка",
                        "выносить плод"
                    ],
                    "target_lang_definitions": {
                        "en": "To carry a fetus in the womb until birth."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "carry",
                                "target_lang_sense_clarification": "to be pregnant with"
                            },
                            {
                                "target_lang_word": "carry to term",
                                "target_lang_sense_clarification": "to complete a pregnancy successfully"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c060675-a1d9-3de1-b728-77b897abcf81",
                    "sense_definition": "Долго обдумывать, создавать в мыслях (план, идею, проект) перед реализацией.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Писатель долго <w>выносил</w> замысел своего нового исторического романа.",
                            "target_lang_translations": {
                                "en": "The writer <w>nurtured</w> the concept of his new historical novel for a long time."
                            }
                        },
                        {
                            "text": "Прежде чем начать бизнес, нужно тщательно <w>выносить</w> эту идею.",
                            "target_lang_translations": {
                                "en": "Before starting a business, you need to carefully <w>incubate</w> this idea."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выносить идею",
                        "выносить план",
                        "выносить замысел"
                    ],
                    "target_lang_definitions": {
                        "en": "To think over, develop, or nurture a plan or idea in one's mind for a long time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nurture",
                                "target_lang_sense_clarification": "to develop an idea over time"
                            },
                            {
                                "target_lang_word": "hatch",
                                "target_lang_sense_clarification": "to create a plan, often secretly"
                            },
                            {
                                "target_lang_word": "incubate",
                                "target_lang_sense_clarification": "to let an idea develop slowly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9bcb6cf-b6ac-3915-9c03-6bd9ad54d2dd",
                    "sense_definition": "Сильно раздражать, надоедать разговорами или странным поведением (обычно в выражении «выносить мозг»).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_annoyance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хватит мне <w>выносить</w> мозг своими постоянными претензиями!",
                            "target_lang_translations": {
                                "en": "Stop <w>messing with</w> my head with your constant complaints!"
                            }
                        },
                        {
                            "text": "Эта сложная задача начинает реально <w>выносить</w> мне мозг.",
                            "target_lang_translations": {
                                "en": "This complex task is starting to really <w>fry</w> my brain."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выносить мозг"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To annoy someone intensely or confuse them (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mess with (someone's head)",
                                "target_lang_sense_clarification": "to confuse or manipulate psychologically"
                            },
                            {
                                "target_lang_word": "fry (someone's brain)",
                                "target_lang_sense_clarification": "to cause mental exhaustion or confusion"
                            },
                            {
                                "target_lang_word": "nag",
                                "target_lang_sense_clarification": "to annoy by constant complaining"
                            }
                        ]
                    }
                }
            ]
        }
    ]

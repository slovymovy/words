                    ],
                    "common_phrases": [
                        "творить отпуск",
                        "малый отпуск"
                    ],
                    "target_lang_definitions": {
                        "en": "The concluding prayer of a church service; the rite of absolution"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dismissal",
                                "target_lang_sense_clarification": "liturgical term"
                            }
                        ]
                    }
                }
            ]

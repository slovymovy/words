    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "93b7468b-dd0b-3aab-a65a-e400d392ce36",
                    "sense_definition": "A person who steals another person's property, especially secretly and without using force.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police arrested the <w>thief</w> who stole the expensive painting.",
                            "target_lang_translations": {
                                "ru": "Полиция арестовала <w>вора</w>, который украл дорогую картину."
                            }
                        },
                        {
                            "text": "He was known as a petty <w>thief</w> who often took fruit from the market stalls.",
                            "target_lang_translations": {
                                "ru": "Он был известен как мелкий <w>воришка</w>, который часто таскал фрукты с рыночных прилавков."
                            }
                        }
                    ],
                    "synonyms": [
                        "stealer",
                        "larcenist",
                        "purloiner",
                        "filcher",
                        "thiever"
                    ],
                    "common_phrases": [
                        "car thief",
                        "identity thief",
                        "petty thief",
                        "thick as thieves",
                        "stop thief"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который крадет чужую собственность, особенно тайно и без применения силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вор",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "воришка",
                                "target_lang_sense_clarification": "мелкий вор (разговорное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab20134e-88e0-3566-a5db-da4ed483ac79",
                    "sense_definition": "A flaw in a candle wick that causes it to melt unevenly and waste wax.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She trimmed the wick carefully to remove the <w>thief</w> that was making the candle burn too fast.",
                            "target_lang_translations": {
                                "ru": "Она осторожно подрезала фитиль, чтобы удалить <w>нагар</w>, из-за которого свеча сгорала слишком быстро."
                            }
                        },
                        {
                            "text": "The flame sputtered and dimmed because of a <w>thief</w> in the candle.",
                            "target_lang_translations": {
                                "ru": "Пламя трещало и тускнело из-за <w>нагара</w> на свече."
                            }
                        }
                    ],
                    "traits": [
                        {

{
    "word_family": [
        "inspect",
        "inspection",
        "inspectorship",
        "inspective"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c6fe308-f661-3305-8391-ed26ef4fed91",
                    "sense_definition": "A person whose job is to officially check that things are being done correctly or that rules are followed.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The health <w>inspector</w> visited the restaurant to ensure the kitchen was clean.",
                            "target_lang_translations": {
                                "ru": "Санитарный <w>инспектор</w> посетил ресторан, чтобы убедиться в чистоте кухни."
                            }
                        },
                        {
                            "text": "A ticket <w>inspector</w> walked through the train asking to see travel passes.",
                            "target_lang_translations": {
                                "ru": "Билетный <w>контролёр</w> прошёл по поезду, проверяя проездные билеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "examiner",
                        "checker",
                        "investigator",
                        "auditor"
                    ],
                    "common_phrases": [
                        "health inspector",
                        "ticket inspector",
                        "building inspector",
                        "school inspector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, чья работа заключается в официальной проверке правильности выполнения чего-либо или соблюдения правил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инспектор",
                                "target_lang_sense_clarification": "должностное лицо"
                            },
                            {
                                "target_lang_word": "ревизор",
                                "target_lang_sense_clarification": "проверяющий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6079a240-d345-3fed-983b-17b5c3c185f2",
                    "sense_definition": "A police officer of a rank usually above a sergeant and below a superintendent.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "police_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>inspector</w> arrived at the crime scene to interview the witnesses.",
                            "target_lang_translations": {
                                "ru": "<w>Инспектор</w> прибыл на место преступления, чтобы опросить свидетелей."
                            }
                        },
                        {
                            "text": "She was promoted to the rank of <w>inspector</w> after ten years of service.",
                            "target_lang_translations": {
                                "ru": "После десяти лет службы её повысили до звания <w>инспектора</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "detective inspector",
                        "chief inspector",
                        "police inspector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полицейский чин, обычно старше сержанта и младше суперинтенданта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инспектор",
                                "target_lang_sense_clarification": "звание в полиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad5b85dd-0962-3b10-8b8e-859040fb396a",
                    "sense_definition": "A software tool used to examine and debug code, files, or system elements.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "software_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Use the web <w>inspector</w> to look at the code behind the page.",
                            "target_lang_translations": {
                                "ru": "Используйте веб-<w>инспектор</w>, чтобы просмотреть код страницы."
                            }
                        },
                        {
                            "text": "The disk <w>inspector</w> found several errors in the file system.",
                            "target_lang_translations": {
                                "ru": "Дисковый <w>инспектор</w> обнаружил несколько ошибок в файловой системе."
                            }
                        }
                    ],
                    "synonyms": [
                        "debugger",
                        "viewer"
                    ],
                    "common_phrases": [
                        "web inspector",
                        "element inspector"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Программный инструмент, используемый для анализа и отладки кода, файлов или системных элементов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инспектор",
                                "target_lang_sense_clarification": "инструмент в ПО"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "oferować",
        "oferta",
        "oferent",
        "ofertowy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ea48214-5b72-3fbc-b292-e39e3511f05e",
                    "sense_definition": "Zaproponować komuś coś, wyrazić gotowość do przekazania czegoś, wykonania usługi lub sprzedaży towaru.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprzedawca <zaoferował> nam bardzo korzystną cenę za ten samochód.",
                            "target_lang_translations": {
                                "ru": "Продавец <w>предложил</w> нам очень выгодную цену за этот автомобиль."
                            }
                        },
                        {
                            "text": "Czy mogę <zaoferować> państwu filiżankę kawy lub herbaty?",
                            "target_lang_translations": {
                                "ru": "Могу ли я <w>предложить</w> вам чашечку кофе или чая?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zaproponować"
                    ],
                    "common_phrases": [
                        "zaoferować pomoc",
                        "zaoferować pracę",
                        "zaoferować wsparcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предложить кому-то что-то, выразить готовность передать что-то, выполнить услугу или продать товар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предложить",
                                "target_lang_sense_clarification": "сделать предложение о чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b83280b2-c63a-3cbb-a764-f7a1dc8f101e",
                    "sense_definition": "Wyrazić chęć i gotowość do zrobienia czegoś dla kogoś, zwykle używane z zaimkiem zwrotnym \"się\".",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat <zaoferował się>, że pomoże mi w przeprowadzce do nowego mieszkania.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>вызвался</w> помочь мне с переездом в новую квартиру."
                            }
                        },
                        {
                            "text": "Nikt nie <zaoferował się> do posprzątania sali po wczorajszym zebraniu.",
                            "target_lang_translations": {
                                "ru": "Никто не <w>вызвался</w> убрать зал после вчерашнего собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgłosić się"
                    ],
                    "common_phrases": [
                        "zaoferować się z pomocą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выразить желание и готовность сделать что-то для кого-то, обычно используется с возвратным местоимением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызваться",
                                "target_lang_sense_clarification": "добровольно предложить свою помощь"
                            },
                            {
                                "target_lang_word": "предложить",
                                "target_lang_sense_clarification": "свою помощь или услуги"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
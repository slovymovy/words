                            {
                                "target_lang_word": "redhead",
                                "target_lang_sense_clarification": "person"
                            },
                            {
                                "target_lang_word": "carrot top",
                                "target_lang_sense_clarification": "colloquial/teasing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рыжий",

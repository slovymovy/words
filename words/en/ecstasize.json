{
    "word_family": [
        "ecstasy",
        "ecstatic",
        "ecstatically"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2e529a67-3f60-34ad-a261-45dc03b9a08f",
                    "sense_definition": "To go into a state of ecstasy or rapture.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "state_of_ecstasy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The critics began to <w>ecstasize</w> over the new collection of poems.",
                            "target_lang_translations": {
                                "ru": "Критики начали <w>восторгаться</w> новым сборником стихов."
                            }
                        },
                        {
                            "text": "She tends to <w>ecstasize</w> whenever she talks about her favorite opera.",
                            "target_lang_translations": {
                                "ru": "Она склонна <w>приходить в экстаз</w> всякий раз, когда говорит о своей любимой опере."
                            }
                        }
                    ],
                    "synonyms": [
                        "rhapsodize",
                        "enthuse",
                        "swoon"
                    ],
                    "antonyms": [
                        "lament",
                        "complain"
                    ],
                    "common_phrases": [
                        "ecstasize over",
                        "ecstasize about"
                    ],
                    "target_lang_definitions": {
                        "ru": "Впадать в состояние экстаза или сильного восторга; бурно выражать восхищение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восторгаться",
                                "target_lang_sense_clarification": "бурно выражать восхищение чем-либо"
                            },
                            {
                                "target_lang_word": "приходить в экстаз",
                                "target_lang_sense_clarification": "впадать в состояние крайнего возбуждения"
                            },
                            {
                                "target_lang_word": "изливать восторги",
                                "target_lang_sense_clarification": "многословно выражать восхищение (часто иронично)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75372a1c-315e-3b13-8d05-1ad627224c7d",
                    "sense_definition": "To send into a state of ecstasy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "state_of_ecstasy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The vibrant music seemed to <w>ecstasize</w> the crowd completely.",
                            "target_lang_translations": {
                                "ru": "Казалось, что энергичная музыка полностью <w>привела в экстаз</w> толпу."
                            }
                        },
                        {
                            "text": "Fresh mountain air can <w>ecstasize</w> the spirit after a long week in the city.",
                            "target_lang_translations": {
                                "ru": "Свежий горный воздух может <w>привести в восторг</w> душу после долгой недели в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "enrapture",
                        "elate",
                        "transport",
                        "thrill"
                    ],
                    "antonyms": [
                        "depress",
                        "sadden",
                        "bore"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить кого-либо в состояние экстаза или высшего наслаждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приводить в экстаз",
                                "target_lang_sense_clarification": "вызывать состояние высшего восторга"
                            },
                            {
                                "target_lang_word": "восхищать",
                                "target_lang_sense_clarification": "вызывать чувство сильного удовлетворения и радости"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "5fb59fb1-aca0-31e2-b1d9-3f4c33b0daa7",
                    "sense_definition": "De werking van een apparaat of machine stoppen; uitschakelen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "turn_off",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil jij de televisie <w>uitzetten</w> voordat je naar bed gaat?",
                            "target_lang_translations": {
                                "ru": "Ты не мог бы <w>выключить</w> телевизор перед тем, как пойдешь спать?",
                                "en": "Would you <w>turn off</w> the television before you go to bed?"
                            }
                        },
                        {
                            "text": "Ik ben vergeten mijn computer <w>uit</w> te <w>zetten</w> op kantoor.",
                            "target_lang_translations": {
                                "ru": "Я забыл <w>выключить</w> свой компьютер в офисе.",
                                "en": "I forgot to <w>turn off</w> my computer at the office."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitschakelen",
                        "afzetten",
                        "afschakelen"
                    ],
                    "antonyms": [
                        "aanzetten",
                        "inschakelen"
                    ],
                    "common_phrases": [
                        "de tv uitzetten",
                        "het licht uitzetten",
                        "de motor uitzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остановить работу устройства или машины; выключить",
                        "en": "To stop the operation of a device or machine; to switch off"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выключать",
                                "target_lang_sense_clarification": "общая"
                            },
                            {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e643bb8a-be3b-3ddd-92a8-442a801045f0",
                    "sense_definition": "Публично произнести речь или исполнить номер перед аудиторией.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "public_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор школы <выступил> с приветственной речью перед учениками."
                        },
                        {
                            "text": "Известная певица <выступит> на городском празднике в эти выходные."
                        }
                    ],
                    "common_phrases": [
                        "выступить с докладом",
                        "выступить на сцене",
                        "выступить с речью",
                        "выступить на собрании"
                    ]
                },
                {
                    "sense_id": "23b3f192-9940-3040-a0b4-604c0b0c4cc9",
                    "sense_definition": "Сделать шаг вперёд, выйти из ряда или строя.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Офицер попросил добровольцев <выступить> из строя."
                        },
                        {
                            "text": "Он решил <выступить> вперёд, чтобы лучше видеть происходящее на сцене."
                        }
                    ],
                    "synonyms": [
                        "вы́йти"
                    ],
                    "common_phrases": [
                        "выступить вперёд",
                        "выступить из строя",
                        "выступить из тени"
                    ]
                },
                {
                    "sense_id": "051531b5-604e-39ed-9e23-38c73a0199eb",
                    "sense_definition": "Появиться на поверхности, просочиться наружу (о жидкости: поте, слезах, крови).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emergence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От сильного волнения у него на лбу <выступил> холодный пот."
                        },
                        {
                            "text": "Когда она услышала эту грустную новость, на её глазах <выступили> слёзы."

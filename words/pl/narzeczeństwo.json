            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a46b90c3-7a92-375f-865d-79fa05edddf6",
                    "sense_definition": "Okres od zaręczyn do ślubu, w którym para przygotowuje się do zawarcia małżeństwa.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ich <w>narzeczeństwo</w> trwało dwa lata, zanim ostatecznie stanęli na ślubnym kobiercu.",
                            "target_lang_translations": {
                                "ru": "Их <w>помолвка</w> длилась два года, прежде чем они наконец пошли под венец."
                            }
                        },
                        {
                            "text": "Wspominają okres <w>narzeczeństwa</w> jako najpiękniejszy i beztroski czas w ich życiu.",
                            "target_lang_translations": {
                                "ru": "Они вспоминают время <w>помолвки</w> как самое прекрасное и беззаботное время в их жизни."
                            }
                        }
                    ],
                    "common_phrases": [
                        "okres narzeczeństwa",
                        "czas narzeczeństwa",
                        "zerwać narzeczeństwo",
                        "długie narzeczeństwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени от помолвки до свадьбы, когда пара готовится к вступлению в брак."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помолвка",
                                "target_lang_sense_clarification": "период пребывания в статусе жениха и невесты"
                            },
                            {
                                "target_lang_word": "время помолвки",
                                "target_lang_sense_clarification": "период"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7eb4870-bd15-3980-8955-1a5cd30ccaa8",
                    "sense_definition": "Para osób, które są po zaręczynach i planują wziąć ślub.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młode <w>narzeczeństwo</w> zaprosiło rodziców na uroczystą kolację zapoznawczą.",
                            "target_lang_translations": {
                                "ru": "Молодые <w>жених и невеста</w> пригласили родителей на торжественный ужин знакомства."
                            }
                        },
                        {
                            "text": "To <w>narzeczeństwo</w> wygląda na bardzo szczęśliwe i zakochane.",
                            "target_lang_translations": {
                                "ru": "Эти <w>жених и невеста</w> выглядят очень счастливыми и влюбленными."
                            }
                        }
                    ],
                    "synonyms": [
                        "narzeczeni"
                    ],
                    "common_phrases": [
                        "młode narzeczeństwo",
                        "szczęśliwe narzeczeństwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пара людей (жених и невеста), которые обручились и планируют свадьбу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жених и невеста",
                                "target_lang_sense_clarification": "пара"
                            },
                            {
                                "target_lang_word": "обручённые",
                                "target_lang_sense_clarification": "люди"
                            }
                        ]
                    }
                }
            ]
        }
    ]

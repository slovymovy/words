{
    "word_family": [
        "waterdichtheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ce7d3bef-a11d-3264-8739-a55e0619a708",
                    "sense_definition": "Ondoordringbaar voor water; zo gemaakt dat er geen water in of uit kan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze jas is volledig <w>waterdicht</w>, dus je blijft droog tijdens de regen.",
                            "target_lang_translations": {
                                "ru": "Эта куртка полностью <w>водонепроницаемая</w>, поэтому ты останешься сухим под дождем."
                            }
                        },
                        {
                            "text": "Ik zoek een <w>waterdicht</w> horloge waarmee ik ook kan gaan zwemmen.",
                            "target_lang_translations": {
                                "ru": "Я ищу <w>водонепроницаемые</w> часы, в которых можно плавать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "waterdichte jas",
                        "waterdicht horloge",
                        "waterdichte schoenen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не пропускающий воду; сделанный так, что вода не может проникнуть внутрь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водонепроницаемый",
                                "target_lang_sense_clarification": "не пропускающий воду (об одежде, часах)"
                            },
                            {
                                "target_lang_word": "водостойкий",
                                "target_lang_sense_clarification": "устойчивый к воздействию воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15e42527-4d97-38a4-b558-f3c548584887",
                    "sense_definition": "Sluitend en onweerlegbaar; zonder fouten, gaten of mogelijkheden tot twijfel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte werd vrijgesproken omdat hij een <w>waterdicht</w> alibi had.",
                            "target_lang_translations": {
                                "ru": "Подозреваемого оправдали, так как у него было <w>неопровержимое</w> алиби."
                            }
                        },
                        {
                            "text": "Haar redenering was <w>waterdicht</w> en niemand kon er iets tegenin brengen.",
                            "target_lang_translations": {
                                "ru": "Её аргументация была <w>безупречной</w>, и никто не мог ничего возразить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "waterdicht alibi",
                        "waterdicht plan",
                        "waterdichte redenering"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопровержимый, убедительный; не имеющий изъянов или слабых мест (об аргументах, планах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неопровержимый",
                                "target_lang_sense_clarification": "об алиби, доказательствах"
                            },
                            {
                                "target_lang_word": "безупречный",
                                "target_lang_sense_clarification": "о плане, системе, логике"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
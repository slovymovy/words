{
    "word_family": [
        "suspension",
        "suspense",
        "suspender",
        "suspensive",
        "suspensory"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "098d5dff-3ce0-3f19-86be-839937def9d1",
                    "sense_definition": "Simple past and past participle of <w>suspend</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee <w>suspended</w> the rules to allow for an immediate vote.",
                            "target_lang_translations": {
                                "ru": "Комитет <w>приостановил</w> действие правил, чтобы провести немедленное голосование."
                            }
                        },
                        {
                            "text": "Production was <w>suspended</w> for two weeks due to a shortage of materials.",
                            "target_lang_translations": {
                                "ru": "Производство было <w>приостановлено</w> на две недели из-за нехватки материалов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола suspend."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приостановил",
                                "target_lang_sense_clarification": "глагол (прошедшее время)"
                            },
                            {
                                "target_lang_word": "приостановленный",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b50ce2ed-aacb-3696-a67d-3873f72c61b8",
                    "sense_definition": "Caused to stop for a while; interrupted or delayed.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "stopped",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player received a <w>suspended</w> sentence for his behavior off the field.",
                            "target_lang_translations": {
                                "ru": "Игрок получил <w>условный</w> срок за своё поведение вне поля."
                            }
                        },
                        {
                            "text": "The project remains in a state of <w>suspended</w> animation until more funding is secured.",
                            "target_lang_translations": {
                                "ru": "Проект остается в <w>подвешенном</w> состоянии, пока не будет обеспечено дополнительное финансирование."
                            }
                        }
                    ],
                    "synonyms": [
                        "paused",
                        "delayed",
                        "inactive"
                    ],
                    "common_phrases": [
                        "suspended sentence",
                        "suspended animation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временно остановленный, прерванный или отложенный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приостановленный",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "условный",
                                "target_lang_sense_clarification": "юридический термин (о приговоре)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e8b7035-8db7-3d92-b82f-fedc6a54ad48",

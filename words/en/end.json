                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The referee blew the whistle to <w>end</w> the game.",
                            "target_lang_translations": {
                                "ru": "Судья дал свисток, чтобы <w>закончить</w> игру."
                            }
                        },
                        {
                            "text": "The government promised to <w>end</w> the conflict soon.",
                            "target_lang_translations": {
                                "ru": "Правительство обещало вскоре <w>прекратить</w> конфликт."
                            }
                        }
                    ],
                    "synonyms": [
                        "finish",
                        "terminate",
                        "conclude",
                        "stop",
                        "finalize",
                        "discontinue"
                    ],
                    "antonyms": [
                        "begin",
                        "start",
                        "launch",
                        "commence"
                    ],
                    "common_phrases": [
                        "end it all"
                    ],
                    "target_lang_definitions": {

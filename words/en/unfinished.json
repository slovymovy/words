{
    "word_family": [
        "finish",
        "finisher"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6839560e-4723-368a-8d78-b4d555e8dc5b",
                    "sense_definition": "Not brought to an end or conclusion.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "incomplete_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist left the painting <w>unfinished</w> when he moved to another city.",
                            "target_lang_translations": {
                                "ru": "Художник оставил картину <w>незаконченной</w>, когда переехал в другой город."
                            }
                        },
                        {
                            "text": "We still have some <w>unfinished</w> business to discuss before the meeting ends.",
                            "target_lang_translations": {
                                "ru": "У нас ещё осталось <w>незаконченное</w> дело, которое нужно обсудить до конца собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "incomplete",
                        "uncompleted"
                    ],
                    "antonyms": [
                        "finished#Adjective",
                        "completed#Adjective",
                        "done#Adjective",
                        "resolved#Adjective"
                    ],
                    "common_phrases": [
                        "unfinished business",
                        "unfinished work",
                        "unfinished symphony",
                        "unfinished task"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не доведённый до конца или завершения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "незаконченный",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "незавершённый",
                                "target_lang_sense_clarification": "о процессах, делах или строительстве"
                            },
                            {
                                "target_lang_word": "неоконченный",
                                "target_lang_sense_clarification": "о произведениях искусства или образовании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45871d57-bc96-303b-a3a0-a80469841c54",
                    "sense_definition": "Not treated with a final protective coating, polish, or paint.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "lacking_surface_treatment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a table made of <w>unfinished</w> pine and decided to stain it herself.",
                            "target_lang_translations": {
                                "ru": "Она купила стол из <w>необработанной</w> сосны и решила сама покрыть его морилкой."
                            }
                        },
                        {
                            "text": "The carpenter sanded the <w>unfinished</w> wood until it was perfectly smooth.",
                            "target_lang_translations": {
                                "ru": "Плотник шлифовал <w>необработанную</w> древесину, пока она не стала идеально гладкой."
                            }
                        }
                    ],
                    "antonyms": [
                        "varnished#Adjective",
                        "polished#Adjective",
                        "painted#Adjective",
                        "coated#Adjective"
                    ],
                    "common_phrases": [
                        "unfinished furniture",
                        "unfinished wood",
                        "unfinished surface"
                    ],
                    "target_lang_definitions": {
                        "ru": "Необработанный защитным слоем, лаком или краской."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "необработанный",
                                "target_lang_sense_clarification": "о материале (дереве, камне)"
                            },
                            {
                                "target_lang_word": "неотделанный",
                                "target_lang_sense_clarification": "о поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c348e68-f404-3ea5-ab54-f0a4933ccb7b",
                    "sense_definition": "Lacking final construction details such as wall coverings or flooring.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "lacking_surface_treatment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They use the cold, <w>unfinished</w> basement primarily for storing old boxes.",
                            "target_lang_translations": {
                                "ru": "Они используют холодный <w>неотделанный</w> подвал в основном для хранения старых коробок."
                            }
                        },
                        {
                            "text": "The house features an <w>unfinished</w> attic that could easily be converted into a bedroom.",
                            "target_lang_translations": {
                                "ru": "В доме есть <w>неотделанный</w> чердак, который можно легко переделать в спальню."
                            }
                        }
                    ],
                    "antonyms": [
                        "fitted out"
                    ],
                    "common_phrases": [
                        "unfinished basement",
                        "unfinished attic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без чистовой отделки (стен, пола и т.д.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неотделанный",
                                "target_lang_sense_clarification": "о помещении без ремонта"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
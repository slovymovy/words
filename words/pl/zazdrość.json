    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8c166e7f-976c-3b77-8bb7-fca26078d4ab",
                    "sense_definition": "Uczucie niezadowolenia wywołane sukcesem lub stanem posiadania innej osoby, połączone z pragnieniem posiadania tego samego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie Bogdan z <w>zazdrością</w> patrzy na nowy samochód sąsiada.",
                            "target_lang_translations": {
                                "ru": "Каждый день Богдан с <w>завистью</w> смотрит на новую машину соседа."
                            }
                        },
                        {
                            "text": "Czuła ogromną <w>zazdrość</w>, gdy jej siostra wygrała prestiżowy konkurs.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала огромную <w>зависть</w>, когда её сестра выиграла престижный конкурс."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawiść"
                    ],
                    "antonyms": [
                        "podziw"
                    ],
                    "common_phrases": [
                        "zzielenieć z zazdrości",
                        "ukłucie zazdrości",
                        "patrzeć z zazdrością"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство недовольства, вызванное успехом или имуществом другого человека, в сочетании с желанием обладать тем же самым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зависть",
                                "target_lang_sense_clarification": "желание иметь то, что есть у других"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59859ad7-89f5-3c8f-9318-d7bce8f86dfa",
                    "sense_definition": "Podejrzenie lub lęk, że ukochana osoba może być niewierna lub interesuje się kimś innym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Monikę gnębiła <w>zazdrość</w> o swojego męża.",
                            "target_lang_translations": {
                                "ru": "Монику мучила <w>ревность</w> к своему мужу."
                            }
                        },
                        {
                            "text": "Jego chorobliwa <w>zazdrość</w> stała się główną przyczyną rozpadu ich małżeństwa.",
                            "target_lang_translations": {
                                "ru": "Его болезненная <w>ревность</w> стала главной причиной распада их брака."
                            }
                        }
                    ],
                    "synonyms": [
                        "zielonooki potwór"
                    ],
                    "antonyms": [
                        "zaufanie"
                    ],
                    "common_phrases": [
                        "scena zazdrości",
                        "chory z zazdrości",
                        "napad zazdrości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подозрение или страх, что любимый человек может быть неверен или интересуется кем-то другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ревность",
                                "target_lang_sense_clarification": "сомнение в верности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "33d1ddae-b0f3-350e-b1d9-b1008220645a",
                    "sense_definition": "Druga osoba liczby pojedynczej trybu rozkazującego czasownika <w>zazdrościć</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>zazdrość</w> innym sukcesu, lecz ciężko pracuj na własny.",
                            "target_lang_translations": {
                                "ru": "Не <w>завидуй</w> успеху других, а усердно работай над собственным."
                            }
                        },
                        {
                            "text": "<w>Zazdrość</w> mu do woli, ale to nie zmieni twojej sytuacji.",
                            "target_lang_translations": {
                                "ru": "<w>Завидуй</w> ему сколько угодно, но это не изменит твоей ситуации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie zazdrość"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма повелительного наклонения глагола zazdrościć (завидовать) во втором лице единственного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завидуй",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

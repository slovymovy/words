            "senses": [
                {
                    "sense_id": "a594043c-3c84-3c1c-bcd6-e55e3ed5e11f",
                    "sense_definition": "Poczuć się dotkniętym czyimś zachowaniem lub słowami, okazać niezadowolenie milczeniem lub zerwaniem kontaktu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat <obraził> się na mnie, bo zjadłem jego ciastko.",
                            "target_lang_translations": {
                                "ru": "Мой брат <обиделся> на меня, потому что я съел его печенье."
                            }
                        },
                        {
                            "text": "Nie warto <obrazić> się o taką drobną pomyłkę.",
                            "target_lang_translations": {
                                "ru": "Не стоит <обижаться> из-за такой мелкой ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "nadąsać się"
                    ],
                    "common_phrases": [
                        "obrazić się na kogoś",
                        "obrazić się o byle co",
                        "śmiertelnie się obrazić"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny używany z \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Почувствовать обиду из-за чьего-то поведения или слов, выразить недовольство молчанием или прекращением общения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обидеться",
                                "target_lang_sense_clarification": "почувствовать обиду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa5436f9-a515-36d7-a90c-f65f99e127b1",
                    "sense_definition": "Sprawić komuś przykrość, naruszyć czyjąś godność lub dobre imię niegrzecznym zachowaniem albo słowami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twoje ostre słowa mogą kogoś niechcący <obrazić>.",
                            "target_lang_translations": {
                                "ru": "Твои резкие слова могут кого-то нечаянно <обидеть>."
                            }
                        },
                        {
                            "text": "Ten artykuł w gazecie <obraził> wielu czytelników.",
                            "target_lang_translations": {
                                "ru": "Эта статья в газете <оскорбила> многих читателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "znieważyć",
                        "urazić",
                        "dotknąć",
                        "zranić",
                        "upokorzyć"
                    ],
                    "common_phrases": [
                        "obrazić kogoś słowem",
                        "obrazić uczucia religijne",
                        "ciężko obrazić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинить кому-то боль, задеть чье-то достоинство или доброе имя грубым поведением или словами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обидеть",
                                "target_lang_sense_clarification": "причинить душевную боль"
                            },
                            {
                                "target_lang_word": "оскорбить",

                    "synonyms": [
                        "мне́ние"
                    ],
                    "common_phrases": [
                        "в один голос",
                        "голос народа",
                        "голос критики"
                    ],
                    "target_lang_definitions": {
                        "en": "The expressed opinion, judgment, or demand of a group of people."

                "word": "MMP",
                "pos": "noun",
                "lang_code": "en",
                "forms": [
                    {
                        "form_id": "3daa79f8-8148-4129-a549-4367ac2a6dfa",
                        "entry_id": "71a6fe30-544a-3355-9d12-f326ff96e64b",

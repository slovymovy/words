            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ce31d13-1295-3b8a-882c-0cad43fdbe44",
                    "sense_definition": "Een vervoermiddel dat regelmatig heen en weer rijdt tussen twee specifieke plaatsen, vaak over een korte afstand.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er ging een <w>shuttle</w> naar het hotel.",
                            "target_lang_translations": {
                                "en": "There was a <w>shuttle</w> to the hotel."
                            }
                        },
                        {
                            "text": "We nemen de <w>shuttle</w> van het vliegveld naar het centrum van de stad.",
                            "target_lang_translations": {
                                "en": "We are taking the <w>shuttle</w> from the airport to the city center."
                            }
                        }
                    ],
                    "synonyms": [
                        "pendelbus"
                    ],
                    "common_phrases": [
                        "met de shuttle",
                        "shuttle naar het vliegveld"
                    ],
                    "target_lang_definitions": {
                        "en": "A vehicle that travels back and forth regularly between two specific places, often over a short distance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shuttle",
                                "target_lang_sense_clarification": "transport service"
                            },
                            {
                                "target_lang_word": "shuttle bus",
                                "target_lang_sense_clarification": "vehicle type"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c5b3a08-a6f4-3098-b633-e0fe4f199c5c",
                    "sense_definition": "Een projectiel bestaande uit een kurken dop en veren (of plastic) dat bij badminton wordt gebruikt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>shuttle</w> wordt na de opslag zo vaak mogelijk heen en weer over het net gespeeld.",
                            "target_lang_translations": {
                                "en": "After the serve, the <w>shuttlecock</w> is hit back and forth over the net as often as possible."
                            }
                        },
                        {
                            "text": "Hij sloeg de <w>shuttle</w> met veel kracht terug naar zijn tegenstander.",
                            "target_lang_translations": {
                                "en": "He hit the <w>shuttlecock</w> back to his opponent with great force."
                            }
                        }

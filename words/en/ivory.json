    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7af2d953-1972-372a-83c0-be867a1d5b0a",
                    "sense_definition": "The hard white form of dentin which forms the tusks of elephants, walruses, and other animals.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "material_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "International laws strictly ban the illegal <w>ivory</w> trade to protect endangered elephants.",
                            "target_lang_translations": {
                                "ru": "Международные законы строго запрещают незаконную торговлю <w>слоновой костью</w> для защиты исчезающих слонов."
                            }
                        },
                        {
                            "text": "The museum displayed a beautiful statue carved from pure <w>ivory</w>.",
                            "target_lang_translations": {
                                "ru": "В музее была выставлена прекрасная статуя, вырезанная из чистой <w>слоновой кости</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ivory trade",
                        "carved ivory",
                        "piece of ivory",
                        "ivory tusk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердое белое вещество, из которого состоят бивни слонов, моржей и других животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слоновая кость",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2c71e59-20e8-33cc-9ea3-17efca87ad23",
                    "sense_definition": "A creamy white color, the color of ivory.",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9669c29e-2368-3df3-9edc-1daf5ebd73e2",
                    "sense_definition": "Een nieuw bestand, profiel of account digitaal creëren of registreren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet even een nieuw account <w>aanmaken</w> voor deze website.",
                            "target_lang_translations": {
                                "en": "I just need to <w>create</w> a new account for this website.",
                                "ru": "Мне нужно <w>создать</w> новый аккаунт для этого сайта."
                            }
                        },
                        {
                            "text": "De systeembeheerder heeft gisteren een profiel voor de nieuwe werknemer <w>aangemaakt</w>.",
                            "target_lang_translations": {
                                "en": "The system administrator <w>created</w> a profile for the new employee yesterday.",
                                "ru": "Вчера системный администратор <w>создал</w> профиль для нового сотрудника."
                            }
                        }
                    ],
                    "synonyms": [
                        "creëren",
                        "openen",
                        "maken"
                    ],
                    "antonyms": [
                        "verwijderen",
                        "wissen"
                    ],
                    "common_phrases": [
                        "een account aanmaken",
                        "een wachtwoord aanmaken",
                        "een profiel aanmaken"
                    ],
                    "target_lang_definitions": {
                        "en": "To digitally create or register a new file, profile, or account.",
                        "ru": "Создать или зарегистрировать новый файл, профиль или аккаунт в цифровом виде."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "create",
                                "target_lang_sense_clarification": "a digital account, profile, or file"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "создать",
                                "target_lang_sense_clarification": "аккаунт, профиль или файл"
                            },
                            {
                                "target_lang_word": "завести",
                                "target_lang_sense_clarification": "аккаунт или учетную запись (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0b35639-f8f0-3615-88de-0ec2bc907348",
                    "sense_definition": "Een vuur of kachel aansteken zodat deze gaat branden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de winter <w>maken</w> we elke avond de open haard <w>aan</w>.",
                            "target_lang_translations": {
                                "en": "In the winter, we <w>light</w> the fireplace every evening.",
                                "ru": "Зимой мы каждый вечер <w>растапливаем</w> камин."
                            }
                        },
                        {
                            "text": "Gebruik de afzuigkap niet wanneer je de kachel <w>aanmaakt</w>.",
                            "target_lang_translations": {
                                "en": "Do not use the extractor hood when you <w>light</w> the stove.",
                                "ru": "Не используйте вытяжку, когда <w>растапливаете</w> печь."
                            }
                        }
                    ],
                    "synonyms": [
                        "aansteken",
                        "ontsteken"
                    ],
                    "antonyms": [
                        "doven",
                        "uitmaken"
                    ],
                    "common_phrases": [
                        "de kachel aanmaken",
                        "het vuur aanmaken",
                        "de haard aanmaken"
                    ],
                    "target_lang_definitions": {
                        "en": "To ignite a fire or stove so that it starts burning.",
                        "ru": "Разжечь огонь или растопить печь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "light",
                                "target_lang_sense_clarification": "a fire, stove, or fireplace"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "растопить",
                                "target_lang_sense_clarification": "печь или камин"
                            },
                            {
                                "target_lang_word": "разжечь",
                                "target_lang_sense_clarification": "огонь или костер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a359bdfd-dbf4-3307-92c5-5b666b74d8e4",
                    "sense_definition": "Eten bereiden of op smaak brengen door ingrediënten te mengen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt de salade <w>aanmaken</w> met een beetje olijfolie en azijn.",
                            "target_lang_translations": {
                                "en": "You can <w>dress</w> the salad with a little olive oil and vinegar.",
                                "ru": "Салат можно <w>заправить</w> небольшим количеством оливкового масла и уксуса."
                            }
                        },
                        {
                            "text": "Mijn moeder <w>maakt</w> het gehakt altijd <w>aan</w> met speciale kruiden en een ei.",
                            "target_lang_translations": {
                                "en": "My mother always <w>seasons</w> the minced meat with special herbs and an egg.",
                                "ru": "Моя мама всегда <w>заправляет</w> фарш особыми специями и яйцом."
                            }
                        }
                    ],
                    "synonyms": [
                        "bereiden",
                        "mengen",
                        "klaarmaken"
                    ],
                    "common_phrases": [
                        "de sla aanmaken",
                        "gehakt aanmaken",
                        "saus aanmaken"
                    ],
                    "target_lang_definitions": {
                        "en": "To prepare food or season it by mixing ingredients.",
                        "ru": "Приготовить еду или приправить её, смешивая ингредиенты."
                    },
                    "translations": {
                        "en": [],
                        "ru": [
                            {
                                "target_lang_word": "заправить",
                                "target_lang_sense_clarification": "салат или блюдо соусом/специями"
                            },
                            {
                                "target_lang_word": "приготовить",
                                "target_lang_sense_clarification": "смесь ингредиентов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d118c515-0c08-3987-ab2d-83447b45bf13",
                    "sense_definition": "Een stof of voorraad produceren (vaak biologisch of industrieel).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het lichaam zal vanzelf antistoffen tegen het virus <w>aanmaken</w>.",
                            "target_lang_translations": {
                                "en": "The body will naturally <w>produce</w> antibodies against the virus.",
                                "ru": "Организм сам начнет <w>вырабатывать</w> антитела против вируса."
                            }
                        },
                        {
                            "text": "Voor de oorlog <w>maakten</w> zij nieuwe wapenvoorraden <w>aan</w>.",
                            "target_lang_translations": {
                                "en": "Before the war, they <w>produced</w> new stockpiles of weapons.",
                                "ru": "Перед войной они <w>создавали</w> новые запасы оружия."
                            }
                        }
                    ],
                    "synonyms": [
                        "produceren",
                        "vervaardigen",
                        "vormen"
                    ],
                    "common_phrases": [
                        "antistoffen aanmaken",
                        "voorraad aanmaken",
                        "weefsel aanmaken"
                    ],
                    "target_lang_definitions": {
                        "en": "To produce a substance or supply (often biologically or industrially).",
                        "ru": "Производить вещество или создавать запас (часто биологически или промышленно)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "produce",

    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f5c5ad56-5df3-343c-9020-221cf2ac33ef",
                    "sense_definition": "Straight; not crooked, oblique, or circuitous; leading by the short or shortest way to a point or end.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_path",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We took the most <w>direct</w> route to the beach.",
                            "target_lang_translations": {
                                "ru": "Мы выбрали самый <w>прямой</w> маршрут к пляжу."
                            }
                        },
                        {
                            "text": "Is there a <w>direct</w> flight to London?",
                            "target_lang_translations": {
                                "ru": "Есть ли <w>прямой</w> рейс в Лондон?"
                            }
                        }
                    ],
                    "synonyms": [
                        "immediate"
                    ],
                    "antonyms": [
                        "indirect"
                    ],
                    "common_phrases": [
                        "direct route",
                        "direct flight",
                        "direct access"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямой; не изогнутый; ведущий кратчайшим путем к цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямой",
                                "target_lang_sense_clarification": "о пути, линии или маршруте"
                            },
                            {
                                "target_lang_word": "беспересадочный",
                                "target_lang_sense_clarification": "о рейсе или поездке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e954830-66eb-3bcd-b39d-75af8a3cff13",
                    "sense_definition": "Straightforward; sincere.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please be <w>direct</w> with me about the problem.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, будь <w>откровенен</w> со мной по поводу этой проблемы."
                            }
                        },
                        {
                            "text": "I appreciate your <w>direct</w> answer.",
                            "target_lang_translations": {
                                "ru": "Я ценю твой <w>прямой</w> ответ."
                            }
                        }
                    ],
                    "antonyms": [
                        "indirect"
                    ],
                    "common_phrases": [
                        "direct answer",
                        "be direct with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямолинейный; искренний; говорящий то, что думает."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямой",
                                "target_lang_sense_clarification": "откровенный, честный"
                            },
                            {
                                "target_lang_word": "прямолинейный",
                                "target_lang_sense_clarification": "о характере или манере общения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f22020f7-be54-38f1-bb95-bd7d009e715f",
                    "sense_definition": "Immediate; express; plain; unambiguous.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His success is a <w>direct</w> result of hard work.",
                            "target_lang_translations": {
                                "ru": "Его успех — это <w>непосредственный</w> результат упорного труда."
                            }
                        },
                        {
                            "text": "There is no <w>direct</w> evidence linking him to the crime.",
                            "target_lang_translations": {
                                "ru": "Нет <w>прямых</w> улик, связывающих его с преступлением."
                            }
                        }
                    ],
                    "synonyms": [
                        "explicit",
                        "patent",
                        "univocal"
                    ],
                    "antonyms": [
                        "indirect"
                    ],
                    "common_phrases": [
                        "direct result",
                        "direct consequence",
                        "direct evidence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непосредственный; явный; очевидный; не имеющий промежуточных звеньев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непосредственный",
                                "target_lang_sense_clarification": "следующий сразу, без посредников"
                            },
                            {
                                "target_lang_word": "прямой",
                                "target_lang_sense_clarification": "явный, очевидный (например, доказательства)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "068fcdaf-63aa-3a33-8172-77062f452ea1",
                    "sense_definition": "In the line of descent; not collateral.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "lineage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a <w>direct</w> descendant of the king.",
                            "target_lang_translations": {
                                "ru": "Он <w>прямой</w> потомок короля."
                            }
                        },
                        {
                            "text": "The property passed down the <w>direct</w> line.",
                            "target_lang_translations": {
                                "ru": "Имущество перешло по <w>прямой</w> линии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "direct descendant",
                        "direct line"
                    ],
                    "target_lang_definitions": {
                        "ru": "По прямой линии (о родстве); кровный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямой",
                                "target_lang_sense_clarification": "о родственной связи"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b62ad104-e228-395b-ab88-d8bb47b3c15f",
                    "sense_definition": "To manage, control, steer.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wants to <w>direct</w> a movie one day.",
                            "target_lang_translations": {
                                "ru": "Она хочет однажды <w>срежиссировать</w> фильм."
                            }
                        },
                        {
                            "text": "The manager will <w>direct</w> operations from the headquarters.",
                            "target_lang_translations": {
                                "ru": "Менеджер будет <w>руководить</w> операциями из штаб-квартиры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "direct a movie",
                        "direct operations",
                        "direct a project"
                    ],
                    "target_lang_definitions": {
                        "ru": "Управлять, руководить; ставить (фильм, спектакль)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руководить",
                                "target_lang_sense_clarification": "управлять людьми или процессами"
                            },
                            {
                                "target_lang_word": "режиссировать",
                                "target_lang_sense_clarification": "ставить фильм или спектакль"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "контролировать ход чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a40205b-ad95-3588-b8aa-1d373c79dd73",
                    "sense_definition": "To aim something at something else.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "aim",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>direct</w> your attention to the screen.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>обратите</w> ваше внимание на экран."
                            }
                        },
                        {
                            "text": "He <w>directed</w> his questions to the speaker.",
                            "target_lang_translations": {
                                "ru": "Он <w>адресовал</w> свои вопросы докладчику."
                            }
                        }
                    ],
                    "common_phrases": [
                        "direct attention to",
                        "direct questions to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять (внимание, вопросы, усилия) на кого-либо или что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "направлять",
                                "target_lang_sense_clarification": "устремлять на что-либо"
                            },
                            {
                                "target_lang_word": "обращать",
                                "target_lang_sense_clarification": "фокусировать (внимание)"
                            },
                            {
                                "target_lang_word": "адресовать",
                                "target_lang_sense_clarification": "вопросы или замечания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d78001b-d070-3ce5-82e8-b1e10235b65f",
                    "sense_definition": "To point out to or show somebody the right course or way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_path",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>direct</w> me to the nearest station?",
                            "target_lang_translations": {
                                "ru": "Можете <w>подсказать дорогу</w> до ближайшей станции?"
                            }
                        },
                        {
                            "text": "The officer <w>directed</w> the traffic away from the accident.",
                            "target_lang_translations": {
                                "ru": "Офицер <w>направлял</w> движение в сторону от места аварии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "direct someone to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывать дорогу; направлять движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "указывать дорогу",
                                "target_lang_sense_clarification": "объяснять, как пройти"
                            },
                            {
                                "target_lang_word": "направлять",
                                "target_lang_sense_clarification": "указывать верный путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea58c8f2-1154-33bf-91a2-6be030f670c0",
                    "sense_definition": "To point out to with authority; to instruct as a superior; to order.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge <w>directed</w> the jury to ignore the comment.",
                            "target_lang_translations": {
                                "ru": "Судья <w>дал указание</w> присяжным игнорировать этот комментарий."
                            }
                        },
                        {
                            "text": "She <w>directed</w> the staff to clean the room.",
                            "target_lang_translations": {
                                "ru": "Она <w>распорядилась</w>, чтобы персонал убрал комнату."
                            }
                        }
                    ],
                    "common_phrases": [
                        "direct someone to do something"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать официальные указания; предписывать; приказывать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давать указания",
                                "target_lang_sense_clarification": "инструктировать"
                            },
                            {
                                "target_lang_word": "предписывать",
                                "target_lang_sense_clarification": "официально требовать исполнения"
                            }
                        ]
                    }
                }
            ]
        },
        {

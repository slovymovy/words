    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7a341548-3e4c-34fe-b92a-8a63d5114606",
                    "sense_definition": "Относящийся к королю или королеве, принадлежащий им или связанный с монархией.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В центре столицы расположен величественный <королевский> дворец.",
                            "target_lang_translations": {
                                "en": "A majestic <w>royal</w> palace is located in the center of the capital."
                            }
                        },
                        {
                            "text": "Вся <королевская> семья собралась на торжественный обед.",
                            "target_lang_translations": {
                                "en": "The entire <w>royal</w> family gathered for a ceremonial dinner."
                            }
                        }
                    ],
                    "synonyms": [
                        "монарший",
                        "царский"
                    ],
                    "common_phrases": [
                        "королевская семья",
                        "королевский двор",
                        "королевская кровь",
                        "королевский трон"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a king or queen, belonging to them, or associated with the monarchy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "royal",
                                "target_lang_sense_clarification": "relating to a monarch or their family"
                            },
                            {
                                "target_lang_word": "kingly",
                                "target_lang_sense_clarification": "pertaining specifically to a king"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41333823-d710-3c5f-9d93-4535597c35ab",
                    "sense_definition": "Отличающийся необыкновенной роскошью, богатством или щедростью; великолепный.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он сделал своей невесте поистине <королевский> подарок.",
                            "target_lang_translations": {
                                "en": "He gave his bride a truly <w>royal</w> gift."
                            }
                        },
                        {
                            "text": "Гостей ждал <королевский> ужин с множеством изысканных блюд.",
                            "target_lang_translations": {
                                "en": "A <w>royal</w> dinner with many exquisite dishes awaited the guests."
                            }
                        }
                    ],
                    "synonyms": [
                        "роскошный",
                        "великолепный",
                        "шикарный",
                        "богатый"
                    ],
                    "antonyms": [
                        "скромный",
                        "убогий",
                        "бедный"
                    ],
                    "common_phrases": [
                        "королевский подарок",
                        "королевская щедрость",
                        "королевский размах"
                    ],
                    "target_lang_definitions": {
                        "en": "Distinguished by unusual luxury, wealth, or generosity; magnificent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "royal",
                                "target_lang_sense_clarification": "magnificent or generous (metaphorical)"
                            },
                            {
                                "target_lang_word": "regal",
                                "target_lang_sense_clarification": "majestic, magnificent"
                            },
                            {
                                "target_lang_word": "kingly",
                                "target_lang_sense_clarification": "generous, magnificent"
                            }
                        ]
                    }
                }
            ]
        }
    ]

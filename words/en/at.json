{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "37cdb4f0-c069-3390-8b70-fca7769ce677",
                    "sense_definition": "Used to indicate a specific location, place, or position.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will meet you <w>at</w> the bus stop in ten minutes.",
                            "target_lang_translations": {
                                "ru": "Я встречу тебя <w>на</w> автобусной остановке через десять минут."
                            }
                        },
                        {
                            "text": "She is sitting <w>at</w> her desk working on a new project.",
                            "target_lang_translations": {
                                "ru": "Она сидит <w>за</w> своим рабочим столом и работает над новым проектом."
                            }
                        }
                    ],
                    "synonyms": [
                        "in",
                        "near"
                    ],
                    "common_phrases": [
                        "at home",
                        "at school",
                        "at work",
                        "at the door",
                        "at the bottom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания конкретного местоположения, места или позиции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "внутри помещения или учреждения"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "на мероприятии или открытом пространстве"
                            },
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "рядом, около чего-либо"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "сидеть за столом/работой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1122cc58-1623-3d22-afe5-b614f0fb55d9",
                    "sense_definition": "Used to indicate a specific point in time or a particular moment.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie starts <w>at</w> eight o'clock in the evening.",
                            "target_lang_translations": {
                                "ru": "Фильм начинается <w>в</w> восемь часов вечера."
                            }
                        },
                        {
                            "text": "We usually eat lunch together <w>at</w> noon.",
                            "target_lang_translations": {
                                "ru": "Мы обычно обедаем вместе <w>в</w> полдень."
                            }
                        }
                    ],
                    "common_phrases": [
                        "at night",
                        "at noon",
                        "at the moment",
                        "at present",
                        "at times"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания конкретного момента времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "с указанием времени (часы, время суток)"
                            }
                        ]

            "senses": [
                {
                    "sense_id": "bb042854-c8eb-3071-a520-e2c58428f538",
                    "sense_definition": "A person's sense of self-esteem, self-importance, or opinion of themselves.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "self_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has such a big <w>ego</w> that he refuses to admit when he is wrong."
                        },
                        {
                            "text": "Winning the championship was a huge <w>ego</w> boost for the entire team."
                        }
                    ],
                    "common_phrases": [
                        "big ego",
                        "ego boost",
                        "bruise someone's ego",
                        "ego trip"
                    ]
                },
                {
                    "sense_id": "8dc6ec39-89c9-39ba-83e2-a1c29ce2c7c7",
                    "sense_definition": "The part of the mind that mediates between the conscious and the unconscious and is responsible for reality testing and a sense of personal identity.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "self_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "According to Freud, the <w>ego</w> must balance our primitive desires with the demands of the real world."
                        },
                        {
                            "text": "A healthy <w>ego</w> allows a person to adapt to their environment without losing their sense of self."
                        }
                    ],
                    "common_phrases": [
                        "id and ego"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "psychology"
                        }
                    ]
                },
                {
                    "sense_id": "66fa48f6-57a5-377c-b77c-1aeb2c9b94b3",
                    "sense_definition": "The self, considered as a distinct individual entity, often viewed as an obstacle to spiritual enlightenment.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "self_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many spiritual traditions teach that suffering arises from attachment to the <w>ego</w>."
                        },
                        {
                            "text": "Meditation practice often involves observing the <w>ego</w> without identifying with it."
                        }
                    ],
                    "synonyms": [
                        "I"
                    ],
                    "common_phrases": [
                        "let go of ego",
                        "death of the ego"
                    ]
                },
                {
                    "sense_id": "f836272b-db8f-3895-b29c-3e5fa493494a",
                    "sense_definition": "The specific individual from whose perspective a family tree or kinship chart is described.",

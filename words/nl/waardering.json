                    "sense_id": "dba1bfaa-2864-3be3-b6e6-06b2810e7205",
                    "sense_definition": "Het besef dat iets of iemand van waarde is, of het tonen van dankbaarheid daarvoor.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "appreciation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur sprak zijn <w>waardering</w> uit over de behaalde resultaten.",
                            "target_lang_translations": {
                                "en": "The director expressed his <w>appreciation</w> for the achieved results.",
                                "ru": "Директор выразил свою <w>признательность</w> за достигнутые результаты."
                            }
                        },
                        {
                            "text": "Als blijk van <w>waardering</w> kreeg het personeel een kerstpakket.",
                            "target_lang_translations": {
                                "en": "As a token of <w>appreciation</w>, the staff received a Christmas hamper.",
                                "ru": "В знак <w>признательности</w> персонал получил рождественский набор."
                            }
                        }
                    ],
                    "synonyms": [
                        "erkenning",
                        "appreciatie",
                        "dankbaarheid"
                    ],
                    "antonyms": [
                        "minachting",
                        "afkeuring"
                    ],
                    "common_phrases": [
                        "waardering hebben voor",
                        "blijk van waardering",
                        "waardering tonen"
                    ],
                    "target_lang_definitions": {
                        "en": "The realization that something or someone is of value, or the showing of gratitude for it.",
                        "ru": "Осознание ценности чего-либо или кого-либо, или выражение благодарности за это."
                    },

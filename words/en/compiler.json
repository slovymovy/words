{
    "word_family": [
        "compile",
        "compilation",
        "recompile",
        "decompile"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "83a84059-5a04-33f8-9fcd-82f5bbc243b3",
                    "sense_definition": "A computer program that translates code written in a high-level programming language into machine language that a computer can execute.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "software_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>compiler</w> found a syntax error in the source code.",
                            "target_lang_translations": {
                                "ru": "<w>Компилятор</w> обнаружил синтаксическую ошибку в исходном коде."
                            }
                        },
                        {
                            "text": "You need a C++ <w>compiler</w> to run this specific program.",
                            "target_lang_translations": {
                                "ru": "Вам нужен <w>компилятор</w> C++ для запуска этой конкретной программы."
                            }
                        }
                    ],
                    "antonyms": [
                        "interpreter"
                    ],
                    "common_phrases": [
                        "compiler error",
                        "code compiler",
                        "optimizing compiler"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа, которая переводит код, написанный на языке высокого уровня, в машинный язык, понятный компьютеру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компилятор",
                                "target_lang_sense_clarification": "программа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88f4ef4a-2359-3171-bc74-f2c1be585624",
                    "sense_definition": "A person who gathers and arranges materials, such as information, texts, or puzzles, into a collection or list.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agent_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>compiler</w> of this anthology selected the best poems from the last century.",
                            "target_lang_translations": {
                                "ru": "<w>Составитель</w> этой антологии отобрал лучшие стихотворения прошлого века."
                            }
                        },
                        {
                            "text": "She works as a crossword <w>compiler</w> for a major newspaper.",
                            "target_lang_translations": {
                                "ru": "Она работает <w>составителем</w> кроссвордов в крупной газете."
                            }
                        }
                    ],
                    "synonyms": [
                        "collector",
                        "editor",
                        "anthologist"
                    ],
                    "common_phrases": [
                        "crossword compiler",
                        "dictionary compiler"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который собирает и систематизирует материалы (информацию, тексты, задачи) в коллекцию или список."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составитель",
                                "target_lang_sense_clarification": "автор сборника"
                            },
                            {
                                "target_lang_word": "компилятор",
                                "target_lang_sense_clarification": "человек"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
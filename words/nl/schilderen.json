    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ed785941-ffc4-3ea2-b471-da929ef295ff",
                    "sense_definition": "Een afbeelding of kunstwerk maken met verf op doek of papier.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "art_creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De beroemde kunstenaar <w>schilderde</w> vooral portretten van rijke mensen.",
                            "target_lang_translations": {
                                "ru": "Знаменитый художник <w>писал</w> в основном портреты богатых людей."
                            }
                        },
                        {
                            "text": "In de les beeldende kunst leren de kinderen met waterverf <w>schilderen</w>.",
                            "target_lang_translations": {
                                "ru": "На уроке изобразительного искусства дети учатся <w>рисовать</w> акварелью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een portret schilderen",
                        "een landschap schilderen",
                        "met olieverf schilderen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать изображение или произведение искусства красками на холсте или бумаге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "писать",
                                "target_lang_sense_clarification": "создавать картины"
                            },
                            {
                                "target_lang_word": "рисовать",
                                "target_lang_sense_clarification": "создавать изображение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56574ff6-e59f-32eb-be36-1c812bee41ad",
                    "sense_definition": "Een laag verf aanbrengen op een muur, gebouw of voorwerp.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "maintenance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader gaat morgen de kozijnen van het huis <w>schilderen</w>.",
                            "target_lang_translations": {
                                "ru": "Мой папа завтра будет <w>красить</w> оконные рамы дома."
                            }
                        },
                        {
                            "text": "We hebben besloten de muren in de woonkamer wit te <w>schilderen</w>.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>покрасить</w> стены в гостиной в белый цвет."
                            }
                        }
                    ],
                    "synonyms": [
                        "verven"
                    ],
                    "common_phrases": [
                        "de muur schilderen",
                        "het huis schilderen",
                        "de deur schilderen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить слой краски на стену, здание или предмет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "красить",
                                "target_lang_sense_clarification": "наносить краску"
                            }
                        ]
                    }
                }
            ]
        }
    ]

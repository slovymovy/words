        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e80cd6b9-7701-3879-8300-c71dcca0f7b5",
                    "sense_definition": "The quality of being truthful, sincere, and free from deceit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "moral_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I really appreciate your <w>honesty</w> regarding the mistake you made.",
                            "target_lang_translations": {
                                "ru": "Я очень ценю твою <w>честность</w> касательно совершенной ошибки."
                            }
                        },
                        {
                            "text": "In all <w>honesty</w>, I do not think this plan is going to work.",
                            "target_lang_translations": {
                                "ru": "Со всей <w>искренностью</w>, я не думаю, что этот план сработает."
                            }
                        }
                    ],
                    "synonyms": [
                        "truthfulness",
                        "sincerity",
                        "candor",
                        "integrity"
                    ],
                    "antonyms": [
                        "dishonesty"
                    ],
                    "common_phrases": [
                        "in all honesty",
                        "honesty is the best policy",
                        "brutal honesty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество быть правдивым, искренним и свободным от обмана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "честность",
                                "target_lang_sense_clarification": "свойство быть честным"
                            },
                            {
                                "target_lang_word": "искренность",
                                "target_lang_sense_clarification": "отсутствие притворства, чистосердечие"
                            },
                            {
                                "target_lang_word": "правдивость",
                                "target_lang_sense_clarification": "склонность говорить правду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f0b303d-a491-3a70-bcc5-67f73ed9518b",
                    "sense_definition": "A plant of the genus Lunaria, grown for its purple flowers and flat, round, translucent seed pods.",

                    "frequency": "High",
                    "semantic_group_id": "9",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I wrote a <w>check</w> to pay for the rent this month.",
                            "target_lang_translations": {
                                "ru": "Я выписал <w>чек</w> для оплаты аренды в этом месяце."
                            }
                        },
                        {
                            "text": "Many stores no longer accept payment by personal <w>check</w>.",
                            "target_lang_translations": {
                                "ru": "Многие магазины больше не принимают оплату персональным <w>чеком</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cheque"
                    ],
                    "common_phrases": [
                        "write a check",
                        "cash a check",
                        "blank check"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US"

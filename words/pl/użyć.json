            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b4ae00e4-b07a-34ac-9826-7cd5d37cb381",
                    "sense_definition": "Skorzystać z czegoś (narzędzia, metody, słowa) jako środka do osiągnięcia celu lub wykonania czynności.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "use_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli chcesz powiesić ten obraz, musisz <użyć> wiertarki i kołków.",
                            "target_lang_translations": {
                                "ru": "Если хочешь повесить эту картину, тебе нужно <воспользоваться> дрелью и дюбелями."
                            }
                        },
                        {
                            "text": "Policjant ostrzegł, że w razie oporu będzie musiał <użyć> siły fizycznej.",
                            "target_lang_translations": {
                                "ru": "Полицейский предупредил, что в случае сопротивления будет вынужден <применить> физическую силу."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykorzystać",
                        "zastosować",
                        "posłużyć się"
                    ],
                    "common_phrases": [
                        "użyć siły",
                        "użyć podstępu",
                        "użyć słowa",
                        "użyć broni",
                        "użyć wyobraźni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать что-либо (инструмент, метод, слово) как средство для достижения цели или выполнения действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "использовать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "применить",
                                "target_lang_sense_clarification": "метод, силу, оружие"
                            },
                            {
                                "target_lang_word": "воспользоваться",
                                "target_lang_sense_clarification": "инструментом, случаем"
                            },
                            {
                                "target_lang_word": "употребить",
                                "target_lang_sense_clarification": "слово, термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de1fd78d-2a46-3046-afd6-5a5eb6175486",
                    "sense_definition": "Zastosować substancję, taką jak kosmetyk, lekarstwo lub przyprawa, nakładając ją lub dodając do czegoś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "use_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyjściem na elegancką kolację Anna <użyła> swoich ulubionych perfum.",
                            "target_lang_translations": {
                                "ru": "Перед выходом на изысканный ужин Анна <воспользовалась> своими любимыми духами."
                            }
                        },
                        {
                            "text": "Do przygotowania tego sosu kucharz <użył> dużej ilości ostrej papryki.",
                            "target_lang_translations": {
                                "ru": "Для приготовления этого соуса повар <использовал> большое количество острого перца."
                            }
                        }
                    ],
                    "synonyms": [
                        "zastosować",
                        "nałożyć"

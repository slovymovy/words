    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "88b51ae7-0bf7-3021-b8bb-dd9b33ae02ee",
                    "sense_definition": "Een vloeistof met een lekkere geur die mensen op hun huid of kleding doen om lekker te ruiken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij kocht een duur flesje <w>parfum</w> voor haar verjaardag.",
                            "target_lang_translations": {
                                "en": "She bought an expensive bottle of <w>perfume</w> for her birthday.",
                                "ru": "Она купила дорогой флакон <w>духов</w> на свой день рождения."
                            }
                        },
                        {
                            "text": "Welk <w>parfum</w> draag jij vandaag?",
                            "target_lang_translations": {
                                "en": "Which <w>perfume</w> are you wearing today?",
                                "ru": "Какие <w>духи</w> на тебе сегодня?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "flesje parfum",
                        "parfum opdoen",
                        "duur parfum"
                    ],
                    "target_lang_definitions": {
                        "en": "A liquid with a pleasant scent applied to the skin or clothing to smell good.",
                        "ru": "Жидкость с приятным запахом, которую наносят на кожу или одежду, чтобы приятно пахнуть."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "perfume",
                                "target_lang_sense_clarification": "cosmetic product"
                            },
                            {
                                "target_lang_word": "fragrance",
                                "target_lang_sense_clarification": "scent product"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "духи",
                                "target_lang_sense_clarification": "косметическое средство"
                            },
                            {
                                "target_lang_word": "парфюм",
                                "target_lang_sense_clarification": "разговорное, общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afc02e5e-7a85-32ec-b394-2e4e6664267b",
                    "sense_definition": "Een aangename geur, vaak afkomstig van bloemen, planten of andere natuurlijke bronnen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rozen in de tuin verspreiden een heerlijk <w>parfum</w>.",
                            "target_lang_translations": {
                                "en": "The roses in the garden spread a lovely <w>fragrance</w>.",
                                "ru": "Розы в саду источают чудесный <w>аромат</w>."
                            }
                        },
                        {
                            "text": "Het <w>parfum</w> van de dennenbomen was in het hele bos te ruiken.",
                            "target_lang_translations": {
                                "en": "The <w>scent</w> of the pine trees could be smelled throughout the forest.",
                                "ru": "<w>Аромат</w> сосен чувствовался во всём лесу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zoet parfum",
                        "heerlijk parfum",
                        "parfum van bloemen"
                    ],
                    "target_lang_definitions": {
                        "en": "A pleasant smell, often originating from flowers, plants, or other natural sources.",
                        "ru": "Приятный запах, часто исходящий от цветов, растений или других природных источников."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scent",
                                "target_lang_sense_clarification": "natural smell"
                            },
                            {
                                "target_lang_word": "fragrance",
                                "target_lang_sense_clarification": "pleasant smell"
                            },
                            {
                                "target_lang_word": "perfume",
                                "target_lang_sense_clarification": "pleasant odor"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "аромат",
                                "target_lang_sense_clarification": "приятный запах"
                            },
                            {
                                "target_lang_word": "благоухание",
                                "target_lang_sense_clarification": "интенсивный приятный запах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0beb3753-b644-3f0f-a1f6-1b7a5e5ecf7e",
                    "sense_definition": "Een kenmerkende sfeer of uitstraling die ergens hangt (figuurlijk).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "atmosphere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er hing een <w>parfum</w> van geheimzinnigheid in de oude bibliotheek.",
                            "target_lang_translations": {
                                "en": "An <w>air</w> of mystery hung in the old library.",
                                "ru": "В старой библиотеке витал <w>флёр</w> таинственности."
                            }
                        },
                        {
                            "text": "Het <w>parfum</w> van de overwinning maakte het team euforisch.",
                            "target_lang_translations": {
                                "en": "The <w>scent</w> of victory made the team euphoric.",
                                "ru": "<w>Аромат</w> победы привел команду в эйфорию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "parfum van nostalgie",
                        "parfum van gevaar"
                    ],
                    "target_lang_definitions": {
                        "en": "A characteristic atmosphere or aura present in a place (figurative).",
                        "ru": "Характерная атмосфера или аура, которая ощущается где-либо (в переносном смысле)."
                    },
                    "translations": {

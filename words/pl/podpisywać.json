    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ac860452-a00e-341b-9d92-746ba394a8f6",
                    "sense_definition": "Składać swój podpis na dokumencie, aby potwierdzić jego treść, wyrazić zgodę lub nadać mu ważność.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "signing_docs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę jutro iść do banku i <podpisywać> ważną umowę kredytową.",
                            "target_lang_translations": {
                                "ru": "Завтра мне нужно пойти в банк и <w>подписывать</w> важный кредитный договор."
                            }
                        },
                        {
                            "text": "Dyrektor nie ma teraz czasu <podpisywać> tych wszystkich dokumentów.",
                            "target_lang_translations": {
                                "ru": "У директора сейчас нет времени <w>подписывать</w> все эти документы."
                            }
                        }
                    ],
                    "synonyms": [
                        "sygnować",
                        "parafować",
                        "zatwierdzać"
                    ],
                    "common_phrases": [
                        "podpisywać umowę",
                        "podpisywać dokumenty",
                        "podpisywać listę obecności",
                        "podpisywać petycję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ставить подпись на документе, чтобы подтвердить его содержание, выразить согласие или придать ему юридическую силу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписывать",
                                "target_lang_sense_clarification": "ставить подпись на документе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b391b5bf-e574-3dd7-a6d8-50537d82d97b",
                    "sense_definition": "Pisać swoje imię i nazwisko, zazwyczaj w celu identyfikacji lub potwierdzenia czegoś (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "signing_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <podpisywać> się czytelnie w prawym dolnym rogu formularza.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>подписывайтесь</w> разборчиво в правом нижнем углу бланка."
                            }
                        },
                        {
                            "text": "Mój dziadek lubił <podpisywać> się pełnym imieniem i nazwiskiem.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка любил <w>подписываться</w> полным именем и фамилией."
                            }
                        }
                    ],
                    "common_phrases": [
                        "podpisywać się czytelnie",
                        "podpisywać się nazwiskiem",
                        "podpisywać się inicjałami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Писать свое имя и фамилию, обычно для идентификации или подтверждения чего-либо (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписываться",
                                "target_lang_sense_clarification": "ставить свою подпись"
                            },
                            {
                                "target_lang_word": "расписываться",
                                "target_lang_sense_clarification": "ставить росчерк, подпись"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b4402e8-9841-357e-93ee-487cd6ff5247",
                    "sense_definition": "Umieszczać napis informacyjny pod ilustracją, zdjęciem lub na przedmiocie, aby go zidentyfikować lub wyjaśnić.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "labeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił uczniów, aby zaczęli <podpisywać> swoje zeszyty.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников начать <w>подписывать</w> свои тетради."
                            }
                        },
                        {
                            "text": "Lubię dokładnie <podpisywać> zdjęcia w rodzinnym albumie, żeby pamiętać daty.",
                            "target_lang_translations": {
                                "ru": "Я люблю подробно <w>подписывать</w> фотографии в семейном альбоме, чтобы помнить даты."
                            }
                        }
                    ],
                    "synonyms": [
                        "opisywać",
                        "oznaczać",
                        "tytułować"
                    ],
                    "common_phrases": [
                        "podpisywać zdjęcia",
                        "podpisywać zeszyty",
                        "podpisywać pudełka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать надпись под иллюстрацией, фотографией или на предмете для его идентификации или пояснения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подписывать",
                                "target_lang_sense_clarification": "делать надпись, помечать"
                            }
                        ]
                    }
                }
            ]
        }
    ]

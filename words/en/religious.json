    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5c9beae8-ef92-3095-875c-393f846faee0",
                    "sense_definition": "Relating to or concerning religion or spiritual beliefs.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The constitution protects the <w>religious</w> freedom of all citizens.",
                            "target_lang_translations": {
                                "ru": "Конституция защищает <w>религиозную</w> свободу всех граждан."
                            }
                        },
                        {
                            "text": "They had a long discussion about their different <w>religious</w> beliefs.",
                            "target_lang_translations": {
                                "ru": "У них была долгая дискуссия об их различных <w>религиозных</w> убеждениях."
                            }
                        }
                    ],
                    "antonyms": [
                        "secular",
                        "irreligious",
                        "profane",
                        "atheistic"
                    ],
                    "common_phrases": [
                        "religious beliefs",
                        "religious freedom",
                        "religious group",
                        "religious education"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к религии или духовным верованиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "религиозный",
                                "target_lang_sense_clarification": "связанный с религией"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afeedc4a-4512-3170-97b4-d47d14341bb7",
                    "sense_definition": "Believing strongly in a religion and following its rules and practices.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "personal_attribute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She grew up in a deeply <w>religious</w> family that attended church every Sunday.",
                            "target_lang_translations": {
                                "ru": "Она выросла в глубоко <w>религиозной</w> семье, которая посещала церковь каждое воскресенье."
                            }
                        },
                        {
                            "text": "He is not very <w>religious</w>, but he respects the traditions of his culture.",
                            "target_lang_translations": {
                                "ru": "Он не очень <w>религиозен</w>, но уважает традиции своей культуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "devout",
                        "pious",
                        "faithful"
                    ],
                    "antonyms": [
                        "irreligious",
                        "areligious"
                    ],
                    "common_phrases": [
                        "deeply religious",
                        "religious person",
                        "religious family"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искренне верующий, соблюдающий религиозные предписания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верующий",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "религиозный",
                                "target_lang_sense_clarification": "набожный, соблюдающий обряды"
                            },
                            {
                                "target_lang_word": "набожный",
                                "target_lang_sense_clarification": "глубоко верующий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59503a0c-53bd-3938-b5d6-4e81ae656654",
                    "sense_definition": "Extremely strict, regular, or conscientious in doing something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He checks his emails with <w>religious</w> regularity every single morning.",
                            "target_lang_translations": {
                                "ru": "Он проверяет электронную почту с <w>неукоснительной</w> регулярностью каждое утро."
                            }
                        },
                        {
                            "text": "The scientist paid <w>religious</w> attention to every detail of the experiment.",
                            "target_lang_translations": {
                                "ru": "Ученый уделял <w>скрупулезное</w> внимание каждой детали эксперимента."
                            }
                        }
                    ],
                    "synonyms": [
                        "conscientious",
                        "scrupulous",
                        "strict"
                    ],
                    "common_phrases": [
                        "religious attention",
                        "religious zeal",
                        "religious regularity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно строгий, тщательный или добросовестный (в переносном смысле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрупулезный",
                                "target_lang_sense_clarification": "о внимании к деталям"
                            },
                            {
                                "target_lang_word": "неукоснительный",
                                "target_lang_sense_clarification": "о соблюдении правил или регулярности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c902406-232b-3dec-97fe-d2fac842056b",
                    "sense_definition": "Belonging to a specific order of monks, nuns, or priests within a church.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "ecclesiastical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>religious</w> priest lives within a community and follows the rule of his order.",
                            "target_lang_translations": {
                                "ru": "<w>Монашествующий</w> священник живет в общине и следует уставу своего ордена."
                            }
                        },
                        {
                            "text": "She decided to join a <w>religious</w> order and dedicate her life to service.",
                            "target_lang_translations": {
                                "ru": "Она решила вступить в <w>монашеский</w> орден и посвятить свою жизнь служению."
                            }
                        }
                    ],
                    "antonyms": [
                        "secular",
                        "diocesan"
                    ],
                    "common_phrases": [
                        "religious order",
                        "religious life",
                        "religious community"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к монашескому ордену или общине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монашеский",
                                "target_lang_sense_clarification": "относящийся к монахам или ордену"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b2b886c-8e49-364f-b4bf-145b6ca53a33",
                    "sense_definition": "A member of a religious order, such as a monk, nun, or friar.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ecclesiastical_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>religious</w> gathered in the chapel for their evening prayers.",
                            "target_lang_translations": {
                                "ru": "<w>Монахи</w> собрались в часовне на вечернюю молитву."
                            }
                        },
                        {
                            "text": "As a <w>religious</w>, he took vows of poverty, chastity, and obedience.",
                            "target_lang_translations": {
                                "ru": "Как <w>монах</w>, он дал обеты бедности, целомудрия и послушания."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член религиозного ордена (монах или монахиня)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монах",
                                "target_lang_sense_clarification": "мужчина"
                            },
                            {
                                "target_lang_word": "монахиня",

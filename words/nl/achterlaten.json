    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dcc14e25-3cc4-38af-8ca3-163df7c613dd",
                    "sense_definition": "Weggaan van een plek zonder iemand of iets mee te nemen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben onze koffers in het hotel <w>achtergelaten</w>.",
                            "target_lang_translations": {
                                "ru": "Мы <w>оставили</w> наши чемоданы в отеле.",
                                "en": "We <w>left</w> our suitcases <w>behind</w> at the hotel."
                            }
                        },
                        {
                            "text": "De inbrekers <w>lieten</w> geen enkel spoor <w>achter</w>.",
                            "target_lang_translations": {
                                "ru": "Взломщики не <w>оставили</w> ни единого следа.",
                                "en": "The burglars <w>left</w> no trace <w>behind</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verlaten",
                        "laten staan"
                    ],
                    "antonyms": [
                        "meenemen"
                    ],
                    "common_phrases": [
                        "een bericht achterlaten",
                        "sporen achterlaten",
                        "rommel achterlaten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уйти или уехать, не взяв с собой кого-то или что-то.",
                        "en": "To go away from a place without taking someone or something with you."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставлять",
                                "target_lang_sense_clarification": "не брать с собой"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "leave behind",
                                "target_lang_sense_clarification": "not take along"
                            },
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "let remain"
                            },
                            {
                                "target_lang_word": "abandon",
                                "target_lang_sense_clarification": "desert or forsake"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4668b72c-3c66-339f-9be2-1bcdeb63e9a2",
                    "sense_definition": "Nabestaanden alleen laten door te overlijden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stierf jong en <w>liet</w> een vrouw en twee kinderen <w>achter</w>.",
                            "target_lang_translations": {
                                "ru": "Он умер молодым и <w>оставил</w> жену и двоих детей.",
                                "en": "He died young and <w>left</w> a wife and two children <w>behind</w>."
                            }
                        },
                        {
                            "text": "De oorlog heeft veel weeskinderen <w>achtergelaten</w>.",
                            "target_lang_translations": {
                                "ru": "Война <w>оставила</w> много сирот.",
                                "en": "The war <w>left</w> many orphans <w>behind</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een gezin achterlaten",
                        "een leegte achterlaten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставить родных одних в результате смерти.",
                        "en": "To leave survivors alone by dying."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставлять",
                                "target_lang_sense_clarification": "после смерти"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "leave behind",
                                "target_lang_sense_clarification": "after death"
                            },
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "be survived by"
                            }
                        ]
                    }
                }
            ]
        }
    ]

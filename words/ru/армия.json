    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "90af2156-b5aa-35d7-8bea-9aa5cf23698f",
                    "sense_definition": "Совокупность всех вооружённых сил государства (сухопутных, морских и воздушных).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "military_forces",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом году наша <w>армия</w> получила новую технику.",
                            "target_lang_translations": {
                                "en": "This year our <w>army</w> received new equipment."
                            }
                        },
                        {
                            "text": "Президент заявил, что <w>армия</w> готова защищать страну.",
                            "target_lang_translations": {
                                "en": "The president stated that the <w>army</w> is ready to defend the country."
                            }
                        }
                    ],
                    "synonyms": [
                        "вооружённые силы",
                        "войско"
                    ],
                    "antonyms": [
                        "гражданское население"
                    ],
                    "common_phrases": [
                        "служить в армии",
                        "российская армия",
                        "профессиональная армия"
                    ],
                    "target_lang_definitions": {
                        "en": "The aggregate of all armed forces of a state (land, sea, and air)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "army",
                                "target_lang_sense_clarification": "general term for a country's military forces"
                            },
                            {
                                "target_lang_word": "armed forces",
                                "target_lang_sense_clarification": "formal term for all military branches"
                            },
                            {
                                "target_lang_word": "military",
                                "target_lang_sense_clarification": "general reference to the armed forces"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "569e82ea-3ad2-3e26-b97b-47f1ef26e4a2",
                    "sense_definition": "Служба в вооружённых силах, выполнение воинского долга.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "military_service",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сразу после школы моего брата забрали в <w>армию</w>.",
                            "target_lang_translations": {
                                "en": "Immediately after school, my brother was drafted into the <w>army</w>."
                            }
                        },
                        {
                            "text": "Для многих молодых людей <w>армия</w> становится школой жизни.",
                            "target_lang_translations": {
                                "en": "For many young people, the <w>army</w> becomes a school of life."
                            }
                        }
                    ],
                    "synonyms": [
                        "армейка"
                    ],
                    "antonyms": [
                        "гражданка"
                    ],
                    "common_phrases": [
                        "идти в армию",
                        "вернуться из армии",
                        "забрать в армию"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to refer to the period of conscription or service"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Service in the armed forces, fulfilling military duty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "army",

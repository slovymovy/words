        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "89984b0b-6948-3952-a3e4-f53a6687312c",
                    "sense_definition": "Used in radio communications to acknowledge that a message has been received and understood.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Roger</w>, I have your position and am moving to intercept.",
                            "target_lang_translations": {
                                "ru": "<w>Вас понял</w>, я определил вашу позицию и выдвигаюсь на перехват."
                            }
                        },
                        {
                            "text": "Control tower, this is Flight 505, <w>roger</w> that.",
                            "target_lang_translations": {
                                "ru": "Диспетчерская вышка, это рейс 505, <w>вас понял</w>."
                            }

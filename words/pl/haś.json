            "senses": [
                {
                    "sense_id": "ff760b54-e973-35ca-a4b5-1ff24a6b3baf",
                    "sense_definition": "w gwarze śląskiej: popiół, pozostałość po spaleniu węgla w piecu",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz codziennie wybierać <w>haś</w> z pieca, żeby dobrze grzało.",
                            "target_lang_translations": {
                                "ru": "Ты должен каждый день выгребать <w>золу</w> из печи, чтобы хорошо грело."
                            }
                        },
                        {
                            "text": "Wysypałem zimny <w>haś</w> do kubła na zewnątrz.",
                            "target_lang_translations": {
                                "ru": "Я высыпал холодную <w>золу</w> в ведро на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) asza"
                    ],
                    "common_phrases": [
                        "wybierać haś",
                        "kubeł na haś"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Górny Śląsk"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В силезском диалекте: зола, остатки после сжигания угля в печи"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зола",
                                "target_lang_sense_clarification": "остаток от сжигания"
                            },
                            {
                                "target_lang_word": "пепел",
                                "target_lang_sense_clarification": "продукт горения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e99bbb0-226d-3cb6-b98c-5c5175f55855",
                    "sense_definition": "w gwarze śląskiej: miał węglowy, bardzo drobny węgiel",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiliśmy na zimę tonę taniego <w>hasiu</w> do palenia.",
                            "target_lang_translations": {
                                "ru": "Мы купили на зиму тонну дешевой <w>угольной пыли</w> для топки."
                            }
                        },
                        {
                            "text": "Palenie samym <w>hasiem</w> jest trudne i wymaga dobrego pieca.",
                            "target_lang_translations": {
                                "ru": "Топить одной <w>угольной пылью</w> трудно, и для этого нужна хорошая печь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "palić hasiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Górny Śląsk"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В силезском диалекте: угольная пыль, очень мелкий уголь"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угольная пыль",
                                "target_lang_sense_clarification": "мелкая фракция угля"
                            },
                            {
                                "target_lang_word": "штыб",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "129e6212-9131-3b8f-815e-a48f5f6916f3",
                    "sense_definition": "В высшей степени, чрезвычайно, очень сильно (употребляется для усиления значения слов, выражающих негативные или интенсивные состояния, такие как усталость, скука, страх).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгой прогулки по горам я <w>смертельно</w> устал и сразу уснул.",
                            "target_lang_translations": {
                                "en": "After a long walk in the mountains, I was <w>dead</w> tired and fell asleep immediately."
                            }
                        },
                        {
                            "text": "Нам было <w>смертельно</w> скучно слушать эту длинную и монотонную лекцию.",
                            "target_lang_translations": {
                                "en": "We were <w>deadly</w> bored listening to this long and monotonous lecture."
                            }
                        }
                    ],
                    "synonyms": [
                        "ужасно",
                        "крайне",
                        "безумно",
                        "невыносимо"
                    ],
                    "common_phrases": [
                        "смертельно устал",
                        "смертельно скучно",
                        "смертельно хочется",
                        "смертельно бледный",
                        "смертельно надоело"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to exaggerate feelings or states"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Extremely, to the highest degree (used to intensify words expressing negative or intense states, such as fatigue, boredom, or fear)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deadly",
                                "target_lang_sense_clarification": "used as an intensifier, e.g., deadly boring"
                            },
                            {
                                "target_lang_word": "terribly",
                                "target_lang_sense_clarification": "extremely, very"
                            },
                            {
                                "target_lang_word": "deathly",
                                "target_lang_sense_clarification": "resembling death, e.g., deathly pale"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27471d92-3cd2-39f6-9305-b3028ed36656",
                    "sense_definition": "Так, что это приводит к смерти или грозит гибелью; фатально.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "death_consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдат был <w>смертельно</w> ранен в бою, но врачи боролись за его жизнь.",
                            "target_lang_translations": {
                                "en": "The soldier was <w>mortally</w> wounded in battle, but the doctors fought for his life."
                            }
                        },
                        {
                            "text": "Это вещество <w>смертельно</w> опасно для человека даже в малых дозах.",
                            "target_lang_translations": {
                                "en": "This substance is <w>deadly</w> dangerous to humans even in small doses."
                            }
                        }
                    ],
                    "synonyms": [
                        "фатально",
                        "гибельно"
                    ],
                    "antonyms": [
                        "жизненно"
                    ],
                    "common_phrases": [
                        "смертельно ранен",
                        "смертельно болен",
                        "смертельно опасный",
                        "смертельно ядовитый"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that causes death or threatens life; fatally."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mortally",
                                "target_lang_sense_clarification": "leading to death"
                            },
                            {
                                "target_lang_word": "fatally",
                                "target_lang_sense_clarification": "causing death"
                            },
                            {
                                "target_lang_word": "deadly",
                                "target_lang_sense_clarification": "dangerous to life"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a2b54798-ed64-340f-8a7f-32e16e0f3a5f",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>смертельный</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "death_consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ранение в голову оказалось <w>смертельно</w>, и помощь не успела прийти.",
                            "target_lang_translations": {
                                "en": "The head wound proved <w>fatal</w>, and help did not arrive in time."
                            }
                        },
                        {
                            "text": "Для маленького насекомого падение с такой высоты не <w>смертельно</w>.",
                            "target_lang_translations": {
                                "en": "For a small insect, a fall from such a height is not <w>fatal</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short neuter singular adjective"
                        }
                    ],
                    "target_lang_definitions": {

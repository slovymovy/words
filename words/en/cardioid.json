                        }
                    ],
                    "common_phrases": [
                        "cardioid curve"
                    ],
                    "target_lang_definitions": {
                        "ru": "Геометрическая кривая в форме сердца, образуемая точкой окружности, катящейся вокруг другой окружности того же размера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кардиоида",
                                "target_lang_sense_clarification": "математическая кривая"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "frustreren",
        "frustratie",
        "frustrerend"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5b2a5b4d-bdf7-3b7f-beee-37ceb0a02252",
                    "sense_definition": "Gevoelens van onmacht en ergernis hebbend omdat wensen of doelen worden belemmerd.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>gefrustreerde</w> leerling was ontevreden omdat hij een hoger cijfer wilde.",
                            "target_lang_translations": {
                                "en": "The <w>frustrated</w> student was dissatisfied because he wanted a higher grade."
                            }
                        },
                        {
                            "text": "Ik raakte <w>gefrustreerd</w> toen ik de moeilijke puzzel niet kon oplossen.",
                            "target_lang_translations": {
                                "en": "I got <w>frustrated</w> when I couldn't solve the difficult puzzle."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gefrustreerd raken",
                        "zich gefrustreerd voelen"
                    ],
                    "target_lang_definitions": {
                        "en": "Having feelings of powerlessness and annoyance because wishes or goals are obstructed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "frustrated",
                                "target_lang_sense_clarification": "feeling annoyed or impatient because you cannot do or achieve what you want"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "abd0a5b9-af8a-33df-934b-91f75304f2b9",
                    "sense_definition": "Voltooid deelwoord van <w>frustreren</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Onze plannen voor het feest werden door het slechte weer <w>gefrustreerd</w>.",
                            "target_lang_translations": {
                                "en": "Our plans for the party were <w>thwarted</w> by the bad weather."
                            }
                        },
                        {
                            "text": "De poging om het record te verbreken is helaas <w>gefrustreerd</w>.",
                            "target_lang_translations": {
                                "en": "The attempt to break the record was unfortunately <w>thwarted</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past participle of <w>frustreren</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thwarted",
                                "target_lang_sense_clarification": "prevented from happening or succeeding (of plans or efforts)"
                            },
                            {
                                "target_lang_word": "frustrated",
                                "target_lang_sense_clarification": "prevented from progressing (formal context)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "74e12645-2edc-3411-b83f-481c54c57be2",
                    "sense_definition": "Op een manier die blijk geeft van frustratie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Gefrustreerd</w> gooide hij de telefoon op de haak toen de verbinding verbrak.",
                            "target_lang_translations": {
                                "en": "<w>Frustrated</w>, he slammed the phone on the hook when the connection broke."
                            }
                        },
                        {
                            "text": "Ze zuchtte <w>gefrustreerd</w> toen ze zag hoe lang de rij voor de kassa was.",
                            "target_lang_translations": {
                                "en": "She sighed <w>in frustration</w> when she saw how long the line for the checkout was."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner that shows frustration."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "frustratedly",
                                "target_lang_sense_clarification": "in a manner showing annoyance"
                            },
                            {
                                "target_lang_word": "in frustration",
                                "target_lang_sense_clarification": "adverbial phrase indicating a state of frustration"
                            }
                        ]
                    }
                }
            ]
        }
    ]

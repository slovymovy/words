    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "44470c3c-972c-362f-8682-1d9ace9ff519",
                    "sense_definition": "A coin worth 5 cents.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I found a <w>nickel</w> on the sidewalk yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера я нашел <w>монету в пять центов</w> на тротуаре."
                            }
                        },
                        {
                            "text": "You need another <w>nickel</w> to buy that candy bar.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно еще <w>пять центов</w>, чтобы купить этот батончик."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wooden nickel",
                        "plug nickel"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Монета достоинством в 5 центов (в США и Канаде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монета в пять центов",
                                "target_lang_sense_clarification": "номинал"
                            },
                            {
                                "target_lang_word": "пять центов",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3cbfecd-50b8-3ee9-80ae-122507fdecdc",
                    "sense_definition": "A silvery elemental metal with an atomic number of 28 and symbol Ni.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stainless steel is often an alloy of iron, chromium, and <w>nickel</w>.",
                            "target_lang_translations": {
                                "ru": "Нержавеющая сталь часто представляет собой сплав железа, хрома и <w>никеля</w>."
                            }
                        },
                        {
                            "text": "Some people are allergic to jewelry that contains <w>nickel</w>.",
                            "target_lang_translations": {
                                "ru": "У некоторых людей аллергия на украшения, содержащие <w>никель</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nickel alloy",
                        "nickel plating",
                        "nickel steel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Серебристый металл, химический элемент с атомным номером 28 и символом Ni."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ни́кель",
                                "target_lang_sense_clarification": "металл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c60283ae-8b15-3fc3-82af-42e8eb9f3f84",
                    "sense_definition": "A defensive formation in American football with five defensive backs.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The coach called for a <w>nickel</w> package to counter the passing game.",
                            "target_lang_translations": {
                                "ru": "Тренер назначил схему <w>никель</w>, чтобы противостоять пасовой игре."
                            }
                        },
                        {
                            "text": "They switched to a <w>nickel</w> defense on third down.",
                            "target_lang_translations": {
                                "ru": "На третьем дауне они переключились на защиту <w>никель</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nickel defense",
                        "nickel back"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "American Football"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оборонительная схема в американском футболе с пятью защитниками (бэками)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никель",
                                "target_lang_sense_clarification": "схема защиты"
                            },
                            {
                                "target_lang_word": "защита никель",
                                "target_lang_sense_clarification": "термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d8a6537-1385-33c1-b7bb-704ed36c13f7",
                    "sense_definition": "A five-year prison sentence.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He served a <w>nickel</w> in the state penitentiary for robbery.",
                            "target_lang_translations": {
                                "ru": "Он отсидел <w>пятёрку</w> в тюрьме штата за ограбление."
                            }
                        },
                        {
                            "text": "The judge sentenced him to a <w>nickel</w> behind bars.",
                            "target_lang_translations": {
                                "ru": "Судья приговорил его к <w>пятёрке</w> за решеткой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "criminal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пятилетний тюремный срок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятёрка",
                                "target_lang_sense_clarification": "жаргон"
                            },
                            {
                                "target_lang_word": "пять лет",
                                "target_lang_sense_clarification": "срок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "343e97df-2501-3c00-b796-eebbcbe05559",
                    "sense_definition": "To coat or plate a surface with nickel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory <w>nickels</w> the car bumpers to prevent them from rusting.",
                            "target_lang_translations": {
                                "ru": "Завод <w>никелирует</w> автомобильные бамперы, чтобы предотвратить их ржавление."
                            }
                        },
                        {
                            "text": "He had the antique door handles <w>nickeled</w> to restore their shine.",
                            "target_lang_translations": {
                                "ru": "Он отдал старинные дверные ручки, чтобы их <w>никелировали</w> и вернули им блеск."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать поверхность изделия слоем никеля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никелирова́ть",
                                "target_lang_sense_clarification": "покрывать металлом"
                            }
                        ]
                    }
                }
            ]
        },
        {

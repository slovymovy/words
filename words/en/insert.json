    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c6fca447-9dc5-31c5-b150-2f9aeef8b36a",
                    "sense_definition": "To place, fit, or push something into something else.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>insert</w> your card into the machine to complete the payment.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>вставьте</w> вашу карту в терминал, чтобы завершить оплату."
                            }
                        },
                        {
                            "text": "You can <w>insert</w> a table into the document to organize the data.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>вставить</w> таблицу в документ, чтобы упорядочить данные."
                            }
                        }
                    ],
                    "synonyms": [
                        "put in",
                        "put inside",
                        "introduce"
                    ],
                    "antonyms": [
                        "delete",
                        "delete"
                    ],
                    "common_phrases": [
                        "insert a coin",
                        "insert a card",
                        "insert text",
                        "insert an image"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать, вставлять или проталкивать что-либо внутрь чего-либо другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вставлять",
                                "target_lang_sense_clarification": "помещать внутрь, вделывать"
                            },
                            {
                                "target_lang_word": "вкладывать",
                                "target_lang_sense_clarification": "класть внутрь чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1bc5e563-fa55-3af6-9dae-dd7282b3ad27",
                    "sense_definition": "A loose page or leaflet placed inside a magazine, newspaper, or package, often for advertising or instruction.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Sunday newspaper contained a glossy <w>insert</w> advertising a new gym.",
                            "target_lang_translations": {
                                "ru": "В воскресной газете был глянцевый <w>вкладыш</w> с рекламой нового спортзала."
                            }
                        },
                        {
                            "text": "Please read the package <w>insert</w> carefully before taking the medication.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, внимательно прочитайте <w>вкладыш</w> в упаковке перед приемом лекарства."

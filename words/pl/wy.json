                {
                    "sense_id": "4a931570-4075-3501-8c08-555345018120",
                    "sense_definition": "Zaimek osobowy drugiej osoby liczby mnogiej, wskazujący na grupę osób, do której zwraca się nadawca.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy to <wy> zrobiliście to zadanie domowe?",
                            "target_lang_translations": {
                                "ru": "Это <вы> сделали это домашнее задание?"
                            }
                        },
                        {
                            "text": "Gdzie <wy> się wybieracie na tegoroczne wakacje?",
                            "target_lang_translations": {
                                "ru": "Куда <вы> собираетесь в отпуск в этом году?"
                            }
                        }
                    ],
                    "antonyms": [
                        "my",
                        "oni",
                        "one"
                    ],
                    "common_phrases": [
                        "wy wszyscy",
                        "a wy",
                        "czy wy",
                        "to wy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Личное местоимение второго лица множественного числа, указывающее на группу лиц, к которой обращается говорящий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "множественное число"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95d133dd-7b62-3c17-b83d-214d31b7610b",
                    "sense_definition": "Forma grzecznościowa używana w stosunku do pojedynczej osoby, charakterystyczna dla gwary wiejskiej lub stylizacji ludowej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "singular_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarzu, jaką <wy> macie piękną i zadbaną zagrodę!",
                            "target_lang_translations": {
                                "ru": "Хозяин, какое у <вас> красивое и ухоженное подворье!"
                            }
                        },
                        {
                            "text": "Dziadku, usiądźcie <wy> sobie tutaj i odpocznijcie.",
                            "target_lang_translations": {
                                "ru": "Дедушка, сядьте-<ка> <вы> здесь и отдохните."
                            }
                        }
                    ],
                    "synonyms": [
                        "pan",
                        "pani"
                    ],
                    "common_phrases": [
                        "co wy",
                        "jak wy"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "często wiejskie lub stylizowane"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уважительная форма обращения к одному лицу, характерная для деревенской речи или народной стилизации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "обращение к одному лицу (народное/деревенское)"

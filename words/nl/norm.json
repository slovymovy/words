            "senses": [
                {
                    "sense_id": "c87911e6-23ee-3d58-ac11-71c19b75de9c",
                    "sense_definition": "Een regel voor gedrag die door een groep mensen wordt geaccepteerd en nageleefd",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rules_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In onze samenleving zijn bepaalde <w>normen</w> en waarden erg belangrijk.",
                            "target_lang_translations": {
                                "en": "In our society, certain <w>norms</w> and values are very important.",
                                "ru": "В нашем обществе очень важны определенные <w>нормы</w> и ценности."
                            }
                        },
                        {
                            "text": "Het is de <w>norm</w> om op tijd te komen voor een afspraak.",
                            "target_lang_translations": {
                                "en": "It is the <w>norm</w> to arrive on time for an appointment.",
                                "ru": "Приходить на встречу вовремя — это <w>норма</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gedragsregel"
                    ],
                    "common_phrases": [
                        "normen en waarden",
                        "de sociale norm",
                        "afwijken van de norm"
                    ],
                    "target_lang_definitions": {
                        "en": "A rule of behavior accepted and followed by a group of people",
                        "ru": "Правило поведения, принятое и соблюдаемое группой людей"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "norm",
                                "target_lang_sense_clarification": "social rule or expectation"
                            },
                            {
                                "target_lang_word": "social norm",
                                "target_lang_sense_clarification": "accepted behavior in a society"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "норма",
                                "target_lang_sense_clarification": "правило поведения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dec31d3c-81a3-34e5-b08a-89bd03bdecde",
                    "sense_definition": "Een vastgestelde maatstaf, standaard of eis waaraan iets of iemand moet voldoen",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "standard_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit nieuwe product voldoet helaas niet aan de Europese <w>norm</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, this new product does not meet the European <w>standard</w>.",
                                "ru": "К сожалению, этот новый продукт не соответствует европейской <w>норме</w>."
                            }
                        },
                        {
                            "text": "De atleet moet de <w>norm</w> halen om zich te kwalificeren voor de wedstrijd.",
                            "target_lang_translations": {
                                "en": "The athlete must meet the <w>standard</w> to qualify for the competition.",
                                "ru": "Атлет должен выполнить <w>норматив</w>, чтобы квалифицироваться на соревнования."
                            }
                        }
                    ],
                    "synonyms": [
                        "standaard",
                        "maatstaf",
                        "richtlijn"
                    ],
                    "common_phrases": [
                        "aan de norm voldoen",
                        "boven de norm",
                        "onder de norm"
                    ],
                    "target_lang_definitions": {
                        "en": "A set measure, standard, or requirement that something or someone must meet",
                        "ru": "Установленная мера, стандарт или требование, которому что-то или кто-то должен соответствовать"
                    },

                },
                {
                    "sense_id": "a1aea9cc-c924-37f3-8871-84ac30f0b5ba",
                    "sense_definition": "Een elektrische stroom of elektromagnetische golf die informatie, geluid of beelden overdraagt via apparatuur.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn telefoon heeft hier in het bos geen <w>signaal</w>.",
                            "target_lang_translations": {
                                "en": "My phone has no <w>signal</w> here in the forest.",
                                "ru": "У моего телефона здесь, в лесу, нет <w>сигнала</w>."
                            }
                        },
                        {
                            "text": "Het <w>signaal</w> van de satelliet is zwak door de zware regenval.",
                            "target_lang_translations": {
                                "en": "The satellite <w>signal</w> is weak due to the heavy rainfall.",
                                "ru": "<w>Сигнал</w> со спутника слабый из-за сильного дождя."
                            }
                        }

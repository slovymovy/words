            "senses": [
                {
                    "sense_id": "c7fc8c17-1a7c-3e00-80eb-4ef7c9914bee",
                    "sense_definition": "Słowo łączące, wprowadzające wzmocnienie lub dopowiedzenie do poprzedniej treści; znaczy „co więcej”, „a nawet”.",

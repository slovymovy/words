    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0e98ff5b-2f2b-3418-a84e-d57b5f614035",
                    "sense_definition": "The act of intentionally killing oneself.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "killing_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police concluded that the death was a <w>suicide</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция пришла к выводу, что смерть наступила в результате <w>самоубийства</w>."
                            }
                        },
                        {
                            "text": "He was suffering from depression and attempted to commit <w>suicide</w>.",
                            "target_lang_translations": {
                                "ru": "Он страдал от депрессии и пытался покончить жизнь <w>самоубийством</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "self-killing",
                        "self-murder"
                    ],
                    "common_phrases": [
                        "commit suicide",
                        "attempt suicide",
                        "suicide note",
                        "suicide rate",
                        "assisted suicide"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт намеренного лишения себя жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самоубийство",
                                "target_lang_sense_clarification": "основной термин"
                            },
                            {
                                "target_lang_word": "суицид",
                                "target_lang_sense_clarification": "медицинский или формальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17c391e5-419a-33b8-b156-0e222975b60d",
                    "sense_definition": "Involving or carried out by someone who intends to die during the act, often to cause harm to others.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "killing_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Security forces stopped a <w>suicide</w> bomber before he could reach the crowded market.",
                            "target_lang_translations": {
                                "ru": "Силы безопасности остановили террориста-<w>смертника</w>, прежде чем он смог добраться до переполненного рынка."
                            }
                        },
                        {
                            "text": "The soldiers knew that going back for the wounded was a <w>suicide</w> mission.",
                            "target_lang_translations": {
                                "ru": "Солдаты знали, что возвращение за ранеными было <w>самоубийственной</w> миссией."
                            }
                        }
                    ],
                    "common_phrases": [
                        "suicide bomber",
                        "suicide mission",
                        "suicide attack",
                        "suicide vest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с намерением умереть при совершении действия, часто для причинения вреда другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смертник",
                                "target_lang_sense_clarification": "в значении 'террорист-смертник'"
                            },
                            {
                                "target_lang_word": "самоубийственный",
                                "target_lang_sense_clarification": "о миссии или атаке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d408cae-f8dd-392f-9f51-141fefb416a0",
                    "sense_definition": "An action that ruins or destroys one's career, reputation, or position.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_ruin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Resigning from the party right before the election would be political <w>suicide</w>.",
                            "target_lang_translations": {
                                "ru": "Выход из партии прямо перед выборами стал бы политическим <w>самоубийством</w>."
                            }
                        },
                        {
                            "text": "It is career <w>suicide</w> to insult your boss in front of the clients.",
                            "target_lang_translations": {
                                "ru": "Оскорблять начальника перед клиентами — это карьерное <w>самоубийство</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "political suicide",
                        "career suicide",
                        "social suicide"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, которое губит или разрушает чью-либо карьеру, репутацию или положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самоубийство",
                                "target_lang_sense_clarification": "в переносном смысле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a7cb5e3-1a79-3890-8087-22184da6c1d8",
                    "sense_definition": "A conditioning exercise involving a series of sprints back and forth between lines on a court.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The coach made the basketball team run <w>suicides</w> at the end of practice.",
                            "target_lang_translations": {
                                "ru": "Тренер заставил баскетбольную команду бегать <w>челночный бег</w> в конце тренировки."
                            }
                        },
                        {
                            "text": "I was exhausted after doing ten minutes of <w>suicides</w> in the gym.",
                            "target_lang_translations": {
                                "ru": "Я был без сил после десяти минут <w>челночного бега</w> в спортзале."
                            }
                        }
                    ],
                    "common_phrases": [
                        "run suicides"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "sports context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тренировочное упражнение, состоящее из серии челночных рывков между линиями на площадке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "челночный бег",
                                "target_lang_sense_clarification": "спортивное упражнение"

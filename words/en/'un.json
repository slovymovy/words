                },
                {
                    "sense_id": "7218215e-7735-35f4-9e88-a4232c389d03",
                    "sense_definition": "A dialectal object pronoun equivalent to <w>him</w>, <w>her</w>, or <w>it</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I saw <w>'un</w> walking down the street yesterday afternoon.",
                            "target_lang_translations": {

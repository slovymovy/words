    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7df03e5e-e68d-3582-975e-0f17dd0a5154",
                    "sense_definition": "Товар или услуга, способные заменить другой продукт при потреблении, так как удовлетворяют схожие потребности.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "economics_substitute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маргарин часто рассматривается как дешёвый <w>субститут</w> сливочного масла.",
                            "target_lang_translations": {
                                "en": "Margarine is often considered a cheap <w>substitute</w> for butter."
                            }
                        },
                        {
                            "text": "Если цена на кофе сильно вырастет, многие покупатели выберут чай как доступный <w>субститут</w>.",
                            "target_lang_translations": {
                                "en": "If the price of coffee rises significantly, many buyers will choose tea as an affordable <w>substitute</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "заменитель",
                        "аналог",
                        "товар-заменитель"
                    ],
                    "antonyms": [
                        "комплемент"
                    ],
                    "common_phrases": [
                        "товар-субститут",
                        "близкий субститут",
                        "наличие субститутов"
                    ],
                    "target_lang_definitions": {
                        "en": "A product or service capable of replacing another product during consumption because it satisfies similar needs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "substitute",
                                "target_lang_sense_clarification": "economics"
                            },
                            {
                                "target_lang_word": "substitute good",
                                "target_lang_sense_clarification": "economics term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf108b97-905e-3085-9da0-1697abb1d1f6",
                    "sense_definition": "Языковая единица (слово или словосочетание), которая используется вместо другой единицы в тексте или речи для избежания повторов.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "linguistics_substitute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Местоимение «он» в этом предложении выступает как <w>субститут</w> имени собственного.",
                            "target_lang_translations": {
                                "en": "The pronoun \"he\" in this sentence acts as a <w>substitute</w> for the proper noun."
                            }
                        },
                        {
                            "text": "Использование <w>субститутов</w> помогает сделать текст более связным и избежать тавтологии.",
                            "target_lang_translations": {
                                "en": "The use of <w>substitutes</w> helps make the text more cohesive and avoid tautology."
                            }
                        }
                    ],
                    "synonyms": [
                        "замена",
                        "заместитель"
                    ],
                    "common_phrases": [
                        "слово-субститут",
                        "использовать как субститут"
                    ],
                    "target_lang_definitions": {
                        "en": "A linguistic unit (word or phrase) used in place of another unit in text or speech to avoid repetition."
                    },

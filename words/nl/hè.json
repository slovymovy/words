{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "4cb3e28c-5ad6-3a01-8d0c-58a381581802",
                    "sense_definition": "Woord aan het einde van een zin om bevestiging of instemming te vragen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "question_tag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is vandaag echt prachtig weer, <w>hè</w>?",
                            "target_lang_translations": {
                                "en": "The weather is really beautiful today, <w>isn't it</w>?",
                                "ru": "Сегодня действительно прекрасная погода, <w>правда</w>?"
                            }
                        },
                        {
                            "text": "Je komt vanavond toch ook naar het feest, <w>hè</w>?",
                            "target_lang_translations": {
                                "en": "You're coming to the party tonight too, <w>right</w>?",
                                "ru": "Ты ведь тоже придешь сегодня вечером на вечеринку, <w>да</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "toch",
                        "nietwaar"
                    ],
                    "common_phrases": [
                        "mooi hè",
                        "goed hè",
                        "vind je niet"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Word used at the end of a sentence to ask for confirmation or agreement.",
                        "ru": "Слово в конце предложения для запроса подтверждения или согласия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "right",
                                "target_lang_sense_clarification": "asking for confirmation"
                            },
                            {
                                "target_lang_word": "isn't it",
                                "target_lang_sense_clarification": "tag question (varies by context)"
                            },
                            {
                                "target_lang_word": "eh",
                                "target_lang_sense_clarification": "informal tag"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "вопрос для подтверждения"
                            },
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "запрос согласия"
                            },
                            {
                                "target_lang_word": "не так ли",
                                "target_lang_sense_clarification": "формальный запрос подтверждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8db2b528-495e-3eb1-8363-8c43df3f75f1",
                    "sense_definition": "Uitroep van verbazing of wanneer men iets niet goed verstaan heeft.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "surprise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hè</w>? Ik dacht dat de winkel vandaag open was.",
                            "target_lang_translations": {
                                "en": "<w>Huh</w>? I thought the shop was open today.",
                                "ru": "<w>А</w>? Я думал, что магазин сегодня открыт."
                            }
                        },
                        {
                            "text": "<w>Hè</w>? Wat zeg je me nou allemaal?",
                            "target_lang_translations": {
                                "en": "<w>What</w>? What are you telling me?",
                                "ru": "<w>Что</w>? Что ты мне такое говоришь?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wat",
                        "huh"
                    ],
                    "common_phrases": [
                        "hè wat"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation of surprise or when one has not heard something clearly.",
                        "ru": "Возглас удивления или переспроса, когда что-то не расслышали или не поняли."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "huh",
                                "target_lang_sense_clarification": "expressing confusion or surprise"
                            },
                            {
                                "target_lang_word": "what",
                                "target_lang_sense_clarification": "asking for repetition"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "переспрос"
                            },
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "удивление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdae257c-e01c-3019-a6f6-e873ab037fd0",
                    "sense_definition": "Uitroep van teleurstelling, ergernis of afkeuring.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "annoyance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hè</w>, wat vervelend dat het nu gaat regenen.",
                            "target_lang_translations": {
                                "en": "<w>Ugh</w>, how annoying that it's starting to rain now.",
                                "ru": "<w>Эх</w>, как досадно, что сейчас пойдет дождь."
                            }
                        },
                        {
                            "text": "<w>Hè</w>, doe niet zo kinderachtig tegen je broertje!",
                            "target_lang_translations": {
                                "en": "<w>Hey</w>, don't be so childish towards your little brother!",
                                "ru": "<w>Ну</w>, не веди себя так по-детски со своим братиком!"
                            }
                        }
                    ],
                    "synonyms": [
                        "bah",
                        "verdorie",
                        "jakkes"
                    ],
                    "common_phrases": [
                        "hè bah",
                        "hè getver",
                        "hè gatsie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation of disappointment, annoyance, or disapproval.",
                        "ru": "Возглас разочарования, досады или неодобрения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ugh",
                                "target_lang_sense_clarification": "expressing annoyance"
                            },
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "expressing disappointment"
                            },
                            {
                                "target_lang_word": "hey",
                                "target_lang_sense_clarification": "expressing disapproval or admonition"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "недовольство"
                            },
                            {
                                "target_lang_word": "эх",
                                "target_lang_sense_clarification": "разочарование"
                            },
                            {
                                "target_lang_word": "фу",
                                "target_lang_sense_clarification": "отвращение или неодобрение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3881c3f-3d35-3e4d-830e-51eadeff3487",
                    "sense_definition": "Uitroep om een gevoel van opluchting uit te drukken.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hè</w>, dat zware werk is eindelijk gedaan.",
                            "target_lang_translations": {
                                "en": "<w>Phew</w>, that heavy work is finally done.",
                                "ru": "<w>Уф</w>, эта тяжелая работа наконец-то сделана."
                            }
                        },
                        {
                            "text": "<w>Hè</w>, wat fijn om weer veilig thuis te zijn.",
                            "target_lang_translations": {
                                "en": "<w>Ah</w>, how nice to be safely home again.",
                                "ru": "<w>Фух</w>, как хорошо снова быть дома в безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "oef"
                    ],
                    "common_phrases": [
                        "hè hè"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation to express a feeling of relief.",
                        "ru": "Возглас для выражения чувства облегчения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "phew",
                                "target_lang_sense_clarification": "sigh of relief"
                            },
                            {
                                "target_lang_word": "ah",
                                "target_lang_sense_clarification": "sigh of satisfaction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уф",
                                "target_lang_sense_clarification": "выдох облегчения"
                            },
                            {
                                "target_lang_word": "фух",
                                "target_lang_sense_clarification": "сильное облегчение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "wybuch",
        "wybuchowy",
        "wybuchowość",
        "wybuchowo"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dc9ec198-f6c2-3d40-a4a4-0e28765590aa",
                    "sense_definition": "Zaczynać się nagle i z dużą siłą (dotyczy zjawisk takich jak wojna, pożar, epidemia czy panika).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "event_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym suchym lesie pożar <w>wybucha</w> niemal każdego lata.",
                            "target_lang_translations": {
                                "ru": "В этом сухом лесу пожар <w>вспыхивает</w> почти каждое лето."
                            }
                        },
                        {
                            "text": "Kiedy w kraju <w>wybucha</w> wojna, cierpią niewinni ludzie.",
                            "target_lang_translations": {
                                "ru": "Когда в стране <w>вспыхивает</w> война, страдают невинные люди."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wojna wybucha",
                        "pożar wybucha",
                        "epidemia wybucha",
                        "panika wybucha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно и с большой силой начинаться (о таких явлениях, как война, пожар, эпидемия или паника)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вспыхивать",
                                "target_lang_sense_clarification": "о войне, пожаре, эпидемии"
                            },
                            {
                                "target_lang_word": "разражаться",
                                "target_lang_sense_clarification": "о грозе, кризисе, войне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7786edd2-f6e0-3b76-9cbb-f976297fa580",
                    "sense_definition": "Ulegać gwałtownemu rozerwaniu pod wpływem ciśnienia lub reakcji chemicznej, czemu zwykle towarzyszy huk i wyrzut materii.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_explosion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten wulkan <w>wybucha</w> rzadko, ale bardzo gwałtownie.",
                            "target_lang_translations": {
                                "ru": "Этот вулкан <w>извергается</w> редко, но очень бурно."
                            }
                        },
                        {
                            "text": "Stara butla z gazem może <w>wybuchać</w> w najmniej oczekiwanym momencie.",
                            "target_lang_translations": {
                                "ru": "Старый газовый баллон может <w>взрываться</w> в самый неожиданный момент."
                            }
                        }
                    ],
                    "synonyms": [
                        "eksplodować"
                    ],
                    "common_phrases": [
                        "wulkan wybucha",
                        "bomba wybucha",
                        "pocisk wybucha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрываться, разлетаясь на части под давлением; извергаться (о вулкане)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрываться",
                                "target_lang_sense_clarification": "о бомбе, газе, механизме"
                            },
                            {
                                "target_lang_word": "извергаться",
                                "target_lang_sense_clarification": "о вулкане"
                            },
                            {
                                "target_lang_word": "разрываться",
                                "target_lang_sense_clarification": "о снаряде, бомбе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08440f92-7869-3310-b201-7435fe4377cc",
                    "sense_definition": "Gwałtownie dawać wyraz silnym emocjom, takim jak śmiech, płacz lub złość.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotional_outburst",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek często <w>wybucha</w> głośnym śmiechem, gdy ogląda komedie.",
                            "target_lang_translations": {
                                "ru": "Марек часто <w>взрывается</w> громким смехом, когда смотрит комедии."
                            }
                        },
                        {
                            "text": "Mój szef jest spokojny i rzadko <w>wybucha</w> gniewem przy pracownikach.",
                            "target_lang_translations": {
                                "ru": "Мой начальник спокоен и редко <w>взрывается</w> гневом при сотрудниках."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wybuchać śmiechem",
                        "wybuchać płaczem",
                        "wybuchać gniewem",
                        "wybuchać złością"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бурно и внезапно выражать сильные эмоции (смех, плач, гнев)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрываться",
                                "target_lang_sense_clarification": "смехом, гневом"
                            },
                            {
                                "target_lang_word": "разражаться",
                                "target_lang_sense_clarification": "смехом, бранью, слезами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
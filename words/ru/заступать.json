    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cb2d9b02-43f3-3c46-8ef5-7d8dbb95298a",
                    "sense_definition": "Заходить за какую-либо черту, линию или край; переступать пределы чего-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пассажирам метро строго запрещено <заступать> за ограничительную линию.",
                            "target_lang_translations": {
                                "en": "Metro passengers are strictly forbidden to <w>step over</w> the safety line."
                            }
                        },
                        {
                            "text": "Спортсмен случайно <заступил> за край площадки и потерял мяч.",
                            "target_lang_translations": {
                                "en": "The athlete accidentally <w>stepped over</w> the edge of the court and lost the ball."
                            }
                        }
                    ],
                    "common_phrases": [
                        "заступать за черту",
                        "заступать за линию",
                        "заступать за край"
                    ],
                    "target_lang_definitions": {
                        "en": "To step beyond a certain line, boundary, or edge; to cross limits."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "step over",
                                "target_lang_sense_clarification": "movement across a physical line"
                            },
                            {
                                "target_lang_word": "cross",
                                "target_lang_sense_clarification": "going beyond a boundary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "045e2708-d4d5-3c91-98db-dfbf2d8ffd51",
                    "sense_definition": "Приступать к выполнению служебных обязанностей (дежурству, смене, караулу), обычно сменяя кого-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новая смена врачей <заступает> на дежурство в восемь утра.",
                            "target_lang_translations": {
                                "en": "The new shift of doctors <w>goes on</w> duty at eight in the morning."
                            }
                        },
                        {
                            "text": "Охранник проверил документы, прежде чем <заступать> на пост.",
                            "target_lang_translations": {
                                "en": "The guard checked the documents before <w>taking up</w> his post."
                            }
                        }
                    ],
                    "common_phrases": [
                        "заступать на дежурство",
                        "заступать на смену",
                        "заступать на пост",
                        "заступать в наряд"
                    ],
                    "target_lang_definitions": {
                        "en": "To start performing official duties (duty, shift, guard), usually relieving someone else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go on duty",
                                "target_lang_sense_clarification": "starting a work shift"
                            },
                            {
                                "target_lang_word": "take up a post",
                                "target_lang_sense_clarification": "assuming a guard position"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53224d28-f6bf-3c69-b476-9d1b935ac774",
                    "sense_definition": "Занимать чьё-либо место или должность, выполняя функции другого человека.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "substitution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После отъезда директора его заместитель временно <заступал> это место.",
                            "target_lang_translations": {
                                "en": "After the director's departure, his deputy temporarily <w>took</w> his place."
                            }
                        },
                        {
                            "text": "Старший сын в семье рано начал работать и <заступал> место отца.",
                            "target_lang_translations": {
                                "en": "The eldest son in the family began working early and <w>took</w> his father's place."
                            }
                        }
                    ],
                    "synonyms": [
                        "замещать"
                    ],
                    "antonyms": [
                        "оставлять"
                    ],
                    "common_phrases": [
                        "заступать место"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'замещать' or 'заменять'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To occupy someone's place or position, performing the functions of another person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take the place of",
                                "target_lang_sense_clarification": "substituting someone"
                            },
                            {
                                "target_lang_word": "replace",
                                "target_lang_sense_clarification": "filling a role"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
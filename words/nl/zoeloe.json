{
    "word_family": [
        "KwaZoeloe-Natal"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4ec671a6-a272-3422-a22e-06c9a787f461",
                    "sense_definition": "Iemand die behoort tot het Afrikaanse volk dat voornamelijk in de provincie KwaZoeloe-Natal in Zuid-Afrika woont.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "zoeloe_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sjaka was de stichter van het grote rijk der <w>Zoeloes</w>.",
                            "target_lang_translations": {
                                "en": "Shaka was the founder of the great empire of the <w>Zulus</w>.",
                                "ru": "Чака был основателем великой империи <w>зулусов</w>."
                            }
                        },
                        {
                            "text": "Een <w>Zoeloe</w> zal zelden op een partij van een andere bevolkingsgroep stemmen.",
                            "target_lang_translations": {
                                "en": "A <w>Zulu</w> will rarely vote for a party of another population group.",
                                "ru": "<w>Зулус</w> редко проголосует за партию другой этнической группы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "koning der Zoeloes",
                        "volk der Zoeloes"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Zuid-Afrika"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone belonging to the African people living mainly in the KwaZulu-Natal province in South Africa.",
                        "ru": "Человек, принадлежащий к африканскому народу, проживающему преимущественно в провинции Квазулу-Натал в Южной Африке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Zulu",
                                "target_lang_sense_clarification": "member of people"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "зулус",
                                "target_lang_sense_clarification": "представитель народа"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c65a5887-afc6-3ace-ba67-450b2b745010",
                    "sense_definition": "Bantoetaal die door miljoenen mensen in Zuid-Afrika wordt gesproken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "zoeloe_group",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "De begeleidende teksten zijn geschreven in het <w>Zoeloe</w> en het Engels.",
                            "target_lang_translations": {
                                "en": "The accompanying texts are written in <w>Zulu</w> and English.",
                                "ru": "Сопроводительные тексты написаны на <w>зулу</w> и английском."
                            }
                        },
                        {
                            "text": "Het <w>Zoeloe</w> is een van de officiële talen van Zuid-Afrika.",
                            "target_lang_translations": {
                                "en": "<w>Zulu</w> is one of the official languages of South Africa.",
                                "ru": "<w>Зулу</w> — один из официальных языков Южной Африки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in het Zoeloe",
                        "Zoeloe spreken"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Zuid-Afrika"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Bantu language spoken by millions of people in South Africa.",
                        "ru": "Язык банту, на котором говорят миллионы людей в Южной Африке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Zulu",
                                "target_lang_sense_clarification": "language"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "зулу",
                                "target_lang_sense_clarification": "язык"
                            },
                            {
                                "target_lang_word": "зулусский язык",
                                "target_lang_sense_clarification": "язык"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "246b4cf8-918b-3442-8585-7f0c333cc750",
                    "sense_definition": "Betrekking hebbend op de Zoeloes, hun cultuur of hun taal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "zoeloe_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>Zoeloe</w> tradities worden van generatie op generatie doorgegeven.",
                            "target_lang_translations": {
                                "en": "The <w>Zulu</w> traditions are passed down from generation to generation.",
                                "ru": "<w>Зулусские</w> традиции передаются из поколения в поколение."
                            }
                        },
                        {
                            "text": "Hij bestudeert de <w>Zoeloe</w> geschiedenis aan de universiteit.",
                            "target_lang_translations": {
                                "en": "He studies <w>Zulu</w> history at the university.",
                                "ru": "Он изучает <w>зулусскую</w> историю в университете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Zoeloe cultuur",
                        "Zoeloe krijger"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Zuid-Afrika"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the Zulus, their culture, or their language.",
                        "ru": "Относящийся к зулусам, их культуре или языку."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Zulu",
                                "target_lang_sense_clarification": "adjective"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "зулусский",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "28a4c624-62ff-3d4b-a025-f316e74c9d82",
                    "sense_definition": "Zatoka o wąskim wylocie, oddzielona od otwartego morza mierzeją.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W te wakacje planujemy pojechać nad <w>zalew</w>, aby odpocząć od miasta.",
                            "target_lang_translations": {
                                "ru": "Этим летом мы планируем поехать на <w>залив</w>, чтобы отдохнуть от города."
                            }
                        },
                        {
                            "text": "Zalew Wiślany to duży <w>zalew</w> oddzielony od Bałtyku Mierzeją Wiślaną.",
                            "target_lang_translations": {
                                "ru": "Вислинский залив — это большой <w>залив</w>, отделенный от Балтики Вислинской косой."
                            }
                        }
                    ],
                    "synonyms": [
                        "zatoka"
                    ],
                    "common_phrases": [
                        "Zalew Wiślany",
                        "Zalew Szczeciński",
                        "nad zalewem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Залив с узким выходом, отделенный от открытого моря косой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "залив",
                                "target_lang_sense_clarification": "географический объект"
                            },
                            {
                                "target_lang_word": "лиман",
                                "target_lang_sense_clarification": "залив в устье реки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "355dbf1b-8029-36dd-89ff-a7ad65e49763",
                    "sense_definition": "Sztuczny zbiornik wodny powstały zazwyczaj w wyniku spiętrzenia wód rzecznych.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lokalny <w>zalew</w> jest popularnym miejscem wędkowania dla mieszkańców.",
                            "target_lang_translations": {
                                "ru": "Местное <w>водохранилище</w> является популярным местом рыбалки для жителей."
                            }
                        },
                        {
                            "text": "Ten sztuczny <w>zalew</w> pełni funkcję przeciwpowodziową i rekreacyjną.",
                            "target_lang_translations": {
                                "ru": "Это искусственное <w>водохранилище</w> выполняет противопаводковую и рекреационную функцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "rezerwuar",
                        "zbiornik retencyjny"
                    ],
                    "common_phrases": [
                        "sztuczny zalew",
                        "budowa zalewu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственный водоем, образованный обычно путем перекрытия реки плотиной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водохранилище",
                                "target_lang_sense_clarification": "искусственный водоем"
                            },
                            {
                                "target_lang_word": "пруд",
                                "target_lang_sense_clarification": "небольшой искусственный водоем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4edbc592-35f7-3a2b-926e-76bbc50d128f",
                    "sense_definition": "Niekontrolowany, masowy napływ lub nagromadzenie dużej liczby ludzi, rzeczy lub zjawisk.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_influx",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W internecie panuje ogromny <w>zalew</w> niezweryfikowanych informacji.",
                            "target_lang_translations": {
                                "ru": "В интернете царит огромный <w>поток</w> непроверенной информации."
                            }
                        },
                        {
                            "text": "Sklep przeżył prawdziwy <w>zalew</w> klientów podczas wyprzedaży.",
                            "target_lang_translations": {
                                "ru": "Магазин пережил настоящий <w>наплыв</w> покупателей во время распродажи."
                            }
                        }
                    ],
                    "synonyms": [
                        "natłok",
                        "inwazja",
                        "ekspansja"
                    ],
                    "common_phrases": [
                        "zalew informacji",
                        "zalew towarów",
                        "zalew pytań"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неконтролируемый массовый приток или скопление большого количества людей, вещей или явлений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наплыв",
                                "target_lang_sense_clarification": "людей или событий"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "информации или слов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e12387cb-18a6-357e-8dde-5c6af7e87bd5",
                    "sense_definition": "Obszar lądu pokryty wodą w wyniku wylania rzeki lub jeziora; także sam proces zalewania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "flood_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wiosennych roztopach na łąkach utworzył się szeroki <w>zalew</w>.",
                            "target_lang_translations": {
                                "ru": "После весенней оттепели на лугах образовался широкий <w>разлив</w>."
                            }
                        },
                        {
                            "text": "Rolnicy obawiają się, że kolejny <w>zalew</w> zniszczy ich uprawy.",
                            "target_lang_translations": {
                                "ru": "Фермеры опасаются, что очередное <w>затопление</w> уничтожит их посевы."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozlewisko",
                        "powódź",
                        "zalewanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок суши, покрытый водой в результате разлива реки или озера; также сам процесс затопления."
                    },
                    "translations": {

            "senses": [
                {
                    "sense_id": "b7c25ff4-559f-3215-825a-ab177aa6180e",
                    "sense_definition": "Niezidentyfikowany obiekt latający; tajemnicze zjawisko lub obiekt na niebie, którego natury nie da się wyjaśnić, potocznie kojarzone ze statkami kosmicznymi obcych cywilizacji",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "ufo_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu mieszkańców twierdziło, że widziało w nocy lądujące <w>ufo</w>.",
                            "target_lang_translations": {
                                "ru": "Многие жители утверждали, что видели ночью приземляющийся <w>НЛО</w>."
                            }
                        },
                        {
                            "text": "Tematyka <w>ufo</w> często pojawia się w filmach science fiction.",
                            "target_lang_translations": {
                                "ru": "Тема <w>НЛО</w> часто встречается в научно-фантастических фильмах."
                            }
                        }
                    ],
                    "synonyms": [
                        "UFO",
                        "NOL",
                        "latający talerz",
                        "latający spodek",
                        "niezidentyfikowany obiekt latający"
                    ],
                    "common_phrases": [
                        "widzieć ufo",
                        "wierzyć w ufo",
                        "porwanie przez ufo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопознанный летающий объект; загадочное явление или объект в небе, природу которого невозможно объяснить, часто ассоциируемый с космическими кораблями пришельцев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "НЛО",
                                "target_lang_sense_clarification": "аббревиатура"
                            },
                            {
                                "target_lang_word": "летающая тарелка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },

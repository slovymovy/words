    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b6e69d8b-2e58-3aa5-aee5-2c22f3f84df2",
                    "sense_definition": "Komunikat (np. w telewizji, internecie, gazecie) zachęcający do kupna produktu lub skorzystania z usługi",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie lubię, gdy ciekawy film w telewizji przerywają długie <w>reklamy</w>.",
                            "target_lang_translations": {
                                "ru": "Я не люблю, когда интересный фильм по телевизору прерывает длинная <w>реклама</w>."
                            }
                        },
                        {
                            "text": "Wczoraj widziałem bardzo zabawną <w>reklamę</w> nowego samochodu.",
                            "target_lang_translations": {
                                "ru": "Вчера я видел очень смешную <w>рекламу</w> новой машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogłoszenie",
                        "anons",
                        "inserat"
                    ],
                    "common_phrases": [
                        "reklama telewizyjna",
                        "blok reklam",
                        "oglądać reklamy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщение (например, по телевидению, в интернете, газете), побуждающее купить продукт или воспользоваться услугой"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реклама",
                                "target_lang_sense_clarification": "коммерческое сообщение"
                            },
                            {
                                "target_lang_word": "объявление",
                                "target_lang_sense_clarification": "в газете или интернете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fc319b9-7310-3fe3-8114-82624a7700dd",
                    "sense_definition": "Ogół działań marketingowych i promocyjnych mających na celu zwiększenie sprzedaży lub popularyzację czegoś",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dobra <w>reklama</w> jest niezbędna, aby nowa firma odniosła sukces na rynku.",
                            "target_lang_translations": {
                                "ru": "Хорошая <w>реклама</w> необходима, чтобы новая фирма добилась успеха на рынке."
                            }
                        },
                        {
                            "text": "Mój brat zajmuje się zawodowo <w>reklamą</w> i wymyślaniem haseł promocyjnych.",
                            "target_lang_translations": {
                                "ru": "Мой брат профессионально занимается <w>рекламой</w> и придумыванием рекламных слоганов."
                            }
                        }
                    ],
                    "antonyms": [
                        "antyreklama"
                    ],
                    "common_phrases": [
                        "reklama dźwignią handlu",
                        "agencja reklamy",
                        "skuteczna reklama"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплекс маркетинговых и рекламных мероприятий, направленных на увеличение продаж или популяризацию чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реклама",
                                "target_lang_sense_clarification": "маркетинг, деятельность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76b6d998-a236-3115-a0d8-7b2ed3399289",
                    "sense_definition": "Fizyczny obiekt, taki jak plakat, szyld lub baner, umieszczony w przestrzeni publicznej w celach promocyjnych",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny wiatr zerwał dużą <w>reklamę</w> z elewacji sąsiedniego budynku.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер сорвал большую <w>рекламу</w> с фасада соседнего здания."
                            }
                        },
                        {
                            "text": "Ta jaskrawa <w>reklama</w> przy drodze rozprasza kierowców w nocy.",
                            "target_lang_translations": {
                                "ru": "Эта яркая <w>реклама</w> у дороги отвлекает водителей ночью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "reklama świetlna",
                        "wieszać reklamę",
                        "wielkoformatowa reklama"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физический объект, такой как плакат, вывеска или баннер, размещенный в общественном месте в рекламных целях"
                    },
                    "translations": {
                        "ru": [
                            {

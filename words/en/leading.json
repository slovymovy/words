    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5f6c81a8-1d00-3a27-8b37-bc56e9401c7a",
                    "sense_definition": "Ranking first; most important or most successful.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She played a <w>leading</w> role in the organization of the festival.",
                            "target_lang_translations": {
                                "ru": "Она сыграла <w>ведущую</w> роль в организации фестиваля."
                            }
                        },
                        {
                            "text": "Heart disease is a <w>leading</w> cause of death in many countries.",
                            "target_lang_translations": {
                                "ru": "Болезни сердца являются <w>основной</w> причиной смерти во многих странах."
                            }
                        }
                    ],
                    "antonyms": [
                        "lagging",
                        "trailing"
                    ],
                    "common_phrases": [
                        "leading role",
                        "leading cause",
                        "leading expert"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимающий первое место; самый важный или успешный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ведущий",
                                "target_lang_sense_clarification": "самый важный, главный"
                            },
                            {
                                "target_lang_word": "главный",
                                "target_lang_sense_clarification": "основной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c9f8fa5-f428-3aae-94f8-fa8edade8ec8",
                    "sense_definition": "Designed to guide someone to a specific conclusion or action, especially in questioning.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer was warned by the judge not to ask <w>leading</w> questions.",
                            "target_lang_translations": {
                                "ru": "Судья предупредил адвоката не задавать <w>наводящих</w> вопросов."
                            }
                        },
                        {
                            "text": "Her <w>leading</w> remarks helped the student find the correct answer.",
                            "target_lang_translations": {
                                "ru": "Её <w>наводящие</w> замечания помогли студенту найти правильный ответ."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leading question"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для того, чтобы подвести кого-то к определенному выводу или действию (о вопросах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наводящий",
                                "target_lang_sense_clarification": "подсказывающий ответ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a0a8b1d-4814-3c3c-9c63-b1d478052f0f",
                    "sense_definition": "Occurring in advance or positioned at the front.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>leading</w> edge of the storm brought heavy rain and wind.",
                            "target_lang_translations": {
                                "ru": "<w>Передний</w> край шторма принес сильный дождь и ветер."
                            }
                        },
                        {
                            "text": "Consumer confidence is often considered a <w>leading</w> economic indicator.",
                            "target_lang_translations": {
                                "ru": "Уверенность потребителей часто считается <w>опережающим</w> экономическим показателем."
                            }
                        }
                    ],
                    "antonyms": [
                        "following"
                    ],
                    "common_phrases": [
                        "leading edge",
                        "leading indicator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся впереди или происходящий заранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опережающий",
                                "target_lang_sense_clarification": "происходящий заранее"
                            },
                            {
                                "target_lang_word": "передний",
                                "target_lang_sense_clarification": "расположенный спереди"
                            }
                        ]
                    }
                }
            ]
        },
        {

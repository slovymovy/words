    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ae48b27-5308-34ab-9bd5-913ad10a5ab7",
                    "sense_definition": "A place on the coast where ships stop to load and unload cargo or passengers; a harbor.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "harbor_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ship arrived at the <w>port</w> early in the morning to unload its cargo.",
                            "target_lang_translations": {
                                "ru": "Корабль прибыл в <w>порт</w> рано утром для разгрузки груза."
                            }
                        },
                        {
                            "text": "We visited a beautiful fishing <w>port</w> during our holiday in Spain.",
                            "target_lang_translations": {
                                "ru": "Мы посетили красивый рыбацкий <w>порт</w> во время нашего отпуска в Испании."
                            }
                        }
                    ],
                    "synonyms": [
                        "harbour",
                        "haven"
                    ],
                    "common_phrases": [
                        "fishing port",
                        "busy port",
                        "port of call",
                        "leave port"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место на побережье, где останавливаются корабли для погрузки и разгрузки грузов или пассажиров; гавань."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порт",
                                "target_lang_sense_clarification": "место стоянки судов"
                            },
                            {
                                "target_lang_word": "гавань",
                                "target_lang_sense_clarification": "защищенное место для стоянки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0d0d292-ec62-3b23-af9d-8aed2d86669c",
                    "sense_definition": "A socket in a computer or network device where cables are connected to transfer data.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please plug the mouse into the USB <w>port</w> on the side of the laptop.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подключите мышь к USB-<w>порту</w> сбоку ноутбука."
                            }
                        },
                        {
                            "text": "The technician checked the Ethernet <w>port</w> to see why the internet was not working.",
                            "target_lang_translations": {
                                "ru": "Техник проверил Ethernet-<w>порт</w>, чтобы понять, почему не работает интернет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "USB port",
                        "serial port",
                        "Ethernet port",
                        "charging port"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разъем в компьютере или сетевом устройстве, к которому подключаются кабели для передачи данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порт",
                                "target_lang_sense_clarification": "компьютерный разъем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aaeb6082-7e9d-3e32-93a6-7eb02e2489c0",
                    "sense_definition": "A strong, sweet dark red wine, traditionally made in Portugal.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "wine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After dinner, the guests sat by the fire and enjoyed a glass of <w>port</w>.",
                            "target_lang_translations": {
                                "ru": "После ужина гости сидели у камина и наслаждались бокалом <w>портвейна</w>."
                            }
                        },
                        {
                            "text": "This vintage <w>port</w> pairs very well with strong cheeses.",
                            "target_lang_translations": {
                                "ru": "Этот марочный <w>портвейн</w> очень хорошо сочетается с острыми сырами."
                            }
                        }
                    ],
                    "synonyms": [
                        "port wine"
                    ],
                    "common_phrases": [
                        "glass of port",
                        "vintage port",
                        "tawny port"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крепкое сладкое темно-красное вино, традиционно производимое в Португалии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портвейн",
                                "target_lang_sense_clarification": "сорт вина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02e297e7-7af6-35ba-b42b-d37e9d033e40",
                    "sense_definition": "The left side of a ship or aircraft when facing forward.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain ordered the crew to look to <w>port</w> to see the approaching island.",
                            "target_lang_translations": {
                                "ru": "Капитан приказал команде смотреть на <w>левый борт</w>, чтобы увидеть приближающийся остров."
                            }
                        },
                        {
                            "text": "All the emergency exits are located on the <w>port</w> side of the aircraft.",
                            "target_lang_translations": {
                                "ru": "Все аварийные выходы расположены по <w>левому борту</w> самолета."
                            }
                        }
                    ],
                    "synonyms": [
                        "left"
                    ],
                    "antonyms": [
                        "starboard"
                    ],
                    "common_phrases": [
                        "port side",
                        "hard to port"
                    ],
                    "target_lang_definitions": {
                        "ru": "Левая сторона корабля или самолета, если смотреть вперед."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левый борт",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34d91525-cc05-3b43-90cd-2289e6bcf3ad",
                    "sense_definition": "A computer program that has been changed so that it can run on a different type of computer or operating system.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mobile <w>port</w> of the game runs surprisingly smoothly on older phones.",
                            "target_lang_translations": {
                                "ru": "Мобильный <w>порт</w> игры работает на удивление плавно на старых телефонах."
                            }
                        },
                        {
                            "text": "Developers released a <w>port</w> of the software for Linux users last week.",
                            "target_lang_translations": {
                                "ru": "Разработчики выпустили <w>порт</w> программного обеспечения для пользователей Linux на прошлой неделе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "console port",
                        "PC port",
                        "bad port"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа, измененная для работы на другом типе компьютера или операционной системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порт",
                                "target_lang_sense_clarification": "адаптированная версия программы"

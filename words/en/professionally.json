    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8360a295-a6e7-3bc0-a2ce-ddd6b5704777",
                    "sense_definition": "As a professional; for one's paid career.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "career_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many young athletes dream of playing sports <w>professionally</w> one day.",
                            "target_lang_translations": {
                                "ru": "Многие молодые спортсмены мечтают однажды заняться спортом <w>профессионально</w>."
                            }
                        },
                        {
                            "text": "She writes <w>professionally</w> for several major magazines and newspapers.",
                            "target_lang_translations": {
                                "ru": "Она <w>профессионально</w> пишет для нескольких крупных журналов и газет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "play professionally",
                        "work professionally",
                        "sing professionally"
                    ],
                    "target_lang_definitions": {
                        "ru": "В качестве профессионала; как основной вид деятельности или карьера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "профессионально",
                                "target_lang_sense_clarification": "в качестве работы или карьеры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ce34719-7cfb-34cc-9f58-6b15261eba31",
                    "sense_definition": "In a professional manner; showing skill, competence, and appropriate behavior.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "manner_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You handled that customer's complaint very <w>professionally</w> despite their anger.",
                            "target_lang_translations": {
                                "ru": "Вы очень <w>профессионально</w> разобрались с жалобой этого клиента, несмотря на его гнев."
                            }
                        },
                        {
                            "text": "It is important to dress and act <w>professionally</w> when attending a job interview.",
                            "target_lang_translations": {
                                "ru": "Важно одеваться и вести себя <w>профессионально</w>, приходя на собеседование."
                            }
                        }
                    ],
                    "synonyms": [
                        "competently",
                        "expertly",
                        "skillfully"
                    ],
                    "antonyms": [
                        "unprofessionally"
                    ],
                    "common_phrases": [
                        "act professionally",
                        "behave professionally",
                        "handle professionally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Профессионально; проявляя мастерство, компетентность и соответствующее поведение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "профессионально",
                                "target_lang_sense_clarification": "мастерски, качественно"
                            },
                            {
                                "target_lang_word": "компетентно",
                                "target_lang_sense_clarification": "со знанием дела"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "55bc1d1b-af46-34f0-b994-59420cce2b9a",
                    "sense_definition": "Wat vele uren in beslag neemt of duurt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "duration_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een <w>urenlange</w> wandeling door het bos waren we erg moe.",
                            "target_lang_translations": {
                                "ru": "После <w>многочасовой</w> прогулки по лесу мы очень устали.",
                                "en": "After an <w>hours-long</w> walk through the forest, we were very tired."
                            }
                        },
                        {
                            "text": "De <w>urenlange</w> vertraging op het vliegveld zorgde voor veel irritatie bij de passagiers.",
                            "target_lang_translations": {
                                "ru": "<w>Многочасовая</w> задержка в аэропорту вызвала сильное раздражение у пассажиров.",
                                "en": "The <w>hours-long</w> delay at the airport caused a lot of irritation among the passengers."
                            }
                        }
                    ],
                    "synonyms": [
                        "langdurig"
                    ],
                    "antonyms": [
                        "kortstondig",
                        "kort"
                    ],
                    "common_phrases": [

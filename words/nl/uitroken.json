            "senses": [
                {
                    "sense_id": "4852b674-d39f-340d-8865-b934c3dfcbe1",
                    "sense_definition": "Iemand of een dier dwingen een schuilplaats of ruimte te verlaten door rook naar binnen te blazen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jagers probeerden de vos uit zijn hol <w>uit</w> te <w>roken</w>.",
                            "target_lang_translations": {
                                "ru": "Охотники пытались <w>выкурить</w> лису из её норы."
                            }
                        },
                        {
                            "text": "Het grote bedrijf probeerde de kleine concurrenten financieel <w>uit</w> te <w>roken</w>.",
                            "target_lang_translations": {
                                "ru": "Крупная компания пыталась финансово <w>выкурить</w> мелких конкурентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "verjagen",
                        "verdrijven"
                    ],
                    "common_phrases": [
                        "een hol uitroken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить человека или животное покинуть убежище или помещение, напустив туда дыма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выкуривать",
                                "target_lang_sense_clarification": "выгонять дымом"
                            },
                            {
                                "target_lang_word": "выживать",
                                "target_lang_sense_clarification": "перен. вытеснять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dcc7d78d-1956-35a1-bdcd-3e5867b148a9",
                    "sense_definition": "Een ruimte of planten met rook behandelen om ongedierte of een plaag te bestrijden.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moesten een professioneel bedrijf bellen om het wespennest te laten <w>uitroken</w>.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось вызвать профессиональную службу, чтобы <w>выкурить</w> осиное гнездо."
                            }
                        },
                        {
                            "text": "De boer besloot de mollen in het weiland <w>uit</w> te <w>roken</w>.",
                            "target_lang_translations": {
                                "ru": "Фермер решил <w>выкурить</w> кротов с пастбища."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestrijden"
                    ],
                    "common_phrases": [
                        "ongedierte uitroken",
                        "een wespennest uitroken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обрабатывать помещение или растения дымом для борьбы с вредителями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окуривать",
                                "target_lang_sense_clarification": "обрабатывать дымом"
                            },
                            {
                                "target_lang_word": "выкуривать",
                                "target_lang_sense_clarification": "изгонять вредителей дымом"
                            }
                        ]
                    }
                }
            ]
        }
    ]

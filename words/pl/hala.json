{
    "word_family": [
        "halowy",
        "halny"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "175bbea8-68ac-301f-9ea6-1837ba23dad4",
                    "sense_definition": "Duże, przestronne pomieszczenie lub budynek o rozległej powierzchni, przeznaczony do celów sportowych, produkcyjnych, handlowych lub wystawowych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W nowej <hali> sportowej odbywają się mecze siatkówki.",
                            "target_lang_translations": {
                                "ru": "В новом спортивном <w>зале</w> проходят волейбольные матчи."
                            }
                        },
                        {
                            "text": "Ta <hala> produkcyjna jest wyposażona w nowoczesne maszyny.",
                            "target_lang_translations": {
                                "ru": "Этот производственный <w>цех</w> оснащен современными машинами."
                            }
                        }
                    ],
                    "synonyms": [
                        "sala"
                    ],
                    "common_phrases": [
                        "hala sportowa",
                        "hala produkcyjna",
                        "hala targowa",
                        "hala odlotów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое, просторное помещение или здание, предназначенное для спортивных, производственных или торговых целей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зал",
                                "target_lang_sense_clarification": "спортивный или выставочный"
                            },
                            {
                                "target_lang_word": "цех",
                                "target_lang_sense_clarification": "производственный"
                            },
                            {
                                "target_lang_word": "павильон",
                                "target_lang_sense_clarification": "торговый или выставочный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a105a25-18a9-3b46-ad7a-1f4e7240bc73",
                    "sense_definition": "Górska łąka położona wysoko w górach, często powyżej granicy lasu, wykorzystywana jako pastwisko.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górale od wieków wypasają owce na tej <hali>.",
                            "target_lang_translations": {
                                "ru": "Горцы веками пасут овец на этом <w>горном пастбище</w>."
                            }
                        },
                        {
                            "text": "Po długiej wspinaczce odpoczęliśmy na słonecznej <hali>.",
                            "target_lang_translations": {
                                "ru": "После долгого восхождения мы отдохнули на солнечном <w>альпийском лугу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wypas na hali",
                        "owce na hali"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горный луг, расположенный высоко в горах, часто выше границы леса, используемый как пастбище."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горное пастбище",
                                "target_lang_sense_clarification": "место для выпаса скота в горах"
                            },
                            {
                                "target_lang_word": "полонина",
                                "target_lang_sense_clarification": "безлесный участок в Карпатах, используемый для выпаса"
                            },
                            {
                                "target_lang_word": "альпийский луг",
                                "target_lang_sense_clarification": "тип растительности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "833aedd7-a9f0-30df-b54b-514a6e8ab99d",
                    "sense_definition": "Ogół osób zgromadzonych w dużej sali lub budynku w celu obejrzenia widowiska.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała <hala> klaskała po udanym występie artysty.",
                            "target_lang_translations": {
                                "ru": "Весь <w>зал</w> аплодировал после удачного выступления артиста."
                            }
                        },
                        {
                            "text": "Gdy zawodnik upadł, <hala> zamarła w ciszy.",
                            "target_lang_translations": {
                                "ru": "Когда спортсмен упал, <w>зал</w> замер в тишине."
                            }
                        }
                    ],
                    "synonyms": [
                        "publiczność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, собравшиеся в большом зале или здании для просмотра зрелища."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зал",
                                "target_lang_sense_clarification": "публика, зрители (метонимия)"
                            },
                            {
                                "target_lang_word": "публика",
                                "target_lang_sense_clarification": "зрители"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1354661d-0079-3c51-8242-926e13e91517",
                    "sense_definition": "Typ kościoła wielonawowego, w którym wszystkie nawy mają tę samą lub zbliżoną wysokość.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten gotycki kościół to klasyczna trzynawowa <hala>.",
                            "target_lang_translations": {
                                "ru": "Эта готическая церковь — классический трехнефный <w>зальный храм</w>."
                            }
                        },
                        {
                            "text": "Przewodnik pokazał nam różnice między bazyliką a <halą>.",
                            "target_lang_translations": {
                                "ru": "Гид показал нам разницу между базиликой и <w>зальным храмом</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "bazylika"
                    ],
                    "common_phrases": [
                        "kościół halowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип многонефного храма, в котором все нефы имеют одинаковую или близкую высоту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зальный храм",
                                "target_lang_sense_clarification": "архитектурный тип"
                            },
                            {
                                "target_lang_word": "церковь зального типа",
                                "target_lang_sense_clarification": "архитектурный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "44e575df-16a3-367b-bb51-eadc7225de65",
                    "sense_definition": "Okrzyk ponaglający, mający na celu zachęcenie kogoś do ruchu lub działania.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Hala>, ruszajcie się szybciej z tą pracą!",
                            "target_lang_translations": {
                                "ru": "<w>А ну</w>, шевелитесь быстрее с этой работой!"
                            }
                        },
                        {
                            "text": "<Hala>, idziemy już, bo robi się późno.",
                            "target_lang_translations": {
                                "ru": "<w>Давай</w>, идем уже, а то становится поздно."
                            }
                        }
                    ],
                    "synonyms": [
                        "dalej!"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, побуждающий кого-либо к движению или действию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "побуждение к действию"
                            },
                            {
                                "target_lang_word": "а ну",
                                "target_lang_sense_clarification": "резкое побуждение"
                            },
                            {
                                "target_lang_word": "давай",
                                "target_lang_sense_clarification": "призыв к действию"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
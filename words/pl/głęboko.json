    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "cf4462a5-dcb0-3468-a1a7-bc6c6f7ccc34",
                    "sense_definition": "Znajdując się w dużej odległości od powierzchni, brzegu lub wejścia, głęboko wewnątrz czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_depth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nurek zszedł <głęboko> pod wodę, aby obejrzeć rafę."
                        },
                        {
                            "text": "Korzenie tego starego dębu sięgają bardzo <głęboko> w ziemię."
                        }
                    ],
                    "antonyms": [
                        "płytko"
                    ],
                    "common_phrases": [
                        "głęboko pod wodą",
                        "głęboko w lesie",
                        "głęboko w ziemi"
                    ]
                },
                {
                    "sense_id": "079eedc5-c60e-39e7-ad90-452ffe2f370b",
                    "sense_definition": "W stopniu bardzo dużym, intensywnym lub przepełnionym emocjami; w sposób świadczący o dużym zaangażowaniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem <głęboko> przekonany, że podjęliśmy właściwą decyzję."
                        },
                        {
                            "text": "Jej smutna historia <głęboko> mnie poruszyła."
                        }
                    ],
                    "synonyms": [
                        "dogłębnie"
                    ],
                    "common_phrases": [
                        "głęboko wierzyć",
                        "głęboko przekonany",
                        "głęboko wzruszony"
                    ]
                },
                {
                    "sense_id": "409314ab-b24a-3043-bad4-5cb3e3201f64",
                    "sense_definition": "Wciągając dużą ilość powietrza do płuc podczas oddychania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "breathing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <głęboko> oddychać, żeby uspokoić nerwy przed egzaminem."
                        },
                        {
                            "text": "Pacjent spał spokojnie i oddychał <głęboko>."
                        }
                    ],
                    "antonyms": [
                        "płytko"
                    ],
                    "common_phrases": [
                        "głęboko oddychać",
                        "głęboko wciągnąć powietrze"
                    ]
                },
                {
                    "sense_id": "377f10c6-bede-3ddd-8c73-8900cf1ff68d",
                    "sense_definition": "W odległym punkcie czasu, na przykład późno w nocy lub dawno w przeszłości.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siedzieliśmy przy ognisku i rozmawialiśmy <głęboko> w noc."
                        },
                        {
                            "text": "Tradycje tego regionu sięgają <głęboko> w przeszłość."
                        }
                    ],
                    "common_phrases": [
                        "głęboko w nocy",
                        "sięgać głęboko w przeszłość"
                    ]
                }
            ]
        }
    ]
}
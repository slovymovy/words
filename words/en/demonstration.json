{
    "word_family": [
        "demonstrate",
        "demonstrator",
        "demonstrative",
        "demonstrable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5fdbdd86-5821-3620-8912-a8a7780ec168",
                    "sense_definition": "A public display of group opinion, such as a protest march or rally.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "public_protest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thousands of people gathered for a peaceful <w>demonstration</w> against the new law.",
                            "target_lang_translations": {
                                "ru": "Тысячи людей собрались на мирную <w>демонстрацию</w> против нового закона."
                            }
                        },
                        {
                            "text": "The police monitored the street <w>demonstration</w> to ensure public safety.",
                            "target_lang_translations": {
                                "ru": "Полиция наблюдала за уличной <w>демонстрацией</w> для обеспечения общественной безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "protest",
                        "march"
                    ],
                    "common_phrases": [
                        "peaceful demonstration",
                        "mass demonstration",
                        "street demonstration",
                        "hold a demonstration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публичное выражение группового мнения, например, марш протеста или митинг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демонстрация",
                                "target_lang_sense_clarification": "политическая акция"
                            },
                            {
                                "target_lang_word": "манифестация",
                                "target_lang_sense_clarification": "массовое шествие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2553862d-3124-3b01-8a1f-2a91994e72d3",
                    "sense_definition": "An event where someone shows how a product works or how a task is performed.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "practical_display",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chef gave a <w>demonstration</w> on how to bake bread.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар провел <w>демонстрацию</w> того, как печь хлеб."
                            }
                        },
                        {
                            "text": "We watched a product <w>demonstration</w> to understand the new software features.",
                            "target_lang_translations": {
                                "ru": "Мы посмотрели <w>демонстрацию</w> продукта, чтобы понять новые функции программного обеспечения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "give a demonstration",
                        "product demonstration",
                        "cooking demonstration",
                        "live demonstration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мероприятие, на котором показывают, как работает продукт или выполняется задача."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демонстрация",
                                "target_lang_sense_clarification": "показ работы"
                            },
                            {
                                "target_lang_word": "показ",
                                "target_lang_sense_clarification": "представление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f95a499-fe9f-3e58-876a-11aee79f760c",
                    "sense_definition": "An outward expression of one's feelings or attitude.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "expression_of_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His gift was a clear <w>demonstration</w> of his affection for her.",
                            "target_lang_translations": {
                                "ru": "Его подарок был явным <w>проявлением</w> его привязанности к ней."
                            }
                        },
                        {
                            "text": "The crowd cheered in a noisy <w>demonstration</w> of support for the team.",
                            "target_lang_translations": {
                                "ru": "Толпа ликовала в шумной <w>демонстрации</w> поддержки команды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "demonstration of affection",
                        "demonstration of support",
                        "demonstration of loyalty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешнее проявление чувств или отношения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проявление",
                                "target_lang_sense_clarification": "чувств"
                            },
                            {
                                "target_lang_word": "выражение",
                                "target_lang_sense_clarification": "эмоций"
                            },
                            {
                                "target_lang_word": "демонстрация",
                                "target_lang_sense_clarification": "нарочитое поведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84470462-b6a2-3802-9162-b6488a4c8cc9",
                    "sense_definition": "A logical argument or evidence that proves something is true.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "logical_proof",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor provided a rigorous <w>demonstration</w> of the mathematical theorem.",
                            "target_lang_translations": {
                                "ru": "Профессор представил строгое <w>доказательство</w> математической теоремы."
                            }
                        },
                        {
                            "text": "This experiment serves as a <w>demonstration</w> of the physical laws involved.",
                            "target_lang_translations": {
                                "ru": "Этот эксперимент служит <w>демонстрацией</w> действующих физических законов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "logical demonstration",
                        "clear demonstration",
                        "scientific demonstration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Логический аргумент или доказательство, подтверждающее истинность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доказательство",
                                "target_lang_sense_clarification": "логическое"
                            },
                            {
                                "target_lang_word": "наглядное подтверждение",
                                "target_lang_sense_clarification": "свидетельство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

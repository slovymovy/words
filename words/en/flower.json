    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "528fc711-149a-321d-9c4d-1f61c8b9348e",
                    "sense_definition": "A colorful, conspicuous structure associated with angiosperms, frequently scented and attracting various insects, and which may or may not be used for sexual reproduction.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "botanical_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bees were attracted to the bright yellow <w>flower</w> in the garden.",
                            "target_lang_translations": {
                                "ru": "Пчел привлек ярко-желтый <w>цветок</w> в саду."
                            }
                        },
                        {
                            "text": "She drew a picture of a large red <w>flower</w> with green leaves.",
                            "target_lang_translations": {
                                "ru": "Она нарисовала большой красный <w>цветок</w> с зелеными листьями."
                            }
                        }
                    ],
                    "synonyms": [
                        "bloom",
                        "blossom"
                    ],
                    "common_phrases": [
                        "beautiful flower",
                        "plant a flower",
                        "smell the flower"
                    ],
                    "target_lang_definitions": {
                        "ru": "Красочная, заметная часть покрытосеменных растений, часто ароматная и привлекающая насекомых, служащая для размножения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цветок",
                                "target_lang_sense_clarification": "часть растения"
                            },
                            {
                                "target_lang_word": "соцветие",
                                "target_lang_sense_clarification": "группа цветков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea5eb94a-f2de-303c-afad-8892e44f8159",
                    "sense_definition": "A plant that bears flowers, especially a plant that is small and lacks wood.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "botanical_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We transplanted the <w>flowers</w> to a larger pot on the balcony.",
                            "target_lang_translations": {
                                "ru": "Мы пересадили <w>цветы</w> в горшок побольше на балконе."
                            }
                        },
                        {
                            "text": "My grandmother grows many different <w>flowers</w> in her backyard.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка выращивает много разных <w>цветов</w> у себя во дворе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grow flowers",
                        "garden flowers",
                        "spring flowers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение, которое цветет, особенно небольшое и травянистое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цветок",
                                "target_lang_sense_clarification": "растение целиком"
                            },
                            {
                                "target_lang_word": "цветы",
                                "target_lang_sense_clarification": "множественное число, общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fa367a1-82e6-3fea-a0cc-10d81b7f3def",

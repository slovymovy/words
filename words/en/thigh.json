    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "986636f3-9aaf-3b93-a061-64fa886a106c",
                    "sense_definition": "The upper leg of a human, between the hip and the knee.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She rested her hands on her <w>thighs</w> after the long run.",
                            "target_lang_translations": {
                                "ru": "Она положила руки на <w>бёдра</w> после долгой пробежки."
                            }
                        },
                        {
                            "text": "The doctor examined the large bruise on his left <w>thigh</w>.",
                            "target_lang_translations": {
                                "ru": "Врач осмотрел большой синяк на его левом <w>бедре</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "inner thigh",
                        "thigh muscle",
                        "right thigh",
                        "left thigh"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя часть ноги человека, расположенная между тазобедренным и коленным суставами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бедро",
                                "target_lang_sense_clarification": "анатомия"
                            },
                            {
                                "target_lang_word": "ляжка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef99bf81-8065-35fc-909c-2fa944534ddd",
                    "sense_definition": "The part of an animal's leg corresponding to the human thigh, often used as food.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are having roasted chicken <w>thighs</w> with vegetables for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Сегодня на ужин у нас жареные куриные <w>бёдра</w> с овощами."
                            }
                        },
                        {
                            "text": "The grasshopper uses its powerful back <w>thighs</w> to jump great distances.",
                            "target_lang_translations": {
                                "ru": "Кузнечик использует свои мощные задние <w>бёдра</w>, чтобы прыгать на большие расстояния."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chicken thighs",
                        "turkey thigh"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть ноги животного, соответствующая человеческому бедру; часто используется в кулинарии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бедро",
                                "target_lang_sense_clarification": "кулинария, анатомия животных"
                            }
                        ]
                    }
                }
            ]
        }
    ]

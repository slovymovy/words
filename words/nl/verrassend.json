    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "14d09fe4-d835-3af3-a5b8-6a4ae2a8c8ce",
                    "sense_definition": "Wat onverwacht komt en daardoor verbazing wekt, vaak op een positieve manier.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "meaning_surprising",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het boek had een <w>verrassend</w> einde dat niemand zag aankomen.",
                            "target_lang_translations": {
                                "ru": "У книги был <w>неожиданный</w> конец, которого никто не предвидел."
                            }
                        },
                        {
                            "text": "We kregen gisteren <w>verrassend</w> nieuws van onze familie uit het buitenland.",
                            "target_lang_translations": {
                                "ru": "Вчера мы получили <w>неожиданные</w> новости от нашей семьи из-за границы."
                            }
                        }
                    ],
                    "synonyms": [
                        "onverwacht",
                        "verbazingwekkend",
                        "onvoorzien"
                    ],
                    "antonyms": [
                        "voorspelbaar",
                        "verwacht",
                        "alledaags"
                    ],
                    "common_phrases": [
                        "een verrassende wending",
                        "verrassend nieuws",
                        "een verrassende uitslag"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что происходит неожиданно и вызывает удивление, часто в положительном смысле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неожиданный",
                                "target_lang_sense_clarification": "непредвиденный, внезапный"
                            },
                            {
                                "target_lang_word": "удивительный",
                                "target_lang_sense_clarification": "вызывающий изумление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2dd50f71-d62f-3654-bf27-5cb90b7cf3a7",
                    "sense_definition": "In een mate of op een manier die je niet had verwacht.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "meaning_surprising",
                    "name_type": "no",
                    "examples": [
                        {

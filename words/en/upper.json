                },
                {
                    "sense_id": "57f36953-7023-3820-b71b-266d3bab843e",
                    "sense_definition": "A tooth or set of false teeth located in the upper jaw.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "teeth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dentist examined his <w>uppers</w> and found two cavities that needed filling.",

                {
                    "sense_id": "34c5ae9e-6daf-38d2-a3b6-8e6c26edc80a",
                    "sense_definition": "Niet geneigd om te werken of zich in te spannen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben vandaag erg <w>lui</w> geweest.",
                            "target_lang_translations": {
                                "en": "I have been very <w>lazy</w> today.",
                                "ru": "Я сегодня был очень <w>ленивым</w>."
                            }
                        },
                        {
                            "text": "Mijn broer is te <w>lui</w> om zijn kamer op te ruimen.",
                            "target_lang_translations": {
                                "en": "My brother is too <w>lazy</w> to clean up his room.",
                                "ru": "Мой брат слишком <w>ленивый</w>, чтобы убраться в своей комнате."
                            }

{
    "word_family": [
        "valid",
        "validity",
        "invalidate",
        "validation"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b0cc8548-e684-3acc-87d1-e34a2168c93a",
                    "sense_definition": "Not valid; not true, correct, acceptable or appropriate.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "validity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system rejected the login attempt because of an <w>invalid</w> password.",
                            "target_lang_translations": {
                                "ru": "Система отклонила попытку входа из-за <w>неверного</w> пароля."
                            }
                        },
                        {
                            "text": "The judge declared the contract <w>invalid</w> due to a lack of proper signatures.",
                            "target_lang_translations": {
                                "ru": "Судья признал договор <w>недействительным</w> из-за отсутствия надлежащих подписей."
                            }
                        }
                    ],
                    "synonyms": [
                        "nonvalid",
                        "disallowed"
                    ],
                    "antonyms": [
                        "valid"
                    ],
                    "common_phrases": [
                        "invalid argument",
                        "invalid contract",
                        "invalid password",
                        "technically invalid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий юридической силы, неверный или неприемлемый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недействительный",
                                "target_lang_sense_clarification": "о документах, договорах, билетах"
                            },
                            {
                                "target_lang_word": "неверный",
                                "target_lang_sense_clarification": "о данных, паролях, результатах"
                            },
                            {
                                "target_lang_word": "необоснованный",
                                "target_lang_sense_clarification": "о доводах или аргументах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "102c9d35-9d62-3f95-8e9d-6caf03fdcef3",
                    "sense_definition": "Suffering from disability or illness.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "health_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He took responsibility for his <w>invalid</w> father who could no longer work.",
                            "target_lang_translations": {
                                "ru": "Он взял на себя заботу о своем <w>нетрудоспособном</w> отце, который больше не мог работать."
                            }
                        },
                        {
                            "text": "The facility was specially designed to accommodate <w>invalid</w> patients.",
                            "target_lang_translations": {
                                "ru": "Учреждение было специально спроектировано для размещения <w>тяжелобольных</w> пациентов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Страдающий от болезни или увечья; нетрудоспособный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нетрудоспособный",
                                "target_lang_sense_clarification": "утративший способность работать"
                            },
                            {
                                "target_lang_word": "больной",
                                "target_lang_sense_clarification": "страдающий болезнью"
                            },
                            {
                                "target_lang_word": "немощный",
                                "target_lang_sense_clarification": "слабый, бессильный из-за болезни или старости"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6f3b71c9-4944-32ae-adcb-232a7059baef",
                    "sense_definition": "A person who is confined to home or bed because of illness, disability or injury.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "health_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She spent years caring for her <w>invalid</w> mother at home.",
                            "target_lang_translations": {
                                "ru": "Она провела годы, ухаживая дома за своей матерью-<w>инвалидом</w>."
                            }
                        },
                        {
                            "text": "The war left him an <w>invalid</w>, unable to walk without assistance.",
                            "target_lang_translations": {
                                "ru": "Война сделала его <w>инвалидом</w>, неспособным ходить без посторонней помощи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "care for an invalid",
                        "chronic invalid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек с ограниченными возможностями здоровья или утративший трудоспособность из-за болезни или травмы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инвалид",
                                "target_lang_sense_clarification": "человек с стойким нарушением функций организма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5e95203-34d2-3bb2-b832-522ffd7fdc8c",
                    "sense_definition": "To exempt from duty, often military, because of injury or ill health.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "health_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the severe injury, the soldier was <w>invalided</w> out of the army.",
                            "target_lang_translations": {
                                "ru": "После тяжелого ранения солдата <w>комиссовали</w> из армии."
                            }
                        },
                        {
                            "text": "He was <w>invalided</w> home to recover from the tropical disease.",
                            "target_lang_translations": {
                                "ru": "Его <w>комиссовали</w> и отправили домой, чтобы он оправился от тропической болезни."
                            }
                        }
                    ],
                    "common_phrases": [
                        "invalided out",
                        "invalided home"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увольнять с военной службы по состоянию здоровья или из-за ранения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комиссовать",
                                "target_lang_sense_clarification": "уволить из армии по состоянию здоровья"
                            },
                            {
                                "target_lang_word": "списать",
                                "target_lang_sense_clarification": "разговорное: признать негодным к службе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                                    {
                                        "sense_id": "400c0456-b7e8-327d-9b63-62ab89153ffa",
                                        "entry_id": "640bd353-a76c-3cff-9b6b-24c9a80334d4",
                                        "glosses": [
                                            "die genauere Betrachtung, Beobachtung oder das Betasten zur Bestimmung eines Sachverhalts oder Objektes"
                                        ],
                                        "tags": [],
                                        "examples": [
                                            {
                                                "example_id": "e90942c1-635e-4c31-86a0-c55bd56aa259",
                                                "sense_id": "400c0456-b7e8-327d-9b63-62ab89153ffa",
                                                "text": "Die Untersuchung ergab, dass es sich um eine Entzündung handelt, die dringend behandelt werden muss."
                                            },
                                            {
                                                "example_id": "c365b6e3-55a7-493b-b052-16516fdaaf07",
                                                "sense_id": "400c0456-b7e8-327d-9b63-62ab89153ffa",
                                                "text": "„Diese Untersuchung greift auf eine vergleichsweise breite Literaturauswahl sowie auf bislang unveröffentlichtes Archivmaterial zurück.“"
                                            },
                                            {
                                                "example_id": "b197f9a2-352e-44b4-9c79-ebaf0cdb2112",
                                                "sense_id": "400c0456-b7e8-327d-9b63-62ab89153ffa",
                                                "text": "„Die Untersuchung von Holzkohle aus vorgeschichtlichen Feuerstellen erfaßt die Veränderungen der Vegetation ungefähr während der letzten 500000 Jahre – seit der Mensch nach Entdeckung des Feuers erste Feuerstellen anlegte.“"
                                            },
                                            {
                                                "example_id": "3b50ef86-cadf-4d5c-8804-c741c6982df0",
                                                "sense_id": "400c0456-b7e8-327d-9b63-62ab89153ffa",
                                                "text": "„Nach einer Untersuchung bei seinem Hausarzt wartet der sexuell akive Junggeselle auf das Ergebnis.“"
                                            }
                                        ],
                                        "sense_index_json": "\"1\""
                                    }
                                ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "318d25f8-ed59-3dbd-9d64-c18b900510a6",
                    "sense_definition": "Een platte hoop of plaats, meestal op een boerderij, waar mest en ander organisch afval wordt verzameld.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "literal_pile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer bracht het afval met een kruiwagen naar de <w>mestvaalt</w>.",
                            "target_lang_translations": {
                                "en": "The farmer took the waste to the <w>dunghill</w> in a wheelbarrow."
                            }
                        },
                        {
                            "text": "Door de warmte begon de <w>mestvaalt</w> achter de stal behoorlijk te stinken.",
                            "target_lang_translations": {
                                "en": "Due to the heat, the <w>dungheap</w> behind the stable started to stink considerably."
                            }
                        }
                    ],
                    "synonyms": [
                        "mesthoop",
                        "drekhoop"
                    ],
                    "common_phrases": [
                        "op de mestvaalt"
                    ],
                    "target_lang_definitions": {
                        "en": "A flat pile or area, usually on a farm, where manure and other organic waste are collected."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dunghill",
                                "target_lang_sense_clarification": "pile of manure"
                            },
                            {
                                "target_lang_word": "dungheap",
                                "target_lang_sense_clarification": "pile of manure"
                            },
                            {
                                "target_lang_word": "manure pile",
                                "target_lang_sense_clarification": "modern agricultural term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7813484-d96b-35ff-af55-82c17e07e43c",
                    "sense_definition": "Een situatie, omgeving of instituut dat als zeer vies, corrupt of moreel verwerpelijk wordt beschouwd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "figurative_mess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De journalist omschreef de corrupte organisatie als een morele <w>mestvaalt</w>.",
                            "target_lang_translations": {
                                "en": "The journalist described the corrupt organization as a moral <w>cesspool</w>."
                            }
                        },
                        {
                            "text": "Hij wilde ontsnappen uit de <w>mestvaalt</w> van criminaliteit in de achterstandswijk.",
                            "target_lang_translations": {
                                "en": "He wanted to escape the <w>cesspool</w> of crime in the deprived neighborhood."
                            }
                        }
                    ],
                    "synonyms": [
                        "vuilnisbelt",
                        "afvalberg"
                    ],
                    "target_lang_definitions": {
                        "en": "A situation, environment, or institution regarded as very dirty, corrupt, or morally reprehensible."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cesspool",
                                "target_lang_sense_clarification": "figurative: place of moral filth"
                            },
                            {
                                "target_lang_word": "dunghill",
                                "target_lang_sense_clarification": "figurative: wretched or loathsome place"
                            },
                            {
                                "target_lang_word": "sink",
                                "target_lang_sense_clarification": "place of vice or corruption"
                            }
                        ]
                    }
                }
            ]
        }
    ]

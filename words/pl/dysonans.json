    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9c27b6bd-0f3e-3602-be56-9c3b8fdc60db",
                    "sense_definition": "Brak zgodności, brak dopasowania między elementami, poglądami lub uczuciami; zgrzyt w relacjach lub odbiorze rzeczywistości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "brak_zgodności",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Między ich deklaracjami a rzeczywistymi działaniami panował wyraźny <w>dysonans</w>.",
                            "target_lang_translations": {
                                "ru": "Между их заявлениями и реальными действиями царил явный <w>диссонанс</w>."
                            }
                        },
                        {
                            "text": "Odczuwam pewien <w>dysonans</w> poznawczy, gdy czytam te sprzeczne informacje w gazecie.",
                            "target_lang_translations": {
                                "ru": "Я испытываю некий когнитивный <w>диссонанс</w>, когда читаю эту противоречивую информацию в газете."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezgodność",
                        "rozbieżność",
                        "sprzeczność",
                        "dysharmonia",
                        "różnica"
                    ],
                    "antonyms": [
                        "zgodność",
                        "harmonia"
                    ],
                    "common_phrases": [
                        "dysonans poznawczy",
                        "wyraźny dysonans",
                        "rodzić dysonans"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие согласия или соответствия между элементами, взглядами или чувствами; разлад в отношениях или восприятии реальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диссонанс",
                                "target_lang_sense_clarification": "переносное значение: противоречие, отсутствие гармонии"
                            },
                            {
                                "target_lang_word": "несоответствие",
                                "target_lang_sense_clarification": "отсутствие совпадения, различие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b70e0178-236f-3658-8716-ae53fff199eb",
                    "sense_definition": "Dźwięki zagrane razem lub po sobie, które nie dają wrażenia harmonijnego współbrzmienia i często budują napięcie muzyczne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "muzyka",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sekunda i septyma są <w>dysonansami</w> znajdującymi się w obrębie oktawy.",
                            "target_lang_translations": {
                                "ru": "Секунда и септима являются <w>диссонансами</w> в пределах октавы."
                            }
                        },
                        {
                            "text": "Kompozytor celowo wprowadził w utworze ostry <w>dysonans</w>, aby zaniepokoić słuchaczy.",
                            "target_lang_translations": {
                                "ru": "Композитор намеренно ввел в произведение резкий <w>диссонанс</w>, чтобы встревожить слушателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "dysharmonia"
                    ],
                    "antonyms": [
                        "konsonans",
                        "harmonia"
                    ],
                    "common_phrases": [
                        "ostry dysonans",
                        "rozwiązanie dysonansu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звуки, звучащие одновременно или последовательно, не образующие гармоничного созвучия и часто создающие музыкальное напряжение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диссонанс",
                                "target_lang_sense_clarification": "музыкальный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

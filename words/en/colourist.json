    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ed57e0d7-e8ff-3481-b92d-d06f11053fbf",
                    "sense_definition": "A hairdresser who specializes in dyeing and tinting hair.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hair_specialist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I made an appointment with my <w>colourist</w> to change my hair to blonde.",
                            "target_lang_translations": {
                                "ru": "Я записалась к своему <w>колористу</w>, чтобы перекраситься в блондинку."
                            }
                        },
                        {
                            "text": "She works as a professional hair <w>colourist</w> at a busy downtown salon.",
                            "target_lang_translations": {
                                "ru": "Она работает профессиональным <w>колористом</w> в популярном салоне в центре города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hair colourist",
                        "expert colourist",
                        "salon colourist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Парикмахер, специализирующийся на окрашивании волос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колорист",
                                "target_lang_sense_clarification": "парикмахер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2693063e-212d-36ad-acdd-251718be430d",
                    "sense_definition": "An artist who is skilled in the use and application of color, often in painting or comics.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "art_specialist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>colourist</w> added vibrant shades to the black and white sketch.",
                            "target_lang_translations": {
                                "ru": "<w>Колорист</w> добавил яркие оттенки к черно-белому эскизу."
                            }
                        },
                        {
                            "text": "He is regarded as a master <w>colourist</w> because of his unique ability to mix paints.",
                            "target_lang_translations": {
                                "ru": "Его считают мастером-<w>колористом</w> благодаря его уникальной способности смешивать краски."
                            }
                        }
                    ],
                    "common_phrases": [
                        "master colourist",
                        "comic book colourist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Художник, искусно использующий цвет, или специалист по раскрашиванию (например, комиксов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колорист",
                                "target_lang_sense_clarification": "художник"
                            }
                        ]
                    }
                }
            ]
        }
    ]

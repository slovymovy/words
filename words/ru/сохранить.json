            "pos": "verb",
            "senses": [
                {
                    "sense_id": "33efc26b-a608-38ba-9bb4-eea42c3d96be",
                    "sense_definition": "Уберечь от порчи, утраты или уничтожения; сберечь.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "preservation_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы должны <сохранить> природу для будущих поколений.",
                            "target_lang_translations": {
                                "en": "We must <preserve> nature for future generations."
                            }
                        },
                        {
                            "text": "Ему удалось <сохранить> старые письма и фотографии.",
                            "target_lang_translations": {
                                "en": "He managed to <save> the old letters and photographs."
                            }
                        }
                    ],
                    "synonyms": [
                        "записа́ть"
                    ],
                    "antonyms": [
                        "потерять",
                        "уничтожить"
                    ],
                    "common_phrases": [
                        "сохранить жизнь",
                        "сохранить природу",
                        "сохранить деньги"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep safe from injury, harm, or destruction; to protect."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "preserve",
                                "target_lang_sense_clarification": "to protect from harm or decay"
                            },
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "to rescue from loss or destruction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67881b14-6b95-3fb8-9d6e-9441ddb8f0c3",
                    "sense_definition": "Записать данные в память компьютера или на внешний носитель информации.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computer_save",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не забудь <сохранить> файл перед выходом из программы.",
                            "target_lang_translations": {
                                "en": "Don't forget to <save> the file before exiting the program."
                            }
                        },
                        {
                            "text": "Я хочу <сохранить> эту фотографию на свой телефон.",
                            "target_lang_translations": {
                                "en": "I want to <save> this photo to my phone."
                            }
                        }
                    ],
                    "synonyms": [
                        "записа́ть"
                    ],
                    "antonyms": [
                        "удалить"
                    ],
                    "common_phrases": [
                        "сохранить файл",
                        "сохранить изменения",
                        "сохранить документ"
                    ],
                    "target_lang_definitions": {
                        "en": "To record data to a storage medium, such as a computer disk."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "save",
                                "target_lang_sense_clarification": "to store data on a computer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68d638ac-6770-3791-a42a-23f6f65562da",
                    "sense_definition": "Оставить в прежнем виде, не дать измениться или исчезнуть (о свойствах, внешности, чувствах).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "preservation_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В любой сложной ситуации важно <сохранить> спокойствие.",
                            "target_lang_translations": {
                                "en": "In any difficult situation, it is important to <keep> calm."
                            }
                        },
                        {
                            "text": "Этот старинный дом смог <сохранить> свой исторический облик.",
                            "target_lang_translations": {
                                "en": "This ancient house was able to <preserve> its historical appearance."
                            }

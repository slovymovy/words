    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "cd623239-c588-3a37-b16d-f11199491245",
                    "sense_definition": "Искренне, правдиво, без обмана и лжи.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скажи мне <честно>, ты действительно хочешь поехать с нами?",
                            "target_lang_translations": {
                                "en": "Tell me <honestly>, do you really want to go with us?"
                            }
                        },
                        {
                            "text": "Я <честно> говоря не ожидал увидеть тебя здесь сегодня.",
                            "target_lang_translations": {
                                "en": "<Honestly> speaking, I didn't expect to see you here today."
                            }
                        }
                    ],
                    "synonyms": [
                        "правдиво"
                    ],
                    "antonyms": [
                        "нечестно"
                    ],
                    "common_phrases": [
                        "честно говоря",
                        "честно признаться",
                        "честно сказать"
                    ],
                    "target_lang_definitions": {
                        "en": "Sincerely, truthfully, without deceit or lies."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "honestly",
                                "target_lang_sense_clarification": "in a truthful manner"
                            },
                            {
                                "target_lang_word": "truthfully",
                                "target_lang_sense_clarification": "telling the truth"
                            },
                            {
                                "target_lang_word": "frankly",
                                "target_lang_sense_clarification": "openly and sincerely"
                            }
                        ]

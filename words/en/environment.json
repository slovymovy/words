                            }
                        },
                        {
                            "text": "Companies must adapt to the changing business <w>environment</w>.",
                            "target_lang_translations": {
                                "ru": "Компании должны адаптироваться к меняющейся бизнес-<w>среде</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "political environment",
                        "social environment",
                        "business environment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенная политическая или социальная ситуация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обстановка",
                                "target_lang_sense_clarification": "ситуация, положение дел"
                            },
                            {
                                "target_lang_word": "среда",
                                "target_lang_sense_clarification": "социальная сфера"
                            }

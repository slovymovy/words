            "senses": [
                {
                    "sense_id": "3eadc3da-2612-3785-ac02-15939b642994",
                    "sense_definition": "Relating to or suitable for children aged between approximately 8 and 12.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store has a large section dedicated to <w>tween</w> fashion and accessories.",
                            "target_lang_translations": {
                                "ru": "В магазине есть большой отдел, посвященный моде и аксессуарам <w>для младших подростков</w>."
                            }
                        },
                        {
                            "text": "She writes books for a <w>tween</w> audience, focusing on friendship and school issues.",
                            "target_lang_translations": {
                                "ru": "Она пишет книги для аудитории <w>младшего подросткового возраста</w>, уделяя внимание вопросам дружбы и школы."
                            }

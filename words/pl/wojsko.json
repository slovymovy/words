    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "700a2c85-2727-37dd-983a-358a574f133a",
                    "sense_definition": "Zorganizowane siły zbrojne państwa, których zadaniem jest obrona kraju i walka w przypadku wojny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polskie <w>wojsko</w> jest cenione za swój profesjonalizm na całym świecie.",
                            "target_lang_translations": {
                                "ru": "Польская <w>армия</w> ценится во всем мире за свой профессионализм."
                            }
                        },
                        {
                            "text": "Rząd postanowił przeznaczyć dodatkowe fundusze na <w>wojsko</w> i nowy sprzęt.",
                            "target_lang_translations": {
                                "ru": "Правительство решило выделить дополнительные средства на <w>армию</w> и новую технику."
                            }
                        }
                    ],
                    "synonyms": [
                        "armia",
                        "siły zbrojne",
                        "oręż"
                    ],
                    "common_phrases": [
                        "polskie wojsko",
                        "służyć w wojsku",
                        "wojsko zawodowe",
                        "wstąpić do wojska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованные вооруженные силы государства, задачей которых является оборона страны и ведение боевых действий в случае войны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "армия",
                                "target_lang_sense_clarification": "вооруженные силы государства"
                            },
                            {
                                "target_lang_word": "вооружённые силы",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9c620cd-b076-3cde-880f-41560cd0d0b6",
                    "sense_definition": "Służba w siłach zbrojnych; okres odbywania przeszkolenia bojowego lub bycia żołnierzem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "service",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój starszy brat idzie do <w>wojska</w> zaraz po skończeniu szkoły.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат идет в <w>армию</w> сразу после окончания школы."
                            }
                        },
                        {
                            "text": "Kiedy dziadek był w <w>wojsku</w>, nauczył się dyscypliny i porządku.",
                            "target_lang_translations": {
                                "ru": "Когда дедушка был в <w>армии</w>, он научился дисциплине и порядку."
                            }
                        }
                    ],
                    "synonyms": [
                        "służba",
                        "służba wojskowa"
                    ],
                    "antonyms": [
                        "cywil"
                    ],
                    "common_phrases": [
                        "iść do wojska",
                        "być w wojsku",
                        "powołanie do wojska",
                        "ucieczka z wojska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служба в вооруженных силах; период прохождения боевой подготовки или пребывания в статусе солдата."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "армия",
                                "target_lang_sense_clarification": "разговорное обозначение службы"
                            },
                            {
                                "target_lang_word": "военная служба",
                                "target_lang_sense_clarification": "процесс исполнения воинских обязанностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d948ee6-b3e1-3e68-8b51-00e0ad3b0207",
                    "sense_definition": "Konkretna grupa żołnierzy lub oddział przemieszczający się w terenie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez naszą małą wioskę przemaszerowało wczoraj <w>wojsko</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера через нашу маленькую деревню прошли <w>войска</w>."
                            }
                        },
                        {
                            "text": "Nieprzyjacielskie <w>wojsko</w> zbliżało się powoli do murów miasta.",
                            "target_lang_translations": {
                                "ru": "Вражеская <w>армия</w> медленно приближалась к стенам города."
                            }
                        }
                    ],
                    "synonyms": [
                        "armia"
                    ],
                    "common_phrases": [
                        "przemarsz wojska",
                        "wojsko maszeruje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная группа солдат или подразделение, перемещающееся по местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "войска",
                                "target_lang_sense_clarification": "солдаты, подразделения (мн. ч.)"
                            },
                            {
                                "target_lang_word": "армия",
                                "target_lang_sense_clarification": "крупное войсковое соединение"
                            }
                        ]
                    }
                }
            ]
        }

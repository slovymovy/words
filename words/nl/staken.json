                {
                    "sense_id": "cdc52606-bbd6-368e-ad7d-dca6f6c377e9",
                    "sense_definition": "het tijdelijk neerleggen van het werk als protestmiddel, meestal om betere arbeidsvoorwaarden af te dwingen",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De werknemers van het openbaar vervoer <w>staken</w> voor een hoger loon.",

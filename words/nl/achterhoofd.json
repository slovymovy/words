    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c6feb59f-b912-3ccf-ba65-7825a99d34b1",
                    "sense_definition": "Het achterste gedeelte van het hoofd.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij viel hard op de grond en stootte zijn <w>achterhoofd</w>.",
                            "target_lang_translations": {
                                "ru": "Он сильно упал на землю и ударился <w>затылком</w>."
                            }
                        },
                        {
                            "text": "De kapper schoor de haren op zijn <w>achterhoofd</w> heel kort op.",
                            "target_lang_translations": {
                                "ru": "Парикмахер очень коротко подстриг волосы на его <w>затылке</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "voorhoofd"
                    ],
                    "common_phrases": [
                        "op het achterhoofd",
                        "kaal achterhoofd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задняя часть головы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затылок",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "040adffe-83b6-3d4c-b50a-d6db445c87be",
                    "sense_definition": "Het gedeelte van het bewustzijn waar men iets onthoudt of overweegt zonder er voortdurend aan te denken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet wel in je <w>achterhoofd</w> houden dat de plannen nog kunnen veranderen.",
                            "target_lang_translations": {
                                "ru": "Нужно иметь в <w>виду</w>, что планы ещё могут измениться."
                            }
                        },
                        {
                            "text": "Het idee bleef de hele dag in mijn <w>achterhoofd</w> spelen.",
                            "target_lang_translations": {
                                "ru": "Эта идея весь день оставалась у меня в <w>подсознании</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in het achterhoofd houden",
                        "in het achterhoofd zitten",
                        "in het achterhoofd hebben"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть сознания, где хранятся мысли или факты, о которых человек знает, но не думает постоянно в данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ум",
                                "target_lang_sense_clarification": "в выражениях 'держать в уме', 'на уме'"
                            },
                            {
                                "target_lang_word": "подсознание",
                                "target_lang_sense_clarification": "скрытые мысли"
                            },
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "в выражении 'иметь в виду'"
                            }
                        ]
                    }
                }
            ]
        }
    ]

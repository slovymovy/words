{
    "word_family": [
        "obejmować",
        "objęcie",
        "objętość"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b9cce184-4c1f-30d6-9aef-ecc4240e65fd",
                    "sense_definition": "Otoczyć kogoś lub coś ramionami, przytulić w celu wyrażenia uczuć lub powitania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym cię mocno <w>objąć</w> na pożegnanie.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы крепко <w>обнять</w> тебя на прощание."
                            }
                        },
                        {
                            "text": "Matka postanowiła czule <w>objąć</w> swoje płaczące dziecko.",
                            "target_lang_translations": {
                                "ru": "Мать решила нежно <w>обнять</w> своего плачущего ребенка."
                            }
                        }
                    ],
                    "synonyms": [
                        "przytulić",
                        "uścisnąć"
                    ],
                    "common_phrases": [
                        "objąć ramieniem",
                        "objąć wpół",
                        "czule objąć",
                        "objąć kogoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обхватить кого-то или что-то руками, прижать к себе для выражения чувств или приветствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнять",
                                "target_lang_sense_clarification": "заключить в объятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a35e16d-3edf-33e4-a61d-d820e3053e43",
                    "sense_definition": "Rozpocząć pełnienie ważnej funkcji, stanowiska lub przejąć władzę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy dyrektor ma oficjalnie <w>objąć</w> stanowisko w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Новый директор должен официально <w>вступить</w> в должность на следующей неделе."
                            }
                        },
                        {
                            "text": "Książę musiał <w>objąć</w> tron po nagłej śmierci króla.",
                            "target_lang_translations": {
                                "ru": "Принц должен был <w>занять</w> трон после внезапной смерти короля."
                            }
                        }
                    ],
                    "synonyms": [
                        "zająć"
                    ],
                    "common_phrases": [
                        "objąć stanowisko",
                        "objąć władzę",
                        "objąć tron",
                        "objąć dowodzenie",
                        "objąć rządy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать выполнять важную функцию, занять должность или принять власть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вступить",
                                "target_lang_sense_clarification": "в должность или права"
                            },
                            {
                                "target_lang_word": "занять",
                                "target_lang_sense_clarification": "пост, трон или должность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6de2cdda-4684-345d-9697-b2debebf9b9e",
                    "sense_definition": "Opanować kogoś lub coś z dużą siłą, rozprzestrzenić się na czymś (często o uczuciach, ogniu lub wojnie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "overwhelm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagle <w>objął</w> go paraliżujący strach przed nieznanym.",
                            "target_lang_translations": {
                                "ru": "Внезапно его <w>охватил</w> парализующий страх перед неизвестным."
                            }
                        },
                        {
                            "text": "Gwałtowny pożar zdołał szybko <w>objąć</w> cały drewniany budynek.",
                            "target_lang_translations": {
                                "ru": "Сильный пожар сумел быстро <w>охватить</w> все деревянное здание."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogarnąć",
                        "owładnąć"
                    ],
                    "common_phrases": [
                        "strach go objął",
                        "ogień objął",
                        "smutek objął",
                        "objąć wzrokiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Овладеть кем-то или чем-то с большой силой, распространиться на что-то (часто о чувствах, огне или войне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охватить",
                                "target_lang_sense_clarification": "о чувствах, огне или войне"
                            },
                            {
                                "target_lang_word": "овладеть",
                                "target_lang_sense_clarification": "полностью подчинить (о чувствах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc43cf59-b8df-3bcd-a450-339f6a069bcb",
                    "sense_definition": "Zawierać coś w swoim zakresie, dotyczyć określonej grupy ludzi lub obszaru.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa ustawa podatkowa ma <w>objąć</w> wszystkich przedsiębiorców.",
                            "target_lang_translations": {
                                "ru": "Новый налоговый закон должен <w>охватить</w> всех предпринимателей."
                            }
                        },
                        {
                            "text": "Firma zdecydowała się <w>objąć</w> to wydarzenie swoim patronatem.",
                            "target_lang_translations": {
                                "ru": "Компания решила <w>взять</w> это событие под свой патронаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "dotyczyć"
                    ],
                    "common_phrases": [
                        "objąć patronatem",
                        "objąć ubezpieczeniem",
                        "objąć programem",
                        "objąć zakazem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Включать что-то в свой состав, касаться определенной группы людей или территории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охватить",
                                "target_lang_sense_clarification": "включить в сферу действия"
                            },
                            {
                                "target_lang_word": "распространиться",
                                "target_lang_sense_clarification": "на кого-то или что-то"
                            },
                            {
                                "target_lang_word": "взять",
                                "target_lang_sense_clarification": "под защиту, контроль или патронаж"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
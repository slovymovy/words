                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола code (писать код, кодировать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "написал код",
                                "target_lang_sense_clarification": "о создании компьютерной программы"
                            },
                            {
                                "target_lang_word": "закодировал",
                                "target_lang_sense_clarification": "преобразовал в код или присвоил код"
                            },
                            {
                                "target_lang_word": "закодированный",
                                "target_lang_sense_clarification": "причастие: имеющий код или шифр"
                            }
                        ]

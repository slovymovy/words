    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6bf83af5-f821-366f-bc1b-4d4b3567c8de",
                    "sense_definition": "Spowodować, że ktoś lub coś znajdzie się w niebezpieczeństwie lub w trudnej sytuacji; wystawić na działanie szkodliwych czynników.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nieodpowiedzialne zachowanie <naraziło> wszystkich uczestników wycieczki na wielkie niebezpieczeństwo.",
                            "target_lang_translations": {
                                "ru": "Его безответственное поведение <подвергло> всех участников экскурсии большой опасности."
                            }
                        },
                        {
                            "text": "Nie chcę <narazić> firmy na dodatkowe koszty związane z tym ryzykownym projektem.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <подвергать> компанию дополнительным расходам, связанным с этим рискованным проектом."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystawić"
                    ],
                    "antonyms": [
                        "ochronić",
                        "zabezpieczyć",
                        "uchronić"
                    ],
                    "common_phrases": [
                        "narazić na niebezpieczeństwo",
                        "narazić na straty",
                        "narazić na szwank",
                        "narazić na śmierć",
                        "narazić zdrowie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, что кто-то или что-то окажется в опасности или в трудной ситуации; подвергнуть воздействию вредных факторов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвергнуть",
                                "target_lang_sense_clarification": "опасности, риску"
                            },
                            {
                                "target_lang_word": "поставить под угрозу",
                                "target_lang_sense_clarification": "безопасность, благополучие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c86dc88-a7ba-37af-a853-9433a1b2137a",
                    "sense_definition": "Sprowadzić na siebie niebezpieczeństwo, kłopoty lub czyjąś niechęć poprzez swoje działanie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Swoim szczerym komentarzem <naraził się> na gniew dyrektora i stracił szansę na awans.",
                            "target_lang_translations": {
                                "ru": "Своим искренним комментарием он <навлек на себя> гнев директора и потерял шанс на повышение."
                            }
                        },
                        {
                            "text": "Jeśli nie zapłacisz mandatu w terminie, <narazisz się> na postępowanie komornicze.",
                            "target_lang_translations": {
                                "ru": "Если ты не оплатишь штраф вовремя, то <подвергнешь себя> исполнительному производству."
                            }
                        }
                    ],
                    "synonyms": [
                        "podpaść"
                    ],
                    "common_phrases": [
                        "narazić się na śmieszność",
                        "narazić się na krytykę",
                        "narazić się na koszty",
                        "narazić się szefowi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Навлечь на себя опасность, неприятности или чье-то недовольство своими действиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навлечь на себя",
                                "target_lang_sense_clarification": "гнев, неприятности"
                            },
                            {
                                "target_lang_word": "подвергнуть себя",
                                "target_lang_sense_clarification": "риску, опасности"
                            },
                            {
                                "target_lang_word": "вызвать недовольство",
                                "target_lang_sense_clarification": "кого-либо (например, начальника)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
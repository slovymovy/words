                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "первый",
                                "target_lang_sense_clarification": "об этаже (в России соответствует 'begane grond')"
                            },
                            {
                                "target_lang_word": "наземный",
                                "target_lang_sense_clarification": "расположенный на уровне земли"
                            }
                        ]

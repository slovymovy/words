            "senses": [
                {
                    "sense_id": "e66c0323-71b9-3b19-9b2f-368f6e2c1b35",
                    "sense_definition": "The pungent, spicy fresh or dried fruit of a capsicum pepper plant, used in cooking to add heat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "food_ingredient",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She chopped a red <w>chili</w> to add some spice to the curry sauce.",
                            "target_lang_translations": {
                                "ru": "Она нарезала красный <w>перец чили</w>, чтобы добавить остроты в соус карри."
                            }
                        },
                        {
                            "text": "Be careful not to touch your eyes after handling a hot <w>chili</w>.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны и не касайтесь глаз после того, как трогали острый <w>чили</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "chili pepper"
                    ],
                    "common_phrases": [
                        "red chili",
                        "green chili",
                        "hot chili",
                        "fresh chili"
                    ],
                    "target_lang_definitions": {
                        "ru": "Острый свежий или сушеный плод стручкового перца, используемый в кулинарии для придания остроты."
                    },
                    "translations": {

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платиновый",
                                "target_lang_sense_clarification": "о музыкальном альбоме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7d2f38e-8e63-3860-8853-f82c89a451f9",
                    "sense_definition": "Of a whitish grey colour, like that of the metal.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is famous for her iconic <w>platinum</w> hair.",
                            "target_lang_translations": {
                                "ru": "Она известна своими культовыми <w>платиновыми</w> волосами."
                            }
                        },
                        {
                            "text": "The sky turned a <w>platinum</w> grey just before the snow started to fall.",
                            "target_lang_translations": {
                                "ru": "Небо стало <w>платиново</w>-серым как раз перед тем, как пошёл снег."
                            }
                        }

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ae3f43d-64db-3027-85f1-401749ef47af",
                    "sense_definition": "Перемещаться внутрь помещения, здания или другого пространства; переступать порог.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_entry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое утро учитель <входит> в класс ровно в девять часов."
                        },
                        {
                            "text": "Пожалуйста, <входите>, дверь не заперта."
                        }
                    ],
                    "antonyms": [
                        "выходить"
                    ],
                    "common_phrases": [
                        "входить в комнату",
                        "входить в дом",
                        "входить без стука",
                        "входить в здание"
                    ]
                },
                {
                    "sense_id": "fc7a36a4-046e-3676-8679-55d3eae251bf",
                    "sense_definition": "Быть составной частью чего-либо, включаться в список или группу.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В стоимость билета <входит> обед и напитки."
                        },
                        {
                            "text": "Этот талантливый игрок <входит> в состав национальной сборной."
                        }
                    ],
                    "antonyms": [
                        "выходить"
                    ],
                    "common_phrases": [
                        "входить в состав",
                        "входить в стоимость",
                        "входить в планы",
                        "входить в историю"
                    ]
                },
                {
                    "sense_id": "24225414-518a-3bb7-b6a6-b63ee5394067",
                    "sense_definition": "Проникать вглубь материала или поверхности (обычно об острых предметах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "penetration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Острый нож легко <входил> в мягкое масло."
                        },
                        {
                            "text": "Гвоздь <входил> в дерево с большим трудом."

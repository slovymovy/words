            "senses": [
                {
                    "sense_id": "f68bcb33-5d72-382f-b8f5-07bbcaa206b5",
                    "sense_definition": "Iemand of iets een naam geven of met een naam aanduiden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "naming_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <noemen> onze nieuwe hond Max.",
                            "target_lang_translations": {
                                "en": "We <call> our new dog Max."
                            }
                        },
                        {
                            "text": "Je mag me gewoon bij mijn voornaam <noemen>.",
                            "target_lang_translations": {
                                "en": "You can just <call> me by my first name."
                            }
                        }
                    ],
                    "synonyms": [
                        "betitelen"
                    ],
                    "common_phrases": [
                        "iemand bij naam noemen",
                        "het beestje bij de naam noemen",
                        "zichzelf noemen"
                    ],
                    "target_lang_definitions": {
                        "en": "To give a name to someone or something, or to address them by a specific name."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "to address or refer to by a name"
                            },
                            {
                                "target_lang_word": "name",
                                "target_lang_sense_clarification": "to give a name to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a06896f3-7809-32f0-9baf-d2bb992a7e80",
                    "sense_definition": "Iets of iemand kort bespreken of als voorbeeld aanhalen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mentioning_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar <noemde> drie belangrijke redenen voor de oorlog.",
                            "target_lang_translations": {
                                "en": "The teacher <mentioned> three important reasons for the war."
                            }
                        },
                        {
                            "text": "Hij wilde tijdens het interview liever geen namen <noemen>.",
                            "target_lang_translations": {
                                "en": "He preferred not to <mention> names during the interview."
                            }
                        }
                    ],
                    "synonyms": [
                        "vermelden",
                        "aanhalen",
                        "opnoemen"
                    ],
                    "antonyms": [
                        "verzwijgen"
                    ],
                    "common_phrases": [
                        "een voorbeeld noemen",
                        "geen namen noemen",
                        "nader te noemen"
                    ],
                    "target_lang_definitions": {
                        "en": "To briefly discuss, mention, or cite something or someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mention",
                                "target_lang_sense_clarification": "to refer to briefly"
                            },
                            {
                                "target_lang_word": "name",
                                "target_lang_sense_clarification": "to specify or identify"
                            },
                            {
                                "target_lang_word": "cite",
                                "target_lang_sense_clarification": "to refer to as an example"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6505f1ec-df67-347c-94a7-82a702f3391f",
                    "sense_definition": "Een bepaalde naam hebben.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "having_name",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hoe <noemt> dat gezellige restaurant op de hoek?",
                            "target_lang_translations": {
                                "en": "What is that cozy restaurant on the corner <called>?"
                            }
                        },
                        {
                            "text": "Onze nieuwe buurman <noemt> blijkbaar Peter.",
                            "target_lang_translations": {
                                "en": "Our new neighbor is apparently <called> Peter."
                            }
                        }
                    ],
                    "synonyms": [

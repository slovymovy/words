{
    "word_family": [
        "zmarszczka",
        "zmarszczyć",
        "przymarszczyć",
        "pomarszczony",
        "marszczenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "071630a4-d75e-3ddc-923a-2fb660e8749a",
                    "sense_definition": "Ściągać skórę na czole lub w okolicach oczu, tworząc bruzdy, zwykle jako wyraz złości, zdziwienia lub głębokiego skupienia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "facial_expressions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel zaczął groźnie <marszczyć> brwi, widząc zachowanie uczniów.",
                            "target_lang_translations": {
                                "ru": "Учитель начал грозно <хмурить> брови, видя поведение учеников."
                            }
                        },
                        {
                            "text": "Nie musisz tak <marszczyć> czoła, to zadanie jest prostsze niż myślisz.",
                            "target_lang_translations": {
                                "ru": "Тебе не нужно так <морщить> лоб, это задание проще, чем ты думаешь."
                            }
                        }
                    ],
                    "synonyms": [
                        "ściągać"
                    ],
                    "common_phrases": [
                        "marszczyć brwi",
                        "marszczyć czoło",
                        "marszczyć nos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стягивать кожу на лбу или вокруг глаз, образуя складки, обычно как выражение злости, удивления или глубокой сосредоточенности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хмурить",
                                "target_lang_sense_clarification": "брови"
                            },
                            {
                                "target_lang_word": "морщить",
                                "target_lang_sense_clarification": "лоб, нос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18b1d892-e9ef-3d66-9a56-773c6c01563a",
                    "sense_definition": "Tracić gładkość, pokrywając się fałdami, bruzdami lub nierównościami (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten tani materiał zaczyna się <marszczyć> już po pierwszym praniu.",
                            "target_lang_translations": {
                                "ru": "Этот дешевый материал начинает <морщиться> уже после первой стирки."
                            }
                        },
                        {
                            "text": "Skóra na dłoniach może się <marszczyć> od długiego przebywania w wodzie.",
                            "target_lang_translations": {
                                "ru": "Кожа на ладонях может <сморщиваться> от долгого пребывания в воде."
                            }
                        }
                    ],
                    "synonyms": [
                        "fałdować się",
                        "ściągać się"
                    ],
                    "common_phrases": [
                        "marszczyć się ze starości",
                        "marszczyć się w praniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Терять гладкость, покрываясь складками, морщинами или неровностями (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морщиться",
                                "target_lang_sense_clarification": "покрываться морщинами"
                            },
                            {
                                "target_lang_word": "сборить",
                                "target_lang_sense_clarification": "ложиться складками (о ткани)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6ef0843-20e1-3125-9013-0e77e90a4fdd",
                    "sense_definition": "Celowo układać tkaninę, papier lub inny materiał w drobne, regularne fałdy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "manipulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krawcowa potrafiła pięknie <marszczyć> jedwab, tworząc efektowne suknie.",
                            "target_lang_translations": {
                                "ru": "Портниха умела красиво <сборить> шелк, создавая эффектные платья."
                            }
                        },
                        {
                            "text": "Musisz delikatnie <marszczyć> bibułę, aby zrobić z niej ozdobny kwiat.",
                            "target_lang_translations": {
                                "ru": "Нужно аккуратно <собирать в складки> папиросную бумагу, чтобы сделать из нее декоративный цветок."
                            }
                        }
                    ],
                    "synonyms": [
                        "karbować"
                    ],
                    "common_phrases": [
                        "marszczyć materiał",
                        "marszczyć tkaninę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренно укладывать ткань, бумагу или другой материал в мелкие, регулярные складки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сборить",
                                "target_lang_sense_clarification": "делать сборки на ткани"
                            },
                            {
                                "target_lang_word": "собирать в складки",
                                "target_lang_sense_clarification": "формировать складки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2254830c-09fd-3a2a-93a6-7769f15d75bc",
                    "sense_definition": "Powodować powstawanie drobnych fal na powierzchni wody.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny wiatr zaczął <marszczyć> taflę jeziora, zapowiadając burzę.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер начал <рябить> гладь озера, предвещая бурю."
                            }
                        },
                        {
                            "text": "Rzucony kamień może <marszczyć> spokojną powierzchnię wody przez długą chwilę.",
                            "target_lang_translations": {
                                "ru": "Брошенный камень может долго <рябить> спокойную поверхность воды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wiatr marszczy wodę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать появление мелких волн на поверхности воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рябить",
                                "target_lang_sense_clarification": "создавать рябь"
                            },
                            {
                                "target_lang_word": "морщить",
                                "target_lang_sense_clarification": "поверхность воды"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
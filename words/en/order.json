{
    "word_family": [
        "disorder",
        "orderly",
        "reorder",
        "ordinal",
        "suborder",
        "coordination"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3438422c-193a-3540-934c-d6a02d074e99",
                    "sense_definition": "A request for some product or service to be made, supplied, or delivered.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "commercial_transaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I would like to place an <w>order</w> for a large pizza.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы сделать <w>заказ</w> на большую пиццу."
                            }
                        },
                        {
                            "text": "The waiter wrote down our drink <w>order</w> on a notepad.",
                            "target_lang_translations": {
                                "ru": "Официант записал наш <w>заказ</w> напитков в блокнот."
                            }
                        }
                    ],
                    "common_phrases": [
                        "place an order",
                        "take an order",
                        "online order"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запрос на изготовление, поставку или доставку какого-либо товара или услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заказ",
                                "target_lang_sense_clarification": "коммерческий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2904413f-12ba-3ff7-ac1a-9f39aed292ef",
                    "sense_definition": "An authoritative instruction or direction to do something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers marched forward to follow the general's <w>orders</w>.",
                            "target_lang_translations": {
                                "ru": "Солдаты маршировали вперед, выполняя <w>приказы</w> генерала."
                            }
                        },
                        {
                            "text": "She gave a strict <w>order</w> that no one should enter the room.",
                            "target_lang_translations": {
                                "ru": "Она отдала строгий <w>приказ</w>, чтобы никто не входил в комнату."
                            }
                        }
                    ],
                    "synonyms": [
                        "command"
                    ],
                    "common_phrases": [
                        "give an order",
                        "follow orders",
                        "under orders"
                    ],
                    "target_lang_definitions": {
                        "ru": "Властное указание или распоряжение сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приказ",
                                "target_lang_sense_clarification": "строгое указание"
                            },
                            {
                                "target_lang_word": "распоряжение",
                                "target_lang_sense_clarification": "указание"
                            },
                            {
                                "target_lang_word": "команда",
                                "target_lang_sense_clarification": "устный приказ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97cf1804-060e-33ad-b68a-4b090bbba4b9",
                    "sense_definition": "The arrangement or disposition of people or things in relation to each other according to a particular sequence or method.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "arrangement_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The books on the shelf are arranged in alphabetical <w>order</w>.",
                            "target_lang_translations": {
                                "ru": "Книги на полке расставлены в алфавитном <w>порядке</w>."
                            }
                        },
                        {
                            "text": "Please list the historical events in chronological <w>order</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, перечислите исторические события в хронологическом <w>порядке</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "disorder"
                    ],
                    "common_phrases": [
                        "in order",
                        "alphabetical order",
                        "chronological order",
                        "reverse order"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположение людей или вещей относительно друг друга в определенной последовательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порядок",
                                "target_lang_sense_clarification": "последовательность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "789a4d2e-5f2d-3be7-9ff4-d00ec6c0bcf8",
                    "sense_definition": "The state of being well arranged, working correctly, or neat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "arrangement_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The elevator is currently out of <w>order</w> and cannot be used.",
                            "target_lang_translations": {
                                "ru": "Лифт в данный момент не в <w>порядке</w> и не может быть использован."
                            }
                        },
                        {
                            "text": "We need to put the house in <w>order</w> before the guests arrive.",
                            "target_lang_translations": {
                                "ru": "Нам нужно привести дом в <w>порядок</w> до прихода гостей."
                            }
                        }
                    ],
                    "antonyms": [
                        "chaos",
                        "disorder"
                    ],
                    "common_phrases": [
                        "out of order",
                        "in order",
                        "in good order"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние исправности, организованности или аккуратности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порядок",
                                "target_lang_sense_clarification": "исправность, аккуратность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8efc7f3f-a11e-357a-b54a-15d7a688854c",
                    "sense_definition": "A state of peace, freedom from confusion, and respect for rules and laws.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_peace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police worked hard to restore <w>order</w> after the riot.",
                            "target_lang_translations": {
                                "ru": "Полиция усердно работала, чтобы восстановить <w>порядок</w> после беспорядков."
                            }
                        },
                        {
                            "text": "The judge called for <w>order</w> in the court.",
                            "target_lang_translations": {
                                "ru": "Судья призвал к <w>порядку</w> в зале суда."
                            }
                        }
                    ],
                    "antonyms": [
                        "chaos",
                        "disorder"
                    ],
                    "common_phrases": [
                        "law and order",
                        "restore order",
                        "public order"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние спокойствия и соблюдения законов и правил в обществе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порядок",
                                "target_lang_sense_clarification": "общественное спокойствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27c7f915-0052-361d-b1cc-7188802dd247",
                    "sense_definition": "A group of people united in a formal way, such as a religious community or an association of knights.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to join a religious <w>order</w> and become a monk.",
                            "target_lang_translations": {
                                "ru": "Он решил вступить в религиозный <w>орден</w> и стать монахом."
                            }
                        },
                        {
                            "text": "The <w>Order</w> of the Garter is the oldest national order of knighthood in the United Kingdom.",
                            "target_lang_translations": {
                                "ru": "<w>Орден</w> Подвязки — старейший национальный рыцарский орден в Соединенном Королевстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "association",
                        "brotherhood",
                        "league",
                        "sisterhood",
                        "society"
                    ],
                    "common_phrases": [
                        "religious order"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, объединенных формальным образом, например, религиозная община или рыцарское братство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "орден",
                                "target_lang_sense_clarification": "организация, братство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92838d82-6d12-3fa9-a274-c5f562a7935a",
                    "sense_definition": "A specific rank, class, or quality of something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rank_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young artist displayed talent of a very high <w>order</w>.",
                            "target_lang_translations": {
                                "ru": "Молодой художник продемонстрировал талант очень высокого <w>уровня</w>."
                            }
                        },
                        {
                            "text": "Issues of this <w>order</w> require immediate attention from the management.",
                            "target_lang_translations": {
                                "ru": "Вопросы такого <w>порядка</w> требуют немедленного внимания руководства."
                            }
                        }
                    ],
                    "synonyms": [
                        "rank",
                        "degree",
                        "sort"
                    ],
                    "common_phrases": [
                        "of the highest order"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный ранг, класс или качество чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "качество, степень"
                            },
                            {
                                "target_lang_word": "порядок",
                                "target_lang_sense_clarification": "масштаб, величина"
                            },
                            {
                                "target_lang_word": "разряд",
                                "target_lang_sense_clarification": "категория"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f00eb2b9-bd60-367e-af9d-cfdec4e8f833",
                    "sense_definition": "A taxonomic rank used in the classification of organisms, ranking below class and above family.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biology_taxonomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Humans belong to the <w>order</w> Primates.",
                            "target_lang_translations": {
                                "ru": "Люди принадлежат к <w>отряду</w> приматов."
                            }
                        },
                        {
                            "text": "This <w>order</w> of insects includes butterflies and moths.",
                            "target_lang_translations": {
                                "ru": "Этот <w>отряд</w> насекомых включает бабочек и мотыльков."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Таксономический ранг в классификации организмов (в зоологии ниже класса и выше семейства)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отряд",
                                "target_lang_sense_clarification": "в биологии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "67bbe26d-aaa2-3dc9-91e6-91c6df2d2946",
                    "sense_definition": "To request a product or service to be made, supplied, or delivered.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "commercial_transaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to <w>order</w> pizza for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>заказать</w> пиццу на ужин сегодня вечером."
                            }
                        },
                        {
                            "text": "You can <w>order</w> almost anything online these days.",
                            "target_lang_translations": {
                                "ru": "В наши дни можно <w>заказать</w> онлайн почти всё что угодно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "order online",
                        "order food"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запрашивать изготовление, поставку или доставку товара или услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заказывать",
                                "target_lang_sense_clarification": "делать заказ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95d4d5fc-8fb2-37eb-9e7d-46b63b0eaa69",
                    "sense_definition": "To give an authoritative instruction to someone to do something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The officer <w>ordered</w> the soldiers to march forward.",
                            "target_lang_translations": {
                                "ru": "Офицер <w>приказал</w> солдатам маршировать вперед."
                            }
                        },
                        {
                            "text": "She <w>ordered</w> him to leave the house immediately.",
                            "target_lang_translations": {
                                "ru": "Она <w>приказала</w> ему немедленно покинуть дом."
                            }
                        }
                    ],
                    "synonyms": [
                        "command"
                    ],
                    "common_phrases": [
                        "order around"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать властное указание кому-либо сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приказывать",
                                "target_lang_sense_clarification": "отдавать приказ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "962ae6a9-9b75-3cbe-b617-1da926cdeed2",
                    "sense_definition": "To arrange or organize things in a particular sequence or pattern.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arrangement_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>order</w> these files alphabetically for easy access.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>упорядочить</w> эти файлы по алфавиту для легкого доступа."
                            }
                        },
                        {
                            "text": "The data was <w>ordered</w> by date to show the trend over time.",
                            "target_lang_translations": {
                                "ru": "Данные были <w>упорядочены</w> по дате, чтобы показать тенденцию с течением времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "sort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Располагать или организовывать вещи в определенной последовательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упорядочивать",
                                "target_lang_sense_clarification": "приводить в систему"
                            },
                            {
                                "target_lang_word": "сортировать",
                                "target_lang_sense_clarification": "распределять по группам"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "738c721c-4186-3944-860d-a7897fd1d7bf",
                    "sense_definition": "The Order of the Arrow, a specific organization.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "specific_organization",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "He was inducted into the <w>Order</w> of the Arrow last summer.",
                            "target_lang_translations": {
                                "ru": "Прошлым летом его приняли в <w>Орден</w> Стрелы."
                            }
                        },
                        {
                            "text": "Members of the <w>Order</w> wear a distinctive sash.",
                            "target_lang_translations": {
                                "ru": "Члены <w>Ордена</w> носят отличительную перевязь."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Орден Стрелы, конкретная организация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Орден",
                                "target_lang_sense_clarification": "название организации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
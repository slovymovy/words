                {
                    "sense_id": "8c5b006e-fcc7-3823-9eca-a4386623b72d",
                    "sense_definition": "Het in veiligheid brengen van personen of zaken die in gevaar verkeren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De duikers waren nauw betrokken bij de <w>redding</w> van de vermiste zwemmer.",
                            "target_lang_translations": {
                                "en": "The divers were closely involved in the <w>rescue</w> of the missing swimmer."
                            }
                        },
                        {
                            "text": "Dankzij de snelle <w>redding</w> door de brandweer is iedereen nu veilig.",
                            "target_lang_translations": {
                                "en": "Thanks to the quick <w>rescue</w> by the fire department, everyone is now safe."
                            }
                        }
                    ],
                    "synonyms": [
                        "bevrijding",
                        "hulpverlening"
                    ],
                    "common_phrases": [
                        "redding brengen",
                        "de redding nabij",
                        "redding van levens"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of bringing people or things in danger to safety."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rescue",
                                "target_lang_sense_clarification": "the act of saving someone from danger"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91f45c88-70d0-35af-a066-35c68359e177",
                    "sense_definition": "Iets of iemand die uitkomst biedt in een noodsituatie; het feit dat men gered wordt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "outcome",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was zijn <w>redding</w> dat hij een stevig zwemvest droeg tijdens de storm.",
                            "target_lang_translations": {
                                "en": "It was his <w>salvation</w> that he wore a sturdy life jacket during the storm."
                            }
                        },
                        {
                            "text": "De onverwachte lening was de <w>redding</w> voor zijn kleine bedrijf.",
                            "target_lang_translations": {
                                "en": "The unexpected loan was the <w>salvation</w> for his small business."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitkomst",
                        "behoud"
                    ],
                    "antonyms": [
                        "ondergang"
                    ],
                    "common_phrases": [
                        "mijn redding zijn",
                        "tot de redding komen"
                    ],
                    "target_lang_definitions": {
                        "en": "Something or someone that offers a solution in an emergency; the fact of being saved."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "salvation",
                                "target_lang_sense_clarification": "a source of help or the state of being saved"
                            },
                            {
                                "target_lang_word": "rescue",
                                "target_lang_sense_clarification": "deliverance from a difficult situation"
                            }
                        ]
                    }
                }
            ]
        }
    ]

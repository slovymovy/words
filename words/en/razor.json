                    ],
                    "common_phrases": [
                        "razor off",
                        "razor cut"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брить или стричь волосы, используя бритву."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брить",
                                "target_lang_sense_clarification": "удалять волосы"
                            },
                            {
                                "target_lang_word": "сбривать",
                                "target_lang_sense_clarification": "полностью удалять"
                            },
                            {
                                "target_lang_word": "подбривать",
                                "target_lang_sense_clarification": "подравнивать края"
                            }
                        ]
                    }
                }
            ]
        },

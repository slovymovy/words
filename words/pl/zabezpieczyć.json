            "senses": [
                {
                    "sense_id": "4f8e7e0e-ce5c-3f03-9341-27b8785cc878",
                    "sense_definition": "Uchronić kogoś lub coś przed zniszczeniem, uszkodzeniem lub utratą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "protection_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy odpowiednio <zabezpieczyć> dach przed nadchodzącą zimą i śniegiem.",
                            "target_lang_translations": {
                                "ru": "Мы должны должным образом <защитить> крышу от предстоящей зимы и снега."
                            }
                        },
                        {
                            "text": "Firma postanowiła lepiej <zabezpieczyć> swoje dane przed atakiem hakerów.",
                            "target_lang_translations": {
                                "ru": "Компания решила лучше <защитить> свои данные от хакерской атаки."
                            }
                        }
                    ],
                    "synonyms": [
                        "ochronić",
                        "uchronić"
                    ],
                    "antonyms": [
                        "narazić",
                        "uszkodzić"
                    ],
                    "common_phrases": [
                        "zabezpieczyć przed wilgocią",
                        "zabezpieczyć tyły",
                        "zabezpieczyć interesy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитить кого-то или что-то от разрушения, повреждения или потери."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защитить",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "обезопасить",
                                "target_lang_sense_clarification": "сделать безопасным"
                            },
                            {
                                "target_lang_word": "предохранить",
                                "target_lang_sense_clarification": "от повреждений, порчи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c88978b5-de24-3d55-92ec-736ee4a36d07",
                    "sense_definition": "Podjąć działania mające na celu ochronę własnej osoby lub interesów przed ryzykiem (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "protection_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyjazdem w góry turyści powinni <zabezpieczyć> się przed nagłą zmianą pogody.",
                            "target_lang_translations": {
                                "ru": "Перед поездкой в горы туристы должны <обезопасить> себя от внезапной смены погоды."
                            }
                        },
                        {
                            "text": "Warto <zabezpieczyć> się finansowo na wypadek niespodziewanej utraty pracy.",
                            "target_lang_translations": {
                                "ru": "Стоит <подстраховаться> финансово на случай неожиданной потери работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "ubezpieczyć się",
                        "ochronić się"
                    ],
                    "antonyms": [
                        "narazić się"
                    ],
                    "common_phrases": [
                        "zabezpieczyć się na przyszłość",
                        "zabezpieczyć się przed inflacją"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять меры для защиты себя или своих интересов от риска."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подстраховаться",
                                "target_lang_sense_clarification": "принять меры предосторожности"
                            },
                            {
                                "target_lang_word": "обезопасить себя",
                                "target_lang_sense_clarification": "защитить себя от риска"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06d6c48d-d35b-3990-8ab8-f8e9a03d539a",
                    "sense_definition": "Oficjalnie przejąć przedmioty do celów dowodowych lub zamknąć dostęp do określonego terenu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "protection_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjanci natychmiast przybyli na miejsce i <zabezpieczyli> ślady przestępstwa.",

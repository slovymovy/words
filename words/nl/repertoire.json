                {
                    "sense_id": "22398eb5-8a40-3b81-8e71-83a0f030e6f9",
                    "sense_definition": "De verzameling stukken (muziek, toneel, etc.) die een uitvoerend kunstenaar of gezelschap heeft ingestudeerd en kan uitvoeren.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De pianist heeft een uitgebreid <w>repertoire</w> van klassieke muziek.",
                            "target_lang_translations": {
                                "ru": "У пианиста обширный <w>репертуар</w> классической музыки."
                            }
                        },
                        {
                            "text": "Dit bekende nummer staat al jaren op hun vaste <w>repertoire</w>.",
                            "target_lang_translations": {
                                "ru": "Этот известный номер уже много лет входит в их постоянный <w>репертуар</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "speellijst",
                        "programma"
                    ],
                    "common_phrases": [
                        "op het repertoire",
                        "breed repertoire",
                        "vast repertoire",
                        "klassiek repertoire"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность пьес, музыкальных произведений или ролей, которые исполнитель или коллектив подготовил и может исполнить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репертуар",
                                "target_lang_sense_clarification": "совокупность исполняемых произведений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cbeb1d0-7d18-306c-ab30-682cfeedda68",
                    "sense_definition": "Een systematische lijst of register van gegevens, boeken of andere items; een repertorium.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bibliothecaris raadpleegde het <w>repertoire</w> om het oude boek te vinden.",
                            "target_lang_translations": {
                                "ru": "Библиотекарь обратился к <w>каталогу</w>, чтобы найти старую книгу."
                            }
                        },
                        {
                            "text": "Dit boekwerk dient als een <w>repertoire</w> van alle bekende plantensoorten in de regio.",
                            "target_lang_translations": {
                                "ru": "Этот труд служит <w>реестром</w> всех известных видов растений в регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "repertorium",
                        "register",
                        "lijst"
                    ],
                    "target_lang_definitions": {
                        "ru": "Систематический список или реестр данных, книг или других элементов; справочник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реестр",

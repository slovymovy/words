    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5cca3d8c-3835-3ddf-985a-ec79a33e74cc",
                    "sense_definition": "Отправить кого-либо или что-либо куда-либо с определённой целью, часто секретной или для выполнения задания.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "send_purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командование решило <заслать> разведчиков в тыл врага для сбора информации.",
                            "target_lang_translations": {
                                "en": "The command decided to <w>send</w> scouts behind enemy lines to gather information."
                            }
                        },
                        {
                            "text": "Мы можем <заслать> к ним своего человека, чтобы узнать их планы.",
                            "target_lang_translations": {
                                "en": "We can <w>plant</w> our own man among them to find out their plans."
                            }
                        }
                    ],
                    "synonyms": [
                        "отправить",
                        "послать",
                        "забросить"
                    ],
                    "antonyms": [
                        "отозвать"
                    ],
                    "common_phrases": [
                        "заслать шпиона",
                        "заслать разведчика",
                        "заслать казачка",
                        "заслать гонца"
                    ],
                    "target_lang_definitions": {
                        "en": "To send someone or something somewhere for a specific purpose, often secretly or to carry out a mission."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to send in",
                                "target_lang_sense_clarification": "to dispatch someone to a specific location for a task"
                            },
                            {
                                "target_lang_word": "to dispatch",
                                "target_lang_sense_clarification": "formal, to send off to a destination"
                            },
                            {
                                "target_lang_word": "to plant",
                                "target_lang_sense_clarification": "to place a spy or agent secretly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d61d523e-2ba8-3078-84f2-a5e51202b07d",
                    "sense_definition": "Послать или отправить по ошибочному адресу, не туда, куда следовало.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "send_wrong",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за ошибки в индексе письмо <заслали> в совершенно другой город.",
                            "target_lang_translations": {
                                "en": "Due to an error in the zip code, the letter was <w>misdirected</w> to a completely different city."
                            }
                        },
                        {
                            "text": "Почта умудрилась <заслать> мою посылку в соседнюю область.",
                            "target_lang_translations": {
                                "en": "The post office managed to <w>misroute</w> my package to the neighboring region."
                            }
                        }
                    ],
                    "synonyms": [
                        "отправить не туда"
                    ],
                    "antonyms": [
                        "доставить верно"
                    ],
                    "common_phrases": [
                        "заслать письмо",
                        "заслать посылку",
                        "заслать груз"
                    ],
                    "target_lang_definitions": {
                        "en": "To send or dispatch to the wrong address or destination by mistake."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to misdirect",
                                "target_lang_sense_clarification": "to send to the wrong place"
                            },
                            {
                                "target_lang_word": "to misroute",
                                "target_lang_sense_clarification": "to send via an incorrect route or to a wrong destination"
                            }
                        ]
                    }
                }
            ]
        }
    ]

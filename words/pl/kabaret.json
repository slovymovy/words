            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3a1027df-1e9c-3bd4-bde7-f84eadc50e63",
                    "sense_definition": "Widowisko rozrywkowe o lekkim repertuarze, składające się ze skeczów, monologów i piosenek, często o charakterze satyrycznym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "entertainment_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo mi się spodobał <w>kabaret</w> przedstawiony wczoraj w telewizji.",
                            "target_lang_translations": {
                                "ru": "Мне очень понравилось <w>кабаре</w>, показанное вчера по телевизору."
                            }
                        },
                        {
                            "text": "W sobotni wieczór chętnie oglądamy dobry <w>kabaret</w> dla relaksu.",
                            "target_lang_translations": {
                                "ru": "В субботний вечер мы охотно смотрим хорошее <w>кабаре</w>, чтобы расслабиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "rewia"
                    ],
                    "common_phrases": [
                        "kabaret satyryczny",
                        "oglądać kabaret",
                        "występ kabaretu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Развлекательное представление с легким репертуаром, состоящее из скетчей, монологов и песен, часто сатирического характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кабаре",
                                "target_lang_sense_clarification": "представление"
                            },
                            {
                                "target_lang_word": "эстрадное представление",
                                "target_lang_sense_clarification": "шоу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "612a5f94-d926-301e-9d47-26e9306b048a",
                    "sense_definition": "Grupa artystów przygotowująca i wystawiająca programy satyryczno-rozrywkowe.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "entertainment_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj do naszej miejscowości przyjechał znany <w>kabaret</w> studencki.",
                            "target_lang_translations": {
                                "ru": "Вчера в наш город приехало известное студенческое <w>кабаре</w>."
                            }
                        },
                        {
                            "text": "Ten młody <w>kabaret</w> zdobył główną nagrodę na festiwalu.",
                            "target_lang_translations": {
                                "ru": "Это молодое <w>кабаре</w> завоевало главный приз на фестивале."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kabaret studencki",
                        "założyć kabaret",
                        "członek kabaretu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа артистов, подготавливающая и исполняющая сатирико-развлекательные программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кабаре",
                                "target_lang_sense_clarification": "коллектив"
                            },
                            {
                                "target_lang_word": "кабаре-группа",

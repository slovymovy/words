            "senses": [
                {
                    "sense_id": "6805998e-91f0-3847-8d69-2128f1b23691",
                    "sense_definition": "A sudden departure of a large number of people.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The war caused a mass <w>exodus</w> of refugees from the region.",
                            "target_lang_translations": {
                                "ru": "Война вызвала массовый <w>исхо́д</w> беженцев из региона."
                            }
                        },
                        {
                            "text": "There has been a rural <w>exodus</w> as young people move to the cities.",
                            "target_lang_translations": {
                                "ru": "Наблюдается <w>отто́к</w> сельского населения, так как молодежь переезжает в города."
                            }
                        }
                    ],
                    "antonyms": [
                        "influx"
                    ],
                    "common_phrases": [
                        "mass exodus",
                        "rural exodus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапный отъезд большого количества людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исхо́д",
                                "target_lang_sense_clarification": "массовый уход"
                            },
                            {
                                "target_lang_word": "ма́ссовый отъе́зд",
                                "target_lang_sense_clarification": "большого количества людей"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "729841d1-47ff-302f-9841-c73105443eeb",
                    "sense_definition": "The departure of the Hebrew slaves from Egypt under the leadership of Moses.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biblical_exodus",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "The story of the <w>Exodus</w> is retold every year during Passover.",
                            "target_lang_translations": {
                                "ru": "История <w>Исхо́да</w> пересказывается каждый год во время Пасхи."
                            }
                        },
                        {
                            "text": "Moses led the <w>Exodus</w> of the Israelites out of Egypt.",
                            "target_lang_translations": {
                                "ru": "Моисей возглавил <w>Исхо́д</w> израильтян из Египта."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уход еврейских рабов из Египта под предводительством Моисея."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Исхо́д",
                                "target_lang_sense_clarification": "библейское событие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "697309f3-9673-3576-a2a4-bbfe15113526",
                    "sense_definition": "The second book of the Old Testament in the Bible, describing the departure of Israelites from Egypt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biblical_exodus",
                    "name_type": "work_of_art_name",
                    "examples": [
                        {
                            "text": "The Ten Commandments are recorded in the book of <w>Exodus</w>.",
                            "target_lang_translations": {
                                "ru": "Десять заповедей записаны в книге <w>Исхо́д</w>."
                            }
                        },
                        {
                            "text": "We are studying <w>Exodus</w> in our religious studies class.",
                            "target_lang_translations": {
                                "ru": "Мы изучаем <w>Исхо́д</w> на уроке религиоведения."
                            }
                        }
                    ],
                    "synonyms": [
                        "Exod."
                    ],
                    "target_lang_definitions": {
                        "ru": "Вторая книга Ветхого Завета, описывающая уход израильтян из Египта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Исхо́д",
                                "target_lang_sense_clarification": "книга Библии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fef683c3-d636-3872-bdc4-01d87323749b",
                    "sense_definition": "To depart from a place in a large group.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd <w>exodused</w> from the stadium after the concert ended.",
                            "target_lang_translations": {
                                "ru": "Толпа <w>ма́ссово поки́нула</w> стадион после окончания концерта."
                            }
                        },
                        {
                            "text": "When the fire alarm rang, everyone <w>exodused</w> the building immediately.",
                            "target_lang_translations": {
                                "ru": "Когда зазвенела пожарная сигнализация, все немедленно <w>поки́нули</w> здание."
                            }
                        }
                    ],
                    "target_lang_definitions": {

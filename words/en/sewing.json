            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2bd8cacc-1216-35da-aa2d-7cd8c2945fae",
                    "sense_definition": "The activity or skill of making, mending, or joining things (such as clothing) using a needle and thread.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother taught me <w>sewing</w> when I was just a child.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка научила меня <w>шитью</w>, когда я была совсем ребёнком."
                            }
                        },
                        {
                            "text": "She bought a new <w>sewing</w> machine to make her own curtains.",
                            "target_lang_translations": {
                                "ru": "Она купила новую <w>швейную</w> машинку, чтобы сшить себе шторы."
                            }
                        }
                    ],
                    "synonyms": [
                        "needlework",
                        "stitching"
                    ],
                    "common_phrases": [
                        "sewing machine",
                        "sewing kit",
                        "sewing needle",
                        "sewing room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность или умение создавать, чинить или соединять вещи (например, одежду) с помощью иглы и нитки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шитьё",
                                "target_lang_sense_clarification": "процесс или занятие"
                            },
                            {
                                "target_lang_word": "пошив",

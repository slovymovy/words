        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "abaf6d6c-14ab-3a55-b2b9-78ff27a6f05a",
                    "sense_definition": "De eigenschap van verlegen zijn; een gevoel van schroom of onzekerheid in het bijzijn van anderen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Haar <w>verlegenheid</w> verdween snel toen ze zich op haar gemak voelde.",
                            "target_lang_translations": {
                                "en": "Her <w>shyness</w> disappeared quickly when she felt at ease.",
                                "ru": "Её <w>застенчивость</w> быстро исчезла, когда она почувствовала себя комфортно."
                            }
                        },
                        {
                            "text": "Hij moest zijn <w>verlegenheid</w> overwinnen om voor de groep te spreken.",
                            "target_lang_translations": {
                                "en": "He had to overcome his <w>shyness</w> to speak in front of the group.",
                                "ru": "Ему пришлось преодолеть свою <w>робость</w>, чтобы выступить перед группой."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedeesdheid"
                    ],
                    "antonyms": [
                        "vrijmoedigheid",
                        "zelfverzekerdheid"
                    ],
                    "common_phrases": [
                        "uit verlegenheid",
                        "grote verlegenheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of being shy; a feeling of diffidence or insecurity in the presence of others.",
                        "ru": "Свойство быть застенчивым; чувство робости или неуверенности в присутствии других."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shyness",
                                "target_lang_sense_clarification": "social anxiety or reserve"
                            },
                            {
                                "target_lang_word": "timidity",
                                "target_lang_sense_clarification": "lack of courage or confidence"
                            },
                            {
                                "target_lang_word": "bashfulness",
                                "target_lang_sense_clarification": "social shyness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "застенчивость",
                                "target_lang_sense_clarification": "черта характера"
                            },
                            {
                                "target_lang_word": "робость",
                                "target_lang_sense_clarification": "чувство неуверенности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4160b0e7-37fe-339f-b058-40a6e7e08513",
                    "sense_definition": "Een toestand van verwarring, ongemak of schaamte door een moeilijke situatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kritische vraag bracht de spreker duidelijk in <w>verlegenheid</w>.",
                            "target_lang_translations": {
                                "en": "The critical question clearly caused the speaker <w>embarrassment</w>.",
                                "ru": "Критический вопрос явно привел оратора в <w>замешательство</w>."
                            }
                        },
                        {
                            "text": "Ik wilde niemand in <w>verlegenheid</w> brengen door erover te beginnen.",
                            "target_lang_translations": {
                                "en": "I did not want to cause anyone <w>embarrassment</w> by bringing it up.",
                                "ru": "Я не хотел никого приводить в <w>смущение</w>, начиная этот разговор."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in verlegenheid brengen",
                        "in verlegenheid zitten"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of confusion, discomfort, or shame caused by a difficult situation.",
                        "ru": "Состояние замешательства, неловкости или стыда, вызванное сложной ситуацией."
                    },
                    "translations": {

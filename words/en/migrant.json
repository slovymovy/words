    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b68cf5c-191c-3db0-a9b4-4158d20e7889",
                    "sense_definition": "A person who moves from one region or country to another, often to find work or better living conditions.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "human_mover",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many <w>migrants</w> arrive in the city every year looking for employment.",
                            "target_lang_translations": {
                                "ru": "Многие <w>мигранты</w> прибывают в город каждый год в поисках работы."
                            }
                        },
                        {
                            "text": "The government has established new laws to protect the rights of <w>migrant</w> workers.",
                            "target_lang_translations": {
                                "ru": "Правительство приняло новые законы для защиты прав рабочих-<w>мигрантов</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "traveler",
                        "mover"
                    ],
                    "antonyms": [
                        "native",
                        "resident"
                    ],
                    "common_phrases": [
                        "migrant worker",
                        "economic migrant",
                        "migrant crisis",
                        "rural migrant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, переезжающий из одного региона или страны в другую, часто в поисках работы или лучших условий жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мигрант",
                                "target_lang_sense_clarification": "человек, меняющий место жительства"
                            },
                            {
                                "target_lang_word": "переселенец",
                                "target_lang_sense_clarification": "тот, кто переселился на новое место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7a720da-72d6-3564-9f8c-13a2d4f29f43",
                    "sense_definition": "An animal, especially a bird, that moves from one region to another according to the seasons.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_mover",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The swallow is a summer <w>migrant</w> that returns to the south when the weather turns cold.",
                            "target_lang_translations": {
                                "ru": "Ласточка — это летний <w>мигрант</w>, который возвращается на юг, когда наступают холода."
                            }
                        },
                        {
                            "text": "Biologists track the paths of these <w>migrants</w> to understand their breeding habits.",
                            "target_lang_translations": {
                                "ru": "Биологи отслеживают пути этих <w>мигрантов</w>, чтобы понять их привычки размножения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "summer migrant",
                        "winter migrant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Животное, особенно птица, которое перемещается из одного региона в другой в зависимости от времени года."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мигрант",
                                "target_lang_sense_clarification": "биологический термин"
                            },
                            {
                                "target_lang_word": "перелётная птица",
                                "target_lang_sense_clarification": "о птицах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8810dbfe-8bd0-3edb-ae90-8423631360a9",
                    "sense_definition": "A type of butterfly belonging to the genus Catopsilia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "butterfly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lemon <w>migrant</w> is a common butterfly species found in parts of Asia.",
                            "target_lang_translations": {
                                "ru": "Лимонный <w>мигрант</w> — распространенный вид бабочек, встречающийся в некоторых частях Азии."
                            }
                        },
                        {
                            "text": "Collectors were excited to find a rare specimen of the mottled <w>migrant</w> in the field.",
                            "target_lang_translations": {
                                "ru": "Коллекционеры были рады найти редкий экземпляр пятнистого <w>мигранта</w> в поле."
                            }
                        }
                    ],
                    "synonyms": [
                        "emigrant"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "zoology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность бабочек, принадлежащих к роду Catopsilia."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "af18e50f-a7b8-3ede-bf4d-480032abce33",
                    "sense_definition": "Relating to or characteristic of migration; moving from place to place.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_moving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farm relies heavily on <w>migrant</w> labor during the busy harvest season.",
                            "target_lang_translations": {
                                "ru": "Ферма сильно зависит от труда <w>мигрантов</w> в разгар сезона сбора урожая."
                            }
                        },
                        {
                            "text": "These <w>migrant</w> tribes travel across the desert with their herds of cattle.",
                            "target_lang_translations": {
                                "ru": "Эти <w>кочевые</w> племена путешествуют по пустыне со своими стадами скота."
                            }
                        }
                    ],
                    "synonyms": [
                        "migratory",
                        "wandering",
                        "roving"
                    ],
                    "antonyms": [
                        "sedentary",
                        "settled"
                    ],
                    "common_phrases": [
                        "migrant labor",
                        "migrant population",
                        "migrant workforce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к миграции или характерный для неё; перемещающийся с места на место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кочевой",
                                "target_lang_sense_clarification": "о племенах, образе жизни"
                            },
                            {
                                "target_lang_word": "мигрирующий",

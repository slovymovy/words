    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5cdc7b58-7e40-331a-afd3-3b89049b86a8",
                    "sense_definition": "To calculate a number or quantity roughly, often based on incomplete information.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "calc_judge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>estimate</w> that the trip will take about four hours if the traffic is light.",
                            "target_lang_translations": {
                                "ru": "Мы <w>оцениваем</w> время в пути примерно в четыре часа, если не будет пробок."
                            }
                        },
                        {
                            "text": "Scientists <w>estimate</w> the island's population to be around one thousand people.",
                            "target_lang_translations": {
                                "ru": "Ученые <w>оценивают</w> население острова примерно в тысячу человек."
                            }
                        }
                    ],
                    "synonyms": [
                        "approximate"
                    ],
                    "common_phrases": [
                        "estimate the cost",
                        "estimate the number",
                        "hard to estimate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приблизительно подсчитывать число или количество, часто основываясь на неполной информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "производить примерный расчёт"
                            },
                            {
                                "target_lang_word": "прикидывать",
                                "target_lang_sense_clarification": "разг. приблизительно считать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5d0ad8c-c6c8-3caf-9dcd-5a1104f82916",
                    "sense_definition": "To judge or form an opinion about the value, quality, or significance of something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "calc_judge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is often difficult to <w>estimate</w> the true impact of such a major decision.",
                            "target_lang_translations": {
                                "ru": "Часто трудно <w>оценить</w> истинное влияние такого важного решения."
                            }
                        },
                        {
                            "text": "She tried to <w>estimate</w> his reaction before telling him the bad news.",
                            "target_lang_translations": {
                                "ru": "Она пыталась <w>оценить</w> его реакцию, прежде чем сообщить плохие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "assess",
                        "appraise"
                    ],
                    "common_phrases": [
                        "estimate the value",
                        "estimate the importance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Судить или составлять мнение о ценности, качестве или значимости чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "судить о качестве или значении"
                            },
                            {
                                "target_lang_word": "расценивать",
                                "target_lang_sense_clarification": "воспринимать определенным образом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "400b7621-1e75-394c-9a05-b88d2a807394",
                    "sense_definition": "A rough calculation or judgment of the value, size, or cost of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "calc_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you give me a rough <w>estimate</w> of how much the repairs will cost?",
                            "target_lang_translations": {
                                "ru": "Можете дать мне грубую <w>оценку</w> того, во сколько обойдется ремонт?"
                            }
                        },
                        {
                            "text": "According to recent <w>estimates</w>, the company has doubled its revenue this year.",
                            "target_lang_translations": {
                                "ru": "Согласно последним <w>оценкам</w>, в этом году компания удвоила свою выручку."
                            }
                        }
                    ],
                    "synonyms": [
                        "estimation"
                    ],
                    "common_phrases": [
                        "rough estimate",
                        "conservative estimate",
                        "make an estimate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приблизительный подсчёт или суждение о ценности, размере или стоимости чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оценка",
                                "target_lang_sense_clarification": "приблизительное суждение"
                            },
                            {
                                "target_lang_word": "подсчёт",
                                "target_lang_sense_clarification": "вычисление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "938d62a3-703b-3d52-8849-6d8d80ba8b97",
                    "sense_definition": "A document or statement specifying how much a specific job is likely to cost.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "calc_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The contractor provided a written <w>estimate</w> for the kitchen renovation project.",
                            "target_lang_translations": {
                                "ru": "Подрядчик предоставил письменную <w>смету</w> на проект ремонта кухни."
                            }
                        },
                        {
                            "text": "We need to get at least three <w>estimates</w> from different builders before we decide.",
                            "target_lang_translations": {
                                "ru": "Нам нужно получить как минимум три <w>сметы</w> от разных строителей, прежде чем мы решим."
                            }
                        }
                    ],
                    "synonyms": [
                        "appraisal"
                    ],
                    "common_phrases": [
                        "written estimate",
                        "get an estimate",
                        "provide an estimate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ или заявление, указывающее вероятную стоимость определенной работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смета",
                                "target_lang_sense_clarification": "документ о предстоящих расходах"
                            },
                            {
                                "target_lang_word": "расчёт",
                                "target_lang_sense_clarification": "калькуляция стоимости"

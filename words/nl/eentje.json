    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ffbee55-a1bc-33b5-b713-3da281d04a8d",
                    "sense_definition": "Verkleinwoord van <w>één</w> of <w>een</w>; verwijst naar één enkel exemplaar van iets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "single_instance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb veel gelezen over die auto, maar ik heb er nog nooit <w>eentje</w> in het echt gezien.",
                            "target_lang_translations": {
                                "en": "I've read a lot about that car, but I've never seen <w>one</w> in real life.",
                                "ru": "Я много читал об этой машине, но вживую ещё ни <w>одной</w> не видел."
                            }
                        },
                        {
                            "text": "Als je nog een koekje wilt, mag je er gerust nog <w>eentje</w> pakken.",
                            "target_lang_translations": {
                                "en": "If you want another cookie, feel free to take <w>one</w>.",
                                "ru": "Если хочешь ещё печенья, можешь смело взять ещё <w>одно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "exemplaar"
                    ],
                    "common_phrases": [
                        "er eentje nemen",
                        "er eentje willen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Diminutive of 'one'; refers to a single instance of something.",
                        "ru": "Уменьшительная форма от \"één\" или \"een\"; обозначает один единственный экземпляр чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "one",
                                "target_lang_sense_clarification": "single instance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "один",
                                "target_lang_sense_clarification": "числительное/местоимение"
                            },
                            {
                                "target_lang_word": "одно",
                                "target_lang_sense_clarification": "средний род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5e7a021-dc38-3e7c-88f1-3b09dce4d7aa",
                    "sense_definition": "Alleen; zonder gezelschap van andere mensen (vooral gebruikt in vaste uitdrukkingen met bezittelijke voornaamwoorden).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "alone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De eigenwijze man wilde niet geholpen worden en deed alles in zijn <w>eentje</w>.",
                            "target_lang_translations": {
                                "en": "The stubborn man didn't want help and did everything on his <w>own</w>.",
                                "ru": "Упрямый мужчина не хотел помощи и делал всё <w>в одиночку</w>."
                            }
                        },
                        {
                            "text": "Ze ging in haar <w>eentje</w> op vakantie om helemaal tot rust te komen.",
                            "target_lang_translations": {
                                "en": "She went on vacation by <w>herself</w> to completely relax.",
                                "ru": "Она поехала в отпуск <w>одна</w>, чтобы полностью отдохнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "alleen",
                        "solo"
                    ],
                    "antonyms": [
                        "samen"
                    ],
                    "common_phrases": [
                        "in je eentje",
                        "in zijn eentje",
                        "in haar eentje"
                    ],
                    "target_lang_definitions": {
                        "en": "Alone; without company (often used in fixed expressions like 'in ... eentje').",
                        "ru": "В одиночку; без компании других людей (обычно используется в устойчивых выражениях с притяжательными местоимениями)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on one's own",
                                "target_lang_sense_clarification": "without help or company"
                            },
                            {
                                "target_lang_word": "by oneself",
                                "target_lang_sense_clarification": "alone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в одиночку",
                                "target_lang_sense_clarification": "самостоятельно"
                            },
                            {
                                "target_lang_word": "один",
                                "target_lang_sense_clarification": "без компании"
                            }
                        ]
                    }
                }
            ]
        }

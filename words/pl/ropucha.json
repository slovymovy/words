            "senses": [
                {
                    "sense_id": "30badd0a-544e-3d5c-a58a-ac1098eb6cca",
                    "sense_definition": "Płaz lądowy o krępej budowie ciała, krótkich kończynach i chropowatej skórze pokrytej brodawkami.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W jaki sposób można odróżnić <w>ropuchę</w> od żaby?",
                            "target_lang_translations": {
                                "ru": "Как можно отличить <w>жабу</w> от лягушки?"
                            }
                        },
                        {
                            "text": "Na ścieżce w lesie siedziała wielka, brązowa <w>ropucha</w>.",
                            "target_lang_translations": {
                                "ru": "На лесной тропинке сидела огромная коричневая <w>жаба</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ropucha szara",
                        "wielka ropucha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наземное земноводное с коренастым телом, короткими конечностями и бугристой кожей, покрытой бородавками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаба",
                                "target_lang_sense_clarification": "земноводное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4fc8529-7007-3510-83ab-649548aeea69",
                    "sense_definition": "Pogardliwe określenie kobiety, która jest postrzegana jako brzydka, niesympatyczna lub złośliwa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "insults",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta wredna <w>ropucha</w> znowu zrobiła nam awanturę o nic.",
                            "target_lang_translations": {
                                "ru": "Эта вредная <w>жаба</w> снова устроила нам скандал на пустом месте."
                            }
                        },
                        {
                            "text": "Nie przejmuj się tym, co mówi ta stara <w>ropucha</w>.",
                            "target_lang_translations": {
                                "ru": "Не обращай внимания на то, что говорит эта старая <w>жаба</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "brzydula"
                    ],
                    "common_phrases": [
                        "stara ropucha",
                        "wredna ropucha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Презрительное название женщины, которую считают некрасивой, неприятной или злобной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаба",
                                "target_lang_sense_clarification": "бранное"
                            },
                            {
                                "target_lang_word": "уродина",
                                "target_lang_sense_clarification": "о внешности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b28e833-5f10-3a45-9c34-001223bde3ff",
                    "sense_definition": "Forma dopełniacza i biernika liczby pojedynczej rzeczownika <w>ropuch</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czarownica zamieniła księcia w obrzydliwego <w>ropucha</w>.",
                            "target_lang_translations": {
                                "ru": "Ведьма превратила принца в отвратительную <w>жабу</w>."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie widziałem tak wielkiego <w>ropucha</w>.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не видел такой огромной <w>жабы</w>."
                            }
                        }
                    ],
                    "traits": [

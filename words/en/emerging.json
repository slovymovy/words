            "senses": [
                {
                    "sense_id": "a6d3ffed-27a5-3e78-8bef-3eae45b4d34b",
                    "sense_definition": "Present participle and gerund of <w>emerge</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sun was <w>emerging</w> from behind the dark clouds.",
                            "target_lang_translations": {
                                "ru": "Солнце <w>выходило</w> из-за темных туч."
                            }
                        },
                        {
                            "text": "New evidence is <w>emerging</w> from the ongoing investigation.",
                            "target_lang_translations": {
                                "ru": "В ходе текущего расследования <w>появляются</w> новые улики."
                            }

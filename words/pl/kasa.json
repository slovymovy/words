                {
                    "sense_id": "1e4080d2-b0f9-3945-9e6b-73fc69d157b7",
                    "sense_definition": "Pancerne pomieszczenie lub szafa służąca do bezpiecznego przechowywania pieniędzy i wartościowych przedmiotów.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "storage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie ważne dokumenty schowaliśmy w <kasie> pancernej.",
                            "target_lang_translations": {
                                "ru": "Все важные документы мы спрятали в <w>сейфе</w>."
                            }
                        },
                        {
                            "text": "Złodzieje bezskutecznie próbowali otworzyć <kasę> banku.",
                            "target_lang_translations": {

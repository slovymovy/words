    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "32222361-1e77-3f71-89d8-bf08a426570b",
                    "sense_definition": "Przypuszczenie, domysł lub prywatny pogląd na dany temat, często niepoparty twardymi dowodami.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hypothesis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam własną <w>teorię</w> na temat tego, dlaczego on nie przyszedł do pracy.",
                            "target_lang_translations": {
                                "ru": "У меня есть собственная <w>теория</w> насчет того, почему он не пришел на работу."
                            }
                        },
                        {
                            "text": "Twoja <w>teoria</w> brzmi ciekawie, ale chyba nie jest prawdziwa.",
                            "target_lang_translations": {
                                "ru": "Твоя <w>теория</w> звучит интересно, но, пожалуй, она неверна."
                            }
                        }
                    ],
                    "synonyms": [
                        "przypuszczenie",
                        "pogląd",
                        "hipoteza"
                    ],
                    "common_phrases": [
                        "mieć teorię",
                        "snuć teorie",
                        "teoria spiskowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej jako synonim domysłu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предположение, догадка или личное мнение по какому-либо вопросу, часто не подкрепленное твердыми доказательствами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "теория",
                                "target_lang_sense_clarification": "догадка, предположение"
                            },
                            {
                                "target_lang_word": "гипотеза",
                                "target_lang_sense_clarification": "научное или логическое предположение"
                            },
                            {
                                "target_lang_word": "предположение",
                                "target_lang_sense_clarification": "допущение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "419b9318-e20e-3762-9d0a-3f99695bdf8d",
                    "sense_definition": "Część egzaminu lub szkolenia sprawdzająca wiedzę z przepisów i zasad, w odróżnieniu od sprawdzianu umiejętności praktycznych.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "exam",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę się dużo uczyć, żeby zdać <w>teorię</w> na prawo jazdy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно много учиться, чтобы сдать <w>теорию</w> на водительские права."
                            }
                        },
                        {
                            "text": "Egzamin składa się z <w>teorii</w> oraz części praktycznej na placu manewrowym.",
                            "target_lang_translations": {
                                "ru": "Экзамен состоит из <w>теории</w> и практической части на автодроме."
                            }
                        }
                    ],
                    "antonyms": [
                        "praktyka"
                    ],
                    "common_phrases": [
                        "zdać teorię",
                        "oblać teorię",
                        "egzamin z teorii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть экзамена или обучения, проверяющая знание правил и принципов, в отличие от проверки практических навыков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "теория",
                                "target_lang_sense_clarification": "теоретическая часть экзамена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af338d60-1d8d-368a-86e7-f97144371b96",
                    "sense_definition": "Spójny system pojęć, definicji i twierdzeń, który wyjaśnia określone zjawiska fizyczne, społeczne lub dziedzinę wiedzy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W szkole uczyliśmy się o <w>teorii</w> ewolucji Darwina.",
                            "target_lang_translations": {
                                "ru": "В школе мы изучали <w>теорию</w> эволюции Дарвина."
                            }
                        },
                        {
                            "text": "Naukowcy wciąż pracują nad nową <w>teorią</w> powstania wszechświata.",
                            "target_lang_translations": {
                                "ru": "Ученые все еще работают над новой <w>теорией</w> возникновения Вселенной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "teoria względności",
                        "teoria ewolucji",
                        "teoria gier",
                        "teoria literatury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связная система понятий, определений и утверждений, объясняющая определенные физические, социальные явления или область знаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "теория",
                                "target_lang_sense_clarification": "научная система знаний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbc52bb2-0cb7-3b8b-89fc-3bae32f54f1d",
                    "sense_definition": "Ogół zasad i założeń teoretycznych danej dziedziny, rozpatrywany w oderwaniu od ich praktycznego zastosowania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <w>teorii</w> ten plan jest idealny, ale w życiu może być różnie.",
                            "target_lang_translations": {
                                "ru": "В <w>теории</w> этот план идеален, но в жизни может быть по-разному."
                            }
                        },
                        {
                            "text": "To tylko czysta <w>teoria</w>, która nie sprawdzi się w naszej firmie.",
                            "target_lang_translations": {
                                "ru": "Это только чистая <w>теория</w>, которая не сработает в нашей фирме."
                            }
                        }
                    ],
                    "antonyms": [
                        "praktyka"
                    ],
                    "common_phrases": [
                        "w teorii",
                        "czysta teoria"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность принципов и теоретических положений данной области, рассматриваемая в отрыве от их практического применения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "теория",
                                "target_lang_sense_clarification": "абстрактное знание, противоположность практике"
                            }
                        ]
                    }
                }
            ]
        }
    ]

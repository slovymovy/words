{
    "word_family": [
        "hazenpad",
        "hazenpeper",
        "hazenlip",
        "hazewind"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8e0e7c8-3a06-3b16-a33f-aeda6fe9c0f4",
                    "sense_definition": "Een zoogdier dat lijkt op een konijn, maar groter is, langere oren heeft en sneller kan rennen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het open veld zagen we plotseling een <w>haas</w> wegrennen.",
                            "target_lang_translations": {
                                "ru": "В открытом поле мы вдруг увидели убегающего <w>зайца</w>."
                            }
                        },
                        {
                            "text": "De <w>haas</w> is veel schuwer dan het konijn en laat zich zelden zien.",
                            "target_lang_translations": {
                                "ru": "<w>Заяц</w> гораздо пугливее кролика и редко показывается на глаза."
                            }
                        }
                    ],
                    "synonyms": [
                        "langoor"
                    ],
                    "common_phrases": [
                        "het hazenpad kiezen",
                        "mijn naam is haas",
                        "zo bang als een haas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Млекопитающее, похожее на кролика, но крупнее, с более длинными ушами и способное быстрее бегать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заяц",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27cc46e4-7e07-3726-b79c-f1bed4df7185",
                    "sense_definition": "Het malste stuk vlees van een dier, specifiek de spier onder de lendenen (bijvoorbeeld van een rund of varken).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij het kerstdiner aten we biefstuk van de <w>haas</w> met rodewijnsaus.",
                            "target_lang_translations": {
                                "ru": "За рождественским ужином мы ели стейк из <w>вырезки</w> с соусом из красного вина."
                            }
                        },
                        {
                            "text": "De slager sneed de <w>haas</w> voorzichtig los van het bot.",
                            "target_lang_translations": {
                                "ru": "Мясник осторожно срезал <w>вырезку</w> с кости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biefstuk van de haas",
                        "varkenshaas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самая нежная часть мяса животного, конкретно мышца под поясницей (например, говяжья или свиная)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырезка",
                                "target_lang_sense_clarification": "часть туши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a83cd787-9e40-33e5-876d-93761f58f464",
                    "sense_definition": "Een hardloper die in een wedstrijd het tempo bepaalt voor andere deelnemers en daarna vaak uitstapt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>haas</w> liep de eerste rondes erg snel om een wereldrecord mogelijk te maken.",
                            "target_lang_translations": {
                                "ru": "<w>Пейсмейкер</w> бежал первые круги очень быстро, чтобы сделать возможным мировой рекорд."
                            }
                        },
                        {
                            "text": "Zonder een goede <w>haas</w> is het lastig om een toptijd te lopen.",
                            "target_lang_translations": {
                                "ru": "Без хорошего <w>пейсмейкера</w> трудно показать лучшее время."
                            }
                        }
                    ],
                    "synonyms": [
                        "tempomaker",
                        "gangmaker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бегун, который задает темп другим участникам в соревновании, а затем часто сходит с дистанции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пейсмейкер",
                                "target_lang_sense_clarification": "спорт"
                            },
                            {
                                "target_lang_word": "заяц",
                                "target_lang_sense_clarification": "спорт, сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "19ba759a-1d48-3041-bb83-1313de540610",
                    "sense_definition": "Een veelvoorkomende achternaam in het Nederlands taalgebied.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Meneer <w>Haas</w> woont al jaren in dit dorp.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Хаас</w> живет в этой деревне уже много лет."
                            }
                        },
                        {
                            "text": "Ik heb gisteren een afspraak gemaakt met mevrouw <w>Haas</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера я договорился о встрече с госпожой <w>Хаас</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенная фамилия в нидерландском языковом пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хаас",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a95f418-9890-301b-808d-1f320ce02cfc",
                    "sense_definition": "Een sterrenbeeld aan de zuidelijke hemel, gelegen onder het sterrenbeeld Orion.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "astronomy",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "In de winter kun je het sterrenbeeld <w>Haas</w> goed waarnemen.",
                            "target_lang_translations": {
                                "ru": "Зимой можно хорошо разглядеть созвездие <w>Заяц</w>."
                            }
                        },
                        {
                            "text": "Astronomen bestuderen de sterren in de <w>Haas</w>.",
                            "target_lang_translations": {
                                "ru": "Астрономы изучают звезды в созвездии <w>Заяц</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Lep",
                        "Leps"
                    ],
                    "target_lang_definitions": {
                        "ru": "Созвездие в южном полушарии неба, расположенное под созвездием Ориона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Заяц",
                                "target_lang_sense_clarification": "созвездие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        "ru": [
                            {
                                "target_lang_word": "шарик",
                                "target_lang_sense_clarification": "мороженого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67bdb8f4-0f1b-3506-acfc-184d9736e3cf",
                    "sense_definition": "Een volledig rond voorwerp of een geometrische vorm, zoals een bal.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aarde is geen platte schijf, maar een <w>bol</w>.",

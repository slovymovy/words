    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d0518fe-8f70-3604-b33b-344fca6692c2",
                    "sense_definition": "Usunąć brud, kurz, plamy lub inne zanieczyszczenia z powierzchni czegoś, sprawiając, że staje się to czyste.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę dokładnie <wyczyścić> buty przed dzisiejszym wyjściem.",
                            "target_lang_translations": {
                                "ru": "Мне нужно тщательно <почистить> ботинки перед сегодняшним выходом."
                            }
                        },
                        {
                            "text": "Ojciec poprosił mnie, żebym <wyczyścił> samochód w sobotę.",
                            "target_lang_translations": {
                                "ru": "Отец попросил меня <почистить> машину в субботу."
                            }
                        }
                    ],
                    "synonyms": [
                        "umyć",
                        "oczyścić",
                        "oporządzić"
                    ],
                    "antonyms": [
                        "ubrudzić",
                        "zanieczyścić",
                        "poplamić"
                    ],
                    "common_phrases": [
                        "wyczyścić buty",
                        "wyczyścić zęby",
                        "wyczyścić do czysta",
                        "wyczyścić na błysk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалить грязь, пыль, пятна или другие загрязнения с поверхности чего-либо, делая это чистым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почистить",
                                "target_lang_sense_clarification": "удалить грязь (напр. с обуви, одежды, зубов)"
                            },
                            {
                                "target_lang_word": "вычистить",
                                "target_lang_sense_clarification": "тщательно очистить"
                            },
                            {
                                "target_lang_word": "очистить",
                                "target_lang_sense_clarification": "сделать чистым, убрав налет или грязь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b05eb623-e288-343f-989f-81d3773e8997",
                    "sense_definition": "Opróżnić coś całkowicie z zawartości; usunąć wszystko, co znajduje się w środku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emptying_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodzieje zdołali <wyczyścić> sejf w ciągu kilku minut.",
                            "target_lang_translations": {
                                "ru": "Воры сумели <обчистить> сейф за несколько минут."
                            }
                        },
                        {
                            "text": "Musimy <wyczyścić> piwnicę ze starych i niepotrzebnych rzeczy.",
                            "target_lang_translations": {
                                "ru": "Мы должны <вычистить> подвал от старых и ненужных вещей."
                            }
                        }
                    ],
                    "synonyms": [
                        "opróżnić"
                    ],
                    "antonyms": [
                        "napełnić",
                        "zapełnić"
                    ],
                    "common_phrases": [
                        "wyczyścić konto",
                        "wyczyścić magazyn",
                        "wyczyścić do zera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью освободить что-либо от содержимого; удалить всё, что находится внутри."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обчистить",
                                "target_lang_sense_clarification": "украсть всё содержимое (разг.)"
                            },
                            {
                                "target_lang_word": "вычистить",
                                "target_lang_sense_clarification": "полностью освободить от содержимого"
                            },
                            {
                                "target_lang_word": "очистить",
                                "target_lang_sense_clarification": "освободить пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d939638e-4dc9-308b-93f3-e51507f4ed3a",
                    "sense_definition": "Usunąć brud ze swojego ciała lub ubrania; doprowadzić się do porządku.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cleaning_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko wróciło z podwórka brudne i musiało się <wyczyścić>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок вернулся со двора грязным, и ему пришлось <почиститься>."
                            }
                        },
                        {
                            "text": "Ubrudziłeś się błotem, idź się <wyczyścić> do łazienki.",
                            "target_lang_translations": {
                                "ru": "Ты испачкался грязью, иди <почистись> в ванную."
                            }
                        }
                    ],
                    "synonyms": [
                        "oczyścić się",
                        "umyć się"
                    ],
                    "antonyms": [
                        "ubrudzić się"
                    ],
                    "common_phrases": [
                        "wyczyścić się z błota"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалить грязь со своего тела или одежды; привести себя в порядок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почиститься",
                                "target_lang_sense_clarification": "привести свою одежду или себя в порядок"
                            },
                            {
                                "target_lang_word": "отчиститься",
                                "target_lang_sense_clarification": "удалить с себя грязь или пятна"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
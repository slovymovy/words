{
    "word_family": [
        "magnificence",
        "magnificently"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ee6f068a-0ac8-3444-8df5-f9c5651ca5aa",
                    "sense_definition": "Grand, elegant, or splendid in appearance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visual_grandeur",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king lived in a <w>magnificent</w> palace filled with gold.",
                            "target_lang_translations": {
                                "ru": "Король жил в <w>роскошном</w> дворце, наполненном золотом."
                            }
                        },
                        {
                            "text": "We stopped to admire the <w>magnificent</w> view of the mountains.",
                            "target_lang_translations": {
                                "ru": "Мы остановились, чтобы полюбоваться <w>великолепным</w> видом на горы."
                            }
                        }
                    ],
                    "synonyms": [
                        "splendid",
                        "grand",
                        "majestic",
                        "impressive",
                        "spectacular"
                    ],
                    "antonyms": [
                        "ugly",
                        "modest",
                        "plain",
                        "unimpressive"
                    ],
                    "common_phrases": [
                        "magnificent view",
                        "magnificent building",
                        "magnificent sight",
                        "magnificent scenery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грандиозный, элегантный или роскошный по внешнему виду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "великолепный",
                                "target_lang_sense_clarification": "общий термин для описания красоты и величия"
                            },
                            {
                                "target_lang_word": "роскошный",
                                "target_lang_sense_clarification": "богатый, пышный (о дворцах, убранстве)"
                            },
                            {
                                "target_lang_word": "величественный",
                                "target_lang_sense_clarification": "внушающий почтение своим видом (о горах, зданиях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91d03734-9c8f-3a81-af77-42b8b0f44bd5",
                    "sense_definition": "Exceptional for its kind; extremely good or excellent.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_excellence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She gave a <w>magnificent</w> performance in the school play.",
                            "target_lang_translations": {
                                "ru": "Она показала <w>блистательную</w> игру в школьном спектакле."
                            }
                        },
                        {
                            "text": "The team did a <w>magnificent</w> job winning the championship.",
                            "target_lang_translations": {
                                "ru": "Команда проделала <w>великолепную</w> работу, выиграв чемпионат."
                            }
                        }
                    ],
                    "synonyms": [
                        "excellent",
                        "superb",
                        "outstanding",
                        "wonderful",
                        "brilliant"
                    ],
                    "antonyms": [
                        "terrible",
                        "poor",
                        "awful",
                        "dreadful"
                    ],
                    "common_phrases": [
                        "magnificent performance",
                        "magnificent job",
                        "magnificent achievement",
                        "magnificent victory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исключительный по качеству; чрезвычайно хороший или превосходный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "превосходный",
                                "target_lang_sense_clarification": "очень высокого качества"
                            },
                            {
                                "target_lang_word": "блистательный",
                                "target_lang_sense_clarification": "выдающийся, впечатляющий (о выступлении, успехе)"
                            },
                            {
                                "target_lang_word": "великолепный",
                                "target_lang_sense_clarification": "очень хороший, отличный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d903716a-bb6e-37f5-af1b-406f2c5de825",
                    "sense_definition": "Grand or noble in action or character.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noble_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>magnificent</w> generosity helped thousands of people in need.",
                            "target_lang_translations": {
                                "ru": "Его <w>благородная</w> щедрость помогла тысячам нуждающихся."
                            }
                        },
                        {
                            "text": "It was a <w>magnificent</w> gesture of peace between the two nations.",
                            "target_lang_translations": {
                                "ru": "Это был <w>величественный</w> жест мира между двумя нациями."
                            }
                        }
                    ],
                    "synonyms": [
                        "noble",
                        "sublime",
                        "exalted"
                    ],
                    "antonyms": [
                        "ignoble",
                        "base",
                        "mean"
                    ],
                    "common_phrases": [
                        "magnificent gesture",
                        "magnificent spirit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвышенный или благородный в поступках или характере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благородный",
                                "target_lang_sense_clarification": "высоконравственный, честный"
                            },
                            {
                                "target_lang_word": "величественный",
                                "target_lang_sense_clarification": "полный достоинства и величия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0addf57a-61bc-3754-bfea-5e897ce445cc",
                    "sense_definition": "Een instrument dat de tijd aanwijst",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "timepiece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kijk even op de <w>klok</w> om te zien hoe laat het is.",
                            "target_lang_translations": {
                                "en": "I'm just checking the <w>clock</w> to see what time it is.",
                                "ru": "Я быстро взгляну на <w>часы</w>, чтобы узнать, который час."
                            }
                        },
                        {
                            "text": "De oude <w>klok</w> in de woonkamer slaat elk half uur.",
                            "target_lang_translations": {
                                "en": "The old <w>clock</w> in the living room strikes every half hour.",
                                "ru": "Старые <w>часы</w> в гостиной бьют каждые полчаса."
                            }
                        }
                    ],
                    "synonyms": [
                        "uurwerk"
                    ],
                    "common_phrases": [
                        "op de klok kijken",
                        "de klok rond"
                    ],
                    "target_lang_definitions": {
                        "en": "An instrument that measures and indicates the time",
                        "ru": "Прибор, который показывает время"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clock",
                                "target_lang_sense_clarification": "device for telling time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "часы",
                                "target_lang_sense_clarification": "прибор для измерения времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f077b574-9d90-3ed9-973a-1f23c67c2e87",
                    "sense_definition": "Een draagbaar uurwerk, meestal om de pols gedragen",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "timepiece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij keek zenuwachtig op zijn <w>klok</w> tijdens het wachten.",
                            "target_lang_translations": {
                                "en": "He looked nervously at his <w>watch</w> while waiting.",
                                "ru": "Он нервно смотрел на свои <w>часы</w> во время ожидания."
                            }
                        },
                        {
                            "text": "Mijn nieuwe <w>klok</w> is waterdicht en digitaal.",
                            "target_lang_translations": {
                                "en": "My new <w>watch</w> is waterproof and digital.",
                                "ru": "Мои новые <w>часы</w> водонепроницаемые и цифровые."
                            }
                        }
                    ],
                    "synonyms": [
                        "horloge"
                    ],
                    "common_phrases": [
                        "een dure klok"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used for 'horloge'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A portable timepiece, usually worn on the wrist",
                        "ru": "Портативные часы, которые обычно носят на запястье"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "watch",
                                "target_lang_sense_clarification": "wrist-worn timepiece"
                            }

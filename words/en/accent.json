{
    "word_family": [
        "accentuate",
        "accentuation",
        "accentual"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "05afec40-4273-3b63-b4c2-7bc2fa0f9859",
                    "sense_definition": "A distinctive way of pronouncing a language, especially one associated with a particular country, area, or social class.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pronunciation_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He speaks English with a strong German <w>accent</w>.",
                            "target_lang_translations": {
                                "ru": "Он говорит по-английски с сильным немецким <w>акцентом</w>."
                            }
                        },
                        {
                            "text": "I could not understand him very well because of his thick <w>accent</w>.",
                            "target_lang_translations": {
                                "ru": "Я плохо его понимал из-за его сильного <w>акцента</w>."
                            }
                        },
                        {
                            "text": "She has a slight American <w>accent</w> even though she lives in London.",
                            "target_lang_translations": {
                                "ru": "У неё легкий американский <w>акцент</w>, хотя она живет в Лондоне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "foreign accent",
                        "strong accent",
                        "thick accent",
                        "slight accent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характерная манера произношения, свойственная жителям определенной местности, страны или социальной группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акцент",
                                "target_lang_sense_clarification": "особенности произношения"
                            },
                            {
                                "target_lang_word": "выговор",
                                "target_lang_sense_clarification": "манера произношения, говор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ede1945-bf4c-3601-a85f-16d7a8da872f",
                    "sense_definition": "The emphasis placed on a particular syllable in a word.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "linguistic_stress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the word \"banana\", the <w>accent</w> falls on the second syllable.",
                            "target_lang_translations": {
                                "ru": "В слове «banana» <w>ударение</w> падает на второй слог."
                            }
                        },
                        {
                            "text": "You should place the <w>accent</w> on the first part of the word.",
                            "target_lang_translations": {
                                "ru": "Вам следует ставить <w>ударение</w> на первую часть слова."
                            }
                        }
                    ],
                    "synonyms": [
                        "stress",
                        "stress"
                    ],
                    "common_phrases": [
                        "place the accent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделение голосом определенного слога в слове."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ударение",
                                "target_lang_sense_clarification": "выделение слога"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4654d610-9b09-35cb-9fc7-e2b413d5006b",
                    "sense_definition": "A mark written above or below a letter to indicate how it is pronounced.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "linguistic_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The word \"café\" is written with an acute <w>accent</w> on the final letter.",
                            "target_lang_translations": {
                                "ru": "Слово «café» пишется с острым <w>акцентом</w> (знаком ударения) над последней буквой."
                            }
                        },
                        {
                            "text": "French uses several different <w>accents</w> in writing to change the sound of vowels.",
                            "target_lang_translations": {
                                "ru": "Во французском языке на письме используется несколько разных <w>акцентов</w> для изменения звучания гласных."
                            }
                        }
                    ],
                    "common_phrases": [
                        "acute accent",
                        "grave accent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак над буквой или под ней, указывающий на то, как она произносится."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знак ударения",
                                "target_lang_sense_clarification": "графический знак"
                            },
                            {
                                "target_lang_word": "акцент",
                                "target_lang_sense_clarification": "диакритический знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "173d1d92-98ae-3295-94ed-0b53487c95ba",
                    "sense_definition": "Special importance or attention given to something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "general_emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this course, the <w>accent</w> is on practical skills rather than theory.",
                            "target_lang_translations": {
                                "ru": "В этом курсе <w>акцент</w> делается на практических навыках, а не на теории."
                            }
                        },
                        {
                            "text": "The hotel puts the <w>accent</w> on comfort and luxury for its guests.",
                            "target_lang_translations": {
                                "ru": "Отель делает <w>акцент</w> на комфорте и роскоши для своих гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "stress",
                        "stress"
                    ],
                    "common_phrases": [
                        "accent on something"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особое внимание или важность, придаваемые чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акцент",
                                "target_lang_sense_clarification": "упор, внимание"
                            },
                            {
                                "target_lang_word": "ударение",
                                "target_lang_sense_clarification": "смысловое выделение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "923c2078-f0c7-3816-a476-fd03f44311a2",
                    "sense_definition": "A detail, such as a color or object, that contrasts with its surroundings to create a special effect.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "artistic_detail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She chose red cushions as an <w>accent</w> for the grey sofa.",
                            "target_lang_translations": {
                                "ru": "Она выбрала красные подушки в качестве <w>акцента</w> для серого дивана."
                            }
                        },
                        {
                            "text": "Use a bright <w>accent</w> color to make the design look more lively.",
                            "target_lang_translations": {
                                "ru": "Используйте яркий <w>акцентный</w> цвет, чтобы дизайн выглядел живее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "accent color",
                        "accent piece",
                        "accent wall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деталь (например, цвет или предмет), которая контрастирует с окружением для создания особого эффекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акцент",
                                "target_lang_sense_clarification": "выделяющаяся деталь в дизайне"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "01ae524c-33d5-3b59-a4d1-2e016868f2b4",
                    "sense_definition": "To make a feature or detail more noticeable or prominent.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_emphasize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The blue dress <w>accented</w> her eyes perfectly.",
                            "target_lang_translations": {
                                "ru": "Голубое платье идеально <w>подчёркивало</w> её глаза."
                            }
                        },
                        {
                            "text": "Soft lighting was used to <w>accent</w> the architectural features of the building.",
                            "target_lang_translations": {
                                "ru": "Мягкое освещение использовалось, чтобы <w>подчеркнуть</w> архитектурные особенности здания."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать особенность или деталь более заметной или выразительной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подчёркивать",
                                "target_lang_sense_clarification": "выделять"
                            },
                            {
                                "target_lang_word": "акцентировать",
                                "target_lang_sense_clarification": "делать акцент"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9e09997c-a833-31b9-aa9b-a51702571feb",
                    "sense_definition": "The particular point in time when something happens, especially regarding whether it is chosen well or poorly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "timing_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your <w>timing</w> is perfect because we just started serving dinner.",
                            "target_lang_translations": {
                                "ru": "Ты выбрал идеальный <w>момент</w>, потому что мы только что начали подавать ужин."
                            }
                        },
                        {
                            "text": "It was just bad <w>timing</w> that the store closed right as we arrived.",
                            "target_lang_translations": {
                                "ru": "Нам просто не повезло с <w>выбором времени</w>: магазин закрылся прямо когда мы приехали."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheduling"
                    ],
                    "common_phrases": [
                        "perfect timing",
                        "bad timing",
                        "good timing",
                        "immaculate timing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный момент времени, когда что-то происходит, особенно с точки зрения удачности или неудачности выбора этого момента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбор момента",
                                "target_lang_sense_clarification": "действие по определению подходящего времени"
                            },
                            {
                                "target_lang_word": "расчёт времени",
                                "target_lang_sense_clarification": "планирование времени события"
                            },
                            {
                                "target_lang_word": "своевременность",
                                "target_lang_sense_clarification": "совершение действия в нужный момент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ff2934e-6168-325f-bbb5-ff73606da6e7",
                    "sense_definition": "The skill or regulation of doing something at exactly the right speed or moment to achieve the best effect.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "timing_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actor used excellent comedic <w>timing</w> to make the audience laugh at the simple joke.",
                            "target_lang_translations": {
                                "ru": "Актёр использовал превосходное комическое <w>чутьё</w>, чтобы рассмешить аудиторию простой шуткой."
                            }
                        },
                        {
                            "text": "A good tennis player relies on split-second <w>timing</w> to return a fast serve.",
                            "target_lang_translations": {
                                "ru": "Хороший теннисист полагается на мгновенный <w>расчёт</w>, чтобы отбить быструю подачу."
                            }
                        }
                    ],
                    "synonyms": [
                        "pacing",
                        "rhythm",
                        "tempo"
                    ],
                    "common_phrases": [
                        "comedic timing",
                        "sense of timing",
                        "split-second timing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Навык или умение совершать действия с точно рассчитанной скоростью или в нужный момент для достижения наилучшего эффекта."
                    },
                    "translations": {
                        "ru": [
                            {

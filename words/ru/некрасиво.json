    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2e843a9c-2ec2-3785-9c80-bed89a69ae8f",
                    "sense_definition": "Лишено красоты, внешней привлекательности; безобразно.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой старой шляпе ты выглядишь <w>некрасиво</w>.",
                            "target_lang_translations": {
                                "en": "You look <w>unattractive</w> in that old hat."
                            }
                        },
                        {
                            "text": "Он пишет очень <w>некрасиво</w>, я не могу разобрать ни слова.",
                            "target_lang_translations": {
                                "en": "He writes very <w>messily</w>, I can't make out a single word."
                            }
                        }
                    ],
                    "synonyms": [
                        "безобразно",
                        "уродливо"
                    ],
                    "antonyms": [
                        "красиво"
                    ],
                    "common_phrases": [
                        "выглядеть некрасиво",
                        "одеваться некрасиво",
                        "написано некрасиво"
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner that lacks beauty or attractiveness; in an ugly or messy way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unattractively",
                                "target_lang_sense_clarification": "regarding appearance"
                            },
                            {
                                "target_lang_word": "badly",
                                "target_lang_sense_clarification": "regarding quality or execution"
                            },
                            {
                                "target_lang_word": "messily",
                                "target_lang_sense_clarification": "regarding handwriting or organization"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fb12da1-d32b-365b-9fd6-6c5dd354f565",
                    "sense_definition": "Непорядочно, недостойно, нарушая принятые правила поведения.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты поступил очень <w>некрасиво</w>, когда не помог своему другу.",
                            "target_lang_translations": {
                                "en": "You behaved very <w>badly</w> when you didn't help your friend."
                            }
                        },
                        {
                            "text": "Перебивать старших во время разговора — это <w>некрасиво</w>.",
                            "target_lang_translations": {
                                "en": "Interrupting elders during a conversation is <w>rude</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "непорядочно",
                        "стыдно",
                        "дурно"
                    ],
                    "antonyms": [
                        "красиво"
                    ],
                    "common_phrases": [
                        "поступить некрасиво",
                        "вести себя некрасиво",
                        "это некрасиво"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that is rude, dishonorable, or socially unacceptable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "badly",
                                "target_lang_sense_clarification": "referring to behavior"
                            },
                            {
                                "target_lang_word": "not nice",
                                "target_lang_sense_clarification": "social judgment"
                            },
                            {
                                "target_lang_word": "wrong",
                                "target_lang_sense_clarification": "moral judgment"
                            },
                            {
                                "target_lang_word": "rude",
                                "target_lang_sense_clarification": "breach of etiquette"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7d088af4-fd16-351b-bc4a-cb73e0530caa",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>некрасивый</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её лицо было <w>некрасиво</w>, но глаза сияли добротой.",
                            "target_lang_translations": {
                                "en": "Her face was <w>plain</w>, but her eyes shone with kindness."
                            }
                        },
                        {
                            "text": "Это здание <w>некрасиво</w> снаружи, но очень удобно внутри.",
                            "target_lang_translations": {
                                "en": "This building is <w>ugly</w> on the outside, but very comfortable inside."
                            }
                        }
                    ],

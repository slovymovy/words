{
    "word_family": [
        "odstępować",
        "odstęp",
        "odstępstwo",
        "odstępca",
        "nieodstępny"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "91492579-dca0-3bfe-9375-7fe9bc5b3886",
                    "sense_definition": "Przekazać komuś coś, co do nas należy, lub zrezygnować z czegoś na czyjąś korzyść (np. z miejsca, prawa lub własności).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transfer_rights",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody chłopak postanowił <odstąpić> miejsce starszej pani w autobusie.",
                            "target_lang_translations": {
                                "ru": "Молодой парень решил <уступить> место пожилой женщине в автобусе."
                            }
                        },
                        {
                            "text": "Czy mógłbyś mi <odstąpić> swoją kolejkę do lekarza, ponieważ bardzo się spieszę?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы <уступить> мне свою очередь к врачу, так как я очень спешу?"
                            }
                        }
                    ],
                    "synonyms": [
                        "oddać",
                        "przekazać",
                        "ustąpić"
                    ],
                    "antonyms": [
                        "zachować",
                        "zatrzymać",
                        "zajmować"
                    ],
                    "common_phrases": [
                        "odstąpić miejsce",
                        "odstąpić prawo",
                        "odstąpić pierwszeństwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать кому-то что-то свое или отказаться от чего-то в чью-то пользу (например, от места, права или собственности)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уступить",
                                "target_lang_sense_clarification": "отдать свое место или право"
                            },
                            {
                                "target_lang_word": "передать",
                                "target_lang_sense_clarification": "отдать во владение другому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90aaed60-80dd-374c-b6a4-8225bc57c897",
                    "sense_definition": "Odsunąć się od kogoś lub czegoś na pewną odległość; zrobić krok w tył.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <odstąpić> od krawędzi peronu dla własnego bezpieczeństwa.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <отойдите> от края платформы ради собственной безопасности."
                            }
                        },
                        {
                            "text": "Policjant nakazał gapiom natychmiast <odstąpić> od miejsca wypadku.",
                            "target_lang_translations": {
                                "ru": "Полицейский приказал зевакам немедленно <отойти> от места происшествия."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsunąć się",
                        "cofnąć się"
                    ],
                    "antonyms": [
                        "przybliżyć się",
                        "podejść"
                    ],
                    "common_phrases": [
                        "odstąpić od okna",
                        "odstąpić na krok",
                        "odstąpić od drzwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отойти от кого-то или чего-то на определенное расстояние; сделать шаг назад."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отойти",
                                "target_lang_sense_clarification": "переместиться на расстояние от объекта"
                            },
                            {
                                "target_lang_word": "отступить",
                                "target_lang_sense_clarification": "сделать шаг назад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "656a71ec-b165-3592-8b0c-f2b2561827a3",
                    "sense_definition": "Zrezygnować z wykonania jakiejś czynności, planu, roszczenia lub udziału w czymś (często w kontekście formalnym).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_withdrawal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klient ma prawo <odstąpić> od umowy w ciągu czternastu dni bez podania przyczyny.",
                            "target_lang_translations": {
                                "ru": "Клиент имеет право <отказаться> от договора в течение четырнадцати дней без объяснения причин."
                            }
                        },
                        {
                            "text": "Prokurator zdecydował się <odstąpić> od oskarżenia z powodu braku dowodów.",
                            "target_lang_translations": {
                                "ru": "Прокурор решил <отказаться> от обвинения из-за отсутствия улик."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrezygnować",
                        "wycofać się",
                        "poniechać"
                    ],
                    "antonyms": [
                        "podtrzymać",
                        "kontynuować"
                    ],
                    "common_phrases": [
                        "odstąpić od umowy",
                        "odstąpić od zamiaru",
                        "odstąpić od wymierzenia kary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказаться от выполнения какого-либо действия, плана, требования или участия в чем-либо (часто в официальном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказаться",
                                "target_lang_sense_clarification": "прекратить участие или действие (напр. от договора)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9b49cd7-bc82-3e68-953e-b49fdf255be2",
                    "sense_definition": "Sprzedać komuś coś, zazwyczaj rzecz używaną lub prawo do czegoś, często po okazyjnej cenie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transfer_rights",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mogę ci <odstąpić> ten bilet do kina za połowę ceny, bo i tak nie mogę iść.",
                            "target_lang_translations": {
                                "ru": "Я могу <уступить> тебе этот билет в кино за полцены, так как все равно не смогу пойти."
                            }
                        },
                        {
                            "text": "Sąsiad chce nam tanio <odstąpić> działkę rekreacyjną pod miastem.",
                            "target_lang_translations": {
                                "ru": "Сосед хочет дешево <уступить> нам дачный участок за городом."
                            }
                        }
                    ],
                    "synonyms": [
                        "odsprzedać",
                        "sprzedać"
                    ],
                    "antonyms": [
                        "kupić",
                        "nabyć"
                    ],
                    "common_phrases": [
                        "odstąpić bilet",
                        "odstąpić mieszkanie",
                        "tanio odstąpić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продать кому-то что-то (обычно подержанную вещь или право), часто по выгодной цене или по договоренности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уступить",
                                "target_lang_sense_clarification": "продать или отдать по сниженной цене"
                            },
                            {
                                "target_lang_word": "перепродать",
                                "target_lang_sense_clarification": "продать купленное ранее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34d60014-51de-3083-9a50-73ed61887b12",
                    "sense_definition": "Przestać przestrzegać ustalonych zasad, norm, wiary lub tradycji; sprzeniewierzyć się czemuś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_withdrawal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nigdy nie powinniśmy <odstąpić> od naszych zasad moralnych, nawet w trudnych chwilach.",
                            "target_lang_translations": {
                                "ru": "Мы никогда не должны <отступать> от наших моральных принципов, даже в трудные моменты."
                            }
                        },
                        {
                            "text": "W dawnych czasach karano tych, którzy chcieli <odstąpić> od wiary przodków.",
                            "target_lang_translations": {
                                "ru": "В давние времена наказывали тех, кто хотел <отступить> от веры предков."
                            }
                        }
                    ],
                    "synonyms": [
                        "odejść",
                        "porzucić",
                        "sprzeniewierzyć się"
                    ],
                    "antonyms": [
                        "dochować",
                        "przestrzegać",
                        "trwać"
                    ],
                    "common_phrases": [
                        "odstąpić od zasad",
                        "odstąpić od wiary",
                        "odstąpić od reguły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать соблюдать установленные правила, нормы, веру или традиции; изменить чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отступить",
                                "target_lang_sense_clarification": "нарушить принципы или правила"
                            },
                            {
                                "target_lang_word": "отойти",
                                "target_lang_sense_clarification": "перестать придерживаться (напр. от традиций)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "739d2d9b-c67f-3ddc-85f4-737550464f0a",
                    "sense_definition": "Vallen, meestal door uitglijden of het verliezen van evenwicht tijdens een beweging zoals fietsen of lopen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "falling_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wielrenner zag zijn tegenstander in de bocht hard <w>onderuitgaan</w>.",
                            "target_lang_translations": {
                                "en": "The cyclist saw his opponent <w>wipe out</w> hard in the bend.",
                                "ru": "Велогонщик увидел, как его соперник на повороте сильно <w>грохнулся</w>."
                            }
                        },
                        {
                            "text": "Pas goed op dat je niet gaat <w>onderuitgaan</w> op het gladde ijs.",
                            "target_lang_translations": {
                                "en": "Be very careful not to <w>slip</w> on the slippery ice.",
                                "ru": "Будь осторожен, чтобы не <w>упасть</w> на скользком льду."
                            }
                        }
                    ],
                    "synonyms": [
                        "vallen",
                        "uitglijden",
                        "slippen"
                    ],
                    "antonyms": [
                        "opstaan"
                    ],
                    "common_phrases": [
                        "hard onderuitgaan",
                        "met de fiets onderuitgaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To fall, usually by slipping or losing balance during movement.",
                        "ru": "Упасть, обычно поскользнувшись или потеряв равновесие (например, с велосипеда или на льду)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wipe out",
                                "target_lang_sense_clarification": "informal, often used for cycling or skating"
                            },
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "general physical action"
                            },
                            {
                                "target_lang_word": "crash",
                                "target_lang_sense_clarification": "specifically for vehicles or bikes"
                            },
                            {
                                "target_lang_word": "slip",
                                "target_lang_sense_clarification": "losing footing on a surface"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "упасть",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "грохнуться",
                                "target_lang_sense_clarification": "разговорное, сильно упасть"
                            },
                            {
                                "target_lang_word": "растянуться",
                                "target_lang_sense_clarification": "упасть во весь рост"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fea190f6-b7d0-37d1-88e8-b4d5a58b36f2",
                    "sense_definition": "Falen, mislukken of in ernstige (financiële) problemen komen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "failing_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zonder extra geld zal het bedrijf waarschijnlijk financieel <w>onderuitgaan</w>.",
                            "target_lang_translations": {
                                "en": "Without extra money, the company will probably <w>go under</w> financially.",
                                "ru": "Без дополнительных средств компания, вероятно, финансово <w>прогорит</w>."
                            }
                        },
                        {
                            "text": "Het ambitieuze plan is volledig <w>onderuitgegaan</w> door een gebrek aan voorbereiding.",
                            "target_lang_translations": {
                                "en": "The ambitious plan completely <w>failed</w> due to a lack of preparation.",
                                "ru": "Амбициозный план полностью <w>провалился</w> из-за недостатка подготовки."
                            }
                        }
                    ],
                    "synonyms": [
                        "falen",
                        "misgaan",
                        "stuklopen",
                        "floppen"
                    ],
                    "common_phrases": [
                        "financieel onderuitgaan",
                        "flink onderuitgaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To fail, collapse, or face serious financial ruin.",
                        "ru": "Потерпеть неудачу, провалиться или оказаться в тяжелом финансовом положении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go under",
                                "target_lang_sense_clarification": "financial context or bankruptcy"
                            },
                            {
                                "target_lang_word": "fail",
                                "target_lang_sense_clarification": "general failure of plans or entities"
                            },
                            {
                                "target_lang_word": "collapse",
                                "target_lang_sense_clarification": "sudden failure of a system or plan"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прогореть",
                                "target_lang_sense_clarification": "о бизнесе или финансах"
                            },
                            {
                                "target_lang_word": "провалиться",
                                "target_lang_sense_clarification": "о планах или проектах"
                            },
                            {
                                "target_lang_word": "потерпеть крах",
                                "target_lang_sense_clarification": "полная неудача"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af6849cb-c493-38d6-a432-1aaf83e9cd80",
                    "sense_definition": "Een wedstrijd of competitie verliezen, vaak met duidelijke cijfers.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "losing_game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De supporters zagen hun club gisteren kansloos <w>onderuitgaan</w>.",
                            "target_lang_translations": {
                                "en": "The supporters saw their club <w>lose</w> hopelessly yesterday.",
                                "ru": "Болельщики видели, как их клуб вчера безнадежно <w>проиграл</w>."
                            }
                        },
                        {
                            "text": "In de finale is de ploeg helaas tegen de kampioen <w>onderuitgegaan</w>.",
                            "target_lang_translations": {
                                "en": "In the final, the team unfortunately <w>went down</w> to the champion.",
                                "ru": "В финале команда, к сожалению, <w>проиграла</w> чемпиону."
                            }
                        }
                    ],
                    "synonyms": [
                        "verliezen"
                    ],
                    "common_phrases": [
                        "kansloos onderuitgaan",
                        "thuis onderuitgaan"
                    ],
                    "target_lang_definitions": {

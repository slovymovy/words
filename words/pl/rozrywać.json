            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0a658963-d354-3e68-b19d-dcb78e70b89a",
                    "sense_definition": "Dzielić coś na części, ciągnąc mocno w przeciwne strony lub szarpiąc; niszczyć strukturę czegoś poprzez rwanie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pies zaczął <w>rozrywać</w> starą poduszkę na drobne kawałki.",
                            "target_lang_translations": {
                                "ru": "Собака начала <w>разрывать</w> старую подушку на мелкие кусочки."
                            }
                        },
                        {
                            "text": "Nie powinieneś <w>rozrywać</w> tego opakowania zębami, bo możesz się skaleczyć.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует <w>разрывать</w> эту упаковку зубами, так как ты можешь пораниться."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozdzierać",
                        "rozszarpywać"
                    ],
                    "common_phrases": [
                        "rozrywać na kawałki",
                        "rozrywać na strzępy",
                        "rozrywać ubranie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делить что-то на части, сильно потянув в разные стороны или дергая; разрушать структуру чего-либо путем разрыва."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрывать",
                                "target_lang_sense_clarification": "делить на части силой"
                            },
                            {
                                "target_lang_word": "рвать",
                                "target_lang_sense_clarification": "нарушать целостность"
                            },
                            {
                                "target_lang_word": "раздирать",
                                "target_lang_sense_clarification": "яростно рвать на куски"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb70f85e-976f-3e29-a7f0-b0b89f7cd13f",
                    "sense_definition": "Dostarczać komuś rozrywki, sprawiać, że czas mija przyjemnie; bawić kogoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "entertainment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zatrudniony klaun miał za zadanie <w>rozrywać</w> dzieci podczas przyjęcia urodzinowego.",
                            "target_lang_translations": {
                                "ru": "Нанятый клоун должен был <w>развлекать</w> детей во время дня рождения."
                            }
                        },
                        {
                            "text": "Dobra komedia potrafi <w>rozrywać</w> widzów i poprawiać im humor.",
                            "target_lang_translations": {
                                "ru": "Хорошая комедия может <w>развлекать</w> зрителей и поднимать им настроение."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabawiać"
                    ],
                    "common_phrases": [
                        "rozrywać gości",
                        "rozrywać publiczność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доставлять кому-то удовольствие, делать так, чтобы время проходило приятно; веселить кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развлекать",
                                "target_lang_sense_clarification": "занимать чем-то приятным"
                            },
                            {
                                "target_lang_word": "веселить",
                                "target_lang_sense_clarification": "вызывать веселье"
                            },
                            {
                                "target_lang_word": "забавлять",
                                "target_lang_sense_clarification": "доставлять удовольствие игрой или шуткой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e584a150-ee89-31b2-9b77-921fa8c11a40",
                    "sense_definition": "Spędzać czas na zabawie i przyjemnościach, aby odpocząć od problemów (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "entertainment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W weekendy studenci lubią się <w>rozrywać</w> w lokalnych klubach.",
                            "target_lang_translations": {
                                "ru": "По выходным студенты любят <w>развлекаться</w> в местных клубах."
                            }
                        },
                        {
                            "text": "Musisz czasem się <w>rozerwać</w>, zamiast ciągle myśleć o pracy.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно иногда <w>развлечься</w>, вместо того чтобы постоянно думать о работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabawiać się"
                    ],
                    "common_phrases": [
                        "lubić się rozrywać",
                        "muszę się rozerwać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить время в веселье и удовольствиях, чтобы отдохнуть от проблем (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развлекаться",
                                "target_lang_sense_clarification": "проводить время весело"
                            },
                            {
                                "target_lang_word": "веселиться",
                                "target_lang_sense_clarification": "предаваться веселью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f90f9639-1242-3261-83e7-2f123dd98a72",
                    "sense_definition": "Ulegać zniszczeniu poprzez pęknięcie, rozerwanie na części lub gwałtowny rozpad (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary materiał jest tak słaby, że zaczyna się <w>rozrywać</w> w rękach.",
                            "target_lang_translations": {
                                "ru": "Старая ткань настолько слабая, что начинает <w>рваться</w> в руках."
                            }
                        },
                        {
                            "text": "Pod wpływem wysokiego ciśnienia zbiornik może się gwałtownie <w>rozerwać</w>.",
                            "target_lang_translations": {
                                "ru": "Под воздействием высокого давления резервуар может внезапно <w>разорваться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozdzierać się",
                        "pękać",
                        "eksplodować",
                        "wybuchać"
                    ],
                    "common_phrases": [
                        "rozrywać się na szwach",
                        "worek się rozrywa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушаться в результате разрыва на части или резкого распада (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рваться",
                                "target_lang_sense_clarification": "о ткани, нитках или материале"
                            },
                            {
                                "target_lang_word": "разрываться",
                                "target_lang_sense_clarification": "лопаться или делиться на части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b15f71e-0f00-3f66-88d9-84103ea5c6c3",
                    "sense_definition": "Być obiektem wielkiego zainteresowania; być masowo i szybko wykupywanym lub angażowanym (często w stronie biernej).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "market_demand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa kolekcja ubrań jest dosłownie <w>rozrywana</w> przez klientów sklepu.",
                            "target_lang_translations": {
                                "ru": "Новую коллекцию одежды буквально <w>расхватывают</w> покупатели магазина."
                            }
                        },
                        {
                            "text": "Ten wybitny specjalista jest ciągle <w>rozrywany</w> przez konkurencyjne firmy.",
                            "target_lang_translations": {
                                "ru": "Этого выдающегося специалиста постоянно <w>разрывают</w> на части конкурирующие фирмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozchwytywać",
                        "rozgrabiać"
                    ],
                    "common_phrases": [
                        "być rozrywanym",
                        "towar rozrywany"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть объектом большого интереса; быстро раскупаться или быть очень востребованным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть нарасхват",
                                "target_lang_sense_clarification": "пользоваться огромным спросом"

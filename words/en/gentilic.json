                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "катойконимический",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "оттопонимический",
                                "target_lang_sense_clarification": "образованный от названия места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59e059fd-bd41-3d56-96af-6dd4ce9ca864",
                    "sense_definition": "Relating to a tribe, clan, or nation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The historian analyzed the <w>gentilic</w> structures of ancient Roman society.",
                            "target_lang_translations": {
                                "ru": "Историк проанализировал <w>родовые</w> структуры древнеримского общества."
                            }

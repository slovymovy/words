    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e56334a8-abd4-3c3e-b730-e6c536ad804a",
                    "sense_definition": "Zacząć zachowywać się nienormalnie, nieracjonalnie lub stracić równowagę psychiczną.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad chyba <sfiksował>, bo krzyczy do telewizora w środku nocy.",
                            "target_lang_translations": {
                                "ru": "Мой сосед, похоже, <свихнулся>, потому что кричит на телевизор посреди ночи."
                            }
                        },
                        {
                            "text": "Można <sfiksować> od tego ciągłego hałasu dochodzącego z ulicy.",
                            "target_lang_translations": {
                                "ru": "Можно <чокнуться> от этого постоянного шума с улицы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwariować",
                        "oszaleć",
                        "zbzikować",
                        "ześwirować"
                    ],
                    "common_phrases": [
                        "można sfiksować",
                        "sfiksować do reszty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать вести себя ненормально, потерять рассудок или душевное равновесие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свихнуться",
                                "target_lang_sense_clarification": "разговорное; потерять рассудок"
                            },
                            {
                                "target_lang_word": "чокнуться",
                                "target_lang_sense_clarification": "разговорное; сойти с ума"
                            },
                            {
                                "target_lang_word": "сойти с ума",
                                "target_lang_sense_clarification": "общеупотребительное; лишиться рассудка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f45a9193-65db-3e8e-86fc-04080fd13dd9",
                    "sense_definition": "Zacząć mieć obsesję na jakimś punkcie, bardzo mocno się czymś zainteresować.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ona całkowicie <sfiksowała> na punkcie zdrowego odżywiania i liczy każdą kalorię.",
                            "target_lang_translations": {
                                "ru": "Она полностью <помешалась> на здоровом питании и считает каждую калорию."
                            }
                        },
                        {
                            "text": "Mój brat <sfiksował> na punkcie starych samochodów i ciągle o nich opowiada.",
                            "target_lang_translations": {
                                "ru": "Мой брат <помешался> на старых машинах и постоянно о них рассказывает."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwariować"
                    ],
                    "common_phrases": [
                        "sfiksować na punkcie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать одержимым чем-либо, проявить чрезмерный интерес к чему-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помешаться",
                                "target_lang_sense_clarification": "стать одержимым какой-либо идеей или предметом"
                            },
                            {
                                "target_lang_word": "зациклиться",
                                "target_lang_sense_clarification": "сосредоточить все внимание на чем-то одном"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f979f62-f566-38a9-91d2-25d964ae80d3",
                    "sense_definition": "Przestać działać poprawnie, ulec awarii (zwykle o urządzeniach elektronicznych lub oprogramowaniu).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "malfunction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój telefon <sfiksował> i sam zaczął wysyłać dziwne wiadomości do znajomych.",
                            "target_lang_translations": {
                                "ru": "Мой телефон <заглючил> и сам начал отправлять странные сообщения друзьям."
                            }
                        },
                        {
                            "text": "System alarmowy w biurze <sfiksował> z powodu gwałtownej burzy.",
                            "target_lang_translations": {
                                "ru": "Сигнализация в офисе <вышла из строя> из-за сильной грозы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "komputer sfiksował",
                        "system sfiksował"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать работать исправно, выйти из строя (обычно об электронике или программах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заглючить",
                                "target_lang_sense_clarification": "сленг; начать работать с ошибками (о технике)"
                            },
                            {
                                "target_lang_word": "выйти из строя",
                                "target_lang_sense_clarification": "сломаться, перестать функционировать"
                            },
                            {
                                "target_lang_word": "взбеситься",
                                "target_lang_sense_clarification": "переносное; начать вести себя непредсказуемо (о технике)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
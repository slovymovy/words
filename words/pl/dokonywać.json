{
    "word_family": [
        "dokonać",
        "dokonanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "408a22b3-9616-3184-9af6-604b3a5d9cf3",
                    "sense_definition": "Wykonywać określoną czynność lub działanie, przeprowadzać coś (często używane w języku oficjalnym i urzędowym).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "perform_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klient <dokonuje> płatności za zakupy przy użyciu karty kredytowej.",
                            "target_lang_translations": {
                                "ru": "Клиент <производит> оплату покупок с помощью кредитной карты."
                            }
                        },
                        {
                            "text": "Komisja rekrutacyjna <dokonuje> oceny wszystkich nadesłanych zgłoszeń.",
                            "target_lang_translations": {
                                "ru": "Приемная комиссия <осуществляет> оценку всех присланных заявок."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopuszczać się",
                        "popełniać"
                    ],
                    "common_phrases": [
                        "dokonywać wyboru",
                        "dokonywać zakupu",
                        "dokonywać zmian",
                        "dokonywać wpłaty",
                        "dokonywać analizy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять определенное действие или процедуру, осуществлять что-либо (часто используется в официальном и деловом стиле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "производить",
                                "target_lang_sense_clarification": "оплату, оценку, расчеты"
                            },
                            {
                                "target_lang_word": "совершать",
                                "target_lang_sense_clarification": "покупку, сделку, выбор"
                            },
                            {
                                "target_lang_word": "осуществлять",
                                "target_lang_sense_clarification": "платеж, деятельность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "337d2443-f8be-3b40-a1ba-9841f6f3cfab",
                    "sense_definition": "Robić coś złego, niewłaściwego lub zabronionego przez prawo.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "commit_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja poszukuje sprawcy, który <dokonywał> kradzieży w naszej okolicy.",
                            "target_lang_translations": {
                                "ru": "Полиция ищет преступника, который <совершал> кражи в нашем районе."
                            }
                        },
                        {
                            "text": "Żaden człowiek nie powinien <dokonywać> samosądu na innych.",
                            "target_lang_translations": {
                                "ru": "Ни один человек не должен <учинять> самосуд над другими."
                            }
                        }
                    ],
                    "synonyms": [
                        "popełniać",
                        "dopuszczać się"
                    ],
                    "common_phrases": [
                        "dokonywać przestępstwa",
                        "dokonywać zbrodni",
                        "dokonywać kradzieży",
                        "dokonywać zamachu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то плохое, неправильное или запрещенное законом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совершать",
                                "target_lang_sense_clarification": "преступление, кражу, ошибку"
                            },
                            {
                                "target_lang_word": "учинять",
                                "target_lang_sense_clarification": "расправу, самосуд, скандал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1366a7ca-bb0a-3c11-8f0d-0e43b4d5ef7c",
                    "sense_definition": "Dziać się, zachodzić lub mieć miejsce (używane z zaimkiem „się” w odniesieniu do procesów lub zmian).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "happen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na naszych oczach <dokonuje się> wielka rewolucja technologiczna.",
                            "target_lang_translations": {
                                "ru": "На наших глазах <совершается> великая технологическая революция."
                            }
                        },
                        {
                            "text": "W przyrodzie nieustannie <dokonują się> powolne zmiany, których nie zauważamy.",
                            "target_lang_translations": {
                                "ru": "В природе постоянно <происходят> медленные изменения, которых мы не замечаем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dokonywać się woli",
                        "dokonywać się przemiany",
                        "sprawiedliwość się dokonuje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить, случаться или иметь место (обычно о процессах, изменениях или важных событиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "происходить",
                                "target_lang_sense_clarification": "о событиях, изменениях"
                            },
                            {
                                "target_lang_word": "совершаться",
                                "target_lang_sense_clarification": "о свершениях, таинствах, революциях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd98fb1a-2faa-3985-91e9-7c4f18018a1b",
                    "sense_definition": "Osiągać coś trudnego, co wymaga dużego wysiłku; doprowadzać coś do skutku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "achieve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarze w tym szpitalu codziennie <dokonywali> cudów, ratując życie pacjentów.",
                            "target_lang_translations": {
                                "ru": "Врачи в этой больнице ежедневно <творили> чудеса, спасая жизни пациентов."
                            }
                        },
                        {
                            "text": "Ten zespół potrafi <dokonywać> rzeczy niemożliwych w krótkim czasie.",
                            "target_lang_translations": {
                                "ru": "Эта команда способна <совершать> невозможное за короткое время."
                            }
                        }
                    ],
                    "synonyms": [
                        "dopuszczać się",
                        "popełniać"
                    ],
                    "common_phrases": [
                        "dokonywać cudów",
                        "dokonywać niemożliwego",
                        "dokonywać odkrycia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достигать чего-то значительного, требующего усилий; делать что-то исключительное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "творить",
                                "target_lang_sense_clarification": "чудеса"
                            },
                            {
                                "target_lang_word": "совершать",
                                "target_lang_sense_clarification": "открытие, подвиг, невозможное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
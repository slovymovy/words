    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "dc9b221c-4eeb-3f0e-b0ff-008eb11d678c",
                    "sense_definition": "Volgend op een bepaald tijdstip of een gebeurtenis; later dan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gaan <w>na</w> het avondeten een stukje wandelen.",
                            "target_lang_translations": {
                                "ru": "Мы пойдем немного прогуляться <w>после</w> ужина.",
                                "en": "We are going for a walk <w>after</w> dinner."
                            }
                        },
                        {
                            "text": "<w>Na</w> regen komt zonneschijn.",
                            "target_lang_translations": {
                                "ru": "<w>После</w> дождя выглядывает солнце.",
                                "en": "Sunshine comes <w>after</w> rain."
                            }
                        },
                        {
                            "text": "Hij kwam pas <w>na</w> middernacht thuis van het feest.",
                            "target_lang_translations": {
                                "ru": "Он вернулся домой с вечеринки только <w>после</w> полуночи.",
                                "en": "He only came home from the party <w>after</w> midnight."
                            }
                        }
                    ],
                    "synonyms": [
                        "later"
                    ],
                    "antonyms": [
                        "voor"
                    ],
                    "common_phrases": [
                        "na verloop van tijd",
                        "keer op keer",
                        "dag na dag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий за определенным моментом или событием; позже.",
                        "en": "Following a certain time or event; later than."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "после",
                                "target_lang_sense_clarification": "предлог времени"
                            },
                            {
                                "target_lang_word": "спустя",
                                "target_lang_sense_clarification": "через некоторое время"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "after",
                                "target_lang_sense_clarification": "time preposition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "417c8f11-e175-3905-9389-165a11bdb98c",
                    "sense_definition": "Achter iemand of iets anders komend in een reeks, volgorde of plaats.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "order_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het alfabet komt de letter B <w>na</w> de A.",
                            "target_lang_translations": {
                                "ru": "В алфавите буква Б идет <w>после</w> А.",
                                "en": "In the alphabet, the letter B comes <w>after</w> the A."
                            }
                        },
                        {
                            "text": "U bent <w>na</w> mij aan de beurt bij de kassa.",
                            "target_lang_translations": {
                                "ru": "Ваша очередь на кассе <w>после</w> меня.",
                                "en": "It is your turn <w>after</w> me at the checkout."
                            }
                        },
                        {
                            "text": "Sla <w>na</w> de kerk direct linksaf.",
                            "target_lang_translations": {
                                "ru": "Поверните сразу налево <w>после</w> церкви.",
                                "en": "Turn left immediately <w>after</w> the church."
                            }
                        }
                    ],
                    "antonyms": [
                        "voor"
                    ],
                    "common_phrases": [
                        "de een na de ander"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий за кем-то или чем-то в очереди, порядке или пространстве.",
                        "en": "Coming after someone or something else in a series, sequence, or place."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "после",
                                "target_lang_sense_clarification": "порядок следования"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "позади, следом"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "after",
                                "target_lang_sense_clarification": "sequence or order"
                            },
                            {
                                "target_lang_word": "behind",
                                "target_lang_sense_clarification": "position or place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "132e21c5-0c8a-30b5-81c1-6132a150222d",
                    "sense_definition": "Met uitzondering van; behalve (vaak gebruikt in combinatie met 'op ... na').",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "exception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zaal was op één stoel <w>na</w> helemaal leeg.",
                            "target_lang_translations": {
                                "ru": "Зал был совершенно пуст, <w>кроме</w> одного стула.",
                                "en": "The hall was completely empty <w>except for</w> one chair."
                            }
                        },
                        {
                            "text": "Brazilië is het op vier <w>na</w> grootste land ter wereld.",
                            "target_lang_translations": {
                                "ru": "Бразилия — самая большая страна в мире <w>после</w> четырех.",
                                "en": "Brazil is the fifth largest country in the world."
                            }
                        },
                        {
                            "text": "Iedereen was aanwezig op het feest, op Jan <w>na</w>.",
                            "target_lang_translations": {
                                "ru": "На вечеринке были все, <w>кроме</w> Яна.",
                                "en": "Everyone was present at the party, <w>except for</w> Jan."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorbij",
                        "later"
                    ],
                    "common_phrases": [
                        "op een na",
                        "op een haar na"
                    ],
                    "target_lang_definitions": {
                        "ru": "За исключением; кроме (часто используется в конструкции 'op ... na').",
                        "en": "With the exception of; except (often used in the construction 'op ... na')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроме",
                                "target_lang_sense_clarification": "исключение"
                            },
                            {
                                "target_lang_word": "за исключением",
                                "target_lang_sense_clarification": "формально"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "except for",
                                "target_lang_sense_clarification": "exclusion"
                            },
                            {
                                "target_lang_word": "apart from",
                                "target_lang_sense_clarification": "exclusion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eafe58e7-f516-3642-92fc-39a2d86fef3e",
                    "sense_definition": "In de richting van (oude vorm van 'naar').",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "direction_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij vertrok <w>na</w> verre oorden.",
                            "target_lang_translations": {
                                "ru": "Он отправился <w>в</w> далекие края.",
                                "en": "He left <w>for</w> distant lands."
                            }
                        },
                        {
                            "text": "De reiziger keerde terug <w>na</w> huis.",
                            "target_lang_translations": {
                                "ru": "Путешественник вернулся <w>домой</w>.",
                                "en": "The traveler returned <w>home</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This form is rarely used in modern Dutch; 'naar' is the standard form."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении чего-либо (устаревшая форма предлога 'naar').",
                        "en": "Towards; in the direction of (archaic form of 'naar')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "направление"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "направление"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "direction"
                            },
                            {
                                "target_lang_word": "towards",
                                "target_lang_sense_clarification": "direction"
                            }
                        ]
                    }
                }
            ]

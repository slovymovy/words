            "senses": [
                {
                    "sense_id": "9948fe3c-8a80-3ccc-96bd-582c6b70915e",
                    "sense_definition": "Het geheel van gebeurtenissen die in het verleden hebben plaatsgevonden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De Tweede Wereldoorlog is een zwarte bladzijde in de <w>geschiedenis</w>.",
                            "target_lang_translations": {
                                "en": "World War II is a dark page in <w>history</w>.",
                                "ru": "Вторая мировая война — это черная страница в <w>истории</w>."
                            }
                        },
                        {
                            "text": "Dat incident is nu <w>geschiedenis</w> en we moeten vooruit kijken.",
                            "target_lang_translations": {
                                "en": "That incident is <w>history</w> now and we must look forward.",
                                "ru": "Этот инцидент теперь <w>история</w>, и мы должны смотреть вперед."
                            }
                        }
                    ],
                    "antonyms": [
                        "toekomst"
                    ],
                    "common_phrases": [
                        "in de geschiedenis",
                        "de vaderlandse geschiedenis"
                    ],
                    "target_lang_definitions": {
                        "en": "The aggregate of events that took place in the past.",
                        "ru": "Совокупность событий, произошедших в прошлом."
                    },
                    "translations": {

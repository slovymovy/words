    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5dc32eec-5620-35bc-8f38-e3a9f525ff3a",
                    "sense_definition": "Грубое, презрительное название женщины, ведущей распутный образ жизни или имеющей беспорядочные половые связи.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Соседи шептались, что она <w>шлюха</w>, потому что к ней часто приходили мужчины.",
                            "target_lang_translations": {
                                "en": "The neighbors whispered that she was a <w>slut</w> because men often visited her."
                            }
                        },
                        {
                            "text": "Он в гневе назвал свою бывшую девушку грязной <w>шлюхой</w>.",
                            "target_lang_translations": {
                                "en": "In anger, he called his ex-girlfriend a dirty <w>whore</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ку́рва"
                    ],
                    "antonyms": [
                        "недотрога"
                    ],
                    "common_phrases": [
                        "грязная шлюха",
                        "подзаборная шлюха",
                        "вести себя как шлюха"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar, rude"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A rude, contemptuous term for a woman who leads a promiscuous lifestyle or has indiscriminate sexual relations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slut",
                                "target_lang_sense_clarification": "a sexually promiscuous woman"
                            },
                            {
                                "target_lang_word": "whore",
                                "target_lang_sense_clarification": "offensive term for a promiscuous woman"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26ee750a-f514-30a2-a3ee-336a2a9a7f33",
                    "sense_definition": "Женщина, которая занимается проституцией; продажная женщина.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом районе города по ночам часто стоят <w>шлюхи</w>.",
                            "target_lang_translations": {
                                "en": "In this part of the city, <w>whores</w> often stand around at night."
                            }
                        },
                        {
                            "text": "Фильм рассказывал о трагической судьбе портовой <w>шлюхи</w>.",
                            "target_lang_translations": {
                                "en": "The film told the tragic fate of a port <w>whore</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "проститу́тка"
                    ],
                    "common_phrases": [
                        "снять шлюху",
                        "дешёвая шлюха",
                        "портовая шлюха"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A woman who engages in prostitution; a woman who sells herself."
                    },
                    "translations": {

{
    "word_family": [
        "wellen",
        "opwellen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b54b9c1a-14ce-352f-9d6d-44a6d6479e70",
                    "sense_definition": "Uit een bron ontstaan en beginnen te stromen (meestal over water).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "water_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rivier <w>ontwelt</w> hoog in de bergen aan een gletsjer.",
                            "target_lang_translations": {
                                "ru": "Река <w>берёт начало</w> высоко в горах из ледника."
                            }
                        },
                        {
                            "text": "Er <w>ontwelt</w> helder water aan de rotsachtige bodem van de grot.",
                            "target_lang_translations": {
                                "ru": "Прозрачная вода <w>вытекает</w> из каменистого дна пещеры."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontspringen",
                        "opwellen",
                        "voortkomen"
                    ],
                    "common_phrases": [
                        "aan de bodem ontwellen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать начало из источника и начинать течь (обычно о воде)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брать начало",
                                "target_lang_sense_clarification": "о реке или источнике"
                            },
                            {
                                "target_lang_word": "вытекать",
                                "target_lang_sense_clarification": "течь из источника"
                            },
                            {
                                "target_lang_word": "проистекать",
                                "target_lang_sense_clarification": "книжное, о происхождении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86442638-38a8-3615-a414-f1cd3997d685",
                    "sense_definition": "Naar boven komen of tevoorschijn komen (meestal over tranen of gevoelens).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "emotions_tears",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tranen <w>ontwelden</w> aan haar ogen toen ze het droevige bericht las.",
                            "target_lang_translations": {
                                "ru": "Слёзы <w>навернулись</w> ей на глаза, когда она прочла печальное сообщение."
                            }
                        },
                        {
                            "text": "Een diepe zucht <w>ontwelde</w> aan zijn borst na de zware inspanning.",
                            "target_lang_translations": {
                                "ru": "Глубокий вздох <w>вырвался</w> из его груди после тяжелого усилия."
                            }
                        }
                    ],
                    "synonyms": [
                        "opwellen",
                        "vloeien"
                    ],
                    "common_phrases": [
                        "aan het oog ontwellen",
                        "tranen ontwellen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появляться или выступать наружу (обычно о слезах или чувствах)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наворачиваться",
                                "target_lang_sense_clarification": "о слезах"
                            },
                            {
                                "target_lang_word": "вырываться",
                                "target_lang_sense_clarification": "о звуках или чувствах"
                            },
                            {
                                "target_lang_word": "подступать",
                                "target_lang_sense_clarification": "о чувствах или слезах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The complex instructions completely <w>dizzied</w> the new students.",
                            "target_lang_translations": {
                                "ru": "Сложные инструкции совершенно <w>сбили с толку</w> новых студентов."
                            }
                        },
                        {
                            "text": "He was <w>dizzied</w> by the sudden changes in the plan.",
                            "target_lang_translations": {
                                "ru": "Внезапные изменения в плане <w>сбили</w> его <w>с толку</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-то чувство неустойчивости, замешательства или растерянности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбивать с толку",
                                "target_lang_sense_clarification": "запутывать"
                            },
                            {
                                "target_lang_word": "кружить голову",
                                "target_lang_sense_clarification": "одурманивать"
                            }
                        ]
                    }
                }
            ]
        },

                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walk through",
                                "target_lang_sense_clarification": "traverse a space"
                            },
                            {
                                "target_lang_word": "walk down",
                                "target_lang_sense_clarification": "follow a path/hallway to the end"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50c0b0d1-2200-308b-ac94-c5a691310ea4",
                    "sense_definition": "Een proces, opleiding of tekst van begin tot eind volgen en voltooien.",

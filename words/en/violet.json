        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc56d64f-7cc8-3708-8c63-e36167ee74c9",
                    "sense_definition": "A small plant of the genus Viola, typically having purple, blue, or white flowers with a sweet scent.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "plants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She planted a beautiful **violet** in the flower pot near the window.",
                            "target_lang_translations": {
                                "ru": "Она посадила красивую <w>фиалку</w> в цветочный горшок у окна."
                            }
                        },
                        {
                            "text": "The sweet smell of the **violet** reminded him of spring.",
                            "target_lang_translations": {
                                "ru": "Сладкий запах <w>фиалки</w> напомнил ему о весне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "African violet",
                        "sweet violet",
                        "shrinking violet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое растение рода Viola, обычно с фиолетовыми, синими или белыми цветами и сладким ароматом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фиалка",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "5de5514b-4771-3240-ab0b-883854c4198e",
                    "sense_definition": "Горная система, расположенная между Восточно-Европейской и Западно-Сибирской равнинами, являющаяся границей между Европой и Азией; также географический регион вокруг этих гор.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geo_region",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Мы планируем поехать в поход на <w>Урал</w> следующим летом.",
                            "target_lang_translations": {
                                "en": "We are planning to go on a hiking trip to <w>the Urals</w> next summer."
                            }
                        },
                        {
                            "text": "<w>Урал</w> славится своими богатыми месторождениями полезных ископаемых.",
                            "target_lang_translations": {
                                "en": "<w>The Urals</w> are famous for their rich mineral deposits."
                            }
                        }
                    ],
                    "synonyms": [
                        "Уральские горы"
                    ],
                    "common_phrases": [
                        "Южный Урал",
                        "Северный Урал",
                        "Средний Урал",
                        "поехать на Урал"
                    ],
                    "target_lang_definitions": {
                        "en": "A mountain system located between the East European and West Siberian plains, serving as the boundary between Europe and Asia; also the geographical region surrounding these mountains."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "the Urals",
                                "target_lang_sense_clarification": "mountain range and region"
                            },
                            {
                                "target_lang_word": "Ural Mountains",
                                "target_lang_sense_clarification": "mountain range"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01df5818-9270-3f07-a909-1325c45437e7",
                    "sense_definition": "Крупная река в Восточной Европе, протекающая по территории России и Казахстана и впадающая в Каспийское море.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "geo_river",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Казаки издавна строили свои поселения там, где течёт <w>Урал</w>.",
                            "target_lang_translations": {
                                "en": "Cossacks have long built their settlements where the <w>Ural</w> flows."
                            }
                        },
                        {
                            "text": "Река <w>Урал</w> является третьей по длине рекой в Европе.",
                            "target_lang_translations": {
                                "en": "The <w>Ural</w> River is the third longest river in Europe."
                            }
                        }
                    ],
                    "synonyms": [
                        "Я́и́к"
                    ],
                    "common_phrases": [
                        "река Урал",
                        "бассейн Урала"
                    ],
                    "target_lang_definitions": {
                        "en": "A large river in Eastern Europe flowing through Russia and Kazakhstan and emptying into the Caspian Sea."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Ural River",
                                "target_lang_sense_clarification": "river"
                            },
                            {
                                "target_lang_word": "Ural",
                                "target_lang_sense_clarification": "river name"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7a5e33ea-0462-3f25-88a8-ba7242a82bd6",
                    "sense_definition": "Марка тяжёлых грузовых автомобилей повышенной проходимости, а также мотоциклов; отдельное транспортное средство этой марки.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "vehicle",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "По размытой лесной дороге мог проехать только мощный <w>Урал</w>.",
                            "target_lang_translations": {
                                "en": "Only a powerful <w>Ural</w> could drive along the washed-out forest road."
                            }
                        },
                        {
                            "text": "Водитель загнал свой старый <w>Урал</w> в гараж для ремонта.",
                            "target_lang_translations": {
                                "en": "The driver drove his old <w>Ural</w> into the garage for repairs."
                            }
                        }
                    ],
                    "common_phrases": [
                        "грузовик Урал",
                        "мотоцикл Урал"
                    ],
                    "target_lang_definitions": {
                        "en": "A brand of heavy off-road trucks and motorcycles; a specific vehicle of this brand."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Ural",
                                "target_lang_sense_clarification": "brand of trucks or motorcycles"
                            }
                        ]
                    }
                }
            ]
        }
    ]

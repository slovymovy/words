    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "201ea268-b69f-37c7-827e-da40b5ec9d3c",
                    "sense_definition": "De snelheid waarmee een handeling wordt uitgevoerd of waarmee gebeurtenissen elkaar opvolgen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "speed_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We liepen in een stevig <w>tempo</w> naar het station om de trein te halen.",
                            "target_lang_translations": {
                                "en": "We walked to the station at a brisk <w>pace</w> to catch the train.",
                                "ru": "Мы шли в быстром <w>темпе</w> к вокзалу, чтобы успеть на поезд."
                            }
                        },
                        {
                            "text": "Je mag de opdrachten in je eigen <w>tempo</w> maken, zolang ze maar op tijd af zijn.",
                            "target_lang_translations": {
                                "en": "You can do the assignments at your own <w>pace</w>, as long as they are finished on time.",
                                "ru": "Ты можешь выполнять задания в своем собственном <w>темпе</w>, главное, чтобы они были готовы вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "snelheid"
                    ],
                    "common_phrases": [
                        "in hoog tempo",
                        "in eigen tempo",
                        "het tempo bepalen"
                    ],
                    "target_lang_definitions": {
                        "en": "The speed at which an action is performed or events follow one another.",
                        "ru": "Скорость, с которой выполняется действие или следуют друг за другом события."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pace",
                                "target_lang_sense_clarification": "speed of walking or working"
                            },
                            {
                                "target_lang_word": "tempo",
                                "target_lang_sense_clarification": "rhythm or rate of activity"
                            },
                            {
                                "target_lang_word": "speed",
                                "target_lang_sense_clarification": "rate of motion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "темп",
                                "target_lang_sense_clarification": "скорость осуществления чего-либо"
                            },
                            {
                                "target_lang_word": "скорость",
                                "target_lang_sense_clarification": "быстрота движения или действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe678cd9-0822-3da0-94e0-79f6581f8a16",
                    "sense_definition": "De snelheid waarmee een muziekstuk wordt gespeeld.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "speed_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dirigent gaf het <w>tempo</w> aan met zijn stokje.",
                            "target_lang_translations": {
                                "en": "The conductor indicated the <w>tempo</w> with his baton.",
                                "ru": "Дирижер задавал <w>темп</w> своей палочкой."
                            }
                        },
                        {
                            "text": "Het <w>tempo</w> van dit nummer ligt te hoog voor de beginnende dansers.",
                            "target_lang_translations": {
                                "en": "The <w>tempo</w> of this song is too fast for the beginning dancers.",
                                "ru": "<w>Темп</w> этой композиции слишком высок для начинающих танцоров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het tempo aangeven"
                    ],
                    "target_lang_definitions": {
                        "en": "The speed at which a piece of music is played.",
                        "ru": "Скорость, с которой исполняется музыкальное произведение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tempo",
                                "target_lang_sense_clarification": "speed of music"
                            },
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "musical rhythm or pulse"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "темп",
                                "target_lang_sense_clarification": "в музыке"
                            }
                        ]
                    }
                }
            ]
        }
    ]

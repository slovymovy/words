                    "target_lang_definitions": {
                        "ru": "Объявлять официальное решение, судебный приговор, мнение или приказ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выносить",
                                "target_lang_sense_clarification": "приговор, решение"
                            },
                            {
                                "target_lang_word": "отдавать",
                                "target_lang_sense_clarification": "приказ"
                            },
                            {
                                "target_lang_word": "издавать",
                                "target_lang_sense_clarification": "указ, постановление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9ee58b5-5d85-30b7-976e-73dc6cb43e49",
                    "sense_definition": "Wytwarzać z siebie dźwięk lub zapach; emitować coś na zewnątrz.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary silnik zaczął <wydawać> niepokojące odgłosy.",
                            "target_lang_translations": {
                                "ru": "Старый двигатель начал <издавать> тревожные звуки."
                            }
                        },
                        {
                            "text": "Kwiaty te wieczorem <wydają> intensywny zapach.",
                            "target_lang_translations": {
                                "ru": "Эти цветы вечером <издают> сильный запах."
                            }
                        }
                    ],
                    "synonyms": [
                        "emitować",
                        "wydzielać",
                        "wytwarzać"
                    ],
                    "common_phrases": [
                        "wydawać dźwięk",
                        "wydawać zapach",
                        "wydawać okrzyk",
                        "wydawać woń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить звук или запах; испускать что-либо наружу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "издавать",
                                "target_lang_sense_clarification": "звук, запах"
                            },
                            {
                                "target_lang_word": "испускать",
                                "target_lang_sense_clarification": "запах, излучение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4167444b-1353-30be-acdb-a3f4a2221101",
                    "sense_definition": "Ujawnić wrogom lub władzom miejsce pobytu kogoś ukrywającego się; zdradzić kogoś.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "betrayal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bał się, że sąsiad <wyda> go policji.",
                            "target_lang_translations": {
                                "ru": "Он боялся, что сосед <выдаст> его полиции."
                            }
                        },
                        {
                            "text": "Nie chciał <wydać> swoich wspólników mimo przesłuchania.",
                            "target_lang_translations": {
                                "ru": "Он не хотел <выдавать> своих сообщников, несмотря на допрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdradzać",
                        "denuncjować",
                        "sypać"
                    ],

                        {
                            "text": "Stories were told about the <w>brave</w> who protected his tribe.",
                            "target_lang_translations": {
                                "ru": "Рассказывали истории о <w>воине</w>, который защищал свое племя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "young brave",
                        "Indian brave"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Североамериканский индейский воин."

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9d223191-16db-35c7-a458-4b19b7b2c3f8",
                    "sense_definition": "Przemieszczać się w powietrzu za pomocą skrzydeł (o ptakach, owadach) lub będąc niesionym przez wiatr (o lekkich przedmiotach).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mały wróbel <w>frunął</w> szybko na gałąź wysokiego drzewa.",
                            "target_lang_translations": {
                                "ru": "Маленький воробей быстро <w>полетел</w> на ветку высокого дерева."
                            }
                        },
                        {
                            "text": "Kolorowy latawiec <w>frunął</w> wysoko na wietrze nad plażą.",
                            "target_lang_translations": {
                                "ru": "Цветной воздушный змей <w>парил</w> высоко на ветру над пляжем."
                            }
                        }
                    ],
                    "synonyms": [
                        "lecieć",
                        "szybować",
                        "unosić się"
                    ],
                    "antonyms": [
                        "iść",
                        "stąpać",
                        "spadać"
                    ],
                    "common_phrases": [
                        "frunąć do góry",
                        "frunąć w powietrzu",
                        "ptaki fruną"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться по воздуху при помощи крыльев (о птицах, насекомых) или под действием ветра (о легких предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лететь",
                                "target_lang_sense_clarification": "перемещаться по воздуху"
                            },
                            {
                                "target_lang_word": "парить",
                                "target_lang_sense_clarification": "держаться в воздухе (о воздушном змее и т.п.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72fd7014-3643-3a1a-ac4e-14b61d90c11d",
                    "sense_definition": "Przemieszczać się bardzo szybko i energicznie, zwykle biegnąc, często z powodu pośpiechu lub radości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_fast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę już <w>frunąć</w> do pracy, bo jestem bardzo spóźniona.",
                            "target_lang_translations": {
                                "ru": "Мне нужно уже <w>лететь</w> на работу, потому что я сильно опаздываю."
                            }
                        },
                        {
                            "text": "Dzieci <w>frunęły</w> do kuchni, gdy tylko poczuły zapach ciasta.",
                            "target_lang_translations": {
                                "ru": "Дети <w>полетели</w> на кухню, как только почувствовали запах пирога."
                            }
                        }
                    ],
                    "synonyms": [
                        "pędzić",
                        "gnać",
                        "biec"
                    ],
                    "antonyms": [
                        "wlec się",
                        "spacerować"
                    ],
                    "common_phrases": [
                        "frunąć jak na skrzydłach",
                        "frunąć do domu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться очень быстро и энергично, обычно бегом, часто из-за спешки или радости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лететь",
                                "target_lang_sense_clarification": "очень быстро бежать, спешить"
                            },
                            {
                                "target_lang_word": "мчаться",
                                "target_lang_sense_clarification": "стремительно двигаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa50234c-b3d9-34d2-9619-14824bdfcf02",
                    "sense_definition": "Gwałtownie spaść z wysokości lub zostać zrzuconym; potocznie także stracić pracę lub stanowisko.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_fall",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj na tej drabinie, żebyś z niej nie <w>frunął</w>.",
                            "target_lang_translations": {
                                "ru": "Осторожнее на этой лестнице, чтобы ты с неё не <w>полетел</w>."
                            }
                        },
                        {
                            "text": "Jeśli nie poprawisz swoich wyników, możesz <w>frunąć</w> z tego stanowiska.",
                            "target_lang_translations": {
                                "ru": "Если не улучшишь свои результаты, можешь <w>вылететь</w> с этой должности."
                            }
                        }
                    ],
                    "synonyms": [
                        "spaść",
                        "zlecieć",
                        "wylecieć"
                    ],
                    "common_phrases": [
                        "frunąć ze schodów",
                        "frunąć z pracy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко упасть с высоты или быть сброшенным; в разговорной речи также потерять работу или должность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полететь",
                                "target_lang_sense_clarification": "упасть, свалиться (разг.)"
                            },
                            {
                                "target_lang_word": "вылететь",
                                "target_lang_sense_clarification": "быть уволенным (разг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
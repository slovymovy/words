    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "589bcf3b-3ef6-3ee8-ad5d-005f6346d668",
                    "sense_definition": "A large amount of something; used primarily with uncountable nouns.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity_large",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We do not have <w>much</w> time before the train leaves.",
                            "target_lang_translations": {
                                "ru": "У нас не так <w>много</w> времени до отправления поезда."
                            }
                        },
                        {
                            "text": "There is too <w>much</w> sugar in this coffee for my taste.",
                            "target_lang_translations": {
                                "ru": "В этом кофе слишком <w>много</w> сахара на мой вкус."
                            }
                        }
                    ],
                    "synonyms": [
                        "a lot of",
                        "a great deal of",
                        "plenty"
                    ],
                    "antonyms": [
                        "little"
                    ],
                    "common_phrases": [
                        "how much",
                        "too much",
                        "so much",
                        "as much"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество чего-либо; используется преимущественно с неисчисляемыми существительными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "много",
                                "target_lang_sense_clarification": "большое количество"
                            },
                            {
                                "target_lang_word": "немало",
                                "target_lang_sense_clarification": "значительное количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26ecf76f-6618-3e30-95c4-924d9328aee8",
                    "sense_definition": "Used to indicate, demonstrate, or compare a specific quantity of something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity_specific",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You only need to add this <w>much</w> water to the mixture.",
                            "target_lang_translations": {
                                "ru": "Вам нужно добавить только <w>столько</w> воды в смесь."
                            }
                        },
                        {
                            "text": "Take as <w>much</w> time as you need to finish the project.",
                            "target_lang_translations": {
                                "ru": "Возьмите <w>столько</w> времени, сколько вам нужно, чтобы закончить проект."
                            }
                        }
                    ],
                    "common_phrases": [
                        "this much",
                        "that much",
                        "as much as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания, демонстрации или сравнения определенного количества чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столько",
                                "target_lang_sense_clarification": "указание на конкретное количество"
                            }
                        ]
                    }
                }
            ]
        },
        {

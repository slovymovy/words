                {
                    "sense_id": "f60f1740-506b-3f75-ac76-e4e837208ddb",
                    "sense_definition": "Zonder gevoel, medeleven of vriendelijkheid; afstandelijk in gedrag.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>koude</w> blik maakte mij een beetje bang.",

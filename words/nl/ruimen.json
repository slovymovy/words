    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0c098a50-4fbf-3ad9-a6af-c9c68f7757be",
                    "sense_definition": "Iets leegmaken of obstakels verwijderen om plaats te maken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "clearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de zware storm moesten de bewoners samen het puin <w>ruimen</w>.",
                            "target_lang_translations": {
                                "en": "After the heavy storm, the residents had to <w>clear</w> the rubble together.",
                                "ru": "После сильного шторма жителям пришлось вместе <w>разбирать</w> завалы."
                            }
                        },
                        {
                            "text": "De gemeente laat morgen vroeg de sneeuw van de fietspaden <w>ruimen</w>.",
                            "target_lang_translations": {
                                "en": "The municipality is having the snow <w>cleared</w> from the bike paths early tomorrow morning.",
                                "ru": "Муниципалитет распорядился завтра рано утром <w>убрать</w> снег с велосипедных дорожек."
                            }
                        }
                    ],
                    "synonyms": [
                        "leegmaken",
                        "ontruimen",
                        "verwijderen"
                    ],
                    "common_phrases": [
                        "puin ruimen",
                        "sneeuw ruimen",
                        "het veld ruimen"
                    ],
                    "target_lang_definitions": {
                        "en": "To empty something or remove obstacles to make space.",
                        "ru": "Освобождать пространство, убирая препятствия, мусор или снег."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "remove obstacles or contents"
                            },
                            {
                                "target_lang_word": "clear away",
                                "target_lang_sense_clarification": "remove completely"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "расчищать",
                                "target_lang_sense_clarification": "освобождать от чего-либо (снега, завалов)"
                            },
                            {
                                "target_lang_word": "убирать",
                                "target_lang_sense_clarification": "удалять мусор или препятствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36b8bc20-47c7-38ce-a74f-b1549de81330",
                    "sense_definition": "Het doden van vee om de verspreiding van een besmettelijke ziekte te voorkomen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "culling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vanwege de vogelgriep moest de boer al zijn kippen laten <w>ruimen</w>.",
                            "target_lang_translations": {
                                "en": "Due to bird flu, the farmer had to have all his chickens <w>culled</w>.",
                                "ru": "Из-за птичьего гриппа фермеру пришлось <w>уничтожить</w> всех своих кур."
                            }
                        },
                        {
                            "text": "De overheid besloot om de besmette varkens direct preventief te <w>ruimen</w>.",
                            "target_lang_translations": {
                                "en": "The government decided to immediately <w>cull</w> the infected pigs as a preventive measure.",
                                "ru": "Правительство решило немедленно превентивно <w>забить</w> зараженных свиней."
                            }
                        }
                    ],
                    "synonyms": [
                        "afmaken"
                    ],
                    "common_phrases": [
                        "preventief ruimen",
                        "dieren ruimen"
                    ],
                    "target_lang_definitions": {
                        "en": "To kill livestock to prevent the spread of an infectious disease.",
                        "ru": "Забивать (уничтожать) скот для предотвращения распространения инфекционных заболеваний."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cull",
                                "target_lang_sense_clarification": "animals"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "забивать",
                                "target_lang_sense_clarification": "убивать животных (в санитарных целях)"
                            },
                            {
                                "target_lang_word": "уничтожать",
                                "target_lang_sense_clarification": "ликвидировать поголовье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89a8541d-61f2-3e28-87e8-90e9738003ec",
                    "sense_definition": "Het leegmaken van een graf nadat de grafrechten zijn verlopen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "clearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op deze oude begraafplaats worden binnenkort enkele graven <w>geruimd</w>.",
                            "target_lang_translations": {
                                "en": "Some graves at this old cemetery will soon be <w>cleared</w>.",
                                "ru": "На этом старом кладбище вскоре будут <w>ликвидированы</w> несколько могил."
                            }
                        },
                        {
                            "text": "De familie werd geïnformeerd dat het graf na twintig jaar wordt <w>geruimd</w>.",
                            "target_lang_translations": {
                                "en": "The family was informed that the grave will be <w>cleared</w> after twenty years.",
                                "ru": "Семье сообщили, что могила будет <w>ликвидирована</w> через двадцать лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een graf ruimen"
                    ],
                    "target_lang_definitions": {
                        "en": "To clear a grave after burial rights have expired.",
                        "ru": "Освобождать место захоронения (могилу) после истечения срока аренды или прав на него."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clear",
                                "target_lang_sense_clarification": "a grave"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ликвидировать",
                                "target_lang_sense_clarification": "убирать могилу"
                            },
                            {
                                "target_lang_word": "сносить",
                                "target_lang_sense_clarification": "удалять надгробие и освобождать место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bdff5d9-26c8-3b02-bb04-d138321d94c9",
                    "sense_definition": "Het veranderen van de windrichting met de wijzers van de klok mee.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "wind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De weerman voorspelde dat de wind in de middag zou gaan <w>ruimen</w>.",
                            "target_lang_translations": {
                                "en": "The weatherman predicted that the wind would <w>veer</w> in the afternoon.",
                                "ru": "Метеоролог предсказал, что во второй половине дня ветер начнет <w>заходить</w>."
                            }
                        },
                        {
                            "text": "Als de wind gaat <w>ruimen</w>, draait hij van het zuiden naar het westen.",
                            "target_lang_translations": {
                                "en": "When the wind <w>veers</w>, it turns from the south to the west.",
                                "ru": "Когда ветер начинает <w>заходить</w>, он поворачивает с юга на запад."
                            }
                        }
                    ],
                    "antonyms": [
                        "krimpen"
                    ],
                    "common_phrases": [
                        "de wind ruimt"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "meteorologie"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "scheepvaart"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To change wind direction clockwise.",
                        "ru": "Изменение направления ветра по часовой стрелке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "veer",
                                "target_lang_sense_clarification": "wind direction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заходить",
                                "target_lang_sense_clarification": "о ветре: меняться по часовой стрелке"
                            },
                            {
                                "target_lang_word": "поворачивать по солнцу",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        },

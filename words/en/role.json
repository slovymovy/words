    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9cbecdce-3c24-31a7-bb63-6e3202e1c6ac",
                    "sense_definition": "A character or part played by a performer or actor in a play, movie, or show.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "acting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She practiced hard to get the lead <w>role</w> in the school play.",
                            "target_lang_translations": {
                                "ru": "Она усердно репетировала, чтобы получить главную <w>роль</w> в школьном спектакле."
                            }
                        },
                        {
                            "text": "The actor became famous for his <w>role</w> as a detective in the series.",
                            "target_lang_translations": {
                                "ru": "Актер прославился своей <w>ролью</w> детектива в сериале."
                            }
                        }
                    ],
                    "synonyms": [
                        "part",
                        "character"
                    ],
                    "common_phrases": [
                        "lead role",
                        "title role",
                        "starring role",
                        "supporting role"
                    ],
                    "target_lang_definitions": {
                        "ru": "Персонаж или партия, исполняемая артистом или актером в пьесе, фильме или шоу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роль",
                                "target_lang_sense_clarification": "персонаж в пьесе или фильме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0a84856-c1d7-3aa4-8708-69d9439948e2",
                    "sense_definition": "The function or position that something has, or the part it plays in an activity, event, or process.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Diet plays a major <w>role</w> in maintaining overall health.",
                            "target_lang_translations": {
                                "ru": "Диета играет важную <w>роль</w> в поддержании общего здоровья."
                            }
                        },
                        {
                            "text": "Local volunteers played an important <w>role</w> in cleaning the beach after the storm.",
                            "target_lang_translations": {
                                "ru": "Местные волонтеры сыграли важную <w>роль</w> в уборке пляжа после шторма."
                            }
                        }
                    ],
                    "synonyms": [
                        "function",
                        "part",
                        "contribution"
                    ],
                    "common_phrases": [
                        "play a role",
                        "key role",
                        "important role",
                        "major role"
                    ],
                    "target_lang_definitions": {
                        "ru": "Функция или положение, которое что-либо занимает, или участие, которое оно принимает в деятельности, событии или процессе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роль",
                                "target_lang_sense_clarification": "значение или участие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "860fcde8-a89f-3dc0-9d59-b7f310189a67",
                    "sense_definition": "A set of responsibilities, tasks, or duties associated with a specific position, job, or situation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He stepped into a leadership <w>role</w> after the previous manager left the company.",
                            "target_lang_translations": {
                                "ru": "Он взял на себя руководящую <w>роль</w> после того, как предыдущий менеджер покинул компанию."
                            }
                        },
                        {
                            "text": "My <w>role</w> involves managing the team and tracking the project's progress.",
                            "target_lang_translations": {
                                "ru": "Моя <w>роль</w> включает в себя управление командой и отслеживание прогресса проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "position",
                        "job",
                        "post",
                        "capacity",
                        "duty"
                    ],
                    "common_phrases": [
                        "leadership role",
                        "advisory role",
                        "management role",
                        "dual role"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор обязанностей, задач или функций, связанных с определенной должностью, работой или ситуацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до́лжность",
                                "target_lang_sense_clarification": "служебное положение"
                            },
                            {
                                "target_lang_word": "роль",
                                "target_lang_sense_clarification": "круг обязанностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc21fd2a-ce8a-3cfa-9c5c-ae064f0ae8e0",
                    "sense_definition": "The expected behaviour of an individual in a society or social setting.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "society",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Traditional gender <w>roles</w> have evolved significantly over the last century.",
                            "target_lang_translations": {
                                "ru": "Традиционные гендерные <w>роли</w> существенно изменились за последнее столетие."
                            }
                        },
                        {
                            "text": "She questioned her <w>role</w> within the family business structure.",
                            "target_lang_translations": {
                                "ru": "Она сомневалась в своей <w>роли</w> в структуре семейного бизнеса."
                            }
                        }
                    ],
                    "synonyms": [
                        "function"
                    ],
                    "common_phrases": [
                        "gender roles",
                        "social role",
                        "role model"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ожидаемое поведение индивида в обществе или социальной среде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роль",
                                "target_lang_sense_clarification": "социальное поведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72d7f307-50e5-3074-a563-c0a8691851bf",
                    "sense_definition": "The grammatical or semantic function of a word in a phrase.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this sentence, the noun phrase functions in the <w>role</w> of the subject.",
                            "target_lang_translations": {
                                "ru": "В этом предложении именная группа выступает в <w>роли</w> подлежащего."
                            }
                        },
                        {
                            "text": "We analyzed the semantic <w>role</w> of each argument in the clause.",
                            "target_lang_translations": {
                                "ru": "Мы проанализировали семантическую <w>роль</w> каждого аргумента в предложении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "semantic role",
                        "thematic role"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая или семантическая функция слова в фразе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роль",
                                "target_lang_sense_clarification": "функция слова"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "315780e1-b17d-3838-a0dc-4f6894698ac3",
                    "sense_definition": "In the Raku programming language, a code element akin to an interface, used for composition of classes.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The developer defined a new <w>role</w> to share behavior across multiple classes.",
                            "target_lang_translations": {
                                "ru": "Разработчик определил новую <w>роль</w> для совместного использования поведения в нескольких классах."
                            }
                        },
                        {
                            "text": "Using a <w>role</w> allows for flexible code composition without deep inheritance trees.",
                            "target_lang_translations": {
                                "ru": "Использование <w>роли</w> позволяет гибко компоновать код без глубоких деревьев наследования."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В языке программирования Raku: элемент кода, похожий на интерфейс, используемый для композиции классов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роль",
                                "target_lang_sense_clarification": "элемент кода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb24401e-7908-3f34-ab05-0bcdbc326cda",
                    "sense_definition": "An ancient unit of quantity consisting of 72 sheets of parchment.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scribe ordered a <w>role</w> of parchment to complete the manuscript.",
                            "target_lang_translations": {
                                "ru": "Писец заказал <w>свиток</w> пергамента, чтобы закончить рукопись."
                            }
                        },
                        {
                            "text": "Historical records mention the purchase of one <w>role</w> of paper for the office.",
                            "target_lang_translations": {
                                "ru": "В исторических записях упоминается покупка одного <w>рулона</w> бумаги для канцелярии."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная единица количества, состоящая из 72 листов пергамента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сви́ток",
                                "target_lang_sense_clarification": "старинная мера"
                            },
                            {
                                "target_lang_word": "руло́н",
                                "target_lang_sense_clarification": "сверток"
                            }
                        ]
                    }
                }
            ]
        }
    ]

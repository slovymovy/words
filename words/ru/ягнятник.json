                    ],
                    "common_phrases": [
                        "горный ягнятник",
                        "парящий ягнятник"
                    ],
                    "target_lang_definitions": {
                        "en": "A large bird of prey of the hawk family, inhabiting mountains and feeding primarily on carrion."
                    },

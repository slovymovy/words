{
    "word_family": [
        "smrodek",
        "smrodzik",
        "smrodny",
        "śmierdzieć",
        "śmierdziel"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "188ceb33-db17-37cc-9f26-9670fcda3c6c",
                    "sense_definition": "Bardzo nieprzyjemny, intensywny i odpychający zapach.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "smell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W całym pomieszczeniu unosił się nieznośny <w>smród</w> spalenizny.",
                            "target_lang_translations": {
                                "ru": "Во всём помещении стояла невыносимая <w>вонь</w> гари."
                            }
                        },
                        {
                            "text": "Nie wytrzymałem tego <w>smrodu</w> i musiałem natychmiast wyjść na zewnątrz.",
                            "target_lang_translations": {
                                "ru": "Я не выдержал этого <w>смрада</w> и был вынужден немедленно выйти на улицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "fetor",
                        "odór",
                        "swąd",
                        "sztynk"
                    ],
                    "antonyms": [
                        "aromat",
                        "wonność"
                    ],
                    "common_phrases": [
                        "straszny smród",
                        "czuć smród",
                        "nieznośny smród"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень неприятный, интенсивный и отталкивающий запах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вонь",
                                "target_lang_sense_clarification": "самое общее слово для неприятного запаха"
                            },
                            {
                                "target_lang_word": "смрад",
                                "target_lang_sense_clarification": "сильный, удушливый, отвратительный запах"
                            },
                            {
                                "target_lang_word": "зловоние",
                                "target_lang_sense_clarification": "формальный или книжный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f546b0d-f4e9-38cf-8b9b-00d3da80e0aa",
                    "sense_definition": "Nieprzyjemna atmosfera, skandal lub zamieszanie wokół jakiejś sprawy, wywołujące negatywne emocje.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "atmosphere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nagłe zwolnienie narobiło sporo <w>smrodu</w> w całej firmie.",
                            "target_lang_translations": {
                                "ru": "Его внезапное увольнение наделало много <w>шума</w> во всей фирме."
                            }
                        },
                        {
                            "text": "Dziennikarze wyciągnęli stare brudy i wokół polityka zrobił się niezły <w>smród</w>.",
                            "target_lang_translations": {
                                "ru": "Журналисты вытащили старое грязное белье, и вокруг политика поднялась изрядная <w>шумиха</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "narobić smrodu",
                        "smród wokół sprawy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятная атмосфера, скандал или шумиха вокруг какого-либо дела, вызывающие негативные эмоции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шумиха",
                                "target_lang_sense_clarification": "громкое обсуждение, суматоха"
                            },
                            {
                                "target_lang_word": "скандал",
                                "target_lang_sense_clarification": "публичный инцидент"
                            },
                            {
                                "target_lang_word": "вонь",
                                "target_lang_sense_clarification": "перен. разг. неприятные сплетни или разборки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ede3521-597d-30bd-8cb3-15ba67616526",
                    "sense_definition": "Lekceważące lub żartobliwe określenie dziecka (zazwyczaj niegrzecznego) lub osoby bardzo młodej.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uciekaj stąd, ty mały <w>smrodzie</w>, bo zawołam twoich rodziców.",
                            "target_lang_translations": {
                                "ru": "Убирайся отсюда, ты мелкий <w>сопляк</w>, иначе я позову твоих родителей."
                            }
                        },
                        {
                            "text": "Co wolno wojewodzie, to nie tobie <w>smrodzie</w>.",
                            "target_lang_translations": {
                                "ru": "Что позволено воеводе, то не тебе, <w>сопляк</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ty mały smrodzie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często obraźliwe lub lekceważące"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пренебрежительное или шутливое название ребенка (обычно непослушного) или очень молодого человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сопляк",
                                "target_lang_sense_clarification": "грубо о молодом, неопытном человеке"
                            },
                            {
                                "target_lang_word": "мелочь",
                                "target_lang_sense_clarification": "пренебрежительно о детях"
                            },
                            {
                                "target_lang_word": "шкет",
                                "target_lang_sense_clarification": "разг. мальчишка, пацан"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
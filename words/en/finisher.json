{
    "word_family": [
        "finish",
        "finished",
        "finishing",
        "unfinished"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "71546a44-106c-33b2-ade5-c0334e492bca",
                    "sense_definition": "A person who reaches the end of a race or competition.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_completes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The top <w>finishers</w> in the marathon received gold medals.",
                            "target_lang_translations": {
                                "ru": "Лучшие <w>финишировавшие</w> в марафоне получили золотые медали."
                            }
                        },
                        {
                            "text": "She is a strong <w>finisher</w> and often overtakes others in the final lap.",
                            "target_lang_translations": {
                                "ru": "Она сильный <w>финишер</w> и часто обгоняет других на последнем круге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "top finisher",
                        "podium finisher",
                        "fast finisher"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который доходит до финиша гонки или соревнования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "финишировавший",
                                "target_lang_sense_clarification": "участник, дошедший до финиша"
                            },
                            {
                                "target_lang_word": "финишер",
                                "target_lang_sense_clarification": "спортсмен с сильным рывком на финише"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e6ca60f-528e-304f-821b-88887f9614f9",
                    "sense_definition": "A player in sports who is skilled at scoring goals or points.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sports_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team needs a clinical <w>finisher</w> to turn these opportunities into goals.",
                            "target_lang_translations": {
                                "ru": "Команде нужен хладнокровный <w>завершитель атак</w>, чтобы превращать эти моменты в голы."
                            }
                        },
                        {
                            "text": "He is known as a composed <w>finisher</w> inside the penalty box.",
                            "target_lang_translations": {
                                "ru": "Он известен как спокойный <w>завершитель</w> в штрафной площадке."
                            }
                        }
                    ],
                    "synonyms": [
                        "scorer"
                    ],
                    "common_phrases": [
                        "clinical finisher",
                        "composed finisher",
                        "natural finisher"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрок в спорте, умеющий эффективно завершать атаки и забивать голы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завершитель атак",
                                "target_lang_sense_clarification": "игрок, реализующий моменты"
                            },
                            {
                                "target_lang_word": "бомбардир",
                                "target_lang_sense_clarification": "игрок, забивающий много голов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "794d2f63-6d37-36c9-a2c6-1f266c920a3d",
                    "sense_definition": "A skilled worker who applies the final surface treatment to materials like furniture, walls, or concrete.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "trade_worker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The furniture <w>finisher</w> applied a smooth coat of varnish to the antique table.",
                            "target_lang_translations": {
                                "ru": "Мебельный <w>отделочник</w> нанес ровный слой лака на антикварный стол."
                            }
                        },
                        {
                            "text": "We hired a professional drywall <w>finisher</w> to prepare the walls for painting.",
                            "target_lang_translations": {
                                "ru": "Мы наняли профессионального <w>отделочника</w> гипсокартона, чтобы подготовить стены к покраске."
                            }
                        }
                    ],
                    "common_phrases": [
                        "concrete finisher",
                        "drywall finisher",
                        "furniture finisher"
                    ],
                    "target_lang_definitions": {
                        "ru": "Квалифицированный рабочий, выполняющий финишную отделку поверхностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отделочник",
                                "target_lang_sense_clarification": "рабочий по отделке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bdc7a36-29f5-38bf-86af-7e28fd146c61",
                    "sense_definition": "A powerful move used to defeat an opponent or end a match, especially in video games or wrestling.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "game_move",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wrestler executed his signature <w>finisher</w> to win the championship belt.",
                            "target_lang_translations": {
                                "ru": "Рестлер выполнил свой коронный <w>финишер</w>, чтобы выиграть чемпионский пояс."
                            }
                        },
                        {
                            "text": "You must use a combo <w>finisher</w> to defeat the final boss.",
                            "target_lang_translations": {
                                "ru": "Вы должны использовать комбо-<w>финишер</w>, чтобы победить финального босса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "signature finisher",
                        "combo finisher"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мощный прием, используемый для победы над противником или завершения матча."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "финишер",
                                "target_lang_sense_clarification": "коронный или добивающий прием"
                            },
                            {
                                "target_lang_word": "добивающий удар",
                                "target_lang_sense_clarification": "финальный удар"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7773f01-4d0e-3813-8007-d774b91d658d",
                    "sense_definition": "An artist who completes the ink drawings over pencil sketches in comic book production.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "art_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>finisher</w> added depth and shadow to the original pencil lines.",
                            "target_lang_translations": {
                                "ru": "<w>Контуровщик</w> добавил глубину и тени к исходным карандашным линиям."
                            }
                        },
                        {
                            "text": "He works as a <w>finisher</w> for a major comic book publisher.",
                            "target_lang_translations": {
                                "ru": "Он работает <w>контуровщиком</w> в крупном издательстве комиксов."
                            }
                        }
                    ],
                    "synonyms": [
                        "inker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Художник, который обводит тушью карандашные наброски при создании комиксов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контуровщик",
                                "target_lang_sense_clarification": "художник по контуру"
                            },
                            {
                                "target_lang_word": "инкер",
                                "target_lang_sense_clarification": "профессиональный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98c46c3a-327a-3255-8100-16549457c37a",
                    "sense_definition": "A construction machine used to smooth and level a newly laid road surface.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "machine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The road crew used a heavy <w>finisher</w> to ensure the asphalt was perfectly flat.",
                            "target_lang_translations": {
                                "ru": "Дорожная бригада использовала тяжелый <w>асфальтоукладчик</w>, чтобы асфальт был идеально ровным."
                            }
                        },
                        {
                            "text": "A mechanical <w>finisher</w> followed the paver down the highway.",
                            "target_lang_translations": {
                                "ru": "Механическая <w>отделочная машина</w> следовала за укладчиком по шоссе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "asphalt finisher",
                        "road finisher"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строительная машина для выравнивания и укладки дорожного покрытия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "асфальтоукладчик",
                                "target_lang_sense_clarification": "дорожная машина"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
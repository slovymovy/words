                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что-то такое",
                                "target_lang_sense_clarification": "используется перед прилагательным для усиления"
                            },
                            {
                                "target_lang_word": "нечто столь",
                                "target_lang_sense_clarification": "более литературный вариант усиления качества"

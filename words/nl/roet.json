    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d29e64d7-22e1-31ee-80a5-b71ae58d0b97",
                    "sense_definition": "Zwarte neerslag van koolstofdeeltjes die ontstaat bij onvolledige verbranding van organisch materiaal.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schoorsteenveger verwijderde al het <w>roet</w> uit de pijp.",
                            "target_lang_translations": {
                                "ru": "Трубочист удалил всю <w>сажу</w> из трубы.",
                                "en": "The chimney sweep removed all the <w>soot</w> from the pipe."
                            }
                        },
                        {
                            "text": "Na de brand zat zijn gezicht helemaal onder het <w>roet</w>.",
                            "target_lang_translations": {
                                "ru": "После пожара его лицо было полностью покрыто <w>сажей</w>.",
                                "en": "After the fire, his face was completely covered in <w>soot</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "roet in het eten gooien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чёрный налет из частиц углерода, образующийся при неполном сгорании органических веществ.",
                        "en": "Black precipitate of carbon particles formed by the incomplete combustion of organic material."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сажа",
                                "target_lang_sense_clarification": "чёрный налет"
                            },
                            {
                                "target_lang_word": "копоть",
                                "target_lang_sense_clarification": "осевший дым"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "soot",
                                "target_lang_sense_clarification": "black carbon residue"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d3e951c-d9db-3c09-9085-813250b3ca18",
                    "sense_definition": "Een plantenziekte veroorzaakt door schimmels, waarbij zwarte sporen ontstaan die op roet lijken.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oogst van dit jaar is grotendeels verloren gegaan door <w>roet</w>.",
                            "target_lang_translations": {
                                "ru": "Урожай этого года был в значительной степени потерян из-за <w>головни</w>.",
                                "en": "This year's harvest was largely lost due to <w>smut</w>."
                            }
                        },
                        {
                            "text": "De boer controleerde het graan op tekenen van <w>roet</w>.",
                            "target_lang_translations": {
                                "ru": "Фермер проверял зерно на наличие признаков <w>головни</w>.",
                                "en": "The farmer checked the grain for signs of <w>smut</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plant pathology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Болезнь растений, вызываемая грибками, при которой образуются черные споры, напоминающие сажу.",
                        "en": "A plant disease caused by fungi, producing black spores that resemble soot."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "головня",
                                "target_lang_sense_clarification": "болезнь злаков"
                            },
                            {
                                "target_lang_word": "чернь",
                                "target_lang_sense_clarification": "грибковое заболевание"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "smut",
                                "target_lang_sense_clarification": "fungal disease in grains"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "628c5b6a-14c4-3f53-b692-9f3f2cf10c22",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>roeten</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>roet</w> het glas voorzichtig boven de kaarsvlam.",

                    "sense_definition": "Potoczne określenie na mężczyznę zachowującego się agresywnie, chuligana lub przestępcę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie kumpluję się już z nią, odkąd wiem, że jej chłopak to straszny <w>zbój</w>.",
                            "target_lang_translations": {
                                "ru": "Я больше не общаюсь с ней, с тех пор как узнала, что её парень — жуткий <w>бандит</w>."
                            }
                        },
                        {
                            "text": "Ten <w>zbój</w> znowu wszczął awanturę pod sklepem i zaczepiał przechodniów.",
                            "target_lang_translations": {
                                "ru": "Этот <w>бандюга</w> снова устроил скандал у магазина и приставал к прохожим."
                            }
                        }
                    ],
                    "synonyms": [
                        "bandzior",
                        "oprych",
                        "zbir",
                        "bandyta"
                    ],
                    "common_phrases": [
                        "straszny zbój",
                        "wyglądać jak zbój",
                        "kawał zbója"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название агрессивного мужчины, хулигана или преступника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бандит",
                                "target_lang_sense_clarification": "преступник, злоумышленник"
                            },
                            {
                                "target_lang_word": "бандюга",
                                "target_lang_sense_clarification": "разговорное, грубое"
                            },
                            {
                                "target_lang_word": "хулиган",
                                "target_lang_sense_clarification": "агрессивный человек, дебошир"
                            }
                        ]

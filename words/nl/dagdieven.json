            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b2481744-956c-3b0e-a8c8-2346cd22eb9e",
                    "sense_definition": "Geen nuttige activiteiten ondernemen; de tijd nutteloos doorbrengen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze hebben de hele dag in het park <w>gedagdiefd</w>.",
                            "target_lang_translations": {
                                "en": "They <w>idled</w> away the whole day in the park.",
                                "ru": "Они весь день <w>бездельничали</w> в парке."
                            }
                        },
                        {
                            "text": "Hij zit de hele middag te <w>dagdieven</w> in plaats van zijn huiswerk te maken.",
                            "target_lang_translations": {
                                "en": "He has been <w>slacking off</w> all afternoon instead of doing his homework.",
                                "ru": "Он весь день <w>лодырничает</w> вместо того, чтобы делать уроки."
                            }
                        }
                    ],
                    "synonyms": [
                        "luieren",
                        "niksen",
                        "nietsdoen"
                    ],
                    "antonyms": [
                        "werken"
                    ],
                    "common_phrases": [
                        "zitten te dagdieven"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend time idly; to waste time doing nothing useful.",
                        "ru": "Проводить время в праздности; не заниматься ничем полезным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to idle",
                                "target_lang_sense_clarification": "to spend time doing nothing"
                            },
                            {
                                "target_lang_word": "to loaf",
                                "target_lang_sense_clarification": "to spend time in a lazy way"
                            },
                            {
                                "target_lang_word": "to slack off",
                                "target_lang_sense_clarification": "to avoid work or effort"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бездельничать",
                                "target_lang_sense_clarification": "проводить время без дела"
                            },
                            {
                                "target_lang_word": "лодырничать",
                                "target_lang_sense_clarification": "лениться, уклоняться от дела"
                            },
                            {
                                "target_lang_word": "лентяйничать",
                                "target_lang_sense_clarification": "проявлять лень"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7109c939-d745-3c93-a698-b87e991ba5d0",

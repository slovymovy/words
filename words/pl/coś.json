{
    "word_family": [
        "cokolwiek",
        "cosik"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "bd1228a5-7ce1-382b-853d-6692b3816827",
                    "sense_definition": "Zaimek zastępujący nazwę przedmiotu, zjawiska, sytuacji lub sprawy, której mówiący nie chce lub nie potrafi precyzyjnie określić.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "indefinite_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę ci <coś> powiedzieć w wielkiej tajemnicy.",
                            "target_lang_translations": {
                                "ru": "Я должен тебе <кое-что> сказать по большому секрету."
                            }
                        },
                        {
                            "text": "Czy masz ochotę zjeść <coś> słodkiego?",
                            "target_lang_translations": {
                                "ru": "Хочешь съесть <что-нибудь> сладенькое?"
                            }
                        }
                    ],
                    "synonyms": [
                        "cosik"
                    ],
                    "antonyms": [
                        "nic",
                        "ktoś"
                    ],
                    "common_phrases": [
                        "coś takiego",
                        "coś innego",
                        "coś w tym stylu",
                        "coś do jedzenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Местоимение, заменяющее название предмета, явления, ситуации или дела, которое говорящий не хочет или не может точно определить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что-то",
                                "target_lang_sense_clarification": "нечто конкретное, но не названное"
                            },
                            {
                                "target_lang_word": "что-нибудь",
                                "target_lang_sense_clarification": "нечто неопределённое, любой предмет"
                            },
                            {
                                "target_lang_word": "кое-что",
                                "target_lang_sense_clarification": "известное говорящему, но скрываемое от слушателя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9a6b630-19f4-3805-93c0-ae84abb09b03",
                    "sense_definition": "Rzecz, zjawisko lub osiągnięcie uważane za wyjątkowe, wartościowe lub godne podziwu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "notable_thing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej występ na scenie to było naprawdę <coś>.",
                            "target_lang_translations": {
                                "ru": "Её выступление на сцене — это было действительно <нечто>."
                            }
                        },
                        {
                            "text": "Mieć taki talent to jest <coś>.",
                            "target_lang_translations": {
                                "ru": "Иметь такой талант — это <нечто>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "to jest coś",
                        "to było coś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to express admiration or emphasis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещь, явление или достижение, считающееся исключительным, ценным или достойным восхищения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нечто",
                                "target_lang_sense_clarification": "что-то выдающееся, особенное"
                            },
                            {
                                "target_lang_word": "что-то",
                                "target_lang_sense_clarification": "в значении «нечто особенное»"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "889b5b0e-ea95-3441-903f-dce1f1fbe5ff",
                    "sense_definition": "Słowo używane do wyrażenia przybliżenia, niepewności co do czasu, ilości lub cechy; mniej więcej, około.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "approximation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary zamek powstał <coś> w czternastym wieku.",
                            "target_lang_translations": {
                                "ru": "Этот старый замок был построен <где-то> в четырнадцатом веке."
                            }
                        },
                        {
                            "text": "Wyglądasz dzisiaj <coś> smutno i niewyraźnie.",
                            "target_lang_translations": {
                                "ru": "Ты выглядишь сегодня <что-то> грустно и неважно."
                            }
                        }
                    ],
                    "synonyms": [
                        "gdzieś",
                        "jakoś"
                    ],
                    "common_phrases": [
                        "coś koło",
                        "coś jakby"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое для выражения приблизительности, неуверенности во времени, количестве или качестве; примерно, около."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "примерно",
                                "target_lang_sense_clarification": "около, приблизительно"
                            },
                            {
                                "target_lang_word": "где-то",
                                "target_lang_sense_clarification": "разг. приблизительно (о времени или числе)"
                            },
                            {
                                "target_lang_word": "что-то",
                                "target_lang_sense_clarification": "в некоторой степени, слегка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2a063ef-483e-3c32-9f69-db9e4808fc27",
                    "sense_definition": "Słowo wskazujące na nieznaną lub nieokreśloną przyczynę; z jakiegoś powodu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ostatnio <coś> nie mogę się skupić na pracy.",
                            "target_lang_translations": {
                                "ru": "В последнее время я <что-то> не могу сосредоточиться на работе."
                            }
                        },
                        {
                            "text": "Silnik samochodu <coś> dziwnie stuka podczas jazdy.",
                            "target_lang_translations": {
                                "ru": "Двигатель машины <почему-то> странно стучит во время езды."
                            }
                        }
                    ],
                    "synonyms": [
                        "czemuś",
                        "dlaczegoś",
                        "z jakiegoś powodu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, указывающее на неизвестную или неопределенную причину; по какой-то причине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что-то",
                                "target_lang_sense_clarification": "по неизвестной причине"
                            },
                            {
                                "target_lang_word": "почему-то",
                                "target_lang_sense_clarification": "по какой-то причине"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d4e2895c-765c-38cf-bd6c-86ff86fe4fa9",
                    "sense_definition": "Przedmiot, istota lub zjawisko, którego nazwa jest nieznana, zapomniana lub trudna do określenia (traktowane jako odmienny rzeczownik).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na półce stały różne kolorowe <cosie>, których przeznaczenia nie znałem.",
                            "target_lang_translations": {
                                "ru": "На полке стояли разные цветные <штуки>, назначения которых я не знал."
                            }
                        },
                        {
                            "text": "To małe <coś> służy do otwierania butelek.",
                            "target_lang_translations": {
                                "ru": "Это маленькое <нечто> служит для открывания бутылок."
                            }
                        }
                    ],
                    "synonyms": [
                        "cosik"
                    ],
                    "common_phrases": [
                        "małe coś",
                        "dziwne coś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, существо или явление, название которого неизвестно, забыто или трудноопределимо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нечто",
                                "target_lang_sense_clarification": "неопределенный предмет"
                            },
                            {
                                "target_lang_word": "штука",
                                "target_lang_sense_clarification": "разг. предмет, название которого неизвестно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "explosion",
        "explosive"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2db8da8-f3c8-3906-b7d0-53ecbeaf25c9",
                    "sense_definition": "To burst or shatter violently and noisily, often due to internal pressure or combustion.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_explosion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fireworks <w>exploded</w> in the night sky with brilliant colors.",
                            "target_lang_translations": {
                                "ru": "Фейерверки <w>взорвались</w> в ночном небе яркими красками."
                            }
                        },
                        {
                            "text": "Gas pipes can <w>explode</w> if they are damaged and leak.",
                            "target_lang_translations": {
                                "ru": "Газовые трубы могут <w>взорваться</w>, если они повреждены и протекают."
                            }
                        }
                    ],
                    "synonyms": [
                        "blow up",
                        "burst",
                        "detonate",
                        "go off"
                    ],
                    "antonyms": [
                        "implode"
                    ],
                    "common_phrases": [
                        "bomb exploded",
                        "explode with a bang",
                        "ready to explode"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрываться на части или разрушаться с сильным шумом и силой, часто из-за внутреннего давления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрываться",
                                "target_lang_sense_clarification": "несовершенный вид"
                            },
                            {
                                "target_lang_word": "взорваться",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38e9d5ec-e304-3143-9b07-28df339adc41",
                    "sense_definition": "To increase suddenly and rapidly in number, amount, or extent.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city's population has <w>exploded</w> in the last ten years.",
                            "target_lang_translations": {
                                "ru": "Население города <w>стремительно выросло</w> за последние десять лет."
                            }
                        },
                        {
                            "text": "Demand for the new smartphone <w>exploded</w> immediately after the launch.",
                            "target_lang_translations": {
                                "ru": "Спрос на новый смартфон <w>резко возрос</w> сразу после запуска."
                            }
                        }
                    ],
                    "synonyms": [
                        "blow up",
                        "burst",
                        "go off",
                        "detonate"
                    ],
                    "common_phrases": [
                        "population exploded",
                        "prices exploded",
                        "sales exploded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно и стремительно увеличиваться в количестве, объеме или степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резко возрастать",
                                "target_lang_sense_clarification": "о количестве или ценах"
                            },
                            {
                                "target_lang_word": "стремительно расти",
                                "target_lang_sense_clarification": "об объеме или спросе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2b77040-e9fa-3288-9a29-2bc368cb0b46",
                    "sense_definition": "To cause something to burst or shatter violently using explosives.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_explosion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The demolition crew <w>exploded</w> the old bridge to make way for a new one.",
                            "target_lang_translations": {
                                "ru": "Команда по сносу <w>взорвала</w> старый мост, чтобы освободить место для нового."
                            }
                        },
                        {
                            "text": "They plan to <w>explode</w> the device safely in a controlled environment.",
                            "target_lang_translations": {
                                "ru": "Они планируют безопасно <w>взорвать</w> устройство в контролируемой среде."
                            }
                        }
                    ],
                    "synonyms": [
                        "blow up",
                        "detonate",
                        "set off"
                    ],
                    "common_phrases": [
                        "explode a bomb",
                        "explode a device"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять что-либо разорваться или разрушиться, используя взрывчатые вещества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрывать",
                                "target_lang_sense_clarification": "разрушать взрывом"
                            },
                            {
                                "target_lang_word": "взорвать",
                                "target_lang_sense_clarification": "совершенный вид"
                            },
                            {
                                "target_lang_word": "подрывать",
                                "target_lang_sense_clarification": "закладывать заряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7f4f1f2-1df3-3204-b45a-35a134a4f2d0",
                    "sense_definition": "To express a strong emotion, such as anger or laughter, suddenly and loudly.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My father <w>exploded</w> with anger when he saw the broken window.",
                            "target_lang_translations": {
                                "ru": "Мой отец <w>взорвался</w> от гнева, когда увидел разбитое окно."
                            }
                        },
                        {
                            "text": "The entire audience <w>exploded</w> in laughter at the comedian's joke.",
                            "target_lang_translations": {
                                "ru": "Вся аудитория <w>взорвалась</w> смехом над шуткой комика."
                            }
                        }
                    ],
                    "synonyms": [
                        "blow up"
                    ],
                    "common_phrases": [
                        "explode with anger",
                        "explode in laughter",
                        "explode with rage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно и бурно выражать сильные эмоции, такие как гнев или смех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрываться",
                                "target_lang_sense_clarification": "терять самообладание"
                            },
                            {
                                "target_lang_word": "взорваться",
                                "target_lang_sense_clarification": "разразиться (смехом, гневом)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df7a7992-d434-3cb3-b402-a56dc8e612bd",
                    "sense_definition": "To show that a belief, theory, or myth is false or unfounded.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Modern science has <w>exploded</w> many ancient myths about the universe.",
                            "target_lang_translations": {
                                "ru": "Современная наука <w>развенчала</w> многие древние мифы о вселенной."
                            }
                        },
                        {
                            "text": "The journalist wrote a detailed article to <w>explode</w> the rumors surrounding the celebrity.",
                            "target_lang_translations": {
                                "ru": "Журналист написал подробную статью, чтобы <w>развеять</w> слухи вокруг знаменитости."
                            }
                        }
                    ],
                    "synonyms": [
                        "displode"
                    ],
                    "common_phrases": [
                        "explode a myth",
                        "explode a theory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доказывать, что убеждение, теория или миф являются ложными или необоснованными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развенчивать",
                                "target_lang_sense_clarification": "мифы или легенды"
                            },
                            {
                                "target_lang_word": "опровергать",
                                "target_lang_sense_clarification": "теории или слухи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa428926-bb52-30ba-af70-4b4e49090954",
                    "sense_definition": "To break a string of text into smaller parts based on a specific separator.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The script <w>explodes</w> the sentence into individual words for analysis.",
                            "target_lang_translations": {
                                "ru": "Скрипт <w>разбивает</w> предложение на отдельные слова для анализа."
                            }
                        },
                        {
                            "text": "You can <w>explode</w> the data string using a comma as the delimiter.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>разбить</w> строку данных, используя запятую в качестве разделителя."
                            }
                        }
                    ],
                    "synonyms": [
                        "split"
                    ],
                    "common_phrases": [
                        "explode a string",
                        "explode an array"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разделять строку текста на более мелкие части на основе определенного разделителя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбивать",
                                "target_lang_sense_clarification": "строку на части"
                            },
                            {
                                "target_lang_word": "разделять",
                                "target_lang_sense_clarification": "данные"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
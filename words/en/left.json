    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "08bb0458-c9ae-3d6f-add0-e9ddad1f0758",
                    "sense_definition": "Designating the side of the body that is to the west when one is facing north; the side where the heart is located in most humans.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_side",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most people write with their <w>left</w> hand.",
                            "target_lang_translations": {
                                "ru": "Большинство людей пишут <w>левой</w> рукой."
                            }
                        },
                        {
                            "text": "In the UK, cars drive on the <w>left</w> side of the road.",
                            "target_lang_translations": {
                                "ru": "В Великобритании машины ездят по <w>левой</w> стороне дороги."
                            }
                        }
                    ],
                    "antonyms": [
                        "right"
                    ],
                    "common_phrases": [
                        "left hand",
                        "left side",
                        "left foot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает сторону тела, которая находится к западу, если смотреть на север; сторона, где у большинства людей находится сердце."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левый",
                                "target_lang_sense_clarification": "сторона тела или направление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43e92a46-99c6-3e8e-b674-28ccdda250d2",
                    "sense_definition": "Relating to the political left; supporting social equality and egalitarianism.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She holds <w>left</w> political views.",
                            "target_lang_translations": {
                                "ru": "Она придерживается <w>левых</w> политических взглядов."
                            }
                        },
                        {
                            "text": "The <w>left</w> wing of the party is calling for reform.",
                            "target_lang_translations": {
                                "ru": "<w>Левое</w> крыло партии призывает к реформам."
                            }
                        }
                    ],
                    "synonyms": [
                        "left wing"
                    ],
                    "antonyms": [
                        "right"
                    ],
                    "common_phrases": [
                        "left wing",
                        "political left"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к политическим левым; поддерживающий социальное равенство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левый",
                                "target_lang_sense_clarification": "политические взгляды"
                            }
                        ]
                    }
                }
            ]
        },
        {

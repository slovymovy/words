    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f58173f-53e0-353f-828c-29c318628b7a",
                    "sense_definition": "The elected or appointed head of the government of a city, town, or other municipality.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "civic_office",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>mayor</w> promised to improve the public parks during his term.",
                            "target_lang_translations": {
                                "ru": "<w>Мэр</w> пообещал улучшить состояние общественных парков во время своего срока."
                            }
                        },
                        {
                            "text": "She decided to run for <w>mayor</w> after serving on the city council for five years.",
                            "target_lang_translations": {
                                "ru": "Она решила баллотироваться в <w>мэры</w> после пяти лет работы в городском совете."
                            }
                        }
                    ],
                    "synonyms": [
                        "provost",
                        "burgomaster"
                    ],
                    "common_phrases": [
                        "run for mayor",
                        "elected mayor",
                        "city mayor",
                        "deputy mayor",
                        "former mayor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Избранный или назначенный глава администрации города или другого муниципального образования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мэр",
                                "target_lang_sense_clarification": "глава городской администрации"
                            },
                            {
                                "target_lang_word": "градоначальник",
                                "target_lang_sense_clarification": "официальное или устаревшее название главы города"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64a53e7d-cdae-391f-9eda-b6ff5c6ac660",
                    "sense_definition": "An informal title for a prominent, influential, or highly visible person within a specific local group or neighborhood.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He knows everyone on this street so well that we call him the <w>mayor</w> of the block.",
                            "target_lang_translations": {
                                "ru": "Он так хорошо знает всех на этой улице, что мы называем его <w>мэром</w> квартала."
                            }
                        },
                        {
                            "text": "She acts like the <w>mayor</w> of the office, always organizing events and settling disputes.",
                            "target_lang_translations": {
                                "ru": "Она ведет себя как <w>мэр</w> офиса, постоянно организуя мероприятия и улаживая споры."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used humorously or affectionately"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальный титул для видного, влиятельного человека в определенной группе или районе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мэр",
                                "target_lang_sense_clarification": "шутливое прозвище для влиятельного человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d34d5f6-b61a-3ca0-bbb3-477f32dce379",
                    "sense_definition": "The chief steward or head official of the royal household in the historical Frankish Empire.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_office",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>mayor</w> of the palace eventually held more power than the king himself.",
                            "target_lang_translations": {
                                "ru": "<w>Майордом</w> со временем стал обладать большей властью, чем сам король."
                            }
                        },
                        {
                            "text": "Historians study how the role of the <w>mayor</w> evolved during the Merovingian dynasty.",
                            "target_lang_translations": {
                                "ru": "Историки изучают, как менялась роль <w>майордома</w> во времена династии Меровингов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mayor of the palace"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context only"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высший сановник или управляющий королевским двором в исторической Франкской империи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "майордом",
                                "target_lang_sense_clarification": "исторический титул"
                            }
                        ]
                    }
                }
            ]
        },
        {

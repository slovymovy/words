    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c5cb75ad-0010-3c02-a94c-0770b6ea0ef1",
                    "sense_definition": "The ruler of an absolute monarchy or the head of state of a constitutional monarchy, such as a king, queen, or emperor.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "ruler_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>monarch</w> greeted the crowd from the palace balcony.",
                            "target_lang_translations": {
                                "ru": "<w>Монарх</w> поприветствовал толпу с балкона дворца."
                            }
                        },
                        {
                            "text": "Under the new constitution, the power of the <w>monarch</w> is limited by parliament.",
                            "target_lang_translations": {
                                "ru": "Согласно новой конституции, власть <w>монарха</w> ограничена парламентом."
                            }
                        }
                    ],
                    "synonyms": [
                        "sovereign",
                        "potentate"
                    ],
                    "antonyms": [
                        "subject",
                        "commoner"
                    ],
                    "common_phrases": [
                        "reigning monarch",
                        "absolute monarch",
                        "constitutional monarch",
                        "British monarch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правитель абсолютной монархии или глава государства в конституционной монархии, например, король, королева или император."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монарх",
                                "target_lang_sense_clarification": "правитель"
                            },
                            {
                                "target_lang_word": "государь",
                                "target_lang_sense_clarification": "правитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "864de590-2267-30c2-9a9e-9490d32d0506",
                    "sense_definition": "A large orange and black butterfly (Danaus plexippus) found primarily in North America, known for its long migrations.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "butterfly_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We planted milkweed in the garden to provide food for the <w>monarch</w>.",
                            "target_lang_translations": {
                                "ru": "Мы посадили в саду ваточник, чтобы обеспечить пищей <w>монарха</w>."
                            }
                        },
                        {
                            "text": "Every year, the <w>monarch</w> migration covers thousands of miles to Mexico.",
                            "target_lang_translations": {
                                "ru": "Каждый год миграция <w>монархов</w> охватывает тысячи миль до Мексики."
                            }
                        }
                    ],
                    "synonyms": [
                        "monarch flycatcher"
                    ],
                    "common_phrases": [
                        "monarch butterfly",
                        "monarch caterpillar",
                        "monarch migration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная оранжево-черная бабочка (Danaus plexippus), обитающая преимущественно в Северной Америке и известная своими длительными миграциями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монарх",
                                "target_lang_sense_clarification": "бабочка"
                            },
                            {
                                "target_lang_word": "бабочка-монарх",
                                "target_lang_sense_clarification": "насекомое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e67987a1-dcc8-30c9-8888-8181c99eb013",
                    "sense_definition": "The most important, powerful, or impressive thing of its kind.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lion is often described as the <w>monarch</w> of the jungle.",
                            "target_lang_translations": {
                                "ru": "Льва часто называют <w>царём</w> джунглей."
                            }
                        },
                        {
                            "text": "That giant oak tree is the undisputed <w>monarch</w> of the forest.",
                            "target_lang_translations": {
                                "ru": "Этот гигантский дуб — бесспорный <w>властелин</w> леса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "monarch of the glen",
                        "monarch of the forest"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or metaphorical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Самое важное, могущественное или впечатляющее существо или предмет в своем роде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "царь",
                                "target_lang_sense_clarification": "метафорическое"
                            },
                            {
                                "target_lang_word": "властелин",
                                "target_lang_sense_clarification": "метафорическое"
                            },
                            {
                                "target_lang_word": "король",
                                "target_lang_sense_clarification": "метафорическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e15084c-495c-3786-b77f-e08cc2d046f3",
                    "sense_definition": "Any bird of the family Monarchidae, which includes monarch flycatchers.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "bird_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The birdwatcher was thrilled to spot a rare <w>monarch</w> in the tropical canopy.",
                            "target_lang_translations": {
                                "ru": "Орнитолог был в восторге, заметив редкого <w>монарха</w> в тропическом пологе леса."
                            }
                        },
                        {
                            "text": "This species of <w>monarch</w> is known for its aggressive defense of its territory.",
                            "target_lang_translations": {
                                "ru": "Этот вид <w>монархов</w> известен агрессивной защитой своей территории."
                            }
                        }
                    ],
                    "synonyms": [
                        "monarch flycatcher"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любая птица из семейства монарховых, к которому относятся монарховые мухоловки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монарх",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "838225a8-c0ca-356e-a3f3-b08ed6e57e42",
                    "sense_definition": "A mature stag that has sixteen or more points or tines on its antlers.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "stag_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hunter waited hours for a glimpse of the magnificent <w>monarch</w>.",
                            "target_lang_translations": {
                                "ru": "Охотник часами ждал, чтобы хоть мельком увидеть великолепного <w>монарха</w>."
                            }
                        },
                        {
                            "text": "Only an old and healthy deer can grow the antlers of a <w>monarch</w>.",
                            "target_lang_translations": {
                                "ru": "Только старый и здоровый олень может отрастить рога <w>монарха</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрослый олень-самец, имеющий шестнадцать или более отростков на рогах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монарх",
                                "target_lang_sense_clarification": "олень"
                            },
                            {
                                "target_lang_word": "олень-монарх",
                                "target_lang_sense_clarification": "охотничий термин"
                            }
                        ]
                    }
                }
            ]
        },

                    ],
                    "synonyms": [
                        "tick",
                        "checkmark"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак (✓), используемый для указания того, что что-то верно, выбрано или выполнено."
                    },
                    "translations": {

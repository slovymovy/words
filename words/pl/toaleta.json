            "senses": [
                {
                    "sense_id": "84e52920-99a6-399a-924a-b53f9e5d743b",
                    "sense_definition": "Pomieszczenie wyposażone w urządzenia sanitarne, służące do załatwiania potrzeb fizjologicznych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przepraszam, czy wiesz, gdzie znajduje się najbliższa <w>toaleta</w>?",
                            "target_lang_translations": {
                                "ru": "Извините, вы не знаете, где находится ближайший <w>туалет</w>?"
                            }
                        },
                        {
                            "text": "W tym budynku <w>toaleta</w> dla klientów jest na pierwszym piętrze.",
                            "target_lang_translations": {
                                "ru": "В этом здании <w>туалет</w> для клиентов находится на втором этаже."
                            }
                        }
                    ],
                    "synonyms": [
                        "ubikacja",
                        "WC",
                        "klozet"
                    ],
                    "common_phrases": [
                        "toaleta publiczna",
                        "toaleta damska",
                        "toaleta męska",
                        "iść do toalety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещение, оборудованное сантехникой, предназначенное для удовлетворения физиологических потребностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туалет",
                                "target_lang_sense_clarification": "помещение"
                            },
                            {
                                "target_lang_word": "уборная",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "730ece63-46c1-3a8c-821f-00eeb6dd4b79",
                    "sense_definition": "Czynności higieniczne i kosmetyczne, takie jak mycie się, czesanie i ubieranie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego poranna <w>toaleta</w> zajmuje zazwyczaj około trzydziestu minut.",
                            "target_lang_translations": {
                                "ru": "Его утренний <w>туалет</w> обычно занимает около тридцати минут."
                            }
                        },
                        {
                            "text": "Pielęgniarka pomaga starszemu pacjentowi przy codziennej <w>toalecie</w>.",
                            "target_lang_translations": {
                                "ru": "Медсестра помогает пожилому пациенту с ежедневным <w>туалетом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mycie się"
                    ],
                    "common_phrases": [
                        "poranna toaleta",
                        "wieczorna toaleta",
                        "robić toaletę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гигиенические и косметические процедуры, такие как мытье, причесывание и одевание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туалет",
                                "target_lang_sense_clarification": "процедура"
                            },
                            {
                                "target_lang_word": "умывание",
                                "target_lang_sense_clarification": "гигиена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4fcca0eb-a5b4-33a0-8716-c028a3692a9c",
                    "sense_definition": "Mebel, zazwyczaj w formie stolika z lustrem, służący do przechowywania kosmetyków i wykonywania zabiegów upiększających.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sypialni stoi zabytkowa <w>toaleta</w> z dużym, owalnym lustrem.",
                            "target_lang_translations": {
                                "ru": "В спальне стоит старинный <w>туалетный столик</w> с большим овальным зеркалом."
                            }
                        },
                        {
                            "text": "Kobieta usiadła przy <w>toalecie</w>, aby poprawić makijaż przed wyjściem.",
                            "target_lang_translations": {
                                "ru": "Женщина села за <w>туалетный столик</w>, чтобы поправить макияж перед выходом."
                            }
                        }
                    ],
                    "synonyms": [
                        "szafka"
                    ],
                    "common_phrases": [
                        "siedzieć przy toalecie",
                        "lustro toalety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мебель, обычно в форме столика с зеркалом, для хранения косметики и ухода за собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "туалетный столик",
                                "target_lang_sense_clarification": "мебель"
                            },
                            {
                                "target_lang_word": "трюмо",
                                "target_lang_sense_clarification": "с зеркалом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb37912b-fe10-3491-8813-35a209c96f66",
                    "sense_definition": "Elegancki, wyjściowy strój kobiecy, zakładany na uroczyste okazje.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hrabina pojawiła się na balu w zachwycającej, jedwabnej <w>toalecie</w>.",
                            "target_lang_translations": {
                                "ru": "Графиня появилась на балу в восхитительном шелковом <w>туалете</w>."
                            }
                        },
                        {
                            "text": "Wszystkie panie zaprezentowały swoje najpiękniejsze wieczorowe <w>toalety</w>.",
                            "target_lang_translations": {
                                "ru": "Все дамы продемонстрировали свои самые красивые вечерние <w>туалеты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kreacja"
                    ],
                    "common_phrases": [
                        "wieczorowa toaleta",
                        "balowa toaleta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элегантный женский наряд, надеваемый по торжественным случаям."
                    },

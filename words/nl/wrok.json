            "senses": [
                {
                    "sense_id": "c5faedad-5019-3772-8cc7-f5ab1d016638",
                    "sense_definition": "Blijvend gevoel van onvrede over geleden of vermeend onrecht",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij koestert al jaren <w>wrok</w> tegen zijn voormalige baas.",
                            "target_lang_translations": {
                                "ru": "Он уже много лет таит <w>злобу</w> на своего бывшего начальника."
                            }
                        },
                        {
                            "text": "Zonder enige <w>wrok</w> nam ze afscheid van haar collega's.",
                            "target_lang_translations": {
                                "ru": "Без какой-либо <w>обиды</w> она попрощалась со своими коллегами."
                            }
                        }
                    ],
                    "synonyms": [
                        "rancune"
                    ],
                    "common_phrases": [
                        "wrok koesteren",
                        "wrok voelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устойчивое чувство недовольства или горечи, вызванное пережитой или воображаемой несправедливостью"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обида",
                                "target_lang_sense_clarification": "чувство горечи от несправедливости"
                            },
                            {
                                "target_lang_word": "злоба",
                                "target_lang_sense_clarification": "затаенный гнев, недоброжелательность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7b43d28d-de08-3c2c-8505-ef70dffab894",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>wrokken</w>",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>wrok</w> niet meer over wat er in het verleden is gebeurd.",
                            "target_lang_translations": {
                                "ru": "Я больше не <w>держу зла</w> за то, что произошло в прошлом."
                            }
                        },
                        {
                            "text": "Soms <w>wrok</w> ik in stilte over de oneerlijke behandeling.",
                            "target_lang_translations": {
                                "ru": "Иногда я молча <w>таю обиду</w> из-за несправедливого обращения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола wrokken (таить обиду)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таить обиду",
                                "target_lang_sense_clarification": "долго помнить зло"
                            },
                            {
                                "target_lang_word": "держать зло",
                                "target_lang_sense_clarification": "испытывать скрытую враждебность"
                            }
                        ]
                    }
                }
            ]
        }
    ]

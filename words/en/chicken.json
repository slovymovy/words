    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b2fc9141-45f2-37e7-92e7-fcb106aab2b0",
                    "sense_definition": "A common domesticated bird kept by humans for its eggs and meat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer feeds the <w>chickens</w> in the yard every morning.",
                            "target_lang_translations": {
                                "ru": "Фермер кормит <w>кур</w> во дворе каждое утро."
                            }
                        },
                        {
                            "text": "We could hear the <w>chickens</w> clucking loudly from inside the coop.",
                            "target_lang_translations": {
                                "ru": "Мы слышали, как <w>куры</w> громко кудахтали в курятнике."
                            }
                        }
                    ],
                    "synonyms": [
                        "hen",
                        "rooster"
                    ],
                    "common_phrases": [
                        "chicken coop",
                        "free-range chicken",
                        "backyard chicken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенная домашняя птица, которую разводят ради яиц и мяса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курица",
                                "target_lang_sense_clarification": "наиболее общее название птицы"
                            },
                            {
                                "target_lang_word": "цыплёнок",
                                "target_lang_sense_clarification": "птенец курицы"
                            },
                            {
                                "target_lang_word": "петух",
                                "target_lang_sense_clarification": "самец курицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d6f3f06-5158-30b5-9876-d944f99b9984",
                    "sense_definition": "The meat of the chicken bird used as food.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I ordered fried <w>chicken</w> with chips for dinner.",
                            "target_lang_translations": {
                                "ru": "Я заказал жареную <w>курицу</w> с картошкой фри на ужин."
                            }
                        },
                        {
                            "text": "Make sure to wash your hands after handling raw <w>chicken</w>.",
                            "target_lang_translations": {
                                "ru": "Обязательно помойте руки после того, как трогали сырую <w>курицу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "poultry"
                    ],
                    "common_phrases": [
                        "fried chicken",
                        "roast chicken",
                        "chicken soup",
                        "chicken breast",
                        "chicken wings"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясо курицы, употребляемое в пищу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курица",
                                "target_lang_sense_clarification": "мясо или блюдо"
                            },
                            {
                                "target_lang_word": "курятина",
                                "target_lang_sense_clarification": "вид мяса"
                            },
                            {
                                "target_lang_word": "цыплёнок",
                                "target_lang_sense_clarification": "блюдо из молодой птицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "752b811b-aaa5-38ef-bfc0-f11d2ae3eba3",
                    "sense_definition": "A person who is not brave and is easily frightened.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cowardice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't be such a <w>chicken</w>; just jump into the water!",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>трусом</w>, просто прыгай в воду!"
                            }
                        },
                        {
                            "text": "He called me a <w>chicken</w> because I refused to walk through the graveyard at night.",
                            "target_lang_translations": {
                                "ru": "Он назвал меня <w>трусом</w>, потому что я отказался идти через кладбище ночью."
                            }
                        }
                    ],
                    "synonyms": [
                        "coward",
                        "wimp"
                    ],
                    "antonyms": [
                        "hero"
                    ],
                    "common_phrases": [
                        "don't be a chicken"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который не отличается храбростью и легко пугается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трус",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "трусишка",
                                "target_lang_sense_clarification": "разговорное, мягкое"
                            },
                            {
                                "target_lang_word": "трусиха",
                                "target_lang_sense_clarification": "о женщине"
                            },
                            {
                                "target_lang_word": "ссыкун",
                                "target_lang_sense_clarification": "грубое, сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "055cd176-0861-3f59-8d58-aceda751f4fc",
                    "sense_definition": "A dangerous game in which participants face a risk, and the first one to yield is considered the loser.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two drivers played a dangerous game of <w>chicken</w> on the highway.",
                            "target_lang_translations": {
                                "ru": "Два водителя играли в опасную игру <w>на испуг</w> на шоссе."
                            }
                        },
                        {
                            "text": "The politicians are playing <w>chicken</w> with the national budget, waiting for the other side to give in.",
                            "target_lang_translations": {
                                "ru": "Политики играют в <w>опасную игру</w> с национальным бюджетом, ожидая, что другая сторона уступит."
                            }
                        }
                    ],
                    "common_phrases": [
                        "game of chicken",
                        "play chicken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опасная игра, в которой участники рискуют, и проигравшим считается тот, кто первым сдастся."
                    },
                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "c3e9f011-eb27-31af-9d07-94acf1067a5f",
                    "sense_definition": "A young person; typically used in negative phrases to indicate someone is no longer young.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "age",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is no spring <w>chicken</w>, but he still runs five miles every day.",
                            "target_lang_translations": {
                                "ru": "Он уже не <w>молод</w>, но все еще пробегает пять миль каждый день."
                            }
                        },
                        {
                            "text": "My aunt admits she is no spring <w>chicken</w>, yet she travels more than anyone I know.",
                            "target_lang_translations": {
                                "ru": "Моя тетя признает, что она уже не <w>девочка</w>, но путешествует больше всех, кого я знаю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "no spring chicken"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "idiomatic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодой человек; обычно используется в отрицательных фразах, указывающих на то, что кто-то уже не молод."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6631614c-42eb-38c9-9e11-1b4c23f4c320",
                    "sense_definition": "Lacking courage; afraid to do something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cowardice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was too <w>chicken</w> to ask him for his phone number.",
                            "target_lang_translations": {
                                "ru": "Я слишком <w>струсила</w>, чтобы попросить у него номер телефона."
                            }
                        },
                        {
                            "text": "Why are you acting so <w>chicken</w> about watching a horror movie?",
                            "target_lang_translations": {
                                "ru": "Почему ты ведешь себя как <w>трус</w> и не хочешь смотреть фильм ужасов?"
                            }
                        }
                    ],
                    "synonyms": [
                        "cowardly",
                        "scared",
                        "fearful",
                        "gutless",
                        "yellow"
                    ],
                    "antonyms": [
                        "brave",
                        "courageous"
                    ],
                    "common_phrases": [
                        "too chicken to"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишенный мужества; боящийся что-то сделать."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c363f3fa-a9dc-36ec-aad9-86a1d9604f62",
                    "sense_definition": "To decide not to do something because one is afraid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cowardice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He wanted to go bungee jumping, but he <w>chickened</w> out at the last second.",
                            "target_lang_translations": {
                                "ru": "Он хотел прыгнуть с тарзанки, но <w>струсил</w> в последнюю секунду."
                            }
                        },
                        {
                            "text": "Don't <w>chicken</w> out now; we are already at the entrance of the haunted house.",
                            "target_lang_translations": {
                                "ru": "Не <w>трусь</w> сейчас, мы уже у входа в дом с привидениями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chicken out"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "usually used with 'out'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Решить не делать что-то из-за страха."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },

                        {
                            "text": "You might need a <w>glass</w> to read the tiny print on this label.",
                            "target_lang_translations": {
                                "ru": "Вам может понадобиться <w>лупа</w>, чтобы прочитать мелкий шрифт на этой этикетке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "magnifying glass"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличительное стекло или лупа, используемые для визуального увеличения объектов."
                    },
                    "translations": {
                        "ru": [

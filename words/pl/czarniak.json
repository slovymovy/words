            "senses": [
                {
                    "sense_id": "0f9d8ba6-6ce0-3596-82cb-17f8fd92da2b",
                    "sense_definition": "Gatunek morskiej ryby z rodziny dorszowatych, poławianej w celach spożywczych; dorsz czarny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fish_species",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sklepie rybnym kupiłem kilogram świeżego <w>czarniaka</w> na obiad.",
                            "target_lang_translations": {
                                "ru": "В рыбном магазине я купил килограмм свежей <w>сайды</w> на обед."
                            }
                        },
                        {
                            "text": "Ten smażony <w>czarniak</w> smakuje wyśmienicie z dodatkiem cytryny i koperku.",
                            "target_lang_translations": {
                                "ru": "Эта жареная <w>сайда</w> восхитительна на вкус с лимоном и укропом."
                            }
                        }
                    ],
                    "synonyms": [
                        "dorsz czarny"
                    ],
                    "common_phrases": [
                        "filet z czarniaka",
                        "świeży czarniak",
                        "połów czarniaka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид морской рыбы из семейства тресковых, имеющий промысловое значение; сайда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сайда",
                                "target_lang_sense_clarification": "рыба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1559ec92-c253-374d-913b-e520cc0b536a",
                    "sense_definition": "W gwarze więziennej: chleb, zwłaszcza ciemny lub razowy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary więzień podzielił się swoim <w>czarniakiem</w> z nowym osadzonym w celi.",
                            "target_lang_translations": {
                                "ru": "Старый заключённый поделился своей <w>черняшкой</w> с новым сокамерником."
                            }
                        },
                        {
                            "text": "Na śniadanie podano im tylko gorzką kawę i czerstwy <w>czarniak</w>.",
                            "target_lang_translations": {
                                "ru": "На завтрак им дали только горький кофе и чёрствую <w>черняшку</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kawałek czarniaka",
                        "bochenek czarniaka"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "więzienny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "На тюремном жаргоне: хлеб, особенно чёрный или цельнозерновой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черняшка",
                                "target_lang_sense_clarification": "жаргон: чёрный хлеб"
                            },
                            {
                                "target_lang_word": "чёрный хлеб",
                                "target_lang_sense_clarification": "хлеб"
                            }
                        ]
                    }
                }
            ]

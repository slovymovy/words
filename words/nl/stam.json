    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6b52dfba-9bab-3a90-87d6-7880778c72f3",
                    "sense_definition": "De hoofdstengel van een boom, tussen de wortels en de kruin.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ruwe <stam> van de oude eik is erg breed.",
                            "target_lang_translations": {
                                "en": "The rough <w>trunk</w> of the old oak is very wide.",
                                "ru": "Шероховатый <ствол> старого дуба очень широкий."
                            }
                        },
                        {
                            "text": "Hij leunde even tegen de <stam> om uit te rusten.",
                            "target_lang_translations": {
                                "en": "He leaned against the <w>trunk</w> for a moment to rest.",
                                "ru": "Он прислонился к <стволу>, чтобы немного отдохнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "boomstam"
                    ],
                    "common_phrases": [
                        "dikke stam",
                        "tegen de stam",
                        "holle stam"
                    ],
                    "target_lang_definitions": {
                        "en": "The main stem of a tree, located between the roots and the crown.",
                        "ru": "Основной ствол дерева, расположенный между корнями и кроной."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trunk",
                                "target_lang_sense_clarification": "main woody stem of a tree"
                            },
                            {
                                "target_lang_word": "tree trunk",
                                "target_lang_sense_clarification": "specific reference to a tree"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ствол",
                                "target_lang_sense_clarification": "часть дерева"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8525f32f-2e82-3a77-b766-bf8edefe555e",
                    "sense_definition": "Een groep mensen die samenleven en vaak een gemeenschappelijke afstamming, taal of cultuur delen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sociology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leden van de <stam> kwamen bijeen voor een belangrijk ritueel.",
                            "target_lang_translations": {
                                "en": "The members of the <w>tribe</w> gathered for an important ritual.",
                                "ru": "Члены <племени> собрались для важного ритуала."
                            }
                        },
                        {
                            "text": "In het regenwoud leeft een <stam> die weinig contact heeft met de buitenwereld.",
                            "target_lang_translations": {
                                "en": "In the rainforest lives a <w>tribe</w> that has little contact with the outside world.",
                                "ru": "В тропическом лесу живет <племя>, которое мало контактирует с внешним миром."
                            }
                        }
                    ],
                    "synonyms": [
                        "volksstam"
                    ],
                    "common_phrases": [
                        "inheemse stam",
                        "hoofd van de stam",
                        "verloren stam"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people living together who often share a common ancestry, language, or culture.",
                        "ru": "Группа людей, живущих вместе и часто имеющих общее происхождение, язык или культуру."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tribe",
                                "target_lang_sense_clarification": "social group with common ancestry"
                            },
                            {
                                "target_lang_word": "ethnic group",
                                "target_lang_sense_clarification": "group sharing cultural background"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "племя",
                                "target_lang_sense_clarification": "этническая общность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db76be1c-ad79-3bda-9e6c-ac65d7f13fab",
                    "sense_definition": "Het onveranderlijke deel van een woord waaraan uitgangen worden toegevoegd.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Om de persoonsvorm te vinden, moet je eerst de <stam> van het werkwoord weten.",
                            "target_lang_translations": {
                                "en": "To find the finite verb, you first need to know the <w>stem</w> of the verb.",
                                "ru": "Чтобы найти личную форму глагола, нужно сначала знать <основу> глагола."
                            }
                        },
                        {
                            "text": "In het Nederlands eindigt de <stam> vaak op een medeklinker.",
                            "target_lang_translations": {
                                "en": "In Dutch, the <w>stem</w> often ends in a consonant.",
                                "ru": "В нидерландском языке <основа> часто заканчивается на согласную."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stam van het werkwoord",
                        "stam plus t"
                    ],
                    "target_lang_definitions": {
                        "en": "The unchanging part of a word to which endings are added.",
                        "ru": "Неизменяемая часть слова, к которой присоединяются окончания."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stem",
                                "target_lang_sense_clarification": "linguistics: base form of a word"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "основа",
                                "target_lang_sense_clarification": "лингвистика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f05d0ece-2431-30ba-911d-4131f0f008d3",
                    "sense_definition": "Een hoofdcategorie in de biologische indeling van organismen, onder het rijk en boven de klasse.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De mens behoort tot de <stam> van de chordadieren.",
                            "target_lang_translations": {
                                "en": "Humans belong to the <w>phylum</w> of chordates.",
                                "ru": "Человек принадлежит к <типу> хордовых."
                            }
                        },
                        {
                            "text": "Biologen hebben een nieuwe <stam> van bacteriën ontdekt.",
                            "target_lang_translations": {
                                "en": "Biologists have discovered a new <w>phylum</w> of bacteria.",
                                "ru": "Биологи открыли новый <тип> бактерий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biologische stam",
                        "stam der gewervelden"
                    ],
                    "target_lang_definitions": {
                        "en": "A principal category in the biological classification of organisms, below kingdom and above class.",
                        "ru": "Высшая категория в биологической классификации организмов, ниже царства и выше класса."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "phylum",
                                "target_lang_sense_clarification": "biological classification rank"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тип",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f981c7e-6d77-3962-9ebf-8350c37d0ba8",
                    "sense_definition": "Een groep personen die van dezelfde voorouder afstammen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "genealogy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij beweert dat hij uit een oude adellijke <stam> komt.",
                            "target_lang_translations": {
                                "en": "He claims to come from an old noble <w>lineage</w>.",
                                "ru": "Он утверждает, что происходит из древнего дворянского <рода>."
                            }
                        },
                        {
                            "text": "De geschiedenis van deze <stam> gaat vele eeuwen terug.",
                            "target_lang_translations": {
                                "en": "The history of this <w>lineage</w> goes back many centuries.",
                                "ru": "История этого <рода> уходит вглубь веков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uit de stam van",
                        "koninklijke stam"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of persons descended from the same ancestor.",
                        "ru": "Группа людей, происходящих от одного предка; род."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lineage",
                                "target_lang_sense_clarification": "direct descent from an ancestor"
                            },
                            {
                                "target_lang_word": "stock",
                                "target_lang_sense_clarification": "ancestry or family line"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "род",
                                "target_lang_sense_clarification": "происхождение"
                            },
                            {
                                "target_lang_word": "династия",
                                "target_lang_sense_clarification": "знатный род"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ffbad070-b076-300b-847d-bbd97e930e68",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>stammen</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <stam> uit een familie van muzikanten.",
                            "target_lang_translations": {
                                "en": "I <w>come</w> from a family of musicians.",
                                "ru": "Я <происхожу> из семьи музыкантов."
                            }
                        },
                        {
                            "text": "Ik <stam> af van voorouders die hier eeuwen geleden woonden.",
                            "target_lang_translations": {
                                "en": "I <w>descend</w> from ancestors who lived here centuries ago.",
                                "ru": "Я <происхожу> от предков, которые жили здесь много веков назад."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "first-person singular present indicative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First-person singular present tense of 'stammen' (to descend/stem).",
                        "ru": "Форма первого лица единственного числа настоящего времени от глагола 'stammen' (происходить)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "descend",
                                "target_lang_sense_clarification": "to originate from an ancestor"
                            },
                            {
                                "target_lang_word": "stem",
                                "target_lang_sense_clarification": "to originate from a source"
                            },
                            {
                                "target_lang_word": "come",
                                "target_lang_sense_clarification": "to have as origin"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "происхожу",
                                "target_lang_sense_clarification": "глагол, 1-е лицо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6284721-69bb-37b2-a20f-137d65b0517e",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>stammen</w> (bij inversie).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Stam> jij ook uit die regio?",
                            "target_lang_translations": {
                                "en": "Do you also <w>come</w> from that region?",
                                "ru": "Ты тоже <происходишь> из этого региона?"
                            }
                        },
                        {
                            "text": "Waar <stam> je precies van af?",
                            "target_lang_translations": {
                                "en": "From whom do you <w>descend</w> exactly?",
                                "ru": "От кого именно ты <происходишь>?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "second-person singular present indicative (inversion)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Second-person singular present tense of 'stammen' (used in inversion).",
                        "ru": "Форма второго лица единственного числа настоящего времени от глагола 'stammen' (происходить)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "descend",
                                "target_lang_sense_clarification": "to originate from an ancestor"
                            },
                            {
                                "target_lang_word": "stem",
                                "target_lang_sense_clarification": "to originate from a source"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "происходишь",
                                "target_lang_sense_clarification": "глагол, 2-е лицо"

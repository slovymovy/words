                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly used in religious or poetic contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To burn aromatic substances to produce smoke (usually in religious rites) or to emit smoke."
                    },
                    "translations": {

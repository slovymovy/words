{
    "word_family": [
        "kelderen",
        "wijnkelder",
        "kelderluik",
        "keldertrap",
        "kelderraam"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5ff7c02f-5b1e-3811-b89d-53bc4524cfd3",
                    "sense_definition": "Een ruimte onder de grond in een huis of gebouw, meestal gebruikt voor opslag.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "building_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Staan er nog flessen wijn in de <w>kelder</w>?",
                            "target_lang_translations": {
                                "ru": "Остались ли еще бутылки вина в <w>погребе</w>?"
                            }
                        },
                        {
                            "text": "We hebben onze oude spullen in de <w>kelder</w> opgeslagen.",
                            "target_lang_translations": {
                                "ru": "Мы сложили наши старые вещи в <w>подвал</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "zolder"
                    ],
                    "common_phrases": [
                        "in de kelder",
                        "naar de kelder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещение под землей в доме или здании, обычно используемое для хранения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подвал",
                                "target_lang_sense_clarification": "общая комната под домом"
                            },
                            {
                                "target_lang_word": "погреб",
                                "target_lang_sense_clarification": "для хранения продуктов или вина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "393ca1fd-7789-3ebb-9016-91949e6178d0",
                    "sense_definition": "De bodem van de zee, vaak gebruikt in uitdrukkingen over zinken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sea_bottom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het zwaar beschadigde schip ging snel naar de <w>kelder</w>.",
                            "target_lang_translations": {
                                "ru": "Тяжело поврежденный корабль быстро пошел ко <w>дну</w>."
                            }
                        },
                        {
                            "text": "Door de explosie verdween de boot naar de <w>kelder</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за взрыва лодка пошла ко <w>дну</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zeebodem"
                    ],
                    "common_phrases": [
                        "naar de kelder gaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Морское дно, часто используется в выражениях о потоплении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дно",
                                "target_lang_sense_clarification": "в выражении 'идти ко дну'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "01b5c8b3-4e09-339e-9deb-fd661adcd6b9",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>kelderen</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>kelder</w> in de ranglijst als ik deze wedstrijd verlies.",
                            "target_lang_translations": {
                                "ru": "Я <w>скачусь</w> в рейтинге, если проиграю этот матч."
                            }
                        },
                        {
                            "text": "Als de beurs instort, <w>kelder</w> ik met mijn aandelen.",
                            "target_lang_translations": {
                                "ru": "Если биржа рухнет, я <w>прогорю</w> со своими акциями."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола kelderen (резко падать, обрушиваться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падаю",
                                "target_lang_sense_clarification": "резко снижаюсь"
                            },
                            {
                                "target_lang_word": "скатываюсь",
                                "target_lang_sense_clarification": "теряю позиции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ae801db-963f-36e4-96f2-f8fd16174e87",
                    "sense_definition": "Gebiedende wijs van <w>kelderen</w>.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Kelder</w> de prijzen niet te ver, anders maken we verlies.",
                            "target_lang_translations": {
                                "ru": "Не <w>снижай</w> цены слишком сильно, иначе мы понесем убытки."
                            }
                        },
                        {
                            "text": "<w>Kelder</w> niet verder in niveau, blijf je best doen.",
                            "target_lang_translations": {
                                "ru": "Не <w>опускайся</w> ниже по уровню, продолжай стараться."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола kelderen."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снижай",
                                "target_lang_sense_clarification": "цены, стоимость"
                            },
                            {
                                "target_lang_word": "опускай",
                                "target_lang_sense_clarification": "уровень, качество"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "4b5a3b43-fd04-3272-814d-6e9d8a4efbcf",
                    "sense_definition": "Związany z językiem jako systemem znaków służącym do komunikacji międzyludzkiej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "linguistic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sierpniu zapiszę się na intensywny kurs <w>językowy</w>.",
                            "target_lang_translations": {
                                "ru": "В августе я запишусь на интенсивный <w>языковой</w> курс."
                            }
                        },
                        {
                            "text": "Bariera <w>językowa</w> często utrudnia swobodne podróżowanie po egzotycznych krajach.",
                            "target_lang_translations": {
                                "ru": "<w>Языковой</w> барьер часто затрудняет свободные путешествия по экзотическим странам."
                            }
                        }
                    ],
                    "synonyms": [
                        "lingwistyczny"
                    ],
                    "common_phrases": [
                        "bariera językowa",
                        "kurs językowy",
                        "rodzina językowa",
                        "błąd językowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с языком как системой знаков, служащей для общения между людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "языковой",
                                "target_lang_sense_clarification": "относящийся к языку, речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "307066c8-952d-315d-b479-c0534a68b7e2",
                    "sense_definition": "Związany z językiem jako narządem mięśniowym znajdującym się w jamie ustnej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anatomical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nalot <w>językowy</w> jest pożywką dla bakterii wywołujących zapalenie dziąseł.",
                            "target_lang_translations": {
                                "ru": "Налет <w>на языке</w> является питательной средой для бактерий, вызывающих воспаление десен."
                            }
                        },
                        {
                            "text": "Tętnica <w>językowa</w> doprowadza krew do mięśni języka i dna jamy ustnej.",
                            "target_lang_translations": {
                                "ru": "<w>Язычная</w> артерия доставляет кровь к мышцам языка и дну полости рта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tętnica językowa",
                        "nalot językowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с языком как мышечным органом, находящимся в полости рта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "язычный",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                }
            ]
        }

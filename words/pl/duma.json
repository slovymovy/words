    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "083beaee-8673-34b5-abd6-d57e02ff6e99",
                    "sense_definition": "Poczucie własnej godności i wartości osobistej; stan zadowolenia z siebie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "self_worth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <duma> nie pozwoliła mu poprosić przyjaciół o pożyczkę.",
                            "target_lang_translations": {
                                "ru": "Его <гордость> не позволила ему попросить друзей о займе."
                            }
                        },
                        {
                            "text": "Polacy z wielką <dumą> śpiewają swój hymn narodowy.",
                            "target_lang_translations": {
                                "ru": "Поляки с большой <гордостью> поют свой национальный гимн."
                            }
                        }
                    ],
                    "synonyms": [
                        "godność",
                        "honor",
                        "ambicja"
                    ],
                    "antonyms": [
                        "wstyd",
                        "hańba"
                    ],
                    "common_phrases": [
                        "duma narodowa",
                        "unosić się dumą",
                        "urażona duma",
                        "mieć swoją dumę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство собственного достоинства и личной ценности; состояние довольства собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гордость",
                                "target_lang_sense_clarification": "чувство достоинства"
                            },
                            {
                                "target_lang_word": "чувство собственного достоинства",
                                "target_lang_sense_clarification": "самоуважение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "229f856f-792a-32e9-a59d-7c173154a270",
                    "sense_definition": "Osoba, rzecz lub osiągnięcie będące powodem czyjegoś wielkiego zadowolenia i satysfakcji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "object_of_pride",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowoczesny stadion to <duma> całego naszego miasta.",
                            "target_lang_translations": {
                                "ru": "Этот современный стадион — <гордость> всего нашего города."
                            }
                        },
                        {
                            "text": "Syn, który skończył studia z wyróżnieniem, jest <dumą> swoich rodziców.",
                            "target_lang_translations": {
                                "ru": "Сын, который окончил университет с отличием, является <гордостью> своих родителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "chluba",
                        "chwała",
                        "ozdoba"
                    ],
                    "antonyms": [
                        "wstyd",
                        "zakała"
                    ],
                    "common_phrases": [
                        "powód do dumy",
                        "duma rodziny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, предмет или достижение, являющиеся причиной чьего-либо большого удовлетворения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гордость",
                                "target_lang_sense_clarification": "предмет гордости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a67f07d7-7c58-3f10-b490-dc8f7765f5d1",
                    "sense_definition": "Zbyt wysokie mniemanie o sobie, traktowanie innych z wyższością.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arrogance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez swoją chorobliwą <dumę> stracił wielu bliskich przyjaciół.",
                            "target_lang_translations": {
                                "ru": "Из-за своей болезненной <гордыни> он потерял многих близких друзей."
                            }
                        },
                        {
                            "text": "W jej głosie słychać było <dumę> i pogardę dla biedniejszych ludzi.",
                            "target_lang_translations": {
                                "ru": "В её голосе слышались <высокомерие> и презрение к более бедным людям."
                            }
                        }
                    ],
                    "synonyms": [
                        "pycha",
                        "wyniosłość",
                        "nieprzystępność",
                        "wyniosłość"
                    ],
                    "antonyms": [
                        "skromność",
                        "pokora"
                    ],
                    "common_phrases": [
                        "fałszywa duma",
                        "przełknąć dumę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезмерно высокое мнение о себе, отношение к другим с превосходством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высокомерие",
                                "target_lang_sense_clarification": "надменность"
                            },
                            {
                                "target_lang_word": "гордыня",
                                "target_lang_sense_clarification": "чрезмерная гордость"
                            },
                            {
                                "target_lang_word": "спесь",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff5ea0f3-e6ba-36b2-b8a1-f9e8770aa225",
                    "sense_definition": "Gatunek pieśni epickiej lub lirycznej o charakterze elegijnym, wywodzący się z Ukrainy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music_literature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary kozak zagrał na bandurze smutną <dumę> o dawnych bitwach.",
                            "target_lang_translations": {
                                "ru": "Старый казак сыграл на бандуре грустную <думу> о давних битвах."
                            }
                        },
                        {
                            "text": "Romantyczni poeci często stylizowali swoje wiersze na ludowe <dumy>.",
                            "target_lang_translations": {
                                "ru": "Романтические поэты часто стилизовали свои стихи под народные <думы>."
                            }
                        }

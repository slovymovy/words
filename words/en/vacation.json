{
    "word_family": [
        "vacate",
        "vacancy",
        "vacant",
        "vacationer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b9a7b35a-2158-3249-92bd-ea0c01a2884d",
                    "sense_definition": "A period of time spent away from work, school, or other daily duties, usually used for rest, recreation, or travel.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "leisure_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are planning a family <w>vacation</w> to the beach this summer.",
                            "target_lang_translations": {
                                "ru": "Этим летом мы планируем семейный <w>отпуск</w> на пляже."
                            }
                        },
                        {
                            "text": "He is currently on <w>vacation</w> and will return to the office next week.",
                            "target_lang_translations": {
                                "ru": "Он сейчас в <w>отпуске</w> и вернется в офис на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "holiday",
                        "break",
                        "time off",
                        "getaway",
                        "annual leave",
                        "leave",
                        "time away",
                        "vacay",
                        "day off"
                    ],
                    "common_phrases": [
                        "on vacation",
                        "summer vacation",
                        "take a vacation",
                        "family vacation",
                        "need a vacation"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Период времени, свободный от работы, учебы или других обязанностей, обычно используемый для отдыха или путешествий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о́тпуск",
                                "target_lang_sense_clarification": "освобождение от работы"
                            },
                            {
                                "target_lang_word": "кани́кулы",
                                "target_lang_sense_clarification": "перерыв в учебе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5eab9427-2d4d-3884-872e-d29c9c23ea67",
                    "sense_definition": "A scheduled period during the year when universities or law courts are officially closed and activity is suspended.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "leisure_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university library has reduced hours during the summer <w>vacation</w>.",
                            "target_lang_translations": {
                                "ru": "Во время летних <w>каникул</w> библиотека университета работает по сокращенному графику."
                            }
                        },
                        {
                            "text": "No hearings are scheduled while the court is in <w>vacation</w>.",
                            "target_lang_translations": {
                                "ru": "Пока суд находится на <w>каникулах</w>, слушания не назначаются."
                            }
                        }
                    ],
                    "synonyms": [
                        "recess",
                        "break"
                    ],
                    "common_phrases": [
                        "summer vacation",
                        "winter vacation",
                        "long vacation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запланированный период в течение года, когда университеты или суды официально закрыты и деятельность приостановлена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кани́кулы",
                                "target_lang_sense_clarification": "в учебных заведениях или судах"
                            },
                            {
                                "target_lang_word": "переры́в",
                                "target_lang_sense_clarification": "в работе учреждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20c89b2f-7f17-3c54-bcfa-9e949ca9d6f4",
                    "sense_definition": "The act of leaving a place, building, or position, making it empty or available.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "act_of_vacating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The landlord demanded the immediate <w>vacation</w> of the apartment by the tenants.",
                            "target_lang_translations": {
                                "ru": "Арендодатель потребовал от жильцов немедленного <w>освобождения</w> квартиры."
                            }
                        },
                        {
                            "text": "His sudden <w>vacation</w> of the leadership post surprised the entire committee.",
                            "target_lang_translations": {
                                "ru": "Его внезапное <w>оставление</w> руководящего поста удивило весь комитет."
                            }
                        }
                    ],
                    "synonyms": [
                        "departure",
                        "moveout"
                    ],
                    "common_phrases": [
                        "vacation of premises"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по освобождению места, здания или должности, делающее их доступными или пустыми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освобожде́ние",
                                "target_lang_sense_clarification": "помещения, здания"
                            },
                            {
                                "target_lang_word": "оставле́ние",
                                "target_lang_sense_clarification": "должности, поста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42a1f427-2311-3679-9674-f0564f32a5d2",
                    "sense_definition": "The legal act of making something void, invalid, or cancelled.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "act_of_vacating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer filed a motion for the <w>vacation</w> of the court's judgment.",
                            "target_lang_translations": {
                                "ru": "Адвокат подал ходатайство об <w>отмене</w> судебного решения."
                            }
                        },
                        {
                            "text": "They sought the <w>vacation</w> of the order due to new evidence.",
                            "target_lang_translations": {
                                "ru": "Они добивались <w>отмены</w> приказа в связи с новыми доказательствами."
                            }
                        }
                    ],
                    "synonyms": [
                        "annulment",
                        "revocation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридический акт признания чего-либо недействительным или аннулированным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отме́на",
                                "target_lang_sense_clarification": "судебного решения, приказа"
                            },
                            {
                                "target_lang_word": "аннули́рование",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f439df3a-4f64-396f-8670-3add0231dca1",
                    "sense_definition": "A ban from the Steam video-gaming service imposed on a user for cheating.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_pun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He used an aimbot and earned himself a permanent <w>vacation</w>.",
                            "target_lang_translations": {
                                "ru": "Он использовал аимбот и заработал себе вечный <w>бан</w>."
                            }
                        },
                        {
                            "text": "Don't cheat in the game or you might get a <w>vacation</w>.",
                            "target_lang_translations": {
                                "ru": "Не читери в игре, иначе можешь получить <w>бан</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Gaming community"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Блокировка пользователя в игровом сервисе Steam за использование читов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бан",
                                "target_lang_sense_clarification": "игровой сленг"
                            },
                            {
                                "target_lang_word": "блокиро́вка",
                                "target_lang_sense_clarification": "аккаунта"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a66263e5-89d1-332e-83cf-ac9c8f93260c",
                    "sense_definition": "To take a holiday; to spend time away from home or work for recreation and pleasure.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "leisure_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They plan to <w>vacation</w> in Mexico this winter to escape the cold.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>отдохнуть</w> в Мексике этой зимой, чтобы сбежать от холода."
                            }
                        },
                        {
                            "text": "We <w>vacationed</w> at a quiet lakeside cabin last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году мы <w>отдыхали</w> в тихом домике у озера."
                            }
                        }
                    ],
                    "synonyms": [
                        "go on holiday",
                        "go on vacation"
                    ],
                    "common_phrases": [
                        "vacation in",
                        "vacation at"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать отпуск; проводить время вдали от дома или работы ради отдыха и удовольствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдыха́ть",
                                "target_lang_sense_clarification": "быть на отдыхе"
                            },
                            {
                                "target_lang_word": "проводи́ть о́тпуск",
                                "target_lang_sense_clarification": "брать отпуск"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
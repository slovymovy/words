                        {
                            "text": "I need to <w>port</w> my landline number to my new cell phone service.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>перенести</w> мой городской номер на новый мобильный тариф."
                            }
                        }
                    ],
                    "common_phrases": [
                        "port a number"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перенести существующий телефонный номер от одного оператора связи к другому."

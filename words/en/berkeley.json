                },
                {
                    "sense_id": "4c53ef53-25f4-39eb-920e-9d98d4e7da47",
                    "sense_definition": "A surname of English origin.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "people",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "The philosopher George <w>Berkeley</w> is known for his theory of immaterialism.",
                            "target_lang_translations": {
                                "ru": "Философ Джордж <w>Беркли</w> известен своей теорией имматериализма."
                            }
                        },
                        {
                            "text": "Mr. <w>Berkeley</w> will see you in his office now.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Беркли</w> примет вас в своем кабинете сейчас."
                            }

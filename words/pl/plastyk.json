                {
                    "sense_id": "5a7a128a-cf86-3b03-89f7-47b7e07f9606",
                    "sense_definition": "Osoba zajmująca się zawodowo twórczością w dziedzinie sztuk wizualnych (np. malarstwem, rzeźbą, grafiką).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "artist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad jest <w>plastykiem</w> i projektuje okładki książek.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — <w>художник</w>, он разрабатывает обложки для книг."
                            }
                        },
                        {
                            "text": "Zatrudniono <w>plastyka</w>, aby przygotował dekoracje na szkolne przedstawienie.",
                            "target_lang_translations": {
                                "ru": "Наняли <w>художника-оформителя</w>, чтобы он подготовил декорации для школьного спектакля."
                            }
                        }
                    ],
                    "synonyms": [
                        "artysta"
                    ],
                    "common_phrases": [
                        "artysta plastyk",
                        "związek plastyków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, профессионально занимающийся изобразительным искусством (например, живописью, скульптурой, графикой)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "художник",
                                "target_lang_sense_clarification": "деятель изобразительного искусства"
                            },
                            {
                                "target_lang_word": "художник-оформитель",
                                "target_lang_sense_clarification": "декоратор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7152041-1dc6-3a49-a3d4-0ccbf4641d25",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>plastyka</w> (w znaczeniu dzieła rzeźbiarskiego lub kompozycji przestrzennej).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum znajduje się wystawa drobnych <w>plastyk</w> z okresu baroku.",
                            "target_lang_translations": {
                                "ru": "В музее находится выставка мелкой <w>пластики</w> эпохи барокко."
                            }
                        },
                        {
                            "text": "Nie widziałem wcześniej tak interesujących <w>plastyk</w> wykonanych z gliny.",
                            "target_lang_translations": {
                                "ru": "Я раньше не видел такой интересной <w>пластики</w>, выполненной из глины."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of plastyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного plastyka (в значении скульптурного произведения или пространственной композиции)."
                    },
                    "translations": {

            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "99b0dd65-2dda-3416-9294-f23818e3f417",
                    "sense_definition": "Wykrzyknik wyrażający powątpiewanie, lekceważenie, zniecierpliwienie lub negatywną opinię o tym, co ktoś powiedział.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>E</w>, i tak ci się nie uda tego naprawić.",
                            "target_lang_translations": {
                                "ru": "<w>Да ну</w>, у тебя всё равно не получится это починить."
                            }
                        },
                        {
                            "text": "<w>E</w>, nie chce mi się dzisiaj nigdzie wychodzić.",
                            "target_lang_translations": {
                                "ru": "<w>Ай</w>, не хочется мне сегодня никуда выходить."
                            }
                        }
                    ],
                    "synonyms": [
                        "eh",
                        "et",
                        "phi"
                    ],
                    "common_phrases": [
                        "e tam",
                        "e gdzie tam"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, выражающее сомнение, пренебрежение, нетерпение или отрицательное мнение о сказанном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да ну",
                                "target_lang_sense_clarification": "выражение пренебрежения или недоверия"
                            },
                            {
                                "target_lang_word": "ай",
                                "target_lang_sense_clarification": "выражение безразличия или досады"
                            },
                            {
                                "target_lang_word": "э",
                                "target_lang_sense_clarification": "звук сомнения или отрицания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6cc4566-b757-3d61-9f77-94e343c0de1a",
                    "sense_definition": "Wykrzyknik służący do zawołania kogoś lub gwałtownego zwrócenia na siebie uwagi, często odbierany jako nieuprzejmy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>E</w>, uważaj trochę, jak chodzisz!",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, поосторожнее, как ты ходишь!"
                            }
                        },
                        {
                            "text": "<w>E</w>, poczekaj chwilę na nas!",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, подожди нас минутку!"
                            }
                        }
                    ],
                    "synonyms": [
                        "ej",
                        "hej",
                        "halo"
                    ],
                    "common_phrases": [
                        "e ty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często niegrzeczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, используемое для оклика или резкого привлечения внимания, часто воспринимается как невежливое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "оклик"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "93f28df9-ba06-3454-b944-91f293e9d534",
                    "sense_definition": "Siódma litera polskiego alfabetu, oznaczająca samogłoskę ustną.",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To trudne słowo pisze się przez <w>e</w>, a nie przez y.",
                            "target_lang_translations": {
                                "ru": "Это трудное слово пишется через <w>е</w>, а не через ы."
                            }
                        },
                        {
                            "text": "W polskim alfabecie litera <w>e</w> znajduje się po d.",
                            "target_lang_translations": {
                                "ru": "В польском алфавите буква <w>е</w> находится после d."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Седьмая буква польского алфавита, обозначающая гласный звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "е",
                                "target_lang_sense_clarification": "буква"
                            }
                        ]
                    }
                }
            ]
        }
    ]

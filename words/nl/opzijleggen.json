                    "sense_id": "aef1df64-e668-3461-9136-f52cccdfa033",
                    "sense_definition": "Geld bewaren om het later te kunnen gebruiken; sparen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "saving_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij hebben genoeg geld <w>opzijgelegd</w> voor een nieuwe auto.",
                            "target_lang_translations": {
                                "en": "We have <w>put aside</w> enough money for a new car.",
                                "ru": "Мы <w>отложили</w> достаточно денег на новую машину."
                            }
                        },
                        {
                            "text": "Je kunt het beste elke maand een vast bedrag <w>opzijleggen</w>.",
                            "target_lang_translations": {
                                "en": "It is best to <w>put aside</w> a fixed amount every month.",
                                "ru": "Лучше всего каждый месяц <w>откладывать</w> фиксированную сумму."
                            }
                        }
                    ],
                    "synonyms": [
                        "sparen",
                        "oppotten",
                        "reserveren"
                    ],
                    "antonyms": [
                        "uitgeven",
                        "spenderen",
                        "verkwisten"
                    ],
                    "common_phrases": [

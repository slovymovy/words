            "senses": [
                {
                    "sense_id": "b092f2a7-5476-3750-bf1c-e14c085ee23b",
                    "sense_definition": "A colloquial clipping of <w>metastases</w>, referring to secondary cancerous growths that have spread from the original site.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medical_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The latest scan showed no evidence of <w>mets</w> in the lungs.",
                            "target_lang_translations": {
                                "ru": "Последнее сканирование не выявило признаков <w>метастазов</w> в легких."
                            }
                        },
                        {
                            "text": "Patients with bone <w>mets</w> often require specialized pain management strategies.",
                            "target_lang_translations": {
                                "ru": "Пациентам с <w>метастазами</w> в костях часто требуются особые методы обезболивания."
                            }
                        }
                    ],
                    "synonyms": [
                        "metastases",
                        "secondaries"
                    ],
                    "common_phrases": [
                        "bone mets",
                        "brain mets",
                        "liver mets",
                        "distant mets"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное сокращение от «metastases» (метастазы), обозначающее вторичные раковые опухоли, распространившиеся из первоначального очага."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "метастазы",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6aeb144-62c9-33b4-8a37-bff4f80845fe",
                    "sense_definition": "Plural of <w>MET</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vigorous exercise usually burns more than six <w>METs</w> per hour.",
                            "target_lang_translations": {
                                "ru": "Интенсивные физические упражнения обычно сжигают более шести <w>МЕТ</w> в час."
                            }
                        },
                        {
                            "text": "The study measured the energy expenditure in <w>METs</w> for various daily activities.",
                            "target_lang_translations": {
                                "ru": "В исследовании измерялся расход энергии в <w>МЕТ</w> для различных видов повседневной деятельности."
                            }

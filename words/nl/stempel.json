    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "50464e2e-1d11-3990-974c-73043f0ac862",
                    "sense_definition": "Een voorwerp met een bewerkt oppervlak waarmee men met inkt of in een zachte stof een afdruk kan maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tool_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De douanier pakte zijn <w>stempel</w> om het paspoort te markeren.",
                            "target_lang_translations": {
                                "en": "The customs officer grabbed his <w>stamp</w> to mark the passport.",
                                "ru": "Таможенник взял свой <w>штамп</w>, чтобы поставить отметку в паспорте."
                            }
                        },
                        {
                            "text": "Je moet goed op het <w>stempel</w> drukken om een duidelijke afdruk te krijgen.",
                            "target_lang_translations": {
                                "en": "You have to press hard on the <w>stamp</w> to get a clear imprint.",
                                "ru": "Нужно хорошо надавить на <w>штамп</w>, чтобы получить четкий оттиск."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een stempel zetten"
                    ],
                    "target_lang_definitions": {
                        "en": "An object with a worked surface used to make an imprint with ink or in a soft material.",
                        "ru": "Предмет с рельефной поверхностью, с помощью которого можно сделать оттиск чернилами или на мягком материале."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stamp",
                                "target_lang_sense_clarification": "tool for making an imprint"
                            },
                            {
                                "target_lang_word": "seal",
                                "target_lang_sense_clarification": "device for making an impression"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "штамп",
                                "target_lang_sense_clarification": "инструмент"
                            },
                            {
                                "target_lang_word": "печать",
                                "target_lang_sense_clarification": "официальная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3d048a1-f5aa-3274-a1a9-a11e6950e9c6",
                    "sense_definition": "De zichtbare afdruk die met inkt of in reliëf is gemaakt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "impression_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>stempel</w> op de brief liet zien dat deze uit Amsterdam kwam.",
                            "target_lang_translations": {
                                "en": "The <w>stamp</w> on the letter showed that it came from Amsterdam.",
                                "ru": "<w>Штемпель</w> на письме показывал, что оно пришло из Амстердама."
                            }
                        },
                        {
                            "text": "Zonder een geldig <w>stempel</w> is dit toegangsbewijs niet bruikbaar.",
                            "target_lang_translations": {
                                "en": "Without a valid <w>stamp</w>, this admission ticket is not usable.",
                                "ru": "Без действительной <w>печати</w> этот входной билет недействителен."
                            }
                        }
                    ],
                    "synonyms": [
                        "afdruk",
                        "markering"
                    ],
                    "target_lang_definitions": {
                        "en": "The visible mark made with ink or in relief.",
                        "ru": "Видимый оттиск, сделанный чернилами или рельефно."
                    },
                    "translations": {
                        "en": [
                            {

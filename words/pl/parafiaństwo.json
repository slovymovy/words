                        "parafiańszczyzna",
                        "parafianizm"
                    ],
                    "antonyms": [
                        "światowość",
                        "otwartość"
                    ],
                    "common_phrases": [
                        "typowe parafiaństwo",
                        "umysłowe parafiaństwo"
                    ],
                    "target_lang_definitions": {

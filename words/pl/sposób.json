                {
                    "sense_id": "b1996e82-4f4d-3492-83b9-474008911a11",
                    "sense_definition": "Określona metoda, tryb lub forma postępowania służąca do wykonania czegoś lub osiągnięcia zamierzonego celu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "method_means",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy znaleźć inny <w>sposób</w> na rozwiązanie tego trudnego problemu.",
                            "target_lang_translations": {
                                "ru": "Мы должны найти другой <w>способ</w> решения этой трудной проблемы."
                            }
                        },
                        {
                            "text": "W ten <w>sposób</w> nigdy nie skończymy pracy na czas.",
                            "target_lang_translations": {
                                "ru": "Таким <w>способом</w> мы никогда не закончим работу вовремя."
                            }
                        }
                    ],
                    "synonyms": [
                        "metoda",
                        "środek",
                        "rozwiązanie"
                    ],
                    "common_phrases": [
                        "w ten sposób",
                        "w jaki sposób",
                        "w żaden sposób",
                        "domowym sposobem",
                        "najlepszy sposób"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный метод, режим или форма действий, служащая для выполнения чего-либо или достижения намеченной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "способ",

                {
                    "sense_id": "6f052c97-e64f-39d6-b538-ddba21217541",
                    "sense_definition": "Znajdujący się po boku lub na skraju czegoś, a nie w środku czy na przodzie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed zmianą pasa ruchu kierowca musi sprawdzić <w>boczne</w> lusterko.",
                            "target_lang_translations": {
                                "ru": "Перед сменой полосы движения водитель должен проверить <w>боковое</w> зеркало."
                            }
                        },
                        {
                            "text": "Do budynku prowadziły duże drzwi frontowe oraz mniejsze wejście <w>boczne</w>.",
                            "target_lang_translations": {
                                "ru": "В здание вели большие парадные двери, а также меньший <w>боковой</w> вход."
                            }
                        }
                    ],
                    "antonyms": [
                        "środkowy",
                        "centralny",
                        "przedni",
                        "tylny"
                    ],
                    "common_phrases": [
                        "boczne lusterko",
                        "boczne wejście",
                        "boczna ściana",
                        "boczna kieszeń",
                        "boczne drzwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся сбоку или на краю чего-либо, а не в центре или спереди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "боковой",
                                "target_lang_sense_clarification": "расположенный сбоку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c6897b9-45df-3ec5-a129-d6f7eda14221",
                    "sense_definition": "Taki, który nie jest główny, lecz dodatkowy, pomocniczy lub odchodzący od głównego traktu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "hierarchy_importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aby ominąć korki w centrum, pojechaliśmy wąską <w>boczną</w> drogą.",
                            "target_lang_translations": {
                                "ru": "Чтобы объехать пробки в центре, мы поехали по узкой <w>боковой</w> дороге."
                            }
                        },
                        {
                            "text": "W tej zabytkowej katedrze znajduje się piękny <w>boczny</w> ołtarz poświęcony świętemu.",
                            "target_lang_translations": {
                                "ru": "В этом старинном соборе находится красивый <w>боковой</w> алтарь, посвящённый святому."
                            }
                        }
                    ],
                    "synonyms": [
                        "poboczny",
                        "drugorzędny",
                        "dodatkowy"
                    ],
                    "antonyms": [
                        "główny"
                    ],
                    "common_phrases": [
                        "boczna droga",
                        "boczna ulica",
                        "boczny ołtarz",
                        "boczny tor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не главный, а дополнительный, вспомогательный или отходящий от основного пути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "боковой",
                                "target_lang_sense_clarification": "отходящий в сторону, смежный"
                            },
                            {
                                "target_lang_word": "второстепенный",
                                "target_lang_sense_clarification": "не основной, побочный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04d148f5-55da-32a5-b19f-bdf02f8234f7",
                    "sense_definition": "Skierowany w bok, działający w kierunku poprzecznym lub nadchodzący z boku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny wiatr <w>boczny</w> utrudniał pilotowi bezpieczne lądowanie na pasie.",
                            "target_lang_translations": {
                                "ru": "Сильный <w>боковой</w> ветер мешал пилоту совершить безопасную посадку на полосу."
                            }
                        },
                        {
                            "text": "Piłkarz wykonał szybkie podanie <w>boczne</w> do kolegi z drużyny.",
                            "target_lang_translations": {
                                "ru": "Футболист сделал быстрый <w>боковой</w> пас товарищу по команде."
                            }
                        }

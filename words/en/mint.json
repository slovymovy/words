                    "common_phrases": [
                        "fresh mint",
                        "mint tea",
                        "mint leaf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ароматное травянистое растение со свежим запахом листьев, часто используемое в кулинарии, чае или медицине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мята",
                                "target_lang_sense_clarification": "plant"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e96b5c00-a981-3267-bdbc-04cb8c7d4bb2",

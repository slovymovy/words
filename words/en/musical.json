    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4dabae97-7089-3a20-9d69-89a4a86b0358",
                    "sense_definition": "Of, belonging or relating to music, or to its performance or notation.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He learned to play a <w>musical</w> instrument at a very young age.",
                            "target_lang_translations": {
                                "ru": "Он научился играть на <w>музыкальном</w> инструменте в очень юном возрасте."
                            }
                        },
                        {
                            "text": "The <w>musical</w> score for the film was composed by a famous artist.",
                            "target_lang_translations": {
                                "ru": "<w>Музыкальная</w> партитура к фильму была написана известным композитором."
                            }
                        }
                    ],
                    "antonyms": [
                        "nonmusical"
                    ],
                    "common_phrases": [
                        "musical instrument",
                        "musical note",
                        "musical style",
                        "musical score"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к музыке, связанный с её исполнением или записью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "музыкальный",
                                "target_lang_sense_clarification": "относящийся к музыке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d64e398-3974-37c7-b3aa-9d0000753075",
                    "sense_definition": "Gifted or skilled in music; having a talent or fondness for music.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She comes from a very <w>musical</w> family where everyone plays an instrument.",
                            "target_lang_translations": {
                                "ru": "Она из очень <w>музыкальной</w> семьи, где все играют на инструментах."
                            }
                        },
                        {
                            "text": "Even though he has a <w>musical</w> ear, he never learned to read notes.",
                            "target_lang_translations": {
                                "ru": "Хотя у него <w>музыкальный</w> слух, он так и не научился читать ноты."
                            }
                        }
                    ],
                    "antonyms": [
                        "unmusical",
                        "tone-deaf"
                    ],
                    "common_phrases": [
                        "musical ear",
                        "musical talent",
                        "musical family"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одарённый музыкально; имеющий талант или любовь к музыке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "музыкальный",
                                "target_lang_sense_clarification": "одарённый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8910bde0-6b1b-3399-b8d9-2fd09d0730ae",
                    "sense_definition": "Pleasing to the ear; melodious or harmonious.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her <w>musical</w> voice made the story sound like a song.",
                            "target_lang_translations": {
                                "ru": "Её <w>мелодичный</w> голос превратил рассказ в песню."
                            }
                        },
                        {
                            "text": "The stream made a <w>musical</w> sound as it flowed over the smooth rocks.",
                            "target_lang_translations": {
                                "ru": "Ручей издавал <w>мелодичный</w> звук, протекая по гладким камням."
                            }
                        }
                    ],
                    "synonyms": [
                        "melodious",
                        "tuneful",
                        "harmonious",
                        "euphonious"
                    ],
                    "antonyms": [
                        "discordant",
                        "unmusical",
                        "harsh"
                    ],
                    "common_phrases": [
                        "musical voice",
                        "musical laughter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приятный для слуха; мелодичный или гармоничный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелодичный",
                                "target_lang_sense_clarification": "приятный на слух"
                            },
                            {
                                "target_lang_word": "музыкальный",
                                "target_lang_sense_clarification": "звучный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48fadb65-7c97-3e68-a6a9-7affa18374d7",
                    "sense_definition": "Pertaining to games in which players move while music plays and stop when it ends, or situations involving repeated changing of positions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children laughed while playing <w>musical</w> chairs at the birthday party.",
                            "target_lang_translations": {
                                "ru": "Дети смеялись, играя в <w>музыкальные</w> стулья на дне рождения."
                            }
                        },
                        {
                            "text": "The recent cabinet reshuffle felt like a game of <w>musical</w> chairs.",
                            "target_lang_translations": {
                                "ru": "Недавние перестановки в кабинете министров напоминали игру в <w>музыкальные</w> стулья."
                            }
                        }
                    ],
                    "common_phrases": [
                        "musical chairs",
                        "musical statues"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к играм, в которых участники двигаются под музыку и останавливаются, когда она заканчивается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "музыкальный",
                                "target_lang_sense_clarification": "в названиях игр"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2e203e1e-4a7e-3eb7-bc92-61a9901146db",
                    "sense_definition": "A stage performance, show or film that involves singing, dancing and musical numbers performed by the cast as well as acting.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bought tickets to see a famous <w>musical</w> in London next week.",
                            "target_lang_translations": {
                                "ru": "Мы купили билеты на известный <w>мюзикл</w> в Лондоне на следующую неделю."
                            }
                        },
                        {
                            "text": "The school puts on a spring <w>musical</w> every year for the parents.",
                            "target_lang_translations": {
                                "ru": "Школа каждый год ставит весенний <w>мюзикл</w> для родителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "musical comedy"
                    ],
                    "common_phrases": [
                        "Broadway musical",
                        "hit musical",
                        "movie musical"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сценическое представление, шоу или фильм, включающие пение и танцы в дополнение к актёрской игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мюзикл",
                                "target_lang_sense_clarification": "жанр спектакля или фильма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c2711a5-2995-3501-b098-d8386e5594cc",
                    "sense_definition": "A meeting or a party for a musical entertainment.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ladies gathered in the parlor for an afternoon <w>musical</w>.",
                            "target_lang_translations": {
                                "ru": "Дамы собрались в гостиной на дневной <w>музыкальный вечер</w>."
                            }
                        },
                        {
                            "text": "She hosted a small <w>musical</w> to showcase her daughter's piano skills.",
                            "target_lang_translations": {
                                "ru": "Она устроила небольшой <w>музыкальный вечер</w>, чтобы продемонстрировать навыки игры на фортепиано своей дочери."
                            }
                        }
                    ],
                    "synonyms": [
                        "musicale"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Встреча или вечеринка для музыкального развлечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "музыкальный вечер",
                                "target_lang_sense_clarification": "мероприятие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

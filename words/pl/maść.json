{
    "word_family": [
        "maścić",
        "umaszczenie",
        "namaszczenie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ae3cf0b-bd3f-3d43-a81d-6b3f1621a71c",
                    "sense_definition": "Lecznicza substancja o półstałej, tłustej konsystencji przeznaczona do smarowania skóry.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "medical_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz przepisał mi skuteczną <w>maść</w> na ból pleców.",
                            "target_lang_translations": {
                                "ru": "Врач прописал мне эффективную <w>мазь</w> от боли в спине."
                            }
                        },
                        {
                            "text": "Musisz smarować to miejsce <w>maścią</w> dwa razy dziennie, rano i wieczorem.",
                            "target_lang_translations": {
                                "ru": "Ты должен мазать это место <w>мазью</w> два раза в день, утром и вечером."
                            }
                        }
                    ],
                    "common_phrases": [
                        "maść przeciwbólowa",
                        "smarować maścią",
                        "maść na uczulenie",
                        "tłusta maść"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лекарственное вещество полутвердой, жирной консистенции, предназначенное для нанесения на кожу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мазь",
                                "target_lang_sense_clarification": "лекарственное средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "126e2d95-b2e3-3917-81bc-523f92898790",
                    "sense_definition": "Barwa sierści zwierząt, szczególnie koni i bydła.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_features",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten piękny koń ma rzadko spotykaną, kasztanowatą <w>maść</w>.",
                            "target_lang_translations": {
                                "ru": "У этой красивой лошади редкая каштановая <w>масть</w>."
                            }
                        },
                        {
                            "text": "Hodowca zwracał szczególną uwagę na <w>maść</w> i budowę ciała bydła.",
                            "target_lang_translations": {
                                "ru": "Заводчик обращал особое внимание на <w>масть</w> и телосложение скота."
                            }
                        }
                    ],
                    "synonyms": [
                        "umaszczenie"
                    ],
                    "common_phrases": [
                        "koń gniadej maści",
                        "maść sierści",
                        "jednolitej maści"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окрас шерсти животных, особенно лошадей и крупного рогатого скота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "масть",
                                "target_lang_sense_clarification": "окрас животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "643c46e6-a623-3047-843e-852fcf4ae7be",
                    "sense_definition": "Rodzaj, gatunek lub kategoria ludzi albo rzeczy (często używane w wyrażeniach idiomatycznych).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "classification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na spotkanie przyszli artyści i politycy wszelkiej <w>maści</w>.",
                            "target_lang_translations": {
                                "ru": "На встречу пришли художники и политики всех <w>мастей</w>."
                            }
                        },
                        {
                            "text": "Unikam oszustów tej <w>maści</w>, ponieważ zawsze sprawiają kłopoty.",
                            "target_lang_translations": {
                                "ru": "Я избегаю мошенников такой <w>масти</w>, так как они всегда доставляют неприятности."
                            }
                        }
                    ],
                    "synonyms": [
                        "rodzaj"
                    ],
                    "common_phrases": [
                        "ludzie wszelkiej maści",
                        "różnej maści",
                        "tej maści"
                    ],
                    "target_lang_definitions": {
                        "ru": "Род, вид или категория людей или вещей (часто используется в идиоматических выражениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "масть",
                                "target_lang_sense_clarification": "разряд, категория (в идиомах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2b3456f-b2a6-368d-986c-4cf322efb609",
                    "sense_definition": "Smalec (tłuszcz zwierzęcy).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "food_regional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodyni podała na stół chleb posmarowany świeżą <w>maścią</w> ze skwarkami.",
                            "target_lang_translations": {
                                "ru": "Хозяйка подала на стол хлеб, намазанный свежим <w>смальцем</w> со шкварками."
                            }
                        },
                        {
                            "text": "W tym regionie na smalec starsi ludzie wciąż mówią <w>maść</w>.",
                            "target_lang_translations": {
                                "ru": "В этом регионе пожилые люди всё ещё называют смалец <w>мазью</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "gwara (Bukowina)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Смалец (животный жир)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смалец",
                                "target_lang_sense_clarification": "топленый жир"
                            },
                            {
                                "target_lang_word": "жир",
                                "target_lang_sense_clarification": "продукт питания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1affb990-03d7-3528-ac64-a1c414501543",
                    "sense_definition": "Druga osoba liczby pojedynczej trybu rozkazującego czasownika <w>maścić</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <w>maść</w> tak grubo tego chleba masłem, bo zabraknie dla innych.",
                            "target_lang_translations": {
                                "ru": "Не <w>мажь</w> хлеб маслом так густо, а то другим не хватит."
                            }
                        },
                        {
                            "text": "<w>Maść</w> buty pastą regularnie, żeby skóra nie popękała na mrozie.",
                            "target_lang_translations": {
                                "ru": "<w>Смазывай</w> ботинки кремом регулярно, чтобы кожа не потрескалась на морозе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "tryb rozkazujący"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма второго лица единственного числа повелительного наклонения глагола \"maścić\" (мазать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мажь",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            },
                            {
                                "target_lang_word": "намазывай",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "ea2cf7fc-8d06-30c3-9b3e-d6e631fd901d",
                    "sense_definition": "Beschikbaar maken voor het publiek, bijvoorbeeld door verkoop, vertoning of publicatie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "publiceren",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De populaire band gaat volgende maand een nieuw album <w>uitbrengen</w>.",
                            "target_lang_translations": {
                                "ru": "Популярная группа собирается <w>выпустить</w> новый альбом в следующем месяце.",
                                "en": "The popular band is going to <w>release</w> a new album next month."
                            }
                        },
                        {
                            "text": "Het bedrijf heeft gisteren een persbericht <w>uitgebracht</w> over de fusie.",
                            "target_lang_translations": {
                                "ru": "Вчера компания <w>опубликовала</w> пресс-релиз о слиянии.",
                                "en": "The company <w>issued</w> a press release about the merger yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "publiceren",
                        "lanceren",
                        "uitgeven"
                    ],
                    "antonyms": [
                        "intrekken"
                    ],
                    "common_phrases": [
                        "een album uitbrengen",
                        "een film uitbrengen",
                        "verslag uitbrengen",
                        "advies uitbrengen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать доступным для публики, например, путем продажи, показа или публикации.",
                        "en": "To make available to the public, for example by sale, screening, or publication."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "альбом, фильм, продукцию"
                            },
                            {
                                "target_lang_word": "публиковать",
                                "target_lang_sense_clarification": "отчет, статью, пресс-релиз"
                            },
                            {
                                "target_lang_word": "издавать",
                                "target_lang_sense_clarification": "книгу, указ"
                            }
                        ],
                        "en": [

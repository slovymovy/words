                        "Ierse stout"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень темное пиво с жженым вкусом, родом из Великобритании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стаут",
                                "target_lang_sense_clarification": "сорт темного пива"
                            }
                        ]
                    }
                }
            ]
        }

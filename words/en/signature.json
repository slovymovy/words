{
    "word_family": [
        "sign",
        "signatory",
        "signer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5d9943ed-b46d-3311-b625-8077937e8d06",
                    "sense_definition": "A person's name written in a distinctive way by themselves, used to identify them or authorize a document.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "identity_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please put your <w>signature</w> at the bottom of the page."
                        },
                        {
                            "text": "The bank refused the check because the <w>signature</w> did not match."
                        }
                    ],
                    "common_phrases": [
                        "official signature",
                        "witness a signature",
                        "forge a signature"
                    ]
                },
                {
                    "sense_id": "2646994d-2068-3df0-82a3-16a02225f317",
                    "sense_definition": "A block of text or images automatically appended to the end of an email or forum post, often containing contact details.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_text",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to update my email <w>signature</w> with my new phone number."
                        },
                        {
                            "text": "Her forum <w>signature</w> includes a funny quote from a movie."
                        }
                    ],
                    "synonyms": [
                        "sig",
                        "siggy"
                    ],
                    "common_phrases": [
                        "email signature"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "synonyms 'sig' and 'siggy' are slang/informal clippings"
                        }
                    ]
                },
                {
                    "sense_id": "ea6ce719-d2b3-333a-b9ea-9ec8927da0f6",
                    "sense_definition": "A distinguishing feature, product, or quality that identifies a particular person or thing.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "characteristic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The truffle pasta is one of the chef's most famous <w>signatures</w>."
                        },
                        {
                            "text": "Bright colors are the <w>signature</w> of this artist's work."
                        }
                    ]
                },
                {
                    "sense_id": "864a1024-a92b-3724-8ca8-d30174c5b68a",
                    "sense_definition": "Signs placed at the beginning of a musical staff to indicate the key or tempo.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_notation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The time <w>signature</w> changed from 4/4 to 3/4 in the middle of the song."
                        },
                        {
                            "text": "You must check the key <w>signature</w> to know which notes are sharp or flat."
                        }
                    ],
                    "common_phrases": [
                        "time signature",
                        "key signature"
                    ]
                },
                {
                    "sense_id": "4f783a19-18af-38a7-910b-827e7c0fcdef",
                    "sense_definition": "The formal act of signing a document or agreement.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "identity_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>signature</w> of the peace treaty was a historic moment."
                        },
                        {
                            "text": "We are waiting for the <w>signature</w> of the contract before starting work."
                        }
                    ]
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f8ccf859-9597-3965-af2d-a061e1efea1d",
                    "sense_definition": "Distinctive and characteristic of a particular person or thing; serving to identify someone.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "characteristic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The restaurant is known for its <w>signature</w> dish, the spicy crab."
                        },
                        {
                            "text": "Wearing a fedora became his <w>signature</w> look over the years."
                        }
                    ],
                    "synonyms": [
                        "iconic"
                    ],
                    "common_phrases": [
                        "signature dish",
                        "signature style",
                        "signature move",
                        "signature scent"
                    ]
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "43cf6165-d93b-3fc4-8902-4055a3d800d4",
                    "sense_definition": "Девушка или молодая женщина (часто используется как вежливое или ироничное обращение).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта милая <w>барышня</w> помогла мне найти дорогу.",
                            "target_lang_translations": {
                                "en": "This sweet <w>young lady</w> helped me find the way."
                            }
                        },
                        {
                            "text": "Послушайте, <w>барышня</w>, вы забыли свой билет на столе.",
                            "target_lang_translations": {
                                "en": "Listen, <w>miss</w>, you forgot your ticket on the table."
                            }
                        }
                    ],
                    "synonyms": [
                        "девушка",
                        "девица"
                    ],
                    "common_phrases": [
                        "молодая барышня",
                        "городская барышня"
                    ],
                    "target_lang_definitions": {
                        "en": "A girl or young woman (often used as a polite or ironic form of address)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "young lady",
                                "target_lang_sense_clarification": "polite reference or address"
                            },
                            {
                                "target_lang_word": "miss",
                                "target_lang_sense_clarification": "form of address"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9917c554-6ab4-3357-ac8b-9455c2f2739e",
                    "sense_definition": "Девушка из дворянской или богатой семьи в дореволюционной России.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Юная <w>барышня</w> целыми днями читала французские романы.",
                            "target_lang_translations": {
                                "en": "The <w>young lady</w> read French novels all day long."
                            }
                        },
                        {
                            "text": "Служанка помогала <w>барышне</w> одеваться к званому ужину.",
                            "target_lang_translations": {
                                "en": "The maid helped the <w>young lady</w> dress for the dinner party."
                            }
                        }
                    ],
                    "common_phrases": [
                        "уездная барышня",
                        "богатая барышня"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A girl from a noble or wealthy family in pre-revolutionary Russia."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "young lady",
                                "target_lang_sense_clarification": "historical/noble context"
                            },
                            {
                                "target_lang_word": "gentlewoman",
                                "target_lang_sense_clarification": "archaic, noble birth"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd369f3d-c8e0-303a-a496-aff951814675",
                    "sense_definition": "Избалованная, изнеженная девушка, которая избегает физического труда.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не будь такой <w>барышней</w>, возьми тряпку и вытри пыль.",
                            "target_lang_translations": {
                                "en": "Don't be such a <w>princess</w>, take a rag and wipe the dust."
                            }
                        },
                        {
                            "text": "Она настоящая кисейная <w>барышня</w>, ничего не умеет делать сама.",
                            "target_lang_translations": {
                                "en": "She is a real delicate <w>flower</w>, she doesn't know how to do anything herself."
                            }
                        }
                    ],
                    "synonyms": [
                        "белоручка"
                    ],
                    "common_phrases": [
                        "кисейная барышня",
                        "изнеженная барышня"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A spoiled, effeminate girl who avoids physical labor."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "princess",
                                "target_lang_sense_clarification": "pejorative, spoiled girl"
                            },
                            {
                                "target_lang_word": "delicate flower",
                                "target_lang_sense_clarification": "ironic, fragile person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "357448ae-f3d2-38d5-a41c-032e57d8e992",
                    "sense_definition": "Работница телефонной станции, соединяющая абонентов.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Алло, <w>барышня</w>, соедините меня скорее с номером пять!",
                            "target_lang_translations": {
                                "en": "Hello, <w>operator</w>, connect me quickly with number five!"
                            }
                        },
                        {
                            "text": "Уставшая <w>барышня</w> на станции перепутала провода.",
                            "target_lang_translations": {
                                "en": "The tired <w>operator</w> at the station mixed up the wires."
                            }
                        }
                    ],
                    "synonyms": [
                        "телефонистка"
                    ],
                    "common_phrases": [
                        "телефонная барышня"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A female worker at a telephone exchange connecting subscribers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "operator",
                                "target_lang_sense_clarification": "telephone exchange worker"
                            }
                        ]
                    }
                }
            ]
        }
    ]

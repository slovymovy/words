                },
                {
                    "sense_id": "4b221688-8bca-3c7f-8c14-5580c9386ab3",
                    "sense_definition": "Een kapsel waarbij het haar op het voorhoofd naar voren is gekamd en boven de wenkbrauwen is afgeknipt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {

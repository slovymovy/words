            "senses": [
                {
                    "sense_id": "5f8d487b-5297-34f3-b0b9-d07f698442a8",
                    "sense_definition": "The act of establishing oneself in a new place, or the process of sinking, adjusting, or coming to rest.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>settling</w> of the foundation caused minor cracks in the walls.",
                            "target_lang_translations": {
                                "ru": "<w>Осадка</w> фундамента привела к появлению небольших трещин в стенах."
                            }
                        },
                        {
                            "text": "The <w>settling</w> of the western frontier took many decades.",
                            "target_lang_translations": {
                                "ru": "<w>Заселение</w> западных рубежей заняло много десятилетий."
                            }
                        }
                    ],
                    "synonyms": [
                        "establishment",
                        "colonization",
                        "subsidence"
                    ],
                    "target_lang_definitions": {

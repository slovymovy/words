                    "sense_id": "d252531c-9829-3e53-b23e-f5bb1d893d45",
                    "sense_definition": "Kierunek studiów lub specjalizacja w szkole muzycznej kształcąca w zakresie dyrygowania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat marzy o tym, aby studiować <w>dyrygenturę</w> na akademii muzycznej.",
                            "target_lang_translations": {
                                "ru": "Мой брат мечтает изучать <w>дирижирование</w> в музыкальной академии."
                            }
                        },
                        {
                            "text": "Egzaminy wstępne na <w>dyrygenturę</w> wymagają od kandydatów doskonałego słuchu.",
                            "target_lang_translations": {
                                "ru": "Вступительные экзамены на <w>дирижирование</w> требуют от кандидатов идеального слуха."
                            }
                        }
                    ],
                    "common_phrases": [

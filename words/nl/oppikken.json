{
    "word_family": [
        "pikken",
        "inpikken",
        "afpikken",
        "uitpikken",
        "meepikken"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1065a401-705e-35f6-afd1-819dd3c08834",
                    "sense_definition": "Iemand of iets ergens ophalen om mee te nemen, meestal met een voertuig.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal je morgenochtend om acht uur bij het station <w>oppikken</w>.",
                            "target_lang_translations": {
                                "en": "I will <w>pick</w> you <w>up</w> at the station at eight o'clock tomorrow morning.",
                                "ru": "Я <w>заберу</w> тебя завтра утром в восемь часов у вокзала."
                            }
                        },
                        {
                            "text": "We hebben onderweg naar huis nog snel even een pizza <w>opgepikt</w>.",
                            "target_lang_translations": {
                                "en": "We quickly <w>picked up</w> a pizza on the way home.",
                                "ru": "По дороге домой мы быстро <w>захватили</w> пиццу."
                            }
                        }
                    ],
                    "synonyms": [
                        "ophalen",
                        "meenemen"
                    ],
                    "antonyms": [
                        "afzetten",
                        "brengen"
                    ],
                    "common_phrases": [
                        "iemand oppikken",
                        "met de auto oppikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To collect someone or something from somewhere to take along, usually with a vehicle.",
                        "ru": "Забирать кого-то или что-то, обычно на транспортном средстве."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pick up",
                                "target_lang_sense_clarification": "to collect someone or something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "забирать",
                                "target_lang_sense_clarification": "заезжать за кем-то/чем-то"
                            },
                            {
                                "target_lang_word": "подбирать",
                                "target_lang_sense_clarification": "брать пассажира по пути"
                            },
                            {
                                "target_lang_word": "захватывать",
                                "target_lang_sense_clarification": "брать с собой попутно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "974a196c-e937-35a0-a64b-c864178b8aa0",
                    "sense_definition": "Iets met de hand vastpakken en van de grond of een ander oppervlak tillen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij bukte zich om het gevallen muntstuk <w>op</w> te <w>pikken</w>.",
                            "target_lang_translations": {
                                "en": "He bent down to <w>pick up</w> the fallen coin.",
                                "ru": "Он наклонился, чтобы <w>поднять</w> упавшую монету."
                            }
                        },
                        {
                            "text": "De vrijwilligers <w>pikten</w> al het zwerfafval in het park <w>op</w>.",
                            "target_lang_translations": {
                                "en": "The volunteers <w>picked up</w> all the litter in the park.",
                                "ru": "Волонтеры <w>собрали</w> весь мусор в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "oprapen",
                        "pakken"
                    ],
                    "antonyms": [
                        "laten vallen",
                        "neerleggen"
                    ],
                    "common_phrases": [
                        "vuilnis oppikken",
                        "iets van de grond oppikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To grasp something with the hand and lift it from the ground or another surface.",
                        "ru": "Поднимать что-то рукой с земли или другой поверхности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pick up",
                                "target_lang_sense_clarification": "to lift from the ground"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поднимать",
                                "target_lang_sense_clarification": "брать что-то упавшее"
                            },
                            {
                                "target_lang_word": "подбирать",
                                "target_lang_sense_clarification": "собирать с земли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "242d1937-6d5a-3692-99a6-d5093a9f84a1",
                    "sense_definition": "Iets leren, begrijpen of waarnemen, vaak zonder er veel moeite voor te doen of door goed op te letten.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens zijn vakantie in Italië heeft hij een paar woorden Italiaans <w>opgepikt</w>.",
                            "target_lang_translations": {
                                "en": "During his holiday in Italy, he <w>picked up</w> a few words of Italian.",
                                "ru": "Во время отпуска в Италии он <w>схватил</w> пару итальянских слов."
                            }
                        },
                        {
                            "text": "De antenne is sterk genoeg om signalen van ver weg <w>op</w> te <w>pikken</w>.",
                            "target_lang_translations": {
                                "en": "The antenna is strong enough to <w>pick up</w> signals from far away.",
                                "ru": "Антенна достаточно мощная, чтобы <w>улавливать</w> сигналы издалека."
                            }
                        },
                        {
                            "text": "Na een lange pauze probeerde hij de draad weer <w>op</w> te <w>pikken</w>.",
                            "target_lang_translations": {
                                "en": "After a long break, he tried to <w>pick up</w> the thread again.",
                                "ru": "После долгой паузы он попытался снова <w>подхватить</w> нить разговора."
                            }
                        }
                    ],
                    "synonyms": [
                        "opvangen",
                        "leren",
                        "verwerven"
                    ],
                    "common_phrases": [
                        "een signaal oppikken",
                        "een taal oppikken",
                        "de draad oppikken",
                        "een gerucht oppikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To learn, understand, or perceive something, often without much effort or by paying close attention.",
                        "ru": "Усваивать знания, улавливать сигналы или возобновлять прерванное действие."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pick up",
                                "target_lang_sense_clarification": "to acquire a skill or detect a signal"
                            },
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "to hear or understand something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "улавливать",
                                "target_lang_sense_clarification": "сигналы, звуки, намеки"
                            },
                            {
                                "target_lang_word": "схватывать",
                                "target_lang_sense_clarification": "быстро усваивать информацию"
                            },
                            {
                                "target_lang_word": "подхватывать",
                                "target_lang_sense_clarification": "продолжать мысль или действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02150809-6c43-3d0e-9f11-28d5919e959c",
                    "sense_definition": "Voedsel met de snavel pakken en opeten (door vogels).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kippen liepen door de tuin en <w>pikten</w> de graankorrels <w>op</w>.",
                            "target_lang_translations": {
                                "en": "The chickens walked through the garden and <w>pecked up</w> the grains.",
                                "ru": "Куры гуляли по саду и <w>клевали</w> зерна."
                            }
                        },
                        {
                            "text": "De vogels hebben al het brood dat we strooiden snel <w>opgepikt</w>.",
                            "target_lang_translations": {
                                "en": "The birds quickly <w>picked up</w> all the bread we scattered.",
                                "ru": "Птицы быстро <w>склевали</w> весь хлеб, который мы разбросали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "graantjes oppikken"
                    ],
                    "target_lang_definitions": {
                        "en": "To take food with the beak and eat it (used for birds).",
                        "ru": "Хватать и съедать корм клювом (о птицах)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "peck up",
                                "target_lang_sense_clarification": "to eat using a beak"
                            },
                            {
                                "target_lang_word": "pick up",
                                "target_lang_sense_clarification": "general action of taking something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "склевывать",
                                "target_lang_sense_clarification": "съедать полностью"
                            },
                            {
                                "target_lang_word": "клевать",
                                "target_lang_sense_clarification": "есть клювом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdfa2ddc-050a-3e05-8d4f-3ecb5dd4f209",
                    "sense_definition": "Iemand of iets uit het water halen, vaak in een reddingssituatie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "rescue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De drenkeling werd door de reddingsboot uit de koude zee <w>opgepikt</w>.",
                            "target_lang_translations": {
                                "en": "The drowning person was <w>picked up</w> from the cold sea by the lifeboat.",
                                "ru": "Утопающий был <w>подобран</w> спасательной шлюпкой из холодного моря."
                            }
                        },
                        {
                            "text": "De helikopter kon de bemanning veilig van het zinkende schip <w>oppikken</w>.",
                            "target_lang_translations": {
                                "en": "The helicopter was able to safely <w>pick up</w> the crew from the sinking ship.",
                                "ru": "Вертолет смог безопасно <w>забрать</w> экипаж с тонущего корабля."
                            }
                        }
                    ],
                    "synonyms": [
                        "opvissen",
                        "redden"
                    ],
                    "target_lang_definitions": {
                        "en": "To take someone or something out of the water, often in a rescue situation.",
                        "ru": "Подбирать или спасать кого-то из воды или опасной ситуации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pick up",
                                "target_lang_sense_clarification": "to rescue or retrieve from water"
                            },
                            {
                                "target_lang_word": "fish out",
                                "target_lang_sense_clarification": "to pull out of water (informal)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подбирать",
                                "target_lang_sense_clarification": "спасать, поднимать на борт"
                            },
                            {
                                "target_lang_word": "вылавливать",
                                "target_lang_sense_clarification": "доставать из воды"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкус",
                                "target_lang_sense_clarification": "общее вкусовое впечатление"
                            },
                            {
                                "target_lang_word": "аромат",
                                "target_lang_sense_clarification": "запах, букет"
                            },
                            {
                                "target_lang_word": "привкус",
                                "target_lang_sense_clarification": "оттенок вкуса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26ff2835-fb44-3d6c-bc36-497a8cc7ba63",
                    "sense_definition": "A substance added to food or drink to give it a specific taste.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "taste_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This beverage contains no artificial colors or <w>flavors</w>.",
                            "target_lang_translations": {
                                "ru": "Этот напиток не содержит искусственных красителей или <w>ароматизаторов</w>."
                            }
                        },
                        {
                            "text": "The chef used a natural vanilla <w>flavor</w> in the cake mix.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар использовал натуральный ванильный <w>ароматизатор</w> в смеси для торта."
                            }
                        }
                    ],
                    "synonyms": [
                        "flavoring",
                        "seasoning"
                    ],
                    "common_phrases": [
                        "artificial flavor",
                        "natural flavor",
                        "add flavor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество, добавляемое в пищу или напиток для придания определенного вкуса."

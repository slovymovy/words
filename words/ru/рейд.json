    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "79de26f7-abd9-357b-b00c-a85a13d47abb",
                    "sense_definition": "Внезапная проверка соблюдения правил или законов, проводимая государственными службами или общественными организациями.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "inspection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция провела ночной <w>рейд</w> по выявлению нелегальных мигрантов.",
                            "target_lang_translations": {
                                "en": "The police conducted a night <w>raid</w> to identify illegal migrants."
                            }
                        },
                        {
                            "text": "Очередной <w>рейд</w> инспекторов выявил множество нарушений в работе рынка.",
                            "target_lang_translations": {
                                "en": "Another <w>raid</w> by inspectors revealed many violations in the market's operation."
                            }
                        }
                    ],
                    "synonyms": [
                        "проверка",
                        "облава"
                    ],
                    "common_phrases": [
                        "полицейский рейд",
                        "рейд по магазинам",
                        "провести рейд"
                    ],
                    "target_lang_definitions": {
                        "en": "A sudden surprise visit by police or other authorities to search a building or check for illegal activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "raid",
                                "target_lang_sense_clarification": "police or inspection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "184644d3-2f16-3027-ad5a-ce428af1deb6",
                    "sense_definition": "Стремительное продвижение или нападение военных сил в тыл противника с боевой целью.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_attack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отряд спецназа совершил дерзкий <w>рейд</w> в тыл врага для захвата документов.",
                            "target_lang_translations": {
                                "en": "A special forces unit carried out a daring <w>raid</w> behind enemy lines to seize documents."
                            }
                        },
                        {
                            "text": "Генерал приказал начать боевой <w>рейд</w> на рассвете.",
                            "target_lang_translations": {
                                "en": "The general ordered a combat <w>raid</w> to start at dawn."
                            }
                        }
                    ],
                    "synonyms": [
                        "набег",
                        "налёт",
                        "вылазка"
                    ],
                    "common_phrases": [
                        "боевой рейд",
                        "рейд по тылам",
                        "ночной рейд"
                    ],
                    "target_lang_definitions": {
                        "en": "A rapid surprise attack on an enemy by troops, aircraft, or other armed forces."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "raid",
                                "target_lang_sense_clarification": "military attack"
                            },
                            {
                                "target_lang_word": "incursion",
                                "target_lang_sense_clarification": "hostile entrance into territory"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c36d7b6-85e3-343b-a033-d8631a8a2862",
                    "sense_definition": "Водное пространство вблизи берега или порта, удобное для якорной стоянки судов.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "maritime",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Торговое судно уже три дня стоит на внешнем <w>рейде</w> в ожидании разгрузки.",
                            "target_lang_translations": {
                                "en": "The merchant ship has been waiting at the outer <w>roadstead</w> for three days to unload."
                            }
                        },
                        {
                            "text": "Капитан решил переждать сильный шторм на безопасном <w>рейде</w>.",
                            "target_lang_translations": {
                                "en": "The captain decided to wait out the strong storm at a safe <w>anchorage</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "стоянка"
                    ],
                    "common_phrases": [
                        "стоять на рейде",
                        "внешний рейд",
                        "охрана рейда"
                    ],
                    "target_lang_definitions": {
                        "en": "A partly sheltered stretch of water near the shore where ships can ride at anchor."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "roadstead",
                                "target_lang_sense_clarification": "maritime term"
                            },
                            {
                                "target_lang_word": "anchorage",
                                "target_lang_sense_clarification": "place for anchoring"
                            },
                            {
                                "target_lang_word": "roads",
                                "target_lang_sense_clarification": "nautical term"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
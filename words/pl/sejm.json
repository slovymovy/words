                    "sense_id": "71dee8d2-d3c9-3962-9ea8-cce284aec9c5",
                    "sense_definition": "Niższa izba polskiego parlamentu, sprawująca władzę ustawodawczą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "politics_institution",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "Obywatele wybierają posłów do <w>Sejmu</w> w wyborach powszechnych co cztery lata.",
                            "target_lang_translations": {
                                "ru": "Граждане избирают депутатов в <w>Сейм</w> на всеобщих выборах каждые четыре года."
                            }
                        },
                        {
                            "text": "Marszałek <w>Sejmu</w> otworzył pierwsze posiedzenie nowej kadencji.",
                            "target_lang_translations": {
                                "ru": "Маршал <w>Сейма</w> открыл первое заседание нового созыва."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Marszałek Sejmu",
                        "wybory do Sejmu",
                        "posiedzenie Sejmu",
                        "kadencja Sejmu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя палата польского парламента, осуществляющая законодательную власть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сейм",
                                "target_lang_sense_clarification": "парламент Польши"

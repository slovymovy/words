    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4c386f92-a31a-39c7-85c8-fab81e5008bf",
                    "sense_definition": "An examination of knowledge or ability, often given in school or university.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "evaluation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have to study hard because I have a math <w>test</w> tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Мне нужно усердно учиться, потому что завтра утром у меня <w>контрольная</w> по математике."
                            }
                        },
                        {
                            "text": "The teacher gave the students a difficult spelling <w>test</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель дал ученикам сложный <w>тест</w> по правописанию."
                            }
                        }
                    ],
                    "synonyms": [
                        "examination",
                        "quiz",
                        "examination"
                    ],
                    "common_phrases": [
                        "pass a test",
                        "fail a test",
                        "take a test",
                        "study for a test"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверка знаний или способностей, часто проводимая в школе или университете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тест",
                                "target_lang_sense_clarification": "общая проверка знаний"
                            },
                            {
                                "target_lang_word": "контрольная работа",
                                "target_lang_sense_clarification": "в школе"
                            },
                            {
                                "target_lang_word": "экзамен",
                                "target_lang_sense_clarification": "формальная проверка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75ee7d60-84a3-3a05-8395-aa43445f5733",
                    "sense_definition": "A procedure intended to establish the quality, performance, or reliability of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "evaluation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor ordered a blood <w>test</w> to check the patient's cholesterol levels.",
                            "target_lang_translations": {
                                "ru": "Врач назначил <w>анализ</w> крови, чтобы проверить уровень холестерина у пациента."
                            }
                        },
                        {
                            "text": "We took the car for a road <w>test</w> before deciding to buy it.",
                            "target_lang_translations": {
                                "ru": "Мы взяли машину на дорожное <w>испытание</w>, прежде чем решить купить её."
                            }
                        }
                    ],
                    "synonyms": [
                        "trial",
                        "examination",
                        "challenge"
                    ],
                    "common_phrases": [
                        "blood test",
                        "road test",
                        "put to the test",
                        "safety test"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процедура, предназначенная для установления качества, эффективности или надежности чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анализ",
                                "target_lang_sense_clarification": "медицинский"
                            },
                            {
                                "target_lang_word": "испытание",
                                "target_lang_sense_clarification": "техническое"
                            },
                            {
                                "target_lang_word": "тест",
                                "target_lang_sense_clarification": "общий"
                            },
                            {
                                "target_lang_word": "проба",
                                "target_lang_sense_clarification": "образец для проверки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27f35146-8715-331a-9135-4d62e8310aad",
                    "sense_definition": "A difficult situation or task that shows how strong, capable, or durable someone or something is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "challenge_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Climbing the steep mountain was a real <w>test</w> of our physical stamina.",
                            "target_lang_translations": {
                                "ru": "Восхождение на крутую гору стало настоящим <w>испытанием</w> нашей физической выносливости."
                            }
                        },
                        {
                            "text": "The long separation was a severe <w>test</w> of their relationship.",
                            "target_lang_translations": {
                                "ru": "Долгая разлука стала суровым <w>испытанием</w> для их отношений."
                            }
                        }
                    ],
                    "synonyms": [
                        "challenge",
                        "trial",
                        "mountain"
                    ],
                    "antonyms": [
                        "breeze",
                        "cakewalk"
                    ],
                    "common_phrases": [
                        "test of strength",
                        "test of character",
                        "endurance test",
                        "ultimate test"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудная ситуация или задача, которая показывает силу, способности или стойкость кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испытание",
                                "target_lang_sense_clarification": "жизненная трудность"
                            },
                            {
                                "target_lang_word": "проверка",
                                "target_lang_sense_clarification": "на прочность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cfb4e3e-3983-3b2b-ae32-1826e373fbd5",
                    "sense_definition": "A match played between international cricket teams, usually lasting up to five days.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Australia won the first <w>Test</w> of the series by a significant margin.",
                            "target_lang_translations": {
                                "ru": "Австралия выиграла первый <w>тестовый матч</w> серии со значительным отрывом."
                            }
                        },
                        {
                            "text": "He has played in over one hundred <w>Test</w> matches for his country.",
                            "target_lang_translations": {
                                "ru": "Он сыграл более чем в ста <w>тестовых</w> матчах за свою страну."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Test match",
                        "Test cricket",
                        "Test series"
                    ],

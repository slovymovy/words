    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f7f072cf-ede5-321f-b68e-1aef3d7bd645",
                    "sense_definition": "Показать документ, билет или вещь официальному лицу для проверки или подтверждения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "show_docs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При входе в офисное здание охрана попросила меня <предъявить> паспорт.",
                            "target_lang_translations": {
                                "en": "At the entrance to the office building, security asked me to <show> my passport."
                            }
                        },
                        {
                            "text": "Пассажир обязан <предъявить> билет контролёру по первому требованию.",
                            "target_lang_translations": {
                                "en": "The passenger is required to <present> their ticket to the inspector upon request."
                            }
                        }
                    ],
                    "synonyms": [
                        "показать",
                        "представить",
                        "продемонстрировать"
                    ],
                    "antonyms": [
                        "скрыть",
                        "спрятать"
                    ],
                    "common_phrases": [
                        "предъявить документы",
                        "предъявить паспорт",
                        "предъявить билет",
                        "предъявить пропуск"
                    ],
                    "target_lang_definitions": {
                        "en": "To show a document, ticket, or item to an official for inspection or verification."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "documents, tickets, or ID"
                            },
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "general usage"
                            },
                            {
                                "target_lang_word": "produce",
                                "target_lang_sense_clarification": "formal request for documents"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70541994-2786-37e5-ade7-fd36c7147ede",
                    "sense_definition": "Заявить о своих правах, требованиях или недовольстве по отношению к кому-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "demands",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Покупатель имеет право <предъявить> претензии к качеству товара в магазине.",
                            "target_lang_translations": {
                                "en": "The buyer has the right to <make> a complaint regarding the quality of the goods in the store."
                            }
                        },
                        {
                            "text": "Работники завода решили <предъявить> требования о повышении зарплаты.",
                            "target_lang_translations": {
                                "en": "The factory workers decided to <put forward> demands for a salary increase."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдвинуть",
                        "заявить",
                        "выставить"
                    ],
                    "common_phrases": [
                        "предъявить требования",
                        "предъявить претензии",
                        "предъявить права"
                    ],
                    "target_lang_definitions": {
                        "en": "To state or put forward rights, demands, or complaints against someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "claims, complaints, or demands"
                            },
                            {
                                "target_lang_word": "put forward",
                                "target_lang_sense_clarification": "demands or requirements"
                            },
                            {
                                "target_lang_word": "assert",
                                "target_lang_sense_clarification": "rights"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0314f515-606b-33e0-acf4-a99b2ef7a25d",
                    "sense_definition": "Официально выдвинуть обвинение в преступлении или подать судебный иск.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Следователь собрал достаточно улик, чтобы <предъявить> обвинение подозреваемому.",
                            "target_lang_translations": {
                                "en": "The investigator gathered enough evidence to <bring> charges against the suspect."
                            }
                        },
                        {
                            "text": "Компания планирует <предъявить> иск конкурентам за нарушение авторских прав.",
                            "target_lang_translations": {
                                "en": "The company plans to <file> a lawsuit against competitors for copyright infringement."
                            }
                        }
                    ],
                    "synonyms": [
                        "выдвинуть",
                        "подать"

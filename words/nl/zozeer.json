{
    "word_family": [
        "hoezeer",
        "evenzeer"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "26cb1d93-79c2-3a74-8081-c050fd087dd8",
                    "sense_definition": "Gebruikt om een tegenstelling of nuance aan te geven, waarbij het ene minder van toepassing is dan het andere; niet in de eerste plaats.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het gaat niet <zozeer> om het geld, maar om het principe.",
                            "target_lang_translations": {
                                "en": "It is not <so much> about the money, but about the principle.",
                                "ru": "Речь идет не <столько> о деньгах, сколько о принципе."
                            }
                        },
                        {
                            "text": "Ze was niet <zozeer> boos als wel teleurgesteld in zijn gedrag.",
                            "target_lang_translations": {
                                "en": "She was not <so much> angry as disappointed in his behavior.",
                                "ru": "Она была не <столько> зла, сколько разочарована его поведением."
                            }
                        }
                    ],
                    "common_phrases": [
                        "niet zozeer ... maar",
                        "niet zozeer ... als wel"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate a contrast or nuance, implying one thing applies less than another; not primarily.",
                        "ru": "Используется для выражения противопоставления или уточнения, когда одно утверждение менее применимо, чем другое; обычно в конструкции «не столько...»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "so much",
                                "target_lang_sense_clarification": "used in comparisons (not so much ... as)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "столько",
                                "target_lang_sense_clarification": "в конструкции «не столько... сколько»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "232ea808-c191-37e9-bc27-7f337537cf49",
                    "sense_definition": "In zo'n hoge mate; dermate.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was <zozeer> overtuigd van zijn gelijk dat hij niet wilde luisteren.",
                            "target_lang_translations": {
                                "en": "He was convinced of his rightness <to such an extent> that he did not want to listen.",
                                "ru": "Он был <настолько> убежден в своей правоте, что не хотел слушать."
                            }
                        },
                        {
                            "text": "De situatie is <zozeer> verslechterd dat ingrijpen noodzakelijk is.",
                            "target_lang_translations": {
                                "en": "The situation has deteriorated <to such an extent> that intervention is necessary.",
                                "ru": "Ситуация <настолько> ухудшилась, что необходимо вмешательство."
                            }
                        }
                    ],
                    "synonyms": [
                        "dermate",
                        "dusdanig"
                    ],
                    "target_lang_definitions": {
                        "en": "To such a high degree; to such an extent.",
                        "ru": "В такой высокой степени; настолько сильно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to such an extent",
                                "target_lang_sense_clarification": "indicating a high degree"
                            },
                            {
                                "target_lang_word": "so much",
                                "target_lang_sense_clarification": "indicating intensity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "настолько",
                                "target_lang_sense_clarification": "указание на степень"
                            },
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "усиление степени"
                            },
                            {
                                "target_lang_word": "до такой степени",
                                "target_lang_sense_clarification": "фразовый синоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "3111dc77-5bad-39d4-8b42-5dd30cbd4920",
                    "sense_definition": "Verbogen vorm van <w>uiterst</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal mijn <w>uiterste</w> best doen om het probleem op te lossen.",

                    "antonyms": [
                        "polyester",
                        "silk",
                        "wool"
                    ],
                    "common_phrases": [
                        "100% cotton",
                        "organic cotton",
                        "printed cotton"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ткань или материя, изготовленная из мягких белых волокон хлопчатника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хло́пок",
                                "target_lang_sense_clarification": "fabric"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e2f70e12-fa27-3ba6-8dbf-50e4465006fa",
                    "sense_definition": "De toestand waarin zaken of personen in maat, aantal, waarde of rang van elkaar verschillen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is een duidelijke <w>ongelijkheid</w> in de verdeling van de taken binnen het team.",
                            "target_lang_translations": {
                                "ru": "В распределении задач внутри команды наблюдается явное <w>неравенство</w>."
                            }
                        },
                        {
                            "text": "De <w>ongelijkheid</w> tussen de prestaties van de twee computers is groot.",
                            "target_lang_translations": {
                                "ru": "<w>Неравенство</w> в производительности двух компьютеров велико."
                            }
                        }
                    ],
                    "antonyms": [
                        "gelijkheid"
                    ],
                    "common_phrases": [
                        "grote ongelijkheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором вещи или люди различаются по размеру, количеству, ценности или рангу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неравенство",
                                "target_lang_sense_clarification": "отсутствие равенства, различие"
                            },
                            {
                                "target_lang_word": "несходство",
                                "target_lang_sense_clarification": "отсутствие сходства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf19595b-b640-3465-b725-d3fd3bd27153",
                    "sense_definition": "Een maatschappelijke situatie waarin bepaalde groepen mensen worden voorgetrokken of achtergesteld ten opzichte van anderen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regering wil de sociale <w>ongelijkheid</w> in het land verminderen met nieuwe wetten.",
                            "target_lang_translations": {
                                "ru": "Правительство хочет сократить социальное <w>неравенство</w> в стране с помощью новых законов."
                            }
                        },
                        {
                            "text": "Armoede is vaak een direct gevolg van structurele economische <w>ongelijkheid</w>.",
                            "target_lang_translations": {
                                "ru": "Бедность часто является прямым следствием структурного экономического <w>неравенства</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "gelijkheid"
                    ],
                    "common_phrases": [
                        "sociale ongelijkheid",
                        "economische ongelijkheid",
                        "groeiende ongelijkheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общественная ситуация, в которой определенные группы людей находятся в привилегированном или неблагоприятном положении по сравнению с другими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неравенство",
                                "target_lang_sense_clarification": "социальное или экономическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "044f7764-58f1-3ecb-9570-283e2c3e07c7",
                    "sense_definition": "Een wiskundige uitdrukking die aangeeft dat twee grootheden niet dezelfde waarde hebben, vaak aangeduid met symbolen zoals < of >.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun jij deze <w>ongelijkheid</w> oplossen en de juiste waarde van x vinden?",
                            "target_lang_translations": {
                                "ru": "Ты можешь решить это <w>неравенство</w> и найти правильное значение x?"
                            }
                        },
                        {
                            "text": "Het symbool voor 'kleiner dan' wordt vaak gebruikt in een <w>ongelijkheid</w>.",
                            "target_lang_translations": {
                                "ru": "Символ «меньше» часто используется в <w>неравенстве</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "gelijkheid"
                    ],
                    "common_phrases": [
                        "lineaire ongelijkheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Математическое выражение, указывающее, что две величины не имеют одинакового значения (часто с символами < или >)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неравенство",
                                "target_lang_sense_clarification": "математическое выражение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
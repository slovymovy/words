                    "sense_id": "3334b078-c129-31cb-a502-7a3116183c85",
                    "sense_definition": "Jedna z linii stanowiących kontur elementu twarzy, decydująca o jej wyglądzie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej delikatne <w>rysy</w> twarzy od razu przykuły moją uwagę.",
                            "target_lang_translations": {
                                "ru": "Её нежные <w>черты</w> лица сразу привлекли моё внимание."
                            }
                        },
                        {
                            "text": "Mężczyzna miał ostre <w>rysy</w>, które nadawały mu groźny wygląd.",
                            "target_lang_translations": {
                                "ru": "У мужчины были острые <w>черты</w>, которые придавали ему грозный вид."
                            }
                        }
                    ],
                    "synonyms": [
                        "kontur"
                    ],
                    "common_phrases": [
                        "rysy twarzy",
                        "ostre rysy",
                        "delikatne rysy",
                        "regularne rysy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из линий, составляющих контур лица и определяющих его внешний вид."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "черта",
                                "target_lang_sense_clarification": "лица"
                            }
                        ]

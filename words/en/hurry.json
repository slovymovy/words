{
    "word_family": [
        "hurried",
        "hurriedly",
        "unhurried"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ab801892-b7cb-37f2-a8d0-3986c84437e8",
                    "sense_definition": "To move, act, or go quickly because there is not much time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>hurry</w> or we will miss the last bus home.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>поторопиться</w>, иначе мы опоздаем на последний автобус домой."
                            }
                        },
                        {
                            "text": "She <w>hurried</w> to answer the phone before it stopped ringing.",
                            "target_lang_translations": {
                                "ru": "Она <w>поспешила</w> ответить на телефон, пока он не перестал звонить."
                            }
                        }
                    ],
                    "synonyms": [
                        "rush",
                        "dash",
                        "race",
                        "speed"
                    ],
                    "antonyms": [
                        "delay",
                        "linger",
                        "crawl"
                    ],
                    "common_phrases": [
                        "hurry up",
                        "hurry back",
                        "hurry home"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться или действовать быстро из-за нехватки времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спешить",
                                "target_lang_sense_clarification": "делать что-то быстро"
                            },
                            {
                                "target_lang_word": "торопиться",
                                "target_lang_sense_clarification": "стараться успеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e33ce65-6b74-3c2d-a4f0-f74f336b83b8",
                    "sense_definition": "To cause someone to do something more quickly or to move someone somewhere quickly.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The guards <w>hurried</w> the prisoners into the waiting truck.",
                            "target_lang_translations": {
                                "ru": "Охранники <w>торопили</w> заключенных в ожидающий грузовик."
                            }
                        },
                        {
                            "text": "Please do not <w>hurry</w> me while I am trying to eat my dinner.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>торопи</w> меня, пока я пытаюсь пообедать."
                            }
                        }
                    ],
                    "synonyms": [
                        "rush",
                        "push",
                        "urge",
                        "hasten"
                    ],
                    "antonyms": [
                        "slow",
                        "delay",
                        "hold back"
                    ],
                    "common_phrases": [
                        "hurry someone along",
                        "hurry things up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-то делать что-то быстрее или быстро перемещать кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торопить",
                                "target_lang_sense_clarification": "побуждать к спешке"
                            },
                            {
                                "target_lang_word": "подгонять",
                                "target_lang_sense_clarification": "заставлять двигаться быстрее"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c19ecf87-9af5-31e0-ba39-5c60d15cb50a",
                    "sense_definition": "A state of moving or acting quickly, often with confusion or excitement.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was in such a big <w>hurry</w> that he forgot his wallet.",
                            "target_lang_translations": {
                                "ru": "Он так сильно <w>торопился</w>, что забыл свой бумажник."
                            }
                        },
                        {
                            "text": "I am in a <w>hurry</w> right now, so I cannot talk to you.",
                            "target_lang_translations": {
                                "ru": "Я сейчас <w>спешу</w>, поэтому не могу с тобой поговорить."
                            }
                        }
                    ],
                    "synonyms": [
                        "rush",
                        "haste"
                    ],
                    "antonyms": [
                        "leisure",
                        "slowness"
                    ],
                    "common_phrases": [
                        "in a hurry",
                        "in a big hurry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние быстрых действий или движений, часто в суматохе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спешка",
                                "target_lang_sense_clarification": "быстрые, торопливые действия"
                            },
                            {
                                "target_lang_word": "поспешность",
                                "target_lang_sense_clarification": "чрезмерная быстрота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d4a50e1-72e6-3ad5-9c8d-1e728d0270e6",
                    "sense_definition": "The need or reason to do something quickly.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Take your time finishing the work because there is no <w>hurry</w>.",
                            "target_lang_translations": {
                                "ru": "Не торопись заканчивать работу, потому что <w>спешить</w> некуда."
                            }
                        },
                        {
                            "text": "I do not understand the <w>hurry</w> since we have plenty of time.",
                            "target_lang_translations": {
                                "ru": "Я не понимаю этой <w>спешки</w>, раз у нас полно времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "urgency",
                        "rush"
                    ],
                    "antonyms": [
                        "delay"
                    ],
                    "common_phrases": [
                        "no hurry",
                        "what's the hurry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Необходимость или причина делать что-то быстро."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спешка",
                                "target_lang_sense_clarification": "необходимость торопиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7602324-7f58-38aa-9472-75a08233e07e",
                    "sense_definition": "A defensive play in American football that forces the quarterback to act faster than intended.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The linebacker recorded three sacks and five <w>hurries</w> during the game.",
                            "target_lang_translations": {
                                "ru": "Лайнбекер записал на свой счет три сэка и пять <w>прессингов</w> за игру."
                            }
                        },
                        {
                            "text": "A quarterback <w>hurry</w> can be just as effective as a sack for the defense.",
                            "target_lang_translations": {
                                "ru": "<w>Давление</w> на квотербека может быть так же эффективно для защиты, как и сэк."
                            }
                        }
                    ],
                    "common_phrases": [
                        "quarterback hurry"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in American football"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитный маневр в американском футболе, заставляющий квотербека действовать быстрее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прессинг",
                                "target_lang_sense_clarification": "давление на игрока"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d2e0956c-d0a4-32f8-8a30-fcac8e622577",
                    "sense_definition": "Cheap, tasteless, useless goods; trinkets.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "cheap_goods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The souvenir shop was filled with plastic <w>tat</w> that nobody really wanted.",
                            "target_lang_translations": {
                                "ru": "Сувенирный магазин был забит пластиковым <w>барахлом</w>, которое никому особо не было нужно."
                            }
                        },
                        {
                            "text": "I am trying to clear out all the old <w>tat</w> from the attic.",
                            "target_lang_translations": {
                                "ru": "Я пытаюсь выгрести весь старый <w>хлам</w> с чердака."
                            }
                        }
                    ],
                    "synonyms": [
                        "Caucasian Persian"
                    ],
                    "common_phrases": [
                        "tourist tat",
                        "load of tat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дешёвые, безвкусные вещи; безделушки, барахло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "барахло",
                                "target_lang_sense_clarification": "разговорное; ненужные вещи низкого качества"
                            },
                            {
                                "target_lang_word": "безделушка",
                                "target_lang_sense_clarification": "мелкая вещь для украшения"
                            },
                            {
                                "target_lang_word": "хлам",
                                "target_lang_sense_clarification": "старые или негодные вещи"
                            },
                            {
                                "target_lang_word": "дешёвка",
                                "target_lang_sense_clarification": "пренебрежительное; вещь низкого качества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50233484-7a3d-3cf4-9c90-a16fdb4c5652",
                    "sense_definition": "A tattoo.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tattoo_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He showed off the new <w>tat</w> on his shoulder to his friends.",
                            "target_lang_translations": {
                                "ru": "Он хвастался перед друзьями новой <w>татухой</w> на плече."
                            }
                        },
                        {
                            "text": "She plans to get a small <w>tat</w> on her wrist next week.",
                            "target_lang_translations": {
                                "ru": "Она планирует сделать небольшую <w>тату</w> на запястье на следующей неделе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Татуировка (разговорное сокращение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тату",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            },
                            {
                                "target_lang_word": "татуха",
                                "target_lang_sense_clarification": "слэнг"
                            },
                            {
                                "target_lang_word": "наколка",
                                "target_lang_sense_clarification": "тюремный или армейский жаргон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "157dc78a-b333-347e-8b53-711a9bece238",
                    "sense_definition": "Some small thing, especially that which is exchanged tit for tat.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The argument became a childish exchange of tit for <w>tat</w>.",
                            "target_lang_translations": {
                                "ru": "Спор превратился в ребяческий обмен любезностями в духе «зуб за <w>зуб</w>»."
                            }
                        },
                        {
                            "text": "They expelled the diplomats in a tit for <w>tat</w> response to the sanctions.",
                            "target_lang_translations": {
                                "ru": "Они выслали дипломатов в ответ на санкции по принципу «око за <w>око</w>»."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tit for tat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть идиомы 'tit for tat', означающей воздаяние равным (зуб за зуб)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зуб",
                                "target_lang_sense_clarification": "в выражении 'зуб за зуб'"
                            },
                            {
                                "target_lang_word": "око",
                                "target_lang_sense_clarification": "в выражении 'око за око'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f4cf7e9a-699f-3176-bfd7-1dfeab811f37",
                    "sense_definition": "To apply a tattoo.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tattoo_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to get <w>tatted</w> up with designs covering both arms.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>забиться</w> рисунками, покрывающими обе руки."
                            }
                        },
                        {
                            "text": "The artist who <w>tats</w> at that shop is famous for her detailed work.",
                            "target_lang_translations": {
                                "ru": "Мастер, которая <w>бьёт тату</w> в этом салоне, славится своими детальными работами."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать татуировку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бить тату",
                                "target_lang_sense_clarification": "наносить татуировку"
                            },
                            {
                                "target_lang_word": "делать татуировку",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "забиваться",
                                "target_lang_sense_clarification": "слэнг; покрывать тело татуировками"
                            }
                        ]

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "83a982c6-1aeb-3e0e-8382-038fde0d9739",
                    "sense_definition": "Psuć lub zakłócać coś, co jest postrzegane jako idealne, czyste, harmonijne lub piękne (np. ciszę, radość, krajobraz).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "corruption_spoiling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głośne rozmowy turystów zaczęły <kazić> powagę tego historycznego miejsca.",
                            "target_lang_translations": {
                                "ru": "Громкие разговоры туристов начали <w>нарушать</w> торжественность этого исторического места."
                            }
                        },
                        {
                            "text": "Żaden zbędny dźwięk nie powinien <kazić> idealnej ciszy panującej w bibliotece.",
                            "target_lang_translations": {
                                "ru": "Ни один лишний звук не должен <w>нарушать</w> идеальную тишину, царящую в библиотеке."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbijać",
                        "przeszkadzać",
                        "likwidować"
                    ],
                    "common_phrases": [
                        "kazić ciszę",
                        "kazić radość",
                        "kazić efekt",
                        "kazić spokój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Портить или нарушать что-то, воспринимаемое как идеальное, чистое, гармоничное или красивое (например, тишину, радость, пейзаж)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портить",
                                "target_lang_sense_clarification": "общий смысл; ухудшать состояние чего-либо"
                            },
                            {
                                "target_lang_word": "нарушать",
                                "target_lang_sense_clarification": "прерывать состояние покоя, тишины или гармонии"
                            },
                            {
                                "target_lang_word": "омрачать",
                                "target_lang_sense_clarification": "делать менее радостным или приятным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d62f5097-a4f5-3694-b428-d2a0ed0ab3b3",
                    "sense_definition": "Czynić coś nieczystym, plamić honor, dobre imię lub świętość; pozbawiać nieskazitelności.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "corruption_spoiling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie pozwolę nikomu <kazić> dobrego imienia mojej rodziny fałszywymi oskarżeniami.",
                            "target_lang_translations": {
                                "ru": "Я никому не позволю <w>порочить</w> доброе имя моей семьи ложными обвинениями."
                            }
                        },
                        {
                            "text": "Starożytni wierzyli, że pewne czyny mogą <kazić> świątynię i wymagać rytualnego oczyszczenia.",
                            "target_lang_translations": {
                                "ru": "Древние верили, что определенные поступки могут <w>осквернять</w> храм и требовать ритуального очищения."
                            }
                        }
                    ],
                    "synonyms": [
                        "kalać",
                        "plamić",
                        "piętnować"
                    ],
                    "common_phrases": [
                        "kazić honor",
                        "kazić dobre imię",
                        "kazić pamięć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то нечистым, пятнать честь, доброе имя или святыню; лишать безупречности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порочить",
                                "target_lang_sense_clarification": "наносить вред репутации или доброму имени"
                            },
                            {
                                "target_lang_word": "пятнать",
                                "target_lang_sense_clarification": "оставлять след позора на чести или совести"
                            },
                            {
                                "target_lang_word": "осквернять",
                                "target_lang_sense_clarification": "подвергать поруганию святыню или что-то высоко чтимое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08f39e3a-adcf-3055-b467-7c512b020d7e",
                    "sense_definition": "Wywierać negatywny wpływ na czyjąś moralność, prowadzić do zepsucia charakteru lub obyczajów.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "corruption_spoiling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złe towarzystwo może szybko <kazić> młode i niedoświadczone umysły.",
                            "target_lang_translations": {
                                "ru": "Плохая компания может быстро <w>развращать</w> молодые и неопытные умы."
                            }
                        },
                        {
                            "text": "Uważano, że nadmierny luksus zaczyna <kazić> proste obyczaje mieszkańców wsi.",
                            "target_lang_translations": {
                                "ru": "Считалось, что чрезмерная роскошь начинает <w>портить</w> простые нравы деревенских жителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "demoralizować",
                        "ganić"
                    ],
                    "common_phrases": [
                        "kazić krew",
                        "kazić obyczaje",
                        "kazić charakter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать негативное влияние на чью-то нравственность, вести к испорченности характера или нравов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развращать",
                                "target_lang_sense_clarification": "морально разлагать, прививать дурные наклонности"
                            },
                            {
                                "target_lang_word": "портить",
                                "target_lang_sense_clarification": "ухудшать характер или поведение человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "934b4c06-cf99-3b8b-974d-51846350ea46",
                    "sense_definition": "Ulegać zepsuciu moralnemu, tracić swoje wartości etyczne lub szlachetność.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "corruption_spoiling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod wpływem władzy i pieniędzy człowiek często zaczyna się <kazić>.",
                            "target_lang_translations": {
                                "ru": "Под влиянием власти и денег человек часто начинает <w>портиться</w>."
                            }
                        },
                        {
                            "text": "Nie chciał <kazić> się nieuczciwymi interesami, więc zrezygnował z tej propozycji.",
                            "target_lang_translations": {
                                "ru": "Он не хотел <w>мараться</w> нечестными сделками, поэтому отказался от этого предложения."
                            }
                        }
                    ],
                    "synonyms": [
                        "demoralizować się",
                        "hańbić się"
                    ],
                    "common_phrases": [
                        "kazić się moralnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддаваться моральному разложению, терять свои этические ценности или благородство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портиться",
                                "target_lang_sense_clarification": "становиться хуже в моральном плане"
                            },
                            {
                                "target_lang_word": "мараться",
                                "target_lang_sense_clarification": "участвовать в чем-то предосудительном или нечестном"
                            }
                        ]
                    }
                }
            ]
        }

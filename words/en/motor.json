                    ],
                    "synonyms": [
                        "moto",
                        "engine"
                    ],
                    "common_phrases": [
                        "nice motor"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Легковой автомобиль или машина (часто в разговорном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тачка",
                                "target_lang_sense_clarification": "разговорное, сленг"
                            },
                            {
                                "target_lang_word": "автомобиль",
                                "target_lang_sense_clarification": "нейтральное"
                            },
                            {
                                "target_lang_word": "мотор",
                                "target_lang_sense_clarification": "устаревшее или сленг (такси)"
                            }
                        ]
                    }

                {
                    "sense_id": "ec94fc7c-5915-3206-adb6-c366006b884f",
                    "sense_definition": "Vrij van vuil, vlekken of besmetting; schoon en helder.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cleanliness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de grote schoonmaak was het hele huis weer fris en <w>rein</w>.",
                            "target_lang_translations": {
                                "ru": "После генеральной уборки весь дом снова стал свежим и <w>чистым</w>."
                            }
                        },
                        {
                            "text": "Het is belangrijk om met een <w>rein</w> geweten te leven.",
                            "target_lang_translations": {
                                "ru": "Важно жить с <w>чистой</w> совестью."
                            }
                        }
                    ],
                    "synonyms": [
                        "schoon",
                        "zuiver",
                        "proper",
                        "net"
                    ],
                    "antonyms": [
                        "vuil",
                        "onrein"
                    ],
                    "common_phrases": [
                        "een rein geweten",
                        "rein van hart"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободный от грязи, пятен или заражения; чистый и ясный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чистый",
                                "target_lang_sense_clarification": "свободный от грязи"
                            },
                            {
                                "target_lang_word": "непорочный",
                                "target_lang_sense_clarification": "нравственно чистый, безгрешный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ac53e31-59e9-3f84-8eb8-c4ef33ee7aac",
                    "sense_definition": "Volledig en uitsluitend; louter (vaak gebruikt ter versterking van een eigenschap).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat hij daar vertelde was <w>je reinste</w> onzin.",
                            "target_lang_translations": {
                                "ru": "То, что он там рассказал, было <w>чистейшей</w> чепухой."
                            }
                        },
                        {
                            "text": "Het hele project bleek uiteindelijk <w>je reinste</w> waanzin te zijn.",
                            "target_lang_translations": {
                                "ru": "Весь проект в итоге оказался <w>чистейшим</w> безумием."
                            }
                        }
                    ],
                    "synonyms": [
                        "puur",
                        "zuiver",
                        "compleet"

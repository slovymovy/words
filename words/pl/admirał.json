    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b4465fd-1e47-323d-93da-cea746bf5757",
                    "sense_definition": "Najwyższy stopień wojskowy w marynarce wojennej lub oficer posiadający ten stopień.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "military_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy <w>admirał</w> przejął dowodzenie nad całą flotą południową.",
                            "target_lang_translations": {
                                "ru": "Новый <w>адмирал</w> принял командование всем южным флотом."
                            }
                        },
                        {
                            "text": "Mój dziadek marzył, by kiedyś otrzymać stopień <w>admirała</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка мечтал когда-нибудь получить звание <w>адмирала</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mianować admirałem",
                        "stopień admirała",
                        "admirał floty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высшее воинское звание в военно-морском флоте или офицер, носящий это звание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адмирал",
                                "target_lang_sense_clarification": "звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70fa1302-8d5f-3596-8d25-eb1c8f7ed4ca",
                    "sense_definition": "Gatunek motyla z rodziny rusałkowatych, charakteryzujący się ciemnymi skrzydłami z czerwonymi pasami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "butterfly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie na krzewie usiadł piękny <w>admirał</w>.",
                            "target_lang_translations": {
                                "ru": "В нашем саду на куст сел красивый <w>адмирал</w>."
                            }
                        },
                        {
                            "text": "Rusałka <w>admirał</w> jest motylem wędrownym, który przylatuje do nas z południa.",
                            "target_lang_translations": {
                                "ru": "Бабочка <w>адмирал</w> — это перелётный вид, который прилетает к нам с юга."
                            }
                        }
                    ],
                    "synonyms": [
                        "rusałka admirał"
                    ],
                    "common_phrases": [
                        "motyl admirał",
                        "rusałka admirał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид бабочек семейства нимфалид, отличающийся темными крыльями с красными полосами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адмирал",
                                "target_lang_sense_clarification": "бабочка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddb71c2b-10c9-3644-8008-d546f1d74d8d",
                    "sense_definition": "Główny okręt we flocie, na którym przebywa dowódca; okręt flagowy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wrogi <w>admirał</w> zatonął po długiej wymianie ognia artyleryjskiego.",
                            "target_lang_translations": {
                                "ru": "Вражеский <w>флагман</w> затонул после долгой артиллерийской перестрелки."
                            }
                        },
                        {
                            "text": "Dowódca obserwował przebieg bitwy, stojąc na mostku swojego <w>admirała</w>.",
                            "target_lang_translations": {
                                "ru": "Командующий наблюдал за ходом битвы, стоя на мостике своего <w>флагмана</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane w kontekście historycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Главный корабль флота, на котором находится командующий; флагманский корабль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "флагман",
                                "target_lang_sense_clarification": "корабль"
                            },
                            {
                                "target_lang_word": "адмиральский корабль",
                                "target_lang_sense_clarification": "корабль"
                            }
                        ]
                    }
                }
            ]
        }
    ]

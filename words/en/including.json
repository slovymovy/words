{
    "word_family": [
        "include",
        "inclusion",
        "inclusive",
        "inclusively",
        "included"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "f51d3e78-210c-3aec-87a2-3720f5bd517b",
                    "sense_definition": "Used to indicate that someone or something is part of a particular group, list, or amount.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_inclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The trip costs fifty dollars, <w>including</w> lunch.",
                            "target_lang_translations": {
                                "ru": "Поездка стоит пятьдесят долларов, <w>включая</w> обед."
                            }
                        },
                        {
                            "text": "Everyone decided to go to the park, <w>including</w> my brother.",
                            "target_lang_translations": {
                                "ru": "Все решили пойти в парк, <w>включая</w> моего брата."
                            }
                        }
                    ],
                    "synonyms": [
                        "inclusive of"
                    ],
                    "antonyms": [
                        "excluding",
                        "except"
                    ],
                    "common_phrases": [
                        "including tax",
                        "including shipping",
                        "including but not limited to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания того, что кто-то или что-то является частью определенной группы, списка или количества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включая",
                                "target_lang_sense_clarification": "предлог; вместе с"
                            },
                            {
                                "target_lang_word": "в том числе",
                                "target_lang_sense_clarification": "среди которых"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0db5d278-1d93-3459-bc64-28255b5f26db",
                    "sense_definition": "Used to introduce a clause that provides a specific detail or example.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_inclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We considered several factors, <w>including</w> that the weather might change.",
                            "target_lang_translations": {
                                "ru": "Мы учли несколько факторов, <w>включая</w> то, что погода может измениться."
                            }
                        },
                        {
                            "text": "There are many reasons to stay, <w>including</w> that we love this city.",
                            "target_lang_translations": {
                                "ru": "Есть много причин остаться, <w>включая</w> то, что мы любим этот город."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения придаточного предложения, которое содержит конкретную деталь или пример."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включая",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "в том числе",
                                "target_lang_sense_clarification": "уточнение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dca30e8d-ea1a-35eb-b651-9a5238250764",
                    "sense_definition": "Present participle and gerund of <w>include</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They are <w>including</w> a free gift with every purchase.",
                            "target_lang_translations": {
                                "ru": "Они <w>включают</w> бесплатный подарок в каждую покупку."

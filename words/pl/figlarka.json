                    "sense_id": "17e9c73c-0d0d-3e98-bed4-458e7eef72c6",
                    "sense_definition": "Kobieta lub dziewczyna, która lubi płatać figle.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "prankster",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta mała <w>figlarka</w> znowu schowała mi klucze dla żartu.",
                            "target_lang_translations": {
                                "ru": "Эта маленькая <w>шалунья</w> снова спрятала мои ключи ради шутки."
                            }
                        },
                        {
                            "text": "Moja siostra to wesoła <w>figlarka</w>, która ciągle kogoś nabiera.",
                            "target_lang_translations": {
                                "ru": "Моя сестра — веселая <w>озорница</w>, которая постоянно кого-то разыгрывает."
                            }
                        }
                    ],
                    "synonyms": [
                        "psotnica",
                        "swawolnica",
                        "żartownisia",
                        "zbytnica"
                    ],
                    "common_phrases": [
                        "mała figlarka",
                        "wesoła figlarka",
                        "niezła figlarka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина или девочка, которая любит шалить и устраивать розыгрыши."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шалунья",

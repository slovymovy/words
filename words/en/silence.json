{
    "word_family": [
        "silent",
        "silencer",
        "silently"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fbd59a30-ef59-3a49-bf2c-36459034b51c",
                    "sense_definition": "The complete absence of any sound or noise.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "absence_of_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>silence</w> in the library helped me focus on my work.",
                            "target_lang_translations": {
                                "ru": "<w>Тишина</w> в библиотеке помогла мне сосредоточиться на работе."
                            }
                        },
                        {
                            "text": "A loud crash suddenly broke the <w>silence</w> of the night.",
                            "target_lang_translations": {
                                "ru": "Громкий грохот внезапно нарушил ночную <w>тишину</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "quietness",
                        "stillness",
                        "soundlessness",
                        "noiselessness"
                    ],
                    "antonyms": [
                        "sound",
                        "loudness"
                    ],
                    "common_phrases": [
                        "break the silence",
                        "dead silence",
                        "total silence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полное отсутствие каких-либо звуков или шума."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тишина",
                                "target_lang_sense_clarification": "отсутствие звуков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a2ec69f-b9b4-3827-ba1b-4b5d082f3bca",
                    "sense_definition": "The act of refraining from speaking or making noise.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "refraining_from_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer told the suspect that he had the right to <w>silence</w>.",
                            "target_lang_translations": {
                                "ru": "Полицейский сообщил подозреваемому, что у него есть право на <w>молчание</w>."
                            }
                        },
                        {
                            "text": "They sat together in <w>silence</w> because there was nothing left to say.",
                            "target_lang_translations": {
                                "ru": "Они сидели вместе в <w>молчании</w>, потому что говорить было больше не о чем."
                            }
                        }
                    ],
                    "synonyms": [
                        "quiet"
                    ],
                    "antonyms": [
                        "speech",
                        "talk"
                    ],
                    "common_phrases": [
                        "right to silence",
                        "in silence",
                        "vow of silence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздержание от разговоров или создания шума."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молчание",
                                "target_lang_sense_clarification": "воздержание от речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed8ec1e9-32ee-387d-8b3c-8788b5cfd486",
                    "sense_definition": "Refraining from speaking for purposes of prayer or meditation, especially in Quaker worship.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "refraining_from_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "During the meeting, the group sat in deep <w>silence</w> to wait for spiritual guidance.",
                            "target_lang_translations": {
                                "ru": "Во время собрания группа сидела в глубоком <w>безмолвии</w>, ожидая духовного наставления."
                            }
                        },
                        {
                            "text": "She found great comfort and peace in the <w>silence</w> of the chapel.",
                            "target_lang_translations": {
                                "ru": "Она находила большое утешение и покой в <w>тишине</w> часовни."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "religious context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздержание от речи с целью молитвы или медитации, особенно в богослужении квакеров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безмолвие",
                                "target_lang_sense_clarification": "духовное или поэтическое"
                            },
                            {
                                "target_lang_word": "молчание",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a8ca1bef-0bba-326b-b09d-7e6768927a71",
                    "sense_definition": "To make someone or something stop making noise.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "make_silent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>silence</w> your mobile phones before the movie starts.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>отключите звук</w> на мобильных телефонах перед началом фильма."
                            }
                        },
                        {
                            "text": "The teacher raised her hand to <w>silence</w> the noisy students.",
                            "target_lang_translations": {
                                "ru": "Учительница подняла руку, чтобы <w>успокоить</w> шумных учеников."
                            }
                        }
                    ],
                    "synonyms": [
                        "mute"
                    ],
                    "common_phrases": [
                        "silence the crowd",
                        "silence the alarm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-то или что-то прекратить шуметь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заставить замолчать",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "заглушить",
                                "target_lang_sense_clarification": "о звуке, механизме или сигнале"
                            },
                            {
                                "target_lang_word": "успокоить",
                                "target_lang_sense_clarification": "о шумной группе людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4b8e26b-b630-383e-8be4-f19c5e36e999",
                    "sense_definition": "To suppress criticism, doubts, or the expression of opposing views.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "suppress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government tried to <w>silence</w> all political opposition by controlling the media.",
                            "target_lang_translations": {
                                "ru": "Правительство пыталось <w>заставить замолчать</w> всю политическую оппозицию, контролируя СМИ."
                            }
                        },
                        {
                            "text": "Winning the championship finally helped to <w>silence</w> his critics.",
                            "target_lang_translations": {
                                "ru": "Победа в чемпионате наконец помогла <w>заткнуть рот</w> его критикам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "silence the critics",
                        "silence the doubters"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подавлять критику, сомнения или выражение противоположных взглядов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заставить замолчать",
                                "target_lang_sense_clarification": "лишить слова"
                            },
                            {
                                "target_lang_word": "заткнуть рот",
                                "target_lang_sense_clarification": "грубое, идиоматическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "675239fb-397f-36c9-a893-402323f248c0",
                    "sense_definition": "To murder someone, usually to prevent them from revealing information.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "kill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The criminal organization decided to <w>silence</w> the witness before the trial.",
                            "target_lang_translations": {
                                "ru": "Преступная организация решила <w>убрать</w> свидетеля до суда."
                            }
                        },
                        {
                            "text": "He was afraid they would <w>silence</w> him if he went to the police.",
                            "target_lang_translations": {
                                "ru": "Он боялся, что они <w>заставят его замолчать</w>, если он пойдет в полицию."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "euphemistic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Убить кого-то, обычно чтобы предотвратить разглашение информации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убрать",
                                "target_lang_sense_clarification": "криминальный сленг"
                            },
                            {
                                "target_lang_word": "ликвидировать",
                                "target_lang_sense_clarification": "формальное/военное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6eddaa1e-dda3-3515-8efa-538a35bf9294",
                    "sense_definition": "To block or prevent the expression of a gene.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists used a new technique to <w>silence</w> the gene responsible for the disease.",
                            "target_lang_translations": {
                                "ru": "Ученые использовали новую методику, чтобы <w>блокировать</w> ген, ответственный за болезнь."
                            }
                        },
                        {
                            "text": "The medication works to <w>silence</w> specific proteins in the virus.",
                            "target_lang_translations": {
                                "ru": "Лекарство работает на то, чтобы <w>подавить</w> специфические белки вируса."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical/biology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Блокировать или предотвращать экспрессию гена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блокировать",
                                "target_lang_sense_clarification": "предотвратить действие"
                            },
                            {
                                "target_lang_word": "подавлять",
                                "target_lang_sense_clarification": "снижать активность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "6c235d9c-32c4-3cc2-8c53-eb7d00f19d55",
                    "sense_definition": "Used to command someone to be quiet.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Silence</w>! I am trying to explain the rules.",
                            "target_lang_translations": {
                                "ru": "<w>Тишина</w>! Я пытаюсь объяснить правила."
                            }
                        },
                        {
                            "text": "<w>Silence</w> in the court!",
                            "target_lang_translations": {
                                "ru": "<w>Тишина</w> в зале суда!"
                            }
                        }
                    ],
                    "synonyms": [
                        "be quiet",
                        "hush",
                        "shush",
                        "shut up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как приказ кому-то замолчать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тишина!",
                                "target_lang_sense_clarification": "требование тишины"
                            },
                            {
                                "target_lang_word": "Молчать!",
                                "target_lang_sense_clarification": "строгий приказ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "caa499a2-6e7b-36cb-954d-f5cdf0d4295c",
                    "sense_definition": "A female given name.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My great-aunt <w>Silence</w> was named after a virtue.",
                            "target_lang_translations": {
                                "ru": "Мою двоюродную бабушку <w>Сайленс</w> назвали в честь добродетели."
                            }
                        },
                        {
                            "text": "We decided to name our daughter <w>Silence</w>.",
                            "target_lang_translations": {
                                "ru": "Мы решили назвать нашу дочь <w>Сайленс</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское личное имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сайленс",
                                "target_lang_sense_clarification": "транслитерация имени"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "591f6ed8-5854-3caf-a997-551caa004ed0",
                    "sense_definition": "Verwijst naar iets dat eerder genoemd is in combinatie met het voorzetsel <w>op</w>; op het, op dat, op ze.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet de pan op het vuur en leg de deksel <w>erop</w>.",
                            "target_lang_translations": {
                                "en": "Put the pan on the stove and put the lid <w>on it</w>.",
                                "ru": "Поставь кастрюлю на огонь и положи крышку <w>сверху</w>."
                            }
                        },
                        {
                            "text": "Ik vertrouw <w>erop</w> dat alles goed komt.",
                            "target_lang_translations": {
                                "en": "I count <w>on it</w> that everything will be fine.",
                                "ru": "Я рассчитываю <w>на то</w>, что всё будет хорошо."
                            }
                        },
                        {
                            "text": "Er staat een grote doos in de gang, ga er niet <w>erop</w> staan.",
                            "target_lang_translations": {
                                "en": "There is a big box in the hallway, don't stand <w>on it</w>.",
                                "ru": "В коридоре стоит большая коробка, не вставай <w>на неё</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "daarop"
                    ],
                    "antonyms": [
                        "eraf",
                        "eronder"
                    ],
                    "common_phrases": [
                        "erop of eronder",
                        "erop los",
                        "erop uit",
                        "erop na houden"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to something mentioned earlier combined with the preposition 'op'; on it, on that, on them.",
                        "ru": "Указывает на ранее упомянутый предмет или ситуацию в сочетании с предлогом «op»; на это, на него, сверху."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on it",
                                "target_lang_sense_clarification": "referring to an object or situation"
                            },
                            {
                                "target_lang_word": "on top",
                                "target_lang_sense_clarification": "physical position"
                            },
                            {
                                "target_lang_word": "on them",
                                "target_lang_sense_clarification": "referring to plural objects"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на это",
                                "target_lang_sense_clarification": "указание на ситуацию или объект"
                            },
                            {
                                "target_lang_word": "сверху",
                                "target_lang_sense_clarification": "физическое положение"
                            },
                            {
                                "target_lang_word": "на него/неё",
                                "target_lang_sense_clarification": "указание на конкретный предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41148097-268c-36ee-a19f-0c6103986e54",
                    "sense_definition": "De daaropvolgende; die erna komt (meestal gebruikt achter een tijdsaanduiding).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We kwamen op maandag aan en de dag <w>erop</w> vertrokken we alweer.",
                            "target_lang_translations": {
                                "en": "We arrived on Monday and the day <w>after</w> we left again.",
                                "ru": "Мы приехали в понедельник, а <w>на следующий</w> день уже уехали."
                            }
                        },
                        {
                            "text": "Ze won de wedstrijd en het jaar <w>erop</w> verdedigde ze haar titel.",
                            "target_lang_translations": {
                                "en": "She won the match and the year <w>after</w> she defended her title.",
                                "ru": "Она выиграла соревнование, и <w>в следующем</w> году защитила свой титул."
                            }
                        }
                    ],
                    "synonyms": [
                        "daarna"
                    ],
                    "antonyms": [
                        "ervoor"
                    ],
                    "common_phrases": [
                        "de dag erop",
                        "de week erop",
                        "het jaar erop"
                    ],
                    "target_lang_definitions": {
                        "en": "The following one; the one coming after (usually used after a time indication).",
                        "ru": "Следующий за чем-либо; тот, который идет после (обычно используется после указания времени)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "after",
                                "target_lang_sense_clarification": "time reference"
                            },
                            {
                                "target_lang_word": "following",
                                "target_lang_sense_clarification": "sequence"
                            },
                            {
                                "target_lang_word": "next",
                                "target_lang_sense_clarification": "sequence"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "следующий",

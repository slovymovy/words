    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "adb9e671-8725-3ce1-8b9e-b22720cd8125",
                    "sense_definition": "Subject to disagreement or debate; not accepted by everyone.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_disagreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The exact cause of the accident remains <w>disputed</w> by experts.",
                            "target_lang_translations": {
                                "ru": "Точная причина аварии остается <w>оспариваемой</w> экспертами."
                            }
                        },
                        {
                            "text": "It is a <w>widely disputed</w> theory that lacks sufficient evidence.",
                            "target_lang_translations": {
                                "ru": "Это <w>широко оспариваемая</w> теория, которой не хватает достаточных доказательств."
                            }
                        }
                    ],
                    "synonyms": [
                        "contested",
                        "controversial",
                        "unsettled",
                        "debated"
                    ],
                    "antonyms": [
                        "undisputed",
                        "certain",
                        "settled",
                        "accepted"
                    ],
                    "common_phrases": [
                        "widely disputed",
                        "hotly disputed",
                        "disputed claim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Являющийся предметом разногласий или дебатов; не принятый всеми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спо́рный",
                                "target_lang_sense_clarification": "вызывающий разногласия"
                            },
                            {
                                "target_lang_word": "оспа́риваемый",
                                "target_lang_sense_clarification": "подвергаемый сомнению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cddce05-b2a7-38be-bfd4-1a59df07a25a",
                    "sense_definition": "Claimed by more than one country or authority.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "geopolitical_dispute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Troops were sent to protect the <w>disputed</w> border region.",
                            "target_lang_translations": {
                                "ru": "Войска были отправлены для защиты <w>спорного</w> пограничного региона."
                            }
                        },
                        {
                            "text": "The two nations have finally agreed to negotiate over the <w>disputed territory</w>.",
                            "target_lang_translations": {
                                "ru": "Две страны наконец согласились провести переговоры о <w>спорной территории</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "contested"
                    ],
                    "antonyms": [
                        "undisputed"
                    ],
                    "common_phrases": [
                        "disputed territory",
                        "disputed border",
                        "disputed region",
                        "disputed land"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория или объект, на который претендуют более одной страны или власти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спо́рный",
                                "target_lang_sense_clarification": "о территории или владении"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d1efe783-8fea-3126-bf0f-b207e5ac2223",
                    "sense_definition": "Simple past and past participle of <w>dispute</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player <w>disputed</w> the referee's decision during the match.",
                            "target_lang_translations": {
                                "ru": "Игрок <w>оспорил</w> решение судьи во время матча."
                            }
                        },
                        {
                            "text": "She <w>disputed</w> the charges on her credit card statement.",
                            "target_lang_translations": {
                                "ru": "Она <w>оспорила</w> списания в выписке по своей кредитной карте."
                            }
                        }
                    ],
                    "synonyms": [
                        "challenged",
                        "contested",
                        "questioned"
                    ],
                    "antonyms": [
                        "accepted",
                        "conceded"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола dispute (спорить, оспаривать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оспо́рил",
                                "target_lang_sense_clarification": "выразил несогласие, опротестовал (совершенный вид)"
                            },
                            {
                                "target_lang_word": "оспа́ривал",
                                "target_lang_sense_clarification": "выражал несогласие (несовершенный вид)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
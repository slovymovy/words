                {
                    "sense_id": "c049fe7a-7163-34c6-a8f8-97a87e3f2ca1",
                    "sense_definition": "To cause intense emotional pain or conflict.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was <w>torn</w> by conflicting emotions regarding the decision.",

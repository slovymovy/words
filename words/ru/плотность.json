    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5bd045c0-0ad8-3b5c-b5e9-b851cc8fb7cd",
                    "sense_definition": "Физическая величина, определяемая как отношение массы тела к занимаемому им объёму.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Как правило, при уменьшении температуры <w>плотность</w> вещества увеличивается.",
                            "target_lang_translations": {
                                "en": "As a rule, when the temperature decreases, the <w>density</w> of a substance increases."
                            }
                        },
                        {
                            "text": "У золота очень высокая <w>плотность</w> по сравнению с водой.",
                            "target_lang_translations": {
                                "en": "Gold has a very high <w>density</w> compared to water."
                            }
                        }
                    ],
                    "synonyms": [
                        "удельная масса"
                    ],
                    "common_phrases": [
                        "высокая плотность",
                        "средняя плотность",
                        "плотность воды",
                        "плотность воздуха"
                    ],
                    "target_lang_definitions": {
                        "en": "A physical quantity defined as the ratio of a body's mass to the volume it occupies."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "density",
                                "target_lang_sense_clarification": "physics"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84d693fc-2e65-3cc8-80f2-48999ba99449",
                    "sense_definition": "Количество объектов или единиц чего-либо, приходящееся на единицу площади или пространства.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом районе самая высокая <w>плотность</w> населения в городе.",
                            "target_lang_translations": {
                                "en": "This district has the highest population <w>density</w> in the city."
                            }
                        },
                        {
                            "text": "Садоводы рекомендуют сажать кусты с <w>плотностью</w> два растения на квадратный метр.",
                            "target_lang_translations": {
                                "en": "Gardeners recommend planting bushes with a <w>density</w> of two plants per square meter."
                            }
                        }
                    ],
                    "common_phrases": [
                        "плотность населения",
                        "плотность застройки",
                        "плотность огня"
                    ],
                    "target_lang_definitions": {
                        "en": "The number of objects or units of something per unit of area or space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "density",
                                "target_lang_sense_clarification": "distribution"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "344e523d-2845-35f4-9154-30b7b53533b3",
                    "sense_definition": "Свойство материала или вещества быть густым, компактным и не содержать пустот.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туман был такой <w>плотности</w>, что водитель с трудом различал дорогу.",
                            "target_lang_translations": {
                                "en": "The fog was of such <w>density</w> that the driver could barely distinguish the road."
                            }
                        },
                        {
                            "text": "Для печати фотографий нужна бумага, имеющая высокую <w>плотность</w>.",
                            "target_lang_translations": {
                                "en": "For printing photos, you need paper that has a high <w>density</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "рыхлость"
                    ],
                    "common_phrases": [
                        "плотность ткани",
                        "плотность бумаги"
                    ],
                    "target_lang_definitions": {
                        "en": "The property of a material or substance being thick, compact, and containing no voids."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "density",
                                "target_lang_sense_clarification": "compactness"
                            },
                            {
                                "target_lang_word": "thickness",
                                "target_lang_sense_clarification": "consistency"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            {
                                "target_lang_word": "совать",
                                "target_lang_sense_clarification": "небрежно класть"
                            },
                            {
                                "target_lang_word": "засовывать",
                                "target_lang_sense_clarification": "помещать внутрь чего-либо"
                            }
                        ]

        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1b83a911-40e6-3e88-88e4-3c55ba2c325a",
                    "sense_definition": "Mieć odwagę lub czelność coś zrobić; pozwalać sobie na ryzykowne zachowanie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dare_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jak <w>śmiesz</w> odzywać się do mnie w ten sposób?",
                            "target_lang_translations": {
                                "ru": "Как ты <w>смеешь</w> разговаривать со мной в таком тоне?"
                            }
                        },
                        {
                            "text": "Nie <w>śmiał</w> nawet spojrzeć jej w oczy po tym, co zrobił.",
                            "target_lang_translations": {
                                "ru": "Он даже не <w>смел</w> взглянуть ей в глаза после того, что сделал."
                            }
                        }
                    ],
                    "synonyms": [

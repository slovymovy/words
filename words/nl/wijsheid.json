                    "sense_id": "ed260a5c-67e1-3528-bc24-13c8ea40359c",
                    "sense_definition": "Het vermogen om kennis en ervaring te gebruiken om verstandige beslissingen te nemen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn jarenlange ervaring heeft hem veel <w>wijsheid</w> gebracht.",
                            "target_lang_translations": {

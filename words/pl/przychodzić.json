{
    "word_family": [
        "przyjść",
        "przychodnia",
        "przychód",
        "przyjście",
        "chodzić"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e97bde70-8d7c-39cb-9c7e-f7afc51c827c",
                    "sense_definition": "Docierać do jakiegoś miejsca, zazwyczaj poruszając się pieszo.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Często <w>przychodzą</w> do nas goście na niedzielny obiad.",
                            "target_lang_translations": {
                                "ru": "К нам часто <w>приходят</w> гости на воскресный обед."
                            }
                        },
                        {
                            "text": "Codziennie rano <w>przychodzę</w> do pracy o tej samej godzinie.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <w>прихожу</w> на работу в одно и то же время."
                            }
                        }
                    ],
                    "synonyms": [
                        "przybywać",
                        "zjawiać się",
                        "pojawiać się",
                        "docierać"
                    ],
                    "antonyms": [
                        "odchodzić"
                    ],
                    "common_phrases": [
                        "przychodzić do domu",
                        "przychodzić z wizytą",
                        "przychodzić na świat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибывать в какое-либо место, обычно передвигаясь пешком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приходить",
                                "target_lang_sense_clarification": "о движении пешком"
                            },
                            {
                                "target_lang_word": "прибывать",
                                "target_lang_sense_clarification": "достигать места назначения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e866d72b-7ab1-3e38-9879-d94b0e773183",
                    "sense_definition": "O czasie lub zjawiskach: zaczynać się, pojawiać się lub następować.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiosna w tym roku <w>przychodzi</w> wyjątkowo wcześnie.",
                            "target_lang_translations": {
                                "ru": "Весна в этом году <w>приходит</w> исключительно рано."
                            }
                        },
                        {
                            "text": "Zawsze po burzy <w>przychodzi</w> słońce i spokój.",
                            "target_lang_translations": {
                                "ru": "После грозы всегда <w>приходит</w> солнце и спокойствие."
                            }
                        }
                    ],
                    "synonyms": [
                        "nadchodzić",
                        "pojawiać się"
                    ],
                    "antonyms": [
                        "odchodzić"
                    ],
                    "common_phrases": [
                        "czas przychodzi",
                        "przychodzić z pomocą"
                    ],
                    "target_lang_definitions": {
                        "ru": "О времени или явлениях: начинаться, появляться или наступать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наступать",
                                "target_lang_sense_clarification": "о времени, событиях"
                            },
                            {
                                "target_lang_word": "приходить",
                                "target_lang_sense_clarification": "о времени года, явлениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2339c42-af3f-3ce4-8eac-07348c940c7e",
                    "sense_definition": "O listach, paczkach lub wiadomościach: być dostarczanym do adresata.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "delivery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Co tydzień <w>przychodzi</w> do mnie list od wnuka z zagranicy.",
                            "target_lang_translations": {
                                "ru": "Каждую неделю мне <w>приходит</w> письмо от внука из-за границы."
                            }
                        },
                        {
                            "text": "Poczta w naszej okolicy <w>przychodzi</w> zazwyczaj około południa.",
                            "target_lang_translations": {
                                "ru": "Почта в нашем районе <w>приходит</w> обычно около полудня."
                            }
                        }
                    ],
                    "synonyms": [
                        "docierać"
                    ],
                    "common_phrases": [
                        "przychodzić pocztą",
                        "wiadomość przychodzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "О письмах, посылках или сообщениях: быть доставленным адресату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приходить",
                                "target_lang_sense_clarification": "о почте, сообщениях"
                            },
                            {
                                "target_lang_word": "доходить",
                                "target_lang_sense_clarification": "до адресата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34d9c25b-939d-3afb-9406-a9cb4dffbc83",
                    "sense_definition": "Pojawiać się w umyśle lub świadomości (o myślach, pomysłach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do głowy <w>przychodzą</w> mi teraz same dobre pomysły.",
                            "target_lang_translations": {
                                "ru": "Мне сейчас в голову <w>приходят</w> одни хорошие идеи."
                            }
                        },
                        {
                            "text": "Często <w>przychodzi</w> mi na myśl wspomnienie z wakacji.",
                            "target_lang_translations": {
                                "ru": "Мне часто <w>приходит</w> на ум воспоминание об отпуске."
                            }
                        }
                    ],
                    "synonyms": [
                        "nasuwać się",
                        "pojawiać się"
                    ],
                    "common_phrases": [
                        "przychodzić do głowy",
                        "przychodzić na myśl"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появляться в уме или сознании (о мыслях, идеях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приходить",
                                "target_lang_sense_clarification": "в голову, на ум"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6450cec9-2304-33c2-a576-bcb5ee738dfa",
                    "sense_definition": "Odzyskiwać określony stan, na przykład zdrowie, przytomność lub równowagę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek bardzo powoli <w>przychodzi</w> do zdrowia po operacji.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка очень медленно <w>поправляется</w> после операции."
                            }
                        },
                        {
                            "text": "Po usłyszeniu złych wieści długo nie mogła <w>przychodzić</w> do siebie.",
                            "target_lang_translations": {
                                "ru": "Услышав плохие новости, она долго не могла <w>прийти</w> в себя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przychodzić do siebie",
                        "przychodzić do zdrowia",
                        "przychodzić do formy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвращаться в определенное состояние, например, здоровья или сознания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приходить",
                                "target_lang_sense_clarification": "в себя, в норму"
                            },
                            {
                                "target_lang_word": "поправляться",
                                "target_lang_sense_clarification": "о здоровье"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c6b9ad5-9883-3b61-81af-e0845ccc5375",
                    "sense_definition": "Być łatwym lub trudnym do wykonania dla kogoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauka języków obcych <w>przychodzi</w> jej z wielką łatwością.",
                            "target_lang_translations": {
                                "ru": "Изучение иностранных языков <w>дается</w> ей с большой легкостью."
                            }
                        },
                        {
                            "text": "Pisanie długich wypracowań <w>przychodzi</w> mi zazwyczaj z trudem.",
                            "target_lang_translations": {
                                "ru": "Написание длинных сочинений <w>дается</w> мне обычно с трудом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przychodzić z łatwością",
                        "przychodzić z trudem",
                        "łatwo przychodzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть легким или трудным для выполнения кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "даваться",
                                "target_lang_sense_clarification": "об учебе, навыках"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
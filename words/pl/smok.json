    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d435d7b5-556e-30cb-bba1-e4bacfcf1190",
                    "sense_definition": "Baśniowe, groźne zwierzę, przedstawiane zwykle jako olbrzymi, latający gad ziejący ogniem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mythical_creature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Według starej legendy w tej jaskini mieszkał kiedyś groźny <w>smok</w>.",
                            "target_lang_translations": {
                                "ru": "Согласно старой легенде, в этой пещере когда-то жил грозный <w>дракон</w>."
                            }
                        },
                        {
                            "text": "Rycerz musiał pokonać <w>smoka</w>, aby uwolnić księżniczkę z wieży.",
                            "target_lang_translations": {
                                "ru": "Рыцарь должен был победить <w>дракона</w>, чтобы освободить принцессу из башни."
                            }
                        }
                    ],
                    "synonyms": [
                        "Dra"
                    ],
                    "common_phrases": [
                        "ziać ogniem",
                        "Smok Wawelski",
                        "walczyć ze smokiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сказочное, грозное животное, обычно изображаемое как огромная летающая рептилия, дышащая огнем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дракон",
                                "target_lang_sense_clarification": "мифологическое существо"
                            },
                            {
                                "target_lang_word": "змей",
                                "target_lang_sense_clarification": "фольклорный персонаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dc31dff-8290-394b-a18e-2a3cd5380192",
                    "sense_definition": "Potoczne określenie smoczka dla niemowlęcia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wyjmij mu z buzi tego <w>smoka</w>, to już przecież duży chłopak.",
                            "target_lang_translations": {
                                "ru": "Вынь у него изо рта эту <w>соску</w>, он ведь уже большой мальчик."
                            }
                        },
                        {
                            "text": "Dziecko płakało, dopóki mama nie podała mu ulubionego <w>smoka</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок плакал, пока мама не дала ему любимую <w>пустышку</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dać smoka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        },
                        {
                            "trait_type": "form",
                            "comment": "short for smoczek"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название соски-пустышки для младенца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соска",
                                "target_lang_sense_clarification": "предмет для сосания"
                            },
                            {
                                "target_lang_word": "пустышка",
                                "target_lang_sense_clarification": "соска без отверстия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f11dc5a-44c0-30da-a10b-11024e178b0e",
                    "sense_definition": "Rodzaj jaszczurki z fałdami skórnymi po bokach ciała, które umożliwiają jej lot ślizgowy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W terrarium w zoo widzieliśmy małego, latającego <w>smoka</w>.",
                            "target_lang_translations": {
                                "ru": "В террариуме в зоопарке мы видели маленького <w>летучего дракона</w>."
                            }
                        },
                        {
                            "text": "Ten gatunek <w>smoka</w> potrafi przeskakiwać między drzewami dzięki specjalnej skórze.",
                            "target_lang_translations": {
                                "ru": "Этот вид <w>дракона</w> способен перепрыгивать с дерева на дерево благодаря особой коже."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид ящерицы с кожными складками по бокам тела, позволяющими ей совершать планирующий полет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "летучий дракон",
                                "target_lang_sense_clarification": "зоология"
                            },
                            {
                                "target_lang_word": "дракон",
                                "target_lang_sense_clarification": "краткое название ящерицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fb0f828-4405-3ee2-989e-8c5df78bd636",
                    "sense_definition": "Funkcjonariusz służby więziennej.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więźniowie unikali kontaktu z tym surowym <w>smokiem</w>.",
                            "target_lang_translations": {
                                "ru": "Заключенные избегали контакта с этим суровым <w>вертухаем</w>."
                            }
                        },
                        {
                            "text": "Na oddział wszedł <w>smok</w> i zarządził natychmiastową ciszę nocną.",
                            "target_lang_translations": {
                                "ru": "В отделение вошел <w>надзиратель</w> и объявил немедленный отбой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "prison slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сотрудник тюремной службы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вертухай",
                                "target_lang_sense_clarification": "тюремный жаргон"
                            },
                            {
                                "target_lang_word": "надзиратель",
                                "target_lang_sense_clarification": "должность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5dd2cba5-aa15-3ebd-8bd6-ffd2aa4f90a4",
                    "sense_definition": "Element pompy służący do zasysania cieczy, często wyposażony w sito chroniące przed zanieczyszczeniami.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechanik wymienił uszkodzony <w>smok</w> w pompie olejowej samochodu.",
                            "target_lang_translations": {
                                "ru": "Механик заменил поврежденный <w>маслоприемник</w> в масляном насосе автомобиля."
                            }
                        },
                        {
                            "text": "Zatkany <w>smok</w> uniemożliwił pobieranie wody ze zbiornika.",
                            "target_lang_translations": {
                                "ru": "Засорившийся <w>храпок</w> сделал невозможным забор воды из резервуара."
                            }
                        }
                    ],
                    "common_phrases": [
                        "smok ssawny"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент насоса, служащий для всасывания жидкости, часто оснащенный ситом для защиты от загрязнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "храпок",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "всасывающая сетка",
                                "target_lang_sense_clarification": "элемент насоса"
                            },
                            {
                                "target_lang_word": "маслоприемник",
                                "target_lang_sense_clarification": "в автомобиле"
                            }
                        ]
                    }
                }
            ]
        },

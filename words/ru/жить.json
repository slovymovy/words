    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "183adda8-08a0-3c11-ac6a-5e68340da549",
                    "sense_definition": "Быть живым, вести физическое существование, функционировать как живой организм.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Человек не может долго <жить> без воды и еды.",
                            "target_lang_translations": {
                                "en": "A human cannot <live> long without water and food."
                            }
                        },
                        {
                            "text": "Мы хотим <жить> долго и счастливо, наслаждаясь каждым днём.",
                            "target_lang_translations": {
                                "en": "We want to <live> happily ever after, enjoying every day."
                            }
                        }
                    ],
                    "synonyms": [
                        "существовать",
                        "быть"
                    ],
                    "antonyms": [
                        "умирать"
                    ],
                    "common_phrases": [
                        "жить долго",
                        "жить счастливо",
                        "хотеть жить"
                    ],
                    "target_lang_definitions": {
                        "en": "To be alive, to maintain physical existence, to function as a living organism."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "live",
                                "target_lang_sense_clarification": "to have life; to be alive"
                            },
                            {
                                "target_lang_word": "exist",
                                "target_lang_sense_clarification": "to have objective reality or being"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41158bd3-d173-3f36-80d4-6c13697dbcf3",
                    "sense_definition": "Использовать дом, квартиру или другое место как постоянное жилище; проживать где-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <живу> в большом красивом доме недалеко от парка.",
                            "target_lang_translations": {
                                "en": "I <live> in a big beautiful house not far from the park."
                            }
                        },
                        {
                            "text": "Где сейчас <живёт> твой лучший друг?",
                            "target_lang_translations": {
                                "en": "Where does your best friend <live> now?"
                            }
                        }
                    ],
                    "synonyms": [
                        "проживать",
                        "обитать",
                        "квартировать"
                    ],
                    "common_phrases": [
                        "жить в городе",
                        "жить дома",
                        "жить за границей",
                        "жить в деревне"
                    ],
                    "target_lang_definitions": {
                        "en": "To use a house, apartment, or other place as a permanent dwelling; to reside somewhere."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "live",
                                "target_lang_sense_clarification": "to have a home in a specific place"
                            },
                            {
                                "target_lang_word": "reside",
                                "target_lang_sense_clarification": "formal; to dwell permanently"
                            },
                            {
                                "target_lang_word": "dwell",
                                "target_lang_sense_clarification": "literary; to live as a resident"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "753bab99-2723-3ca2-a515-4d2bea587176",
                    "sense_definition": "Иметь место, сохраняться или продолжаться (о чувствах, идеях, памяти, традициях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Надежда на лучшее будущее всегда <живёт> в душе человека.",
                            "target_lang_translations": {
                                "en": "Hope for a better future always <lives> in a person's soul."
                            }
                        },
                        {
                            "text": "Память о героях войны вечно <живёт> в наших сердцах.",
                            "target_lang_translations": {
                                "en": "The memory of war heroes <lives> forever in our hearts."
                            }
                        }
                    ],
                    "synonyms": [
                        "существовать",
                        "бытовать"
                    ],
                    "antonyms": [
                        "умирать"
                    ],
                    "common_phrases": [
                        "память живёт",
                        "надежда живёт",
                        "любовь живёт"
                    ],
                    "target_lang_definitions": {
                        "en": "To persist, continue, or remain (regarding feelings, ideas, memory, or traditions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "live",
                                "target_lang_sense_clarification": "to endure or remain in memory/spirit"
                            },
                            {
                                "target_lang_word": "live on",
                                "target_lang_sense_clarification": "to continue to exist over time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91ed792e-1232-3851-8f9e-46db8e66c4aa",
                    "sense_definition": "Добывать средства к существованию, содержать себя за счёт каких-либо доходов.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "subsistence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Многие студенты <живут> на стипендию и помощь родителей.",
                            "target_lang_translations": {
                                "en": "Many students <live> on a scholarship and help from their parents."
                            }
                        },
                        {
                            "text": "Он умеет <жить> своим трудом и ни от кого не зависеть.",
                            "target_lang_translations": {
                                "en": "He knows how to <live> by his own labor and depend on no one."
                            }
                        }
                    ],
                    "synonyms": [
                        "зарабатывать",
                        "кормиться"
                    ],
                    "common_phrases": [
                        "жить на зарплату",
                        "жить своим трудом",
                        "жить за счёт"
                    ],
                    "target_lang_definitions": {
                        "en": "To obtain the means for existence; to support oneself financially."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "live on",
                                "target_lang_sense_clarification": "to support oneself using specific resources"
                            },
                            {
                                "target_lang_word": "subsist",
                                "target_lang_sense_clarification": "to maintain existence, often at a minimum level"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0130a956-79a2-39f2-a6bc-83a60029ce35",
                    "sense_definition": "Быть полностью поглощённым каким-либо делом, интересом или целью; считать что-то главным смыслом.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "passion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот талантливый художник <живёт> только своим искусством.",
                            "target_lang_translations": {
                                "en": "This talented artist <lives> only for his art."
                            }
                        },
                        {
                            "text": "Она <живёт> мечтой о кругосветном путешествии.",
                            "target_lang_translations": {
                                "en": "She <lives> for the dream of a trip around the world."
                            }
                        }
                    ],
                    "synonyms": [
                        "увлекаться",
                        "гореть"
                    ],
                    "common_phrases": [
                        "жить работой",
                        "жить мечтой",
                        "жить идеей"
                    ],
                    "target_lang_definitions": {
                        "en": "To be completely absorbed by a cause, interest, or goal; to consider something the main purpose of life."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "live for",
                                "target_lang_sense_clarification": "to devote one's life to something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ef813b7-7fa7-3773-b239-29e1cac71ec1",
                    "sense_definition": "Состоять в любовной связи, вести совместное хозяйство (обычно без регистрации брака).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Они решили просто <жить> вместе, не устраивая пышной свадьбы.",
                            "target_lang_translations": {
                                "en": "They decided to simply <live> together without organizing a lavish wedding."
                            }
                        },
                        {
                            "text": "Мой брат уже год <живёт> со своей девушкой в центре города.",
                            "target_lang_translations": {
                                "en": "My brother has been <living> with his girlfriend in the city center for a year already."
                            }
                        }
                    ],
                    "synonyms": [
                        "сожительствовать",
                        "спать"
                    ],
                    "common_phrases": [
                        "жить вместе",
                        "жить с кем-то"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies cohabitation or sexual relationship"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be in a romantic relationship and share a household (usually without marriage)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "live together",
                                "target_lang_sense_clarification": "to cohabit with a partner"
                            },
                            {
                                "target_lang_word": "cohabit",
                                "target_lang_sense_clarification": "formal; to live together as a couple"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
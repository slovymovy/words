    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2739bb18-3238-3669-a38b-ec3663e3e194",
                    "sense_definition": "Kobieta pracująca jako <w>portier</w>; pracownica dozorująca wejście do budynku, hotelu lub instytucji.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zostawię klucze do mieszkania u pani <w>portierki</w>.",
                            "target_lang_translations": {
                                "ru": "Я оставлю ключи от квартиры у <w>вахтёрши</w>."
                            }
                        },
                        {
                            "text": "Nowa <w>portierka</w> zawsze wita wszystkich mieszkańców uśmiechem.",
                            "target_lang_translations": {
                                "ru": "Новая <w>вахтёрша</w> всегда встречает всех жильцов улыбкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pani portierka",
                        "uprzejma portierka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "żeńska forma rzeczownika portier"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, работающая швейцаром; сотрудница, контролирующая вход в здание, гостиницу или учреждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вахтёрша",
                                "target_lang_sense_clarification": "женщина-вахтёр"
                            },
                            {
                                "target_lang_word": "дежурная",
                                "target_lang_sense_clarification": "сотрудница на входе"
                            },
                            {
                                "target_lang_word": "портье",
                                "target_lang_sense_clarification": "в отеле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33a8651a-6eec-333b-9415-c07bf9725cff",
                    "sense_definition": "Małe pomieszczenie przy wejściu do budynku przeznaczone dla portiera.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klucze do sali konferencyjnej niech zostawią w <w>portierce</w>.",
                            "target_lang_translations": {
                                "ru": "Пусть оставят ключи от конференц-зала в <w>вахтёрской</w>."
                            }
                        },
                        {
                            "text": "W małej <w>portierce</w> paliło się światło przez całą noc.",
                            "target_lang_translations": {
                                "ru": "В маленькой <w>вахтёрской</w> всю ночь горел свет."
                            }
                        }
                    ],
                    "synonyms": [
                        "portiernia"
                    ],
                    "common_phrases": [
                        "siedzieć w portierce",
                        "klucz w portierce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое помещение у входа в здание, предназначенное для портье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вахтёрская",
                                "target_lang_sense_clarification": "помещение"
                            },
                            {
                                "target_lang_word": "вахта",
                                "target_lang_sense_clarification": "место дежурства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "659bb541-72ea-34af-bb1c-c8899e7114b8",
                    "sense_definition": "Zdrobnienie od słowa <w>portiera</w>; zasłona zawieszana w otworze drzwiowym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aksamitna <w>portierka</w> oddzielała salon od ciemnego korytarza.",
                            "target_lang_translations": {
                                "ru": "Бархатная <w>портьера</w> отделяла гостиную от тёмного коридора."
                            }
                        },
                        {
                            "text": "Odsunęła ciężką <w>portierkę</w>, aby zajrzeć do sąsiedniego pokoju.",
                            "target_lang_translations": {
                                "ru": "Она отодвинула тяжёлую <w>портьеру</w>, чтобы заглянуть в соседнюю комнату."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aksamitna portierka",
                        "ciężka portierka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zdrobnienie od słowa portiera"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова «портьера»; занавесь, висящая в дверном проёме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "портьера",
                                "target_lang_sense_clarification": "дверная занавесь"
                            },
                            {
                                "target_lang_word": "занавеска",
                                "target_lang_sense_clarification": "штора"
                            }
                        ]
                    }
                }
            ]
        }
    ]

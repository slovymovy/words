                {
                    "sense_id": "088bca77-d37a-37b2-ac5c-0bcf4aa3bb65",
                    "sense_definition": "Wewnętrzne odczucie pozwalające rozróżniać dobro i zło oraz oceniać własne postępowanie jako moralne lub niemoralne.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "moral_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po kłótni z bratem dręczyły go silne wyrzuty <w>sumienia</w>.",
                            "target_lang_translations": {
                                "ru": "После ссоры с братом его мучили сильные угрызения <w>совести</w>."
                            }
                        },
                        {
                            "text": "Mam czyste <w>sumienie</w>, ponieważ nikogo nie oszukałem.",
                            "target_lang_translations": {
                                "ru": "У меня чистая <w>совесть</w>, так как я никого не обманул."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyrzuty sumienia",
                        "czyste sumienie",
                        "rachunek sumienia",
                        "głos sumienia",
                        "wolność sumienia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутреннее чувство, позволяющее различать добро и зло, а также оценивать собственные поступки как нравственные или безнравственные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совесть",
                                "target_lang_sense_clarification": "чувство нравственной ответственности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68b5e6d5-4b26-358e-8014-355378375d50",
                    "sense_definition": "Dawne lub przenośne określenie piersi, rozumianych jako siedlisko uczuć lub serce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "body_part_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych tekstach literackich <w>sumienie</w> oznaczało czasem serce lub piersi.",
                            "target_lang_translations": {
                                "ru": "В старых литературных текстах слово <w>совесть</w> иногда означало сердце или грудь."
                            }
                        },
                        {
                            "text": "Bohater uderzył się w <w>sumienie</w> na znak głębokiej skruchy.",
                            "target_lang_translations": {
                                "ru": "Герой ударил себя в <w>грудь</w> в знак глубокого раскаяния."
                            }
                        }
                    ],
                    "synonyms": [

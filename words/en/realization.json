                {
                    "sense_id": "9fd821ed-729a-3354-9040-ba58cceb378b",
                    "sense_definition": "The act of becoming fully aware of something or understanding a fact clearly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "cognitive_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She had a sudden <w>realization</w> that she had left her keys at the office.",
                            "target_lang_translations": {
                                "ru": "К ней пришло внезапное <w>осознание</w> того, что она оставила ключи в офисе."
                            }
                        },
                        {
                            "text": "His <w>realization</w> of the problem came too late to fix it.",
                            "target_lang_translations": {
                                "ru": "Его <w>осознание</w> проблемы пришло слишком поздно, чтобы исправить её."
                            }
                        }
                    ],
                    "synonyms": [
                        "awareness",
                        "recognition",
                        "discovery",
                        "understanding",
                        "epiphany"
                    ],
                    "antonyms": [
                        "ignorance",
                        "misunderstanding"
                    ],
                    "common_phrases": [
                        "come to a realization",
                        "sudden realization",
                        "dawning realization",
                        "startling realization"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс полного осознания чего-либо или ясного понимания факта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осознание",

                {
                    "sense_id": "510ef782-f00f-397e-8e76-6d97ca6a5b66",
                    "sense_definition": "Samen een geheel zijn of deel uitmaken van iets.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "constitute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twaalf provincies <w>vormen</w> samen het land Nederland.",
                            "target_lang_translations": {

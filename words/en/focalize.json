{
    "word_family": [
        "focus",
        "focal",
        "focalization"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f236a84e-0144-3a3f-81e3-d53b7c00cd0c",
                    "sense_definition": "To bring something into focus or to concentrate attention on a specific subject.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The management team decided to <w>focalize</w> their efforts on improving customer service.",
                            "target_lang_translations": {
                                "ru": "Руководство решило <w>сфокусировать</w> свои усилия на улучшении обслуживания клиентов."
                            }
                        },
                        {
                            "text": "This special lens helps to <w>focalize</w> the light rays onto a single point.",
                            "target_lang_translations": {
                                "ru": "Эта специальная линза помогает <w>сфокусировать</w> световые лучи в одной точке."
                            }
                        }
                    ],
                    "synonyms": [
                        "focus",
                        "concentrate",
                        "center"
                    ],
                    "antonyms": [
                        "defocus",
                        "scatter"
                    ],
                    "common_phrases": [
                        "focalize on",
                        "focalize attention"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить в фокус или концентрировать внимание на конкретном предмете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фокусировать",
                                "target_lang_sense_clarification": "оптика, внимание"
                            },
                            {
                                "target_lang_word": "сосредоточивать",
                                "target_lang_sense_clarification": "внимание, усилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4a43f2e-e808-370b-a2f3-75735a7093be",
                    "sense_definition": "To restrict or confine something to a particular location or area.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The infection tends to <w>focalize</w> in the lungs before spreading to other parts of the body.",
                            "target_lang_translations": {
                                "ru": "Инфекция имеет тенденцию <w>локализоваться</w> в легких, прежде чем распространиться на другие части тела."
                            }
                        },
                        {
                            "text": "Police tried to <w>focalize</w> the search within the downtown district.",
                            "target_lang_translations": {
                                "ru": "Полиция пыталась <w>локализовать</w> поиски в центральном районе."
                            }
                        }
                    ],
                    "synonyms": [
                        "localize",
                        "confine"
                    ],
                    "antonyms": [
                        "generalize",
                        "spread"
                    ],
                    "common_phrases": [
                        "focalize in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограничивать или локализовать что-либо определенным местом или областью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "локализовать",
                                "target_lang_sense_clarification": "ограничивать распространение"
                            },
                            {
                                "target_lang_word": "сосредоточивать",
                                "target_lang_sense_clarification": "собирать в одном месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45706070-648f-34eb-bdba-eb3aa90fe10b",
                    "sense_definition": "To present a story or narrative from the specific perspective of a character.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author chooses to <w>focalize</w> the narrative through the eyes of a young child.",
                            "target_lang_translations": {
                                "ru": "Автор решает <w>фокализировать</w> повествование через восприятие маленького ребенка."
                            }
                        },
                        {
                            "text": "Although written in the third person, the novel <w>focalizes</w> on the protagonist's inner thoughts.",
                            "target_lang_translations": {
                                "ru": "Хотя роман написан от третьего лица, он <w>фокусируется</w> на внутренних мыслях главного героя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "focalize through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представлять историю или повествование с точки зрения конкретного персонажа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фокализировать",
                                "target_lang_sense_clarification": "литературоведческий термин"
                            },
                            {
                                "target_lang_word": "фокусировать",
                                "target_lang_sense_clarification": "общая перспектива"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "02262465-6f2e-319e-94c2-2a517e4fe1f0",
                    "sense_definition": "Resembling or characteristic of a tassel.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dog wagged its long, <w>tasselly</w> tail as we approached.",
                            "target_lang_translations": {
                                "ru": "Собака виляла своим длинным хвостом <w>с кисточкой</w>, когда мы подошли."
                            }
                        },
                        {
                            "text": "The corn stalks had <w>tasselly</w> tops swaying in the wind.",
                            "target_lang_translations": {
                                "ru": "У стеблей кукурузы были верхушки <w>с кисточками</w>, качающиеся на ветру."
                            }
                        }
                    ],
                    "synonyms": [
                        "tufted"
                    ],
                    "common_phrases": [
                        "tasselly tail",
                        "tasselly appearance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий кисточку или характерный для неё."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напоминающий кисточку",
                                "target_lang_sense_clarification": "о внешнем виде"
                            },
                            {
                                "target_lang_word": "с кисточкой",
                                "target_lang_sense_clarification": "имеющий кисточку на конце"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ba83793-6c8b-30be-94a9-c9603dc9e81a",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9c261cec-3c19-3fa8-b624-4db59450a126",
                    "sense_definition": "Взрастить, воспитать кого-либо с большой любовью, заботой и нежностью.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мать с огромной нежностью <w>взлелеяла</w> своего единственного сына.",
                            "target_lang_translations": {
                                "en": "The mother <w>nurtured</w> her only son with immense tenderness."
                            }
                        },
                        {
                            "text": "Бабушка <w>взлелеяла</w> внуков, отдавая им всё своё душевное тепло.",
                            "target_lang_translations": {
                                "en": "The grandmother <w>cherished</w> her grandchildren, giving them all the warmth of her soul."
                            }
                        }
                    ],
                    "synonyms": [
                        "взрастить",
                        "выпестовать",
                        "воспитать"
                    ],
                    "common_phrases": [
                        "взлелеять дитя",
                        "взлелеять сына",
                        "взлелеять цветок"
                    ],
                    "target_lang_definitions": {
                        "en": "To raise or bring up someone with great love, care, and tenderness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nurture",
                                "target_lang_sense_clarification": "to care for and encourage the growth of someone"
                            },
                            {
                                "target_lang_word": "cherish",
                                "target_lang_sense_clarification": "to protect and care for with affection"
                            },
                            {
                                "target_lang_word": "raise with loving care",
                                "target_lang_sense_clarification": "to bring up a child with great affection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b6a7b31-254c-3ca3-8bd6-2d81e720bc06",
                    "sense_definition": "Любовно выносить, укрепить и развить в себе какую-либо мысль, чувство или мечту.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он долгие годы <w>взлелеял</w> мечту о кругосветном путешествии.",
                            "target_lang_translations": {
                                "en": "For many years he <w>cherished</w> the dream of a trip around the world."
                            }
                        },
                        {
                            "text": "Художник <w>взлелеял</w> этот замысел в своём сердце, прежде чем приступить к работе.",
                            "target_lang_translations": {
                                "en": "The artist <w>nurtured</w> this idea in his heart before starting work."
                            }
                        }
                    ],
                    "synonyms": [
                        "выносить",
                        "взрастить"
                    ],
                    "common_phrases": [
                        "взлелеять мечту",
                        "взлелеять надежду",
                        "взлелеять мысль"
                    ],
                    "target_lang_definitions": {
                        "en": "To lovingly harbor, strengthen, and develop a thought, feeling, or dream."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cherish",
                                "target_lang_sense_clarification": "a hope, dream, or memory"
                            },
                            {
                                "target_lang_word": "nurture",
                                "target_lang_sense_clarification": "an idea, plan, or ambition"
                            },
                            {
                                "target_lang_word": "foster",
                                "target_lang_sense_clarification": "a feeling or sentiment"
                            }
                        ]
                    }
                }
            ]
        }
    ]

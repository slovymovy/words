    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "986ede99-3723-359b-92d6-b5b270b9bc14",
                    "sense_definition": "Słowo używane, aby zaprzeczyć wcześniejszej wypowiedzi i wprowadzić twierdzenie przeciwstawne.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "logic_connector",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie czuję się gorzej, wręcz <w>przeciwnie</w>, czuję się znakomicie.",
                            "target_lang_translations": {
                                "ru": "Я не чувствую себя хуже, как раз <w>наоборот</w>, я чувствую себя замечательно."
                            }
                        },
                        {
                            "text": "To nie był nudny wykład; <w>przeciwnie</w>, wszyscy słuchali z uwagą.",
                            "target_lang_translations": {
                                "ru": "Это не была скучная лекция; <w>напротив</w>, все слушали с вниманием."
                            }
                        }
                    ],
                    "synonyms": [
                        "na odwrót",
                        "odwrotnie"
                    ],
                    "antonyms": [
                        "tak"
                    ],
                    "common_phrases": [
                        "wręcz przeciwnie",
                        "a przeciwnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое для отрицания предыдущего высказывания и введения противоположного утверждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напротив",
                                "target_lang_sense_clarification": "вводное слово, выражающее противопоставление"
                            },
                            {
                                "target_lang_word": "наоборот",
                                "target_lang_sense_clarification": "в значении «совсем не так»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "937e9bab-ca4c-3c55-a545-e5f13919308d",
                    "sense_definition": "W sposób odwrotny do oczekiwanego, niezgodny z czymś lub skierowany w drugą stronę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dwa magnesy zwrócone do siebie tymi samymi biegunami działają <w>przeciwnie</w>.",
                            "target_lang_translations": {
                                "ru": "Два магнита, обращенные друг к другу одинаковыми полюсами, действуют <w>противоположно</w>."
                            }
                        },
                        {
                            "text": "Postąpił zupełnie <w>przeciwnie</w> do moich wyraźnych instrukcji.",
                            "target_lang_translations": {
                                "ru": "Он поступил совершенно <w>вопреки</w> моим четким инструкциям."
                            }
                        }
                    ],
                    "synonyms": [
                        "odwrotnie",
                        "na odwrót",
                        "wspak"
                    ],
                    "common_phrases": [
                        "działać przeciwnie",
                        "skierowany przeciwnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или состояние, обратное ожидаемому, несогласованное с чем-либо или направленное в другую сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противоположно",
                                "target_lang_sense_clarification": "в противоположном направлении или смысле"
                            },
                            {
                                "target_lang_word": "вопреки",
                                "target_lang_sense_clarification": "наперекор, против чего-либо"
                            },
                            {
                                "target_lang_word": "наперекор",
                                "target_lang_sense_clarification": "назло, против желания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "werkwoord_vorm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>in</w> het geld voor de sportvereniging elke maand.",
                            "target_lang_translations": {
                                "en": "I <w>collect</w> the money for the sports club every month."
                            }
                        },
                        {
                            "text": "<w>In</w> dat geld nu eindelijk eens!",
                            "target_lang_translations": {
                                "en": "<w>Collect</w> that money finally!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "vervoeging"
                        }
                    ],
                    "target_lang_definitions": {

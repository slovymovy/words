    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8c3a8ef-9575-3d5c-8ce6-64337a7037a2",
                    "sense_definition": "A powered heavier-than-air aircraft with fixed wings.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to travel to Europe by <w>airplane</w> to save time.",
                            "target_lang_translations": {
                                "ru": "Мы решили отправиться в Европу на <w>самолёте</w>, чтобы сэкономить время."
                            }
                        },
                        {
                            "text": "The <w>airplane</w> took off smoothly from the runway.",
                            "target_lang_translations": {
                                "ru": "<w>Самолёт</w> плавно взлетел с взлётно-посадочной полосы."
                            }
                        }
                    ],
                    "synonyms": [
                        "plane",
                        "aircraft",
                        "aeroplane"
                    ],
                    "common_phrases": [
                        "by airplane",
                        "airplane ticket",
                        "airplane mode",
                        "paper airplane",
                        "fly an airplane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Летательный аппарат тяжелее воздуха с двигателем и неподвижным крылом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самолёт",
                                "target_lang_sense_clarification": "воздушное судно"
                            },
                            {
                                "target_lang_word": "аэроплан",
                                "target_lang_sense_clarification": "устаревшее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8db5b136-3531-3206-b392-13cd9a6b35d1",
                    "sense_definition": "A game to encourage small children to eat, in which a spoon of food is pretended to be an aircraft flying into the child's mouth.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The father started playing <w>airplane</w> to get the toddler to finish his dinner.",
                            "target_lang_translations": {
                                "ru": "Отец начал играть в <w>самолётик</w>, чтобы малыш доел свой ужин."
                            }
                        },
                        {
                            "text": "She moved the spoon like an <w>airplane</w> and made zooming noises.",
                            "target_lang_translations": {
                                "ru": "Она двигала ложкой, как <w>самолётом</w>, и издавала гудящие звуки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "play airplane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игра, побуждающая детей есть, в которой ложка с едой изображает самолёт, летящий в рот ребёнку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самолётик",
                                "target_lang_sense_clarification": "детская игра"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cbf4517f-eeb3-35b1-9ff5-3aca4e116d7d",
                    "sense_definition": "To travel or fly in an airplane.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "travel_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>airplaned</w> across the country to visit their relatives for the holidays.",
                            "target_lang_translations": {
                                "ru": "Они <w>полетели на самолёте</w> через всю страну, чтобы навестить родственников на праздники."
                            }
                        },
                        {
                            "text": "He has <w>airplaned</w> to many international destinations for his job.",
                            "target_lang_translations": {
                                "ru": "Он <w>летал на самолёте</w> во многие страны по работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "fly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествовать или летать на самолёте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лететь на самолёте",
                                "target_lang_sense_clarification": "путешествовать по воздуху"
                            },
                            {
                                "target_lang_word": "путешествовать самолётом",
                                "target_lang_sense_clarification": "способ передвижения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33f842bc-3a86-3ce3-8c87-bfd3aa334207",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aee96484-8e8c-3524-999f-86260a12add8",
                    "sense_definition": "De wettelijke en officiële verbintenis tussen twee personen om samen te leven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "union_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hun <w>huwelijk</w> duurde meer dan vijftig jaar en was erg gelukkig.",
                            "target_lang_translations": {
                                "ru": "Их <w>брак</w> продлился более пятидесяти лет и был очень счастливым."
                            }
                        },
                        {
                            "text": "In Nederland is het burgerlijk <w>huwelijk</w> voor iedereen toegankelijk.",
                            "target_lang_translations": {
                                "ru": "В Нидерландах гражданский <w>брак</w> доступен для всех."
                            }
                        }
                    ],
                    "synonyms": [
                        "echt"
                    ],
                    "common_phrases": [
                        "in het huwelijk treden",
                        "een burgerlijk huwelijk",
                        "vragen ten huwelijk",
                        "een gelukkig huwelijk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Законный и официальный союз двух людей для совместной жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брак",
                                "target_lang_sense_clarification": "официальный союз"
                            },
                            {
                                "target_lang_word": "супружество",
                                "target_lang_sense_clarification": "состояние в браке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09b21dd0-e34a-3f08-9730-917514273231",
                    "sense_definition": "Een religieuze verbintenis tussen partners, vaak voltrokken in een kerk of gebedshuis.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "union_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de ceremonie op het stadhuis volgde het kerkelijk <w>huwelijk</w>.",
                            "target_lang_translations": {
                                "ru": "За церемонией в ратуше последовало церковное <w>венчание</w>."
                            }
                        },
                        {
                            "text": "Voor veel gelovigen is het <w>huwelijk</w> een heilig sacrament.",
                            "target_lang_translations": {
                                "ru": "Для многих верующих <w>брак</w> является святым таинством."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kerkelijk huwelijk",
                        "het heilig huwelijk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Религиозный союз между партнерами, часто заключаемый в церкви."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "венчание",
                                "target_lang_sense_clarification": "церковный обряд"
                            },
                            {
                                "target_lang_word": "брак",
                                "target_lang_sense_clarification": "религиозный союз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c84f81d0-0ee6-35fd-b584-ee360b4894b8",
                    "sense_definition": "Een nauwe samenwerking of verbond tussen twee partijen, zoals bedrijven of politieke partijen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "union_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>huwelijk</w> tussen de twee grote banken bleek zeer succesvol te zijn.",
                            "target_lang_translations": {
                                "ru": "<w>Слияние</w> двух крупных банков оказалось весьма успешным."
                            }
                        },
                        {
                            "text": "Dit politieke <w>huwelijk</w> tussen links en rechts hield helaas niet lang stand.",
                            "target_lang_translations": {
                                "ru": "Этот политический <w>союз</w> левых и правых, к сожалению, продержался недолго."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een politiek huwelijk",
                        "een zakelijk huwelijk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тесное сотрудничество или объединение двух сторон, например, компаний или партий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слияние",
                                "target_lang_sense_clarification": "объединение компаний"
                            },
                            {
                                "target_lang_word": "союз",
                                "target_lang_sense_clarification": "альянс"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c400c3ae-f3ef-3cc4-9205-a84ece4e0ea4",
                    "sense_definition": "Najjaśniejsza z barw; kolor śniegu, mleka lub kredy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "color_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą krajobraz zdominowała wszechobecna <w>biel</w> śniegu.",
                            "target_lang_translations": {
                                "ru": "Зимой в пейзаже преобладала вездесущая <w>белизна</w> снега."
                            }
                        },
                        {
                            "text": "Ściany w nowym mieszkaniu pomalowaliśmy na czystą <w>biel</w>.",
                            "target_lang_translations": {
                                "ru": "Стены в новой квартире мы покрасили в чистый <w>белый цвет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "białość"
                    ],
                    "antonyms": [
                        "czerń"
                    ],
                    "common_phrases": [
                        "śnieżna biel",
                        "czysta biel",
                        "gołębia biel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый светлый из цветов; цвет снега, молока или мела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "белизна",
                                "target_lang_sense_clarification": "свойство, качество"
                            },
                            {
                                "target_lang_word": "белый цвет",
                                "target_lang_sense_clarification": "цвет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d246e59-42fb-31f9-bd78-4c426ebc8c5c",
                    "sense_definition": "Ubranie lub strój w białym kolorze.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "color_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Panna młoda wystąpiła w olśniewającej <w>bieli</w>.",
                            "target_lang_translations": {
                                "ru": "Невеста появилась в ослепительном <w>белом</w>."
                            }
                        },
                        {
                            "text": "Lekarze i pielęgniarki często chodzą w <w>bieli</w>, co kojarzy się z higieną.",
                            "target_lang_translations": {
                                "ru": "Врачи и медсестры часто ходят в <w>белом</w>, что ассоциируется с гигиеной."
                            }
                        }
                    ],
                    "antonyms": [
                        "czerń"
                    ],
                    "common_phrases": [
                        "ubrany w biel",
                        "dama w bieli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда или наряд белого цвета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "белое",
                                "target_lang_sense_clarification": "одежда"
                            },
                            {
                                "target_lang_word": "белое одеяние",
                                "target_lang_sense_clarification": "наряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "951f56f4-2c17-31bc-afd5-325445aeb00f",
                    "sense_definition": "Substancja barwiąca o białym kolorze, używana w malarstwie i przemyśle.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "color_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artysta zmieszał błękit z <w>bielą</w>, aby uzyskać jaśniejszy odcień nieba.",
                            "target_lang_translations": {
                                "ru": "Художник смешал лазурь с <w>белилами</w>, чтобы получить более светлый оттенок неба."
                            }
                        },
                        {
                            "text": "<w>Biel</w> cynkowa jest popularnym pigmentem stosowanym w farbach olejnych.",
                            "target_lang_translations": {
                                "ru": "Цинковые <w>белила</w> — популярный пигмент, используемый в масляных красках."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biel cynkowa",
                        "biel tytanowa",
                        "biel ołowiowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "w kontekście artystycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Красящее вещество белого цвета, используемое в живописи и промышленности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "белила",
                                "target_lang_sense_clarification": "пигмент"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ae5474ec-dd82-3313-ab61-1be505c427f9",
                    "sense_definition": "Forma czasownika <w>bielić</w> (tryb rozkazujący, 2. osoba liczby pojedynczej).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Biel</w> ściany ostrożnie, żeby nie zachlapać podłogi farbą.",
                            "target_lang_translations": {
                                "ru": "<w>Бели</w> стены осторожно, чтобы не забрызгать пол краской."
                            }
                        },
                        {
                            "text": "Nie <w>biel</w> zębów domowymi sposobami, bo możesz uszkodzić szkliwo.",
                            "target_lang_translations": {
                                "ru": "Не <w>отбеливай</w> зубы домашними средствами, так как можешь повредить эмаль."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола 'bielić' (повелительное наклонение, 2-е лицо единственного числа)."

{
    "word_family": [
        "uitsluiting"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7f482876-b8ac-3a50-8867-c24de3020bc0",
                    "sense_definition": "Iemand of iets de toegang weigeren of niet laten meedoen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Verwanten van de organisatoren zijn van deelname <w>uitgesloten</w>.",
                            "target_lang_translations": {
                                "ru": "Родственники организаторов <w>не допускаются</w> к участию."
                            }
                        },
                        {
                            "text": "De vereniging mag geen nieuwe leden <w>uitsluiten</w> zonder geldige reden.",
                            "target_lang_translations": {
                                "ru": "Ассоциация не может <w>исключать</w> новых членов без уважительной причины."
                            }
                        }
                    ],
                    "synonyms": [
                        "buitensluiten"
                    ],
                    "antonyms": [
                        "toelaten",
                        "insluiten"
                    ],
                    "common_phrases": [
                        "iemand uitsluiten",
                        "van deelname uitsluiten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказать кому-либо или чему-либо в доступе или не позволить участвовать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исключать",
                                "target_lang_sense_clarification": "из группы, списка или состава"
                            },
                            {
                                "target_lang_word": "не допускать",
                                "target_lang_sense_clarification": "к участию или входу"
                            },
                            {
                                "target_lang_word": "отстранять",
                                "target_lang_sense_clarification": "от деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9df1bdc5-1bfe-3fd9-8f7b-00b2dc9d905c",
                    "sense_definition": "Iets als onmogelijk beschouwen of niet meerekenen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We kunnen hem vanwege zijn alibi als verdachte <w>uitsluiten</w>.",
                            "target_lang_translations": {
                                "ru": "Мы можем <w>исключить</w> его из числа подозреваемых из-за его алиби."
                            }
                        },
                        {
                            "text": "De politie wil brandstichting voorlopig nog niet <w>uitsluiten</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция пока не хочет <w>исключать</w> версию поджога."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een mogelijkheid uitsluiten",
                        "niets uitsluiten",
                        "risico uitsluiten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать что-либо невозможным или не принимать в расчёт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исключать",
                                "target_lang_sense_clarification": "возможность, риск или вариант"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
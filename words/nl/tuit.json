    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "643a087b-6f32-3699-abac-918146f9ce49",
                    "sense_definition": "Een buisvormig uitsteeksel aan een kan, ketel of fles waardoor vloeistof naar buiten wordt geschonken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spout_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er komt hete stoom uit de <w>tuit</w> van de fluitketel.",
                            "target_lang_translations": {
                                "en": "Hot steam is coming out of the <w>spout</w> of the whistling kettle."
                            }
                        },
                        {
                            "text": "Houd de theepot recht zodat de <w>tuit</w> niet lekt.",
                            "target_lang_translations": {
                                "en": "Keep the teapot upright so the <w>spout</w> doesn't leak."
                            }
                        }
                    ],
                    "synonyms": [
                        "schenktuit",
                        "bek"
                    ],
                    "common_phrases": [
                        "tuit van de theepot",
                        "schenken uit de tuit"
                    ],
                    "target_lang_definitions": {
                        "en": "A tubular projection on a jug, kettle, or bottle through which liquid is poured out."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spout",
                                "target_lang_sense_clarification": "of a container"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e3ff5b3-f056-3281-8b24-eb3c02922877",
                    "sense_definition": "Een smal, spits toelopend uiteinde of vorm, vaak gebruikt om de vorm van de mond te beschrijven.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "spout_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kabouter droeg een muts met een lange, slappe <w>tuit</w>.",
                            "target_lang_translations": {
                                "en": "The gnome wore a hat with a long, limp <w>point</w>."
                            }
                        },
                        {
                            "text": "Zijn mond stond in een <w>tuit</w> van verbazing.",
                            "target_lang_translations": {
                                "en": "His mouth was in a <w>pout</w> of surprise."
                            }
                        }
                    ],
                    "synonyms": [
                        "punt"
                    ],
                    "common_phrases": [
                        "een tuit trekken"
                    ],
                    "target_lang_definitions": {
                        "en": "A narrow, pointed end or shape, often used to describe the shape of the mouth."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pout",
                                "target_lang_sense_clarification": "facial expression"
                            },
                            {
                                "target_lang_word": "point",
                                "target_lang_sense_clarification": "shape of an object"
                            },
                            {
                                "target_lang_word": "tip",
                                "target_lang_sense_clarification": "end of an object"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9a490703-fa92-3ac9-b785-8340f3042ed5",
                    "sense_definition": "Vervoeging van <w>tuiten</w>: eerste, tweede en derde persoon enkelvoud tegenwoordige tijd, en gebiedende wijs.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn linkeroor <w>tuit</w> door het harde geluid van de muziek.",
                            "target_lang_translations": {
                                "en": "My left ear <w>rings</w> from the loud sound of the music."
                            }
                        },
                        {
                            "text": "Zij <w>tuit</w> haar lippen om het kind een kusje te geven.",
                            "target_lang_translations": {
                                "en": "She <w>puckers</w> her lips to give the child a kiss."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugation of 'tuiten' (to ring or to pucker): first, second, and third person singular present tense."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rings",
                                "target_lang_sense_clarification": "ears"
                            },
                            {
                                "target_lang_word": "puckers",
                                "target_lang_sense_clarification": "lips"
                            },
                            {
                                "target_lang_word": "purses",
                                "target_lang_sense_clarification": "lips"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18572985-a8c2-3cce-a86e-709d10bff0d6",
                    "sense_definition": "Vervoeging van <w>tuien</w>: tweede en derde persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tuinman <w>tuit</w> de jonge boom vast aan een paal.",
                            "target_lang_translations": {
                                "en": "The gardener <w>tethers</w> the young tree to a post."
                            }
                        },
                        {
                            "text": "Hij <w>tuit</w> het schip zodat het niet wegdrijft.",
                            "target_lang_translations": {
                                "en": "He <w>secures</w> the ship so it doesn't drift away."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugation of 'tuien' (to tether or guy): second and third person singular present tense."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tethers",
                                "target_lang_sense_clarification": "fasten with a rope"
                            },
                            {
                                "target_lang_word": "secures",
                                "target_lang_sense_clarification": "fasten firmly"
                            },
                            {
                                "target_lang_word": "guys",
                                "target_lang_sense_clarification": "secure with a rope"
                            }
                        ]
                    }
                }
            ]
        }
    ]

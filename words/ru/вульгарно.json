    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0ca0997b-c0a9-3dd0-a9d8-b7cf204e947f",
                    "sense_definition": "Грубо, безвкусно, пошло; лишённо изящества и эстетического вкуса.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она накрасилась слишком ярко и выглядела <w>вульгарно</w>.",
                            "target_lang_translations": {
                                "en": "She put on too much makeup and looked <w>vulgar</w>."
                            }
                        },
                        {
                            "text": "Это сочетание цветов смотрится дёшево и <w>вульгарно</w>.",
                            "target_lang_translations": {
                                "en": "This color combination looks cheap and <w>tacky</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "пошло"
                    ],
                    "antonyms": [
                        "утончённо"
                    ],
                    "common_phrases": [
                        "выглядеть вульгарно",
                        "одеваться вульгарно",
                        "звучать вульгарно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a coarse, tasteless, or tacky manner; lacking grace and aesthetic taste."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tastelessly",
                                "target_lang_sense_clarification": "lacking aesthetic judgment"
                            },
                            {
                                "target_lang_word": "garishly",
                                "target_lang_sense_clarification": "obtrusively bright or showy"
                            },
                            {
                                "target_lang_word": "vulgarly",
                                "target_lang_sense_clarification": "lacking refinement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "888d6da9-e6d6-377e-8d96-77db9ab21733",
                    "sense_definition": "Развязно, неприлично; нарушая нормы общественной морали и приличия.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мужчина за соседним столиком вёл себя <w>вульгарно</w> и громко кричал.",
                            "target_lang_translations": {
                                "en": "The man at the next table was behaving <w>vulgarly</w> and shouting loudly."
                            }
                        },
                        {
                            "text": "Не стоит так <w>вульгарно</w> шутить в приличном обществе.",
                            "target_lang_translations": {
                                "en": "One shouldn't joke so <w>crudely</w> in polite society."
                            }
                        }
                    ],
                    "synonyms": [
                        "развязно"
                    ],
                    "antonyms": [
                        "скромно"
                    ],
                    "common_phrases": [
                        "вести себя вульгарно",
                        "шутить вульгарно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a cheeky or indecent manner; violating norms of public morality and decency."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vulgarly",
                                "target_lang_sense_clarification": "indecent or obscene behavior"
                            },
                            {
                                "target_lang_word": "crudely",
                                "target_lang_sense_clarification": "rude or offensive"
                            },
                            {
                                "target_lang_word": "coarsely",
                                "target_lang_sense_clarification": "rough or unrefined manners"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56def2e6-fb35-33b0-ba27-79828219ee98",
                    "sense_definition": "Крайне упрощённо, примитивно; искажая истинный смысл сложного явления.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автор статьи трактует эту философскую идею слишком <w>вульгарно</w>.",
                            "target_lang_translations": {
                                "en": "The author of the article interprets this philosophical idea too <w>simplistically</w>."
                            }
                        },
                        {
                            "text": "Нельзя понимать законы экономики так прямолинейно и <w>вульгарно</w>.",
                            "target_lang_translations": {
                                "en": "One cannot understand economic laws so straightforwardly and <w>primitively</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "понимать вульгарно",
                        "толковать вульгарно"
                    ],
                    "target_lang_definitions": {
                        "en": "Extremely simplistically or primitively; distorting the true meaning of a complex phenomenon."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "simplistically",
                                "target_lang_sense_clarification": "ignoring complexities"
                            },
                            {
                                "target_lang_word": "primitively",
                                "target_lang_sense_clarification": "unsophisticated understanding"
                            }
                        ]
                    }
                }
            ]
        },

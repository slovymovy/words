    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "df5f0d0c-c4cf-32a5-8a66-08204d138922",
                    "sense_definition": "Uczestnik zbrojnego wystąpienia ludności przeciwko władzy państwowej lub okupantowi.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <w>powstaniec</w> warszawski jest w Polsce traktowany z wielkim szacunkiem.",
                            "target_lang_translations": {
                                "ru": "К каждому варшавскому <w>повстанцу</w> в Польше относятся с большим уважением."
                            }
                        },
                        {
                            "text": "Mój pradziadek był <w>powstańcem</w> i walczył o niepodległość naszego kraju.",
                            "target_lang_translations": {
                                "ru": "Мой прадед был <w>повстанцем</w> и сражался за независимость нашей страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "insurgent"
                    ],
                    "common_phrases": [
                        "powstaniec warszawski",
                        "powstaniec styczniowy",
                        "powstaniec śląski",
                        "mały powstaniec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участник вооруженного восстания населения против государственной власти или оккупантов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повстанец",
                                "target_lang_sense_clarification": "участник восстания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6317350-e963-3885-91cf-0193ff18a137",
                    "sense_definition": "W gwarze więziennej: chłopiec zajmujący się żebractwem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "prison_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten mały <w>powstaniec</w> ciągle prosi przechodniów o drobne na jedzenie.",
                            "target_lang_translations": {
                                "ru": "Этот маленький <w>попрошайка</w> постоянно просит у прохожих мелочь на еду."
                            }
                        },
                        {
                            "text": "W celi nazywali go <w>powstańcem</w>, bo wcześniej żebrał na ulicy.",
                            "target_lang_translations": {
                                "ru": "В камере его называли <w>попрошайкой</w>, потому что раньше он попрошайничал на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "pętak"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gwara więzienna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В тюремном жаргоне: мальчик, занимающийся попрошайничеством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попрошайка",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d44784a-6781-321b-925a-0e6e87064db7",
                    "sense_definition": "W gwarze więziennej: osoba początkująca w złodziejskim fachu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "prison_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako <w>powstaniec</w> musiał się jeszcze wiele nauczyć od starszych kolegów.",
                            "target_lang_translations": {
                                "ru": "Как <w>новичок</w>, он должен был ещё многому научиться у старших товарищей."
                            }
                        },
                        {
                            "text": "Nie traktowali go poważnie, bo był to dopiero <w>powstaniec</w> w ich fachu.",
                            "target_lang_translations": {
                                "ru": "Они не воспринимали его всерьёз, потому что он был всего лишь <w>новичком</w> в их деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "pętak"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gwara więzienna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В тюремном жаргоне: новичок в воровском деле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "новичок",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]

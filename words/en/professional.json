                    ],
                    "antonyms": [
                        "amateur"
                    ],
                    "common_phrases": [
                        "turn professional",
                        "golf professional",
                        "tennis professional"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который занимается спортом или другой деятельностью в качестве оплачиваемой работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "профессионал",
                                "target_lang_sense_clarification": "спортсмен или работник по найму"

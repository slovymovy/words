{
    "word_family": [
        "selection",
        "selective",
        "selector",
        "selectivity"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17d9a57f-e67c-342e-829a-eb5ed48b7919",
                    "sense_definition": "To choose one or more elements from a group or set of options.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "choice_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>select</w> the correct answer from the choices below.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>выберите</w> правильный ответ из предложенных ниже вариантов."
                            }
                        },
                        {
                            "text": "He looked at the menu and <w>selected</w> the roast beef for dinner.",
                            "target_lang_translations": {
                                "ru": "Он посмотрел в меню и <w>выбрал</w> ростбиф на ужин."
                            }
                        }
                    ],
                    "synonyms": [
                        "choose",
                        "opt"
                    ],
                    "antonyms": [
                        "deselect"
                    ],
                    "common_phrases": [
                        "select an option",
                        "select a file",
                        "select from the list",
                        "randomly select"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбрать один или несколько элементов из группы или набора вариантов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбирать",
                                "target_lang_sense_clarification": "общий термин для принятия решения"
                            },
                            {
                                "target_lang_word": "отбирать",
                                "target_lang_sense_clarification": "производить отбор, выделять из общего числа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ac5d615-e86b-320f-a9a7-273f173f925e",
                    "sense_definition": "To retrieve specific data from a database using a query.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "database_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system will <w>select</w> all active users from the database.",
                            "target_lang_translations": {
                                "ru": "Система <w>выберет</w> всех активных пользователей из базы данных."
                            }
                        },
                        {
                            "text": "You need to write a query to <w>select</w> the relevant records.",
                            "target_lang_translations": {
                                "ru": "Вам нужно написать запрос, чтобы <w>выбрать</w> соответствующие записи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "select data",
                        "select query",
                        "select all"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Извлекать определенные данные из базы данных с помощью запроса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбирать",
                                "target_lang_sense_clarification": "получать данные (в контексте SQL)"
                            },
                            {
                                "target_lang_word": "делать выборку",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "896b5ce8-538c-3874-9d46-31d10340b42a",
                    "sense_definition": "Carefully chosen as the best or most suitable; exclusive and privileged.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "exclusive_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Only a <w>select</w> few were invited to the private ceremony.",
                            "target_lang_translations": {
                                "ru": "Лишь <w>избранные</w> немногие были приглашены на закрытую церемонию."
                            }
                        },
                        {
                            "text": "The club is very exclusive and admits only a <w>select</w> group of members.",
                            "target_lang_translations": {
                                "ru": "Клуб очень эксклюзивный и принимает только <w>избранную</w> группу членов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "select few",
                        "select group",
                        "select committee"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательно отобранный как лучший или наиболее подходящий; исключительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избранный",
                                "target_lang_sense_clarification": "о людях или круге лиц"
                            },
                            {
                                "target_lang_word": "элитный",
                                "target_lang_sense_clarification": "привилегированный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78d8516d-2dfc-3a37-b525-ce1045b8c8c2",
                    "sense_definition": "Of the highest quality or grade; top-notch.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "exclusive_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chef uses only <w>select</w> ingredients to prepare this special dish.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар использует только <w>отборные</w> ингредиенты для приготовления этого особого блюда."
                            }
                        },
                        {
                            "text": "This butcher shop is known for selling <w>select</w> cuts of meat.",
                            "target_lang_translations": {
                                "ru": "Эта мясная лавка известна продажей <w>отборных</w> кусков мяса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "select wines",
                        "select cuts",
                        "select produce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высочайшего качества или сорта; первоклассный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отборный",
                                "target_lang_sense_clarification": "о продуктах или материалах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e54305f3-1aad-3647-b124-74c929f522d2",
                    "sense_definition": "A specific button on a game controller used to choose options or open menus.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "gaming_button",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Press <w>select</w> to open the map and check your location.",
                            "target_lang_translations": {
                                "ru": "Нажмите <w>Select</w>, чтобы открыть карту и проверить свое местоположение."
                            }
                        },
                        {
                            "text": "You need to hit the <w>select</w> button to change your weapon.",
                            "target_lang_translations": {
                                "ru": "Вам нужно нажать кнопку <w>Select</w>, чтобы сменить оружие."
                            }
                        }
                    ],
                    "common_phrases": [
                        "press select",
                        "hit select",
                        "select button"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "gaming"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кнопка на игровом контроллере для выбора опций или меню."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Select",
                                "target_lang_sense_clarification": "название кнопки (часто не переводится)"
                            },
                            {
                                "target_lang_word": "кнопка выбора",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "da5f8a9f-a1c0-30f9-82f0-7d403c43bf03",
                    "sense_definition": "Potoczne, często pieszczotliwe lub dawne określenie ojca.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "family",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>papa</w> zakazał mi się z nim spotykać.",
                            "target_lang_translations": {
                                "ru": "Мой <w>папа</w> запретил мне с ним встречаться."
                            }
                        },
                        {
                            "text": "Kiedy wrócisz dzisiaj z pracy, <w>papo</w>?",
                            "target_lang_translations": {
                                "ru": "Когда ты вернешься сегодня с работы, <w>папа</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "tata",
                        "ojciec"
                    ],
                    "common_phrases": [
                        "kochany papa",
                        "mój papa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie rzadziej używane niż 'tata'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, часто ласковое или устаревшее название отца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "папа",
                                "target_lang_sense_clarification": "отец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43c0f4d8-275b-3494-8e34-c5bb3589a296",
                    "sense_definition": "Materiał budowlany nasączony smołą lub asfaltem, używany do izolacji wilgociowej i pokrywania dachów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dach tego starego domu jest pokryty <w>papą</w>.",
                            "target_lang_translations": {
                                "ru": "Крыша этого старого дома покрыта <w>рубероидом</w>."
                            }
                        },
                        {
                            "text": "Musimy kupić rolkę <w>papy</w>, żeby uszczelnić garaż.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить рулон <w>рубероида</w>, чтобы загерметизировать гараж."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kryć papą",
                        "papa termozgrzewalna",
                        "papa dachowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строительный материал, пропитанный смолой или асфальтом, используемый для гидроизоляции и покрытия крыш."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рубероид",
                                "target_lang_sense_clarification": "кровельный материал"
                            },
                            {
                                "target_lang_word": "толь",
                                "target_lang_sense_clarification": "кровельный картон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "882d5824-ad45-3dfe-b635-98c98893f197",
                    "sense_definition": "Pospolite, często lekceważące lub obraźliwe określenie twarzy albo ust.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "body_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zamknij <w>papę</w> i przestań wreszcie narzekać.",
                            "target_lang_translations": {
                                "ru": "Закрой <w>пасть</w> и перестань наконец жаловаться."
                            }
                        },
                        {
                            "text": "Na samą myśl o wyjeździe <w>papa</w> mi się śmieje.",
                            "target_lang_translations": {
                                "ru": "При одной мысли о поездке <w>рожа</w> расплывается в улыбке."
                            }
                        }
                    ],
                    "synonyms": [
                        "gęba",
                        "morda",
                        "japa",
                        "jadaczka"
                    ],
                    "common_phrases": [
                        "drzeć papę",
                        "zamknąć papę",
                        "cieszyć papę"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Просторечное, часто пренебрежительное или оскорбительное название лица или рта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "рот (грубо)"
                            },
                            {
                                "target_lang_word": "морда",
                                "target_lang_sense_clarification": "лицо (грубо)"
                            },
                            {
                                "target_lang_word": "харя",
                                "target_lang_sense_clarification": "лицо (слэнг)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31be439e-cf1d-337e-8679-3698e6556e5b",
                    "sense_definition": "Przestarzałe określenie papieża, głowy Kościoła katolickiego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "W dawnych kronikach zapisano, że <w>papa</w> przyjął poselstwo.",
                            "target_lang_translations": {
                                "ru": "В старинных хрониках записано, что <w>папа</w> принял посольство."
                            }
                        },
                        {
                            "text": "Ludzie zgromadzeni na placu wiwatowali na cześć <w>papy</w>.",
                            "target_lang_translations": {
                                "ru": "Люди, собравшиеся на площади, приветствовали <w>папу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "papież"
                    ],
                    "common_phrases": [
                        "papa rzymski"
                    ],
                    "traits": [

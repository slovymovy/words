{
    "word_family": [
        "elevate",
        "elevator",
        "elevated"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bc438d9c-b23c-38af-8c58-0ddc84ca6ab0",
                    "sense_definition": "The height of a place measured from sea level.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "height_measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city is located at an <w>elevation</w> of 1,500 meters.",
                            "target_lang_translations": {
                                "ru": "Город расположен на <w>высоте</w> 1500 метров."
                            }
                        },
                        {
                            "text": "Snow is much more common at higher <w>elevations</w>.",
                            "target_lang_translations": {
                                "ru": "Снег встречается гораздо чаще на больших <w>высотах</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "altitude",
                        "height"
                    ],
                    "common_phrases": [
                        "high elevation",
                        "low elevation",
                        "above sea level"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высота местности, измеренная относительно уровня моря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "расстояние от уровня моря"
                            },
                            {
                                "target_lang_word": "высота над уровнем моря",
                                "target_lang_sense_clarification": "полный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3081f3dc-416f-38a8-924e-964751d5eaea",
                    "sense_definition": "The act of moving someone to a higher or more important rank or position.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "rank_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>elevation</w> to the board of directors was a surprise to many.",
                            "target_lang_translations": {
                                "ru": "Его <w>повышение</w> до члена совета директоров стало для многих сюрпризом."
                            }
                        },
                        {
                            "text": "We celebrated her <w>elevation</w> to the status of partner.",
                            "target_lang_translations": {
                                "ru": "Мы отпраздновали её <w>повышение</w> до статуса партнёра."
                            }
                        }
                    ],
                    "synonyms": [
                        "promotion",
                        "advancement"
                    ],
                    "antonyms": [
                        "demotion"
                    ],
                    "common_phrases": [
                        "elevation to status",
                        "elevation to peerage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по переводу кого-либо на более высокую или важную должность или позицию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повышение",
                                "target_lang_sense_clarification": "в должности или звании"
                            },
                            {
                                "target_lang_word": "возвышение",
                                "target_lang_sense_clarification": "приобретение высокого статуса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abad612b-b196-3150-9e12-cf122e578fe1",
                    "sense_definition": "A piece of ground that is higher than the area around it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The castle stands on a slight <w>elevation</w> overlooking the valley.",
                            "target_lang_translations": {
                                "ru": "Замок стоит на небольшой <w>возвышенности</w>, с которой открывается вид на долину."
                            }
                        },
                        {
                            "text": "We found a grassy <w>elevation</w> where we could have a picnic.",
                            "target_lang_translations": {
                                "ru": "Мы нашли травянистую <w>возвышенность</w>, где можно было устроить пикник."
                            }
                        }
                    ],
                    "synonyms": [
                        "hill",
                        "rise"
                    ],
                    "antonyms": [
                        "depression"
                    ],
                    "common_phrases": [
                        "slight elevation",
                        "natural elevation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок земли, который выше окружающей местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвышенность",
                                "target_lang_sense_clarification": "холм или высокий участок"
                            },
                            {
                                "target_lang_word": "подъём",
                                "target_lang_sense_clarification": "участок рельефа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a27fb24e-a8ff-37e8-ae63-fcaf3ebe1d75",
                    "sense_definition": "A drawing that shows one side of a building as if seen from the front or side.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The architect showed us the front <w>elevation</w> of the new house.",
                            "target_lang_translations": {
                                "ru": "Архитектор показал нам передний <w>фасад</w> нового дома."
                            }
                        },
                        {
                            "text": "You need to look at the side <w>elevation</w> to understand the roof structure.",
                            "target_lang_translations": {
                                "ru": "Вам нужно посмотреть на боковой <w>фасад</w>, чтобы понять конструкцию крыши."
                            }
                        }
                    ],
                    "common_phrases": [
                        "front elevation",
                        "side elevation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чертёж, изображающий внешнюю сторону здания спереди или сбоку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фасад",
                                "target_lang_sense_clarification": "архитектурный чертёж"
                            },
                            {
                                "target_lang_word": "проекция",
                                "target_lang_sense_clarification": "вид сбоку или спереди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45c812b1-5ecd-3c70-adc6-31eaa520048f",
                    "sense_definition": "A feeling of great happiness, pride, or high moral character.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt a sense of <w>elevation</w> after hearing the inspiring speech.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала душевный <w>подъём</w>, услышав эту вдохновляющую речь."
                            }
                        },
                        {
                            "text": "The poem is known for its <w>elevation</w> of thought and style.",
                            "target_lang_translations": {
                                "ru": "Это стихотворение известно <w>возвышенностью</w> мысли и стиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "exaltation"
                    ],
                    "antonyms": [
                        "depression"
                    ],
                    "common_phrases": [
                        "elevation of mind",
                        "elevation of spirit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство большой радости, гордости или высокого морального духа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подъём",
                                "target_lang_sense_clarification": "душевный или эмоциональный"
                            },
                            {
                                "target_lang_word": "возвышенность",
                                "target_lang_sense_clarification": "высокий стиль или мысли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6e7447a-75e0-33a0-acd5-31396f4c31ee",
                    "sense_definition": "The angle of an object, such as a gun or a star, above the horizon.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "angle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sniper adjusted the <w>elevation</w> of the rifle before firing.",
                            "target_lang_translations": {
                                "ru": "Снайпер скорректировал <w>угол возвышения</w> винтовки перед выстрелом."
                            }
                        },
                        {
                            "text": "We measured the sun's <w>elevation</w> to calculate our position.",
                            "target_lang_translations": {
                                "ru": "Мы измерили <w>высоту</w> солнца, чтобы вычислить наше местоположение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "angle of elevation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Угол нахождения объекта, например оружия или звезды, над горизонтом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угол возвышения",
                                "target_lang_sense_clarification": "военный термин"
                            },
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "астрономический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
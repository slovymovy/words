    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f6f05a1b-0031-3b7c-9d04-ce0dd331a32d",
                    "sense_definition": "Czynność polegająca na fizycznym zetknięciu się z kimś lub czymś; kontakt fizyczny.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poczuła jego delikatny <w>dotyk</w> na swojej twarzy.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала его нежное <w>прикосновение</w> на своем лице."
                            }
                        },
                        {
                            "text": "Ekran tego telefonu reaguje na najlżejszy <w>dotyk</w>.",
                            "target_lang_translations": {
                                "ru": "Экран этого телефона реагирует на малейшее <w>прикосновение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kontakt",
                        "styczność",
                        "muśnięcie"
                    ],
                    "common_phrases": [
                        "delikatny dotyk",
                        "reagować na dotyk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в физическом контакте с кем-либо или чем-либо; физический контакт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикосновение",
                                "target_lang_sense_clarification": "физический контакт"
                            },
                            {
                                "target_lang_word": "касание",
                                "target_lang_sense_clarification": "легкий, поверхностный контакт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1e1006a-d503-367a-9c45-2eed6df88d48",
                    "sense_definition": "Jeden z podstawowych zmysłów umożliwiający odczuwanie bodźców mechanicznych, termicznych i bólowych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sense_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wzrok, słuch, węch, smak i <w>dotyk</w> to pięć zmysłów.",
                            "target_lang_translations": {
                                "ru": "Зрение, слух, обоняние, вкус и <w>осязание</w> — это пять чувств."
                            }
                        },
                        {
                            "text": "Ten materiał jest bardzo przyjemny w <w>dotyku</w>.",
                            "target_lang_translations": {
                                "ru": "Этот материал очень приятен на <w>ощупь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "czucie"
                    ],
                    "common_phrases": [
                        "zmysł dotyku",
                        "przyjemny w dotyku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из основных чувств, позволяющее воспринимать механические, термические и болевые раздражители."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осязание",
                                "target_lang_sense_clarification": "одно из пяти чувств"
                            },
                            {
                                "target_lang_word": "ощупь",
                                "target_lang_sense_clarification": "восприятие поверхности (на ощупь)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

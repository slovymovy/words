{
    "word_family": [],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "799d8479-abab-3556-9d93-afe8408d96be",
                    "sense_definition": "Ukrywać się i czekać w napięciu w jakimś miejscu, aby kogoś zaatakować lub złapać.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drapieżnik cierpliwie <czyhał> w wysokiej trawie na swoją ofiarę.",
                            "target_lang_translations": {
                                "ru": "Хищник терпеливо <подстерегал> свою жертву в высокой траве."
                            }
                        },
                        {
                            "text": "Złodziej <czyhał> w ciemnej bramie, obserwując przechodniów.",
                            "target_lang_translations": {
                                "ru": "Вор <караулил> в темной подворотне, наблюдая за прохожими."
                            }
                        }
                    ],
                    "synonyms": [
                        "czaić się",
                        "dybać"
                    ],
                    "common_phrases": [
                        "czyhać w ukryciu",
                        "czyhać na ofiarę",
                        "czyhać na życie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прятаться и напряженно ждать в каком-либо месте, чтобы напасть на кого-то или схватить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подстерегать",
                                "target_lang_sense_clarification": "ждать в засаде"
                            },
                            {
                                "target_lang_word": "караулить",
                                "target_lang_sense_clarification": "сторожить, выжидать"
                            },
                            {
                                "target_lang_word": "стеречь",
                                "target_lang_sense_clarification": "выслеживать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00e80310-e1a9-3800-9c90-e10251ee11ba",
                    "sense_definition": "Stanowić ukryte niebezpieczeństwo lub zagrożenie, które może się ujawnić w każdej chwili.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wysokich górach na nieostrożnych turystów <czyha> wiele niebezpieczeństw.",
                            "target_lang_translations": {
                                "ru": "В высоких горах неосторожных туристов <подстерегает> множество опасностей."
                            }
                        },
                        {
                            "text": "Nigdy nie wiesz, co <czyha> w ciemnościach gęstego lasu.",
                            "target_lang_translations": {
                                "ru": "Никогда не знаешь, что <таится> в темноте густого леса."
                            }
                        }
                    ],
                    "synonyms": [
                        "zagrażać",
                        "grozić"
                    ],
                    "common_phrases": [
                        "niebezpieczeństwo czyha",
                        "śmierć czyha",
                        "zło czyha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представлять собой скрытую опасность или угрозу, которая может проявиться в любой момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подстерегать",
                                "target_lang_sense_clarification": "об опасности"
                            },
                            {
                                "target_lang_word": "таиться",
                                "target_lang_sense_clarification": "быть скрытым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d05f2198-466b-3193-a039-f76f54a15c63",
                    "sense_definition": "Czekać z niecierpliwością na sprzyjającą okazję, aby coś zrobić lub zdobyć, zazwyczaj z negatywnym zamiarem.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przeciwnicy polityczni tylko <czyhają> na najmniejszy błąd ministra.",
                            "target_lang_translations": {
                                "ru": "Политические противники только и <подстерегают> малейшую ошибку министра."
                            }
                        },
                        {
                            "text": "Wielu fałszywych przyjaciół <czyhało> jedynie na jego majątek.",
                            "target_lang_translations": {
                                "ru": "Многие фальшивые друзья <охотились> лишь за его состоянием."
                            }
                        }
                    ],
                    "synonyms": [
                        "dybać"
                    ],
                    "common_phrases": [
                        "czyhać na okazję",
                        "czyhać na błąd",
                        "czyhać na potknięcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "С нетерпением ждать удобного случая для совершения действия, часто с недобрыми намерениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выжидать",
                                "target_lang_sense_clarification": "ждать удобного момента"
                            },
                            {
                                "target_lang_word": "подстерегать",
                                "target_lang_sense_clarification": "ждать ошибки или случая"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
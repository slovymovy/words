    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "174f06b5-4d6e-3818-8d23-ad87df8a17b6",
                    "sense_definition": "Produkt spożywczy w postaci gęstej masy, otrzymywany z mleka krowiego, koziego bądź owczego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W biegu zrobił dwie kanapki z <w>serem</w>.",
                            "target_lang_translations": {
                                "ru": "На бегу он сделал два бутерброда с <w>сыром</w>."
                            }
                        },
                        {
                            "text": "Na śniadanie często jem biały <w>ser</w> ze szczypiorkiem.",
                            "target_lang_translations": {
                                "ru": "На завтрак я часто ем <w>творог</w> с зеленым луком."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) kejza"
                    ],
                    "common_phrases": [
                        "żółty ser",
                        "biały ser",
                        "ser pleśniowy",
                        "ser topiony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пищевой продукт в виде густой массы, получаемый из коровьего, козьего или овечьего молока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сыр",
                                "target_lang_sense_clarification": "желтый, твердый"
                            },
                            {
                                "target_lang_word": "творог",
                                "target_lang_sense_clarification": "белый, мягкий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4375a686-f924-38fc-abaa-5e60daa774ae",
                    "sense_definition": "Określony gatunek lub rodzaj sera.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Francuskie <w>sery</w> są znane ze swojego wyjątkowego smaku.",
                            "target_lang_translations": {
                                "ru": "Французские <w>сыры</w> известны своим исключительным вкусом."
                            }
                        },
                        {
                            "text": "W tym sklepie można kupić różne regionalne <w>sery</w>.",
                            "target_lang_translations": {
                                "ru": "В этом магазине можно купить различные региональные <w>сыры</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "deska serów",
                        "wybór serów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный сорт или вид сыра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сыр",
                                "target_lang_sense_clarification": "сорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17d1baf9-b957-3501-bc28-c4fa03c76224",
                    "sense_definition": "Cały krąg lub blok sera uformowany podczas produkcji.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz przyniósł z piwnicy wielki, dojrzewający <w>ser</w>.",
                            "target_lang_translations": {
                                "ru": "Хозяин принес из подвала большой созревающий <w>сыр</w>."
                            }
                        },
                        {
                            "text": "Ten <w>ser</w> jest tak ciężki, że trudno go podnieść jedną ręką.",
                            "target_lang_translations": {
                                "ru": "Этот <w>сыр</w> такой тяжелый, что его трудно поднять одной рукой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "krąg sera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Целый круг или блок сыра, сформированный в процессе производства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сыр",
                                "target_lang_sense_clarification": "головка или блок"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
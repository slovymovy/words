{
    "word_family": [
        "narzeczony",
        "narzeczeństwo",
        "zaręczyny",
        "zaręczyć"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0b52f6a2-e84a-3303-a7dd-135a954a83de",
                    "sense_definition": "Kobieta, która przyjęła oświadczyny i oficjalnie przygotowuje się do zawarcia związku małżeńskiego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fiancée",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tego wieczora przyjdę do ciebie na kolację z moją <w>narzeczoną</w>.",
                            "target_lang_translations": {
                                "ru": "Сегодня вечером я приду к тебе на ужин с моей <w>невестой</w>."
                            }
                        },
                        {
                            "text": "Marek i jego <w>narzeczona</w> planują wziąć ślub w przyszłym roku w czerwcu.",
                            "target_lang_translations": {
                                "ru": "Марек и его <w>невеста</w> планируют пожениться в июне следующего года."
                            }
                        }
                    ],
                    "synonyms": [
                        "oblubienica",
                        "nupturientka",
                        "gw. (Górny Śląsk) libsta, lipsta"
                    ],
                    "common_phrases": [
                        "zostać narzeczoną",
                        "była narzeczona",
                        "szczęśliwa narzeczona"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, принявшая предложение руки и сердца и официально готовящаяся к бракосочетанию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невеста",
                                "target_lang_sense_clarification": "обручённая женщина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "241cd72c-3ce2-3772-8063-2f3e35977acc",
                    "sense_definition": "Kobieta będąca z kimś w stałym związku uczuciowym, nazywana tak potocznie nawet bez formalnych zaręczyn.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "partner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na imprezie urodzinowej był Rafał ze swoją nową <w>narzeczoną</w>.",
                            "target_lang_translations": {
                                "ru": "На вечеринке по случаю дня рождения был Рафал со своей новой <w>девушкой</w>."
                            }
                        },
                        {
                            "text": "To jest moja <w>narzeczona</w>, mieszkamy razem od trzech lat.",
                            "target_lang_translations": {
                                "ru": "Это моя <w>невеста</w>, мы живём вместе уже три года."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziewczyna"
                    ],
                    "common_phrases": [
                        "nowa narzeczona",
                        "przedstawić narzeczoną"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w znaczeniu partnerki życiowej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, находящаяся с кем-либо в постоянных отношениях; так называют партнёршу даже без официальной помолвки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девушка",
                                "target_lang_sense_clarification": "партнёрша в отношениях"
                            },
                            {
                                "target_lang_word": "гражданская жена",
                                "target_lang_sense_clarification": "сожительница"
                            },
                            {
                                "target_lang_word": "невеста",
                                "target_lang_sense_clarification": "разг. в значении партнёрши"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2fa46c3c-a040-316f-9e58-7bf621255d48",
                    "sense_definition": "Abbreviation of Saint; used as a title before the name of a canonized religious figure.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "titles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The famous cathedral is dedicated to <w>St</w> Paul.",
                            "target_lang_translations": {
                                "ru": "Знаменитый собор посвящен <w>св.</w> Павлу."
                            }
                        },
                        {
                            "text": "Many schools are named after <w>St</w> Mary.",
                            "target_lang_translations": {
                                "ru": "Многие школы названы в честь <w>св.</w> Марии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "St Paul",
                        "St John",
                        "St George"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"Saint\" (святой); используется как титул перед именем канонизированного религиозного деятеля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "св.",
                                "target_lang_sense_clarification": "сокращение от \"святой\""
                            },
                            {
                                "target_lang_word": "святой",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e08cd07b-176e-3283-9e07-fa349a76c5dc",
                    "sense_definition": "Abbreviation of street; commonly used in written addresses.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "addresses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please send the letter to 10 Downing <w>St</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отправьте письмо на Даунинг-<w>стрит</w>, 10."
                            }
                        },
                        {
                            "text": "Turn left when you reach Main <w>St</w>.",
                            "target_lang_translations": {
                                "ru": "Поверните налево, когда дойдете до Мэйн-<w>стрит</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "High St",
                        "Main St",
                        "Wall St"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"street\" (улица); обычно используется в написании адресов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ул.",
                                "target_lang_sense_clarification": "сокращение от \"улица\""
                            },
                            {
                                "target_lang_word": "улица",
                                "target_lang_sense_clarification": "полная форма"
                            },
                            {
                                "target_lang_word": "-стрит",
                                "target_lang_sense_clarification": "часть названия улицы в адресах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c79fcb54-eb6e-3636-a54c-626d12a55407",
                    "sense_definition": "Abbreviation of stone; a British unit of weight equal to 14 pounds.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has lost almost a <w>st</w> in weight since January.",
                            "target_lang_translations": {
                                "ru": "С января он сбросил почти один <w>стоун</w> веса."
                            }
                        },
                        {
                            "text": "The boxer weighed in at exactly 12 <w>st</w>.",
                            "target_lang_translations": {
                                "ru": "Боксер весил ровно 12 <w>стоунов</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ten st",
                        "weighs twelve st"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"stone\" (стоун); британская единица веса, равная 14 фунтам (около 6,35 кг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоун",
                                "target_lang_sense_clarification": "единица измерения веса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "151ee094-89ea-3a12-979d-3cd5b2f4272d",
                    "sense_definition": "Abbreviation of stitch; used in knitting and crocheting patterns.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "crafts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cast on twenty <w>sts</w> to begin the scarf.",
                            "target_lang_translations": {
                                "ru": "Наберите двадцать <w>петель</w>, чтобы начать шарф."
                            }
                        },
                        {
                            "text": "Knit one <w>st</w> and then purl the next one.",
                            "target_lang_translations": {
                                "ru": "Провяжите одну <w>петлю</w> лицевой, а следующую — изнаночной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "knit one st",
                        "remaining sts"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"stitch\" (петля); используется в схемах вязания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "п.",
                                "target_lang_sense_clarification": "сокращение от \"петля\""
                            },
                            {
                                "target_lang_word": "петля",
                                "target_lang_sense_clarification": "элемент вязания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2743e58-8c2a-3401-be8d-dc165e57eb56",
                    "sense_definition": "Initialism of source text; the original text that is to be translated into another language.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The translator must ensure the target text reflects the meaning of the <w>ST</w>.",
                            "target_lang_translations": {
                                "ru": "Переводчик должен убедиться, что целевой текст отражает смысл <w>ИТ</w>."
                            }
                        },
                        {
                            "text": "Students compared the <w>ST</w> with its various translations.",
                            "target_lang_translations": {
                                "ru": "Студенты сравнивали <w>ИТ</w> с его различными переводами."
                            }
                        }
                    ],
                    "antonyms": [
                        "TT"
                    ],
                    "common_phrases": [
                        "ST and TT",
                        "analysis of the ST"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от \"source text\" (исходный текст); оригинал текста, который подлежит переводу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ИТ",
                                "target_lang_sense_clarification": "исходный текст"
                            },
                            {
                                "target_lang_word": "исходный текст",
                                "target_lang_sense_clarification": "полная форма"
                            },
                            {
                                "target_lang_word": "оригинал",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                }
            ]
        },

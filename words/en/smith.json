    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "2b91f46b-8eff-3ab9-9542-cd4edf2b471c",
                    "sense_definition": "An extremely common English family name, originally derived from the occupation of a metalworker.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Please introduce me to Mr. <w>Smith</w> when he arrives.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, представьте меня мистеру <w>Смиту</w>, когда он приедет."
                            }
                        },
                        {
                            "text": "John <w>Smith</w> is one of the most common names in the English language.",
                            "target_lang_translations": {
                                "ru": "Джон <w>Смит</w> — одно из самых распространенных имен в английском языке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "occupational origin"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно распространенная английская фамилия, первоначально происходящая от названия профессии металлурга (кузнеца)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Смит",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5b70697-e4e4-33c1-833f-b1492893ccee",
                    "sense_definition": "A prestigious women's liberal arts college located in Massachusetts, United States.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "She decided to attend <w>Smith</w> because of its excellent history program.",
                            "target_lang_translations": {
                                "ru": "Она решила поступить в <w>Смит</w> из-за его отличной исторической программы."
                            }
                        },
                        {
                            "text": "My aunt is a proud graduate of <w>Smith</w> and visits the campus often.",
                            "target_lang_translations": {
                                "ru": "Моя тетя — гордая выпускница <w>Смита</w> и часто посещает кампус."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Престижный женский гуманитарный колледж, расположенный в штате Массачусетс, США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Смит",
                                "target_lang_sense_clarification": "название колледжа"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "77358483-3de3-3d39-afa8-1439a2e26756",
                    "sense_definition": "A craftsperson who works with metal, shaping it into desired forms using a hammer and other tools, often by heating it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The village <w>smith</w> repaired the broken iron gate.",
                            "target_lang_translations": {
                                "ru": "Деревенский <w>кузнец</w> починил сломанные железные ворота."
                            }
                        },
                        {
                            "text": "In fantasy stories, a <w>smith</w> often forges magical swords for heroes.",
                            "target_lang_translations": {
                                "ru": "В фэнтези-историях <w>кузнец</w> часто кует магические мечи для героев."
                            }
                        }
                    ],
                    "synonyms": [
                        "metalsmith"
                    ],
                    "common_phrases": [
                        "village smith",
                        "master smith"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремесленник, работающий с металлом, придающий ему нужную форму с помощью молота и других инструментов, часто путем нагревания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кузнец",
                                "target_lang_sense_clarification": "профессия"
                            },
                            {
                                "target_lang_word": "коваль",
                                "target_lang_sense_clarification": "устаревшее/диалектное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "499f8ac2-f0cb-3c44-bfce-8de0fbfad078",
                    "sense_definition": "To forge or shape metal, usually by heating it and pounding it on an anvil.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artisan <w>smithed</w> the silver into a delicate ring.",
                            "target_lang_translations": {
                                "ru": "Ремесленник <w>выковал</w> из серебра изящное кольцо."
                            }
                        },
                        {
                            "text": "He spent years learning how to <w>smith</w> iron into useful tools.",
                            "target_lang_translations": {
                                "ru": "Он потратил годы, учась <w>ковать</w> из железа полезные инструменты."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ковать или придавать форму металлу, обычно нагревая его и ударяя по нему на наковальне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ковать",
                                "target_lang_sense_clarification": "обрабатывать металл"
                            },
                            {
                                "target_lang_word": "выковывать",
                                "target_lang_sense_clarification": "создавать ковкой"
                            }
                        ]
                    }
                }
            ]
        }
    ]

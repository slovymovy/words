    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1af7c71c-4a55-330a-ab4d-f8581c787add",
                    "sense_definition": "Принимать меры к выздоровлению, проходить курс терапии, использовать лекарства или процедуры для избавления от болезни.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_by_patient",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если у тебя высокая температура, нужно остаться дома и <w>лечиться</w>.",
                            "target_lang_translations": {
                                "en": "If you have a high fever, you need to stay home and <w>treat yourself</w>."
                            }
                        },
                        {
                            "text": "Мой дедушка предпочитает <w>лечиться</w> народными средствами, а не таблетками.",
                            "target_lang_translations": {
                                "en": "My grandfather prefers to <w>treat himself</w> with folk remedies rather than pills."
                            }
                        }
                    ],
                    "synonyms": [
                        "проходить лечение",
                        "исцеляться",
                        "оздоравливаться"
                    ],
                    "antonyms": [
                        "болеть",
                        "запускать болезнь"
                    ],
                    "common_phrases": [
                        "лечиться дома",
                        "лечиться у врача",
                        "лечиться от гриппа",
                        "долго лечиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To take measures for recovery, undergo a course of therapy, or use medication to get rid of an illness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be treated",
                                "target_lang_sense_clarification": "general passive or reflexive meaning"
                            },
                            {
                                "target_lang_word": "treat oneself",
                                "target_lang_sense_clarification": "to apply remedies independently"
                            },
                            {
                                "target_lang_word": "receive treatment",
                                "target_lang_sense_clarification": "formal, usually from a doctor"
                            },
                            {
                                "target_lang_word": "undergo treatment",
                                "target_lang_sense_clarification": "to go through a medical process"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b9b45b4-d388-3d4e-bd6c-f5c120ed9a91",
                    "sense_definition": "Поддаваться терапии, устраняться с помощью медицинского вмешательства (о болезни или травме).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "property_of_disease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В современном мире эта инфекция легко <w>лечится</w> антибиотиками.",
                            "target_lang_translations": {
                                "en": "In the modern world, this infection is easily <w>treated</w> with antibiotics."
                            }
                        },
                        {
                            "text": "К сожалению, не все генетические заболевания <w>лечатся</w> полностью.",
                            "target_lang_translations": {
                                "en": "Unfortunately, not all genetic diseases are fully <w>cured</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "излечиваться",
                        "устраняться",
                        "проходить"
                    ],
                    "common_phrases": [
                        "лечиться с трудом",
                        "успешно лечиться",
                        "не лечиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To respond to therapy, to be eliminated through medical intervention (referring to a disease or injury)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be treated",
                                "target_lang_sense_clarification": "passive, regarding a disease"
                            },
                            {
                                "target_lang_word": "be curable",
                                "target_lang_sense_clarification": "capability of being healed"
                            },
                            {
                                "target_lang_word": "respond to treatment",
                                "target_lang_sense_clarification": "medical reaction of a disease"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
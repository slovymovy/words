    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "665ceffa-9bc5-3764-a29a-37eed8897e18",
                    "sense_definition": "A person who makes one or more purchases.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every person who steps through the door is a potential <w>buyer</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый человек, входящий в дверь, — потенциальный <w>покупатель</w>."
                            }
                        },
                        {
                            "text": "We finally found a <w>buyer</w> for our old car.",
                            "target_lang_translations": {
                                "ru": "Мы наконец нашли <w>покупателя</w> для нашей старой машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "purchaser"
                    ],
                    "antonyms": [
                        "seller"
                    ],
                    "common_phrases": [
                        "potential buyer",
                        "first-time buyer",
                        "prospective buyer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который совершает одну или несколько покупок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покупатель",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "клиент",
                                "target_lang_sense_clarification": "лицо, пользующееся услугами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6142fdda-f2a0-3d3e-8264-c5fe7052fccf",
                    "sense_definition": "A person who purchases items for resale in a retail establishment.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "professional_buyer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The supermarket's new <w>buyer</w> decided to stock a larger range of vegetarian foods.",
                            "target_lang_translations": {
                                "ru": "Новый <w>закупщик</w> супермаркета решил расширить ассортимент вегетарианских продуктов."
                            }
                        },
                        {
                            "text": "She works as a fashion <w>buyer</w> for a large department store.",
                            "target_lang_translations": {
                                "ru": "Она работает модным <w>байером</w> в крупном универмаге."
                            }
                        }
                    ],
                    "synonyms": [
                        "purchasing agent"
                    ],
                    "common_phrases": [
                        "fashion buyer",
                        "retail buyer",
                        "senior buyer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сотрудник, отвечающий за закупку товаров для перепродажи в розничной торговле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закупщик",
                                "target_lang_sense_clarification": "профессия"
                            },
                            {
                                "target_lang_word": "байер",
                                "target_lang_sense_clarification": "в индустрии моды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48d4ea41-181b-32ed-b0e4-bcbd9bfd81ea",
                    "sense_definition": "A person who purchases items consumed or used as components in the manufacture of products.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "professional_buyer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>buyer</w> negotiated a better price for the raw materials.",
                            "target_lang_translations": {
                                "ru": "<w>Закупщик</w> договорился о более выгодной цене на сырьё."
                            }
                        },
                        {
                            "text": "He works as a <w>buyer</w> for a large automotive company.",
                            "target_lang_translations": {
                                "ru": "Он работает <w>закупщиком</w> в крупной автомобильной компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "purchasing agent"
                    ],
                    "common_phrases": [
                        "industrial buyer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист, закупающий материалы или компоненты для производства продукции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закупщик",
                                "target_lang_sense_clarification": "специалист по закупкам"
                            },
                            {
                                "target_lang_word": "снабженец",
                                "target_lang_sense_clarification": "сотрудник отдела снабжения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
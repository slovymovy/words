            "senses": [
                {
                    "sense_id": "af3e6aa9-95a9-385d-8c2c-6c05d018a79f",
                    "sense_definition": "A member of the Celtic ethnic group native to Ireland, Scotland, and the Isle of Man, historically associated with the Gaelic languages.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The historian gave a lecture on the ancient customs of the <w>Gaels</w>.",
                            "target_lang_translations": {
                                "ru": "Историк прочитал лекцию о древних обычаях <w>гэлов</w>."
                            }
                        },
                        {
                            "text": "As a Scottish <w>Gael</w>, he was very proud of his linguistic heritage.",
                            "target_lang_translations": {
                                "ru": "Будучи шотландским <w>гэлом</w>, он очень гордился своим языковым наследием."
                            }
                        }
                    ],
                    "synonyms": [
                        "Celt"
                    ],
                    "common_phrases": [
                        "Scottish Gael",
                        "Irish Gael"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Ireland, Scotland, Isle of Man"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель кельтской этнической группы, коренной житель Ирландии, Шотландии и острова Мэн, исторически связанный с гэльскими языками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гэл",
                                "target_lang_sense_clarification": "представитель народа"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c3326a6d-66f0-3b49-9351-e7c8acc073c3",
                    "sense_definition": "A first name given to either males or females.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We are waiting for <w>Gael</w> to arrive before we start dinner.",

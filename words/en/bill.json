{
    "word_family": [
        "billing",
        "billable",
        "billboard",
        "billfold",
        "billhook"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6ced1a47-e704-3fc4-907c-7a9419da2f6d",
                    "sense_definition": "A written statement of details and costs for goods or services provided, requesting payment.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "payment_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can we have the <w>bill</w>, please?",
                            "target_lang_translations": {
                                "ru": "Можно нам <w>счёт</w>, пожалуйста?"
                            }
                        },
                        {
                            "text": "He forgot to pay his electricity <w>bill</w> on time.",
                            "target_lang_translations": {
                                "ru": "Он забыл вовремя оплатить <w>счёт</w> за электричество."
                            }
                        }
                    ],
                    "synonyms": [
                        "invoice",
                        "account",
                        "check"
                    ],
                    "common_phrases": [
                        "pay the bill",
                        "foot the bill",
                        "utility bill",
                        "phone bill"
                    ],
                    "target_lang_definitions": {
                        "ru": "Письменный документ с указанием стоимости предоставленных товаров или услуг и требованием оплаты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "счёт",
                                "target_lang_sense_clarification": "документ на оплату"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "026597b6-4b19-38fd-9bbe-f30a6fb2b11b",
                    "sense_definition": "A piece of paper money.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "payment_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I found a ten-dollar <w>bill</w> on the sidewalk.",
                            "target_lang_translations": {
                                "ru": "Я нашёл десятидолларовую <w>купюру</w> на тротуаре."
                            }
                        },
                        {
                            "text": "The vending machine would not accept the crumpled <w>bill</w>.",
                            "target_lang_translations": {
                                "ru": "Торговый автомат не принимал мятую <w>купюру</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "banknote",
                        "note"
                    ],
                    "antonyms": [
                        "coin"
                    ],
                    "common_phrases": [
                        "dollar bill",
                        "hundred-dollar bill"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mostly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бумажный денежный знак."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "купюра",
                                "target_lang_sense_clarification": "денежный знак"
                            },
                            {
                                "target_lang_word": "банкнота",
                                "target_lang_sense_clarification": "бумажные деньги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d75da685-a9b9-3fcd-bf48-60909681aa72",
                    "sense_definition": "A formal proposal for a new law presented to a legislature for discussion and voting.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legislation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Senate passed the education <w>bill</w> after a long debate.",
                            "target_lang_translations": {
                                "ru": "Сенат принял <w>законопроект</w> об образовании после долгих дебатов."
                            }
                        },
                        {
                            "text": "They introduced a <w>bill</w> to protect local wildlife.",
                            "target_lang_translations": {
                                "ru": "Они внесли <w>законопроект</w> о защите местной дикой природы."
                            }
                        }
                    ],
                    "synonyms": [
                        "measure"
                    ],
                    "common_phrases": [
                        "pass a bill",
                        "introduce a bill",
                        "veto a bill"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное предложение нового закона, вынесенное на обсуждение законодательного органа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "законопроект",
                                "target_lang_sense_clarification": "проект закона"
                            },
                            {
                                "target_lang_word": "билль",
                                "target_lang_sense_clarification": "в англосаксонском праве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53a4f9de-8aad-30cd-b96e-f780993d25d9",
                    "sense_definition": "The hard, projecting mouthpart of a bird.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pelican has a very large pouch attached to its <w>bill</w>.",
                            "target_lang_translations": {
                                "ru": "У пеликана есть очень большой мешок, прикреплённый к <w>клюву</w>."
                            }
                        },
                        {
                            "text": "The bird used its sharp <w>bill</w> to crack open the seeds.",
                            "target_lang_translations": {
                                "ru": "Птица использовала свой острый <w>клюв</w>, чтобы раскалывать семена."
                            }
                        }
                    ],
                    "synonyms": [
                        "beak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твёрдая выступающая часть рта птицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клюв",
                                "target_lang_sense_clarification": "часть тела птицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b539c20d-a296-33a7-b7f3-6db7a4992e82",
                    "sense_definition": "The projecting front part of a cap that shades the eyes.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He pulled the <w>bill</w> of his baseball cap down to shield his eyes from the sun.",
                            "target_lang_translations": {
                                "ru": "Он надвинул <w>козырёк</w> своей бейсболки, чтобы защитить глаза от солнца."
                            }
                        },
                        {
                            "text": "The <w>bill</w> of the hat was bent out of shape.",
                            "target_lang_translations": {
                                "ru": "<w>Козырёк</w> кепки потерял форму."
                            }
                        }
                    ],
                    "synonyms": [
                        "brim",
                        "peak",
                        "visor"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mostly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступающая передняя часть головного убора, защищающая глаза от солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "козырёк",
                                "target_lang_sense_clarification": "часть кепки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e8fb764c-fdb4-3a97-909e-5afb6868488f",
                    "sense_definition": "To send a request for payment to a customer for goods or services.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "payment_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer will <w>bill</w> you for the consultation hours.",
                            "target_lang_translations": {
                                "ru": "Адвокат <w>выставит</w> вам <w>счёт</w> за часы консультации."
                            }
                        },
                        {
                            "text": "Please <w>bill</w> the expenses directly to my company.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>выставьте счёт</w> за расходы непосредственно моей компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "charge",
                        "invoice"
                    ],
                    "common_phrases": [
                        "bill someone for something"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправить клиенту требование об оплате товаров или услуг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выставить счёт",
                                "target_lang_sense_clarification": "потребовать оплату"
                            },
                            {
                                "target_lang_word": "выписать счёт",
                                "target_lang_sense_clarification": "оформить оплату"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52eda12b-c16e-3ffd-aad8-48451439117f",
                    "sense_definition": "To announce, describe, or advertise someone or something in a specific way.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "advertising",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie was <w>billed</w> as the greatest comedy of the year.",
                            "target_lang_translations": {
                                "ru": "Фильм был <w>анонсирован</w> как величайшая комедия года."
                            }
                        },
                        {
                            "text": "She is <w>billed</w> to perform at the festival next weekend.",
                            "target_lang_translations": {
                                "ru": "<w>Объявлено</w>, что она выступит на фестивале в следующие выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "advertise",
                        "promote"
                    ],
                    "common_phrases": [
                        "billed as"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объявлять, описывать или рекламировать кого-то или что-то определённым образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анонсировать",
                                "target_lang_sense_clarification": "объявлять о выступлении"
                            },
                            {
                                "target_lang_word": "рекламировать",
                                "target_lang_sense_clarification": "представлять публике"
                            },
                            {
                                "target_lang_word": "объявлять",
                                "target_lang_sense_clarification": "в афише"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "0dbd4b05-d424-34e0-a174-cea7a0024f06",
                    "sense_definition": "A common diminutive (short form) of the male given name William.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My uncle <w>Bill</w> lives in Chicago.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Билл</w> живёт в Чикаго."
                            }
                        },
                        {
                            "text": "He prefers to be called <w>Bill</w> rather than William.",
                            "target_lang_translations": {
                                "ru": "Он предпочитает, чтобы его называли <w>Билл</w>, а не Уильям."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространённая уменьшительная форма мужского имени Уильям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Билл",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
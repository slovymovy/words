{
    "word_family": [
        "contemporaneous",
        "contemporaneity",
        "contemporaneously"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b0fe0e13-25d0-3e1b-a8de-9512e0a50f17",
                    "sense_definition": "Modern; belonging to the present time.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_modern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum has a large collection of <w>contemporary</w> art from around the world.",
                            "target_lang_translations": {
                                "ru": "В музее собрана большая коллекция <w>современного</w> искусства со всего мира."
                            }
                        },
                        {
                            "text": "Many <w>contemporary</w> social issues are discussed in this magazine.",
                            "target_lang_translations": {
                                "ru": "В этом журнале обсуждаются многие <w>современные</w> социальные проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "modern",
                        "current",
                        "present",
                        "present-day"
                    ],
                    "antonyms": [
                        "archaic"
                    ],
                    "common_phrases": [
                        "contemporary art",
                        "contemporary music",
                        "contemporary society",
                        "contemporary world"
                    ],
                    "target_lang_definitions": {
                        "ru": "Современный; относящийся к настоящему времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совреме́нный",
                                "target_lang_sense_clarification": "нынешний, актуальный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9dfde312-eb8f-38da-b212-1fbf0ded7068",
                    "sense_definition": "Existing or occurring at the same period of time.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_coexistent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The historian relied on <w>contemporary</w> accounts to describe the battle accurately.",
                            "target_lang_translations": {
                                "ru": "Историк опирался на <w>современные</w> той эпохе свидетельства, чтобы точно описать битву."
                            }
                        },
                        {
                            "text": "This style of architecture was <w>contemporary</w> with the reign of the first emperor.",
                            "target_lang_translations": {
                                "ru": "Этот архитектурный стиль был <w>современен</w> правлению первого императора."
                            }
                        }
                    ],
                    "synonyms": [
                        "contemporaneous",
                        "coeval",
                        "coetaneous"
                    ],
                    "antonyms": [
                        "anachronistic"
                    ],
                    "common_phrases": [
                        "contemporary with",
                        "contemporary accounts",
                        "contemporary sources"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий или происходящий в тот же период времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совреме́нный",
                                "target_lang_sense_clarification": "относящийся к той же эпохе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5bde3608-fc4c-3a3d-b634-46cb60f72b79",
                    "sense_definition": "A person or thing living or existing at the same time as another.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_coexistent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bach was a <w>contemporary</w> of Handel, though they never met.",
                            "target_lang_translations": {
                                "ru": "Бах был <w>современником</w> Генделя, хотя они никогда не встречались."
                            }
                        },
                        {
                            "text": "She is widely respected by her <w>contemporaries</w> in the scientific community.",
                            "target_lang_translations": {
                                "ru": "Она пользуется большим уважением среди <w>современников</w> в научном сообществе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "a contemporary of",
                        "his contemporaries",
                        "her contemporaries"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, живущий в одно время с кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совреме́нник",
                                "target_lang_sense_clarification": "человек одной эпохи (мужской род)"
                            },
                            {
                                "target_lang_word": "совреме́нница",
                                "target_lang_sense_clarification": "человек одной эпохи (женский род)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
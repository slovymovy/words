    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab8a82c4-1673-3ad7-add8-3b99139ab361",
                    "sense_definition": "Zewnętrzny narząd słuchu u ludzi i zwierząt, znajdujący się po obu stronach głowy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_hearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko zasłoniło <uszy> rękami, ponieważ muzyka była zbyt głośna.",
                            "target_lang_translations": {
                                "ru": "Ребенок закрыл <уши> руками, потому что музыка была слишком громкой."
                            }
                        },
                        {
                            "text": "Muszę iść do lekarza, bo od wczoraj boli mnie lewe <ucho>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно к врачу, потому что со вчерашнего дня у меня болит левое <ухо>."
                            }
                        }
                    ],
                    "synonyms": [
                        "małżowina"
                    ],
                    "common_phrases": [
                        "nadstawiać ucha",
                        "wpaść w ucho",
                        "ośle uszy",
                        "słuchać jednym uchem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешний орган слуха у людей и животных, расположенный по обеим сторонам головы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухо",
                                "target_lang_sense_clarification": "орган слуха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "820f0699-e6fd-322e-a67c-483a049ea381",
                    "sense_definition": "Zdolność rozpoznawania dźwięków, talent muzyczny.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek ma świetne <ucho> i potrafi zagrać każdą usłyszaną melodię.",
                            "target_lang_translations": {
                                "ru": "У Марека отличный <слух>, и он может сыграть любую услышанную мелодию."
                            }
                        },
                        {
                            "text": "Niestety słoń nadepnął mi na <ucho>, więc nie będę śpiewać publicznie.",
                            "target_lang_translations": {
                                "ru": "К сожалению, мне медведь на <ухо> наступил, поэтому я не буду петь на публике."
                            }
                        }
                    ],
                    "synonyms": [
                        "słuch"
                    ],
                    "common_phrases": [
                        "mieć gumowe ucho",
                        "mieć dobre ucho"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность различать звуки, музыкальный талант."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слух",
                                "target_lang_sense_clarification": "музыкальный талант"
                            },
                            {
                                "target_lang_word": "ухо",
                                "target_lang_sense_clarification": "в идиомах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58c9788c-9de6-39ab-9a29-c275c94b833a",
                    "sense_definition": "Część czapki opuszczana w dół w celu ochrony uszu przed zimnem.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W mroźne dni zawsze opuszczam <uszy> w mojej zimowej czapce.",
                            "target_lang_translations": {
                                "ru": "В морозные дни я всегда опускаю <уши> на моей зимней шапке."
                            }
                        },
                        {
                            "text": "Ta czapka ma futrzane <uszy>, które świetnie grzeją w wietrzną pogodę.",
                            "target_lang_translations": {
                                "ru": "У этой шапки меховые <уши>, которые отлично греют в ветреную погоду."
                            }
                        }
                    ],
                    "synonyms": [
                        "nausznik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть шапки, опускаемая вниз для защиты ушей от холода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухо",
                                "target_lang_sense_clarification": "часть шапки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed1c3e6c-13e6-303b-8d0f-0993353d674a",
                    "sense_definition": "Zagięty róg kartki w książce lub zeszycie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie zaginaj rogów, bo w książce zrobią się brzydkie <uszy>.",
                            "target_lang_translations": {
                                "ru": "Не загибай углы, иначе в книге появятся некрасивые <заломы>."
                            }
                        },
                        {
                            "text": "Zamiast używać zakładki, zrobił <ucho> na stronie, którą właśnie czytał.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы использовать закладку, он загнул <уголок> на странице, которую читал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "robić ośle uszy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Загнутый уголок страницы в книге или тетради."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уголок",
                                "target_lang_sense_clarification": "страницы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0de855d-6178-3101-a766-d704a5b1d55d",
                    "sense_definition": "Osoba, która donosi na innych, współpracuje z władzą lub przełożonymi przeciwko grupie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więźniowie unikali go, ponieważ podejrzewali, że jest <uchem>.",
                            "target_lang_translations": {
                                "ru": "Заключенные избегали его, так как подозревали, что он <стукач>."
                            }
                        },
                        {
                            "text": "Nikt nie chciał rozmawiać przy nim szczerze, bo uchodził za <ucho> szefa.",
                            "target_lang_translations": {
                                "ru": "Никто не хотел разговаривать при нем искренне, так как он слыл <стукачом> босса."
                            }
                        }
                    ],
                    "synonyms": [
                        "konfident",
                        "kabel"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который доносит на других, сотрудничает с властями или начальством против группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стукач",
                                "target_lang_sense_clarification": "доносчик"
                            },
                            {
                                "target_lang_word": "доносчик",
                                "target_lang_sense_clarification": "формально"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20ad4874-52e1-3687-ab0e-788cdba10765",
                    "sense_definition": "Element naczynia, torby lub innego przedmiotu służący do jego trzymania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złapał kubek za <ucho> i ostrożnie upił łyk gorącej kawy.",
                            "target_lang_translations": {
                                "ru": "Он схватил кружку за <ручку> и осторожно отпил глоток горячего кофе."
                            }
                        },
                        {
                            "text": "Urwało mi się <ucho> od torby z zakupami i wszystko wypadło na ziemię.",
                            "target_lang_translations": {
                                "ru": "У меня оторвалась <ручка> от сумки с покупками, и всё выпало на землю."
                            }
                        }
                    ],
                    "synonyms": [
                        "uchwyt"
                    ],
                    "common_phrases": [
                        "ucho od śledzia",
                        "ucho kubka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент посуды, сумки или другого предмета, служащий для его удержания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ручка",
                                "target_lang_sense_clarification": "посуды или сумки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2de6b5d1-ca72-3759-af52-629d6d6d5308",
                    "sense_definition": "Mały otwór w igle, przez który przewleka się nitkę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <ucho> jest tak małe, że nie mogę nawlec przez nie nitki.",
                            "target_lang_translations": {
                                "ru": "Это <ушко> такое маленькое, что я не могу продеть через него нитку."
                            }
                        },
                        {
                            "text": "Krawcowa sprawnym ruchem przełożyła czerwoną nić przez <ucho> igły.",
                            "target_lang_translations": {
                                "ru": "Портниха ловким движением продела красную нить через <ушко> иглы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ucho igielne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькое отверстие в игле, через которое продевается нитка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ушко",
                                "target_lang_sense_clarification": "иглы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
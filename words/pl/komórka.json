    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "57b78bd0-855c-39bc-a795-62f34ec99088",
                    "sense_definition": "Potoczne określenie telefonu przenośnego, który działa w oparciu o sieć radiową.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "phone_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj kupiłem sobie nową <komórkę> w salonie.",
                            "target_lang_translations": {
                                "ru": "Вчера я купил себе новый <мобильник> в салоне."
                            }
                        },
                        {
                            "text": "Muszę szybko naładować <komórkę>, bo bateria mi pada.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро зарядить <мобильник>, а то батарейка садится."
                            }
                        }
                    ],
                    "synonyms": [
                        "telefon komórkowy",
                        "komóra"
                    ],
                    "common_phrases": [
                        "dzwonić na komórkę",
                        "wyłączyć komórkę",
                        "ładować komórkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "ellipsis of 'telefon komórkowy'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название мобильного телефона, работающего в радиосети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мобильник",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "сотовый",
                                "target_lang_sense_clarification": "общеупотребительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a89980a6-988e-3458-8315-dc236b5b782d",
                    "sense_definition": "Podstawowa jednostka strukturalna i funkcjonalna organizmów żywych, zdolna do przeprowadzania procesów życiowych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unit_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biolog obserwował żywe <komórki> pod mikroskopem.",
                            "target_lang_translations": {
                                "ru": "Биолог наблюдал за живыми <клетками> под микроскопом."
                            }
                        },
                        {
                            "text": "Każda <komórka> w naszym ciele potrzebuje tlenu.",
                            "target_lang_translations": {
                                "ru": "Каждая <клетка> в нашем теле нуждается в кислороде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "komórka jajowa",
                        "komórka nerwowa",
                        "podział komórki",
                        "budowa komórki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная структурная и функциональная единица живых организмов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клетка",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69c923f2-1be6-3a61-a9b0-db56de6ef07e",
                    "sense_definition": "Niewielkie pomieszczenie gospodarcze, często w piwnicy lub na strychu, służące do przechowywania rzadziej używanych przedmiotów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "room_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trzymamy stary rower w <komórce> w piwnicy.",
                            "target_lang_translations": {
                                "ru": "Мы держим старый велосипед в <кладовке> в подвале."
                            }
                        },
                        {
                            "text": "Znieś te niepotrzebne pudła do <komórki>.",
                            "target_lang_translations": {
                                "ru": "Отнеси эти ненужные коробки в <кладовку>."
                            }
                        }
                    ],
                    "synonyms": [
                        "schowek",
                        "składzik"
                    ],
                    "common_phrases": [
                        "komórka lokatorska",
                        "komórka na węgiel",
                        "klucz do komórki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое подсобное помещение, часто в подвале или на чердаке, для хранения вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кладовка",
                                "target_lang_sense_clarification": "помещение для хранения"
                            },
                            {
                                "target_lang_word": "чулан",
                                "target_lang_sense_clarification": "темное помещение"
                            },
                            {
                                "target_lang_word": "сарай",
                                "target_lang_sense_clarification": "хозяйственная постройка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50c4b861-5eb1-306f-93e7-2608ea1c038d",
                    "sense_definition": "Pojedyncze pole w arkuszu kalkulacyjnym, powstałe z przecięcia wiersza i kolumny, w którym wpisuje się dane.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unit_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wpisz wynik obliczeń w odpowiednią <komórkę> tabeli.",
                            "target_lang_translations": {
                                "ru": "Впиши результат вычислений в соответствующую <ячейку> таблицы."
                            }
                        },
                        {
                            "text": "Ta <komórka> zawiera błędną formułę matematyczną.",
                            "target_lang_translations": {
                                "ru": "Эта <ячейка> содержит ошибочную математическую формулу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zaznaczyć komórkę",
                        "formatowanie komórek",
                        "adres komórki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельное поле в электронной таблице на пересечении строки и столбца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ячейка",
                                "target_lang_sense_clarification": "в таблице"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8470bc9-d07e-30a4-a564-eb3ac7513e98",
                    "sense_definition": "Mała grupa ludzi stanowiąca najniższy szczebel w strukturze organizacji, na przykład partii politycznej lub grupy konspiracyjnej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "unit_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tajana <komórka> spotykała się w prywatnym mieszkaniu.",
                            "target_lang_translations": {
                                "ru": "Тайная <ячейка> встречалась в частной квартире."
                            }
                        },
                        {
                            "text": "To najmniejsza <komórka> organizacyjna w naszej firmie.",
                            "target_lang_translations": {
                                "ru": "Это самая маленькая организационная <ячейка> в нашей фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogniwo"
                    ],
                    "common_phrases": [
                        "komórka organizacyjna",
                        "komórka wywiadu",
                        "komórka społeczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Малая группа людей, составляющая низшее звено в структуре организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ячейка",
                                "target_lang_sense_clarification": "организационная единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2d6b878-ed0a-3154-852c-b6b8583bb941",
                    "sense_definition": "Mała, wyodrębniona przestrzeń wewnątrz większej struktury, na przykład sześciokątny otwór w plastrze miodu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "unit_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pszczoły gromadzą słodki miód w woskowych <komórkach>.",
                            "target_lang_translations": {
                                "ru": "Пчелы собирают сладкий мед в восковых <ячейках>."
                            }
                        },
                        {
                            "text": "Każda <komórka> w tej szafce ma swój numer.",
                            "target_lang_translations": {
                                "ru": "Каждая <ячейка> в этом шкафчике имеет свой номер."
                            }
                        }
                    ],
                    "synonyms": [
                        "przegródka"
                    ],
                    "common_phrases": [
                        "komórka plastra miodu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое обособленное пространство или углубление внутри большей структуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ячейка",
                                "target_lang_sense_clarification": "отделение"
                            },
                            {
                                "target_lang_word": "сота",
                                "target_lang_sense_clarification": "пчелиная"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "77c5b0fd-f4c0-3823-bc67-9083a41d3150",
                    "sense_definition": "Badanie cech, elementów lub struktury jakiegoś przedmiotu, pojęcia lub zjawiska w celu jego lepszego zrozumienia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy przeprowadzić dokładną <w>analizę</w> tego problemu przed podjęciem decyzji.",
                            "target_lang_translations": {
                                "ru": "Мы должны провести тщательный <w>анализ</w> этой проблемы перед принятием решения."
                            }
                        },
                        {
                            "text": "Jego rozważania opierają się o zdroworozsądkową <w>analizę</w> pojęć.",
                            "target_lang_translations": {
                                "ru": "Его рассуждения основываются на здравом <w>анализе</w> понятий."
                            }
                        }
                    ],
                    "antonyms": [
                        "synteza"
                    ],
                    "common_phrases": [
                        "analiza danych",
                        "szczegółowa analiza",
                        "analiza rynku",
                        "poddać analizie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изучение характеристик, элементов или структуры предмета, понятия или явления для его лучшего понимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анализ",
                                "target_lang_sense_clarification": "метод исследования"
                            },
                            {
                                "target_lang_word": "разбор",
                                "target_lang_sense_clarification": "детальное рассмотрение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "090a8952-dff3-3701-82b1-ce8960588b3f",

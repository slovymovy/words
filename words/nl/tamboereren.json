                            },
                            {
                                "target_lang_word": "педалировать",
                                "target_lang_sense_clarification": "усиленно подчеркивать"
                            },
                            {
                                "target_lang_word": "настаивать",
                                "target_lang_sense_clarification": "упорно утверждать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f864f0d8-dbea-3456-ae99-41ffa395d753",
                    "sense_definition": "Luidruchtig uiting geven aan vreugde of triomf.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rejoice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten niet te vroeg <w>tamboereren</w> over de goede resultaten.",
                            "target_lang_translations": {
                                "ru": "Нам не следует слишком рано <w>трубить</w> о хороших результатах."
                            }

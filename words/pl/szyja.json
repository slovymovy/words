                            {
                                "target_lang_word": "шея",
                                "target_lang_sense_clarification": "в некоторых идиомах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c42b42ee-d3f0-38df-ab06-9c24189180ce",
                    "sense_definition": "Część ubioru otaczająca szyję; kołnierz.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krawiec musiał zwęzić <w>szyję</w> w tym płaszczu, bo była za luźna.",
                            "target_lang_translations": {
                                "ru": "Портной должен был ушить <w>горловину</w> этого пальто, так как она была слишком свободной."
                            }
                        },
                        {
                            "text": "Ten sweter ma bardzo szeroką <w>szyję</w>, więc łatwo się go zakłada.",
                            "target_lang_translations": {
                                "ru": "У этого свитера очень широкая <w>горловина</w>, поэтому его легко надевать."
                            }
                        }
                    ],
                    "synonyms": [
                        "kołnierz"
                    ],
                    "common_phrases": [
                        "szeroka szyja"
                    ],
                    "target_lang_definitions": {

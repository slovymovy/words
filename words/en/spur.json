{
    "word_family": [
        "spurred"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0a3bcb58-4931-3571-a5aa-538dd0069bf7",
                    "sense_definition": "To urge or encourage someone to action, or to promote the development of something.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "urge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The promise of a bonus <w>spurred</w> the team to finish the project ahead of schedule.",
                            "target_lang_translations": {
                                "ru": "Обещание бонуса <w>подстегнуло</w> команду закончить проект раньше срока."
                            }
                        },
                        {
                            "text": "Lower taxes often <w>spur</w> economic growth by encouraging investment.",
                            "target_lang_translations": {
                                "ru": "Снижение налогов часто <w>стимулирует</w> экономический рост, поощряя инвестиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "encourage",
                        "motivate",
                        "stimulate",
                        "prompt",
                        "incite",
                        "drive",
                        "propel",
                        "urge"
                    ],
                    "antonyms": [
                        "deter",
                        "discourage"
                    ],
                    "common_phrases": [
                        "spur on",
                        "spur into action",
                        "spur growth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Побуждать кого-либо к действию или способствовать развитию чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "побуждать",
                                "target_lang_sense_clarification": "склонять к действию"
                            },
                            {
                                "target_lang_word": "стимулировать",
                                "target_lang_sense_clarification": "давать толчок к развитию (обычно в экономике или биологии)"
                            },
                            {
                                "target_lang_word": "подстёгивать",
                                "target_lang_sense_clarification": "ускорять, активизировать (образное выражение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d49e66ef-57d0-3de6-9e77-4bc3b520dde8",
                    "sense_definition": "To prod a horse with a metal device on the heel to make it go faster.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "prod",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rider <w>spurred</w> his horse to jump over the tall fence.",
                            "target_lang_translations": {
                                "ru": "Всадник <w>пришпорил</w> коня, чтобы перепрыгнуть через высокий забор."
                            }
                        },
                        {
                            "text": "She <w>spurred</w> her mount forward to catch up with the rest of the group.",
                            "target_lang_translations": {
                                "ru": "Она <w>пришпорила</w> своего скакуна, чтобы догнать остальную группу."
                            }
                        }
                    ],
                    "synonyms": [
                        "goad",
                        "prod"
                    ],
                    "common_phrases": [
                        "spur a horse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Колоть лошадь металлическим устройством на каблуке, чтобы заставить её бежать быстрее"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пришпоривать",
                                "target_lang_sense_clarification": "колоть шпорами"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b97330e6-0db9-3e48-8508-b45c46d7ada2",
                    "sense_definition": "Something that acts as an incentive or motivation to do something.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "incentive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The competition served as a <w>spur</w> for the students to improve their grades.",
                            "target_lang_translations": {
                                "ru": "Соревнование послужило <w>стимулом</w> для студентов улучшить свои оценки."
                            }
                        },
                        {
                            "text": "Fear of failure can sometimes be a powerful <w>spur</w> to action.",
                            "target_lang_translations": {
                                "ru": "Страх неудачи иногда может быть мощным <w>толчком</w> к действию."
                            }
                        }
                    ],
                    "synonyms": [
                        "incentive",
                        "stimulus",
                        "impetus",
                        "motivation"
                    ],
                    "antonyms": [
                        "deterrent",
                        "disincentive"
                    ],
                    "common_phrases": [
                        "on the spur of the moment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то, что действует как побуждение или мотивация сделать что-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стимул",
                                "target_lang_sense_clarification": "побудительная причина"
                            },
                            {
                                "target_lang_word": "толчок",
                                "target_lang_sense_clarification": "импульс к действию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5c66d27-85b5-357d-8c5e-64b60f4cf16e",
                    "sense_definition": "A metal tool with a small spike or wheel worn on a rider's heel, used to urge a horse forward.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cowboy adjusted his silver <w>spurs</w> before mounting the horse.",
                            "target_lang_translations": {
                                "ru": "Ковбой поправил свои серебряные <w>шпоры</w> перед тем, как сесть на лошадь."
                            }
                        },
                        {
                            "text": "He dug his <w>spurs</w> into the horse's flanks to make it gallop.",
                            "target_lang_translations": {
                                "ru": "Он вонзил <w>шпоры</w> в бока лошади, чтобы заставить её скакать галопом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "earn one's spurs",
                        "win one's spurs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлический инструмент с маленьким шипом или колесиком на каблуке всадника, используемый для управления лошадью"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпора",
                                "target_lang_sense_clarification": "приспособление для верховой езды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19207865-4bc0-32b6-85df-815d2de68092",
                    "sense_definition": "A short branch road or railway line that extends from a main route.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "branch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train was moved to a siding on a <w>spur</w> line.",
                            "target_lang_translations": {
                                "ru": "Поезд перевели на запасной путь на железнодорожной <w>ветке</w>."
                            }
                        },
                        {
                            "text": "We took a <w>spur</w> off the main highway to reach the remote village.",
                            "target_lang_translations": {
                                "ru": "Мы свернули на <w>ответвление</w> от главного шоссе, чтобы добраться до отдаленной деревни."
                            }
                        }
                    ],
                    "synonyms": [
                        "branch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая боковая дорога или железнодорожная линия, отходящая от основного маршрута"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветка",
                                "target_lang_sense_clarification": "железнодорожная линия"
                            },
                            {
                                "target_lang_word": "ответвление",
                                "target_lang_sense_clarification": "дороги или пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f71f2c9-b02a-3320-9fae-685821298da0",
                    "sense_definition": "A ridge or projection of land extending from a mountain or mountain range.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "branch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They climbed a rocky <w>spur</w> to get a better view of the valley.",
                            "target_lang_translations": {
                                "ru": "Они взобрались на скалистый <w>отрог</w>, чтобы лучше рассмотреть долину."
                            }
                        },
                        {
                            "text": "The castle was built on a <w>spur</w> of the hill for better defense.",
                            "target_lang_translations": {
                                "ru": "Замок был построен на <w>отроге</w> холма для лучшей обороны."
                            }
                        }
                    ],
                    "synonyms": [
                        "projection",
                        "ridge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горный хребет или выступ суши, отходящий от горы или горной цепи"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отрог",
                                "target_lang_sense_clarification": "ответвление горного хребта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88a98c7a-9f86-3e99-896d-99956fea1e82",
                    "sense_definition": "A sharp, bony spike on the leg of a rooster or certain other birds.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rooster used its sharp <w>spur</w> to defend the flock.",
                            "target_lang_translations": {
                                "ru": "Петух использовал свою острую <w>шпору</w> для защиты стаи."
                            }
                        },
                        {
                            "text": "Be careful of the <w>spurs</w> on the legs of that large bird.",
                            "target_lang_translations": {
                                "ru": "Остерегайтесь <w>шпор</w> на ногах этой крупной птицы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Острый костяной шип на ноге петуха или некоторых других птиц"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шпора",
                                "target_lang_sense_clarification": "роговой нарост у птиц"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d7bcce7a-3acd-3cd0-b514-afc03be8e124",
                    "sense_definition": "An English surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Spur</w> arrived at the office early this morning.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Спур</w> прибыл в офис сегодня рано утром."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Английская фамилия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Спур",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
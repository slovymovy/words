                {
                    "sense_id": "eb48720e-31c1-301a-9a10-a315a4a1358c",
                    "sense_definition": "O przedmiotach, miejscach lub organizmach: mający rozmiary znacznie przekraczające przeciętną; zajmujący dużo miejsca w przestrzeni.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "size_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W centrum miasta stoi <w>wielki</w> pomnik bohatera narodowego.",
                            "target_lang_translations": {
                                "ru": "В центре города стоит <w>большой</w> памятник национальному герою."
                            }
                        },
                        {
                            "text": "Mój sąsiad kupił sobie <w>wielki</w> dom z ogrodem.",
                            "target_lang_translations": {
                                "ru": "Мой сосед купил себе <w>большой</w> дом с садом."
                            }
                        }
                    ],
                    "synonyms": [
                        "duży",
                        "ogromny",
                        "olbrzymi",
                        "pokaźny"
                    ],
                    "antonyms": [
                        "mały",
                        "niewielki"
                    ],
                    "common_phrases": [

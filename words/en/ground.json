    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "88fe2e19-09a2-3051-91a6-a160da00c6ed",
                    "sense_definition": "The solid surface of the earth that people walk on.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "earth_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I sat on the <w>ground</w> to rest after the long walk.",
                            "target_lang_translations": {
                                "ru": "Я сел на <w>землю</w>, чтобы отдохнуть после долгой прогулки."
                            }
                        },
                        {
                            "text": "The apple fell from the tree and hit the <w>ground</w>.",
                            "target_lang_translations": {
                                "ru": "Яблоко упало с дерева и ударилось о <w>землю</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "earth"
                    ],
                    "common_phrases": [
                        "on the ground",
                        "fall to the ground",
                        "hit the ground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердая поверхность земли, по которой ходят люди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "земля",
                                "target_lang_sense_clarification": "поверхность планеты"
                            },
                            {
                                "target_lang_word": "поверхность",
                                "target_lang_sense_clarification": "верхний слой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b2a0169-6ef1-323e-a1fb-44668c18ef73",
                    "sense_definition": "The soil or earth on the surface of the land.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "earth_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gardener dug a hole in the <w>ground</w> to plant the rose bush.",
                            "target_lang_translations": {
                                "ru": "Садовник выкопал яму в <w>земле</w>, чтобы посадить розовый куст."
                            }
                        },
                        {
                            "text": "The <w>ground</w> was too hard to dig because of the frost.",
                            "target_lang_translations": {
                                "ru": "<w>Земля</w> была слишком твердой, чтобы копать, из-за мороза."
                            }
                        }
                    ],
                    "synonyms": [
                        "earth",
                        "milled"
                    ],
                    "common_phrases": [
                        "dig in the ground",
                        "hard ground",
                        "soft ground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Почва или земля на поверхности суши."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "земля",
                                "target_lang_sense_clarification": "почва, грунт"
                            },
                            {
                                "target_lang_word": "грунт",
                                "target_lang_sense_clarification": "почва, основание"
                            },
                            {
                                "target_lang_word": "почва",
                                "target_lang_sense_clarification": "плодородный слой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c48d35a-d35e-36ad-8963-7b34a0b6a07c",
                    "sense_definition": "An area of land used for a specific purpose or activity.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children played happily on the school sports <w>ground</w>.",
                            "target_lang_translations": {
                                "ru": "Дети весело играли на школьной спортивной <w>площадке</w>."
                            }
                        },
                        {
                            "text": "This forest has been a hunting <w>ground</w> for centuries.",
                            "target_lang_translations": {
                                "ru": "Этот лес веками был охотничьими <w>угодьями</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sports ground",
                        "hunting ground",
                        "breeding ground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок земли, используемый для определенной цели или деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "площадка",
                                "target_lang_sense_clarification": "место для игр или спорта"
                            },
                            {
                                "target_lang_word": "угодья",
                                "target_lang_sense_clarification": "территория для охоты"
                            },
                            {
                                "target_lang_word": "территория",
                                "target_lang_sense_clarification": "участок местности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f835da18-c848-3b7b-b7a9-219eff5c1965",
                    "sense_definition": "A reason or justification for an action, belief, or argument.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was fired from his job on the <w>grounds</w> of misconduct.",
                            "target_lang_translations": {
                                "ru": "Его уволили с работы на <w>основании</w> проступка."
                            }
                        },
                        {
                            "text": "There are no reasonable <w>grounds</w> for complaint in this situation.",
                            "target_lang_translations": {
                                "ru": "В этой ситуации нет разумных <w>оснований</w> для жалобы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the grounds of",
                        "grounds for",
                        "reasonable grounds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причина или обоснование действия, убеждения или аргумента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основание",
                                "target_lang_sense_clarification": "причина, повод"
                            },
                            {
                                "target_lang_word": "почва",
                                "target_lang_sense_clarification": "перен. основа для чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e15d48ed-32f7-3010-a7cf-f2aa37bf353d",
                    "sense_definition": "A stadium or field where sports matches are played.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fans gathered at the football <w>ground</w> hours before the match started.",
                            "target_lang_translations": {
                                "ru": "Болельщики собрались на футбольном <w>стадионе</w> за несколько часов до начала матча."
                            }
                        },
                        {
                            "text": "The team has not lost a single game on their home <w>ground</w> this season.",
                            "target_lang_translations": {
                                "ru": "Команда не проиграла ни одной игры на своем <w>поле</w> в этом сезоне."
                            }
                        }
                    ],
                    "common_phrases": [
                        "home ground",
                        "football ground"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стадион или поле, где проводятся спортивные матчи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стадион",
                                "target_lang_sense_clarification": "спортивное сооружение"
                            },
                            {
                                "target_lang_word": "поле",
                                "target_lang_sense_clarification": "игровая площадка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa9c5291-8bb1-3e92-90e2-837c5f969678",
                    "sense_definition": "A connection between an electrical circuit and the earth, used for safety.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "electricity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must ensure the equipment has a proper <w>ground</w> to prevent electric shocks.",
                            "target_lang_translations": {
                                "ru": "Вы должны убедиться, что оборудование имеет надлежащее <w>заземление</w> для предотвращения поражения электрическим током."
                            }
                        },
                        {
                            "text": "The third wire in the cable serves as the <w>ground</w>.",
                            "target_lang_translations": {
                                "ru": "Третий провод в кабеле служит <w>землей</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "earth"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in American English; 'earth' is used in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединение электрической цепи с землей, используемое для безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заземление",
                                "target_lang_sense_clarification": "защитное соединение"
                            },
                            {
                                "target_lang_word": "земля",
                                "target_lang_sense_clarification": "провод заземления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "15db357c-f24b-3603-ae78-0b87a3b1c133",
                    "sense_definition": "The past tense and past participle of <w>grind</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "form_grind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>ground</w> the coffee beans fresh this morning.",
                            "target_lang_translations": {
                                "ru": "Она свеже <w>смолола</w> кофейные зерна сегодня утром."
                            }
                        },
                        {
                            "text": "The wheat was <w>ground</w> into flour at the old mill.",
                            "target_lang_translations": {
                                "ru": "Пшеница была <w>смолота</w> в муку на старой мельнице."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past of grind"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола grind (молоть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молол",

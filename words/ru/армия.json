                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical distinction"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The main part of land troops (as opposed to the privileged guard)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regular army",
                                "target_lang_sense_clarification": "standard troops vs elite guards"
                            },
                            {
                                "target_lang_word": "line army",

                },
                {
                    "sense_id": "f7332dd3-9205-3828-968c-218eed065542",
                    "sense_definition": "dzieło lub dokument powstały z połączenia fragmentów innych tekstów lub materiałów",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_general",
                    "name_type": "no",
                    "examples": [
                        {

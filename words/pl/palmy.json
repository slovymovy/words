                {
                    "sense_id": "608eec65-9d8d-3c5e-89bc-a1e76bd58676",
                    "sense_definition": "odmieniona forma rzeczownika <w>palma</w> (mianownik, biernik i wołacz liczby mnogiej lub dopełniacz liczby pojedynczej)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na tej rajskiej plaży rosną wysokie <palmy>.",
                            "target_lang_translations": {
                                "ru": "На этом райском пляже растут высокие <w>пальмы</w>."
                            }
                        },
                        {
                            "text": "Nie widzę tej <palmy> na twoim zdjęciu z wakacji.",
                            "target_lang_translations": {
                                "ru": "Я не вижу этой <w>пальмы</w> на твоей фотографии с отпуска."
                            }
                        }
                    ],
                    "common_phrases": [
                        "palmy kokosowe",
                        "liście palmy",
                        "cień palmy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "rzeczownika palma"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Склоняемая форма существительного <w>palma</w> (пальма)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пальмы",
                                "target_lang_sense_clarification": "деревья (мн.ч. или род.п. ед.ч.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d870fd53-af74-3cee-9aa3-131e71387269",
                    "sense_definition": "rodzina roślin jednoliściennych, obejmująca drzewa i krzewy strefy międzyzwrotnikowej, zazwyczaj o kłodzinie zakończonej pióropuszem liści",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Palmy> są ważnym elementem krajobrazu w krajach tropikalnych.",
                            "target_lang_translations": {
                                "ru": "<w>Пальмы</w> являются важным элементом ландшафта в тропических странах."
                            }
                        },
                        {
                            "text": "Wiele gatunków z rodziny <palm> dostarcza cennych surowców, takich jak olej czy włókna.",
                            "target_lang_translations": {
                                "ru": "Многие виды из семейства <w>пальм</w> дают ценное сырье, такое как масло или волокна."
                            }
                        }
                    ],
                    "synonyms": [
                        "arekowate"
                    ],
                    "common_phrases": [
                        "rodzina palm",
                        "uprawa palm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Семейство однодольных растений (пальмовые)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пальмовые",
                                "target_lang_sense_clarification": "семейство растений"
                            },
                            {
                                "target_lang_word": "пальмы",
                                "target_lang_sense_clarification": "общее название растений"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a741a87c-f845-36cf-9b18-0a370e483dc5",
                    "sense_definition": "odmieniona forma czasownika <w>palić</w> (tryb rozkazujący, pierwsza osoba liczby mnogiej); zachęta do spalania czegoś lub palenia tytoniu",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie <palmy> śmieci w lesie, bo to niebezpieczne.",
                            "target_lang_translations": {
                                "ru": "Не <w>будем жечь</w> мусор в лесу, так как это опасно."
                            }
                        },
                        {
                            "text": "Chodźmy na balkon i <palmy> papierosy tam, a nie w domu.",
                            "target_lang_translations": {
                                "ru": "Пойдемте на балкон и <w>покурим</w> сигареты там, а не дома."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie palmy",
                        "palmy ognisko"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownika palić"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола <w>palić</w> (1-е лицо мн. ч., повелительное наклонение); призыв жечь или курить"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давайте жечь",
                                "target_lang_sense_clarification": "призыв сжигать что-то"
                            },
                            {
                                "target_lang_word": "давайте курить",
                                "target_lang_sense_clarification": "призыв курить"
                            },
                            {
                                "target_lang_word": "покурим",
                                "target_lang_sense_clarification": "разговорная форма"
                            }
                        ]
                    }
                }
            ]
        }

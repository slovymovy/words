    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3f546860-b0bf-3d2f-8bc5-4572899a5711",
                    "sense_definition": "Граница, разделяющая какие-либо территории; линия раздела.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Многие наши друзья сейчас живут и работают за <w>рубежом</w>.",
                            "target_lang_translations": {
                                "en": "Many of our friends now live and work <w>abroad</w>."
                            }
                        },
                        {
                            "text": "Река служит естественным <w>рубежом</w> между двумя соседними государствами.",
                            "target_lang_translations": {
                                "en": "The river serves as a natural <w>boundary</w> between the two neighboring states."
                            }
                        }
                    ],
                    "synonyms": [
                        "граница",
                        "предел"
                    ],
                    "common_phrases": [
                        "за рубежом",
                        "из-за рубежа",
                        "охрана рубежей"
                    ],
                    "target_lang_definitions": {
                        "en": "A line or frontier area separating political or geographical regions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "border",
                                "target_lang_sense_clarification": "separating political or geographical regions"
                            },
                            {
                                "target_lang_word": "boundary",
                                "target_lang_sense_clarification": "dividing line between two areas"
                            },
                            {
                                "target_lang_word": "frontier",
                                "target_lang_sense_clarification": "border area between countries"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6ee55f4-2648-3986-9433-38d0dcb7879f",
                    "sense_definition": "Момент перехода от одного периода времени к другому; важный поворотный момент.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это великое научное открытие произошло на <w>рубеже</w> девятнадцатого и двадцатого веков.",
                            "target_lang_translations": {
                                "en": "This great scientific discovery happened at the <w>turn</w> of the nineteenth and twentieth centuries."
                            }
                        },
                        {
                            "text": "Мы живём на <w>рубеже</w> эпох, когда технологии меняют мир каждый день.",
                            "target_lang_translations": {
                                "en": "We live on the <w>threshold</w> of eras, when technology changes the world every day."
                            }
                        }
                    ],
                    "synonyms": [
                        "веха",
                        "этап"
                    ],
                    "common_phrases": [
                        "на рубеже веков",
                        "на рубеже тысячелетий"
                    ],
                    "target_lang_definitions": {
                        "en": "A point of transition between two distinct periods or states; a turning point."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "point of change in time, e.g., turn of the century"
                            },
                            {
                                "target_lang_word": "threshold",
                                "target_lang_sense_clarification": "point of entry or beginning of a new state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65c3506a-4a1b-39ce-bb10-2de430b9044c",
                    "sense_definition": "Участок местности, занятый или подготовленный войсками для ведения боя.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полк получил приказ занять новый оборонительный <w>рубеж</w> до рассвета.",
                            "target_lang_translations": {
                                "en": "The regiment received an order to occupy a new defensive <w>line</w> before dawn."
                            }
                        },
                        {
                            "text": "Артиллерия вышла на огневой <w>рубеж</w> и приготовилась к стрельбе.",
                            "target_lang_translations": {
                                "en": "The artillery reached the firing <w>line</w> and prepared to fire."
                            }
                        }
                    ],
                    "synonyms": [
                        "линия"
                    ],
                    "common_phrases": [
                        "оборонительный рубеж",
                        "огневой рубеж",
                        "выйти на рубеж"
                    ],
                    "target_lang_definitions": {
                        "en": "A strip of terrain occupied or prepared by troops for defense or combat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "line",
                                "target_lang_sense_clarification": "military defensive position or boundary"
                            },
                            {
                                "target_lang_word": "position",
                                "target_lang_sense_clarification": "location occupied by troops"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
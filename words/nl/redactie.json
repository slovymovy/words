{
    "word_family": [
        "redacteur",
        "redactioneel",
        "redigeren",
        "hoofdredacteur",
        "eindredacteur"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c92dace2-43ab-3eb1-b840-8a6eac6fbf5e",
                    "sense_definition": "De groep personen die verantwoordelijk is voor de samenstelling en inhoud van een publicatie, zoals een krant, tijdschrift of televisieprogramma.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>redactie</w> heeft besloten om het ingezonden artikel niet te plaatsen.",
                            "target_lang_translations": {
                                "en": "The <w>editorial staff</w> has decided not to publish the submitted article."
                            }
                        },
                        {
                            "text": "Zij werkt al jaren bij de <w>redactie</w> van een groot landelijk dagblad.",
                            "target_lang_translations": {
                                "en": "She has been working on the <w>editorial staff</w> of a large national newspaper for years."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de voltallige redactie",
                        "de hoofdredactie"
                    ],
                    "target_lang_definitions": {
                        "en": "The group of people responsible for the content and compilation of a publication."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "editorial staff",
                                "target_lang_sense_clarification": "collective group of editors"
                            },
                            {
                                "target_lang_word": "editors",
                                "target_lang_sense_clarification": "people who edit"
                            },
                            {
                                "target_lang_word": "editorial board",
                                "target_lang_sense_clarification": "senior editorial group"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "616f72a7-2702-357a-a646-6fc2bfaa58fd",
                    "sense_definition": "De fysieke ruimte of het kantoor waar de redacteuren hun werkzaamheden verrichten.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga even langs de <w>redactie</w> om mijn stuk in te leveren.",
                            "target_lang_translations": {
                                "en": "I'm dropping by the <w>editorial office</w> to hand in my piece."
                            }
                        },
                        {
                            "text": "Op de <w>redactie</w> is het altijd erg druk vlak voor de deadline.",
                            "target_lang_translations": {
                                "en": "It is always very busy in the <w>newsroom</w> right before the deadline."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op de redactie",
                        "naar de redactie"
                    ],
                    "target_lang_definitions": {
                        "en": "The office or physical space where editorial work is done."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "editorial office",
                                "target_lang_sense_clarification": "physical location"
                            },
                            {
                                "target_lang_word": "newsroom",
                                "target_lang_sense_clarification": "office for journalists"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a2dd0d7-a90a-35aa-8b6f-e2d6b195cfa4",
                    "sense_definition": "Het proces van het bewerken, corrigeren en persklaar maken van teksten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit wetenschappelijke boek is verschenen onder <w>redactie</w> van professor Jansen.",
                            "target_lang_translations": {
                                "en": "This scientific book was published under the <w>editorship</w> of Professor Jansen."
                            }
                        },
                        {
                            "text": "De <w>redactie</w> van het manuscript nam meer tijd in beslag dan verwacht.",
                            "target_lang_translations": {
                                "en": "The <w>editing</w> of the manuscript took more time than expected."
                            }
                        }
                    ],
                    "synonyms": [
                        "redigering"
                    ],
                    "common_phrases": [
                        "onder redactie van",
                        "de eindredactie doen"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of editing or preparing text for publication."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "editing",
                                "target_lang_sense_clarification": "act of correcting text"
                            },
                            {
                                "target_lang_word": "editorship",
                                "target_lang_sense_clarification": "supervision of a publication"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58e72107-48f5-3bb8-a8d3-98223e314573",
                    "sense_definition": "De specifieke manier waarop iets onder woorden is gebracht of geformuleerd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>redactie</w> van deze wetstekst luistert erg nauw en moet precies zijn.",
                            "target_lang_translations": {
                                "en": "The <w>wording</w> of this legal text is very sensitive and must be precise."
                            }
                        },
                        {
                            "text": "Hij maakte bezwaar tegen de slordige <w>redactie</w> van de brief.",
                            "target_lang_translations": {
                                "en": "He objected to the sloppy <w>drafting</w> of the letter."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The specific wording or formulation of a text."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wording",
                                "target_lang_sense_clarification": "choice of words"
                            },
                            {
                                "target_lang_word": "drafting",
                                "target_lang_sense_clarification": "formulation of a document"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
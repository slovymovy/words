                        "ru": [
                            {
                                "target_lang_word": "ранний",
                                "target_lang_sense_clarification": "об историческом периоде или этапе развития"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "052295fa-3bcb-367a-a98c-4b8c0d9ad672",
                    "sense_definition": "Eerder plaatsvindend dan verwacht of normaal is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_premature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij stierf helaas een veel te <w>vroege</w> dood.",
                            "target_lang_translations": {
                                "en": "Unfortunately, she died a much too <w>early</w> death.",
                                "ru": "К сожалению, она умерла слишком <w>ранней</w> смертью."
                            }
                        },
                        {
                            "text": "Door het warme weer was er dit jaar een <w>vroege</w> bloei van de planten.",
                            "target_lang_translations": {
                                "en": "Due to the warm weather, there was an <w>early</w> bloom of the plants this year.",
                                "ru": "Из-за теплой погоды в этом году было <w>раннее</w> цветение растений."
                            }
                        }
                    ],
                    "synonyms": [
                        "voortijdig"
                    ],
                    "antonyms": [
                        "laat"
                    ],
                    "target_lang_definitions": {
                        "en": "Happening sooner than expected or normal.",
                        "ru": "Случающийся раньше, чем ожидалось или является нормой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "early",
                                "target_lang_sense_clarification": "sooner than expected"
                            },
                            {
                                "target_lang_word": "premature",
                                "target_lang_sense_clarification": "happening too soon"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преждевременный",
                                "target_lang_sense_clarification": "наступивший раньше срока"
                            },
                            {
                                "target_lang_word": "ранний",
                                "target_lang_sense_clarification": "скорый, досрочный"
                            }
                        ]
                    }
                }
            ]
        },

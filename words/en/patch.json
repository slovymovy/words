                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "connect_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sound engineer had to <w>patch</w> the microphone into a different channel.",
                            "target_lang_translations": {
                                "ru": "Звукорежиссеру пришлось <w>подключить</w> микрофон в другой канал."
                            }
                        },
                        {
                            "text": "Can you <w>patch</w> me through to the director's office?",
                            "target_lang_translations": {
                                "ru": "Можете <w>соединить</w> меня с кабинетом директора?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "patch through",
                        "patch into"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временно соединять электронное оборудование с помощью кабеля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединять",
                                "target_lang_sense_clarification": "подключать"
                            },
                            {
                                "target_lang_word": "коммутировать",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0670835c-dbf8-34f6-9603-4b5d01f0332f",
                    "sense_definition": "Wiadomość, wpis lub komentarz opublikowany w internecie, na przykład na forum dyskusyjnym, blogu lub w mediach społecznościowych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "internet_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój <w>post</w> na forum ma bardzo dużo błędów ortograficznych.",
                            "target_lang_translations": {
                                "ru": "В твоем <w>посте</w> на форуме очень много орфографических ошибок."
                            }
                        },
                        {
                            "text": "Muszę edytować ten <w>post</w>, ponieważ zapomniałem dodać zdjęcia.",
                            "target_lang_translations": {
                                "ru": "Мне нужно отредактировать этот <w>пост</w>, так как я забыл добавить фото."
                            }
                        }
                    ],
                    "synonyms": [
                        "wiadomość",
                        "wypowiedź"
                    ],
                    "common_phrases": [
                        "napisać post",
                        "opublikować post",
                        "udostępnić post",
                        "skomentować post"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщение, запись или комментарий, опубликованные в интернете, например, на форуме, в блоге или социальных сетях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пост",
                                "target_lang_sense_clarification": "интернет"
                            },
                            {
                                "target_lang_word": "запись",
                                "target_lang_sense_clarification": "в блоге или соцсети"
                            },
                            {
                                "target_lang_word": "публикация",
                                "target_lang_sense_clarification": "формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88e042a3-22be-3fc8-bad1-8959b936cb70",
                    "sense_definition": "Dobrowolna rezygnacja ze spożywania niektórych pokarmów (zwykle mięsnych) lub powstrzymywanie się od jedzenia w ogóle, często z przyczyn religijnych lub zdrowotnych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fasting_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas ramadanu wszystkich dorosłych muzułmanów obowiązuje ścisły <w>post</w>.",
                            "target_lang_translations": {
                                "ru": "Во время Рамадана все взрослые мусульмане обязаны соблюдать строгий <w>пост</w>."
                            }
                        },
                        {
                            "text": "Lekarz zalecił mi jednodniowy <w>post</w> dla oczyszczenia organizmu.",
                            "target_lang_translations": {
                                "ru": "Врач порекомендовал мне однодневное <w>голодание</w> для очищения организма."
                            }
                        }
                    ],
                    "antonyms": [
                        "rozpusta"
                    ],
                    "common_phrases": [
                        "ścisły post",
                        "zachowywać post",
                        "post o chlebie i wodzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добровольный отказ от употребления определенной пищи (обычно мясной) или воздержание от еды вообще, часто по религиозным или медицинским причинам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пост",
                                "target_lang_sense_clarification": "религиозное воздержание"
                            },
                            {
                                "target_lang_word": "голодание",
                                "target_lang_sense_clarification": "медицинское"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac536cea-97f8-33c9-861b-b9a33ad300f6",
                    "sense_definition": "Okres w roku liturgicznym lub kalendarzowym, w którym obowiązuje wstrzemięźliwość od jedzenia i hucznych zabaw.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fasting_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielki <w>Post</w> w kościele katolickim trwa czterdzieści dni.",
                            "target_lang_translations": {
                                "ru": "Великий <w>пост</w> в католической церкви длится сорок дней."
                            }
                        },
                        {
                            "text": "Współcześnie okres adwentu nie jest już traktowany jako czas <w>postu</w>.",
                            "target_lang_translations": {
                                "ru": "В наше время Адвент уже не считается временем <w>поста</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "karnawał"
                    ],
                    "common_phrases": [
                        "Wielki Post",
                        "czas postu",
                        "okres postu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период в литургическом или календарном году, в который предписывается воздержание от пищи и развлечений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пост",
                                "target_lang_sense_clarification": "период времени"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "2e139139-268e-332d-ae0a-5265a62e66c7",
                    "sense_definition": "The third-person singular form of the verb <w>mean</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A red light <w>means</w> that you have to stop the car immediately.",
                            "target_lang_translations": {
                                "ru": "Красный свет <w>означает</w>, что вы должны немедленно остановить машину."
                            }
                        },
                        {
                            "text": "She <w>means</w> well, but her advice is not always helpful.",
                            "target_lang_translations": {
                                "ru": "Она <w>хочет</w> как лучше, но её советы не всегда полезны."
                            }
                        }

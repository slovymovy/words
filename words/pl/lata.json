                    "sense_id": "132b1ed6-c5dc-31a1-b8f7-ab0f4badd7f5",
                    "sense_definition": "Liczba mnoga rzeczownika <w>rok</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkam w tym mieście przez dwa <w>lata</w>.",

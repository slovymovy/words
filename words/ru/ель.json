    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6626b2d1-9932-3e5a-823d-b9ec689777c2",
                    "sense_definition": "Вечнозелёное хвойное дерево семейства сосновых с конусообразной кроной и колючей хвоей.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tree_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В лесу росла высокая и пушистая <w>ель</w>.",
                            "target_lang_translations": {
                                "en": "A tall and fluffy <w>spruce</w> grew in the forest."
                            }
                        },
                        {
                            "text": "Зимой <w>ель</w> остаётся зелёной, когда другие деревья сбрасывают листву.",
                            "target_lang_translations": {
                                "en": "In winter, the <w>spruce</w> remains green when other trees shed their leaves."
                            }
                        },
                        {
                            "text": "И <w>ель</w> сквозь иней зеленеет.",
                            "target_lang_translations": {
                                "en": "And the <w>spruce</w> shows green through the frost."
                            }
                        }
                    ],
                    "synonyms": [
                        "ёлка"
                    ],
                    "common_phrases": [
                        "голубая ель",
                        "ветка ели",
                        "под елью",
                        "стройная ель"
                    ],
                    "target_lang_definitions": {
                        "en": "An evergreen coniferous tree of the pine family with a cone-shaped crown and prickly needles."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spruce",
                                "target_lang_sense_clarification": "tree"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f83db37-559d-3acb-92d8-d727babe13a0",
                    "sense_definition": "Древесина этого дерева, используемая как строительный или поделочный материал.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот музыкальный инструмент изготовлен из качественной <w>ели</w>.",
                            "target_lang_translations": {
                                "en": "This musical instrument is made of high-quality <w>spruce</w>."
                            }
                        },
                        {
                            "text": "Для строительства дома мы выбрали брус из <w>ели</w>.",
                            "target_lang_translations": {
                                "en": "We chose <w>spruce</w> timber for building the house."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мебель из ели",
                        "массив ели",
                        "корпус из ели"
                    ],
                    "target_lang_definitions": {
                        "en": "The wood of this tree, used as a building or crafting material."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spruce",
                                "target_lang_sense_clarification": "wood"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f58eeabd-549a-32dc-be69-736b992b2527",
                    "sense_definition": "Simple past and past participle of <w>occupy</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The army <w>occupied</w> the capital city without resistance.",
                            "target_lang_translations": {
                                "ru": "Армия <w>оккупировала</w> столицу без сопротивления."
                            }
                        },
                        {
                            "text": "He has <w>occupied</w> this position for over ten years.",
                            "target_lang_translations": {
                                "ru": "Он <w>занимал</w> эту должность более десяти лет."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола <w>occupy</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оккупи́ровал",
                                "target_lang_sense_clarification": "прошедшее время (о войсках)"
                            },
                            {
                                "target_lang_word": "занима́л",
                                "target_lang_sense_clarification": "прошедшее время (о должности или месте)"
                            },
                            {
                                "target_lang_word": "за́нятый",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2c4153b0-c6a2-3606-b70b-469a46be6eac",
                    "sense_definition": "Being used by someone; not free or available.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "usage_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot enter because the bathroom is <w>occupied</w>.",
                            "target_lang_translations": {
                                "ru": "Я не могу войти, потому что ванная <w>занята</w>."
                            }
                        },
                        {
                            "text": "Is this seat <w>occupied</w>, or may I sit here?",
                            "target_lang_translations": {
                                "ru": "Это место <w>занято</w>, или я могу сесть здесь?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ocupado",
                        "ocupado"
                    ],
                    "antonyms": [
                        "vacant",
                        "free",
                        "empty"
                    ],
                    "common_phrases": [
                        "seat is occupied",
                        "room is occupied",
                        "currently occupied"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используемый кем-то; не свободный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за́нятый",
                                "target_lang_sense_clarification": "не свободный (о месте, помещении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "556b4c08-0c80-36d1-87ae-88bb590003e6",
                    "sense_definition": "Busy doing something; having one's attention or time taken up.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "usage_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>occupied</w> with a client at the moment.",
                            "target_lang_translations": {
                                "ru": "В данный момент она <w>занята</w> с клиентом."
                            }
                        },
                        {
                            "text": "I try to keep myself <w>occupied</w> during the long winter evenings.",
                            "target_lang_translations": {
                                "ru": "Я стараюсь оставаться <w>занятым</w> долгими зимними вечерами."
                            }
                        }
                    ],
                    "synonyms": [
                        "ocupado",
                        "ocupado"
                    ],
                    "antonyms": [
                        "idle",
                        "free"
                    ],
                    "common_phrases": [
                        "keep occupied",
                        "fully occupied",
                        "stay occupied"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занятый делом; поглощенный чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за́нятый",
                                "target_lang_sense_clarification": "поглощенный делом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3832117-aa5e-30e9-ad22-0d01d6d9aa65",
                    "sense_definition": "Controlled by a foreign military force.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Life was very difficult for civilians living in the <w>occupied</w> territory.",
                            "target_lang_translations": {
                                "ru": "Жизнь была очень тяжелой для мирных жителей на <w>оккупированной</w> территории."
                            }
                        },
                        {
                            "text": "My grandfather travelled to <w>occupied</w> Japan shortly after the war.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка ездил в <w>оккупированную</w> Японию вскоре после войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "ocupado",
                        "ocupado"
                    ],
                    "antonyms": [
                        "liberated",
                        "free"
                    ],
                    "common_phrases": [
                        "occupied territory",
                        "occupied zone",
                        "occupied lands"
                    ],
                    "target_lang_definitions": {
                        "ru": "Контролируемый иностранными войсками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оккупи́рованный",
                                "target_lang_sense_clarification": "захваченный войсками"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "cc02a3a9-3b5b-32e6-b12c-a4358a9dc6f3",
                    "sense_definition": "Used to communicate that a room, especially a toilet, is currently in use.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When he knocked on the stall door, I quickly shouted, \"<w>Occupied</w>!\"",
                            "target_lang_translations": {
                                "ru": "Когда он постучал в дверь кабинки, я быстро крикнул: «<w>Занято</w>!»"
                            }
                        },
                        {
                            "text": "Just yell \"<w>Occupied</w>\" if someone tries to open the door.",
                            "target_lang_translations": {
                                "ru": "Просто крикни «<w>Занято</w>», если кто-то попытается открыть дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "ocupado"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы сообщить, что помещение (особенно туалет) сейчас занято."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за́нято",
                                "target_lang_sense_clarification": "возглас"
                            }
                        ]
                    }
                }
            ]
        }
    ]

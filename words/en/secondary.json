                    ],
                    "common_phrases": [
                        "secondary school",
                        "secondary education",
                        "secondary level"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in UK, Australia, Canada; equivalent to high school in US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к обучению детей в возрасте от 11 до 18 лет, следующему за начальным образованием."
                    },
                    "translations": {

                {
                    "sense_id": "eb93a552-a447-3c15-a4b8-204f99ffae73",
                    "sense_definition": "The highest value, amount, or setting possible; a shortened form of <w>maximum</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_maximum",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He turned the volume up to the <w>max</w> to hear the song clearly.",
                            "target_lang_translations": {
                                "ru": "Он включил громкость на <w>максимум</w>, чтобы отчетливо слышать песню."
                            }
                        },
                        {
                            "text": "The car was loaded to the <w>max</w> with camping gear for the trip.",
                            "target_lang_translations": {
                                "ru": "Машина была загружена по <w>максимуму</w> туристическим снаряжением для поездки."
                            }
                        }
                    ],
                    "antonyms": [
                        "min"
                    ],
                    "common_phrases": [
                        "to the max"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наивысшее возможное значение или количество; сокращение от слова \"максимум\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "максимум",
                                "target_lang_sense_clarification": "наивысшее значение"
                            },
                            {
                                "target_lang_word": "предел",
                                "target_lang_sense_clarification": "крайняя степень"
                            }

{
    "word_family": [
        "drinken",
        "drank",
        "drinker",
        "dronkenschap",
        "drinkbaar"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e6562b05-b6cf-344f-b073-13e43a7e1f92",
                    "sense_definition": "Verleden tijd meervoud van <w>drinken</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>dronken</w> gisteren een heerlijk kopje koffie in de stad.",
                            "target_lang_translations": {
                                "en": "We <w>drank</w> a delicious cup of coffee in the city yesterday.",
                                "ru": "Вчера мы <w>пили</w> вкусный кофе в городе."
                            }
                        },
                        {
                            "text": "De kinderen <w>dronken</w> hun glazen limonade in één keer leeg.",
                            "target_lang_translations": {
                                "en": "The children <w>drank</w> their glasses of lemonade in one go.",
                                "ru": "Дети залпом <w>выпили</w> свои стаканы с лимонадом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wij dronken",
                        "zij dronken",
                        "jullie dronken"
                    ],
                    "target_lang_definitions": {
                        "en": "Past tense plural of <w>to drink</w>.",
                        "ru": "Прошедшее время множественного числа глагола <w>drinken</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drank",
                                "target_lang_sense_clarification": "simple past tense"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пили",
                                "target_lang_sense_clarification": "прошедшее время мн. ч. от 'пить'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8c3f8c70-5312-3bcb-8061-d111b84432d4",
                    "sense_definition": "Onder invloed van alcohol waardoor men de controle over zichzelf verliest.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie hield de <w>dronken</w> bestuurder aan voor een controle.",
                            "target_lang_translations": {
                                "en": "The police stopped the <w>drunk</w> driver for a check.",
                                "ru": "Полиция остановила <w>пьяного</w> водителя для проверки."
                            }
                        },
                        {
                            "text": "Hij werd snel <w>dronken</w> omdat hij niets had gegeten.",
                            "target_lang_translations": {
                                "en": "He got <w>drunk</w> quickly because he hadn't eaten anything.",
                                "ru": "Он быстро стал <w>пьяным</w>, так как ничего не ел."
                            }
                        }
                    ],
                    "synonyms": [
                        "beschonken",
                        "zat",
                        "bezopen"
                    ],
                    "antonyms": [
                        "nuchter"
                    ],
                    "common_phrases": [
                        "dronken worden",
                        "dronken zijn",
                        "dronken voeren"
                    ],
                    "target_lang_definitions": {
                        "en": "Under the influence of alcohol, causing one to lose control of oneself.",
                        "ru": "Находящийся под воздействием алкоголя, из-за чего человек теряет контроль над собой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drunk",
                                "target_lang_sense_clarification": "intoxicated by alcohol"
                            },
                            {
                                "target_lang_word": "intoxicated",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пьяный",
                                "target_lang_sense_clarification": "находящийся в состоянии опьянения"
                            },
                            {
                                "target_lang_word": "нетрезвый",
                                "target_lang_sense_clarification": "выпивший, находящийся под хмельком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc907dc6-da2f-3363-88a9-193440eb9f6c",
                    "sense_definition": "In een toestand van grote opwinding of verwarring verkeren, vergelijkbaar met intoxicatie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze was helemaal <w>dronken</w> van geluk toen ze het nieuws hoorde.",
                            "target_lang_translations": {
                                "en": "She was completely <w>drunk</w> with happiness when she heard the news.",
                                "ru": "Она была совершенно <w>пьяна</w> от счастья, когда услышала эту новость."
                            }
                        },
                        {
                            "text": "De soldaten waren <w>dronken</w> van de overwinning en vierden feest.",
                            "target_lang_translations": {
                                "en": "The soldiers were <w>drunk</w> with victory and celebrated.",
                                "ru": "Солдаты были <w>пьяны</w> от победы и праздновали."
                            }
                        }
                    ],
                    "antonyms": [
                        "nuchter"
                    ],
                    "common_phrases": [
                        "dronken van geluk",
                        "dronken van slaap"
                    ],
                    "target_lang_definitions": {
                        "en": "In a state of great excitement or confusion, similar to intoxication.",
                        "ru": "Находящийся в состоянии сильного возбуждения или замешательства, похожем на опьянение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drunk",
                                "target_lang_sense_clarification": "metaphorical: elated or emboldened"
                            },
                            {
                                "target_lang_word": "intoxicated",
                                "target_lang_sense_clarification": "metaphorical"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "опьянённый",
                                "target_lang_sense_clarification": "перен. охваченный сильным чувством"
                            },
                            {
                                "target_lang_word": "пьяный",
                                "target_lang_sense_clarification": "перен. в состоянии эйфории"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "71336aff-3b28-3f56-a574-465db04499f0",
                    "sense_definition": "Meervoud van <w>dronk</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het feestmaal werden er vele <w>dronken</w> uitgebracht op het bruidspaar.",
                            "target_lang_translations": {
                                "en": "During the banquet, many <w>toasts</w> were proposed to the bridal couple.",
                                "ru": "Во время застолья за молодожёнов было произнесено много <w>тостов</w>."
                            }
                        },
                        {
                            "text": "Na enkele stevige <w>dronken</w> uit de beker voelde hij zich beter.",
                            "target_lang_translations": {
                                "en": "After several hearty <w>drinks</w> from the cup, he felt better.",
                                "ru": "Сделав несколько больших <w>глотков</w> из кубка, он почувствовал себя лучше."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of <w>dronk</w>.",
                        "ru": "Множественное число от <w>dronk</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "toasts",
                                "target_lang_sense_clarification": "act of drinking in honor of someone"
                            },
                            {
                                "target_lang_word": "drinks",
                                "target_lang_sense_clarification": "quantities of liquid swallowed"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тосты",
                                "target_lang_sense_clarification": "заздравные речи"
                            },
                            {
                                "target_lang_word": "глотки",
                                "target_lang_sense_clarification": "количество жидкости, проглатываемое за раз"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
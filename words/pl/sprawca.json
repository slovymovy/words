    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12e65111-fa70-3de3-a330-9466368d4e0b",
                    "sense_definition": "Osoba, która popełniła przestępstwo, wykroczenie lub spowodowała negatywne zdarzenie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "perpetrator",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja wciąż poszukuje <w>sprawcy</w> wczorajszego napadu na bank.",
                            "target_lang_translations": {
                                "ru": "Полиция все еще разыскивает <w>преступника</w>, совершившего вчерашнее ограбление банка."
                            }
                        },
                        {
                            "text": "Pijany kierowca był <w>sprawcą</w> tego tragicznego wypadku.",
                            "target_lang_translations": {
                                "ru": "Пьяный водитель был <w>виновником</w> этой трагической аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "winowajca",
                        "winny"
                    ],
                    "antonyms": [
                        "ofiara",
                        "poszkodowany"
                    ],
                    "common_phrases": [
                        "sprawca wypadku",
                        "sprawca przestępstwa",
                        "nieznany sprawca",
                        "ujęcie sprawcy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, совершившее преступление, правонарушение или ставшее причиной негативного события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виновник",
                                "target_lang_sense_clarification": "причина происшествия или беды"
                            },
                            {
                                "target_lang_word": "преступник",
                                "target_lang_sense_clarification": "тот, кто совершил уголовное преступление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ecab4b6-69e6-3855-9f2e-a43a13bf5612",
                    "sense_definition": "Osoba lub czynnik będący przyczyną powstania czegoś lub sprawiający, że coś się dzieje (często w kontekście podniosłym lub abstrakcyjnym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "creator_cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu filozofów uważało Absolut za pierwszego <w>sprawcę</w> wszechświata.",
                            "target_lang_translations": {
                                "ru": "Многие философы считали Абсолют первым <w>творцом</w> вселенной."
                            }
                        },
                        {
                            "text": "To on był głównym <w>sprawcą</w> naszego niespodziewanego sukcesu.",
                            "target_lang_translations": {
                                "ru": "Это он был главным <w>автором</w> нашего неожиданного успеха."
                            }
                        }
                    ],
                    "synonyms": [
                        "autor",
                        "twórca",
                        "stwórca",
                        "pomysłodawca"
                    ],
                    "common_phrases": [
                        "główny sprawca",
                        "bezpośredni sprawca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или фактор, являющийся причиной возникновения чего-либо или создателем (часто в возвышенном или абстрактном смысле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "творец",
                                "target_lang_sense_clarification": "создатель"
                            },
                            {
                                "target_lang_word": "автор",
                                "target_lang_sense_clarification": "инициатор или причина успеха"
                            },
                            {
                                "target_lang_word": "создатель",
                                "target_lang_sense_clarification": "первопричина"
                            }
                        ]
                    }
                }
            ]
        }
    ]

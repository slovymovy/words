{
    "word_family": [
        "pełny",
        "wypełniać",
        "dopełnić",
        "uzupełnić",
        "napełnić",
        "przepełnić",
        "wypełnienie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "068f7e79-32f3-338b-94c6-1c31c8a1ef78",
                    "sense_definition": "Wpisać wymagane informacje w puste pola na dokumencie, formularzu lub ankiecie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "administrative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz dokładnie <w>wypełnić</w> ten wniosek o paszport.",
                            "target_lang_translations": {
                                "ru": "Ты должен тщательно <w>заполнить</w> это заявление на паспорт."
                            }
                        },
                        {
                            "text": "Proszę <w>wypełnić</w> formularz drukowanymi literami.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>заполните</w> бланк печатными буквами."
                            }
                        }
                    ],
                    "synonyms": [
                        "uzupełnić",
                        "wypisać"
                    ],
                    "common_phrases": [
                        "wypełnić formularz",
                        "wypełnić wniosek",
                        "wypełnić ankietę",
                        "wypełnić rubrykę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вписать требуемую информацию в пустые поля документа, бланка или анкеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заполнить",
                                "target_lang_sense_clarification": "документ, бланк, анкету"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b6045b8-9c07-3b57-87f0-edc0b42d0e62",
                    "sense_definition": "Sprawić, że pusta przestrzeń, naczynie lub pomieszczenie staje się pełne; zająć czymś całe dostępne miejsce.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_filling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner <w>wypełnił</w> nasze kieliszki czerwonym winem.",
                            "target_lang_translations": {
                                "ru": "Официант <w>наполнил</w> наши бокалы красным вином."
                            }
                        },
                        {
                            "text": "Gęsty dym szybko <w>wypełnił</w> całe mieszkanie.",
                            "target_lang_translations": {
                                "ru": "Густой дым быстро <w>заполнил</w> всю квартиру."
                            }
                        }
                    ],
                    "synonyms": [
                        "napełnić",
                        "zapełnić"
                    ],
                    "antonyms": [
                        "opróżnić"
                    ],
                    "common_phrases": [
                        "wypełnić wodą",
                        "wypełnić po brzegi",
                        "wypełnić lukę",
                        "wypełnić pustkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать так, чтобы пустое пространство, сосуд или помещение стало полным; занять чем-либо всё доступное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наполнить",
                                "target_lang_sense_clarification": "налить или насыпать в сосуд"
                            },
                            {
                                "target_lang_word": "заполнить",
                                "target_lang_sense_clarification": "занять всё пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2949359a-62e5-30f0-8c34-172835b0012e",
                    "sense_definition": "Zrealizować coś, co było zaplanowane, obiecane lub nakazane (np. obowiązek, wolę, obietnicę).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_fulfillment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz musi bez wahania <w>wypełnić</w> każdy rozkaz dowódcy.",
                            "target_lang_translations": {
                                "ru": "Солдат обязан без колебаний <w>выполнить</w> любой приказ командира."
                            }
                        },
                        {
                            "text": "Syn obiecał <w>wypełnić</w> ostatnią wolę swojego ojca.",
                            "target_lang_translations": {
                                "ru": "Сын пообещал <w>исполнить</w> последнюю волю своего отца."
                            }
                        }
                    ],
                    "synonyms": [
                        "spełnić",
                        "zrealizować",
                        "wykonać"
                    ],
                    "antonyms": [
                        "zaniedbać",
                        "zlekceważyć"
                    ],
                    "common_phrases": [
                        "wypełnić obowiązek",
                        "wypełnić obietnicę",
                        "wypełnić rozkaz",
                        "wypełnić zadanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осуществить то, что было запланировано, обещано или приказано (например, обязанность, волю, обещание)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выполнить",
                                "target_lang_sense_clarification": "приказ, задание, долг"
                            },
                            {
                                "target_lang_word": "исполнить",
                                "target_lang_sense_clarification": "волю, обещание, желание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63efcc02-b104-353a-b4e5-e32b21952162",
                    "sense_definition": "Stać się pełnym; zostać zajętym w całości przez kogoś lub coś (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_filling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sala koncertowa szybko <w>wypełniła</w> się niecierpliwą publicznością.",
                            "target_lang_translations": {
                                "ru": "Концертный зал быстро <w>заполнился</w> нетерпеливой публикой."
                            }
                        },
                        {
                            "text": "Jej oczy <w>wypełniły</w> się łzami na wieść o wypadku.",
                            "target_lang_translations": {
                                "ru": "Её глаза <w>наполнились</w> слезами при новости об аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapełnić się",
                        "napełnić się"
                    ],
                    "antonyms": [
                        "opróżnić się",
                        "opustoszeć"
                    ],
                    "common_phrases": [
                        "wypełnić się ludźmi",
                        "wypełnić się dymem",
                        "oczy wypełniły się łzami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать полным; быть занятым целиком кем-то или чем-то (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наполниться",
                                "target_lang_sense_clarification": "стать полным (о сосуде, глазах)"
                            },
                            {
                                "target_lang_word": "заполниться",
                                "target_lang_sense_clarification": "стать занятым (о помещении, пространстве)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
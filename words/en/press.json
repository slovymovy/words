    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "40c82802-e526-38b6-a28f-35ea0c7b2cd3",
                    "sense_definition": "To apply force to a button, switch, or key to make a machine or device work.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>press</w> the red button to stop the machine immediately.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>нажмите</w> красную кнопку, чтобы немедленно остановить машину."
                            }
                        },
                        {
                            "text": "She <w>pressed</w> the doorbell and waited for someone to answer.",
                            "target_lang_translations": {
                                "ru": "Она <w>нажала</w> на дверной звонок и стала ждать ответа."
                            }
                        }
                    ],
                    "synonyms": [
                        "push",
                        "click"
                    ],
                    "common_phrases": [
                        "press a button",
                        "press play",
                        "press enter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикладывать силу к кнопке, переключателю или клавише, чтобы привести в действие механизм или устройство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нажимать",
                                "target_lang_sense_clarification": "на кнопку, клавишу"
                            },
                            {
                                "target_lang_word": "жать",
                                "target_lang_sense_clarification": "давить на что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b35c99d-758a-3057-8be2-3bd214567d5c",
                    "sense_definition": "To push something firmly against a surface or object.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>pressed</w> his ear against the wall to hear what they were saying.",
                            "target_lang_translations": {
                                "ru": "Он <w>прижал</w> ухо к стене, чтобы услышать, о чем они говорят."
                            }
                        },
                        {
                            "text": "The doctor <w>pressed</w> gently on my stomach to check for pain.",
                            "target_lang_translations": {
                                "ru": "Врач осторожно <w>надавил</w> мне на живот, чтобы проверить, нет ли боли."
                            }
                        }
                    ],
                    "synonyms": [
                        "push",
                        "squeeze"
                    ],
                    "common_phrases": [
                        "press against",
                        "press down"
                    ],
                    "target_lang_definitions": {
                        "ru": "С силой толкать или прислонять что-либо к поверхности или объекту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прижимать",
                                "target_lang_sense_clarification": "плотно прислонять"
                            },
                            {
                                "target_lang_word": "давить",
                                "target_lang_sense_clarification": "надавливать на поверхность"
                            },
                            {
                                "target_lang_word": "прикладывать",
                                "target_lang_sense_clarification": "ухо, руку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac964253-f7d9-3463-8bd7-4a983b13fc22",
                    "sense_definition": "To make clothes or fabric smooth and remove creases using a hot iron.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_household",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>press</w> my shirt before the job interview tomorrow.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>погладить</w> рубашку перед завтрашним собеседованием."
                            }
                        },
                        {
                            "text": "The dry cleaner will clean and <w>press</w> your suit for a small fee.",
                            "target_lang_translations": {
                                "ru": "В химчистке почистят и <w>погладят</w> ваш костюм за небольшую плату."
                            }
                        }
                    ],
                    "synonyms": [
                        "iron",
                        "smooth"
                    ],
                    "antonyms": [
                        "wrinkle",
                        "crease"
                    ],
                    "common_phrases": [
                        "press a shirt",
                        "wash and press"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разглаживать одежду или ткань и удалять складки с помощью горячего утюга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гладить",
                                "target_lang_sense_clarification": "утюгом"
                            },
                            {
                                "target_lang_word": "утюжить",
                                "target_lang_sense_clarification": "разглаживать ткань"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8aea8e4-eb81-387f-9249-c3e3ca010519",
                    "sense_definition": "To strongly urge or try to persuade someone to do something, or to demand something insistently.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The reporters <w>pressed</w> the politician for an answer regarding the scandal.",
                            "target_lang_translations": {
                                "ru": "Репортеры <w>давили</w> на политика, требуя ответа по поводу скандала."
                            }
                        },
                        {
                            "text": "My parents are <w>pressing</w> me to make a decision about college soon.",
                            "target_lang_translations": {
                                "ru": "Родители <w>давят</w> на меня, чтобы я скорее принял решение насчет колледжа."
                            }
                        }
                    ],
                    "synonyms": [
                        "urge",
                        "push",
                        "insist"
                    ],
                    "common_phrases": [
                        "press for details",
                        "press charges",
                        "press the issue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво убеждать кого-либо сделать что-либо или настойчиво требовать чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давить",
                                "target_lang_sense_clarification": "оказывать давление, настаивать"
                            },
                            {
                                "target_lang_word": "настаивать",
                                "target_lang_sense_clarification": "требовать"
                            },
                            {
                                "target_lang_word": "наседать",
                                "target_lang_sense_clarification": "разг. настойчиво просить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4646b16-4a73-3020-97ae-4018a7848271",
                    "sense_definition": "To squeeze fruit or vegetables in order to extract juice or oil.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_processing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Farmers <w>press</w> the olives immediately after harvest to make high-quality oil.",
                            "target_lang_translations": {
                                "ru": "Фермеры <w>выжимают</w> оливки сразу после сбора урожая, чтобы получить высококачественное масло."
                            }
                        },
                        {
                            "text": "We drank freshly <w>pressed</w> apple juice at the orchard.",
                            "target_lang_translations": {
                                "ru": "Во фруктовом саду мы пили свеже<w>выжатый</w> яблочный сок."
                            }
                        }
                    ],
                    "synonyms": [
                        "squeeze",
                        "crush"
                    ],
                    "common_phrases": [
                        "cold pressed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сжимать фрукты или овощи для извлечения сока или масла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выжимать",
                                "target_lang_sense_clarification": "сок, масло"
                            },
                            {
                                "target_lang_word": "давить",
                                "target_lang_sense_clarification": "измельчать для получения сока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7493c3e0-14bf-3ab6-8eaa-f56893f557d3",
                    "sense_definition": "To hold someone or something closely in one's arms or against one's body.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>pressed</w> the child to her chest to comfort him.",
                            "target_lang_translations": {
                                "ru": "Она <w>прижала</w> ребенка к груди, чтобы утешить его."
                            }
                        },
                        {
                            "text": "He <w>pressed</w> her hand warmly before saying goodbye.",
                            "target_lang_translations": {
                                "ru": "Он тепло <w>пожал</w> ей руку перед прощанием."
                            }
                        }
                    ],
                    "synonyms": [
                        "embrace",
                        "hug",
                        "clasp"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "common in literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крепко держать кого-либо или что-либо в объятиях или прижав к телу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прижимать",
                                "target_lang_sense_clarification": "к себе, обнимать"
                            },
                            {
                                "target_lang_word": "пожимать",
                                "target_lang_sense_clarification": "руку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2218d51d-d598-38d8-a8df-85029054b8be",
                    "sense_definition": "To move forward or gather together in large numbers.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fans <w>pressed</w> against the barriers to get a better view of the band.",
                            "target_lang_translations": {
                                "ru": "Фанаты <w>прижались</w> к ограждениям, чтобы лучше видеть группу."
                            }
                        },
                        {
                            "text": "Crowds <w>pressed</w> around the entrance waiting for the doors to open.",
                            "target_lang_translations": {
                                "ru": "Толпы людей <w>теснились</w> у входа в ожидании открытия дверей."
                            }
                        }
                    ],
                    "synonyms": [
                        "crowd",
                        "throng",
                        "surge"
                    ],
                    "common_phrases": [
                        "press forward"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться вперед или собираться вместе в большом количестве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тесниться",
                                "target_lang_sense_clarification": "толпиться"
                            },
                            {
                                "target_lang_word": "напирать",
                                "target_lang_sense_clarification": "налегать толпой"
                            },
                            {
                                "target_lang_word": "пробиваться",
                                "target_lang_sense_clarification": "сквозь толпу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5986a1d7-7744-3769-b758-1f250215d644",
                    "sense_definition": "Newspapers, magazines, and other news media, or the journalists who work for them.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local <w>press</w> reported extensively on the mayor's decision.",
                            "target_lang_translations": {
                                "ru": "Местная <w>пресса</w> подробно освещала решение мэра."
                            }
                        },
                        {
                            "text": "The famous actor refused to speak to the <w>press</w> after the incident.",
                            "target_lang_translations": {
                                "ru": "Известный актер отказался общаться с <w>прессой</w> после инцидента."
                            }
                        }
                    ],
                    "synonyms": [
                        "media",
                        "journalists",
                        "reporters"
                    ],
                    "common_phrases": [
                        "freedom of the press",
                        "press conference",
                        "press release",
                        "bad press"
                    ],
                    "target_lang_definitions": {
                        "ru": "Газеты, журналы и другие средства массовой информации или журналисты, работающие в них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пресса",
                                "target_lang_sense_clarification": "СМИ, журналисты"
                            },
                            {
                                "target_lang_word": "печать",
                                "target_lang_sense_clarification": "печатные издания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e52a0c0-8df7-39bc-9a81-691f5af3b2d8",
                    "sense_definition": "An act of pushing something or applying force.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With a single <w>press</w> of the button, the engine roared to life.",
                            "target_lang_translations": {
                                "ru": "Одним <w>нажатием</w> кнопки двигатель с ревом ожил."
                            }
                        },
                        {
                            "text": "Give the door a firm <w>press</w> to make sure it latches correctly.",
                            "target_lang_translations": {
                                "ru": "Сделай сильный <w>нажим</w> на дверь, чтобы убедиться, что она захлопнулась."
                            }
                        }
                    ],
                    "synonyms": [
                        "push"
                    ],
                    "common_phrases": [
                        "at the press of a button"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в толкании чего-либо или применении силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нажатие",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "нажим",
                                "target_lang_sense_clarification": "давление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "170d57d2-5386-3b04-8e60-80415671ba63",
                    "sense_definition": "A machine used for printing books, newspapers, or magazines.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_machine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The morning newspapers are hot off the <w>press</w> and ready for delivery.",
                            "target_lang_translations": {
                                "ru": "Утренние газеты только что из <w>печати</w> и готовы к доставке."
                            }
                        },
                        {
                            "text": "The invention of the printing <w>press</w> allowed knowledge to spread rapidly.",
                            "target_lang_translations": {
                                "ru": "Изобретение <w>печатного станка</w> позволило знаниям распространяться стремительно."
                            }
                        }
                    ],
                    "synonyms": [
                        "printing press"
                    ],
                    "common_phrases": [
                        "hot off the press",
                        "stop the presses"
                    ],
                    "target_lang_definitions": {
                        "ru": "Машина, используемая для печати книг, газет или журналов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печатный станок",
                                "target_lang_sense_clarification": "оборудование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2064434e-b29b-30ca-8d43-a111944663f3",
                    "sense_definition": "A company that publishes books, often used in the names of such companies.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She works as an editor for a large university <w>press</w>.",
                            "target_lang_translations": {
                                "ru": "Она работает редактором в крупном университетском <w>издательстве</w>."
                            }
                        },
                        {
                            "text": "The novel was published by a small independent <w>press</w> in London.",
                            "target_lang_translations": {
                                "ru": "Роман был опубликован небольшим независимым <w>издательством</w> в Лондоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "publisher",
                        "publishing house"
                    ],
                    "common_phrases": [
                        "university press"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компания, издающая книги; часто используется в названиях таких компаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "издательство",
                                "target_lang_sense_clarification": "организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88097fdf-a70b-33e5-990f-44584c557f64",
                    "sense_definition": "A device or machine used to apply pressure to something to flatten, shape, or extract juice from it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_machine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used a flower <w>press</w> to preserve the roses from her garden.",
                            "target_lang_translations": {
                                "ru": "Она использовала цветочный <w>пресс</w>, чтобы сохранить розы из своего сада."
                            }
                        },
                        {
                            "text": "Put the garlic clove in the <w>press</w> and squeeze the handles together.",
                            "target_lang_translations": {
                                "ru": "Положите зубчик чеснока в <w>пресс</w> и сожмите ручки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "garlic press",
                        "flower press",
                        "trouser press"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство или машина, используемая для оказания давления на что-либо с целью сплющивания, придания формы или извлечения сока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пресс",
                                "target_lang_sense_clarification": "устройство для давления"
                            },
                            {
                                "target_lang_word": "давилка",
                                "target_lang_sense_clarification": "разг. для чеснока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea7ab813-1a44-3a4b-8dad-48c0bc1155f6",
                    "sense_definition": "A large cupboard or closet used for storing clothes or bed linen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please put the clean towels away in the linen <w>press</w> upstairs.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, убери чистые полотенца в бельевой <w>шкаф</w> наверху."
                            }
                        },
                        {
                            "text": "She opened the <w>press</w> to find a fresh tablecloth for dinner.",
                            "target_lang_translations": {
                                "ru": "Она открыла <w>шкаф</w>, чтобы найти чистую скатерть к ужину."
                            }
                        }
                    ],
                    "synonyms": [
                        "cupboard",
                        "closet"
                    ],
                    "common_phrases": [
                        "hot press",
                        "linen press"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly Irish and Scottish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой шкаф для хранения одежды или постельного белья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шкаф",
                                "target_lang_sense_clarification": "для белья"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "3a243bcd-a13b-3683-aa51-76984f076663",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Press</w> will see you now regarding your appointment.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Пресс</w> примет вас сейчас по поводу вашей встречи."
                            }
                        },
                        {
                            "text": "The <w>Press</w> family has lived in this town for generations.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Пресс</w> живет в этом городе уже несколько поколений."
                            }
                        }

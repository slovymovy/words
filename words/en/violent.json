                },
                {
                    "sense_id": "ea8a9136-4baf-3954-b4f1-39e65b765b6f",
                    "sense_definition": "Involving extreme force or uncontrolled motion.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "natural_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>violent</w> wind ripped the heavy branch from the old tree.",

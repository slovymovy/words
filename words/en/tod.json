            "senses": [
                {
                    "sense_id": "afd906ca-b06a-3355-be14-2562d9b86763",
                    "sense_definition": "The specific time when a person died; an initialism for Time of Death.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "acronyms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The coroner examined the body to determine the <w>TOD</w>.",
                            "target_lang_translations": {
                                "ru": "Коронер осмотрел тело, чтобы определить <w>время смерти</w>."
                            }
                        },
                        {
                            "text": "Police established the <w>TOD</w> to be between midnight and 2 AM.",
                            "target_lang_translations": {
                                "ru": "Полиция установила, что <w>время смерти</w> наступило между полуночью и двумя часами ночи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "estimated TOD",
                        "determine TOD"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Точное время, когда наступила смерть человека; аббревиатура от Time of Death."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "время смерти",
                                "target_lang_sense_clarification": "медицинский/полицейский термин"
                            }
                        ]
                    }
                },
                {

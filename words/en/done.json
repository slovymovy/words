                {
                    "sense_id": "1f54c052-3d48-30de-b03c-b1e86990c817",
                    "sense_definition": "Socially acceptable or fashionable.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adj_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is simply not the <w>done</w> thing to wear jeans to a wedding.",
                            "target_lang_translations": {
                                "ru": "Надевать джинсы на свадьбу просто не <w>принято</w>."
                            }
                        },
                        {
                            "text": "Speaking rudely to your elders is just not <w>done</w>.",
                            "target_lang_translations": {
                                "ru": "Грубить старшим просто не <w>принято</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the done thing",
                        "not done"
                    ],
                    "target_lang_definitions": {
                        "ru": "Социально приемлемый или модный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принято",
                                "target_lang_sense_clarification": "общепринятый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f4422e7-1fc7-300e-8fec-2c577f4621c8",
                    "sense_definition": "Punished or charged with an offense.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adj_punished",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He got <w>done</w> for speeding on the highway last week.",
                            "target_lang_translations": {
                                "ru": "Он <w>попался</w> на превышении скорости на шоссе на прошлой неделе."
                            }
                        },
                        {
                            "text": "You can't get <w>done</w> for a crime you didn't commit.",
                            "target_lang_translations": {
                                "ru": "Тебя не могут <w>осудить</w> за преступление, которое ты не совершал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get done",
                        "done for"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наказанный или обвинённый в правонарушении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попался",
                                "target_lang_sense_clarification": "был пойман (разг.)"
                            },
                            {
                                "target_lang_word": "осуждённый",
                                "target_lang_sense_clarification": "юридически"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "40a8c489-e12c-3ad8-821d-da992a5db424",
                    "sense_definition": "Used to express that a task has been completed.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intj_completion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I just need to sign this paper and... <w>done</w>!",
                            "target_lang_translations": {
                                "ru": "Мне просто нужно подписать эту бумагу и... <w>готово</w>!"

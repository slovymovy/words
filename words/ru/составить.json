        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d525f346-bb7e-331b-ad1b-a087fb28d413",
                    "sense_definition": "Написать, разработать или подготовить какой-либо документ, текст, список или план, собирая и упорядочивая необходимые сведения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "create_plan_doc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам нужно срочно <составить> подробный план действий на следующую неделю.",
                            "target_lang_translations": {
                                "en": "We need to urgently <draw up> a detailed action plan for next week."
                            }
                        },
                        {
                            "text": "Юрист помог мне грамотно <составить> договор аренды квартиры.",
                            "target_lang_translations": {
                                "en": "The lawyer helped me correctly <draw up> the apartment lease agreement."
                            }
                        }
                    ],
                    "synonyms": [
                        "подготовить",
                        "написать",
                        "разработать",
                        "сформировать"
                    ],
                    "common_phrases": [
                        "составить план",
                        "составить список",
                        "составить резюме",
                        "составить договор",
                        "составить график"
                    ],
                    "target_lang_definitions": {
                        "en": "To write, develop, or prepare a document, text, list, or plan by collecting and organizing necessary information."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "draw up",
                                "target_lang_sense_clarification": "a document, plan, or contract"
                            },
                            {
                                "target_lang_word": "compile",
                                "target_lang_sense_clarification": "a list or collection of information"
                            },
                            {
                                "target_lang_word": "make up",
                                "target_lang_sense_clarification": "a list or plan"
                            },
                            {
                                "target_lang_word": "prepare",
                                "target_lang_sense_clarification": "a document or report"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3132e9ec-c30e-3d91-92c1-467fac95f98d",
                    "sense_definition": "Оказаться равным какому-либо числу, количеству или сумме в результате подсчёта.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "amount_to",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Общая стоимость всех покупок <составила> пять тысяч рублей.",
                            "target_lang_translations": {
                                "en": "The total cost of all purchases <amounted to> five thousand rubles."
                            }
                        },
                        {
                            "text": "Расстояние до ближайшего города <составит> около ста километров.",
                            "target_lang_translations": {
                                "en": "The distance to the nearest city <will be> about one hundred kilometers."
                            }
                        }
                    ],
                    "synonyms": [
                        "равняться",
                        "достигнуть"
                    ],
                    "common_phrases": [
                        "составить сумму",
                        "доход составил",
                        "прибыль составила",
                        "итоговая цифра"
                    ],
                    "target_lang_definitions": {
                        "en": "To equal a specific number, quantity, or sum as a result of counting or calculation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "amount to",
                                "target_lang_sense_clarification": "a total sum or quantity"
                            },
                            {

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "32d78056-d160-33ba-bf74-8ee5a9f5ebbb",
                    "sense_definition": "Skrócić coś lub oddzielić część czegoś za pomocą ostrego narzędzia, na przykład nożyczek lub noża.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_cutting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść do fryzjera, aby w końcu <w>obciąć</w> włosy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти в парикмахерскую, чтобы наконец <w>подстричь</w> волосы."
                            }
                        },
                        {
                            "text": "Mama poprosiła mnie, żebym pomógł jej <w>obciąć</w> suche gałęzie w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Мама попросила меня помочь ей <w>обрезать</w> сухие ветки в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "skrócić",
                        "przyciąć",
                        "uciąć"
                    ],
                    "antonyms": [
                        "przedłużyć",
                        "zapuścić"
                    ],
                    "common_phrases": [
                        "obciąć włosy",
                        "obciąć paznokcie",
                        "krótko obciąć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Укоротить что-либо или отделить часть чего-либо с помощью острого инструмента, например, ножниц или ножа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрезать",
                                "target_lang_sense_clarification": "укоротить, удалить часть"
                            },
                            {
                                "target_lang_word": "подстричь",
                                "target_lang_sense_clarification": "волосы, ногти"
                            },
                            {
                                "target_lang_word": "остричь",
                                "target_lang_sense_clarification": "волосы, ногти (полностью или коротко)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e2e6c2f-06f7-3a37-bb2f-7c4ecf8abeb4",
                    "sense_definition": "Zmniejszyć ilość, wartość lub zakres czegoś, na przykład pieniędzy, punktów lub przywilejów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_reduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z powodu kryzysu firma musiała drastycznie <w>obciąć</w> koszty produkcji.",
                            "target_lang_translations": {
                                "ru": "Из-за кризиса фирма была вынуждена радикально <w>урезать</w> производственные расходы."
                            }
                        },
                        {
                            "text": "Nauczyciel zdecydował <w>obciąć</w> uczniowi punkty za błędy ortograficzne.",
                            "target_lang_translations": {
                                "ru": "Учитель решил <w>снять</w> ученику баллы за орфографические ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "zredukować",
                        "zmniejszyć",
                        "obniżyć",
                        "uciąć"
                    ],
                    "antonyms": [
                        "zwiększyć",
                        "podwyższyć"
                    ],
                    "common_phrases": [
                        "obciąć koszty",
                        "obciąć budżet",
                        "obciąć pensję",
                        "obciąć fundusze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшить количество, стоимость или объем чего-либо, например, денег, баллов или привилегий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "урезать",
                                "target_lang_sense_clarification": "бюджет, расходы, зарплату"
                            },
                            {
                                "target_lang_word": "сократить",
                                "target_lang_sense_clarification": "уменьшить количество"
                            },
                            {
                                "target_lang_word": "снять",
                                "target_lang_sense_clarification": "баллы, очки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e858a5f-9abc-30b7-8ec4-2b89efdff6e1",
                    "sense_definition": "Spojrzeć na kogoś uważnie i krytycznie, często oceniając wygląd tej osoby.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_observation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopak stojący przy barze próbował dyskretnie <w>obciąć</w> ją wzrokiem.",
                            "target_lang_translations": {
                                "ru": "Парень, стоящий у бара, пытался незаметно <w>смерить</w> её взглядом."
                            }
                        },
                        {
                            "text": "Nie lubię, gdy obcy ludzie na ulicy próbują mnie tak bezczelnie <w>obciąć</w>.",
                            "target_lang_translations": {
                                "ru": "Я не люблю, когда незнакомые люди на улице пытаются меня так нагло <w>оглядеть</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmierzyć",
                        "otaksować"
                    ],
                    "common_phrases": [
                        "obciąć wzrokiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внимательно и критически посмотреть на кого-либо, часто оценивая внешний вид (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смерить взглядом",
                                "target_lang_sense_clarification": "оценить внешность"
                            },
                            {
                                "target_lang_word": "оглядеть",
                                "target_lang_sense_clarification": "посмотреть внимательно"
                            }
                        ]
                    }
                }
            ]

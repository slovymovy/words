            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec048fe0-f6f9-372a-89e0-d5ae4604ab55",
                    "sense_definition": "Szybkie przemieszczanie się za pomocą nóg, podczas którego w pewnych momentach obie nogi są oderwane od ziemi.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codzienny poranny <w>bieg</w> w parku poprawia kondycję i samopoczucie.",
                            "target_lang_translations": {
                                "ru": "Ежедневный утренний <w>бег</w> в парке улучшает физическую форму и самочувствие."
                            }
                        },
                        {
                            "text": "Chłopiec ruszył <w>biegiem</w> w stronę nadjeżdżającego autobusu.",
                            "target_lang_translations": {
                                "ru": "Мальчик бросился <w>бегом</w> к подъезжающему автобусу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szybki bieg",
                        "ruszyć biegiem",
                        "bieg truchtem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстрое перемещение с помощью ног, при котором в определенные моменты обе ноги оторваны от земли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бег",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "997aba8d-05ad-3fa2-857d-8de15bf0bb2a",
                    "sense_definition": "Konkurencja sportowa polegająca na jak najszybszym pokonaniu określonego dystansu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polski zawodnik zdobył złoty medal w <w>biegu</w> na sto metrów.",
                            "target_lang_translations": {
                                "ru": "Польский спортсмен завоевал золотую медаль в <w>беге</w> на сто метров."
                            }
                        },
                        {
                            "text": "W naszej szkole co roku organizowany jest <w>bieg</w> przełajowy dla uczniów.",
                            "target_lang_translations": {
                                "ru": "В нашей школе каждый год организуется кроссовый <w>бег</w> для учеников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bieg narciarski",
                        "bieg przez płotki",
                        "bieg maratoński",
                        "bieg sztafetowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивное соревнование, заключающееся в максимально быстром преодолении определенной дистанции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бег",
                                "target_lang_sense_clarification": "вид спорта"
                            },
                            {
                                "target_lang_word": "забег",
                                "target_lang_sense_clarification": "отдельное соревнование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47e49d62-1bac-3546-9855-9eee77588458",
                    "sense_definition": "Ustawienie mechanizmu w skrzyni biegów pojazdu, które pozwala na zmianę siły i prędkości poruszania się.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mechanics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca zredukował <w>bieg</w> przed wjazdem na strome wzniesienie.",
                            "target_lang_translations": {
                                "ru": "Водитель понизил <w>передачу</w> перед въездом на крутой подъем."
                            }
                        },
                        {
                            "text": "Musisz wrzucić wsteczny <w>bieg</w>, żeby wyjechać z garażu.",
                            "target_lang_translations": {
                                "ru": "Ты должен включить заднюю <w>передачу</w>, чтобы выехать из гаража."
                            }
                        }
                    ],
                    "synonyms": [
                        "przekładnia"
                    ],
                    "common_phrases": [
                        "skrzynia biegów",
                        "zmienić bieg",
                        "wrzucić bieg",
                        "jałowy bieg"

            "senses": [
                {
                    "sense_id": "1fab1713-d789-3894-87e5-6d8c84c2b31d",
                    "sense_definition": "Voltooid deelwoord van <w>tekenen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb gisteren het contract eindelijk <w>getekend</w>.",
                            "target_lang_translations": {
                                "en": "I finally <w>signed</w> the contract yesterday.",
                                "ru": "Я наконец <w>подписал</w> контракт вчера."
                            }
                        },
                        {
                            "text": "De kinderen hebben de hele middag mooi <w>getekend</w>.",
                            "target_lang_translations": {
                                "en": "The children <w>drew</w> beautifully all afternoon.",
                                "ru": "Дети весь день красиво <w>рисовали</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past participle of 'tekenen' (to sign or to draw).",
                        "ru": "Причастие прошедшего времени от 'tekenen' (подписывать или рисовать)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "signed",
                                "target_lang_sense_clarification": "put a signature on"
                            },
                            {
                                "target_lang_word": "drawn",
                                "target_lang_sense_clarification": "created a picture"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подписанный",
                                "target_lang_sense_clarification": "скрепленный подписью"
                            },
                            {
                                "target_lang_word": "нарисованный",
                                "target_lang_sense_clarification": "изображенный художником"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "80064d8f-3777-3184-ae3a-10487d79c214",
                    "sense_definition": "Voorzien van een handtekening ter goedkeuring of bevestiging.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>getekende</w> contract is nu rechtsgeldig.",
                            "target_lang_translations": {
                                "en": "The <w>signed</w> contract is now legally valid.",
                                "ru": "<w>Подписанный</w> контракт теперь имеет юридическую силу."
                            }
                        },
                        {
                            "text": "Zonder een <w>getekende</w> verklaring kunnen we uw aanvraag niet verwerken.",
                            "target_lang_translations": {
                                "en": "Without a <w>signed</w> statement, we cannot process your application.",
                                "ru": "Без <w>подписанного</w> заявления мы не можем обработать ваш запрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "ondertekend"
                    ],
                    "antonyms": [
                        "ongetekend"
                    ],
                    "common_phrases": [
                        "getekend contract",
                        "getekende verklaring"
                    ],
                    "target_lang_definitions": {
                        "en": "Provided with a signature for approval or confirmation.",
                        "ru": "Снабженный подписью для утверждения или подтверждения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "signed",
                                "target_lang_sense_clarification": "bearing a signature"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подписанный",
                                "target_lang_sense_clarification": "имеющий подпись"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3fd6f81-6f40-33c4-896e-04b54450b486",
                    "sense_definition": "Gemaakt door middel van tekenen; afgebeeld met lijnen en vormen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het prachtig <w>getekende</w> portret hangt in de gang.",
                            "target_lang_translations": {
                                "en": "The beautifully <w>drawn</w> portrait hangs in the hallway.",
                                "ru": "Красиво <w>нарисованный</w> портрет висит в коридоре."
                            }
                        },
                        {
                            "text": "In het boek staan veel <w>getekende</w> illustraties van dieren.",
                            "target_lang_translations": {
                                "en": "The book contains many <w>drawn</w> illustrations of animals.",
                                "ru": "В книге много <w>рисованных</w> иллюстраций животных."
                            }
                        }
                    ],
                    "synonyms": [
                        "geschetst"
                    ],
                    "common_phrases": [
                        "getekend portret",
                        "getekende versie"
                    ],
                    "target_lang_definitions": {
                        "en": "Created by means of drawing; depicted with lines and shapes.",
                        "ru": "Созданный посредством рисования; изображенный линиями и формами."
                    },

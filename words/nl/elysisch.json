{
    "word_family": [
        "Elysium"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "106cb9d4-c68b-3a2f-bb76-5dd6688c5362",
                    "sense_definition": "Idyllisch, vreedzaam en gelukzalig, vaak verwijzend naar een volmaakte staat van geluk of schoonheid.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens onze vakantie genoten we van een <w>elysisch</w> uitzicht over de groene vallei.",
                            "target_lang_translations": {
                                "ru": "Во время отпуска мы наслаждались <w>райским</w> видом на зеленую долину."
                            }
                        },
                        {
                            "text": "De rustige muziek bracht de luisteraars in <w>elysische</w> sferen.",
                            "target_lang_translations": {
                                "ru": "Спокойная музыка погрузила слушателей в <w>блаженное</w> состояние."
                            }
                        }
                    ],
                    "synonyms": [
                        "paradijselijk",
                        "hemels",
                        "verrukkelijk"
                    ],
                    "common_phrases": [
                        "in elysische sferen",
                        "een elysisch landschap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идиллический, мирный и блаженный, часто относящийся к совершенному состоянию счастья или красоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "райский",
                                "target_lang_sense_clarification": "напоминающий рай, восхитительный"
                            },
                            {
                                "target_lang_word": "блаженный",
                                "target_lang_sense_clarification": "полный счастья и покоя"
                            },
                            {
                                "target_lang_word": "идиллический",
                                "target_lang_sense_clarification": "безмятежный и прекрасный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f381fd7-b2d7-3203-a827-c9412dc0ca0e",
                    "sense_definition": "Betrekking hebbend op het <w>Elysium</w>, de verblijfplaats van de gelukzaligen in de Griekse mythologie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de Griekse mythen worden de <w>Elysische</w> Velden beschreven als een paradijs voor helden.",
                            "target_lang_translations": {
                                "ru": "В греческих мифах <w>Елисейские</w> поля описываются как рай для героев."
                            }
                        },
                        {
                            "text": "De dichter verwees naar het <w>elysisch</w> bestaan in het hiernamaals.",
                            "target_lang_translations": {
                                "ru": "Поэт ссылался на <w>елисейское</w> существование в загробной жизни."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Elysische Velden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Элизиуму, обители блаженных в греческой мифологии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "елисейский",
                                "target_lang_sense_clarification": "относящийся к Элизиуму (мифология)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
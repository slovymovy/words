    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb7a3864-9caa-3d99-a76c-7a90e389f2e4",
                    "sense_definition": "Изображение лица святого на иконе, а также внешний вид или образ в религиозном контексте.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "image_icon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Верующие с благоговением смотрели на святой <лик>, изображенный на старинной фреске.",
                            "target_lang_translations": {
                                "en": "The believers gazed with reverence at the holy <face> depicted on the ancient fresco."
                            }
                        },
                        {
                            "text": "Художник постарался передать кроткий <лик> Богородицы с максимальной точностью и любовью.",
                            "target_lang_translations": {
                                "en": "The artist tried to convey the Mother of God's gentle <countenance> with maximum precision and love."
                            }
                        }
                    ],
                    "synonyms": [
                        "икона",
                        "образ"
                    ],
                    "common_phrases": [
                        "святой лик",
                        "светлый лик",
                        "лик Христа"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "religious"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The image of a saint's face on an icon, or a person's appearance/countenance in a religious or solemn context."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "countenance",
                                "target_lang_sense_clarification": "formal or religious facial expression"
                            },
                            {
                                "target_lang_word": "face",
                                "target_lang_sense_clarification": "of a holy figure or saint"
                            },
                            {
                                "target_lang_word": "visage",
                                "target_lang_sense_clarification": "literary or solemn appearance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "752d0a10-3cba-3f0b-b7cb-ccc19c7331b8",
                    "sense_definition": "Лицо человека (обычно употребляется в высоком, поэтическом стиле или в устаревшей речи).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "face_poetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Луна ярко осветила её бледный <лик>, придав ему загадочное и печальное выражение.",
                            "target_lang_translations": {
                                "en": "The moon brightly illuminated her pale <visage>, giving it a mysterious and sad expression."
                            }
                        },
                        {
                            "text": "Он поднял свой суровый <лик> к небу, словно ожидая ответа на немой вопрос.",
                            "target_lang_translations": {
                                "en": "He raised his stern <face> to the sky, as if awaiting an answer to a silent question."
                            }
                        }
                    ],
                    "synonyms": [
                        "лицо"
                    ],
                    "common_phrases": [
                        "прекрасный лик",
                        "бледный лик",
                        "суровый лик"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "often used in literary or poetic contexts"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "bookish"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person's face, used in a high, poetic, or archaic style."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "visage",
                                "target_lang_sense_clarification": "poetic or literary"
                            },
                            {
                                "target_lang_word": "countenance",
                                "target_lang_sense_clarification": "expression or face in high style"
                            },
                            {
                                "target_lang_word": "face",
                                "target_lang_sense_clarification": "literary context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "143fbc03-0c7d-35f4-a275-a71bdc078ca1",
                    "sense_definition": "Собрание, множество или совокупность (чаще всего используется в устойчивых выражениях о святых или ангелах).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "assembly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За свои великие деяния и чудеса он был официально причислен к <лику> святых.",
                            "target_lang_translations": {
                                "en": "For his great deeds and miracles, he was officially numbered among the <host> of saints."
                            }
                        },
                        {
                            "text": "Весь ангельский <лик> торжественно воспевал славу небесам в едином порыве.",
                            "target_lang_translations": {
                                "en": "The entire angelic <choir> solemnly sang glory to the heavens in a single impulse."
                            }
                        }
                    ],
                    "synonyms": [
                        "сонм"
                    ],
                    "common_phrases": [
                        "причислить к лику",
                        "ангельский лик",
                        "лик святых"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly preserved in fixed religious phrases"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A gathering, multitude, or assembly, typically referring to angels or saints in religious texts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "host",

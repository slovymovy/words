                    ],
                    "common_phrases": [
                        "личный охранник",
                        "нанять охранника"
                    ],
                    "target_lang_definitions": {
                        "en": "A person hired to protect a specific individual from physical harm."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bodyguard",
                                "target_lang_sense_clarification": "protects a person"
                            },
                            {
                                "target_lang_word": "personal guard",
                                "target_lang_sense_clarification": "synonym for bodyguard"
                            }
                        ]
                    }
                }
            ]

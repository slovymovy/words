                {
                    "sense_id": "f9c2d2f2-b832-3160-9a21-b6706e8fccaa",
                    "sense_definition": "Dokument lub tekst zawierający uporządkowany wykaz danych, osób lub przedmiotów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "list_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę przygotować dokładne <w>zestawienie</w> wszystkich wydatków za ostatni miesiąc.",
                            "target_lang_translations": {
                                "ru": "Мне нужно подготовить подробный <w>список</w> всех расходов за последний месяц."
                            }
                        },
                        {
                            "text": "To <w>zestawienie</w> zawiera nazwiska uczestników, którzy potwierdzili swoją obecność na konferencji.",
                            "target_lang_translations": {
                                "ru": "Этот <w>список</w> содержит фамилии участников, которые подтвердили своё присутствие на конференции."
                            }
                        }
                    ],
                    "synonyms": [
                        "spis",
                        "wykaz"
                    ],
                    "common_phrases": [
                        "zestawienie kosztów",
                        "zestawienie danych",
                        "szczegółowe zestawienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ или текст, содержащий упорядоченный перечень данных, лиц или предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "список",

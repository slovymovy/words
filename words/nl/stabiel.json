{
    "word_family": [
        "stabiliteit",
        "stabiliseren",
        "stabilisatie",
        "stabilisator"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bbe0bd0d-4407-370c-8e3a-58f4bfa2795c",
                    "sense_definition": "Niet of nauwelijks aan verandering onderhevig; vast en bestendig.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De patiënt ligt in een <w>stabiele</w> toestand in het ziekenhuis.",
                            "target_lang_translations": {
                                "en": "The patient is in <w>stable</w> condition in the hospital.",
                                "ru": "Пациент находится в больнице в <w>стабильном</w> состоянии."
                            }
                        },
                        {
                            "text": "De economie is momenteel <w>stabiel</w> en groeit langzaam.",
                            "target_lang_translations": {
                                "en": "The economy is currently <w>stable</w> and growing slowly.",
                                "ru": "Экономика в настоящее время <w>стабильна</w> и медленно растет."
                            }
                        }
                    ],
                    "synonyms": [
                        "duurzaam",
                        "stevig",
                        "permanent",
                        "standvastig"
                    ],
                    "antonyms": [
                        "veranderlijk",
                        "onstabiel",
                        "instabiel"
                    ],
                    "common_phrases": [
                        "stabiele situatie",
                        "stabiele koers",
                        "stabiel blijven"
                    ],
                    "target_lang_definitions": {
                        "en": "Not or hardly subject to change; fixed and steady.",
                        "ru": "Не подверженный изменениям или почти не меняющийся; прочный и постоянный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stable",
                                "target_lang_sense_clarification": "unchanging, steady"
                            },
                            {
                                "target_lang_word": "steady",
                                "target_lang_sense_clarification": "firm, constant"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стабильный",
                                "target_lang_sense_clarification": "неизменный, постоянный"
                            },
                            {
                                "target_lang_word": "устойчивый",
                                "target_lang_sense_clarification": "твердый, надежный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08f47eb2-3875-3cb6-996d-7303c438b2a3",
                    "sense_definition": "Geestelijk evenwichtig en betrouwbaar; niet snel uit balans te brengen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is een heel <w>stabiel</w> persoon op wie je altijd kunt bouwen.",
                            "target_lang_translations": {
                                "en": "She is a very <w>stable</w> person you can always rely on.",
                                "ru": "Она очень <w>уравновешенный</w> человек, на которого всегда можно положиться."
                            }
                        },
                        {
                            "text": "Ondanks de grote druk bleef hij emotioneel <w>stabiel</w>.",
                            "target_lang_translations": {
                                "en": "Despite the great pressure, he remained emotionally <w>stable</w>.",
                                "ru": "Несмотря на сильное давление, он оставался эмоционально <w>стабильным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "evenwichtig",
                        "betrouwbaar"
                    ],
                    "antonyms": [
                        "labiel",
                        "wispelturig",
                        "onevenwichtig"
                    ],
                    "common_phrases": [
                        "emotioneel stabiel",
                        "mentaal stabiel"
                    ],
                    "target_lang_definitions": {
                        "en": "Mentally balanced and reliable; not easily thrown off balance.",
                        "ru": "Психологически уравновешенный и надежный; которого трудно вывести из равновесия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stable",
                                "target_lang_sense_clarification": "emotionally balanced"
                            },
                            {
                                "target_lang_word": "balanced",
                                "target_lang_sense_clarification": "mentally steady"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уравновешенный",
                                "target_lang_sense_clarification": "психологически спокойный"
                            },
                            {
                                "target_lang_word": "устойчивый",
                                "target_lang_sense_clarification": "эмоционально стойкий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b63cbc8-993f-3a55-886a-62a1177f2ab4",
                    "sense_definition": "In staat om na een verstoring terug te keren naar de oorspronkelijke evenwichtstoestand (vaak over systemen of stoffen).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Grafiet is bij kamertemperatuur een <w>stabielere</w> vorm van koolstof dan diamant.",
                            "target_lang_translations": {
                                "en": "Graphite is a more <w>stable</w> form of carbon than diamond at room temperature.",
                                "ru": "Графит при комнатной температуре является более <w>устойчивой</w> формой углерода, чем алмаз."
                            }
                        },
                        {
                            "text": "De nieuwe versie van de software is eindelijk <w>stabiel</w> en crasht niet meer.",
                            "target_lang_translations": {
                                "en": "The new version of the software is finally <w>stable</w> and no longer crashes.",
                                "ru": "Новая версия программного обеспечения наконец <w>стабильна</w> и больше не дает сбоев."
                            }
                        }
                    ],
                    "antonyms": [
                        "instabiel",
                        "labiel"
                    ],
                    "common_phrases": [
                        "chemisch stabiel",
                        "stabiel systeem"
                    ],
                    "target_lang_definitions": {
                        "en": "Capable of returning to the original state of equilibrium (often regarding systems or substances).",
                        "ru": "Способный возвращаться в исходное состояние равновесия (часто о системах или веществах)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stable",
                                "target_lang_sense_clarification": "chemically or technically secure"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стабильный",
                                "target_lang_sense_clarification": "о программном обеспечении или системах"
                            },
                            {
                                "target_lang_word": "устойчивый",
                                "target_lang_sense_clarification": "химически или физически"
                            }
                        ]
                    }
                }
            ]
        }
    ]

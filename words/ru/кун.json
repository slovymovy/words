    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "62381c50-3a19-3e88-8667-334bd711f549",
                    "sense_definition": "Парень, юноша, молодой человек (сленговое слово, распространенное среди любителей аниме и японской культуры).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На фестивале я познакомилась с одним очень милым <куном>.",
                            "target_lang_translations": {
                                "en": "At the festival, I met a very cute <guy>."
                            }
                        },
                        {
                            "text": "Этот <кун> помог мне найти дорогу к метро.",
                            "target_lang_translations": {
                                "en": "This <guy> helped me find the way to the subway."
                            }
                        }
                    ],
                    "synonyms": [
                        "кунчик"
                    ],
                    "antonyms": [
                        "тян"
                    ],
                    "common_phrases": [
                        "ламповый кун",
                        "мой кун"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "интернет-сленг, заимствование из японского"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Guy, young man (slang word common among fans of anime and Japanese culture)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "guy",
                                "target_lang_sense_clarification": "slang context"
                            },
                            {
                                "target_lang_word": "boy",
                                "target_lang_sense_clarification": "slang context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20057141-45b1-323e-8b5c-ea3803c4eac8",
                    "sense_definition": "Выкуп за убийство или увечье, выплачиваемый родственникам пострадавшего для предотвращения кровной мести (исторический термин у тюркских и кавказских народов).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Семья виновного обязана была выплатить огромный <кун>.",
                            "target_lang_translations": {
                                "en": "The guilty party's family was obliged to pay a huge <blood money>."
                            }
                        },
                        {
                            "text": "Старейшины рода собрались, чтобы определить размер <куна>.",
                            "target_lang_translations": {
                                "en": "The clan elders gathered to determine the amount of the <blood money>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "платить кун",
                        "размер куна"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "исторический термин"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "у тюркских и кавказских народов"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Ransom for murder or injury paid to the victim's relatives to prevent blood feuds (historical term among Turkic and Caucasian peoples)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blood money",
                                "target_lang_sense_clarification": "historical context"
                            },
                            {
                                "target_lang_word": "ransom",
                                "target_lang_sense_clarification": "historical context"
                            }
                        ]
                    }
                }
            ]
        },

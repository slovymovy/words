            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "f09cd519-e138-32c6-b56b-f41531ffa948",
                    "sense_definition": "Фонетический вариант предлога «к», употребляемый для благозвучия перед словами, начинающимися с определённых сочетаний согласных (например, «мн», «вт», «ль», «р»), а также перед формой местоимения «мне».",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Подойди <ко> мне, я хочу тебе кое-что показать.",
                            "target_lang_translations": {
                                "en": "Come <w>to</w> me, I want to show you something."
                            }
                        },
                        {
                            "text": "Мы должны закончить этот проект <ко> вторнику.",
                            "target_lang_translations": {
                                "en": "We must finish this project <w>by</w> Tuesday."
                            }
                        }
                    ],
                    "synonyms": [
                        "к"
                    ],
                    "antonyms": [
                        "от"
                    ],
                    "common_phrases": [
                        "ко мне",
                        "ко дну",
                        "ко сну",
                        "ко времени",
                        "ко всему"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "вариант предлога к"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Phonetic variant of the preposition \"к\" (to, towards, by), used for euphony before specific consonant clusters or the pronoun \"мне\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "direction or recipient"
                            },
                            {
                                "target_lang_word": "towards",
                                "target_lang_sense_clarification": "direction"
                            },
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "time limit"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

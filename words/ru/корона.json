                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crown",
                                "target_lang_sense_clarification": "championship title"
                            },
                            {
                                "target_lang_word": "title",
                                "target_lang_sense_clarification": "status of winner"
                            }
                        ]

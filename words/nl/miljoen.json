                    "sense_id": "537cb935-b7a3-3e81-87ba-f027aa3bccaa",
                    "sense_definition": "Het getal 1.000.000, dat volgt op 999.999.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het aantal gebruikers is vorige maand het <w>miljoen</w> gepasseerd.",
                            "target_lang_translations": {

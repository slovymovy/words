    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b8477429-004e-32fb-8ef8-6f37f730ca78",
                    "sense_definition": "Семья пчёл или других подобных насекомых, образующих во главе с маткой обособленную группу.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "insect_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромный пчелиный <w>рой</w> с громким жужжанием вылетел из улья.",
                            "target_lang_translations": {
                                "en": "A huge <w>swarm</w> of bees flew out of the hive with a loud buzzing."
                            }
                        },
                        {
                            "text": "Над цветущим лугом кружился <w>рой</w> мохнатых шмелей.",
                            "target_lang_translations": {
                                "en": "A <w>swarm</w> of furry bumblebees circled over the blooming meadow."
                            }
                        }
                    ],
                    "synonyms": [
                        "стая"
                    ],
                    "common_phrases": [
                        "пчелиный рой",
                        "рой комаров",
                        "гудит как рой"
                    ],
                    "target_lang_definitions": {
                        "en": "A large group of insects, especially bees, moving together."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swarm",
                                "target_lang_sense_clarification": "of insects"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0bae6e8-a26f-3686-ba0f-ccb545fcca98",
                    "sense_definition": "Значительное скопление, множество кого-либо или чего-либо, обычно находящееся в движении.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "multitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В моей голове постоянно вертелся целый <w>рой</w> странных мыслей.",
                            "target_lang_translations": {
                                "en": "A whole <w>swarm</w> of strange thoughts constantly swirled in my head."
                            }
                        },
                        {
                            "text": "Вокруг знаменитого артиста всегда вьётся <w>рой</w> восторженных поклонниц.",
                            "target_lang_translations": {
                                "en": "A <w>swarm</w> of enthusiastic fans always hovers around the famous artist."
                            }
                        }
                    ],
                    "synonyms": [
                        "вереница",
                        "ряд"
                    ],
                    "common_phrases": [
                        "рой мыслей",
                        "рой воспоминаний",
                        "рой вопросов"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A large number of people or things, usually in motion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swarm",
                                "target_lang_sense_clarification": "figurative"
                            },
                            {
                                "target_lang_word": "multitude",
                                "target_lang_sense_clarification": "large number"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9c5fc4d1-7366-342b-8da7-de62fda68eb9",
                    "sense_definition": "Форма второго лица единственного числа повелительного наклонения глагола <w>рыть</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не <w>рой</w> другому яму, так как сам можешь в неё попасть.",
                            "target_lang_translations": {
                                "en": "Do not <w>dig</w> a hole for another, as you might fall into it yourself."
                            }
                        },
                        {
                            "text": "<w>Рой</w> землю осторожно, чтобы не повредить корни молодых деревьев.",
                            "target_lang_translations": {
                                "en": "<w>Dig</w> the earth carefully so as not to damage the roots of the young trees."
                            }
                        }
                    ],
                    "common_phrases": [

{
    "word_family": [
        "grofheid",
        "vergroven",
        "grofweg"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "71a779fb-af0a-3b31-948e-71ea2bcc0dc3",
                    "sense_definition": "Niet fijn van structuur; bestaande uit grote delen, korrels of met een ruw oppervlak.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De textuur van de stenen vloer was erg <w>grof</w>.",
                            "target_lang_translations": {
                                "ru": "Текстура каменного пола была очень <w>грубой</w>."
                            }
                        },
                        {
                            "text": "Voor dit recept heb je <w>grof</w> zeezout en peper nodig.",
                            "target_lang_translations": {
                                "ru": "Для этого рецепта вам понадобятся <w>крупная</w> морская соль и перец."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruw"
                    ],
                    "antonyms": [
                        "fijn"
                    ],
                    "common_phrases": [
                        "grof zand",
                        "grof volkoren",
                        "grof zeezout"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нетонкой структуры; состоящий из крупных частей, зерен или имеющий шероховатую поверхность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крупный",
                                "target_lang_sense_clarification": "о зернистых веществах (соль, песок)"
                            },
                            {
                                "target_lang_word": "грубый",
                                "target_lang_sense_clarification": "на ощупь, о текстуре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3d8f2da-b2fd-3b12-9be7-2e2c209799f6",
                    "sense_definition": "Onbeleefd, lomp of zonder manieren; aanstootgevend in gedrag of taalgebruik.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Grof</w> taalgebruik wordt in dit restaurant niet op prijs gesteld.",
                            "target_lang_translations": {
                                "ru": "<w>Грубая</w> лексика в этом ресторане не приветствуется."
                            }
                        },
                        {
                            "text": "Zijn <w>grove</w> opmerkingen kwetsten haar gevoelens enorm.",
                            "target_lang_translations": {
                                "ru": "Его <w>грубые</w> замечания сильно задели ее чувства."
                            }
                        }
                    ],
                    "synonyms": [
                        "onbeleefd",
                        "ruw",
                        "rauw"
                    ],
                    "antonyms": [
                        "beleefd",
                        "fijn"
                    ],
                    "common_phrases": [
                        "grove taal",
                        "een grove belediging",
                        "grof in de mond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Невежливый, невоспитанный; оскорбительный в поведении или речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грубый",
                                "target_lang_sense_clarification": "невежливый, резкий"
                            },
                            {
                                "target_lang_word": "хамский",
                                "target_lang_sense_clarification": "вызывающе грубый, наглый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abef016c-d00b-3460-b17e-f95062fc7393",
                    "sense_definition": "Niet nauwkeurig of gedetailleerd; slechts bij benadering.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "precision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is slechts een <w>grove</w> schatting van de totale kosten.",
                            "target_lang_translations": {
                                "ru": "Это лишь <w>грубая</w> оценка общих расходов."
                            }
                        },
                        {
                            "text": "Ik kan je in <w>grove</w> lijnen vertellen wat er gisteren is gebeurd.",
                            "target_lang_translations": {
                                "ru": "Я могу в <w>общих</w> чертах рассказать тебе, что произошло вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruw"
                    ],
                    "antonyms": [
                        "fijn"
                    ],
                    "common_phrases": [
                        "een grove schatting",
                        "in grove lijnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неточный или недетализированный; лишь приблизительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приблизительный",
                                "target_lang_sense_clarification": "не точный"
                            },
                            {
                                "target_lang_word": "грубый",
                                "target_lang_sense_clarification": "о подсчетах или набросках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e88d4bf-b421-3f0c-a25c-d6c0d4889682",
                    "sense_definition": "Erg groot, ernstig of aanzienlijk (vaak gebruikt bij fouten, geld of middelen).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "magnitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het over het hoofd zien van dit detail was een <w>grove</w> fout.",
                            "target_lang_translations": {
                                "ru": "Упустить эту деталь было <w>грубой</w> ошибкой."
                            }
                        },
                        {
                            "text": "De aannemer verdient <w>grof</w> geld met de bouw van die nieuwe huizen.",
                            "target_lang_translations": {
                                "ru": "Подрядчик зарабатывает <w>огромные</w> деньги на строительстве этих новых домов."
                            }
                        }
                    ],
                    "antonyms": [
                        "fijn"
                    ],
                    "common_phrases": [
                        "grof geld",
                        "een grove fout",
                        "grof geschut"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень большой, серьезный или значительный (часто используется с ошибками или деньгами)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грубый",
                                "target_lang_sense_clarification": "об ошибках или нарушениях"
                            },
                            {
                                "target_lang_word": "крупный",
                                "target_lang_sense_clarification": "о деньгах или масштабе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
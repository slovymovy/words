                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень",
                                "target_lang_sense_clarification": "ранг или показатель развития"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00ecd02d-e012-310d-8bfe-69148cad7401",
                    "sense_definition": "De mate van kwaliteit, hoogte of ontwikkeling; synoniem voor niveau.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten de samenwerking naar een hoger <w>level</w> tillen.",
                            "target_lang_translations": {
                                "ru": "Мы должны вывести наше сотрудничество на новый <w>уровень</w>."
                            }
                        },
                        {
                            "text": "Zij presteert op een heel ander <w>level</w> dan de rest.",
                            "target_lang_translations": {
                                "ru": "Она выступает на совершенно ином <w>уровне</w>, чем остальные."
                            }
                        }
                    ],
                    "synonyms": [

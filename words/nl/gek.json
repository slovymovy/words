                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative form of 'gekken' (to kid/joke).",
                        "ru": "Повелительное наклонение глагола 'gekken' (шутить)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kid",
                                "target_lang_sense_clarification": "to joke playfully"
                            },
                            {
                                "target_lang_word": "joke",
                                "target_lang_sense_clarification": "to make a joke"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шути",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            },
                            {
                                "target_lang_word": "дурачься",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    ],
                    "common_phrases": [
                        "искусственная кожа",
                        "экологическая кожа"
                    ],
                    "target_lang_definitions": {
                        "en": "Artificial material imitating the properties and appearance of treated animal skin."
                    },
                    "translations": {

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bf79588a-4b3e-345c-b23f-13b7f0b5de33",
                    "sense_definition": "Wydawać oficjalne polecenie, rozkaz lub zarządzenie, oczekując jego bezwzględnego wykonania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dowódca <nakazywał> żołnierzom zachowanie absolutnej ciszy podczas nocnego patrolu.",
                            "target_lang_translations": {
                                "ru": "Командир <приказывал> солдатам соблюдать абсолютную тишину во время ночного патруля."
                            }
                        },
                        {
                            "text": "Nowa ustawa <nakazuje> wszystkim obywatelom segregowanie odpadów komunalnych.",
                            "target_lang_translations": {
                                "ru": "Новый закон <предписывает> всем гражданам сортировать бытовые отходы."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozkazywać",
                        "zarządzać",
                        "kazać",
                        "polecać"
                    ],
                    "antonyms": [
                        "zakazywać",
                        "zabraniać"
                    ],
                    "common_phrases": [
                        "nakazywać ostrożność",
                        "nakazywać milczenie",
                        "prawo nakazuje",
                        "przepisy nakazują"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать официальное распоряжение, приказ или постановление, ожидая его безусловного выполнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приказывать",
                                "target_lang_sense_clarification": "давать распоряжения"
                            },
                            {
                                "target_lang_word": "предписывать",
                                "target_lang_sense_clarification": "устанавливать правило (о законах, нормах)"
                            },
                            {
                                "target_lang_word": "велеть",
                                "target_lang_sense_clarification": "давать указание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92272a70-09b3-3794-9908-6054ffb9b0b6",
                    "sense_definition": "Sprawiać, że pewne działanie staje się konieczne lub właściwe ze względu na okoliczności, zasady moralne lub logikę.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zdrowy rozsądek <nakazuje> nam unikać ryzyka w tak niebezpiecznej sytuacji.",
                            "target_lang_translations": {
                                "ru": "Здравый смысл <велит> нам избегать риска в такой опасной ситуации."
                            }
                        },
                        {
                            "text": "Moje sumienie <nakazuje> mi przyznać się do błędu i przeprosić za wyrządzoną krzywdę.",
                            "target_lang_translations": {
                                "ru": "Моя совесть <велит> мне признать ошибку и извиниться за причиненный вред."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymagać"
                    ],
                    "common_phrases": [
                        "rozsądek nakazuje",
                        "sumienie nakazuje",
                        "sytuacja nakazuje",
                        "logika nakazuje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать какое-либо действие необходимым или правильным в силу обстоятельств, моральных принципов или логики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велеть",
                                "target_lang_sense_clarification": "внутреннее побуждение (совесть, сердце)"
                            },
                            {
                                "target_lang_word": "диктовать",
                                "target_lang_sense_clarification": "навязывать условиями или логикой"
                            },
                            {
                                "target_lang_word": "требовать",
                                "target_lang_sense_clarification": "делать необходимым"
                            }
                        ]
                    }
                }
            ]
        }
    ]

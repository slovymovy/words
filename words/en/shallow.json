    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5959c6dc-4241-38f1-a42c-6607f501b1b8",
                    "sense_definition": "Having a short distance from the top to the bottom; not deep.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_depth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children played safely in the <w>shallow</w> water near the beach.",
                            "target_lang_translations": {
                                "ru": "Дети безопасно играли в <w>мелкой</w> воде у пляжа."
                            }
                        },
                        {
                            "text": "You should fry the onions in a <w>shallow</w> pan.",
                            "target_lang_translations": {
                                "ru": "Лук следует жарить на <w>мелкой</w> сковороде."
                            }
                        }
                    ],
                    "antonyms": [
                        "deep"
                    ],
                    "common_phrases": [
                        "shallow water",
                        "shallow end",
                        "shallow breathing",
                        "shallow grave"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий небольшое расстояние от поверхности до дна; неглубокий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелкий",
                                "target_lang_sense_clarification": "о воде, посуде или предметах"
                            },
                            {
                                "target_lang_word": "неглубокий",
                                "target_lang_sense_clarification": "общая характеристика глубины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acb0bfea-8e7a-36ff-a3d3-5afc2f49d9c6",
                    "sense_definition": "Lacking serious thought, feelings, or substance; concerned only with superficial things.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_depth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to leave the <w>shallow</w> world of celebrity culture.",
                            "target_lang_translations": {
                                "ru": "Он решил покинуть <w>поверхностный</w> мир культуры знаменитостей."
                            }
                        },
                        {
                            "text": "Her analysis of the problem was <w>shallow</w> and missed the main point.",
                            "target_lang_translations": {
                                "ru": "Ее анализ проблемы был <w>поверхностным</w> и упускал главную суть."
                            }
                        }
                    ],
                    "synonyms": [
                        "superficial",
                        "skin-deep"
                    ],
                    "antonyms": [
                        "deep"
                    ],
                    "common_phrases": [
                        "shallow person",
                        "shallow argument",
                        "shallow understanding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишенный серьезности, глубины чувств или содержания; поверхностный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поверхностный",
                                "target_lang_sense_clarification": "о знаниях, чувствах или анализе"
                            },
                            {
                                "target_lang_word": "мелкий",
                                "target_lang_sense_clarification": "о личности или интересах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3eadc97f-c10f-3ef3-8ffa-8532c32bd1b8",
                    "sense_definition": "Having a low angle or slope; not steep.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "angle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot began a <w>shallow</w> climb to reach a higher altitude.",
                            "target_lang_translations": {
                                "ru": "Пилот начал <w>пологий</w> набор высоты, чтобы подняться выше."
                            }
                        },
                        {
                            "text": "The roof has a very <w>shallow</w> pitch to match the style of the house.",
                            "target_lang_translations": {
                                "ru": "У крыши очень <w>пологий</w> скат, чтобы соответствовать стилю дома."
                            }
                        }
                    ],
                    "antonyms": [
                        "deep",
                        "deep"
                    ],
                    "common_phrases": [
                        "shallow angle",
                        "shallow slope",
                        "shallow dive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий небольшой угол наклона; некрутой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пологий",
                                "target_lang_sense_clarification": "о склоне, крыше или траектории"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8c81ec69-075b-370e-94b0-b2022962acc8",
                    "sense_definition": "An area in a body of water where the water is not deep.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_depth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We could see small fish swimming in the clear <w>shallows</w>.",
                            "target_lang_translations": {
                                "ru": "Мы видели маленьких рыбок, плавающих на прозрачном <w>мелководье</w>."
                            }
                        },
                        {
                            "text": "The boat ran aground in the sandy <w>shallows</w> near the island.",
                            "target_lang_translations": {
                                "ru": "Лодка села на песчаную <w>мель</w> возле острова."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in the shallows"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок водоема с небольшой глубиной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелководье",
                                "target_lang_sense_clarification": "неглубокое место"
                            },
                            {
                                "target_lang_word": "мель",
                                "target_lang_sense_clarification": "возвышение дна, препятствие"
                            },
                            {
                                "target_lang_word": "отмель",
                                "target_lang_sense_clarification": "наносная полоса мелководья"
                            }
                        ]
                    }
                }
            ]
        },
        {

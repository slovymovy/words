    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9a3d73ec-3654-3ad1-84b3-684b40765b89",
                    "sense_definition": "An area of land that is higher than the surrounding country; a mountainous region.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The explorers had to cross a rugged <w>highland</w> to reach the river valley.",
                            "target_lang_translations": {
                                "ru": "Исследователям пришлось пересечь суровое <w>нагорье</w>, чтобы добраться до речной долины."
                            }
                        },
                        {
                            "text": "Vegetation in the <w>highland</w> is quite different from that in the plains.",
                            "target_lang_translations": {
                                "ru": "Растительность на <w>нагорье</w> сильно отличается от растительности на равнинах."
                            }
                        }
                    ],
                    "synonyms": [
                        "upland"
                    ],
                    "antonyms": [
                        "lowland"
                    ],
                    "common_phrases": [
                        "highland area",
                        "rugged highland"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок суши, расположенный выше окружающей местности; горный регион."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нагорье",
                                "target_lang_sense_clarification": "обширный участок земной поверхности с горным рельефом"
                            },
                            {
                                "target_lang_word": "горная местность",
                                "target_lang_sense_clarification": "общий термин для территории с горами"
                            },
                            {
                                "target_lang_word": "возвышенность",
                                "target_lang_sense_clarification": "место, приподнятое над окружающим ландшафтом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7723a084-e48f-3321-81ab-1d3c1b666bce",
                    "sense_definition": "Relating to or characteristic of a high-lying region, especially the Highlands of Scotland.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went to see the traditional <w>highland</w> games during our trip to Scotland.",
                            "target_lang_translations": {
                                "ru": "Мы ходили смотреть традиционные игры <w>горцев</w> во время нашей поездки в Шотландию."
                            }
                        },
                        {
                            "text": "The <w>highland</w> terrain can be difficult to navigate in winter weather.",
                            "target_lang_translations": {
                                "ru": "Ориентироваться на <w>горной</w> местности может быть трудно в зимнюю погоду."
                            }
                        }
                    ],
                    "antonyms": [
                        "lowland"
                    ],
                    "common_phrases": [
                        "highland games",
                        "highland cattle",
                        "highland dress"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к высокогорному региону или характерный для него, особенно для Шотландского нагорья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горный",
                                "target_lang_sense_clarification": "относящийся к горам или расположенный в горах"
                            },
                            {
                                "target_lang_word": "высокогорный",
                                "target_lang_sense_clarification": "расположенный на большой высоте"
                            },
                            {
                                "target_lang_word": "хайлендский",
                                "target_lang_sense_clarification": "относящийся к Хайленду (Шотландия)"
                            }
                        ]
                    }
                }
            ]
        },
        {

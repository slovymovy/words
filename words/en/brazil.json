    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "60c84949-f5c1-3c14-97fd-c5df767a3c36",
                    "sense_definition": "A large Portuguese-speaking country in South America.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "My friend is going to travel to <w>Brazil</w> for her vacation.",
                            "target_lang_translations": {
                                "ru": "Моя подруга собирается поехать в <w>Бразилию</w> в отпуск."
                            }
                        },
                        {
                            "text": "The Amazon rainforest covers a large part of <w>Brazil</w>.",
                            "target_lang_translations": {
                                "ru": "Тропические леса Амазонки покрывают большую часть <w>Бразилии</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "visit Brazil",
                        "made in Brazil",
                        "people of Brazil"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная португалоязычная страна в Южной Америке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бразилия",
                                "target_lang_sense_clarification": "страна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a71142d-68c8-3e06-b1d5-9e6ff2c9da3f",
                    "sense_definition": "A surname of Irish origin.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "We have an appointment with Dr. <w>Brazil</w> at three o'clock.",
                            "target_lang_translations": {
                                "ru": "У нас назначена встреча с доктором <w>Брэзилом</w> на три часа."
                            }
                        },
                        {
                            "text": "The famous artist, Sarah <w>Brazil</w>, will open her gallery tonight.",
                            "target_lang_translations": {
                                "ru": "Известная художница Сара <w>Брэзил</w> откроет свою галерею сегодня вечером."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "from Irish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия ирландского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Брэзил",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c0fe682-1cf4-3a13-bdf6-df11fd69b807",
                    "sense_definition": "A city in Indiana, United States.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They stopped in the town of <w>Brazil</w> while driving through Indiana.",
                            "target_lang_translations": {
                                "ru": "Они остановились в городе <w>Бразил</w>, проезжая через Индиану."
                            }
                        },
                        {
                            "text": "The local festival in <w>Brazil</w> attracts visitors from nearby counties.",
                            "target_lang_translations": {
                                "ru": "Местный фестиваль в <w>Бразиле</w> привлекает посетителей из соседних округов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в штате Индиана, США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бразил",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e1de366-3ea6-3b43-b5d6-9983bafc25a9",
                    "sense_definition": "A Brazil nut.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I picked a few <w>brazils</w> from the bowl of mixed nuts.",
                            "target_lang_translations": {
                                "ru": "Я взял несколько <w>бразильских орехов</w> из миски со смесью орехов."
                            }
                        },
                        {
                            "text": "Chocolate-covered <w>brazils</w> are a popular treat during the holidays.",
                            "target_lang_translations": {
                                "ru": "<w>Бразильские орехи</w> в шоколаде — популярное угощение во время праздников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bag of brazils",
                        "shelled brazils"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бразильский орех."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бразильский орех",
                                "target_lang_sense_clarification": "плод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "172a5fe4-586b-3ac1-9df4-ca16d347b973",
                    "sense_definition": "The hard, brown wood of the brazilwood tree, historically used for dye and violin bows.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The finest violin bows are often made from high-quality <w>brazil</w>.",
                            "target_lang_translations": {
                                "ru": "Лучшие скрипичные смычки часто делают из высококачественного <w>фернамбука</w>."
                            }
                        },
                        {
                            "text": "Merchants in the sixteenth century traded heavily in <w>brazil</w>.",
                            "target_lang_translations": {
                                "ru": "В шестнадцатом веке купцы активно торговали <w>бразильским деревом</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердая коричневая древесина цезальпинии, исторически используемая для красителей и изготовления скрипичных смычков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фернамбук",
                                "target_lang_sense_clarification": "древесина для смычков"
                            },
                            {
                                "target_lang_word": "бразильское дерево",
                                "target_lang_sense_clarification": "историческое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e19ccd1-3293-3bcb-a2a2-95ddaff4d885",
                    "sense_definition": "A red-orange dye obtained from brazilwood.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fabric was stained with <w>brazil</w> to achieve a vibrant red color.",
                            "target_lang_translations": {
                                "ru": "Ткань окрашивали <w>бразильским деревом</w>, чтобы получить ярко-красный цвет."
                            }
                        },
                        {
                            "text": "Ancient textile workers used <w>brazil</w> to color their wool.",
                            "target_lang_translations": {
                                "ru": "Древние текстильщики использовали <w>бразильское дерево</w> для окраски шерсти."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Красно-оранжевый краситель, получаемый из бразильского дерева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бразильское дерево",
                                "target_lang_sense_clarification": "источник красителя"
                            },
                            {
                                "target_lang_word": "бразилин",
                                "target_lang_sense_clarification": "пигмент"
                            }
                        ]
                    }
                }
            ]
        }
    ]

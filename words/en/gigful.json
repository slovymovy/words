    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9ad7628-9115-33ec-a7bc-c8f5d8e0be09",
                    "sense_definition": "The quantity of people or things that a gig (a light two-wheeled carriage or a small boat) can hold.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "container_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer drove a <w>gigful</w> of fresh produce to the local market.",
                            "target_lang_translations": {
                                "ru": "Фермер отвез <w>полную двуколку</w> свежих продуктов на местный рынок."
                            }
                        },
                        {
                            "text": "He transported a <w>gigful</w> of passengers across the bay in his small boat.",
                            "target_lang_translations": {
                                "ru": "Он перевез <w>полную гичку</w> пассажиров через залив на своей маленькой лодке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "Refers to historical vehicles."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество людей или груза, которое может вместить гиг (легкая двуколка или узкая шлюпка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полная двуколка",
                                "target_lang_sense_clarification": "объем груза в повозке"
                            },
                            {
                                "target_lang_word": "полная гичка",
                                "target_lang_sense_clarification": "объем груза в лодке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c6ff1ed-c5a8-3ace-87d8-d6f797a9581f",
                    "sense_definition": "The amount of material, such as songs, sufficient for a musical performance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abstract_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band rehearsed a <w>gigful</w> of new tracks before the tour started.",
                            "target_lang_translations": {
                                "ru": "Группа отрепетировала <w>программу на целый концерт</w> из новых треков перед началом тура."
                            }
                        },
                        {
                            "text": "They needed to write a few more tunes to have a complete <w>gigful</w> for the night.",
                            "target_lang_translations": {
                                "ru": "Им нужно было написать еще несколько мелодий, чтобы набралась <w>полная программа</w> на вечер."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объем материала (например, песен), достаточный для одного музыкального выступления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "программа на концерт",
                                "target_lang_sense_clarification": "музыкальный материал"
                            },
                            {
                                "target_lang_word": "репертуар на выступление",
                                "target_lang_sense_clarification": "набор композиций"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            },
                            {
                                "target_lang_word": "Шарик",
                                "target_lang_sense_clarification": "культурный эквивалент стереотипной клички"
                            },
                            {
                                "target_lang_word": "Бобик",
                                "target_lang_sense_clarification": "культурный эквивалент стереотипной клички"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e36327b-bd1f-34a9-9fcd-6e1c55a404b1",
                    "sense_definition": "A British brand of automobiles that is no longer in production.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "brands",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "My grandfather carefully restored a vintage <w>Rover</w> in his garage.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка бережно отреставрировал старинный <w>Ровер</w> в своем гараже."
                            }
                        },
                        {
                            "text": "The company ceased manufacturing the <w>Rover</w> several years ago.",
                            "target_lang_translations": {
                                "ru": "Компания прекратила производство автомобилей <w>Ровер</w> несколько лет назад."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical brand"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Британская марка автомобилей, которая больше не производится."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ровер",
                                "target_lang_sense_clarification": "марка автомобиля"
                            }
                        ]
                    }
                }
            ]
        }
    ]

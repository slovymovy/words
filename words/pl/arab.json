                    "sense_id": "a2dd087b-0d12-3823-9bd7-3e927a9d4579",
                    "sense_definition": "Osoba należąca do ludów semickich zamieszkujących Półwysep Arabski i północną Afrykę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ethnicity",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "Mój nowy sąsiad to <w>Arab</w>, który pochodzi z Egiptu.",
                            "target_lang_translations": {
                                "ru": "Мой новый сосед — <w>араб</w>, родом из Египта."
                            }
                        },
                        {
                            "text": "Wielu <w>Arabów</w> mówi płynnie po francusku.",
                            "target_lang_translations": {
                                "ru": "Многие <w>арабы</w> свободно говорят по-французски."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kraje Arabów",
                        "kultura Arabów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель семитских народов, населяющих Аравийский полуостров и Северную Африку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "араб",
                                "target_lang_sense_clarification": "национальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d168c1a3-b38b-35fc-a02e-4942b822ba18",
                    "sense_definition": "Koń rasy gorącokrwistej wywodzącej się z Półwyspu Arabskiego, uznawany za jedną z najstarszych i najpiękniejszych ras.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej stadninie hoduje się piękne <w>araby</w>.",
                            "target_lang_translations": {
                                "ru": "На этом конном заводе разводят красивых <w>арабских скакунов</w>."
                            }
                        },
                        {
                            "text": "Najdroższe na licytacji są zawsze <w>araby</w> czystej krwi.",
                            "target_lang_translations": {
                                "ru": "Самыми дорогими на аукционе всегда являются чистокровные <w>арабы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "koń arabski",
                        "koń czystej krwi arabskiej"
                    ],
                    "common_phrases": [
                        "stado arabów",
                        "hodowla arabów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лошадь чистокровной арабской породы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арабский скакун",
                                "target_lang_sense_clarification": "порода лошади"
                            },
                            {
                                "target_lang_word": "араб",
                                "target_lang_sense_clarification": "лошадь (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19c95c63-c631-35b2-a487-d7baa37c6034",
                    "sense_definition": "Policjant.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "police_slang",
                    "name_type": "no",
                    "examples": [
                        {

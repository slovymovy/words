            "senses": [
                {
                    "sense_id": "759070d0-50f5-38c3-94d1-4b5b033793f1",
                    "sense_definition": "Of or relating to the spine or the spinal cord.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor ordered an X-ray to check for any <w>spinal</w> injuries after the fall.",
                            "target_lang_translations": {
                                "ru": "Врач назначил рентген, чтобы проверить наличие <w>травм позвоночника</w> после падения."
                            }
                        },
                        {
                            "text": "Nerves in the <w>spinal</w> cord transmit messages between the brain and the body.",
                            "target_lang_translations": {
                                "ru": "Нервы в <w>спинном</w> мозге передают сообщения между головным мозгом и телом."
                            }
                        }
                    ],
                    "synonyms": [
                        "vertebral"
                    ],
                    "common_phrases": [
                        "spinal cord",
                        "spinal column",
                        "spinal injury",
                        "spinal fluid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к позвоночнику или спинному мозгу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спинно́й",
                                "target_lang_sense_clarification": "относящийся к спинному мозгу"
                            },
                            {
                                "target_lang_word": "позвоно́чный",
                                "target_lang_sense_clarification": "относящийся к позвоночнику"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "66af9541-fad4-37cc-a96c-ef5fe0e3c2ec",
                    "sense_definition": "An injection of anesthetic into the spine to numb the lower body.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to have a <w>spinal</w> instead of general anesthesia for the surgery.",
                            "target_lang_translations": {
                                "ru": "Она решила выбрать <w>спинальную анестезию</w> вместо общего наркоза для операции."
                            }
                        },
                        {
                            "text": "The patient remained awake during the operation because he was given a <w>spinal</w>.",
                            "target_lang_translations": {
                                "ru": "Пациент оставался в сознании во время операции, так как ему сделали <w>спинальную анестезию</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have a spinal",
                        "give a spinal"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short for spinal anesthesia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Инъекция анестетика в позвоночный канал; спинальная анестезия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спина́льная анестези́я",
                                "target_lang_sense_clarification": "вид обезболивания"
                            }
                        ]
                    }
                }
            ]
        }
    ]

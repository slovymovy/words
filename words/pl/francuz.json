                {
                    "sense_id": "38afe81c-2aee-32bb-8034-1b51f63ffa88",
                    "sense_definition": "Potoczna nazwa klucza nastawnego, używanego do odkręcania śrub o różnych rozmiarach.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podaj mi <w>francuza</w>, bo muszę dokręcić tę rurę pod zlewem.",

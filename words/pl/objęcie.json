                    ],
                    "common_phrases": [
                        "rzucić się w objęcia",
                        "wyrwać się z objęć",
                        "czułe objęcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жест, заключающийся в обхватывании кого-либо руками, выражающий близость, нежность или приветствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объятие",
                                "target_lang_sense_clarification": "жест"
                            }
                        ]

                    }
                },
                {
                    "sense_id": "5b74f0f2-e59b-3943-ab36-567ee885a5b8",
                    "sense_definition": "Kradzież lub przywłaszczenie cudzej własności.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "crime",
                    "name_type": "no",
                    "examples": [
                        {

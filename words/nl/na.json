                {
                    "sense_id": "dc9b221c-4eeb-3f0e-b0ff-008eb11d678c",
                    "sense_definition": "Volgend op een bepaald tijdstip of een gebeurtenis; later dan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gaan <w>na</w> het avondeten een stukje wandelen.",
                            "target_lang_translations": {
                                "ru": "Мы пойдем немного прогуляться <w>после</w> ужина."
                            }
                        },
                        {
                            "text": "<w>Na</w> regen komt zonneschijn.",
                            "target_lang_translations": {
                                "ru": "<w>После</w> дождя выглядывает солнце."
                            }
                        },
                        {
                            "text": "Hij kwam pas <w>na</w> middernacht thuis van het feest.",
                            "target_lang_translations": {
                                "ru": "Он вернулся домой с вечеринки только <w>после</w> полуночи."
                            }
                        }
                    ],
                    "synonyms": [
                        "later"
                    ],
                    "antonyms": [
                        "voor"
                    ],
                    "common_phrases": [
                        "na verloop van tijd",
                        "keer op keer",
                        "dag na dag"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следующий за определенным моментом или событием; позже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "после",
                                "target_lang_sense_clarification": "предлог времени"

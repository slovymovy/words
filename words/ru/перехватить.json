    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e45207c6-d41e-3a19-bc14-13705aa21ac3",
                    "sense_definition": "Задержать, остановить или поймать кого-либо или что-либо во время движения, не дав дойти до цели.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "interception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Защитник сумел <перехватить> мяч и начать контратаку.",
                            "target_lang_translations": {
                                "en": "The defender managed to <w>intercept</w> the ball and start a counterattack."
                            }
                        },
                        {
                            "text": "Полиция планировала <перехватить> преступников на выезде из города.",
                            "target_lang_translations": {
                                "en": "The police planned to <w>intercept</w> the criminals at the city exit."
                            }
                        },
                        {
                            "text": "Я попытался <перехватить> его по дороге на работу, чтобы поговорить.",
                            "target_lang_translations": {
                                "en": "I tried to <w>catch</w> him on the way to work to have a talk."
                            }
                        }
                    ],
                    "synonyms": [
                        "задержать",
                        "остановить",
                        "поймать"
                    ],
                    "antonyms": [
                        "пропустить",
                        "упустить"
                    ],
                    "common_phrases": [
                        "перехватить мяч",
                        "перехватить письмо",
                        "перехватить по дороге"
                    ],
                    "target_lang_definitions": {
                        "en": "To detain, stop, or catch someone or something while in motion, preventing them from reaching their destination."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "intercept",
                                "target_lang_sense_clarification": "stop something in transit"
                            },
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "meet or stop someone on their way"
                            },
                            {
                                "target_lang_word": "cut off",
                                "target_lang_sense_clarification": "block someone's path"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf1735f1-edec-327e-8c9f-651efce2d2b2",
                    "sense_definition": "Быстро, наскоро съесть что-нибудь; утолить голод между основными приемами пищи.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "eating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <перехватить> бутерброд перед выходом из дома.",
                            "target_lang_translations": {
                                "en": "We decided to <w>grab</w> a sandwich before leaving the house."
                            }
                        },
                        {
                            "text": "У меня не было времени на обед, удалось только <перехватить> пирожок.",
                            "target_lang_translations": {
                                "en": "I didn't have time for lunch, I only managed to <w>grab</w> a pie."
                            }
                        }
                    ],
                    "synonyms": [
                        "перекусить",
                        "подкрепиться"
                    ],
                    "common_phrases": [
                        "успеть перехватить",
                        "перехватить еды",
                        "перехватить бутерброд"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To eat something quickly or hastily; to satisfy hunger between main meals."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grab a bite",
                                "target_lang_sense_clarification": "eat quickly"
                            },
                            {
                                "target_lang_word": "have a snack",
                                "target_lang_sense_clarification": "eat lightly"
                            },
                            {
                                "target_lang_word": "grab",
                                "target_lang_sense_clarification": "take food quickly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ab81dc3-88d8-3a56-9d58-ca9dd02f2048",
                    "sense_definition": "Взять деньги в долг на короткое время.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты не мог бы <перехватить> мне тысячу рублей до зарплаты?",
                            "target_lang_translations": {
                                "en": "Could I <w>borrow</w> a thousand rubles from you until payday?"
                            }
                        },
                        {
                            "text": "Студентам часто приходится <перехватить> денег у родителей.",
                            "target_lang_translations": {
                                "en": "Students often have to <w>borrow</w> money from their parents."
                            }
                        }
                    ],
                    "synonyms": [
                        "одолжить",
                        "занять"
                    ],
                    "common_phrases": [
                        "перехватить денег",
                        "перехватить до зарплаты",
                        "перехватить у друга"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To borrow money for a short time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "borrow",
                                "target_lang_sense_clarification": "money temporarily"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c0bf941-a7b0-35a9-8bc4-4b066efaebf7",
                    "sense_definition": "Уловить, заметить или услышать что-либо быстротечное (взгляд, слово, сигнал, сообщение).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "interception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ей удалось <перехватить> удивлённый взгляд начальника.",
                            "target_lang_translations": {
                                "en": "She managed to <w>catch</w> the boss's surprised glance."
                            }
                        },
                        {
                            "text": "Разведка смогла <перехватить> секретное сообщение противника.",
                            "target_lang_translations": {
                                "en": "Intelligence was able to <w>intercept</w> the enemy's secret message."
                            }
                        }
                    ],
                    "synonyms": [
                        "уловить",
                        "заметить"
                    ],
                    "antonyms": [
                        "пропустить"
                    ],
                    "common_phrases": [
                        "перехватить взгляд",
                        "перехватить сигнал",
                        "перехватить сообщение"
                    ],
                    "target_lang_definitions": {
                        "en": "To catch, notice, or hear something fleeting (a glance, word, signal, or message)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "notice a look or glance"
                            },
                            {
                                "target_lang_word": "intercept",
                                "target_lang_sense_clarification": "a message or signal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c37a3016-3ee0-3880-8313-7fa3a7ac100e",
                    "sense_definition": "Опередив других, взять что-либо в свои руки; проявить инициативу или занять лидерство.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "interception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой сотрудник сумел <перехватить> инициативу в переговорах.",
                            "target_lang_translations": {
                                "en": "The young employee managed to <w>seize</w> the initiative in the negotiations."
                            }
                        },
                        {
                            "text": "Нам нужно срочно <перехватить> управление проектом, пока не стало поздно.",
                            "target_lang_translations": {
                                "en": "We need to urgently <w>take over</w> project management before it's too late."
                            }
                        }
                    ],
                    "synonyms": [
                        "захватить",
                        "завладеть"
                    ],
                    "antonyms": [
                        "уступить",
                        "отдать"
                    ],
                    "common_phrases": [
                        "перехватить инициативу",
                        "перехватить управление",
                        "перехватить лидерство"
                    ],
                    "target_lang_definitions": {
                        "en": "To take control or initiative ahead of others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seize",
                                "target_lang_sense_clarification": "initiative or opportunity"
                            },
                            {
                                "target_lang_word": "take over",
                                "target_lang_sense_clarification": "control or management"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44644ea6-4fe2-38d0-a76e-1c24ae2c362f",
                    "sense_definition": "Временно остановить или затруднить дыхание (от сильных эмоций, холода или спазма).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physiological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От ледяной воды у пловца <перехватило> дыхание.",
                            "target_lang_translations": {
                                "en": "The icy water made the swimmer's breath <w>catch</w>."
                            }
                        },
                        {
                            "text": "Когда она увидела высоту, у неё от страха <перехватило> горло.",
                            "target_lang_translations": {
                                "en": "When she saw the height, fear made her throat <w>tighten</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "дыхание перехватило",
                        "горло перехватило",
                        "дух перехватило"
                    ],
                    "target_lang_definitions": {
                        "en": "To temporarily stop or impede breathing due to strong emotion, cold, or spasm."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "of breath or voice"
                            },
                            {
                                "target_lang_word": "take away",
                                "target_lang_sense_clarification": "of breath"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "123343fb-9fe5-3b50-9372-5fd071110045",
                    "sense_definition": "Взять что-либо иначе, переместить руки в другое место предмета для удобства.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Грузчик решил <перехватить> коробку поудобнее, чтобы она не упала.",
                            "target_lang_translations": {
                                "en": "The loader decided to <w>adjust his grip on</w> the box so it wouldn't fall."
                            }
                        },
                        {
                            "text": "Водитель был вынужден <перехватить> руль на крутом повороте.",
                            "target_lang_translations": {
                                "en": "The driver was forced to <w>regrip</w> the steering wheel on a sharp turn."
                            }
                        }
                    ],
                    "common_phrases": [
                        "перехватить поудобнее",
                        "перехватить руль",
                        "перехватить другой рукой"
                    ],
                    "target_lang_definitions": {
                        "en": "To change one's hold on an object for better convenience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regrip",
                                "target_lang_sense_clarification": "change hold"
                            },
                            {
                                "target_lang_word": "adjust one's grip",
                                "target_lang_sense_clarification": "move hands"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0c5fc74-2f67-39b2-999b-01017b4a8e67",
                    "sense_definition": "Туго обвязать, стянуть или перетянуть что-либо поперек.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старик решил <перехватить> пальто простым веревочным поясом.",
                            "target_lang_translations": {
                                "en": "The old man decided to <w>tie</w> his coat with a simple rope belt."
                            }
                        },
                        {
                            "text": "Девушка любила <перехватить> волосы лентой.",
                            "target_lang_translations": {
                                "en": "The girl liked to <w>tie back</w> her hair with a ribbon."
                            }
                        }
                    ],
                    "synonyms": [
                        "перевязать",
                        "стянуть",
                        "подпоясать"
                    ],
                    "common_phrases": [
                        "перехватить ремнём",
                        "перехватить верёвкой",
                        "перехватить лентой"
                    ],
                    "target_lang_definitions": {
                        "en": "To tie tightly or bind something across the middle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tie",
                                "target_lang_sense_clarification": "with a belt or ribbon"
                            },
                            {
                                "target_lang_word": "cinch",
                                "target_lang_sense_clarification": "tighten around"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
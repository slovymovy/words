{
    "word_family": [
        "mystic",
        "mystical",
        "mysticism",
        "mystify",
        "mystification",
        "mystery",
        "mysterious"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "fccbf510-ccf6-3c41-a399-33ad373d9966",
                    "sense_definition": "In a way that relates to spiritual or supernatural powers beyond ordinary human understanding.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spiritual_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient ruins felt <w>mystically</w> charged with energy during the solstice.",
                            "target_lang_translations": {
                                "ru": "Древние руины казались <w>мистически</w> заряженными энергией во время солнцестояния."
                            }
                        },
                        {
                            "text": "She felt <w>mystically</w> connected to the natural world around her.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала себя <w>мистически</w> связанной с окружающим миром природы."
                            }
                        }
                    ],
                    "synonyms": [
                        "spiritually",
                        "supernaturally",
                        "transcendentally"
                    ],
                    "antonyms": [
                        "rationally",
                        "scientifically",
                        "physically"
                    ],
                    "common_phrases": [
                        "mystically connected",
                        "mystically inclined",
                        "mystically united"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, связанным с духовными или сверхъестественными силами, находящимися за пределами обычного человеческого понимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мистически",
                                "target_lang_sense_clarification": "сверхъестественным образом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "866bd5fa-7e5a-3c0f-8eef-2bf0f6266772",
                    "sense_definition": "In a way that is secret, hidden, or difficult to understand; with a concealed meaning.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cryptic_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old sage spoke <w>mystically</w> about the future, confusing the villagers.",
                            "target_lang_translations": {
                                "ru": "Старый мудрец <w>таинственно</w> говорил о будущем, сбивая с толку жителей деревни."
                            }
                        },
                        {
                            "text": "He wrote <w>mystically</w> in his journals to hide his true thoughts from others.",
                            "target_lang_translations": {
                                "ru": "Он писал <w>загадочно</w> в своих дневниках, чтобы скрыть свои истинные мысли от других."
                            }
                        }
                    ],
                    "synonyms": [
                        "cryptically",
                        "enigmatically",
                        "obscurely"
                    ],
                    "antonyms": [
                        "clearly",
                        "plainly",
                        "openly"
                    ],
                    "common_phrases": [
                        "spoke mystically",
                        "wrote mystically"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, который является тайным, скрытым или трудным для понимания; со скрытым смыслом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таинственно",
                                "target_lang_sense_clarification": "скрытно, с секретом"
                            },
                            {
                                "target_lang_word": "загадочно",
                                "target_lang_sense_clarification": "непонятно, иносказательно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
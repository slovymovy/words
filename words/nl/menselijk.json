    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7d3fd8d5-8a90-3f1f-a3aa-e96ccabdd6ad",
                    "sense_definition": "Betrekking hebbend op de mens als soort of wezen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_biological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>menselijk</w> lichaam bestaat voor een groot deel uit water.",
                            "target_lang_translations": {
                                "en": "The <w>human</w> body consists largely of water.",
                                "ru": "<w>Человеческое</w> тело в значительной степени состоит из воды."
                            }
                        },
                        {
                            "text": "Wetenschappers doen onderzoek naar de evolutie van het <w>menselijk</w> ras.",
                            "target_lang_translations": {
                                "en": "Scientists are researching the evolution of the <w>human</w> race.",
                                "ru": "Ученые исследуют эволюцию <w>человеческой</w> расы."
                            }
                        }
                    ],
                    "antonyms": [
                        "natuurlijk"
                    ],
                    "common_phrases": [
                        "menselijk lichaam",
                        "menselijk contact",
                        "menselijk verstand"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to humans as a species or being",
                        "ru": "Относящийся к человеку как к виду или существу"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "human",
                                "target_lang_sense_clarification": "relating to the species"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "человеческий",
                                "target_lang_sense_clarification": "относящийся к людям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bfdb97c-361a-338c-9efb-6174af08a1be",
                    "sense_definition": "Kenmerkend voor de aard van mensen, inclusief hun zwakheden en emoties",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vergissen is <w>menselijk</w>, dus maak je geen zorgen over die kleine fout.",
                            "target_lang_translations": {
                                "en": "To err is <w>human</w>, so don't worry about that small mistake.",
                                "ru": "Ошибаться — <w>человечно</w>, поэтому не беспокойся об этой маленькой ошибке."
                            }
                        },
                        {
                            "text": "Het is heel <w>menselijk</w> om af en toe jaloers of boos te zijn.",
                            "target_lang_translations": {
                                "en": "It is very <w>human</w> to be jealous or angry every now and then.",
                                "ru": "Это очень <w>человечно</w> — время от времени ревновать или злиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "menseigen"
                    ],
                    "common_phrases": [
                        "vergissen is menselijk",
                        "iets menselijks"
                    ],
                    "target_lang_definitions": {
                        "en": "Characteristic of human nature, including weaknesses and emotions",
                        "ru": "Характерный для природы людей, включая их слабости и эмоции"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "human",
                                "target_lang_sense_clarification": "characteristic of people"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "человеческий",
                                "target_lang_sense_clarification": "свойственный людям"
                            },
                            {
                                "target_lang_word": "житейский",
                                "target_lang_sense_clarification": "обыденный, свойственный жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebaf4cfa-2dac-3b15-873c-42a6b1f381d1",
                    "sense_definition": "Getuigend van medeleven, vriendelijkheid en beschaving",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_ethical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gevangenen werden op een <w>menselijke</w> manier behandeld door de bewakers.",
                            "target_lang_translations": {
                                "en": "The prisoners were treated in a <w>humane</w> way by the guards.",
                                "ru": "Охранники обращались с заключенными <w>гуманным</w> образом."
                            }
                        },
                        {
                            "text": "We moeten zoeken naar een <w>menselijke</w> oplossing voor dit sociale probleem.",
                            "target_lang_translations": {
                                "en": "We must look for a <w>humane</w> solution to this social problem.",
                                "ru": "Мы должны искать <w>гуманное</w> решение этой социальной проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "humaan"
                    ],
                    "antonyms": [
                        "onmenselijk",
                        "inhumaan"
                    ],
                    "common_phrases": [
                        "menselijk gezicht",
                        "menselijke maat"
                    ],
                    "target_lang_definitions": {
                        "en": "Showing compassion, kindness, and civilization",
                        "ru": "Свидетельствующий о сострадании, доброте и цивилизованности"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "humane",
                                "target_lang_sense_clarification": "compassionate"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гуманный",
                                "target_lang_sense_clarification": "человеколюбивый"
                            },
                            {
                                "target_lang_word": "человечный",
                                "target_lang_sense_clarification": "проявляющий доброту и отзывчивость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35289d02-7736-3074-a357-ae17910d4d9d",
                    "sense_definition": "Veroorzaakt of gemaakt door mensen",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_causal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het ongeluk op het spoor was het gevolg van een <w>menselijke</w> fout.",
                            "target_lang_translations": {
                                "en": "The accident on the railway was the result of a <w>human</w> error.",
                                "ru": "Авария на железной дороге произошла в результате <w>человеческой</w> ошибки."
                            }
                        },
                        {
                            "text": "De opwarming van de aarde wordt deels toegeschreven aan <w>menselijke</w> invloeden.",
                            "target_lang_translations": {
                                "en": "Global warming is partly attributed to <w>human</w> influences.",
                                "ru": "Глобальное потепление отчасти приписывается <w>человеческому</w> влиянию."
                            }
                        }
                    ],
                    "synonyms": [
                        "antropogeen"
                    ],
                    "antonyms": [
                        "natuurlijk"
                    ],
                    "common_phrases": [
                        "menselijke fout",
                        "menselijke invloed"
                    ],
                    "target_lang_definitions": {
                        "en": "Caused or made by humans",
                        "ru": "Вызванный или созданный людьми"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "human",
                                "target_lang_sense_clarification": "caused by people"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "человеческий",
                                "target_lang_sense_clarification": "созданный людьми"
                            },
                            {
                                "target_lang_word": "антропогенный",
                                "target_lang_sense_clarification": "научный термин, вызванный деятельностью человека"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7c4b1de7-dd2c-350e-9b94-c68ec4736b0f",
                    "sense_definition": "Опуститься, свеситься вниз под собственной тяжестью или из-за потери натяжения",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После сильного ливня ветки сирени тяжело <w>обвисли</w> под тяжестью воды.",
                            "target_lang_translations": {
                                "en": "After the heavy downpour, the lilac branches <w>drooped</w> heavily under the weight of the water."
                            }
                        },
                        {
                            "text": "Его плечи устало <w>обвисли</w> после долгого и трудного рабочего дня.",
                            "target_lang_translations": {
                                "en": "His shoulders <w>slumped</w> wearily after a long and difficult workday."
                            }
                        }
                    ],
                    "synonyms": [
                        "повиснуть",
                        "свеситься",
                        "поникнуть",
                        "опуститься"
                    ],
                    "antonyms": [
                        "натянуться",
                        "подняться"
                    ],
                    "common_phrases": [
                        "плечи обвисли",
                        "ветки обвисли",
                        "усы обвисли",
                        "одежда обвисла"
                    ],
                    "target_lang_definitions": {
                        "en": "To drop or hang down under its own weight or due to a loss of tension"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "droop",
                                "target_lang_sense_clarification": "bend or hang downwards limply"
                            },
                            {
                                "target_lang_word": "hang down",
                                "target_lang_sense_clarification": "be suspended loosely"
                            },
                            {
                                "target_lang_word": "slump",
                                "target_lang_sense_clarification": "fall or lean heavily, often referring to shoulders or posture"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43b25c93-b4d4-389c-acf6-4ae7199847d3",
                    "sense_definition": "Стать дряблым, потерять упругость и опуститься (о коже, мышцах или частях тела)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "С возрастом кожа на шее потеряла эластичность и заметно <w>обвисла</w>.",
                            "target_lang_translations": {
                                "en": "With age, the skin on the neck lost its elasticity and noticeably <w>sagged</w>."
                            }
                        },
                        {
                            "text": "Его щёки <w>обвисли</w>, что придавало лицу унылое выражение.",
                            "target_lang_translations": {
                                "en": "His cheeks <w>sagged</w>, giving his face a gloomy expression."
                            }
                        }
                    ],
                    "synonyms": [
                        "одрябнуть",
                        "отвиснуть"
                    ],
                    "antonyms": [
                        "подтянуться"
                    ],
                    "common_phrases": [
                        "щёки обвисли",
                        "живот обвис",
                        "кожа обвисла",
                        "грудь обвисла"
                    ],
                    "target_lang_definitions": {
                        "en": "To become flabby, lose elasticity, and hang loose (referring to skin, muscles, or body parts)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sag",
                                "target_lang_sense_clarification": "sink downwards due to lack of strength or elasticity"
                            },
                            {
                                "target_lang_word": "become flabby",
                                "target_lang_sense_clarification": "lose firmness and tone"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "313cc9f4-6e3c-3059-a2be-2984bcb244ec",
                    "sense_definition": "The state of being secluded from the presence, sight, or knowledge of others.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need my <w>privacy</w>, so please stay out of my room.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>личное пространство</w>, поэтому, пожалуйста, не заходи в мою комнату."
                            }
                        },
                        {
                            "text": "The tall hedges around the garden provide total <w>privacy</w> from the neighbors.",
                            "target_lang_translations": {
                                "ru": "Высокая живая изгородь вокруг сада обеспечивает полное <w>уединение</w> от соседей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "respect privacy",
                        "invasion of privacy",
                        "need some privacy",
                        "total privacy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние уединения, изолированности от присутствия или взглядов других людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уединение",
                                "target_lang_sense_clarification": "состояние одиночества, изоляции от других"
                            },
                            {
                                "target_lang_word": "личное пространство",
                                "target_lang_sense_clarification": "физическая или психологическая зона комфорта"
                            },
                            {
                                "target_lang_word": "приватность",
                                "target_lang_sense_clarification": "обстановка, скрытая от посторонних глаз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c986a82-281d-351f-937c-f6d05000e999",
                    "sense_definition": "Freedom from damaging publicity, public scrutiny, surveillance, and disclosure of personal information, usually by a government or a private organization.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should always read the <w>privacy</w> policy before signing up for a new service.",
                            "target_lang_translations": {
                                "ru": "Вам следует всегда читать политику <w>конфиденциальности</w> перед регистрацией в новом сервисе."
                            }
                        },
                        {
                            "text": "Internet <w>privacy</w> has become a major concern for many users today.",
                            "target_lang_translations": {
                                "ru": "<w>Приватность</w> в Интернете стала сегодня серьезной проблемой для многих пользователей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "privacy policy",
                        "data privacy",
                        "online privacy",
                        "right to privacy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свобода от публичности, слежки и разглашения личной информации (обычно в контексте данных и прав)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конфиденциальность",
                                "target_lang_sense_clarification": "секретность данных, неразглашение информации"
                            },
                            {
                                "target_lang_word": "неприкосновенность частной жизни",
                                "target_lang_sense_clarification": "юридическое право на защиту личной сферы"
                            },
                            {
                                "target_lang_word": "приватность",
                                "target_lang_sense_clarification": "защита личных данных в интернете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86afa88f-e1a6-3d3b-8025-116fc5582700",
                    "sense_definition": "Secrecy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They were married in strict <w>privacy</w> to avoid the attention of the press.",
                            "target_lang_translations": {
                                "ru": "Они поженились в строгой <w>тайне</w>, чтобы избежать внимания прессы."
                            }
                        },
                        {
                            "text": "He preferred to grieve in the <w>privacy</w> of his own home.",
                            "target_lang_translations": {
                                "ru": "Он предпочел предаваться скорби в <w>уединении</w> своего собственного дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "secret"
                    ],
                    "common_phrases": [
                        "in privacy",
                        "strict privacy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Секретность, сохранение чего-либо в тайне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тайна",
                                "target_lang_sense_clarification": "скрытность действий"
                            },
                            {
                                "target_lang_word": "секретность",
                                "target_lang_sense_clarification": "отсутствие огласки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92c6d585-71d4-3b89-84b7-ae7f8f0eba71",
                    "sense_definition": "A relationship between parties seen as being a result of their mutual interest or participation in a given transaction, contract etc.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer argued that there was no <w>privacy</w> of contract between the claimant and the manufacturer.",
                            "target_lang_translations": {
                                "ru": "Адвокат утверждал, что между истцом и производителем отсутствовала <w>договорная связь</w>."
                            }
                        },
                        {
                            "text": "This legal rule applies only where there is <w>privacy</w> of estate.",
                            "target_lang_translations": {
                                "ru": "Это правовое правило применяется только там, где существует <w>имущественная связь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "privity"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly replaced by the term privity in modern legal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридическая связь между сторонами, возникающая из их взаимного интереса или участия в договоре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "договорная связь",
                                "target_lang_sense_clarification": "юридический термин (privity of contract)"
                            },
                            {
                                "target_lang_word": "имущественная связь",
                                "target_lang_sense_clarification": "юридический термин (privity of estate)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

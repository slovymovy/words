    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7e55ba79-67c7-37d9-ba4e-61d6f9d5a493",
                    "sense_definition": "Вступать в брак с женщиной, брать кого-либо в жёны (о мужчине).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "marriage_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>женился</w> на Кате исключительно по любви.",
                            "target_lang_translations": {
                                "en": "He <w>married</w> Katya purely for love."
                            }
                        },
                        {
                            "text": "Мой старший брат планирует <w>жениться</w> в следующем году.",
                            "target_lang_translations": {
                                "en": "My older brother plans to <w>get married</w> next year."
                            }
                        }
                    ],
                    "synonyms": [
                        "брать в жёны",
                        "взять в жёны"
                    ],
                    "antonyms": [
                        "развестись"
                    ],
                    "common_phrases": [
                        "жениться по любви",
                        "жениться на девушке",
                        "собираться жениться",
                        "обещать жениться"
                    ],
                    "target_lang_definitions": {
                        "en": "To marry a woman; to take someone as a wife (referring to a man)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "marry",
                                "target_lang_sense_clarification": "to take as a wife"
                            },
                            {
                                "target_lang_word": "get married",
                                "target_lang_sense_clarification": "to enter into marriage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10d2214b-cdf7-3443-a807-d6a565d42ae8",
                    "sense_definition": "Вступать в брак друг с другом, становиться мужем и женой (о паре).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "marriage_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Они встречались три года и наконец <w>женились</w>.",
                            "target_lang_translations": {
                                "en": "They dated for three years and finally <w>got married</w>."
                            }
                        },
                        {
                            "text": "Мои лучшие друзья <w>женятся</w> в эту субботу.",
                            "target_lang_translations": {
                                "en": "My best friends are <w>getting married</w> this Saturday."
                            }
                        }
                    ],
                    "synonyms": [
                        "пожениться"
                    ],
                    "antonyms": [
                        "развестись"
                    ],
                    "common_phrases": [
                        "они женятся",
                        "решили жениться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "о паре"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To marry each other; to become husband and wife (referring to a couple)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get married",
                                "target_lang_sense_clarification": "to become husband and wife"
                            },
                            {
                                "target_lang_word": "marry",
                                "target_lang_sense_clarification": "to enter into marriage with one another"
                            }
                        ]
                    }
                }
            ]
        }

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ноль",
                                "target_lang_sense_clarification": "число"
                            },
                            {
                                "target_lang_word": "нуль",
                                "target_lang_sense_clarification": "число (часто в математике)"
                            }

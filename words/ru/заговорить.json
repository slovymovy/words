                            },
                            {
                                "target_lang_word": "bewitch",
                                "target_lang_sense_clarification": "to cast a spell upon someone"
                            },
                            {
                                "target_lang_word": "cast a spell on",
                                "target_lang_sense_clarification": "general magic action"
                            }
                        ]
                    }
                }

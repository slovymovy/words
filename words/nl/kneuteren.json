            "senses": [
                {
                    "sense_id": "3a6d5ef0-37de-39cc-a203-e75fec999c71",
                    "sense_definition": "Gezellig en ontspannen met elkaar praten of bezig zijn, vaak in een huiselijke sfeer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op zondagochtend zitten we graag samen aan de ontbijttafel te <w>kneuteren</w>.",
                            "target_lang_translations": {
                                "en": "On Sunday mornings we like to sit together at the breakfast table and <w>chat cozily</w>.",
                                "ru": "По воскресным утрам мы любим сидеть вместе за завтраком и <w>уютно болтать</w>."
                            }
                        },
                        {
                            "text": "De vriendinnen waren de hele avond gezellig aan het <w>kneuteren</w> met een kop thee.",
                            "target_lang_translations": {
                                "en": "The friends were <w>nattering</w> cozily all evening with a cup of tea.",
                                "ru": "Подруги весь вечер <w>ворковали</w> за чашкой чая."
                            }
                        }
                    ],
                    "synonyms": [
                        "keuvelen",
                        "babbelen",
                        "kletsen"
                    ],
                    "common_phrases": [
                        "lekker zitten kneuteren",
                        "wat af kneuteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To talk or occupy oneself in a cozy and relaxed manner, often in a domestic atmosphere.",
                        "ru": "Уютно и расслабленно общаться или заниматься мелкими делами, часто в домашней обстановке."
                    },
                    "translations": {

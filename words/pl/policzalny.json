                        "niepoliczalny"
                    ],
                    "common_phrases": [
                        "rzeczownik policzalny"
                    ],
                    "target_lang_definitions": {
                        "ru": "В грамматике: существительное, обозначающее предметы, которые можно пересчитать поштучно, и имеющее форму множественного числа."
                    },

{
    "word_family": [
        "dzbanek",
        "dzbanuszek",
        "dzbanowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "040aff2a-74e0-3c72-bb6c-75e3f87c3fe6",
                    "sense_definition": "Naczynie użytkowe lub dekoracyjne, zwykle pękate i zwężone u góry, posiadające ucho i dzióbek, służące do przechowywania i nalewania płynów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vessel_container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na stole w kuchni stał duży, gliniany <w>dzban</w> z zimną wodą.",
                            "target_lang_translations": {
                                "ru": "На кухонном столе стоял большой глиняный <w>кувшин</w> с холодной водой."
                            }
                        },
                        {
                            "text": "Artysta namalował piękny <w>dzban</w> pełen polnych kwiatów.",
                            "target_lang_translations": {
                                "ru": "Художник нарисовал красивый <w>кувшин</w>, полный полевых цветов."
                            }
                        }
                    ],
                    "synonyms": [
                        "kruża"
                    ],
                    "common_phrases": [
                        "gliniany dzban",
                        "rozbić dzban",
                        "ucho od dzbana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хозяйственный или декоративный сосуд, обычно пузатый и сужающийся кверху, с ручкой и носиком, предназначенный для хранения и разливания жидкостей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кувшин",
                                "target_lang_sense_clarification": "сосуд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0301415-711b-3bce-a44e-6785a636eb73",
                    "sense_definition": "Ilość płynu lub innej substancji, która mieści się w naczyniu zwanym dzbanem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vessel_container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spragnieni goście wypili cały <w>dzban</w> soku pomarańczowego.",
                            "target_lang_translations": {
                                "ru": "Измученные жаждой гости выпили целый <w>кувшин</w> апельсинового сока."
                            }
                        },
                        {
                            "text": "Na przyjęcie przygotowaliśmy wielki <w>dzban</w> mrożonej herbaty.",
                            "target_lang_translations": {
                                "ru": "Для вечеринки мы приготовили огромный <w>кувшин</w> чая со льдом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzban wina",
                        "wypić dzban"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество жидкости или другого вещества, которое помещается в кувшин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кувшин",
                                "target_lang_sense_clarification": "мера объема"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f751fc88-c8fd-3d49-9662-462abbfc2c2c",
                    "sense_definition": "Osoba niemądra, naiwna lub zachowująca się w sposób głupi i irytujący.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten kierowca to kompletny <w>dzban</w>, bo zablokował wyjazd z parkingu.",
                            "target_lang_translations": {
                                "ru": "Этот водитель — полный <w>болван</w>, так как заблокировал выезд с парковки."
                            }
                        },
                        {
                            "text": "Nie bądź takim <w>dzbanem</w> i oddaj mi wreszcie te pieniądze.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>тупицей</w> и верни мне наконец эти деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "cymbał"
                    ],
                    "common_phrases": [
                        "ale dzban",
                        "ciężki dzban",
                        "być dzbanem"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "popularne określenie w języku młodzieżowym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Глупый, наивный человек или тот, кто ведет себя раздражающе глупо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болван",
                                "target_lang_sense_clarification": "глупый человек"
                            },
                            {
                                "target_lang_word": "тупица",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
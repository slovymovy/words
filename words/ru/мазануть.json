    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "66bf6982-314f-3aae-9b4e-64d4f337eff2",
                    "sense_definition": "Совершить однократное действие по значению глагола <w>мазать</w>; быстро провести чем-то красящим, жирным или грязным по поверхности, оставив след.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_smear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Художник решил <мазануть> широкой кистью по самому центру холста.",
                            "target_lang_translations": {
                                "en": "The artist decided to <swipe> a broad brush right across the center of the canvas."
                            }
                        },
                        {
                            "text": "Он случайно <мазанул> грязной рукой по белой стене.",
                            "target_lang_translations": {
                                "en": "He accidentally <smeared> his dirty hand across the white wall."
                            }
                        }
                    ],
                    "synonyms": [
                        "мазнуть",
                        "провести"
                    ],
                    "antonyms": [
                        "стереть"
                    ],
                    "common_phrases": [
                        "мазануть кистью",
                        "мазануть краской",
                        "мазануть по лицу"
                    ],
                    "target_lang_definitions": {
                        "en": "To quickly run something coloring, greasy, or dirty over a surface, leaving a mark; to dab or smear once."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dab",
                                "target_lang_sense_clarification": "apply with a quick, light touch"
                            },
                            {
                                "target_lang_word": "smear",
                                "target_lang_sense_clarification": "spread a substance messily"
                            },
                            {
                                "target_lang_word": "swipe",
                                "target_lang_sense_clarification": "move quickly across a surface"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86c5d68a-d031-3b70-bf24-b91a03f45a8f",
                    "sense_definition": "Совершить однократное действие по значению глагола <w>мазать</w>; не попасть в цель при стрельбе, броске или ударе.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_miss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный охотник поторопился и обидно <мазанул> мимо утки.",
                            "target_lang_translations": {
                                "en": "The experienced hunter hurried and annoyingly <missed> the duck."
                            }
                        },
                        {
                            "text": "В решающий момент нападающий <мазанул> по пустым воротам.",
                            "target_lang_translations": {
                                "en": "At the decisive moment, the forward <missed> the empty goal."
                            }
                        }
                    ],
                    "synonyms": [
                        "промахнуться",
                        "промазать"
                    ],
                    "antonyms": [
                        "попасть"
                    ],
                    "common_phrases": [
                        "мазануть мимо",
                        "мазануть по воротам"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in speech regarding sports or games"
                        }
                    ],
                    "target_lang_definitions": {

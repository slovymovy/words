            "pos": "noun",
            "senses": [
                {
                    "sense_id": "feee6172-fe35-39a2-b7db-85772e4b38bb",
                    "sense_definition": "Het onzichtbare mengsel van gassen dat de aarde omringt en dat mensen en dieren inademen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_gas",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zet het raam even open voor wat frisse <w>lucht</w>.",
                            "target_lang_translations": {
                                "en": "I'm opening the window for some fresh <w>air</w>.",
                                "ru": "Я ненадолго открою окно, чтобы впустить свежий <w>воздух</w>."
                            }
                        },
                        {
                            "text": "Zonder <w>lucht</w> kunnen mensen en dieren niet leven.",
                            "target_lang_translations": {
                                "en": "Without <w>air</w>, people and animals cannot live.",
                                "ru": "Без <w>воздуха</w> люди и животные не могут жить."
                            }
                        }
                    ],
                    "synonyms": [
                        "atmosfeer"
                    ],
                    "common_phrases": [
                        "frisse lucht",
                        "in de open lucht",
                        "lucht scheppen"
                    ],
                    "target_lang_definitions": {
                        "en": "The invisible mixture of gases that surrounds the earth and that people and animals breathe.",
                        "ru": "Невидимая смесь газов, окружающая Землю, которой дышат люди и животные."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "air",
                                "target_lang_sense_clarification": "gas mixture"
                            },
                            {
                                "target_lang_word": "atmosphere",
                                "target_lang_sense_clarification": "surrounding gases"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "воздух",
                                "target_lang_sense_clarification": "смесь газов"
                            },
                            {
                                "target_lang_word": "атмосфера",
                                "target_lang_sense_clarification": "газовая оболочка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8bf9f35-4ff3-34ee-8e89-1929f50200e4",
                    "sense_definition": "De ruimte boven de aarde die je ziet als je naar boven kijkt, waar wolken, de zon en sterren staan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_sky",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk eens naar die mooie vogels hoog in de <w>lucht</w>.",
                            "target_lang_translations": {
                                "en": "Look at those beautiful birds high in the <w>sky</w>.",
                                "ru": "Посмотри на этих красивых птиц высоко в <w>небе</w>."
                            }
                        },
                        {
                            "text": "De <w>lucht</w> is vandaag strakblauw en er is geen wolkje te zien.",
                            "target_lang_translations": {
                                "en": "The <w>sky</w> is clear blue today and there isn't a cloud to be seen.",
                                "ru": "<w>Небо</w> сегодня ярко-синее, и не видно ни облачка."
                            }
                        }
                    ],
                    "synonyms": [
                        "hemel",
                        "uitspansel"
                    ],
                    "common_phrases": [
                        "in de lucht",
                        "de lucht in",
                        "uit de lucht vallen"
                    ],
                    "target_lang_definitions": {
                        "en": "The space above the earth that you see when you look up, where clouds, the sun, and stars are located.",
                        "ru": "Пространство над землей, которое видно при взгляде вверх, где находятся облака, солнце и звезды."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sky",
                                "target_lang_sense_clarification": "space above earth"
                            },
                            {
                                "target_lang_word": "air",
                                "target_lang_sense_clarification": "open space above ground"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "небо",
                                "target_lang_sense_clarification": "небесный свод"
                            },
                            {
                                "target_lang_word": "воздух",
                                "target_lang_sense_clarification": "пространство над землей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1250a9b4-d48d-3484-9e32-10e751e507a3",
                    "sense_definition": "Een geur die je kunt ruiken, soms gebruikt om een onaangename geur aan te duiden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_smell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er hangt een vreemde <w>lucht</w> in de keuken sinds gisteren.",
                            "target_lang_translations": {
                                "en": "There has been a strange <w>smell</w> in the kitchen since yesterday.",
                                "ru": "Со вчерашнего дня в кухне стоит странный <w>запах</w>."
                            }
                        },
                        {
                            "text": "Die <w>lucht</w> van gebakken vis vind ik niet zo lekker.",
                            "target_lang_translations": {
                                "en": "I don't like that <w>smell</w> of fried fish very much.",
                                "ru": "Мне не очень нравится этот <w>запах</w> жареной рыбы."
                            }
                        }
                    ],
                    "synonyms": [
                        "geur",
                        "reuk"
                    ],
                    "common_phrases": [
                        "een vreemde lucht",
                        "een vieze lucht",
                        "een luchtje aan zitten"
                    ],
                    "target_lang_definitions": {
                        "en": "A scent that you can perceive, sometimes used to indicate an unpleasant smell.",
                        "ru": "Запах, который можно почувствовать; иногда используется для обозначения неприятного запаха."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smell",
                                "target_lang_sense_clarification": "sensory perception"
                            },
                            {

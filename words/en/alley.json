                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Alley</w> will be joining the meeting shortly.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Элли</w> скоро присоединится к собранию."
                            }
                        },
                        {
                            "text": "I have never met anyone with the last name <w>Alley</w> before.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не встречал никого с фамилией <w>Элли</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }

                    "sense_id": "dd1d16b7-1f41-3884-938a-d5799b17fd5f",
                    "sense_definition": "Een mannelijke voornaam, oorspronkelijk een verkorte vorm van Jakob, Johannes of Jozef.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn buurman heet <w>Joop</w> en hij is erg aardig.",
                            "target_lang_translations": {
                                "en": "My neighbor is named <w>Joop</w> and he is very nice.",
                                "ru": "Моего соседа зовут <w>Йоп</w>, и он очень приятный человек."
                            }
                        },
                        {
                            "text": "Vroeger was <w>Joop</w> een van de populairste namen in Nederland.",
                            "target_lang_translations": {
                                "en": "In the past, <w>Joop</w> was one of the most popular names in the Netherlands.",
                                "ru": "Раньше <w>Йоп</w> было одним из самых популярных имен в Нидерландах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Ome Joop"
                    ],
                    "target_lang_definitions": {
                        "en": "A male first name, originally a shortened form of Jakob, Johannes, or Jozef.",
                        "ru": "Мужское имя, изначально сокращенная форма имен Якоб, Йоханнес или Йозеф."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Joop",
                                "target_lang_sense_clarification": "male given name"
                            }
                        ],
                        "ru": [

                {
                    "sense_id": "e9b1eebe-3df4-306a-a4a7-c0746474384c",
                    "sense_definition": "Vriendelijk en prettig in de omgang met anderen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "friendly_pleasant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Floris is een <w>aardige</w> jongen die iedereen helpt.",
                            "target_lang_translations": {

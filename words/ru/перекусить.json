                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "commonly used in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To eat a small amount of food, usually quickly, to satisfy hunger between main meals."
                    },
                    "translations": {

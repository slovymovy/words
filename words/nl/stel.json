            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8e85084e-d7d2-35ab-8592-f142e083cd66",
                    "sense_definition": "Twee mensen die bij elkaar horen, meestal partners in een liefdesrelatie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "noun_couple",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk wat een leuk <w>stel</w> daar hand in hand loopt.",
                            "target_lang_translations": {
                                "en": "Look what a nice <w>couple</w> is walking there hand in hand.",
                                "ru": "Посмотри, какая милая <w>пара</w> идет там, держась за руки."
                            }
                        },
                        {
                            "text": "Mijn buren zijn al vijftig jaar een gelukkig <w>stel</w>.",
                            "target_lang_translations": {
                                "en": "My neighbors have been a happy <w>couple</w> for fifty years.",
                                "ru": "Мои соседи — счастливая <w>пара</w> уже пятьдесят лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "paar",
                        "set"
                    ],
                    "common_phrases": [
                        "een jong stel",
                        "een mooi stel",
                        "het gelukkige stel"
                    ],
                    "target_lang_definitions": {
                        "en": "Two people who belong together, usually partners in a romantic relationship.",
                        "ru": "Двое людей, которые принадлежат друг другу, обычно партнеры в любовных отношениях."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "couple",
                                "target_lang_sense_clarification": "romantic partners"
                            },
                            {
                                "target_lang_word": "pair",
                                "target_lang_sense_clarification": "two people together"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пара",
                                "target_lang_sense_clarification": "двое людей в отношениях"
                            },
                            {
                                "target_lang_word": "чета",
                                "target_lang_sense_clarification": "супружеская пара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97b908a7-71e5-37d1-85e9-d11e7152d8ff",
                    "sense_definition": "Een onbepaald aantal personen of dingen bij elkaar; een groepje.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er stond een <w>stel</w> fietsen slordig tegen de muur geparkeerd.",
                            "target_lang_translations": {
                                "en": "There was a <w>bunch</w> of bicycles parked untidily against the wall.",
                                "ru": "У стены была небрежно припаркована <w>куча</w> велосипедов."
                            }
                        },
                        {
                            "text": "Met mijn 43 jaar was ik duidelijk de oudste van het <w>stel</w>.",
                            "target_lang_translations": {
                                "en": "At 43, I was clearly the oldest of the <w>bunch</w>.",
                                "ru": "В свои 43 года я был явно самым старшим из всей <w>компании</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "groep",
                        "paar"
                    ],
                    "common_phrases": [
                        "een heel stel",
                        "een stel mensen",
                        "een stelletje ongeregeld"
                    ],
                    "target_lang_definitions": {
                        "en": "An indefinite number of people or things together; a small group.",
                        "ru": "Неопределенное количество людей или вещей вместе; небольшая группа."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bunch",
                                "target_lang_sense_clarification": "group of things or people"
                            },
                            {
                                "target_lang_word": "couple",
                                "target_lang_sense_clarification": "a small number of"
                            },
                            {
                                "target_lang_word": "lot",
                                "target_lang_sense_clarification": "a group or collection"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "куча",
                                "target_lang_sense_clarification": "скопление предметов или людей (разг.)"
                            },
                            {
                                "target_lang_word": "компания",
                                "target_lang_sense_clarification": "группа людей"
                            },
                            {
                                "target_lang_word": "группа",
                                "target_lang_sense_clarification": "совокупность людей или предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4c8aeaa-ca58-3506-82a9-8a4f00ec8eb1",
                    "sense_definition": "Een verzameling bij elkaar horende voorwerpen die samen een geheel vormen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_set",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vader kocht een nieuw <w>stel</w> banden voor de auto.",
                            "target_lang_translations": {
                                "en": "Father bought a new <w>set</w> of tires for the car.",
                                "ru": "Папа купил новый <w>комплект</w> шин для машины."
                            }
                        },
                        {
                            "text": "We hebben een mooi <w>stel</w> wijnglazen gekregen voor onze bruiloft.",
                            "target_lang_translations": {
                                "en": "We received a beautiful <w>set</w> of wine glasses for our wedding.",
                                "ru": "Мы получили красивый <w>набор</w> бокалов для вина на нашу свадьбу."
                            }
                        }
                    ],
                    "synonyms": [
                        "set"
                    ],
                    "common_phrases": [
                        "een stel hersens",
                        "een stel tanden"
                    ],
                    "target_lang_definitions": {
                        "en": "A collection of matching objects that form a whole.",
                        "ru": "Набор предметов, составляющих единое целое."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "matching collection"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "набор",
                                "target_lang_sense_clarification": "совокупность предметов"
                            },
                            {
                                "target_lang_word": "комплект",
                                "target_lang_sense_clarification": "полный набор чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e99488f5-83fb-3eb3-bb61-13e165533425",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>stellen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>stel</w> voor dat we morgen naar het strand gaan.",
                            "target_lang_translations": {
                                "en": "I <w>propose</w> that we go to the beach tomorrow.",
                                "ru": "Я <w>предлагаю</w>, чтобы мы завтра пошли на пляж."
                            }
                        },
                        {
                            "text": "Ik <w>stel</w> hoge eisen aan de kwaliteit van mijn werk.",
                            "target_lang_translations": {
                                "en": "I <w>set</w> high standards for the quality of my work.",
                                "ru": "Я <w>предъявляю</w> высокие требования к качеству своей работы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik stel voor",
                        "ik stel vast"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "first-person singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'stellen'.",
                        "ru": "Первое лицо единственного числа настоящего времени от глагола 'stellen'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "propose",
                                "target_lang_sense_clarification": "to suggest (in 'voorstellen')"
                            },
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "to establish or place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "предлагаю",
                                "target_lang_sense_clarification": "в значении 'voorstellen'"
                            },
                            {
                                "target_lang_word": "ставлю",
                                "target_lang_sense_clarification": "устанавливать или помещать"
                            },
                            {
                                "target_lang_word": "предъявляю",
                                "target_lang_sense_clarification": "в значении 'eisen stellen'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2f5aeaf-e6a9-3ec5-a1cd-e4c4faf6ba42",
                    "sense_definition": "Gebiedende wijs van <w>stellen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Stel</w> niet zulke moeilijke vragen als ik moe ben.",
                            "target_lang_translations": {
                                "en": "Don't <w>ask</w> such difficult questions when I am tired.",
                                "ru": "Не <w>задавай</w> таких сложных вопросов, когда я устал."
                            }
                        },
                        {
                            "text": "<w>Stel</w> de klok een uur vooruit als de zomertijd begint.",
                            "target_lang_translations": {
                                "en": "<w>Set</w> the clock one hour forward when daylight saving time starts.",
                                "ru": "<w>Переведи</w> часы на час вперед, когда начинается летнее время."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stel je voor",
                        "stel je niet aan"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative mood of 'stellen'.",
                        "ru": "Повелительное наклонение глагола 'stellen'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to pose a question"
                            },
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "to adjust (a clock/device)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "задавай",
                                "target_lang_sense_clarification": "в значении 'vragen stellen'"
                            },
                            {
                                "target_lang_word": "переведи",
                                "target_lang_sense_clarification": "настраивать (часы)"
                            },
                            {
                                "target_lang_word": "представь",
                                "target_lang_sense_clarification": "воображать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69badea9-ee29-3959-84ec-97bc78950206",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>stellen</w> (bij inversie).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waarom <w>stel</w> je die vraag eigenlijk aan mij?",
                            "target_lang_translations": {
                                "en": "Why do you actually <w>ask</w> me that question?",
                                "ru": "Почему ты вообще <w>задаешь</w> мне этот вопрос?"
                            }
                        },
                        {
                            "text": "<w>Stel</w> je prijs op een kopje koffie of thee?",
                            "target_lang_translations": {
                                "en": "Do you <w>appreciate</w> a cup of coffee or tea?",
                                "ru": "Ты <w>ценишь</w> чашечку кофе или чая?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inversion"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Second person singular present tense of 'stellen' (used in inversion).",
                        "ru": "Второе лицо единственного числа настоящего времени от глагола 'stellen' (используется при инверсии)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to pose a question"
                            },
                            {
                                "target_lang_word": "appreciate",
                                "target_lang_sense_clarification": "to value (in 'prijs stellen op')"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "задаешь",
                                "target_lang_sense_clarification": "спрашивать"
                            },
                            {
                                "target_lang_word": "ценишь",
                                "target_lang_sense_clarification": "в выражении 'prijs stellen op'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "9e4e02ae-0593-3b40-b0fb-830c8d02df14",
                    "sense_definition": "Gebruikt om een hypothetische situatie of veronderstelling in te leiden; neem aan dat.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "conj_suppose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Stel</w> dat we de trein missen, hoe komen we dan thuis?",
                            "target_lang_translations": {
                                "en": "<w>Suppose</w> that we miss the train, how do we get home then?",
                                "ru": "<w>Допустим</w>, мы опоздаем на поезд, как мы тогда доберемся домой?"
                            }
                        },
                        {
                            "text": "<w>Stel</w> je wint de loterij, wat zou je dan als eerste kopen?",
                            "target_lang_translations": {
                                "en": "<w>Imagine</w> you win the lottery, what would you buy first?",
                                "ru": "<w>Представь</w>, что ты выиграл в лотерею, что бы ты купил в первую очередь?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "stel dat",
                        "stel je eens voor"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to introduce a hypothetical situation or assumption; assume that.",
                        "ru": "Используется для введения гипотетической ситуации или предположения; допустим, что."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suppose",
                                "target_lang_sense_clarification": "introducing a hypothesis"
                            },
                            {
                                "target_lang_word": "imagine",
                                "target_lang_sense_clarification": "visualizing a scenario"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "допустим",
                                "target_lang_sense_clarification": "предположение"
                            },
                            {
                                "target_lang_word": "представь",
                                "target_lang_sense_clarification": "воображаемая ситуация"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7b35ff91-f15a-3e69-9f49-c1040cb09b67",
                    "sense_definition": "Переместиться резко вниз под действием силы тяжести, оказавшись на земле или другой поверхности.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Книга соскользнула с края стола и <w>упала</w> на пол.",
                            "target_lang_translations": {
                                "en": "The book slipped off the edge of the table and <w>fell</w> to the floor."
                            }
                        },
                        {
                            "text": "Мальчик быстро бежал, споткнулся и <w>упал</w>.",
                            "target_lang_translations": {
                                "en": "The boy was running fast, tripped, and <w>fell down</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "пасть"
                    ],
                    "antonyms": [
                        "подняться"
                    ],
                    "common_phrases": [
                        "упасть на землю",
                        "упасть со стула",
                        "упасть в обморок",
                        "упасть на колени"
                    ],
                    "target_lang_definitions": {
                        "en": "To move sharply downwards under the force of gravity, ending up on the ground or another surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "general movement downwards"
                            },
                            {
                                "target_lang_word": "fall down",
                                "target_lang_sense_clarification": "to collapse to the ground"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "to fall suddenly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e04379da-df51-3660-bdef-138705dd8e5a",
                    "sense_definition": "Уменьшиться в уровне, силе, качестве или значении (о температуре, ценах, настроении и т. п.).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К вечеру температура воздуха сильно <w>упала</w>.",
                            "target_lang_translations": {
                                "en": "By evening, the air temperature had <w>dropped</w> significantly."
                            }
                        },
                        {
                            "text": "Цены на старые модели телефонов заметно <w>упали</w>.",
                            "target_lang_translations": {
                                "en": "Prices for old phone models have noticeably <w>fallen</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "вырасти",
                        "подняться"
                    ],
                    "common_phrases": [
                        "цены упали",
                        "настроение упало",
                        "зрение упало",
                        "давление упало"
                    ],
                    "target_lang_definitions": {
                        "en": "To decrease in level, strength, quality, or value (regarding temperature, prices, mood, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "decrease in value or amount"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "decrease sharply"
                            },
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "become lower"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f5be47e-6ef0-349d-803e-a11945c1b9d1",
                    "sense_definition": "Внезапно перестать работать из-за сбоя (о сервере, сайте или компьютерной программе).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш рабочий сервер внезапно <w>упал</w> посреди дня.",
                            "target_lang_translations": {
                                "en": "Our work server suddenly <w>crashed</w> in the middle of the day."
                            }
                        },
                        {
                            "text": "Сайт магазина <w>упал</w> из-за наплыва покупателей.",
                            "target_lang_translations": {
                                "en": "The store's website <w>went down</w> due to the influx of shoppers."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сервер упал",
                        "сайт упал",
                        "база упала"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "computer slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To suddenly stop working due to a failure (regarding a server, website, or computer program)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crash",
                                "target_lang_sense_clarification": "computer system failure"
                            },
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "become unavailable (of a system)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
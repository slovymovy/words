    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8084f996-fb50-36c5-a5ad-f37c0d31d1e5",
                    "sense_definition": "To carry, lead, or transport someone or something from one place to another.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>take</w> these dirty plates to the kitchen.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>отнеси</w> эти грязные тарелки на кухню."
                            }
                        },
                        {
                            "text": "My father used to <w>take</w> me to the park every Sunday.",
                            "target_lang_translations": {
                                "ru": "Мой отец <w>водил</w> меня в парк каждое воскресенье."
                            }
                        }
                    ],
                    "synonyms": [
                        "carry",
                        "bring",
                        "transport",
                        "lead",
                        "convey"
                    ],
                    "antonyms": [
                        "bring"
                    ],
                    "common_phrases": [
                        "take home",
                        "take away",
                        "take with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать, вести или перевозить кого-либо или что-либо из одного места в другое."

{
    "word_family": [
        "silence",
        "silently",
        "silencer"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3a08eedd-a886-3854-a1e7-2bfd6837c578",
                    "sense_definition": "Free from sound or noise; absolutely still; perfectly quiet.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound_absence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The library was completely <w>silent</w> while the students studied.",
                            "target_lang_translations": {
                                "ru": "В библиотеке было совершенно <w>тихо</w>, пока студенты занимались."
                            }
                        },
                        {
                            "text": "How <w>silent</w> is this town at night!",
                            "target_lang_translations": {
                                "ru": "Как <w>тих</w> этот город ночью!"
                            }
                        }
                    ],
                    "synonyms": [
                        "quiet",
                        "noiseless",
                        "soundless",
                        "still"
                    ],
                    "antonyms": [
                        "loud",
                        "noisy"
                    ],
                    "common_phrases": [
                        "silent night",
                        "silent prayer",
                        "remain silent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободный от звука или шума; абсолютно спокойный; совершенно тихий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тихий",
                                "target_lang_sense_clarification": "общий термин для отсутствия шума"
                            },
                            {
                                "target_lang_word": "безмолвный",
                                "target_lang_sense_clarification": "полное отсутствие звуков, часто поэтическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6535091f-8df5-3ddf-bdbe-3155efcd5db6",
                    "sense_definition": "Not speaking; unwilling to talk or temporarily refraining from speech.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "speech_absence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He remained <w>silent</w> during the meeting because he was nervous.",
                            "target_lang_translations": {
                                "ru": "Он <w>молчал</w> во время собрания, потому что нервничал."
                            }
                        },
                        {
                            "text": "She was <w>silent</w> for a moment before answering the difficult question.",
                            "target_lang_translations": {
                                "ru": "Она минуту <w>молчала</w>, прежде чем ответить на сложный вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "speechless",
                        "taciturn",
                        "mute",
                        "quiet"
                    ],
                    "common_phrases": [
                        "keep silent",
                        "silent majority",
                        "stay silent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не говорящий; не желающий говорить или временно воздерживающийся от речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молчаливый",
                                "target_lang_sense_clarification": "склонный молчать или находящийся в состоянии молчания"
                            },
                            {
                                "target_lang_word": "безмолвный",
                                "target_lang_sense_clarification": "не издающий ни звука"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c08fee45-defd-3891-b090-f0f392d90f5e",
                    "sense_definition": "With the sound turned off; usually referring to a mobile phone setting.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "device_mode",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My phone was on <w>silent</w> so I missed your call.",
                            "target_lang_translations": {
                                "ru": "Мой телефон был на <w>беззвучном</w> режиме, поэтому я пропустил твой звонок."
                            }
                        },
                        {
                            "text": "Please put your devices on <w>silent</w> before the movie starts.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поставьте свои устройства на <w>беззвучный</w> режим перед началом фильма."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on silent",
                        "silent mode"
                    ],
                    "target_lang_definitions": {
                        "ru": "С выключенным звуком; обычно относится к настройке мобильного телефона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беззвучный",
                                "target_lang_sense_clarification": "режим устройства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c35f95f-ae89-3f83-bb79-a6ca4e6ef423",
                    "sense_definition": "Of a letter in a word: written but not pronounced when spoken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The letter k in the word knee is <w>silent</w>.",
                            "target_lang_translations": {
                                "ru": "Буква k в слове knee — <w>непроизносимая</w>."
                            }
                        },
                        {
                            "text": "<w>Silent</w> letters can make some English words difficult to spell.",
                            "target_lang_translations": {
                                "ru": "<w>Непроизносимые</w> буквы могут затруднять написание некоторых английских слов."
                            }
                        }
                    ],
                    "synonyms": [
                        "mute"
                    ],
                    "common_phrases": [
                        "silent letter",
                        "silent e"
                    ],
                    "target_lang_definitions": {
                        "ru": "О букве в слове: пишется, но не произносится при разговоре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непроизносимый",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "немой",
                                "target_lang_sense_clarification": "традиционный термин (например, немое e)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8739e6d4-6b78-3d5d-a4d6-f38d6d8fb299",
                    "sense_definition": "Participating or involved in a business without being publicly known or active in daily operations.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "business_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a <w>silent</w> partner in the firm and does not attend meetings.",
                            "target_lang_translations": {
                                "ru": "Он — <w>негласный</w> партнер в фирме и не посещает собрания."
                            }
                        },
                        {
                            "text": "She preferred to remain a <w>silent</w> investor in the project.",
                            "target_lang_translations": {
                                "ru": "Она предпочла остаться <w>тайным</w> инвестором проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "hidden",
                        "concealed"
                    ],
                    "common_phrases": [
                        "silent partner"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "business"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Участвующий в бизнесе, но не известный публично и не активный в повседневной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "негласный",
                                "target_lang_sense_clarification": "о партнере или участии"
                            },
                            {
                                "target_lang_word": "тайный",
                                "target_lang_sense_clarification": "скрытый от публики"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "482cc696-dcf7-3865-97ac-1a886c279c88",
                    "sense_definition": "Undiagnosed or undetected because of an absence of obvious symptoms.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "High blood pressure is often called a <w>silent</w> killer because it has no warning signs.",
                            "target_lang_translations": {
                                "ru": "Высокое кровяное давление часто называют <w>тихим</w> убийцей, потому что у него нет предупреждающих признаков."
                            }
                        },
                        {
                            "text": "The patient suffered a <w>silent</w> heart attack without realizing it.",
                            "target_lang_translations": {
                                "ru": "Пациент перенес <w>бессимптомный</w> сердечный приступ, не осознавая этого."
                            }
                        }
                    ],
                    "common_phrases": [
                        "silent killer",
                        "silent ischemia"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Недиагностированный или невыявленный из-за отсутствия явных симптомов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бессимптомный",
                                "target_lang_sense_clarification": "медицинский термин"
                            },
                            {
                                "target_lang_word": "скрытый",
                                "target_lang_sense_clarification": "протекающий незаметно"
                            },
                            {
                                "target_lang_word": "тихий",
                                "target_lang_sense_clarification": "прямой перевод в устойчивых фразах (тихий убийца)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1112b835-fd03-3383-aafd-734465389185",
                    "sense_definition": "A film with no synchronized recorded sound, especially one from the early era of filmmaking.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cinema",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum is showing a collection of early <w>silents</w> this weekend.",
                            "target_lang_translations": {
                                "ru": "В эти выходные музей показывает коллекцию раннего <w>немого кино</w>."
                            }
                        },
                        {
                            "text": "Many actors from the <w>silents</w> struggled to transition to talking pictures.",
                            "target_lang_translations": {
                                "ru": "Многим актерам <w>немого кино</w> было трудно перейти к звуковым фильмам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "early silents"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "film"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фильм без синхронизированного записанного звука, особенно эпохи раннего кинематографа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "немое кино",
                                "target_lang_sense_clarification": "жанр кинематографа"
                            },
                            {
                                "target_lang_word": "немой фильм",
                                "target_lang_sense_clarification": "конкретная кинокартина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "845177a4-7b0d-3816-a695-da5b589a2a96",
                    "sense_definition": "A member of the Silent Generation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "demographic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandfather is a <w>Silent</w> who remembers the post-war years vividly.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка — <w>представитель молчаливого поколения</w>, который живо помнит послевоенные годы."
                            }
                        },
                        {
                            "text": "Surveys show that <w>Silents</w> often have different political views than Millennials.",
                            "target_lang_translations": {
                                "ru": "Опросы показывают, что <w>представители молчаливого поколения</w> часто имеют иные политические взгляды, чем миллениалы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "sociology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель «молчаливого поколения» (родившиеся примерно с 1928 по 1945 год)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представитель молчаливого поколения",
                                "target_lang_sense_clarification": "социологический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
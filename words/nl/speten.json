                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вертела",
                                "target_lang_sense_clarification": "стержни для жарки мяса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2319094f-5e94-3b68-805a-33c11687f8f3",
                    "sense_definition": "meervoud van <w>speet</w>",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_speet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De visser maakte de houten <w>speten</w> schoon na gebruik.",
                            "target_lang_translations": {
                                "ru": "Рыбак чистил деревянные <прутья> после использования."
                            }
                        },
                        {
                            "text": "Aan de <w>speten</w> hing de vis te drogen in de wind.",
                            "target_lang_translations": {
                                "ru": "На <прутьях> висела рыба, высыхая на ветру."
                            }

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "06c8b875-1f93-30b4-ad1c-5b9d106c8600",
                    "sense_definition": "Werkelijk bestaand en niet denkbeeldig; overeenstemmend met de werkelijkheid.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "reality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze extreme hitte vormde een <w>reëel</w> gevaar voor de ouderen.",
                            "target_lang_translations": {
                                "en": "This extreme heat posed a <w>real</w> danger to the elderly.",
                                "ru": "Эта сильная жара представляла <w>реальную</w> опасность для пожилых людей."
                            }
                        },
                        {
                            "text": "Er is een <w>reële</w> kans dat we de wedstrijd winnen.",
                            "target_lang_translations": {
                                "en": "There is a <w>real</w> chance that we will win the match.",
                                "ru": "Существует <w>реальный</w> шанс, что мы выиграем матч."
                            }
                        }
                    ],
                    "synonyms": [
                        "echt",
                        "werkelijk"
                    ],
                    "antonyms": [
                        "irreëel",
                        "onecht",
                        "onwerkelijk"
                    ],
                    "common_phrases": [
                        "een reëel gevaar",
                        "een reële kans",
                        "reële waarde"
                    ],
                    "target_lang_definitions": {
                        "en": "Existing in fact and not imaginary; corresponding to reality.",
                        "ru": "Существующий на самом деле, не воображаемый; соответствующий действительности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "real",
                                "target_lang_sense_clarification": "existing in fact"
                            },
                            {
                                "target_lang_word": "actual",
                                "target_lang_sense_clarification": "existing in fact"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "реальный",
                                "target_lang_sense_clarification": "существующий в действительности"
                            },
                            {
                                "target_lang_word": "действительный",
                                "target_lang_sense_clarification": "настоящий, не мнимый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "699bcf7a-ccf0-3231-88ad-484bcc4cc891",
                    "sense_definition": "Gebaseerd op de feiten en daardoor haalbaar of uitvoerbaar; getuigend van een nuchtere kijk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "practicality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een stuk <w>reëler</w> geworden in zijn verwachtingen.",
                            "target_lang_translations": {
                                "en": "He has become much more <w>realistic</w> in his expectations.",
                                "ru": "Он стал намного <w>реалистичнее</w> в своих ожиданиях."
                            }
                        },
                        {
                            "text": "We moeten zoeken naar een <w>reële</w> oplossing voor dit probleem.",
                            "target_lang_translations": {
                                "en": "We must look for a <w>realistic</w> solution to this problem.",
                                "ru": "Мы должны искать <w>реалистичное</w> решение этой проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "realistisch"
                    ],
                    "antonyms": [
                        "onrealistisch"
                    ],
                    "common_phrases": [
                        "reële verwachtingen",
                        "een reële optie",
                        "reële plannen"
                    ],
                    "target_lang_definitions": {
                        "en": "Based on facts and therefore achievable; showing a sensible outlook.",
                        "ru": "Основанный на фактах и поэтому выполнимый; свидетельствующий о трезвом взгляде на вещи."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "realistic",
                                "target_lang_sense_clarification": "practical and feasible"
                            },
                            {
                                "target_lang_word": "feasible",
                                "target_lang_sense_clarification": "possible to do"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "реалистичный",
                                "target_lang_sense_clarification": "трезвый, выполнимый"
                            },
                            {
                                "target_lang_word": "разумный",
                                "target_lang_sense_clarification": "обоснованный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5170b45b-82b0-3b55-96d4-22fa0b5de2a8",
                    "sense_definition": "Gecorrigeerd voor de invloed van inflatie; uitgedrukt in koopkracht.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "economics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>reële</w> inkomen van de bevolking is dit jaar gedaald.",
                            "target_lang_translations": {
                                "en": "The population's <w>real</w> income has fallen this year.",
                                "ru": "<w>Реальный</w> доход населения в этом году снизился."
                            }
                        },
                        {
                            "text": "Beleggers kijken vooral naar het <w>reële</w> rendement op hun investeringen.",
                            "target_lang_translations": {
                                "en": "Investors mainly look at the <w>real</w> return on their investments.",
                                "ru": "Инвесторы смотрят прежде всего на <w>реальную</w> доходность своих вложений."
                            }
                        }
                    ],
                    "antonyms": [
                        "theoretisch"
                    ],
                    "common_phrases": [
                        "reëel inkomen",
                        "reële rente",
                        "reële groei"

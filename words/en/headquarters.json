            "senses": [
                {
                    "sense_id": "32b5dc0c-0ca3-3b33-8eec-b1ca1fd5be3e",
                    "sense_definition": "The main office or center of an organization's operations or administration.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company moved its <w>headquarters</w> to a larger building in the city center.",
                            "target_lang_translations": {
                                "ru": "Компания перенесла свою <w>штаб-квартиру</w> в более просторное здание в центре города."
                            }
                        },
                        {
                            "text": "She works at the corporate <w>headquarters</w> of a major international bank.",
                            "target_lang_translations": {
                                "ru": "Она работает в корпоративном <w>головном офисе</w> крупного международного банка."
                            }
                        }
                    ],
                    "synonyms": [

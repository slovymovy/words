{
    "word_family": [
        "start",
        "startować",
        "startowy",
        "starter"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "80773551-6f9c-3b89-a814-bcd0f32e6a74",
                    "sense_definition": "Wzbić się w powietrze (o samolocie, rakiecie) lub gwałtownie ruszyć z linii startu (o zawodniku, pojeździe).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samolot pasażerski <w>wystartował</w> z lotniska punktualnie o godzinie ósmej.",
                            "target_lang_translations": {
                                "ru": "Пассажирский самолет <w>взлетел</w> из аэропорта ровно в восемь часов."
                            }
                        },
                        {
                            "text": "Wszyscy biegacze <w>wystartowali</w> natychmiast po usłyszeniu sygnału sędziego.",
                            "target_lang_translations": {
                                "ru": "Все бегуны <w>стартовали</w> сразу же, услышав сигнал судьи."
                            }
                        }
                    ],
                    "antonyms": [
                        "wylądować"
                    ],
                    "common_phrases": [
                        "wystartować z lotniska",
                        "wystartować do biegu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взлететь (о самолете, ракете) или резко тронуться с места (о спортсмене, транспортном средстве)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взлететь",
                                "target_lang_sense_clarification": "о самолете, птице"
                            },
                            {
                                "target_lang_word": "стартовать",
                                "target_lang_sense_clarification": "начать движение, гонку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44d2d450-9c48-35aa-aff8-5a420e174562",
                    "sense_definition": "Wziąć udział w rywalizacji, na przykład w zawodach sportowych, konkursie lub wyborach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "participation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat postanowił <w>wystartować</w> w maratonie po raz pierwszy w życiu.",
                            "target_lang_translations": {
                                "ru": "Мой брат решил <w>принять участие</w> в марафоне первый раз в жизни."
                            }
                        },
                        {
                            "text": "W nadchodzących wyborach prezydenckich <w>wystartuje</w> pięciu kandydatów.",
                            "target_lang_translations": {
                                "ru": "В предстоящих президентских выборах <w>примут участие</w> пять кандидатов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wystartować w zawodach",
                        "wystartować w wyborach",
                        "wystartować w konkursie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять участие в состязании, например, в спортивных соревнованиях, конкурсе или выборах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принять участие",
                                "target_lang_sense_clarification": "в соревнованиях, конкурсе"
                            },
                            {
                                "target_lang_word": "баллотироваться",
                                "target_lang_sense_clarification": "в выборах"
                            },
                            {
                                "target_lang_word": "выступить",
                                "target_lang_sense_clarification": "на соревнованиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fe2c53e-c9a4-3c94-b0b2-f5a6ef951f4d",
                    "sense_definition": "Rozpocząć funkcjonowanie, działanie lub nowy etap (często o firmie, projekcie lub systemie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_start",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa strona internetowa naszej firmy <w>wystartowała</w> wczoraj wieczorem.",
                            "target_lang_translations": {
                                "ru": "Новый веб-сайт нашей компании <w>запустился</w> вчера вечером."
                            }
                        },
                        {
                            "text": "Dzięki dotacji unijnej ten innowacyjny projekt wkrótce <w>wystartuje</w>.",
                            "target_lang_translations": {
                                "ru": "Благодаря гранту ЕС этот инновационный проект скоро <w>стартует</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozpocząć"
                    ],
                    "common_phrases": [
                        "wystartować z projektem",
                        "system wystartował"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать функционировать, работать или начать новый этап (часто о компании, проекте или системе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запуститься",
                                "target_lang_sense_clarification": "о системе, сайте, программе"
                            },
                            {
                                "target_lang_word": "стартовать",
                                "target_lang_sense_clarification": "о проекте, начинании"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
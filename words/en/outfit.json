                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снаряжение",
                                "target_lang_sense_clarification": "совокупность предметов для чего-либо"
                            },
                            {
                                "target_lang_word": "экипировка",
                                "target_lang_sense_clarification": "снаряжение, обмундирование"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "15a66150-358c-3b3c-8838-c6f4fc6485ce",
                    "sense_definition": "To provide someone or something with necessary equipment or clothing for a specific purpose.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "to_equip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The expedition was <w>outfitted</w> with the latest navigation technology.",
                            "target_lang_translations": {
                                "ru": "Экспедиция была <w>снаряжена</w> новейшими навигационными технологиями."
                            }
                        },
                        {
                            "text": "The store can <w>outfit</w> you with everything you need for the ski trip.",
                            "target_lang_translations": {
                                "ru": "В этом магазине вас могут <w>экипировать</w> всем необходимым для лыжной поездки."
                            }
                        }
                    ],

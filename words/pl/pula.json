{
    "word_family": [],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1e62a7c-4e0c-3578-b1d3-c4400802e6a9",
                    "sense_definition": "Zbiór zasobów, pieniędzy, danych lub przedmiotów zgromadzonych w jednym miejscu do wspólnego wykorzystania lub podziału.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym roku <pula> nagród w konkursie jest wyjątkowo wysoka.",
                            "target_lang_translations": {
                                "ru": "В этом году <призовой фонд> конкурса исключительно велик."
                            }
                        },
                        {
                            "text": "Firma wyczerpała już swoją <pulę> numerów telefonicznych na ten miesiąc.",
                            "target_lang_translations": {
                                "ru": "Компания уже исчерпала свой <пул> телефонных номеров на этот месяц."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pula nagród",
                        "pula genowa",
                        "pula środków",
                        "wspólna pula"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность ресурсов, денег или данных, собранных в одном месте для совместного использования или распределения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фонд",
                                "target_lang_sense_clarification": "ресурсов, наград"
                            },
                            {
                                "target_lang_word": "пул",
                                "target_lang_sense_clarification": "данных, общий набор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2526360e-e3e9-3745-aaf9-5c5eda8ed9b1",
                    "sense_definition": "W grach losowych i hazardowych: łączna suma pieniędzy lub żetonów wpłaconych przez graczy, którą wygrywa zwycięzca.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gracz zaryzykował wszystko, co miał, i zgarnął całą <pulę>.",
                            "target_lang_translations": {
                                "ru": "Игрок рискнул всем, что у него было, и сорвал весь <банк>."
                            }
                        },
                        {
                            "text": "Przed rozdaniem kart każdy uczestnik musi dorzucić żeton do <puli>.",
                            "target_lang_translations": {
                                "ru": "Перед раздачей карт каждый участник должен добавить фишку в <банк>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stawka"
                    ],
                    "common_phrases": [
                        "zgarnąć pulę",
                        "rozbić pulę",
                        "wchodzić do puli"
                    ],
                    "target_lang_definitions": {
                        "ru": "В азартных играх: общая сумма денег или фишек, поставленных игроками, которую получает победитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "банк",
                                "target_lang_sense_clarification": "в играх"
                            },
                            {
                                "target_lang_word": "кон",
                                "target_lang_sense_clarification": "ставка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "083e972c-1930-37a4-94c9-311b6de36329",
                    "sense_definition": "Podstawowa jednostka monetarna używana w Botswanie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas podróży po Afryce wymieniłem dolary na walutę zwaną <pula>.",
                            "target_lang_translations": {
                                "ru": "Во время путешествия по Африке я обменял доллары на валюту под названием <пула>."
                            }
                        },
                        {
                            "text": "Jedna <pula> dzieli się na sto tebe.",
                            "target_lang_translations": {
                                "ru": "Одна <пула> делится на сто тебе."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная единица Ботсваны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пула",
                                "target_lang_sense_clarification": "валюта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81935a56-f386-3d3e-b839-2da82a883f8c",
                    "sense_definition": "Naczynie na płyny, zazwyczaj metalowa kanka lub bańka.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia poprosiła mnie, żebym wziął <pulę> i poszedł po świeże mleko.",
                            "target_lang_translations": {
                                "ru": "Бабушка попросила меня взять <бидон> и сходить за свежим молоком."
                            }
                        },
                        {
                            "text": "Postaw tę ciężką <pulę> z wodą na ławce przed domem.",
                            "target_lang_translations": {
                                "ru": "Поставь этот тяжелый <бидон> с водой на скамейку перед домом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Górny Śląsk"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосуд для жидкостей, обычно металлический бидон или банка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бидон",
                                "target_lang_sense_clarification": "сосуд"
                            },
                            {
                                "target_lang_word": "банка",
                                "target_lang_sense_clarification": "сосуд"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c8f5cd7f-73c7-3352-86a7-268032c70172",
                    "sense_definition": "Miasto w Chorwacji położone na południowym skraju półwyspu Istria nad Morzem Adriatyckim.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W te wakacje planujemy zwiedzić rzymski amfiteatr, z którego słynie <Pula>.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы планируем посетить римский амфитеатр, которым славится <Пула>."
                            }
                        },
                        {
                            "text": "<Pula> jest ważnym portem i ośrodkiem turystycznym w Chorwacji.",
                            "target_lang_translations": {
                                "ru": "<Пула> является важным портом и туристическим центром в Хорватии."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в Хорватии, расположенный на южной оконечности полуострова Истрия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пула",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "719b8552-0cfa-3b6e-b23e-1b5cb69dbba6",
                    "sense_definition": "Trzecia osoba liczby pojedynczej czasu teraźniejszego czasownika <w>pulać</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe dziecko czasem nieświadomie <pula> w pieluchę.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок иногда неосознанно <писает> в подгузник."
                            }
                        },
                        {
                            "text": "Pies sąsiada codziennie rano <pula> na nasz trawnik.",
                            "target_lang_translations": {
                                "ru": "Соседская собака каждое утро <писает> на наш газон."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "lub mowa dziecięca (sikać)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Третье лицо единственного числа настоящего времени глагола 'pulać' (мочиться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "писает",
                                "target_lang_sense_clarification": "мочится"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c534b8e9-f27b-315a-8a06-4a0e620affce",
                    "sense_definition": "Обратиться к кому-либо с вежливым предложением сделать что-либо, дать что-либо или разрешить что-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "request_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец <попросил> у дочери прощения за свою ошибку."
                        },
                        {
                            "text": "Я хочу <попросить> тебя помочь мне с домашним заданием."
                        }
                    ],
                    "antonyms": [
                        "приказать",
                        "потребовать"
                    ],
                    "common_phrases": [
                        "попросить прощения",
                        "попросить помощи",
                        "попросить денег",
                        "попросить разрешения",
                        "попросить совета"
                    ]
                },
                {
                    "sense_id": "76bd63e8-fc72-3fd5-be61-3afe9f2264cd",
                    "sense_definition": "Вежливо пригласить кого-либо войти, подойти или направиться куда-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "request_invite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Секретарь <попросил> посетителя пройти в кабинет директора."
                        },
                        {
                            "text": "Хозяйка дома улыбнулась и <попросила> гостей к столу."
                        }
                    ],
                    "synonyms": [
                        "пригласить",
                        "позвать"
                    ],
                    "common_phrases": [
                        "попросить к телефону",
                        "попросить войти",
                        "попросить к столу"
                    ]
                },
                {
                    "sense_id": "a48374cd-0bb6-34f8-b4b0-b3ff747b387e",
                    "sense_definition": "Строго потребовать прекратить нежелательные действия или слова (часто используется как реакция на грубость или неуместное поведение).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "demand_stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <попрошу> вас не разговаривать со мной в таком тоне!"
                        },
                        {
                            "text": "— Я бы <попросил>! — возмутился учитель, услышав шум на задней парте."
                        }
                    ],
                    "synonyms": [
                        "потребовать"
                    ],
                    "common_phrases": [
                        "я попрошу",
                        "я бы попросил"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in phrases like 'я попрошу!'"
                        }
                    ]
                },
                {
                    "sense_id": "b9d84b92-b2bb-32dd-b9ef-7d2ae08a0bbc",
                    "sense_definition": "Вежливо, но категорично потребовать уйти, покинуть помещение или освободить должность.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "demand_leave",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если вы продолжите шуметь, охрана <попросит> вас из зала."
                        },
                        {
                            "text": "После громкого скандала сотрудника вежливо <попросили> с работы."
                        }
                    ],
                    "synonyms": [
                        "выпроводить",
                        "уволить"
                    ],
                    "common_phrases": [
                        "попросить на выход",
                        "попросить вон"
                    ]
                }
            ]
        }
    ]

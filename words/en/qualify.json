{
    "word_family": [
        "qualification",
        "qualifier",
        "disqualification",
        "disqualify",
        "qualified",
        "unqualified"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "93122f41-0874-34e6-98be-c447fd5c17c0",
                    "sense_definition": "To compete successfully in a stage of a competition and become eligible for the next stage.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "meeting_requirements",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our team needs to win this match to <w>qualify</w> for the finals.",
                            "target_lang_translations": {
                                "ru": "Нашей команде нужно выиграть этот матч, чтобы <w>квалифицироваться</w> в финал."
                            }
                        },
                        {
                            "text": "She trained very hard to <w>qualify</w> for the Olympic Games.",
                            "target_lang_translations": {
                                "ru": "Она очень усердно тренировалась, чтобы <w>отобраться</w> на Олимпийские игры."
                            }
                        }
                    ],
                    "antonyms": [
                        "disqualify"
                    ],
                    "common_phrases": [
                        "qualify for the finals",
                        "qualify for the tournament",
                        "qualify for the next round"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пройти отборочный этап соревнования и получить право участвовать в следующем этапе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квалифицироваться",
                                "target_lang_sense_clarification": "пройти отбор в спорте"
                            },
                            {
                                "target_lang_word": "пройти квалификацию",
                                "target_lang_sense_clarification": "успешно завершить отборочный этап"
                            },
                            {
                                "target_lang_word": "отобраться",
                                "target_lang_sense_clarification": "разг. пройти отбор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cec75459-007d-3167-b6ab-ad111eb0f8d5",
                    "sense_definition": "To have or provide the necessary skills, knowledge, or credentials to be eligible for a position or task.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "meeting_requirements",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It takes many years of study and practice to <w>qualify</w> as a surgeon.",
                            "target_lang_translations": {
                                "ru": "Требуется много лет учебы и практики, чтобы <w>получить квалификацию</w> хирурга."
                            }
                        },
                        {
                            "text": "His lack of relevant experience does not <w>qualify</w> him for the manager role.",
                            "target_lang_translations": {
                                "ru": "Его недостаток соответствующего опыта не <w>дает</w> ему <w>права</w> претендовать на роль менеджера."
                            }
                        }
                    ],
                    "synonyms": [
                        "certify",
                        "authorize",
                        "license"
                    ],
                    "antonyms": [
                        "disqualify"
                    ],
                    "common_phrases": [
                        "qualify as a doctor",
                        "qualify for the job",
                        "qualify as a teacher"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь необходимые навыки, знания или диплом для выполнения определенной работы или задачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получить квалификацию",
                                "target_lang_sense_clarification": "приобрести профессию или статус"
                            },
                            {
                                "target_lang_word": "давать право",
                                "target_lang_sense_clarification": "делать кого-либо пригодным для чего-либо"
                            },
                            {
                                "target_lang_word": "аттестовать",
                                "target_lang_sense_clarification": "присваивать звание или разряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28171603-b286-3a5a-8593-a0252c820a2f",
                    "sense_definition": "To fit a specific description or meet the necessary conditions to be classified in a certain way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "meeting_requirements",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This purchase might <w>qualify</w> as a business expense for tax purposes.",
                            "target_lang_translations": {
                                "ru": "Эта покупка может <w>расцениваться</w> как деловые расходы для целей налогообложения."
                            }
                        },
                        {
                            "text": "To <w>qualify</w> for the student discount, you must show a valid ID card.",
                            "target_lang_translations": {
                                "ru": "Чтобы <w>иметь право</w> на студенческую скидку, вы должны предъявить действительное удостоверение."
                            }
                        }
                    ],
                    "synonyms": [
                        "count"
                    ],
                    "common_phrases": [
                        "qualify as success",
                        "qualify for a grant",
                        "qualify for a discount"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствовать определенным условиям или подходить под определенное описание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь право",
                                "target_lang_sense_clarification": "соответствовать условиям для получения льготы"
                            },
                            {
                                "target_lang_word": "расцениваться",
                                "target_lang_sense_clarification": "считаться, восприниматься как что-либо"
                            },
                            {
                                "target_lang_word": "квалифицировать",
                                "target_lang_sense_clarification": "относить к какой-либо категории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1a82c44-db3f-356b-ac2d-079f052e156e",
                    "sense_definition": "To add conditions, exceptions, or limitations to a statement to make it less absolute or more precise.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I would like to <w>qualify</w> my previous statement to avoid any misunderstanding.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>уточнить</w> свое предыдущее заявление, чтобы избежать недопонимания."
                            }
                        },
                        {
                            "text": "In grammar, adjectives usually <w>qualify</w> nouns by giving more information about them.",
                            "target_lang_translations": {
                                "ru": "В грамматике прилагательные обычно <w>определяют</w> существительные, давая о них больше информации."
                            }
                        }
                    ],
                    "synonyms": [
                        "moderate",
                        "restrict",
                        "modify"
                    ],
                    "common_phrases": [
                        "qualify a statement",
                        "qualify an answer",
                        "qualify an opinion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уточнять или ограничивать значение высказывания, делая его менее категоричным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уточнять",
                                "target_lang_sense_clarification": "делать более точным"
                            },
                            {
                                "target_lang_word": "оговаривать",
                                "target_lang_sense_clarification": "делать оговорку"
                            },
                            {
                                "target_lang_word": "определять",
                                "target_lang_sense_clarification": "грам. характеризовать слово"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c6deeed-0f36-3dbb-83af-57fde707b9da",
                    "sense_definition": "Potoczne określenie Warszawskiej Kolei Dojazdowej, systemu transportu szynowego łączącego Warszawę z okolicznymi miejscowościami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "wkd_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkańcy podwarszawskich miejscowości często wybierają <w>wukadkę</w> jako najszybszy środek transportu do centrum stolicy.",
                            "target_lang_translations": {
                                "ru": "Жители подваршавских населенных пунктов часто выбирают <w>WKD</w> как самый быстрый способ добраться до центра столицы."
                            }
                        },
                        {
                            "text": "Z powodu awarii sieci trakcyjnej <w>wukadka</w> nie kursowała przez kilka godzin.",
                            "target_lang_translations": {
                                "ru": "Из-за аварии контактной сети <w>WKD</w> не ходила несколько часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "WKD",
                        "kolejka"
                    ],
                    "common_phrases": [
                        "jechać wukadką",
                        "stacja wukadki",
                        "trasa wukadki"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Warszawa i okolice"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название Варшавской пригородной железной дороги (WKD), системы рельсового транспорта, соединяющей Варшаву с окрестными населенными пунктами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "WKD",
                                "target_lang_sense_clarification": "аббревиатура, часто используемая в речи"
                            },
                            {
                                "target_lang_word": "Варшавская пригородная железная дорога",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57137afe-c194-376a-b351-653dbc78fe03",
                    "sense_definition": "Pociąg należący do taboru Warszawskiej Kolei Dojazdowej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "wkd_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy się pospieszyć, bo następna <w>wukadka</w> odjeżdża ze stacji dopiero za trzydzieści minut.",
                            "target_lang_translations": {
                                "ru": "Нам нужно поторопиться, потому что следующая <w>электричка</w> отходит от станции только через тридцать минут."
                            }
                        },
                        {
                            "text": "Na peron wjechała nowoczesna <w>wukadka</w>, która zabierze wszystkich pasażerów do Grodziska.",
                            "target_lang_translations": {
                                "ru": "На перрон въехала современная <w>электричка</w>, которая отвезет всех пассажиров в Гродзиск."
                            }
                        }
                    ],
                    "synonyms": [
                        "pociąg",
                        "skład"
                    ],
                    "common_phrases": [
                        "czekać na wukadkę",
                        "wsiąść do wukadki",
                        "zatłoczona wukadka"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Warszawa i okolice"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поезд, принадлежащий подвижному составу Варшавской пригородной железной дороги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электричка",
                                "target_lang_sense_clarification": "пригородный поезд"
                            },
                            {
                                "target_lang_word": "поезд WKD",
                                "target_lang_sense_clarification": "поезд конкретной линии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba3a6bfe-a6e6-32bb-b40d-b3cd2c67f16d",
                    "sense_definition": "Gramatycznie uporządkowany zespół słów zawierający orzeczenie, stanowiący logiczną całość.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił ucznia o napisanie jednego <w>zdania</w> na tablicy.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика написать одно <w>предложение</w> на доске."
                            }
                        },
                        {
                            "text": "To <w>zdanie</w> jest bardzo długie i skomplikowane.",
                            "target_lang_translations": {
                                "ru": "Это <w>предложение</w> очень длинное и сложное."
                            }
                        }
                    ],
                    "antonyms": [
                        "równoważnik zdania"
                    ],
                    "common_phrases": [
                        "zdanie proste",
                        "zdanie złożone",
                        "koniec zdania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматически упорядоченная группа слов, содержащая сказуемое и представляющая собой логическое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предложение",
                                "target_lang_sense_clarification": "грамматическая единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e484217-a91d-3d94-a7d0-7b74a348fcb8",
                    "sense_definition": "Osobisty pogląd, osąd lub opinia na jakiś temat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moim <w>zdaniem</w> ten film był bardzo interesujący.",
                            "target_lang_translations": {
                                "ru": "По моему <w>мнению</w>, этот фильм был очень интересным."
                            }
                        },
                        {
                            "text": "Ona często zmienia <w>zdanie</w> w błahych sprawach.",
                            "target_lang_translations": {
                                "ru": "Она часто меняет <w>мнение</w> по пустякам."
                            }
                        }
                    ],
                    "synonyms": [
                        "opinia",
                        "pogląd",
                        "sąd"
                    ],
                    "common_phrases": [
                        "moim zdaniem",
                        "zmienić zdanie",
                        "mieć inne zdanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Личный взгляд, суждение или мнение по какому-либо вопросу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мнение",
                                "target_lang_sense_clarification": "точка зрения"
                            },
                            {
                                "target_lang_word": "взгляд",
                                "target_lang_sense_clarification": "убеждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bec0a344-5353-34cb-946a-f583e135b4c0",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>zdać</w> (zakończyć sukcesem egzamin lub przekazać coś).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pomyślne <w>zdanie</w> egzaminu na prawo jazdy wymagało wielu godzin nauki.",
                            "target_lang_translations": {
                                "ru": "Успешная <w>сдача</w> экзамена на водительские права потребовала многих часов учёбы."
                            }
                        },
                        {
                            "text": "Czekamy na <w>zdanie</w> relacji z przebiegu wydarzeń.",
                            "target_lang_translations": {
                                "ru": "Мы ждем <w>сдачи</w> отчета о ходе событий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zdanie egzaminu",
                        "zdanie relacji",
                        "zdanie matury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное от «zdać» (успешно завершить экзамен или передать что-либо)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдача",
                                "target_lang_sense_clarification": "экзамена, отчёта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e06e54d-d59f-344d-882f-179e8058b533",
                    "sense_definition": "W logice: twierdzenie, któremu można przypisać wartość prawdy lub fałszu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W logice matematycznej każde <w>zdanie</w> ma określoną wartość logiczną.",
                            "target_lang_translations": {
                                "ru": "В математической логике каждое <w>высказывание</w> имеет определенное логическое значение."
                            }
                        },
                        {
                            "text": "To twierdzenie stanowi <w>zdanie</w> fałszywe.",
                            "target_lang_translations": {
                                "ru": "Эта теорема представляет собой ложное <w>высказывание</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sąd"
                    ],
                    "common_phrases": [
                        "zdanie logiczne",
                        "wartość zdania"
                    ],
                    "target_lang_definitions": {
                        "ru": "В логике: утверждение, которому можно приписать значение истины или лжи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высказывание",
                                "target_lang_sense_clarification": "логическое"
                            },
                            {
                                "target_lang_word": "суждение",
                                "target_lang_sense_clarification": "логическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c48d9aa0-1f49-37f1-895a-7b900212f75d",
                    "sense_definition": "Fragment utworu muzycznego stanowiący pewną całość, zazwyczaj czterotaktowy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kompozytor zakończył to <w>zdanie</w> muzyczne w zaskakujący sposób.",
                            "target_lang_translations": {
                                "ru": "Композитор закончил это музыкальное <w>предложение</w> неожиданным образом."
                            }
                        },
                        {
                            "text": "Pianista zagrał pierwsze <w>zdanie</w> z wielką ekspresją.",
                            "target_lang_translations": {
                                "ru": "Пианист сыграл первое <w>предложение</w> с большой экспрессией."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zdanie muzyczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фрагмент музыкального произведения, представляющий собой некое целое, обычно четырехтактный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предложение",

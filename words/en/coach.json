                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эконом-классом",
                                "target_lang_sense_clarification": "наречие"
                            },
                            {
                                "target_lang_word": "в эконом-классе",
                                "target_lang_sense_clarification": "обстоятельство места"
                            }

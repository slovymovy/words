            "pos": "verb",
            "senses": [
                {
                    "sense_id": "853edc43-8dc1-3831-9112-4f91b351172f",
                    "sense_definition": "Пытаться совершить какое-либо действие, прилагать усилия для достижения цели.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "attempt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>пробую</w> открыть эту дверь, но замок, кажется, сломан."
                        },
                        {
                            "text": "Если у тебя не получилось с первого раза, нужно <w>пробовать</w> снова."
                        }
                    ],
                    "synonyms": [
                        "пытаться",
                        "стараться"
                    ],
                    "common_phrases": [
                        "пробовать сделать",
                        "пробовать снова",
                        "пробовать договориться"
                    ]
                },
                {
                    "sense_id": "704ddf72-678e-3e5d-a309-5ecdceaeaac7",
                    "sense_definition": "Есть или пить немного чего-либо, чтобы определить вкус или готовность.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "taste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Повар <w>пробует</w> суп, чтобы проверить, хватает ли в нём соли."
                        },
                        {
                            "text": "Мы пришли в ресторан и весь вечер <w>пробовали</w> местные блюда."
                        }
                    ],
                    "synonyms": [
                        "дегустировать",
                        "отведывать"
                    ],
                    "common_phrases": [
                        "пробовать на вкус",
                        "пробовать еду",
                        "дай попробовать"
                    ]
                },
                {
                    "sense_id": "9f6a61ae-9f3a-3f8e-aa5d-22978050e8f1",
                    "sense_definition": "Проверять на опыте свойства, качества или пригодность кого-либо или чего-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "test",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Молодой спортсмен решил <w>пробовать</w> свои силы в международных соревнованиях."
                        },
                        {
                            "text": "Инженеры сейчас <w>пробуют</w> новый прибор в действии."
                        }
                    ],
                    "synonyms": [
                        "испытывать",
                        "проверять",
                        "тестировать",
                        "опробовать"
                    ],
                    "common_phrases": [
                        "пробовать свои силы",
                        "пробовать метод",
                        "пробовать в деле"
                    ]
                }
            ]

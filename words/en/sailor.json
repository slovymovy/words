{
    "word_family": [
        "sail",
        "sailing",
        "sailboat",
        "seaman",
        "seafarer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3d951843-a999-3c05-b678-0c54b8a55684",
                    "sense_definition": "A person who works on a ship or boat as part of the crew.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person_sailing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sailor</w> scrubbed the deck early in the morning.",
                            "target_lang_translations": {
                                "ru": "<w>Матрос</w> драил палубу рано утром."
                            }
                        },
                        {
                            "text": "Every <w>sailor</w> on the ship missed their family back home.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>моряк</w> на корабле скучал по своей семье, оставшейся дома."
                            }
                        },
                        {
                            "text": "He decided to become a <w>sailor</w> and travel the world.",
                            "target_lang_translations": {
                                "ru": "Он решил стать <w>моряком</w> и путешествовать по миру."
                            }
                        }
                    ],
                    "synonyms": [
                        "seaman",
                        "mariner",
                        "seafarer"
                    ],
                    "antonyms": [
                        "landlubber",
                        "nonsailor"
                    ],
                    "common_phrases": [
                        "experienced sailor",
                        "common sailor",
                        "navy sailor",
                        "drunken sailor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который работает на корабле или лодке в составе экипажа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моряк",
                                "target_lang_sense_clarification": "общий термин, профессия"
                            },
                            {
                                "target_lang_word": "матрос",
                                "target_lang_sense_clarification": "звание, рядовой член экипажа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b949118-ac4e-39dc-85b7-a8badcd6b0c0",
                    "sense_definition": "A person who sails a boat for sport or pleasure rather than as a job.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_sailing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a keen <w>sailor</w> and spends every weekend on her yacht.",
                            "target_lang_translations": {
                                "ru": "Она заядлый <w>яхтсмен</w> и проводит каждые выходные на своей яхте."
                            }
                        },
                        {
                            "text": "The lake was full of <w>sailors</w> enjoying the windy weather.",
                            "target_lang_translations": {
                                "ru": "Озеро было полно <w>любителей парусного спорта</w>, наслаждающихся ветреной погодой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "keen sailor",
                        "amateur sailor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, управляющий лодкой ради спорта или удовольствия, а не по работе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яхтсмен",
                                "target_lang_sense_clarification": "спортсмен"
                            },
                            {
                                "target_lang_word": "любитель парусного спорта",
                                "target_lang_sense_clarification": "хобби"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfa8e16f-a65f-3768-8463-56d5fc165439",
                    "sense_definition": "A stiff straw hat with a flat top and a straight brim.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school uniform included a traditional straw <w>sailor</w>.",
                            "target_lang_translations": {
                                "ru": "Школьная форма включала традиционное соломенное <w>канотье</w>."
                            }
                        },
                        {
                            "text": "He looked old-fashioned wearing a blazer and a <w>sailor</w>.",
                            "target_lang_translations": {
                                "ru": "Он выглядел старомодно в блейзере и <w>канотье</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "straw sailor"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to a specific style of hat popular in the past"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жесткая соломенная шляпа с плоским верхом и прямыми полями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "канотье",
                                "target_lang_sense_clarification": "тип шляпы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c3dc4d5-0953-3aaa-9821-dc1c4f59f6b8",
                    "sense_definition": "A type of butterfly with white markings on dark wings that glides while flying.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The common <w>sailor</w> rested on a leaf in the garden.",
                            "target_lang_translations": {
                                "ru": "Обыкновенная <w>пеструшка</w> отдыхала на листе в саду."
                            }
                        },
                        {
                            "text": "We spotted a <w>sailor</w> gliding between the trees.",
                            "target_lang_translations": {
                                "ru": "Мы заметили <w>пеструшку</w>, планирующую между деревьями."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность бабочки с белыми отметинами на темных крыльях, которая планирует в полете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пеструшка",
                                "target_lang_sense_clarification": "энтомология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27fa8abd-2dfa-39b9-98eb-48c62d3416f1",
                    "sense_definition": "A brick laid vertically on its smallest end.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The top of the wall was finished with a row of <w>sailors</w>.",
                            "target_lang_translations": {
                                "ru": "Верх стены был отделан рядом <w>вертикальных кирпичей</w>."
                            }
                        },
                        {
                            "text": "The mason placed a <w>sailor</w> to create a decorative pattern.",
                            "target_lang_translations": {
                                "ru": "Каменщик положил <w>кирпич вертикально</w>, чтобы создать декоративный узор."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кирпич, уложенный вертикально на наименьшую грань."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вертикальный кирпич",
                                "target_lang_sense_clarification": "строительство"
                            }
                        ]
                    }
                }
            ]
        },
        {

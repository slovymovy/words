                },
                {
                    "sense_id": "f5df4a34-1854-3d9e-b7e6-3e9440523ff0",
                    "sense_definition": "Specjalistyczne narzędzie używane przez lekarzy podczas operacji i zabiegów medycznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pielęgniarka podała chirurgowi odpowiedni <w>instrument</w> potrzebny do zabiegu.",
                            "target_lang_translations": {
                                "ru": "Медсестра подала хирургу подходящий <w>инструмент</w>, необходимый для операции."
                            }
                        },
                        {
                            "text": "Wszystkie <w>instrumenty</w> muszą zostać poddane sterylizacji przed użyciem.",
                            "target_lang_translations": {
                                "ru": "Все <w>инструменты</w> должны пройти стерилизацию перед использованием."
                            }
                        }

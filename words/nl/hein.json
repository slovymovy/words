    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "1afd0cd9-8ed2-3833-85c0-30b2fdc80ea7",
                    "sense_definition": "Een jongensnaam.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn buurman heet <w>Hein</w> en hij is erg aardig.",
                            "target_lang_translations": {
                                "ru": "Моего соседа зовут <w>Хайн</w>, и он очень приятный человек."
                            }
                        },
                        {
                            "text": "<w>Hein</w> gaat morgen met ons mee naar de bioscoop.",
                            "target_lang_translations": {
                                "ru": "<w>Хайн</w> пойдет с нами завтра в кино."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хайн",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6596000a-f790-370a-902a-b5a47610f690",
                    "sense_definition": "De personificatie van de dood, vaak voorgesteld als een skelet met een zeis.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In veel oude verhalen komt Magere <w>Hein</w> de zielen van de overledenen halen.",
                            "target_lang_translations": {
                                "ru": "Во многих старых легендах Костлявая <w>Смерть</w> приходит забирать души умерших."
                            }
                        },
                        {
                            "text": "De tekening beeldde Magere <w>Hein</w> af met zijn bekende zeis.",
                            "target_lang_translations": {
                                "ru": "На рисунке была изображена Костлявая <w>Смерть</w> со своей знаменитой косой."
                            }
                        }
                    ],
                    "synonyms": [
                        "de dood"
                    ],
                    "common_phrases": [
                        "Magere Hein"
                    ],
                    "target_lang_definitions": {
                        "ru": "Олицетворение смерти, часто изображаемое в виде скелета с косой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Смерть",
                                "target_lang_sense_clarification": "персонификация"
                            },
                            {
                                "target_lang_word": "Костлявая",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2558d058-1f73-33f3-97cd-29f69deae656",
                    "sense_definition": "Een fysieke afscheiding tussen velden of erven, zoals een hek, muur of sloot.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer plaatste een nieuwe <w>hein</w> om het vee binnen te houden.",
                            "target_lang_translations": {
                                "ru": "Фермер поставил новую <w>изгородь</w>, чтобы удержать скот."
                            }
                        },
                        {
                            "text": "Langs de <w>hein</w> groeiden allerlei wilde bloemen en struiken.",
                            "target_lang_translations": {
                                "ru": "Вдоль <w>ограды</w> росли всевозможные полевые цветы и кустарники."
                            }
                        }
                    ],
                    "synonyms": [
                        "heining",
                        "omheining",
                        "afscheiding"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly replaced by 'heining' or 'omheining'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая преграда между полями или дворами, например, забор или изгородь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изгородь",
                                "target_lang_sense_clarification": "ограждение"
                            },
                            {
                                "target_lang_word": "ограда",
                                "target_lang_sense_clarification": "забор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39540427-1481-3e55-a284-26ee7bf9429d",
                    "sense_definition": "Een zeer mager persoon.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "skinny_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zijn lange ziekte zag hij eruit als een echte <w>hein</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой болезни он выглядел как настоящий <w>скелет</w>."
                            }
                        },
                        {
                            "text": "Die jongen is zo'n <w>hein</w> dat zijn kleren veel te groot lijken.",
                            "target_lang_translations": {
                                "ru": "Этот парень — такая <w>жердь</w>, что одежда кажется ему слишком большой."
                            }
                        }
                    ],
                    "synonyms": [
                        "geraamte",
                        "spriet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень худой человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скелет",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "жердь",
                                "target_lang_sense_clarification": "о высоком и худом человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cc23462-66e0-36c4-842c-909fdf093d93",
                    "sense_definition": "Een mager paard.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "skinny_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude <w>hein</w> stond stilletjes in de wei te grazen.",
                            "target_lang_translations": {
                                "ru": "Старая <w>кляча</w> тихо паслась на лугу."
                            }
                        },
                        {
                            "text": "Niemand wilde op die zwakke <w>hein</w> rijden.",
                            "target_lang_translations": {
                                "ru": "Никто не хотел ехать верхом на этой слабой <w>кляче</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "knol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тощая лошадь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кляча",
                                "target_lang_sense_clarification": "о лошади"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
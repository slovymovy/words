    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "07594c4e-6dae-3418-83fa-3f16ea0c3934",
                    "sense_definition": "Вникнуть в подробности, понять суть, причины или детали чего-либо; выяснить непонятное.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "understand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В электросварке и за полгода не <w>разберёшься</w>!",
                            "target_lang_translations": {
                                "en": "You won't <w>figure out</w> electric welding even in six months!"
                            }
                        },
                        {
                            "text": "Нам нужно спокойно сесть и <w>разобраться</w> в этой сложной ситуации.",
                            "target_lang_translations": {
                                "en": "We need to sit down calmly and <w>figure out</w> this complex situation."
                            }
                        }
                    ],
                    "synonyms": [
                        "понять",
                        "выяснить",
                        "уяснить"
                    ],
                    "common_phrases": [
                        "разобраться в ситуации",
                        "разобраться в людях",
                        "попытаться разобраться"
                    ],
                    "target_lang_definitions": {
                        "en": "To understand the details, essence, or reasons of something; to clear up something confusing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "figure out",
                                "target_lang_sense_clarification": "to understand or solve something through thinking"
                            },
                            {
                                "target_lang_word": "understand",
                                "target_lang_sense_clarification": "to grasp the meaning or cause of something"
                            },
                            {
                                "target_lang_word": "get to the bottom of",
                                "target_lang_sense_clarification": "to find the true cause or explanation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6021dbbf-b4bb-36c3-8d1f-f4d4ac472bba",
                    "sense_definition": "Разрешить спор, уладить дело, справиться с проблемой или кем-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "deal_with",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция обещала быстро <w>разобраться</w> с уличными хулиганами.",
                            "target_lang_translations": {
                                "en": "The police promised to quickly <w>deal with</w> the street hooligans."
                            }
                        },
                        {
                            "text": "Я сейчас очень занят, но завтра обязательно <w>разберусь</w> с твоим вопросом.",
                            "target_lang_translations": {
                                "en": "I'm very busy right now, but tomorrow I will definitely <w>sort out</w> your issue."
                            }
                        }
                    ],
                    "common_phrases": [
                        "разобраться с делами",
                        "разобраться с проблемой",
                        "разобраться с долгами"
                    ],
                    "target_lang_definitions": {
                        "en": "To resolve a dispute, settle a matter, or deal with a problem or person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deal with",
                                "target_lang_sense_clarification": "to take action to solve a problem or handle a person"
                            },
                            {
                                "target_lang_word": "sort out",
                                "target_lang_sense_clarification": "to resolve a problem or difficult situation"
                            },
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "to manage a situation or person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7928d82b-e83c-33cf-b720-aed71ac26345",
                    "sense_definition": "Разложить свои вещи по местам, навести порядок (обычно после приезда или переезда).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "unpack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гостям дали время отдохнуть и <w>разобраться</w> с дороги перед обедом.",
                            "target_lang_translations": {
                                "en": "The guests were given time to rest and <w>unpack</w> after the journey before lunch."
                            }
                        },
                        {
                            "text": "Мне нужно <w>разобраться</w> в шкафу, потому что там полный беспорядок.",
                            "target_lang_translations": {
                                "en": "I need to <w>sort out</w> the closet because it's a complete mess in there."
                            }
                        }
                    ],
                    "synonyms": [
                        "устроиться"
                    ],
                    "antonyms": [
                        "собраться"
                    ],
                    "common_phrases": [
                        "разобраться в вещах",
                        "разобраться с дороги",
                        "разобраться в комнате"
                    ],
                    "target_lang_definitions": {
                        "en": "To put one's things in order or tidy up (usually after arriving or moving)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unpack",
                                "target_lang_sense_clarification": "to take things out of luggage after a journey"
                            },
                            {
                                "target_lang_word": "sort out",
                                "target_lang_sense_clarification": "to organize things or put them in order"
                            },
                            {
                                "target_lang_word": "tidy up",
                                "target_lang_sense_clarification": "to arrange things neatly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
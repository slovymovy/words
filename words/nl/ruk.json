    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4a0c83e4-f96a-3de4-8f06-0577ed6aee1b",
                    "sense_definition": "Een snelle, krachtige trekkende beweging.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De harde wind gaf plotseling een flinke <w>ruk</w> aan het open raam.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер внезапно сильно <w>дёрнул</w> открытое окно.",
                                "en": "The strong wind suddenly gave a sharp <w>jerk</w> to the open window."
                            }
                        },
                        {
                            "text": "Met een krachtige <w>ruk</w> trok hij het vastzittende touw eindelijk los.",
                            "target_lang_translations": {
                                "ru": "Резким <w>рывком</w> он наконец освободил застрявшую веревку.",
                                "en": "With a powerful <w>yank</w>, he finally pulled the stuck rope free."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een ruk geven",
                        "met een ruk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстрое, резкое тянущее движение.",
                        "en": "A quick, forceful pulling movement."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рывок",
                                "target_lang_sense_clarification": "резкое движение"
                            },
                            {
                                "target_lang_word": "дёрганье",
                                "target_lang_sense_clarification": "действие"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "jerk",
                                "target_lang_sense_clarification": "sudden sharp movement"
                            },
                            {
                                "target_lang_word": "yank",
                                "target_lang_sense_clarification": "strong pull"
                            },
                            {
                                "target_lang_word": "tug",
                                "target_lang_sense_clarification": "forceful pull"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efa97e47-e6b4-3727-8df2-735990c21b19",
                    "sense_definition": "Een aaneengesloten periode zonder onderbreking of pauze.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben de hele afstand naar Parijs in één <w>ruk</w> gereden.",
                            "target_lang_translations": {
                                "ru": "Мы проехали весь путь до Парижа в один <w>присест</w>.",
                                "en": "We drove the entire distance to Paris in one <w>go</w>."
                            }
                        },
                        {
                            "text": "Hij werkte in één <w>ruk</w> door om het project op tijd af te krijgen.",
                            "target_lang_translations": {
                                "ru": "Он работал без перерыва, в один <w>присест</w>, чтобы закончить проект вовремя.",
                                "en": "He worked straight through in one <w>go</w> to finish the project on time."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in één ruk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрерывный период времени без перерывов или пауз.",
                        "en": "A continuous period without interruption or pause."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присест",
                                "target_lang_sense_clarification": "в выражении 'в один присест'"
                            },
                            {
                                "target_lang_word": "рывок",
                                "target_lang_sense_clarification": "период усилия"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "in the phrase 'in one go'"
                            },
                            {
                                "target_lang_word": "stretch",
                                "target_lang_sense_clarification": "continuous period"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7411ac98-e2e4-3d76-a17e-f0e506a7fcfe",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>rukken</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>ruk</w> het onkruid met wortel en al uit de droge aarde.",
                            "target_lang_translations": {
                                "ru": "Я <w>вырываю</w> сорняки с корнем из сухой земли.",
                                "en": "I <w>yank</w> the weeds, root and all, out of the dry earth."
                            }
                        },
                        {
                            "text": "Voorzichtig <w>ruk</w> ik de pleister van mijn arm om de pijn te beperken.",
                            "target_lang_translations": {
                                "ru": "Я осторожно <w>срываю</w> пластырь с руки, чтобы уменьшить боль.",
                                "en": "I carefully <w>rip</w> the plaster off my arm to minimize the pain."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола <w>rukken</w> (дёргать, рвать).",
                        "en": "First person singular present tense of <w>rukken</w> (to yank, to jerk)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дёргаю",
                                "target_lang_sense_clarification": "резко тяну"
                            },
                            {
                                "target_lang_word": "вырываю",
                                "target_lang_sense_clarification": "извлекаю с силой"
                            },
                            {
                                "target_lang_word": "срываю",
                                "target_lang_sense_clarification": "удаляю рывком"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "yank",
                                "target_lang_sense_clarification": "pull sharply"
                            },
                            {
                                "target_lang_word": "rip",
                                "target_lang_sense_clarification": "pull off forcefully"
                            },
                            {
                                "target_lang_word": "jerk",
                                "target_lang_sense_clarification": "pull suddenly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59a95c88-71cc-301e-9778-f8b58211f4a7",
                    "sense_definition": "Gebiedende wijs van <w>rukken</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ruk</w> niet zo hard aan de deurklink, anders gaat hij nog kapot.",
                            "target_lang_translations": {
                                "ru": "Не <w>дёргай</w> так сильно за дверную ручку, иначе она сломается.",
                                "en": "Don't <w>yank</w> the door handle so hard, otherwise it might break."
                            }
                        },
                        {
                            "text": "<w>Ruk</w> het stuur om als je de auto niet meer onder controle hebt.",
                            "target_lang_translations": {
                                "ru": "<w>Рвани</w> руль, если потеряешь управление автомобилем.",
                                "en": "<w>Jerk</w> the steering wheel around if you lose control of the car."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола <w>rukken</w>.",
                        "en": "Imperative mood of <w>rukken</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дёргай",
                                "target_lang_sense_clarification": "тяни резко"
                            },
                            {
                                "target_lang_word": "рви",
                                "target_lang_sense_clarification": "тяни с силой"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "yank",
                                "target_lang_sense_clarification": "pull sharply"
                            },
                            {
                                "target_lang_word": "jerk",
                                "target_lang_sense_clarification": "pull suddenly"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "18b9d45e-ba9b-3441-ad9b-3f3392e61e66",
                    "sense_definition": "Van zeer slechte kwaliteit, vervelend of waardeloos.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het hele concert was gewoon best wel <w>ruk</w> door het slechte geluid.",
                            "target_lang_translations": {
                                "ru": "Весь концерт был довольно <w>отстойным</w> из-за плохого звука.",
                                "en": "The whole concert was just pretty <w>crap</w> because of the bad sound."
                            }
                        },
                        {
                            "text": "Ik vind het echt <w>ruk</w> dat we onze vakantie nu moeten annuleren.",
                            "target_lang_translations": {
                                "ru": "По-моему, это полный <w>отстой</w>, что нам теперь приходится отменять отпуск.",
                                "en": "I think it is really <w>crap</w> that we have to cancel our holiday now."
                            }
                        }
                    ],
                    "synonyms": [
                        "kut"
                    ],
                    "common_phrases": [
                        "echt ruk",
                        "best wel ruk"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "informal usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень плохого качества, неприятный или никчёмный.",
                        "en": "Of very bad quality, annoying, or worthless."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отстой",
                                "target_lang_sense_clarification": "сущ. в роли сказуемого"
                            },
                            {
                                "target_lang_word": "паршивый",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "хреновый",
                                "target_lang_sense_clarification": "грубое"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "crap",
                                "target_lang_sense_clarification": "bad quality"
                            },
                            {
                                "target_lang_word": "rubbish",
                                "target_lang_sense_clarification": "worthless"
                            },
                            {
                                "target_lang_word": "lame",
                                "target_lang_sense_clarification": "disappointing"
                            }
                        ]
                    }
                }
            ]

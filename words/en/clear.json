{
    "word_family": [
        "clarity",
        "clarify",
        "clearly",
        "clearance",
        "clearing",
        "clarification"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1deceb4b-fb04-3f78-95a0-d184b36c6386",
                    "sense_definition": "Free of ambiguity or doubt; easily understood.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "understandable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>clear</w> that we need to change our plan.",
                            "target_lang_translations": {
                                "ru": "<w>Я́сно</w>, что нам нужно менять план."
                            }
                        },
                        {
                            "text": "She gave <w>clear</w> instructions on how to get to the station.",
                            "target_lang_translations": {
                                "ru": "Она дала <w>поня́тные</w> инструкции, как добраться до станции."
                            }
                        }
                    ],
                    "antonyms": [
                        "obscure",
                        "nebulous"
                    ],
                    "common_phrases": [
                        "make it clear",
                        "crystal clear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишённый двусмысленности или сомнений; легко понимаемый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поня́тный",
                                "target_lang_sense_clarification": "доступный для понимания"
                            },
                            {
                                "target_lang_word": "я́сный",
                                "target_lang_sense_clarification": "очевидный, не вызывающий сомнений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0e8c471-6fff-3ad4-aa3d-760fa9562f7a",
                    "sense_definition": "Transparent in colour; allowing light to pass through so that objects behind can be distinctly seen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transparent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The water in the mountain lake was crystal <w>clear</w>.",
                            "target_lang_translations": {
                                "ru": "Вода в горном озере была кристально <w>чи́стой</w>."
                            }
                        },
                        {
                            "text": "Use <w>clear</w> tape to repair the torn page.",
                            "target_lang_translations": {
                                "ru": "Используйте <w>прозра́чную</w> ленту, чтобы заклеить порванную страницу."
                            }
                        }
                    ],
                    "synonyms": [
                        "transparent",
                        "pellucid"
                    ],
                    "antonyms": [
                        "opaque",
                        "cloudy",
                        "turbid"
                    ],
                    "common_phrases": [
                        "clear glass",
                        "clear water"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прозрачный по цвету; пропускающий свет так, что предметы позади отчётливо видны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозра́чный",
                                "target_lang_sense_clarification": "просвечивающий насквозь"
                            },
                            {
                                "target_lang_word": "чи́стый",
                                "target_lang_sense_clarification": "без примесей, прозрачный (о воде, стекле)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c0e6dcb-2427-370d-a824-c3193dca30af",
                    "sense_definition": "Of the sky or weather: free from clouds or mist.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a beautiful, <w>clear</w> day for a picnic.",
                            "target_lang_translations": {
                                "ru": "Это был прекрасный <w>я́сный</w> день для пикника."
                            }
                        },
                        {
                            "text": "We are hoping for <w>clear</w> skies tomorrow.",
                            "target_lang_translations": {
                                "ru": "Мы надеемся на <w>я́сное</w> небо завтра."
                            }
                        }
                    ],
                    "antonyms": [
                        "cloudy"
                    ],
                    "common_phrases": [
                        "clear sky",
                        "clear weather"
                    ],
                    "target_lang_definitions": {
                        "ru": "О небе или погоде: без облаков или тумана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "я́сный",
                                "target_lang_sense_clarification": "безоблачный, солнечный"
                            },
                            {
                                "target_lang_word": "чи́стый",
                                "target_lang_sense_clarification": "без облаков (о небе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf78684d-550e-3abb-a32a-e0555f0ac3bd",
                    "sense_definition": "Free of obstacles or anything that blocks the way.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unobstructed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wait until the intersection is <w>clear</w> before you drive across.",
                            "target_lang_translations": {
                                "ru": "Подождите, пока перекрёсток будет <w>свобо́ден</w>, прежде чем проезжать."
                            }
                        },
                        {
                            "text": "The driver thought the road was <w>clear</w>.",
                            "target_lang_translations": {
                                "ru": "Водитель думал, что дорога <w>свобо́дна</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the coast is clear",
                        "keep clear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободный от препятствий или всего, что преграждает путь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свобо́дный",
                                "target_lang_sense_clarification": "незанятый, без препятствий"
                            },
                            {
                                "target_lang_word": "чи́стый",
                                "target_lang_sense_clarification": "пустой, без посторонних предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f272e560-1658-3ece-a145-ef85daab9d56",
                    "sense_definition": "Easily or distinctly heard; audible.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "audible",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The radio signal was loud and <w>clear</w>.",
                            "target_lang_translations": {
                                "ru": "Радиосигнал был громким и <w>чётким</w>."
                            }
                        },
                        {
                            "text": "Please speak in a <w>clear</w> voice so everyone can hear you.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, говорите <w>я́сным</w> голосом, чтобы все могли вас слышать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "loud and clear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко или отчётливо слышимый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чёткий",
                                "target_lang_sense_clarification": "разборчивый, отчётливый"
                            },
                            {
                                "target_lang_word": "я́сный",
                                "target_lang_sense_clarification": "хорошо слышимый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1502b23-d0f2-3ec7-a090-4e63f6758910",
                    "sense_definition": "Free of guilt or suspicion.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "guilt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I can sleep well because I have a <w>clear</w> conscience.",
                            "target_lang_translations": {
                                "ru": "Я могу спать спокойно, потому что у меня <w>чи́стая</w> совесть."
                            }
                        },
                        {
                            "text": "He wants to be <w>clear</w> of all accusations before returning to work.",
                            "target_lang_translations": {
                                "ru": "Он хочет быть <w>свобо́ден</w> от всех обвинений перед возвращением на работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clear conscience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободный от вины или подозрений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чи́стый",
                                "target_lang_sense_clarification": "незапятнанный (о совести)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a589cbb-2397-3e3e-a8b8-a1b294092abc",
                    "sense_definition": "Without defects or blemishes, especially regarding skin.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "blemish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drinking plenty of water helps keep your skin <w>clear</w>.",
                            "target_lang_translations": {
                                "ru": "Обильное питьё помогает сохранять кожу <w>чи́стой</w>."
                            }
                        },
                        {
                            "text": "She has a beautiful, <w>clear</w> complexion.",
                            "target_lang_translations": {
                                "ru": "У неё красивая, <w>чи́стая</w> кожа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clear skin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без дефектов или изъянов, особенно в отношении кожи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чи́стый",
                                "target_lang_sense_clarification": "без прыщей или пятен"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4b228dfc-c7c0-3f9e-9527-073c1b55119d",
                    "sense_definition": "To remove obstructions, impediments, or unwanted items from a place or surface.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "remove",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>clear</w> the table after we finish dinner.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>убери́</w> со стола, когда мы закончим ужинать."
                            }
                        },
                        {
                            "text": "It took hours to <w>clear</w> the snow from the driveway.",
                            "target_lang_translations": {
                                "ru": "Потребовались часы, чтобы <w>убра́ть</w> снег с подъездной дорожки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clear the table",
                        "clear the way",
                        "clear the room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убирать препятствия или ненужные предметы с места или поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убира́ть",
                                "target_lang_sense_clarification": "удалять лишнее"
                            },
                            {
                                "target_lang_word": "очища́ть",
                                "target_lang_sense_clarification": "делать чистым, освобождать от чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2f01b94-df9d-3422-b237-d0a4e64a4b6f",
                    "sense_definition": "To become free from obstruction, clouds, or mist; to become transparent or bright.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "become_bright",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sky <w>cleared</w> nicely after the heavy rain.",
                            "target_lang_translations": {
                                "ru": "Небо прекрасно <w>проясни́лось</w> после сильного дождя."
                            }
                        },
                        {
                            "text": "The fog should <w>clear</w> by the middle of the morning.",
                            "target_lang_translations": {
                                "ru": "Туман должен <w>рассе́яться</w> к середине утра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clear up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Освобождаться от препятствий, облаков или тумана; становиться прозрачным или светлым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проясня́ться",
                                "target_lang_sense_clarification": "становиться ясным (о небе, погоде)"
                            },
                            {
                                "target_lang_word": "рассе́иваться",
                                "target_lang_sense_clarification": "исчезать (о тумане, дыме)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2edc6ecb-aaab-340d-ad2b-89850bc0af90",
                    "sense_definition": "To eliminate ambiguity or doubt from a matter; to explain or resolve.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "resolve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>clear</w> up this misunderstanding immediately.",
                            "target_lang_translations": {
                                "ru": "Нам нужно немедленно <w>проясни́ть</w> это недоразумение."
                            }
                        },
                        {
                            "text": "The teacher helped <w>clear</w> up the confusion about the assignment.",
                            "target_lang_translations": {
                                "ru": "Учитель помог <w>разъясни́ть</w> путаницу с заданием."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clear up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устранять двусмысленность или сомнения; объяснять или разрешать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проясни́ть",
                                "target_lang_sense_clarification": "внести ясность"
                            },
                            {
                                "target_lang_word": "разъясни́ть",
                                "target_lang_sense_clarification": "растолковать, сделать понятным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c226804-c29d-34d3-b2e8-43ba48bc1dc9",
                    "sense_definition": "To approve or authorize for a particular purpose or action.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "authorize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Air traffic control <w>cleared</w> the plane to land.",
                            "target_lang_translations": {
                                "ru": "Авиадиспетчер <w>дал разреше́ние</w> самолёту на посадку."
                            }
                        },
                        {
                            "text": "My manager has to <w>clear</w> the project before we can start.",
                            "target_lang_translations": {
                                "ru": "Мой менеджер должен <w>одо́брить</w> проект, прежде чем мы сможем начать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cleared for takeoff"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одобрять или давать разрешение для определённой цели или действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разреша́ть",
                                "target_lang_sense_clarification": "давать право на действие"
                            },
                            {
                                "target_lang_word": "дава́ть добро́",
                                "target_lang_sense_clarification": "неформальное одобрение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55fd7aea-1cc4-3765-96fc-ecda710ff432",
                    "sense_definition": "To pass by or over something without touching it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pass_over",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The horse easily <w>cleared</w> the fence.",
                            "target_lang_translations": {
                                "ru": "Лошадь легко <w>перепры́гнула</w> через забор."
                            }
                        },
                        {
                            "text": "Make sure the truck can <w>clear</w> the low bridge.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что грузовик сможет <w>пройти́</w> под низким мостом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить мимо или над чем-то, не касаясь этого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преодолева́ть",
                                "target_lang_sense_clarification": "справляться с препятствием"
                            },
                            {
                                "target_lang_word": "проходи́ть",
                                "target_lang_sense_clarification": "двигаться мимо или сквозь без столкновения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ece92e7-4283-378d-8a1c-0fea31abac97",
                    "sense_definition": "To remove from suspicion, especially of having committed a crime.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "exonerate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "New DNA evidence helped <w>clear</w> him of the crime.",
                            "target_lang_translations": {
                                "ru": "Новые доказательства ДНК помогли <w>оправда́ть</w> его в преступлении."
                            }
                        },
                        {
                            "text": "She is working hard to <w>clear</w> her name.",
                            "target_lang_translations": {
                                "ru": "Она усердно работает, чтобы <w>обели́ть</w> своё имя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clear one's name"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снимать подозрения, особенно в совершении преступления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оправда́ть",
                                "target_lang_sense_clarification": "признать невиновным"
                            },
                            {
                                "target_lang_word": "снять обвине́ния",
                                "target_lang_sense_clarification": "освободить от подозрений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edf2e49f-68bb-33cb-a60d-8619f7e92b65",
                    "sense_definition": "Of a check or financial transaction: to be processed successfully so that funds are transferred.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It will take a few days for the check to <w>clear</w>.",
                            "target_lang_translations": {
                                "ru": "Потребуется несколько дней, чтобы чек <w>прошёл</w>."
                            }
                        },
                        {
                            "text": "We cannot ship the goods until the payment has <w>cleared</w>.",
                            "target_lang_translations": {
                                "ru": "Мы не можем отправить товар, пока платёж не <w>пройдёт</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О чеке или финансовой операции: быть успешно обработанным для перевода средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пройти́",
                                "target_lang_sense_clarification": "быть обработанным (о платеже)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c78ea9c3-c71f-377e-af24-b89bd8f10a02",
                    "sense_definition": "Not near something or touching it; at a safe distance.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "distance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please stand <w>clear</w> of the closing doors.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отойдите <w>в сто́рону</w> от закрывающихся дверей."
                            }
                        },
                        {
                            "text": "You should steer <w>clear</w> of that dangerous area.",
                            "target_lang_translations": {
                                "ru": "Вам следует держаться <w>пода́льше</w> от этой опасной зоны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stand clear",
                        "steer clear",
                        "keep clear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не рядом с чем-то и не касаясь его; на безопасном расстоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в стороне́",
                                "target_lang_sense_clarification": "на расстоянии"
                            },
                            {
                                "target_lang_word": "пода́льше",
                                "target_lang_sense_clarification": "на некотором удалении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3699b48a-a6fb-30fb-b667-e4a864fb8652",
                    "sense_definition": "All the way; entirely.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "entirely",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He kicked the ball <w>clear</w> over the roof of the house.",
                            "target_lang_translations": {
                                "ru": "Он пнул мяч <w>пря́мо</w> через крышу дома."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "151d460b-7101-3bbc-b50d-890241f240f2",
                    "sense_definition": "The physical world, including plants, animals, the landscape, and natural phenomena, as opposed to humans or human creations.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_world",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went for a long walk to enjoy the beauty of <w>nature</w>.",
                            "target_lang_translations": {
                                "ru": "Мы отправились на долгую прогулку, чтобы насладиться красотой <w>природы</w>."
                            }
                        },
                        {
                            "text": "He loves taking photographs of <w>nature</w>, especially in the mountains.",
                            "target_lang_translations": {
                                "ru": "Он любит фотографировать <w>природу</w>, особенно в горах."
                            }
                        }
                    ],
                    "synonyms": [
                        "caliber",
                        "essence"
                    ],
                    "common_phrases": [
                        "nature reserve",
                        "nature lover",
                        "back to nature",
                        "in nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физический мир, включая растения, животных, ландшафт и природные явления, в отличие от созданного человеком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "природа",
                                "target_lang_sense_clarification": "окружающий мир, флора и фауна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2d7afbf-d52d-31db-a143-d6375adb1ea9",
                    "sense_definition": "The basic or inherent features, character, or qualities of a person or thing.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is simply human <w>nature</w> to want to be loved and accepted.",
                            "target_lang_translations": {
                                "ru": "Это просто человеческая <w>природа</w> — хотеть быть любимым и принятым."
                            }
                        },
                        {
                            "text": "She has a very kind <w>nature</w> and always helps others.",
                            "target_lang_translations": {
                                "ru": "У неё очень добрая <w>натура</w>, и она всегда помогает другим."
                            }
                        }
                    ],
                    "synonyms": [
                        "essence",
                        "caliber"
                    ],
                    "common_phrases": [
                        "human nature",
                        "by nature",
                        "second nature",
                        "in the nature of things"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основные или врожденные черты, характер или качества человека или вещи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "натура",
                                "target_lang_sense_clarification": "характер, темперамент человека"
                            },
                            {
                                "target_lang_word": "природа",
                                "target_lang_sense_clarification": "врожденная сущность, естество"
                            },
                            {
                                "target_lang_word": "сущность",
                                "target_lang_sense_clarification": "глубинное содержание, основа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "650278e6-fc58-3d44-b58f-6c2fa081c358",
                    "sense_definition": "A particular kind, sort, or class of thing or situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot discuss the exact <w>nature</w> of my work with you.",
                            "target_lang_translations": {
                                "ru": "Я не могу обсуждать с вами точный <w>характер</w> моей работы."
                            }
                        },
                        {
                            "text": "Problems of this <w>nature</w> are often difficult to solve quickly.",
                            "target_lang_translations": {
                                "ru": "Проблемы такого <w>рода</w> часто трудно решить быстро."
                            }
                        }
                    ],
                    "synonyms": [
                        "caliber",
                        "essence"
                    ],
                    "common_phrases": [
                        "of this nature",
                        "exact nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особый вид, сорт или класс вещей или ситуаций; суть дела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "характер",
                                "target_lang_sense_clarification": "свойства, особенности ситуации или деятельности"
                            },
                            {
                                "target_lang_word": "род",
                                "target_lang_sense_clarification": "тип, разновидность (в выражениях типа \"такого рода\")"
                            },
                            {
                                "target_lang_word": "суть",
                                "target_lang_sense_clarification": "главное содержание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4f1d3e2-58bd-3a59-b884-f3c0750f3306",
                    "sense_definition": "The physical need to urinate or defecate.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had to stop the car at the next exit to answer the call of <w>nature</w>.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось остановить машину на следующем съезде, чтобы ответить на зов <w>природы</w>."
                            }
                        },
                        {
                            "text": "When the call of <w>nature</w> comes, you simply have to go.",
                            "target_lang_translations": {
                                "ru": "Когда раздается зов <w>природы</w>, нужно просто идти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "call of nature"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used euphemistically"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Физиологическая потребность в мочеиспускании или дефекации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "природа",
                                "target_lang_sense_clarification": "в идиоме \"зов природы\""
                            },
                            {
                                "target_lang_word": "нужда",
                                "target_lang_sense_clarification": "физиологическая потребность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "8c548ea7-b952-3625-a45d-7238cbeadfc3",
                    "sense_definition": "The natural forces of the universe personified as a controlling power or mother figure.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "other",
                    "examples": [
                        {
                            "text": "Mother <w>Nature</w> can be both beautiful and cruel at the same time.",
                            "target_lang_translations": {
                                "ru": "Матушка-<w>Природа</w> может быть одновременно прекрасной и жестокой."
                            }
                        },
                        {
                            "text": "Farmers know that they must work in harmony with <w>Nature</w> to succeed.",
                            "target_lang_translations": {
                                "ru": "Фермеры знают, что для успеха они должны работать в гармонии с <w>Природой</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Mother Nature",
                        "force of Nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Природные силы вселенной, олицетворяемые как управляющая сила или материнская фигура."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Природа",
                                "target_lang_sense_clarification": "персонифицированная сила (часто с большой буквы)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
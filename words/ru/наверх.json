            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "61974af7-6c65-3657-a32c-f9da7797fc2e",
                    "sense_definition": "В направлении вверх, в более высокое положение",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Посмотри <наверх>, там летит большая птица.",
                            "target_lang_translations": {
                                "en": "Look <up>, a big bird is flying there."
                            }
                        },
                        {
                            "text": "Мы медленно поднимались <наверх> по крутому склону горы.",
                            "target_lang_translations": {
                                "en": "We were slowly climbing <up> the steep mountain slope."
                            }
                        }
                    ],
                    "synonyms": [
                        "вверх",
                        "кверху",
                        "ввысь"
                    ],
                    "antonyms": [
                        "вниз"
                    ],
                    "common_phrases": [
                        "посмотреть наверх",
                        "руки наверх",
                        "поднять наверх"
                    ],
                    "target_lang_definitions": {
                        "en": "In an upward direction, to a higher position"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "up",
                                "target_lang_sense_clarification": "direction"
                            },
                            {
                                "target_lang_word": "upwards",
                                "target_lang_sense_clarification": "direction"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30a4e9a6-73f2-363c-89de-50e771c38b26",
                    "sense_definition": "На верхний этаж дома или здания",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети побежали <наверх> в свою комнату.",
                            "target_lang_translations": {
                                "en": "The children ran <upstairs> to their room."
                            }
                        },
                        {
                            "text": "Лифт едет <наверх>, на десятый этаж.",
                            "target_lang_translations": {
                                "en": "The elevator is going <up> to the tenth floor."
                            }
                        }
                    ],
                    "synonyms": [
                        "вверх"
                    ],
                    "antonyms": [
                        "вниз"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f64d03f-7212-390f-aded-1c059000363b",
                    "sense_definition": "Dobrać dwa lub więcej elementów tak, aby tworzyły spójną całość pod względem wyglądu, rozmiaru lub kształtu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_matching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <w>dopasować</w> krawat do koloru swojej nowej marynarki.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>подобрать</w> галстук под цвет своего нового пиджака."
                            }
                        },
                        {
                            "text": "Stolarz musiał precyzyjnie <w>dopasować</w> do siebie wszystkie drewniane elementy.",
                            "target_lang_translations": {
                                "ru": "Плотник должен был точно <w>подогнать</w> друг к другу все деревянные элементы."
                            }
                        }
                    ],
                    "synonyms": [
                        "podpasować się"
                    ],
                    "common_phrases": [
                        "dopasować ubrania",
                        "dopasować kolor",
                        "idealnie dopasować",
                        "dopasować rozmiar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подобрать или соединить элементы так, чтобы они соответствовали друг другу по виду, размеру или форме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подобрать",
                                "target_lang_sense_clarification": "найти подходящее сочетание (напр. одежды)"
                            },
                            {
                                "target_lang_word": "подогнать",
                                "target_lang_sense_clarification": "изменить размер или форму для точного соединения"
                            },
                            {
                                "target_lang_word": "сочетать",
                                "target_lang_sense_clarification": "соединить гармонично"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ad988de-a1f7-34d2-beea-8c7b42c30bc5",
                    "sense_definition": "Zmodyfikować lub ustawić coś w taki sposób, aby spełniało określone wymagania, potrzeby lub warunki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_adjustment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma musi <w>dopasować</w> swoją ofertę do zmieniających się potrzeb rynku.",
                            "target_lang_translations": {
                                "ru": "Компания должна <w>адаптировать</w> свое предложение к меняющимся потребностям рынка."
                            }
                        },
                        {
                            "text": "Kierowca powinien zawsze <w>dopasować</w> prędkość jazdy do warunków pogodowych.",
                            "target_lang_translations": {
                                "ru": "Водитель всегда должен <w>подстраивать</w> скорость езды под погодные условия."
                            }
                        }
                    ],
                    "synonyms": [
                        "doregulować"
                    ],
                    "common_phrases": [
                        "dopasować ofertę",
                        "dopasować budżet",
                        "dopasować termin",
                        "dopasować plan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить или настроить что-либо так, чтобы оно отвечало определенным требованиям или условиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адаптировать",
                                "target_lang_sense_clarification": "изменить под новые условия"
                            },
                            {
                                "target_lang_word": "приспособить",
                                "target_lang_sense_clarification": "сделать пригодным для чего-либо"
                            },
                            {
                                "target_lang_word": "подстроить",
                                "target_lang_sense_clarification": "отрегулировать в соответствии с чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2982f3c-306e-339b-92bf-709957e72591",
                    "sense_definition": "Zmienić swoje zachowanie, wygląd lub przyzwyczajenia, aby być zgodnym z panującymi zasadami, grupą ludzi lub nową sytuacją (używane z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_adaptation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy pracownik starał się szybko <w>dopasować</w> się do kultury organizacyjnej firmy.",
                            "target_lang_translations": {
                                "ru": "Новый сотрудник старался быстро <w>адаптироваться</w> к организационной культуре компании."
                            }
                        },
                        {
                            "text": "Trudno jest się <w>dopasować</w> do otoczenia, gdy nie zna się lokalnego języka.",
                            "target_lang_translations": {
                                "ru": "Трудно <w>приспособиться</w> к окружению, когда не знаешь местного языка."
                            }
                        }
                    ],
                    "synonyms": [
                        "podpasować się"
                    ],
                    "antonyms": [
                        "wyróżniać się"
                    ],
                    "common_phrases": [
                        "dopasować się do sytuacji",
                        "dopasować się do grupy",
                        "dopasować się do otoczenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить свое поведение или привычки, чтобы соответствовать правилам, группе людей или новой ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приспособиться",
                                "target_lang_sense_clarification": "привыкнуть и адаптироваться"
                            },
                            {
                                "target_lang_word": "подстроиться",
                                "target_lang_sense_clarification": "изменить свое поведение под кого-то"
                            },
                            {
                                "target_lang_word": "вписаться",
                                "target_lang_sense_clarification": "стать частью коллектива или обстановки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

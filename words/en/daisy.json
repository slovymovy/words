        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "66636a85-533f-3399-86bf-fc2cd5edaf7c",
                    "sense_definition": "A wild flowering plant of the species Bellis perennis, typically having a yellow center and white petals.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "plants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She picked a small white <w>daisy</w> from the grass.",
                            "target_lang_translations": {
                                "ru": "Она сорвала в траве маленькую белую <w>маргаритку</w>."
                            }
                        },
                        {
                            "text": "The meadow was covered in beautiful <w>daisies</w>.",
                            "target_lang_translations": {
                                "ru": "Луг был усеян красивыми <w>маргаритками</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fresh as a daisy",
                        "pushing up daisies",
                        "oops-a-daisy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дикий цветок вида Bellis perennis, обычно с желтой серединой и белыми лепестками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маргаритка",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]

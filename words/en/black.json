                    "antonyms": [
                        "white"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often capitalized"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель этнических групп, обычно имеющих тёмную пигментацию кожи."
                    },
                    "translations": {
                        "ru": [

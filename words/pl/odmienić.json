                    "sense_id": "7e2a9f91-ab57-3e4d-a5ed-95d9d9ca1f30",
                    "sense_definition": "Sprawić, że ktoś lub coś stanie się inne, zmieni swój wygląd, charakter lub naturę",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta długa podróż całkowicie <odmieniła> jego podejście do życia.",
                            "target_lang_translations": {
                                "ru": "Это долгое путешествие полностью <изменило> его отношение к жизни."
                            }
                        },
                        {
                            "text": "Nowa fryzura i makijaż bardzo ją <odmieniły>.",
                            "target_lang_translations": {
                                "ru": "Новая прическа и макияж сильно её <преобразили>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmienić",
                        "przeobrazić",
                        "przekształcić"
                    ],
                    "antonyms": [
                        "zachować",
                        "utrzymać"
                    ],
                    "common_phrases": [
                        "odmienić los",
                        "odmienić życie",
                        "odmienić oblicze",
                        "odmienić wygląd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать кого-то или что-то иным, изменить внешний вид, характер или сущность"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изменить",
                                "target_lang_sense_clarification": "сделать другим"
                            },

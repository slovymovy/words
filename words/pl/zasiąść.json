{
    "word_family": [
        "zasiadać",
        "zasiadanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "41ccb3e6-b0a8-3ae1-9ff8-e6d569f9fa1c",
                    "sense_definition": "Usiąść, zajmując miejsce, często wygodnie lub na dłuższy czas.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sitting_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długim spacerze dziadek <zasiadł> w swoim ulubionym fotelu.",
                            "target_lang_translations": {
                                "ru": "После долгой прогулки дедушка <уселся> в своё любимое кресло."
                            }
                        },
                        {
                            "text": "Goście <zasiedli> w salonie i zaczęli rozmawiać o pogodzie.",
                            "target_lang_translations": {
                                "ru": "Гости <расселись> в гостиной и начали разговаривать о погоде."
                            }
                        }
                    ],
                    "synonyms": [
                        "usiąść",
                        "usadowić się"
                    ],
                    "antonyms": [
                        "wstać"
                    ],
                    "common_phrases": [
                        "zasiąść w fotelu",
                        "zasiąść wygodnie",
                        "zasiąść przed telewizorem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сесть, заняв место, часто удобно или на продолжительное время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сесть",
                                "target_lang_sense_clarification": "занять место"
                            },
                            {
                                "target_lang_word": "усесться",
                                "target_lang_sense_clarification": "устроиться удобно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0893897-ba2a-38ff-b68e-af856dd05307",
                    "sense_definition": "Usiąść w celu wykonania określonej czynności, na przykład jedzenia lub pracy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sitting_purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy domownicy <zasiedli> do uroczystego obiadu w jadalni.",
                            "target_lang_translations": {
                                "ru": "Все домочадцы <сели> за праздничный обед в столовой."
                            }
                        },
                        {
                            "text": "Muszę w końcu <zasiąść> do nauki, bo jutro mam ważny egzamin.",
                            "target_lang_translations": {
                                "ru": "Я должен наконец <сесть> за учёбу, так как завтра у меня важный экзамен."
                            }
                        }
                    ],
                    "synonyms": [
                        "usiąść"
                    ],
                    "antonyms": [
                        "wstać"
                    ],
                    "common_phrases": [
                        "zasiąść do stołu",
                        "zasiąść do posiłku",
                        "zasiąść do pracy",
                        "zasiąść do rozmów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сесть с целью выполнения определенного действия, например, еды или работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сесть",
                                "target_lang_sense_clarification": "за работу, за стол"
                            },
                            {
                                "target_lang_word": "приступить",
                                "target_lang_sense_clarification": "начать действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "998dbda5-9967-37af-9de7-aaabddfb135d",
                    "sense_definition": "Objąć oficjalną funkcję, stając się członkiem gremium, rady lub wstępując na tron.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "official_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy władca <zasiadł> na tronie po śmierci swojego ojca.",
                            "target_lang_translations": {
                                "ru": "Новый правитель <взошёл> на трон после смерти своего отца."
                            }
                        },
                        {
                            "text": "Wielu wybitnych ekspertów <zasiadło> w komisji oceniającej projekty.",
                            "target_lang_translations": {
                                "ru": "Многие выдающиеся эксперты <вошли в состав> комиссии, оценивающей проекты."
                            }
                        }
                    ],
                    "synonyms": [
                        "przystąpić"
                    ],
                    "antonyms": [
                        "ustąpić"
                    ],
                    "common_phrases": [
                        "zasiąść na tronie",
                        "zasiąść w radzie",
                        "zasiąść w komitecie",
                        "zasiąść w ławie przysięgłych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занять официальную должность, став членом органа, совета или взойдя на престол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взойти",
                                "target_lang_sense_clarification": "на трон"
                            },
                            {
                                "target_lang_word": "войти в состав",
                                "target_lang_sense_clarification": "совета, комиссии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
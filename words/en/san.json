                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "san_people_group",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Linguists are working to document the complex click sounds found in <w>San</w>.",
                            "target_lang_translations": {
                                "ru": "Лингвисты работают над документированием сложных щелкающих звуков, встречающихся в <w>языках сан</w>."
                            }
                        },
                        {
                            "text": "Very few people speak <w>San</w> fluently in the modern era.",
                            "target_lang_translations": {
                                "ru": "В наше время очень мало людей свободно говорят на <w>языке сан</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "San languages"
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "krieuwelig",
        "krieuwel"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1f2fa031-6512-305f-9eed-798126835e15",
                    "sense_definition": "Zich in grote aantallen kriskras door elkaar bewegen, vaak gezegd van kleine dieren of mensenmenigtes.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De mierenhoop <w>krieuwelde</w> van het leven in de zomerzon.",
                            "target_lang_translations": {
                                "ru": "Муравейник <w>кишел</w> жизнью под летним солнцем."
                            }
                        },
                        {
                            "text": "Het <w>krieuwelt</w> hier van de toeristen tijdens het hoogseizoen.",
                            "target_lang_translations": {
                                "ru": "В разгар сезона здесь <w>кишит</w> туристами."
                            }
                        }
                    ],
                    "synonyms": [
                        "krioelen",
                        "wemelen"
                    ],
                    "common_phrases": [
                        "krieuwelen van de"
                    ],
                    "target_lang_definitions": {
                        "ru": "Беспорядочно двигаться в большом количестве (о насекомых или толпе); кишеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кишеть",
                                "target_lang_sense_clarification": "быть переполненным множеством движущихся существ"
                            },
                            {
                                "target_lang_word": "копошиться",
                                "target_lang_sense_clarification": "беспорядочно двигаться, возиться (обычно о множестве мелких существ)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eeb1d4d0-aade-3258-8f55-377e422a7ab0",
                    "sense_definition": "Een licht prikkelend of jeukend gevoel op de huid veroorzaken of ervaren.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik voel iets <w>krieuwelen</w> in mijn nek, is het een vlieg?",
                            "target_lang_translations": {
                                "ru": "Я чувствую, как что-то <w>щекочет</w> мне шею, это муха?"
                            }
                        },
                        {
                            "text": "De wollen trui <w>krieuwelde</w> vreselijk op zijn blote huid.",
                            "target_lang_translations": {
                                "ru": "Шерстяной свитер ужасно <w>кололся</w> на голое тело."
                            }
                        }
                    ],
                    "synonyms": [
                        "kriebelen",
                        "jeuken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать или испытывать легкое щекочущее, колющее или зудящее ощущение на коже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щекотать",
                                "target_lang_sense_clarification": "вызывать легкое раздражение кожи движением"
                            },
                            {
                                "target_lang_word": "колоться",
                                "target_lang_sense_clarification": "вызывать покалывание (о ткани, например, шерсти)"
                            },
                            {
                                "target_lang_word": "зудеть",
                                "target_lang_sense_clarification": "вызывать навязчивое ощущение чесотки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec689471-bd8b-3d12-bca2-7c0149e92551",
                    "sense_definition": "Met heel kleine, onduidelijke en dicht op elkaar staande letters schrijven.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "writing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>krieuwelde</w> snel nog wat aantekeningen in de kantlijn van het boek.",
                            "target_lang_translations": {
                                "ru": "Он быстро <w>начеркал</w> еще несколько заметок на полях книги."
                            }
                        },
                        {
                            "text": "Zit niet zo te <w>krieuwelen</w>, anders kan niemand je handschrift lezen.",
                            "target_lang_translations": {
                                "ru": "Не надо так <w>мельчить</w>, иначе никто не сможет прочитать твой почерк."
                            }
                        }
                    ],
                    "synonyms": [
                        "priegelen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Писать очень мелкими, неразборчивыми и тесно стоящими буквами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мельчить",
                                "target_lang_sense_clarification": "писать слишком мелко"
                            },
                            {
                                "target_lang_word": "черкать",
                                "target_lang_sense_clarification": "писать быстро, небрежно и мелко"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
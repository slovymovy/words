                    "common_phrases": [
                        "собрать чемодан",
                        "собрать в дорогу",
                        "собрать ребёнка",
                        "собрать сумку"
                    ],
                    "target_lang_definitions": {
                        "en": "To prepare someone or something for a trip or activity (e.g., packing things)."
                    },
                    "translations": {

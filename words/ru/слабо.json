    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "410abb9f-d6d2-34d8-b8cd-a1b5a0645679",
                    "sense_definition": "С небольшой силой, интенсивностью или энергией; едва заметно.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ветер дул <w>слабо</w>, едва шевеля жёлтые листья на деревьях.",
                            "target_lang_translations": {
                                "en": "The wind blew <w>weakly</w>, barely stirring the yellow leaves on the trees."
                            }
                        },
                        {
                            "text": "Мне <w>слабо</w> верится, что они успеют закончить работу вовремя.",
                            "target_lang_translations": {
                                "en": "I find it <w>hard</w> to believe that they will manage to finish the work on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "несильно",
                        "хило"
                    ],
                    "antonyms": [
                        "сильно"
                    ],
                    "common_phrases": [
                        "слабо верится",
                        "слабо выраженный",
                        "слабо разбираться"
                    ],
                    "target_lang_definitions": {
                        "en": "With little force, intensity, or energy; barely noticeably."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weakly",
                                "target_lang_sense_clarification": "general lack of strength or force"
                            },
                            {
                                "target_lang_word": "faintly",
                                "target_lang_sense_clarification": "barely perceptible (sound, light, belief)"
                            },
                            {
                                "target_lang_word": "feebly",
                                "target_lang_sense_clarification": "lacking physical strength or effectiveness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "094dd0b5-f947-3d20-90e6-12fef88b272e",
                    "sense_definition": "Неудачно, неудовлетворительно, на низком уровне качества.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша команда выступила очень <w>слабо</w> в этом сезоне и заняла последнее место.",
                            "target_lang_translations": {
                                "en": "Our team performed very <w>poorly</w> this season and took last place."
                            }
                        },
                        {
                            "text": "Студент <w>слабо</w> подготовился к экзамену и получил низкую оценку.",
                            "target_lang_translations": {
                                "en": "The student prepared <w>poorly</w> for the exam and received a low grade."
                            }
                        }
                    ],
                    "synonyms": [
                        "плохо",
                        "неудачно"
                    ],
                    "antonyms": [
                        "хорошо"
                    ],
                    "common_phrases": [
                        "слабо сыграть",
                        "слабо подготовиться"
                    ],
                    "target_lang_definitions": {
                        "en": "Unsuccessfully, unsatisfactorily, or of poor quality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "poorly",
                                "target_lang_sense_clarification": "badly done or insufficient"
                            },
                            {
                                "target_lang_word": "weakly",
                                "target_lang_sense_clarification": "lacking substance or quality"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e52b5173-b724-3e13-9eac-1cb17d64fcbf",
                    "sense_definition": "Отсутствие смелости или способности сделать что-либо (часто используется как вызов в споре).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dare",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спорим, что тебе <w>слабо</w> поднять этот тяжёлый чемодан одной рукой?",
                            "target_lang_translations": {
                                "en": "I bet you <w>can't</w> lift this heavy suitcase with one hand?"
                            }
                        },
                        {
                            "text": "Друзья взяли его на <w>слабо</w>, и он согласился прыгнуть с вышки.",
                            "target_lang_translations": {
                                "en": "His friends <w>dared</w> him, and he agreed to jump from the tower."
                            }
                        }
                    ],
                    "common_phrases": [
                        "тебе слабо",
                        "взять на слабо",
                        "мне не слабо"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in dares and challenges"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Lack of courage or ability to do something (often used as a challenge)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dare",
                                "target_lang_sense_clarification": "used in phrases like 'take on a dare'"
                            },
                            {
                                "target_lang_word": "can't",
                                "target_lang_sense_clarification": "used in challenges like 'bet you can't'"
                            },
                            {
                                "target_lang_word": "chicken",
                                "target_lang_sense_clarification": "slang for lacking courage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "476ea145-0dad-31b4-950a-c31771c3681b",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>слабый</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зрение у старика было уже <w>слабо</w>, и он носил очки с толстыми стёклами.",
                            "target_lang_translations": {
                                "en": "The old man's eyesight was already <w>weak</w>, and he wore glasses with thick lenses."
                            }
                        },
                        {
                            "text": "Это утешение слишком <w>слабо</w> для человека в таком большом горе.",
                            "target_lang_translations": {
                                "en": "This consolation is too <w>weak</w> for a person in such great grief."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short adjective form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short form of the neuter singular adjective 'weak'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weak",
                                "target_lang_sense_clarification": "lacking strength or intensity"
                            }
                        ]
                    }
                }
            ]
        }
    ]

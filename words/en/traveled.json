                {
                    "sense_id": "4ff71b52-e192-3282-a626-66a5ea6bcbb9",
                    "sense_definition": "Simple past and past participle of <w>travel</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>traveled</w> by train to the coast last weekend.",
                            "target_lang_translations": {
                                "ru": "В прошлые выходные мы <w>ездили</w> на поезде на побережье."
                            }
                        },
                        {
                            "text": "She has <w>traveled</w> to many different countries for her job.",
                            "target_lang_translations": {
                                "ru": "Она <w>путешествовала</w> по многим разным странам по работе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "traveled abroad",
                        "traveled far",
                        "traveled alone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола travel (путешествовать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путешествовал",
                                "target_lang_sense_clarification": "совершал путешествие"
                            },
                            {
                                "target_lang_word": "ездил",
                                "target_lang_sense_clarification": "перемещался транспортом"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "60e288e0-93ca-38c7-a0f5-2b528861bc03",
                    "sense_definition": "Frequented by travelers; used often by people going somewhere.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to take the less <w>traveled</w> road to avoid the heavy traffic.",
                            "target_lang_translations": {
                                "ru": "Мы решили выбрать менее <w>оживленную</w> дорогу, чтобы избежать пробок."
                            }
                        },
                        {
                            "text": "This hiking trail is heavily <w>traveled</w> during the summer months.",
                            "target_lang_translations": {
                                "ru": "Эта туристическая тропа очень <w>посещаема</w> в летние месяцы."
                            }
                        }
                    ],
                    "synonyms": [
                        "frequented",
                        "busy"
                    ],
                    "antonyms": [
                        "deserted",
                        "unfrequented"
                    ],
                    "common_phrases": [
                        "well-traveled road",
                        "heavily traveled",
                        "less traveled path"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часто посещаемый путешественниками; активно используемый для передвижения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оживленный",
                                "target_lang_sense_clarification": "о дороге или маршруте с интенсивным движением"
                            },
                            {
                                "target_lang_word": "посещаемый",
                                "target_lang_sense_clarification": "популярный среди людей"
                            },
                            {
                                "target_lang_word": "исхоженный",
                                "target_lang_sense_clarification": "о тропе, по которой часто ходят"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57614d0c-391a-3ae5-b095-7d8fbc1b71d0",
                    "sense_definition": "Having traveled to many places; experienced in travel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As a widely <w>traveled</w> journalist, she has seen many parts of the world.",
                            "target_lang_translations": {
                                "ru": "Как много <w>путешествовавший</w> журналист, она повидала многие уголки мира."
                            }
                        },
                        {
                            "text": "He is a <w>traveled</w> man who can speak several languages fluently.",
                            "target_lang_translations": {
                                "ru": "Он — <w>бывалый</w> человек, который свободно говорит на нескольких языках."
                            }
                        }
                    ],
                    "synonyms": [
                        "worldly",
                        "experienced"
                    ],
                    "antonyms": [
                        "unworldly"
                    ],
                    "common_phrases": [
                        "widely traveled",
                        "much traveled"
                    ],
                    "target_lang_definitions": {
                        "ru": "Побывавший во многих местах; имеющий большой опыт путешествий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "много путешествовавший",
                                "target_lang_sense_clarification": "имеющий опыт поездок"
                            },
                            {
                                "target_lang_word": "бывалый",
                                "target_lang_sense_clarification": "опытный, видавший виды"
                            },
                            {
                                "target_lang_word": "повидавший мир",

                    }
                },
                {
                    "sense_id": "1afb59a6-c307-306b-aa6a-bed2d9e83de9",
                    "sense_definition": "Dotrzymywać zobowiązania, obietnicy lub pozostawać wiernym danemu słowu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_hold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prawdziwy przyjaciel to ktoś, kto zawsze <trzyma> dane słowo.",
                            "target_lang_translations": {
                                "ru": "Настоящий друг — это тот, кто всегда <держит> данное слово."

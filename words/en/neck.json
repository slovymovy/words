    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a7f4cd09-5d64-3dc6-9577-8f0be78cf136",
                    "sense_definition": "The part of the body connecting the head and the trunk.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Giraffes have very long <w>necks</w> to reach high leaves.",
                            "target_lang_translations": {
                                "ru": "У жирафов очень длинные <w>шеи</w>, чтобы доставать до высоких листьев."
                            }
                        },
                        {
                            "text": "She wrapped a warm wool scarf around her <w>neck</w>.",
                            "target_lang_translations": {
                                "ru": "Она обмотала теплый шерстяной шарф вокруг <w>шеи</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stiff neck",
                        "pain in the neck",
                        "break one's neck"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть тела, соединяющая голову с туловищем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шея",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bda898f-c2af-3347-ab2b-27ef86585d9f",
                    "sense_definition": "The part of a shirt, dress, or other garment that fits around or covers the neck.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He unbuttoned the <w>neck</w> of his shirt because it was too tight.",
                            "target_lang_translations": {
                                "ru": "Он расстегнул <w>ворот</w> рубашки, потому что он был слишком тугим."
                            }
                        },
                        {
                            "text": "This sweater has a round <w>neck</w>.",
                            "target_lang_translations": {
                                "ru": "У этого свитера круглый <w>вырез</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "V-neck",
                        "crew neck",
                        "polo neck",
                        "round neck"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть рубашки, платья или другой одежды, которая облегает или закрывает шею."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ворот",
                                "target_lang_sense_clarification": "одежда"
                            },
                            {
                                "target_lang_word": "вырез",
                                "target_lang_sense_clarification": "декольте"
                            },
                            {
                                "target_lang_word": "горловина",
                                "target_lang_sense_clarification": "деталь кроя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34c93837-430f-3a24-9fd7-9f1957a352b3",
                    "sense_definition": "The narrow, tapered upper part of a bottle or similar container.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She held the beer bottle by its long <w>neck</w>.",
                            "target_lang_translations": {
                                "ru": "Она держала пивную бутылку за длинное <w>горлышко</w>."
                            }
                        },
                        {
                            "text": "The coin was too large to fit through the <w>neck</w> of the jar.",
                            "target_lang_translations": {
                                "ru": "Монета была слишком большой, чтобы пролезть в <w>горлышко</w> банки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bottle neck"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкая верхняя часть бутылки или подобного сосуда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горлышко",
                                "target_lang_sense_clarification": "бутылки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2ac5373-6c86-3708-9323-64a2e3c86ef8",
                    "sense_definition": "A person's life or safety, often used in phrases about risk or danger.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "idiomatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am not going to risk my <w>neck</w> for such a small reward.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь рисковать своей <w>головой</w> ради такой маленькой награды."
                            }
                        },
                        {
                            "text": "He lied to the police to save his own <w>neck</w>.",
                            "target_lang_translations": {
                                "ru": "Он солгал полиции, чтобы спасти свою <w>шкуру</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "save one's neck",
                        "risk one's neck",
                        "stick one's neck out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жизнь или безопасность человека (часто используется в идиомах о риске)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шкура",
                                "target_lang_sense_clarification": "разг. жизнь"
                            },
                            {
                                "target_lang_word": "голова",
                                "target_lang_sense_clarification": "перен. жизнь"
                            },
                            {
                                "target_lang_word": "шея",
                                "target_lang_sense_clarification": "в идиомах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9510062e-62c3-31dd-a801-49d726e65923",
                    "sense_definition": "The long, narrow extension of a stringed musical instrument where the fingerboard is mounted.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His hand slid quickly up and down the <w>neck</w> of the guitar.",
                            "target_lang_translations": {
                                "ru": "Его рука быстро скользила вверх и вниз по <w>грифу</w> гитары."
                            }
                        },
                        {
                            "text": "The <w>neck</w> of the violin is made of maple wood.",
                            "target_lang_translations": {
                                "ru": "<w>Гриф</w> скрипки сделан из клена."
                            }
                        }
                    ],
                    "common_phrases": [
                        "guitar neck"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная узкая часть струнного музыкального инструмента, вдоль которой натянуты струны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гриф",
                                "target_lang_sense_clarification": "музыка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce753f91-a92e-3fd7-89cf-b1cfe9a74e6d",
                    "sense_definition": "A narrow strip of land connecting two larger areas or projecting into water.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They built a cottage on a narrow <w>neck</w> of land between the lake and the sea.",
                            "target_lang_translations": {
                                "ru": "Они построили коттедж на узком <w>перешейке</w> земли между озером и морем."
                            }
                        },
                        {
                            "text": "I haven't seen you in this <w>neck</w> of the woods for a long time.",
                            "target_lang_translations": {
                                "ru": "Давненько я не видел тебя в этих <w>краях</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "neck of the woods",
                        "neck of land"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкая полоса земли, соединяющая два больших участка суши или выступающая в воду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перешеек",
                                "target_lang_sense_clarification": "география"
                            },
                            {
                                "target_lang_word": "коса",
                                "target_lang_sense_clarification": "полоса земли"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "997242c2-49ba-3f01-99c6-f3c19f2fa2c4",
                    "sense_definition": "To drink or swallow something rapidly and completely.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>necked</w> his coffee and ran out the door to catch the bus.",
                            "target_lang_translations": {
                                "ru": "Он <w>выпил залпом</w> свой кофе и выбежал за дверь, чтобы успеть на автобус."
                            }
                        },
                        {
                            "text": "The crowd cheered as he <w>necked</w> the pint of beer in one go.",
                            "target_lang_translations": {
                                "ru": "Толпа закричала, когда он <w>опрокинул</w> пинту пива за один присест."
                            }
                        }
                    ],
                    "synonyms": [
                        "chug"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пить или глотать что-либо быстро и полностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пить залпом",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "опрокидывать",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed814c8b-2abd-3f80-9c4e-9fdc29a6456e",
                    "sense_definition": "To kiss and caress passionately.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "affection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The couple was caught <w>necking</w> in the back row of the cinema.",
                            "target_lang_translations": {
                                "ru": "Парочку застукали <w>зажимающейся</w> в заднем ряду кинотеатра."
                            }
                        },
                        {
                            "text": "They spent the whole evening <w>necking</w> on the sofa.",
                            "target_lang_translations": {
                                "ru": "Они провели весь вечер, <w>милуясь</w> на диване."
                            }
                        }
                    ],
                    "synonyms": [
                        "make out",
                        "smooch",
                        "snog"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Страстно целоваться и обниматься."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зажиматься",
                                "target_lang_sense_clarification": "разг."
                            },
                            {
                                "target_lang_word": "миловаться",
                                "target_lang_sense_clarification": "устар./ирон."
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "entangle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rope <w>fouled</w> the propeller and stopped the boat.",
                            "target_lang_translations": {
                                "ru": "Веревка <w>запутала</w> винт и остановила лодку."
                            }
                        },
                        {
                            "text": "Be careful that the anchor line does not <w>foul</w> on the rocks.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны, чтобы якорный трос не <w>запутался</w> в камнях."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Запутывать или запутываться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запутывать",
                                "target_lang_sense_clarification": "сбивать в ком, переплетать"
                            },
                            {
                                "target_lang_word": "запутываться",
                                "target_lang_sense_clarification": "становиться запутанным"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "frequency": "Middle",
                    "semantic_group_id": "compartment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The truck backed into the loading <w>bay</w> to deliver the goods.",
                            "target_lang_translations": {
                                "ru": "Грузовик заехал задним ходом в погрузочный <w>отсек</w> для доставки товаров."
                            }

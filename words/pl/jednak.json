    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "b887c2a6-aff0-3c8e-b17d-7444d08bc614",
                    "sense_definition": "Służy do przyłączenia zdania lub jego części, które zawierają treść przeciwstawną do tego, co zostało powiedziane wcześniej; wyraża sprzeczność lub ograniczenie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałem pójść na spacer, <w>jednak</w> zaczął padać deszcz.",
                            "target_lang_translations": {
                                "ru": "Я хотел пойти на прогулку, <w>однако</w> пошёл дождь."
                            }
                        },
                        {
                            "text": "Był bardzo zmęczony, <w>jednak</w> pracował dalej bez przerwy.",
                            "target_lang_translations": {
                                "ru": "Он был очень уставшим, <w>однако</w> продолжал работать без перерыва."
                            }
                        }
                    ],
                    "synonyms": [
                        "ale",
                        "lecz",
                        "jednakże",
                        "natomiast",
                        "niemniej",
                        "atoli",
                        "wszelako"
                    ],
                    "common_phrases": [
                        "a jednak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для присоединения предложения или его части, содержащих противопоставление сказанному ранее; выражает противоречие или ограничение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "однако",
                                "target_lang_sense_clarification": "союз, выражающий противопоставление"
                            },
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "союз, выражающий противопоставление"
                            },
                            {
                                "target_lang_word": "тем не менее",
                                "target_lang_sense_clarification": "выражает уступку или ограничение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a5d7f50b-faee-3e5d-abe9-6203aa2119ce",
                    "sense_definition": "Słowo używane do podkreślenia, że sytuacja potoczyła się inaczej niż przewidywano lub że podjęto decyzję mimo wcześniejszych wątpliwości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szef powiedział, że <w>jednak</w> musimy przyjść do biura jutro.",
                            "target_lang_translations": {
                                "ru": "Начальник сказал, что нам <w>всё-таки</w> нужно прийти в офис завтра."
                            }
                        },
                        {
                            "text": "Myślałem, że nie zdążę na pociąg, ale <w>jednak</w> się udało.",
                            "target_lang_translations": {
                                "ru": "Я думал, что не успею на поезд, но мне <w>всё же</w> удалось."
                            }
                        }
                    ],
                    "synonyms": [
                        "mimo wszystko",
                        "rzeczywiście",
                        "naprawdę"
                    ],
                    "common_phrases": [
                        "jednak nie",
                        "to jednak prawda",
                        "jednak warto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Употребляется для подчеркивания того, что ситуация сложилась иначе, чем предполагалось, или что решение было принято, несмотря на сомнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всё-таки",
                                "target_lang_sense_clarification": "частица, выражающая противопоставление ожидаемому"
                            },
                            {
                                "target_lang_word": "всё же",
                                "target_lang_sense_clarification": "частица, усиливающая значение противопоставления"
                            },
                            {
                                "target_lang_word": "однако",
                                "target_lang_sense_clarification": "вводное слово в значении «тем не менее»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15090198-46ad-3b1f-81f3-26903b3bad96",
                    "sense_definition": "W taki sam sposób; identycznie (znaczenie historyczne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obaj bracia postąpili w tej sprawie <w>jednak</w>, nie zważając na różnice zdań.",
                            "target_lang_translations": {
                                "ru": "Оба брата поступили в этом деле <w>одинаково</w>, не обращая внимания на разногласия."
                            }
                        },
                        {
                            "text": "Wszyscy żołnierze byli <w>jednak</w> uzbrojeni i gotowi do walki.",
                            "target_lang_translations": {
                                "ru": "Все солдаты были <w>одинаково</w> вооружены и готовы к бою."
                            }
                        }
                    ],
                    "synonyms": [
                        "jednakowo",
                        "jednako",
                        "niezmiennie"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie rzadko używane w tym znaczeniu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Таким же образом; одинаково (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одинаково",
                                "target_lang_sense_clarification": "наречие образа действия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

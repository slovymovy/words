    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8dfc1705-b985-3e90-babd-2b14efb5e636",
                    "sense_definition": "То, что взято взаймы (обычно деньги) и должно быть возвращено.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "debt_finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг попросил у меня денег в <w>долг</w> до зарплаты.",
                            "target_lang_translations": {
                                "en": "My friend asked me for a <w>loan</w> until payday."
                            }
                        },
                        {
                            "text": "Вам нужно как можно скорее вернуть этот <w>долг</w> банку.",
                            "target_lang_translations": {
                                "en": "You need to repay this <w>debt</w> to the bank as soon as possible."
                            }
                        }
                    ],
                    "synonyms": [
                        "задолженность"
                    ],
                    "common_phrases": [
                        "взять в долг",
                        "дать в долг",
                        "вернуть долг",
                        "быть в долгу",
                        "влезть в долги"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that is borrowed (usually money) and must be returned."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "debt",
                                "target_lang_sense_clarification": "money owed"
                            },
                            {
                                "target_lang_word": "loan",
                                "target_lang_sense_clarification": "money borrowed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0050bff8-7dac-39bd-a95b-788e90fc741e",
                    "sense_definition": "Моральная или юридическая обязанность, то, что необходимо исполнить.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "moral_obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач считал своим профессиональным <w>долгом</w> помочь каждому пациенту.",
                            "target_lang_translations": {
                                "en": "The doctor considered it his professional <w>duty</w> to help every patient."
                            }
                        },
                        {
                            "text": "Защита природы — это наш общий гражданский <w>долг</w>.",
                            "target_lang_translations": {
                                "en": "Protecting nature is our common civic <w>duty</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "обязанность",
                        "обязательство"
                    ],
                    "common_phrases": [
                        "чувство долга",
                        "гражданский долг",
                        "выполнить долг",
                        "по долгу службы",
                        "священный долг"
                    ],
                    "target_lang_definitions": {
                        "en": "A moral or legal obligation, something that must be fulfilled."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "duty",
                                "target_lang_sense_clarification": "moral or legal obligation"
                            },
                            {
                                "target_lang_word": "obligation",
                                "target_lang_sense_clarification": "responsibility"
                            }
                        ]
                    }
                }
            ]
        }
    ]

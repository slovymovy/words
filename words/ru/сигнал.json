    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "46d81c6b-d663-3e7c-8ba3-27dbe1cbb441",
                    "sense_definition": "Условный знак (звуковой, световой и т. п.) для передачи какого-либо сообщения, предупреждения или команды.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Капитан корабля подал громкий <сигнал> к отплытию.",
                            "target_lang_translations": {
                                "en": "The ship's captain gave a loud <w>signal</w> for departure."
                            }
                        },
                        {
                            "text": "Водитель нажал на <сигнал>, чтобы предупредить пешехода об опасности.",
                            "target_lang_translations": {
                                "en": "The driver pressed the <w>horn</w> to warn the pedestrian of danger."
                            }
                        }
                    ],
                    "synonyms": [
                        "знак"
                    ],
                    "common_phrases": [
                        "подать сигнал",
                        "сигнал тревоги",
                        "звуковой сигнал",
                        "световой сигнал"
                    ],
                    "target_lang_definitions": {
                        "en": "A conventional sign (sound, light, etc.) used to convey a message, warning, or command."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "signal",
                                "target_lang_sense_clarification": "sign, warning, or command"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63f54781-5607-3c7c-817e-a68ddae6634b",
                    "sense_definition": "Физический процесс (электрический импульс, радиоволны и т. д.), несущий информацию в технических системах и связи.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В туннеле телефон потерял <сигнал> сотовой сети.",
                            "target_lang_translations": {
                                "en": "In the tunnel, the phone lost the cellular network <w>signal</w>."
                            }
                        },
                        {
                            "text": "Спутник передаёт цифровой <сигнал> на наземную станцию.",
                            "target_lang_translations": {
                                "en": "The satellite transmits a digital <w>signal</w> to the ground station."
                            }
                        }
                    ],
                    "common_phrases": [
                        "потеря сигнала",
                        "цифровой сигнал",
                        "уровень сигнала",
                        "обработка сигналов"
                    ],
                    "target_lang_definitions": {
                        "en": "A physical process (electric impulse, radio waves, etc.) carrying information in technical systems and communication."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "signal",
                                "target_lang_sense_clarification": "connection or data transmission"
                            }
                        ]
                    }
                }
            ]
        }
    ]

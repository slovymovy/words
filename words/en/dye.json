                    ],
                    "synonyms": [
                        "tint",
                        "shade"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary or poetic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой оттенок или цвет, создаваемый красящим веществом или напоминающий его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оттенок",
                                "target_lang_sense_clarification": "цветовой тон"
                            },
                            {
                                "target_lang_word": "краска",
                                "target_lang_sense_clarification": "цвет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4397266-02a1-3071-b3a3-4706eaf5acd9",

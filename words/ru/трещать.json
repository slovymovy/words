    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b5686ac7-12ea-36ec-995c-a576bd926064",
                    "sense_definition": "Издавать сухой, резкий звук (треск) при ломании, горении или разрыве.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сухие дрова весело <трещали> в камине.",
                            "target_lang_translations": {
                                "en": "Dry firewood <crackled> merrily in the fireplace."
                            }
                        },
                        {
                            "text": "Тонкий лёд начал опасно <трещать> под ногами.",
                            "target_lang_translations": {
                                "en": "The thin ice began to dangerously <crack> underfoot."
                            }
                        }
                    ],
                    "common_phrases": [
                        "лёд трещит",
                        "дрова трещат",
                        "сучья трещат"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a dry, sharp sound (crackle) when breaking, burning, or tearing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crackle",
                                "target_lang_sense_clarification": "fire, electricity, dry wood"
                            },
                            {
                                "target_lang_word": "crack",
                                "target_lang_sense_clarification": "ice, breaking objects"
                            },
                            {
                                "target_lang_word": "snap",
                                "target_lang_sense_clarification": "breaking suddenly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b92426f-aee1-3189-9f33-ce336b3d9f6a",
                    "sense_definition": "Очень сильно болеть (обычно о голове).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После шумной вечеринки у меня <трещит> голова.",
                            "target_lang_translations": {
                                "en": "After the noisy party, my head is <splitting>."
                            }
                        },
                        {
                            "text": "Голова <трещит> от множества проблем и забот.",
                            "target_lang_translations": {
                                "en": "My head is <splitting> from the multitude of problems and worries."
                            }
                        }
                    ],
                    "common_phrases": [
                        "голова трещит"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To hurt very badly (usually about the head)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "split",
                                "target_lang_sense_clarification": "of a headache"
                            },
                            {
                                "target_lang_word": "throb",
                                "target_lang_sense_clarification": "pulsating pain"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab0785d9-7456-3fc5-8912-f9945de30515",
                    "sense_definition": "Говорить очень быстро, громко и без умолку.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Подруги могут часами <трещать> по телефону ни о чём.",
                            "target_lang_translations": {
                                "en": "Girlfriends can <chatter> on the phone for hours about nothing."
                            }
                        },
                        {
                            "text": "Она продолжала <трещать> без умолку, рассказывая последние новости.",
                            "target_lang_translations": {
                                "en": "She continued to <rattle on> incessantly, telling the latest news."
                            }
                        }
                    ],
                    "synonyms": [
                        "тарато́рить",
                        "тарахте́ть"
                    ],
                    "common_phrases": [
                        "трещать без умолку",
                        "трещать по телефону"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To speak very quickly, loudly, and incessantly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chatter",
                                "target_lang_sense_clarification": "talk rapidly and trivially"
                            },
                            {
                                "target_lang_word": "jabber",
                                "target_lang_sense_clarification": "talk rapidly or unintelligibly"
                            },
                            {
                                "target_lang_word": "rattle on",
                                "target_lang_sense_clarification": "talk continuously"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "632a7586-8f32-38b6-bc6b-c9206ee5e36e",
                    "sense_definition": "Издавать характерные стрекочущие звуки (о насекомых, например, кузнечиках).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sound_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В высокой траве громко <трещали> кузнечики.",
                            "target_lang_translations": {
                                "en": "Grasshoppers <chirped> loudly in the tall grass."
                            }
                        },
                        {
                            "text": "Вечером в саду <трещали> цикады.",
                            "target_lang_translations": {
                                "en": "Cicadas <chirred> in the garden in the evening."
                            }
                        }
                    ],
                    "common_phrases": [
                        "кузнечики трещат",
                        "цикады трещат"
                    ],
                    "target_lang_definitions": {
                        "en": "To make characteristic chirping sounds (about insects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chirp",
                                "target_lang_sense_clarification": "short sharp sound"
                            },
                            {
                                "target_lang_word": "chirr",
                                "target_lang_sense_clarification": "prolonged trilling sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "774b2284-0537-3af1-9564-f244706fb176",
                    "sense_definition": "Находиться под угрозой разрушения, распада или неудачи.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Семейный бюджет начал <трещать> из-за непредвиденных расходов.",
                            "target_lang_translations": {
                                "en": "The family budget began to <crack> due to unforeseen expenses."
                            }
                        },
                        {
                            "text": "Их план обороны стал <трещать> по швам под натиском противника.",
                            "target_lang_translations": {
                                "en": "Their defense plan began to <burst> at the seams under the enemy's onslaught."
                            }
                        }
                    ],
                    "common_phrases": [
                        "трещать по швам"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be under threat of destruction, collapse, or failure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fall apart",
                                "target_lang_sense_clarification": "fail or collapse"
                            },
                            {
                                "target_lang_word": "burst at the seams",
                                "target_lang_sense_clarification": "be under severe strain"
                            },
                            {
                                "target_lang_word": "crack",
                                "target_lang_sense_clarification": "start to fail"
                            }
                        ]
                    }
                }
            ]
        }

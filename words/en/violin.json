    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3465c9ae-b94e-3ab9-baa9-0cc1e5cc12eb",
                    "sense_definition": "A small stringed musical instrument with four strings, usually held against the chin and played with a bow.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has been learning to play the <w>violin</w> since she was five years old.",
                            "target_lang_translations": {
                                "ru": "Она учится играть на <w>скрипке</w> с пяти лет."
                            }
                        },
                        {
                            "text": "He carefully placed his expensive <w>violin</w> back into its protective case.",
                            "target_lang_translations": {
                                "ru": "Он осторожно положил свою дорогую <w>скрипку</w> обратно в защитный футляр."
                            }
                        }
                    ],
                    "synonyms": [
                        "fiddle"
                    ],
                    "common_phrases": [
                        "play the violin",
                        "electric violin",
                        "violin bow",
                        "violin case"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой струнный музыкальный инструмент с четырьмя струнами, который обычно держат у подбородка и на котором играют смычком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрипка",
                                "target_lang_sense_clarification": "музыкальный инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f3ce547-cc0b-37e5-a338-534209d6bbc0",
                    "sense_definition": "A musician who plays the violin in an orchestra or musical group.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The first <w>violin</w> stood up to tune the orchestra before the concert began.",
                            "target_lang_translations": {
                                "ru": "Первая <w>скрипка</w> встала, чтобы настроить оркестр перед началом концерта."
                            }
                        },
                        {
                            "text": "The conductor signaled for the <w>violins</w> to play the melody with more emotion.",
                            "target_lang_translations": {
                                "ru": "Дирижёр дал знак <w>скрипкам</w> играть мелодию более эмоционально."
                            }
                        }
                    ],
                    "common_phrases": [
                        "first violin",
                        "second violin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкант, играющий на скрипке в оркестре или музыкальной группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрипка",
                                "target_lang_sense_clarification": "роль в оркестре"
                            },
                            {
                                "target_lang_word": "скрипач",
                                "target_lang_sense_clarification": "музыкант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e4d5931-ca7a-311f-bca0-4a9768965c73",
                    "sense_definition": "Any instrument belonging to the violin family, which includes the viola and cello.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The string quartet is composed entirely of instruments from the <w>violin</w> family.",
                            "target_lang_translations": {
                                "ru": "Струнный квартет состоит исключительно из инструментов <w>скрипичного</w> семейства."
                            }
                        },
                        {
                            "text": "This museum exhibits rare examples of the <w>violin</w> group from the 17th century.",
                            "target_lang_translations": {
                                "ru": "В этом музее выставлены редкие образцы <w>скрипичной</w> группы XVII века."
                            }
                        }
                    ],
                    "common_phrases": [
                        "violin family"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой инструмент, принадлежащий к семейству скрипичных, включая альт и виолончель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрипичный",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "скрипка",
                                "target_lang_sense_clarification": "семейство инструментов"
                            }
                        ]
                    }
                }
            ]
        },
        {

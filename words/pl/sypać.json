    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "61e70ea8-51f1-38f1-8e6b-5300e0a6107c",
                    "sense_definition": "O śniegu: padać gęsto i intensywnie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą w górach często <sypie> śnieg.",
                            "target_lang_translations": {
                                "ru": "Зимой в горах часто <валит> снег."
                            }
                        },
                        {
                            "text": "Cały dzień <sypało>, więc drogi są białe.",
                            "target_lang_translations": {
                                "ru": "Весь день <сыпало>, поэтому дороги белые."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Śląsk Cieszyński) suć",
                        "szafować"
                    ],
                    "common_phrases": [
                        "śnieg sypie",
                        "sypać śniegiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "О снеге: падать густо и интенсивно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "валить",
                                "target_lang_sense_clarification": "о сильном снегопаде"
                            },
                            {
                                "target_lang_word": "сыпать",
                                "target_lang_sense_clarification": "разг. о снеге или дожде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a05dcd4-8da2-3706-84ff-4a8fea81666a",
                    "sense_definition": "Powodować spadanie lub przemieszczanie się sypkiej substancji (np. piasku, soli, mąki).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kucharz powoli <sypie> przyprawy do gotującej się zupy.",
                            "target_lang_translations": {
                                "ru": "Повар медленно <сыплет> приправы в варящийся суп."
                            }
                        },
                        {
                            "text": "Drogowcy zaczęli <sypać> piasek na oblodzone chodniki.",
                            "target_lang_translations": {
                                "ru": "Дорожники начали <сыпать> песок на обледенелые тротуары."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sypać sól",
                        "sypać piaskiem",
                        "sypać mąkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять падать или перемещаться сыпучее вещество (например, песок, соль, муку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сыпать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "посыпать",
                                "target_lang_sense_clarification": "покрывать поверхность чем-то сыпучим"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "735bfd15-86d7-33c3-aa17-2ce656153340",
                    "sense_definition": "Dawać coś (np. pieniądze) lub wypowiadać słowa (np. żarty, komplementy) w dużej ilości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_abundance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wujek przy świątecznym stole lubi <sypać> żartami.",
                            "target_lang_translations": {
                                "ru": "Дядя за праздничным столом любит <сыпать> шутками."
                            }
                        },
                        {
                            "text": "Nie możesz tak bezmyślnie <sypać> pieniędzmi na prawo i lewo.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь так бездумно <сорить> деньгами направо и налево."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sypać żartami",
                        "sypać komplementami",
                        "sypać pieniędzmi",
                        "sypać jak z rękawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать что-то (например, деньги) или произносить слова (например, шутки, комплименты) в большом количестве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сыпать",
                                "target_lang_sense_clarification": "словами, шутками"
                            },
                            {
                                "target_lang_word": "сорить",
                                "target_lang_sense_clarification": "деньгами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aff79006-c6d2-3e64-9dfe-d4698e4a7433",
                    "sense_definition": "Zdradzać tajemnice lub donosić na kogoś policji lub władzom.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_inform",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zatrzymany przestępca zaczął <sypać> swoich wspólników.",
                            "target_lang_translations": {
                                "ru": "Задержанный преступник начал <сдавать> своих сообщников."
                            }
                        },
                        {
                            "text": "Wszyscy bali się, że on zacznie <sypać> podczas przesłuchania.",
                            "target_lang_translations": {
                                "ru": "Все боялись, что он начнет <стучать> во время допроса."
                            }
                        }
                    ],
                    "synonyms": [
                        "szafować"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдавать тайны или доносить на кого-то полиции или властям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "выдавать сообщников"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "доносить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e58c6597-ab1b-3b2a-a57c-803ff9f7944f",
                    "sense_definition": "Usypywać coś, budować wzniesienie poprzez gromadzenie ziemi lub piasku w jednym miejscu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkańcy wsi pomagali <sypać> wały przeciwpowodziowe.",
                            "target_lang_translations": {
                                "ru": "Жители деревни помогали <насыпать> противопаводковые валы."
                            }
                        },
                        {
                            "text": "Dzieci lubią <sypać> kopce z piasku na plaży.",
                            "target_lang_translations": {
                                "ru": "Дети любят <насыпать> кучи из песка на пляже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sypać wały",
                        "sypać kopiec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Насыпать что-то, возводить возвышение путем скопления земли или песка в одном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "насыпать",
                                "target_lang_sense_clarification": "создавать насыпь или кучу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f58eab94-3897-340e-82bf-959191ee632c",
                    "sense_definition": "O substancjach sypkich: wysypywać się, spadać w dół lub przemieszczać się.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mąka <sypie się> z dziurawego worka na podłogę.",
                            "target_lang_translations": {
                                "ru": "Мука <сыплется> из дырявого мешка на пол."
                            }
                        },
                        {
                            "text": "Tynk <sypie się> ze ścian w tym starym budynku.",
                            "target_lang_translations": {
                                "ru": "Штукатурка <осыпается> со стен в этом старом здании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sypać się na głowę",
                        "tynk się sypie"
                    ],
                    "target_lang_definitions": {
                        "ru": "О сыпучих веществах: высыпаться, падать вниз или перемещаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сыпаться",
                                "target_lang_sense_clarification": "падать (о песке, зерне)"
                            },
                            {
                                "target_lang_word": "осыпаться",
                                "target_lang_sense_clarification": "падать с поверхности (о штукатурке, листьях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4b8c49f-08cf-33da-9b02-906118d8ac87",
                    "sense_definition": "Psuć się, niszczeć lub tracić sprawność (o przedmiotach, zdrowiu lub planach).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_deteriorate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój stary samochód zaczyna <się sypać> i wymaga naprawy.",
                            "target_lang_translations": {
                                "ru": "Моя старая машина начинает <сыпаться> и требует ремонта."
                            }
                        },
                        {
                            "text": "Wszystkie nasze plany wyjazdowe zaczęły <się sypać> w ostatniej chwili.",
                            "target_lang_translations": {
                                "ru": "Все наши планы поездки начали <рушиться> в последний момент."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wszystko się sypie",
                        "zdrowie się sypie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ломаться, ветшать или терять работоспособность (о предметах, здоровье или планах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сыпаться",
                                "target_lang_sense_clarification": "разг. приходить в негодность"
                            },
                            {
                                "target_lang_word": "разваливаться",
                                "target_lang_sense_clarification": "разрушаться на части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f3bf431-1787-331f-acb4-a6b76278b7df",
                    "sense_definition": "Pojawiać się licznie i gwałtownie (np. o iskrach, ciosach, pytaniach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reflexive_abundance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trudne pytania <sypią się> jedno za drugim podczas egzaminu.",
                            "target_lang_translations": {
                                "ru": "Трудные вопросы <сыплются> один за другим во время экзамена."
                            }
                        },
                        {
                            "text": "Iskry <sypią się> z ogniska prosto na nasze ubrania.",
                            "target_lang_translations": {
                                "ru": "Искры <сыплются> из костра прямо на нашу одежду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "iskry się sypią",
                        "gromy się sypią"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появляться в большом количестве и стремительно (например, об искрах, ударах, вопросах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сыпаться",
                                "target_lang_sense_clarification": "появляться во множестве"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "60cda96a-08f4-31a4-901b-61e544455230",
                    "sense_definition": "The basic or inherent nature of a thing or idea that determines its character.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "inherent_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Time is of the <w>essence</w> regarding this urgent medical matter.",
                            "target_lang_translations": {
                                "ru": "В этом срочном медицинском вопросе время имеет <w>существенное</w> значение."
                            }
                        },
                        {
                            "text": "In <w>essence</w>, the agreement requires both parties to share the costs equally.",
                            "target_lang_translations": {
                                "ru": "По <w>сути</w>, соглашение требует, чтобы обе стороны делили расходы поровну."
                            }
                        }
                    ],
                    "synonyms": [
                        "nature",
                        "gist",
                        "crux",
                        "quintessence",
                        "marrow"
                    ],
                    "common_phrases": [
                        "in essence",
                        "of the essence",
                        "essence of the matter",
                        "time is of the essence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная или неотъемлемая природа вещи или идеи, определяющая её характер; суть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сущность",
                                "target_lang_sense_clarification": "философское понятие; внутренняя основа предмета"
                            },
                            {
                                "target_lang_word": "суть",
                                "target_lang_sense_clarification": "самое главное и существенное в чём-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d34adfe-98d8-3287-b28d-2d2f9671a0e6",
                    "sense_definition": "A concentrated liquid obtained from a plant or food used for flavoring or scent.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "concentrate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She added a few drops of vanilla <w>essence</w> to the cake mix.",
                            "target_lang_translations": {
                                "ru": "Она добавила несколько капель ванильной <w>эссенции</w> в смесь для торта."
                            }
                        },
                        {
                            "text": "The chef used almond <w>essence</w> to give the pastry a distinct flavor.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар использовал миндальную <w>эссенцию</w>, чтобы придать выпечке особый вкус."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vanilla essence",
                        "almond essence",
                        "peppermint essence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Концентрированная жидкость, полученная из растения или продукта, используемая для придания вкуса или запаха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эссенция",
                                "target_lang_sense_clarification": "концентрированный раствор летучих веществ"
                            },
                            {
                                "target_lang_word": "экстракт",
                                "target_lang_sense_clarification": "вытяжка из растительного сырья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e05aeaff-143e-3cf4-b740-c1ed012a491e",
                    "sense_definition": "A fragrance or perfume.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "scent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The expensive perfume captured the delicate <w>essence</w> of wild roses.",
                            "target_lang_translations": {
                                "ru": "Дорогие духи передавали тонкий <w>аромат</w> диких роз."
                            }
                        },
                        {
                            "text": "A sweet floral <w>essence</w> filled the room when she opened the bottle.",
                            "target_lang_translations": {
                                "ru": "Сладкий цветочный <w>аромат</w> наполнил комнату, когда она открыла флакон."
                            }
                        }
                    ],
                    "synonyms": [
                        "aroma",
                        "bouquet"
                    ],
                    "common_phrases": [
                        "sweet essence",
                        "floral essence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приятный запах, благоухание или парфюм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аромат",
                                "target_lang_sense_clarification": "приятный запах, благоухание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0122826f-47a1-3c45-9cc3-a8243883a945",
                    "sense_definition": "A being, especially one that is purely spiritual.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "spiritual_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The philosopher argued that spiritual <w>essences</w> exist beyond the physical world.",
                            "target_lang_translations": {
                                "ru": "Философ утверждал, что духовные <w>сущности</w> существуют за пределами физического мира."
                            }
                        },
                        {
                            "text": "He believed that an invisible <w>essence</w> guarded the ancient forest.",
                            "target_lang_translations": {
                                "ru": "Он верил, что невидимая <w>сущность</w> охраняла древний лес."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "literary or spiritual context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Существо, особенно нематериальной или духовной природы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сущность",
                                "target_lang_sense_clarification": "духовное или мистическое существо"
                            }
                        ]
                    }
                }
            ]
        },

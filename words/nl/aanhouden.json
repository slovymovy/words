{
    "word_family": [
        "aanhouding",
        "aanhouder",
        "aanhoudend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3e93bea6-3e1e-3ba2-8b5f-2c4b05a75b24",
                    "sense_definition": "Kledingstukken blijven dragen en niet uittrekken.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "kleding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is koud buiten, dus je kunt beter je jas <w>aanhouden</w>.",
                            "target_lang_translations": {
                                "en": "It is cold outside, so you had better <w>keep</w> your coat <w>on</w>.",
                                "ru": "На улице холодно, поэтому лучше <w>не снимать</w> куртку."
                            }
                        },
                        {
                            "text": "Mag ik mijn schoenen <w>aanhouden</w> of moet ik ze uitdoen bij de deur?",
                            "target_lang_translations": {
                                "en": "May I <w>keep</w> my shoes <w>on</w> or do I have to take them off at the door?",
                                "ru": "Можно <w>не снимать</w> обувь или мне нужно разуться у двери?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "je jas aanhouden",
                        "schoenen aanhouden",
                        "kleren aanhouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To continue wearing a garment and not take it off.",
                        "ru": "Не снимать одежду, оставаться в ней."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep on",
                                "target_lang_sense_clarification": "clothing"
                            },
                            {
                                "target_lang_word": "leave on",
                                "target_lang_sense_clarification": "not remove"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "не снимать",
                                "target_lang_sense_clarification": "об одежде"
                            },
                            {
                                "target_lang_word": "оставить",
                                "target_lang_sense_clarification": "не снимая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f518c819-46d1-3944-892d-dc7abf354ac8",
                    "sense_definition": "Door de politie meegenomen worden naar het bureau omdat men verdacht wordt van een strafbaar feit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "politie_actie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie heeft gisteren een verdachte <w>aangehouden</w> voor de diefstal.",
                            "target_lang_translations": {
                                "en": "The police <w>arrested</w> a suspect yesterday for the theft.",
                                "ru": "Вчера полиция <w>задержала</w> подозреваемого в краже."
                            }
                        },
                        {
                            "text": "Tijdens de controle werd de bestuurder <w>aangehouden</w> omdat hij te veel gedronken had.",
                            "target_lang_translations": {
                                "en": "During the check, the driver was <w>arrested</w> because he had drunk too much.",
                                "ru": "Во время проверки водителя <w>задержали</w>, так как он слишком много выпил."
                            }
                        }
                    ],
                    "synonyms": [
                        "arresteren",
                        "oppakken"
                    ],
                    "common_phrases": [
                        "een verdachte aanhouden",
                        "iemand aanhouden",
                        "worden aangehouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To be taken into custody by the police for a suspected crime.",
                        "ru": "Быть задержанным полицией по подозрению в совершении преступления."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arrest",
                                "target_lang_sense_clarification": "take into custody"
                            },
                            {
                                "target_lang_word": "detain",
                                "target_lang_sense_clarification": "hold in custody"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "задержать",
                                "target_lang_sense_clarification": "полиция"
                            },
                            {
                                "target_lang_word": "арестовать",
                                "target_lang_sense_clarification": "взять под стражу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6b4ad7f-cbb8-349b-b390-329b010e2b85",
                    "sense_definition": "Niet ophangen tijdens een telefoongesprek terwijl men wacht.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "telefonie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kunt u even <w>aanhouden</w> terwijl ik de gegevens erbij pak?",
                            "target_lang_translations": {
                                "en": "Could you <w>hold</w> for a moment while I get the data?",
                                "ru": "Можете <w>не вешать трубку</w>, пока я ищу данные?"
                            }
                        },
                        {
                            "text": "De receptioniste vroeg of ik wilde <w>aanhouden</w> zodat ze me kon doorverbinden.",
                            "target_lang_translations": {
                                "en": "The receptionist asked if I wanted to <w>hold</w> so she could put me through.",
                                "ru": "Секретарь спросила, могу ли я <w>оставаться на линии</w>, чтобы она могла меня соединить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "even aanhouden",
                        "blijven aanhouden",
                        "wilt u aanhouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To wait on a telephone line without hanging up.",
                        "ru": "Не вешать трубку во время телефонного разговора, ожидая ответа."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "telephone"
                            },
                            {
                                "target_lang_word": "hold the line",
                                "target_lang_sense_clarification": "wait on phone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "не вешать трубку",
                                "target_lang_sense_clarification": "ждать на линии"
                            },
                            {
                                "target_lang_word": "оставаться на линии",
                                "target_lang_sense_clarification": "телефон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc74b724-ac4f-3e84-91f0-b14385cba861",
                    "sense_definition": "Iemand of een voertuig laten stoppen, vaak om iets te vragen of te controleren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "politie_actie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de drukke stad moest ik een taxi <w>aanhouden</w> om op tijd bij de vergadering te zijn.",
                            "target_lang_translations": {
                                "en": "In the busy city I had to <w>hail</w> a taxi to be at the meeting on time.",
                                "ru": "В оживленном городе мне пришлось <w>поймать</w> такси, чтобы успеть на встречу вовремя."
                            }
                        },
                        {
                            "text": "De agent <w>hield</w> de fietser <w>aan</w> omdat zijn licht het niet deed.",
                            "target_lang_translations": {
                                "en": "The officer <w>stopped</w> the cyclist because his light wasn't working.",
                                "ru": "Полицейский <w>остановил</w> велосипедиста, потому что у него не работал свет."
                            }
                        }
                    ],
                    "synonyms": [
                        "tegenhouden"
                    ],
                    "common_phrases": [
                        "een taxi aanhouden",
                        "iemand op straat aanhouden",
                        "een auto aanhouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To signal someone or a vehicle to stop.",
                        "ru": "Остановить кого-либо или транспортное средство, часто для проверки или просьбы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "make someone stop"
                            },
                            {
                                "target_lang_word": "hail",
                                "target_lang_sense_clarification": "a taxi or vehicle"
                            },
                            {
                                "target_lang_word": "flag down",
                                "target_lang_sense_clarification": "signal to stop"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "остановить",
                                "target_lang_sense_clarification": "транспорт или человека"
                            },
                            {
                                "target_lang_word": "поймать",
                                "target_lang_sense_clarification": "такси"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65506b06-d95f-30cf-beb3-3d5d05a5c794",
                    "sense_definition": "Blijven duren en niet ophouden, vaak gezegd van weer, pijn of een toestand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "continueren",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als de koorts blijft <w>aanhouden</w>, moet je contact opnemen met de huisarts.",
                            "target_lang_translations": {
                                "en": "If the fever <w>persists</w>, you must contact the doctor.",
                                "ru": "Если температура будет <w>держаться</w>, нужно обратиться к врачу."
                            }
                        },
                        {
                            "text": "Het slechte weer zal nog enkele dagen <w>aanhouden</w> volgens de voorspelling.",
                            "target_lang_translations": {
                                "en": "The bad weather will <w>continue</w> for a few more days according to the forecast.",
                                "ru": "По прогнозу плохая погода <w>продержится</w> еще несколько дней."
                            }
                        }
                    ],
                    "synonyms": [
                        "tegenhouden",
                        "arresteren",
                        "oppakken"
                    ],
                    "common_phrases": [
                        "de pijn houdt aan",
                        "aanhoudende regen",
                        "de droogte houdt aan"
                    ],
                    "target_lang_definitions": {
                        "en": "To continue to exist or last, often regarding weather, pain, or a condition.",
                        "ru": "Длиться, не прекращаться (часто о погоде, боли или состоянии)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "persist",
                                "target_lang_sense_clarification": "continue to exist"
                            },
                            {
                                "target_lang_word": "continue",
                                "target_lang_sense_clarification": "last"
                            },
                            {
                                "target_lang_word": "last",
                                "target_lang_sense_clarification": "endure"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "продолжаться",
                                "target_lang_sense_clarification": "длиться"
                            },
                            {
                                "target_lang_word": "держаться",
                                "target_lang_sense_clarification": "не проходить"
                            },
                            {
                                "target_lang_word": "сохраняться",
                                "target_lang_sense_clarification": "о погоде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f24cd9c-b030-3e99-8a11-7a951d09b135",
                    "sense_definition": "Een bepaalde richting, snelheid of gewoonte blijven volgen of handhaven.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "continueren",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten deze snelheid <w>aanhouden</w> als we voor het donker thuis willen zijn.",
                            "target_lang_translations": {
                                "en": "We must <w>maintain</w> this speed if we want to be home before dark.",
                                "ru": "Мы должны <w>держать</w> эту скорость, если хотим быть дома до темноты."
                            }
                        },
                        {
                            "text": "Het schip moest een zuidelijke koers <w>aanhouden</w> om de storm te ontwijken.",
                            "target_lang_translations": {
                                "en": "The ship had to <w>keep to</w> a southern course to avoid the storm.",
                                "ru": "Корабль должен был <w>придерживаться</w> южного курса, чтобы избежать шторма."
                            }
                        }
                    ],
                    "synonyms": [
                        "tegenhouden",
                        "arresteren",
                        "oppakken",
                        "tegenhouden"
                    ],
                    "common_phrases": [
                        "een koers aanhouden",
                        "dezelfde snelheid aanhouden",
                        "een regel aanhouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To maintain a specific direction, speed, or habit.",
                        "ru": "Следовать определенному направлению, скорости или правилу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "maintain",
                                "target_lang_sense_clarification": "speed or course"
                            },
                            {
                                "target_lang_word": "keep to",
                                "target_lang_sense_clarification": "adhere to"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "придерживаться",
                                "target_lang_sense_clarification": "курса, правил"
                            },
                            {
                                "target_lang_word": "держать",
                                "target_lang_sense_clarification": "скорость"
                            },
                            {
                                "target_lang_word": "сохранять",
                                "target_lang_sense_clarification": "темп"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95440505-d2e6-3132-9e4d-b437a691522d",
                    "sense_definition": "Een rechtszaak of beslissing uitstellen naar een later moment.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "juridisch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter besloot de zaak <w>aan te houden</w> voor nader onderzoek.",
                            "target_lang_translations": {
                                "en": "The judge decided to <w>adjourn</w> the case for further investigation.",
                                "ru": "Судья решил <w>отложить</w> дело для дальнейшего расследования."
                            }
                        },
                        {
                            "text": "Het vonnis werd voor onbepaalde tijd <w>aangehouden</w> door de rechtbank.",
                            "target_lang_translations": {
                                "en": "The verdict was <w>suspended</w> indefinitely by the court.",
                                "ru": "Вынесение приговора было <w>отложено</w> судом на неопределенный срок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de zaak aanhouden",
                        "het vonnis aanhouden"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "juridisch"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To delay or suspend a legal proceeding or decision.",
                        "ru": "Отложить судебное разбирательство или решение на более поздний срок."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "adjourn",
                                "target_lang_sense_clarification": "legal case"
                            },
                            {
                                "target_lang_word": "suspend",
                                "target_lang_sense_clarification": "judgment or sentence"
                            },
                            {
                                "target_lang_word": "stay",
                                "target_lang_sense_clarification": "legal proceedings"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отложить",
                                "target_lang_sense_clarification": "судебное дело"
                            }
                        ]
                    }
                }
            ]
        }
    ]

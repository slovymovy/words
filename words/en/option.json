{
    "word_family": [
        "opt",
        "optional",
        "optionally"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dcff310c-d338-3f6a-9e51-706afdd519a3",
                    "sense_definition": "One of a set of choices that can be made.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "choice_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We have several <w>options</w> for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "У нас есть несколько <w>вариантов</w> для ужина сегодня вечером."
                            }
                        },
                        {
                            "text": "Taking the bus is the cheapest <w>option</w> available to us.",
                            "target_lang_translations": {
                                "ru": "Поездка на автобусе — это самый дешевый <w>вариант</w>, доступный нам."
                            }
                        }
                    ],
                    "synonyms": [
                        "choice",
                        "alternative",
                        "possibility"
                    ],
                    "common_phrases": [
                        "available options",
                        "explore options",
                        "viable option",
                        "best option"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из набора вариантов, которые можно выбрать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вариант",
                                "target_lang_sense_clarification": "разновидность, одна из возможностей"
                            },
                            {
                                "target_lang_word": "выбор",
                                "target_lang_sense_clarification": "то, из чего выбирают"
                            },
                            {
                                "target_lang_word": "опция",
                                "target_lang_sense_clarification": "дополнительная возможность или функция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29eb01a4-0d74-3129-9741-3f431a4f20fb",
                    "sense_definition": "The freedom, power, or right to choose.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "choice_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You have the <w>option</w> to cancel the subscription at any time.",
                            "target_lang_translations": {
                                "ru": "У вас есть <w>возможность</w> отменить подписку в любое время."
                            }
                        },
                        {
                            "text": "She had no <w>option</w> but to accept the offer.",
                            "target_lang_translations": {
                                "ru": "У нее не было другого <w>выбора</w>, кроме как принять предложение."
                            }
                        }
                    ],
                    "synonyms": [
                        "choice"
                    ],
                    "common_phrases": [
                        "have the option",
                        "no option but",
                        "keep options open"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свобода, возможность или право выбирать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбор",
                                "target_lang_sense_clarification": "право или свобода выбирать"
                            },
                            {
                                "target_lang_word": "возможность",
                                "target_lang_sense_clarification": "свобода действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ec36ab5-a90a-3081-b152-c169af014499",
                    "sense_definition": "A contract giving the holder the right to buy or sell an asset at a set price within a specific time.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company offered him stock <w>options</w> as part of his bonus package.",
                            "target_lang_translations": {
                                "ru": "Компания предложила ему фондовые <w>опционы</w> как часть бонусного пакета."
                            }
                        },
                        {
                            "text": "Investors can buy a call <w>option</w> if they expect the price to rise.",
                            "target_lang_translations": {
                                "ru": "Инвесторы могут купить колл-<w>опцион</w>, если ожидают роста цены."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stock option",
                        "call option",
                        "put option",
                        "buy an option"
                    ],
                    "target_lang_definitions": {
                        "ru": "Контракт, дающий держателю право купить или продать актив по установленной цене в течение определенного времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опцион",
                                "target_lang_sense_clarification": "финансовый инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f0961f4-4ccb-3d69-b6ab-59f41b45adec",
                    "sense_definition": "The legal act of acquiring or retaining a nationality through personal choice.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Residents were given the <w>option</w> to retain their original citizenship after the border change.",
                            "target_lang_translations": {
                                "ru": "Жителям было предоставлено <w>право выбора</w> сохранить свое первоначальное гражданство после изменения границ."
                            }
                        },
                        {
                            "text": "The treaty included a right of <w>option</w> for inhabitants of the transferred territory.",
                            "target_lang_translations": {
                                "ru": "Договор включал право <w>оптации</w> для жителей переданной территории."
                            }
                        }
                    ],
                    "common_phrases": [
                        "right of option"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридический акт выбора гражданства при изменении государственных границ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оптация",
                                "target_lang_sense_clarification": "выбор гражданства"
                            },
                            {
                                "target_lang_word": "право выбора",
                                "target_lang_sense_clarification": "юридическое право"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6cd0b55f-e733-362b-8615-5dae634a009b",
                    "sense_definition": "To purchase the exclusive right to use something, such as a story or script, for a future production.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The film studio <w>optioned</w> the novel shortly after it was published.",
                            "target_lang_translations": {
                                "ru": "Киностудия <w>приобрела права на экранизацию</w> романа вскоре после его публикации."
                            }
                        },
                        {
                            "text": "His screenplay was <w>optioned</w> by a major producer last year.",
                            "target_lang_translations": {
                                "ru": "На его сценарий был <w>куплен опцион</w> крупным продюсером в прошлом году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "option a book",
                        "option the rights"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приобретать эксклюзивное право на использование чего-либо (например, сценария) для будущей постановки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приобретать права",
                                "target_lang_sense_clarification": "на экранизацию или постановку"
                            },
                            {
                                "target_lang_word": "покупать опцион",
                                "target_lang_sense_clarification": "предварительный контракт на права"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c110c8f-93b0-36cf-95d7-7523103f43a1",
                    "sense_definition": "To configure a device or software by setting specific preferences.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car can be <w>optioned</w> with heated seats and a sunroof.",
                            "target_lang_translations": {
                                "ru": "Автомобиль может быть <w>укомплектован</w> подогревом сидений и люком."
                            }
                        },
                        {
                            "text": "The software was <w>optioned</w> to run automatically upon startup.",
                            "target_lang_translations": {
                                "ru": "Программное обеспечение было <w>настроено</w> на автоматический запуск."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Настраивать устройство или программное обеспечение, выбирая определенные параметры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укомплектовывать",
                                "target_lang_sense_clarification": "оснащать дополнительным оборудованием"
                            },
                            {
                                "target_lang_word": "настраивать",
                                "target_lang_sense_clarification": "задавать параметры"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
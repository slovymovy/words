    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "33617256-c386-32b9-afc1-353b74d2b854",
                    "sense_definition": "Najmniejsza jednostka podziału administracyjnego w niektórych wspólnotach chrześcijańskich, wspólnota wiernych kierowana przez proboszcza",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "admin_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W maju w naszej <w>parafii</w> odbędzie się uroczystość pierwszej komunii świętej.",
                            "target_lang_translations": {
                                "ru": "В мае в нашем <w>приходе</w> состоится церемония первого причастия."
                            }
                        },
                        {
                            "text": "Nowy ksiądz został bardzo ciepło przyjęty przez lokalną społeczność, gdy objął tę <w>parafię</w>.",
                            "target_lang_translations": {
                                "ru": "Новый священник был очень тепло принят местным сообществом, когда возглавил этот <w>приход</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "parochia"
                    ],
                    "common_phrases": [
                        "należeć do parafii",
                        "parafia rzymskokatolicka",
                        "proboszcz parafii",
                        "zmiana parafii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наименьшая административная единица в некоторых христианских конфессиях; община верующих, возглавляемая настоятелем"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приход",
                                "target_lang_sense_clarification": "административная единица"
                            },
                            {
                                "target_lang_word": "церковный приход",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "166bd1f9-8b42-30aa-8f01-f2097cd5957a",
                    "sense_definition": "Ogół mieszkańców lub wiernych należących do danej jednostki kościelnej",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "community",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała <w>parafia</w> przyszła na mszę z udziałem arcybiskupa.",
                            "target_lang_translations": {
                                "ru": "Весь <w>приход</w> пришел на мессу с участием архиепископа."
                            }
                        },
                        {
                            "text": "Podczas zbiórki charytatywnej <w>parafia</w> okazała się niezwykle hojna i pomocna.",
                            "target_lang_translations": {
                                "ru": "Во время благотворительного сбора <w>приход</w> оказался невероятно щедрым и отзывчивым."
                            }
                        }
                    ],
                    "synonyms": [
                        "parafianie"
                    ],
                    "common_phrases": [
                        "cała parafia",
                        "życie parafii",
                        "wspólnota parafii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность жителей или верующих, принадлежащих к данной церковной единице"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приход",
                                "target_lang_sense_clarification": "община людей"
                            },
                            {
                                "target_lang_word": "прихожане",
                                "target_lang_sense_clarification": "люди, посещающие церковь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3fe93d7-8721-3274-96a2-978123856978",
                    "sense_definition": "Budynek kościoła parafialnego",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spotkamy się przed <w>parafią</w> tuż po zakończeniu niedzielnej mszy.",
                            "target_lang_translations": {
                                "ru": "Встретимся перед <w>церковью</w> сразу после окончания воскресной мессы."
                            }
                        },
                        {
                            "text": "Ta zabytkowa <w>parafia</w> wymaga natychmiastowego remontu dachu.",
                            "target_lang_translations": {
                                "ru": "Эта старинная <w>приходская церковь</w> требует немедленного ремонта крыши."
                            }
                        }
                    ],
                    "synonyms": [

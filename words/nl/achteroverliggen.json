    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9c69cecf-1585-34c6-8b3f-8177223c7525",
                    "sense_definition": "Op de rug liggen, meestal om te rusten of te ontspannen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een lange wandeling wil ik even in het gras <w>achteroverliggen</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой прогулки я хочу немного <w>полежать на спине</w> в траве."
                            }
                        },
                        {
                            "text": "Hij heeft de hele middag in de zon <w>achterovergelegen</w>.",
                            "target_lang_translations": {
                                "ru": "Он весь день <w>пролежал на спине</w> на солнце."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lekker achteroverliggen",
                        "in het gras achteroverliggen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лежать на спине, обычно чтобы отдохнуть или расслабиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лежать на спине",
                                "target_lang_sense_clarification": "физическое положение"
                            },
                            {
                                "target_lang_word": "откинуться назад",
                                "target_lang_sense_clarification": "принять горизонтальное положение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69159675-4614-3e92-9368-ad6f2ce1b1a9",
                    "sense_definition": "Helemaal niets doen en passief afwachten; lui zijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We kunnen nu niet <w>achteroverliggen</w>, want het project moet morgen af zijn.",
                            "target_lang_translations": {
                                "ru": "Мы не можем сейчас <w>сидеть сложа руки</w>, так как проект должен быть готов завтра."
                            }
                        },
                        {
                            "text": "Het is irritant dat hij altijd wil <w>achteroverliggen</w> terwijl wij hard werken.",
                            "target_lang_translations": {
                                "ru": "Раздражает, что он всегда хочет <w>бездельничать</w>, пока мы усердно работаем."
                            }
                        }
                    ],
                    "synonyms": [
                        "luieren",
                        "lanterfanten"
                    ],
                    "antonyms": [
                        "werken",
                        "aanpakken"
                    ],
                    "common_phrases": [
                        "niet achteroverliggen",
                        "gewoon achteroverliggen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ничего не делать, пассивно выжидать; лениться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидеть сложа руки",
                                "target_lang_sense_clarification": "идиома: бездействовать"
                            },
                            {
                                "target_lang_word": "бездельничать",
                                "target_lang_sense_clarification": "лениться, не работать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "target_lang_definitions": {
                        "ru": "Защитное или декоративное покрытие поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрытие",
                                "target_lang_sense_clarification": "верхний слой"
                            },
                            {
                                "target_lang_word": "отделка",
                                "target_lang_sense_clarification": "внешнее оформление"

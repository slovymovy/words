            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0803f8d7-8887-3a29-998e-55cf60eb429f",
                    "sense_definition": "Сделать более сильным, мощным или крепким (физически или количественно).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир принял решение <усилить> охрану склада дополнительными солдатами.",
                            "target_lang_translations": {
                                "en": "The commander decided to <reinforce> the warehouse security with additional soldiers."
                            }
                        },
                        {
                            "text": "Инженеры смогли <усилить> конструкцию моста, чтобы он выдержал больший вес.",
                            "target_lang_translations": {
                                "en": "The engineers were able to <strengthen> the bridge structure so it could withstand more weight."
                            }
                        }
                    ],
                    "synonyms": [
                        "укрепить"
                    ],
                    "antonyms": [
                        "ослабить"
                    ],
                    "common_phrases": [
                        "усилить охрану",
                        "усилить звук",
                        "усилить позиции",
                        "усилить сигнал"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something stronger, more powerful, or sturdier (physically or quantitatively)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strengthen",
                                "target_lang_sense_clarification": "general physical strength"
                            },
                            {
                                "target_lang_word": "reinforce",
                                "target_lang_sense_clarification": "add support, material, or personnel"
                            },
                            {
                                "target_lang_word": "amplify",
                                "target_lang_sense_clarification": "sound or signal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff26b4cc-aabb-3dc5-865e-0f15bd1e1a3d",
                    "sense_definition": "Повысить степень интенсивности, напряжения или проявления чего-либо (действия, чувства, состояния).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство обещало <усилить> контроль за качеством продуктов питания.",
                            "target_lang_translations": {
                                "en": "The government promised to <tighten> control over food quality."
                            }
                        },
                        {
                            "text": "Эта неожиданная новость только <усилила> её тревогу.",
                            "target_lang_translations": {
                                "en": "This unexpected news only <heightened> her anxiety."
                            }
                        }
                    ],
                    "synonyms": [
                        "повысить",
                        "увеличить"
                    ],
                    "antonyms": [
                        "ослабить"
                    ],
                    "common_phrases": [
                        "усилить контроль",
                        "усилить внимание",
                        "усилить давление",
                        "усилить впечатление"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase the degree of intensity, tension, or manifestation of something (actions, feelings, states)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "intensify",
                                "target_lang_sense_clarification": "make more intense"
                            },
                            {
                                "target_lang_word": "tighten",
                                "target_lang_sense_clarification": "rules, control, or security"
                            },
                            {
                                "target_lang_word": "heighten",
                                "target_lang_sense_clarification": "feelings or awareness"
                            },
                            {
                                "target_lang_word": "step up",
                                "target_lang_sense_clarification": "efforts or measures"
                            }
                        ]
                    }
                }
            ]

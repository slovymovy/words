    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bfd243da-a259-38f4-99a4-ea2ae36458d7",
                    "sense_definition": "To make a choice or select a particular option from a range of possibilities.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>opted</w> for the salad rather than the steak during our lunch.",
                            "target_lang_translations": {
                                "ru": "Она <w>выбрала</w> салат, а не стейк, во время нашего обеда."
                            }
                        },
                        {
                            "text": "They <w>opted</w> not to take the train because the bus was much cheaper.",
                            "target_lang_translations": {
                                "ru": "Они <w>решили</w> не ехать на поезде, потому что автобус был намного дешевле."
                            }
                        }
                    ],
                    "synonyms": [
                        "choose",
                        "select",
                        "pick",
                        "decide"
                    ],
                    "antonyms": [
                        "reject",
                        "decline"
                    ],
                    "common_phrases": [
                        "opt for",
                        "opt out",
                        "opt in",
                        "opt to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать выбор или предпочесть определенный вариант из ряда возможностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбирать",
                                "target_lang_sense_clarification": "делать выбор из нескольких вариантов"
                            },
                            {
                                "target_lang_word": "решать",
                                "target_lang_sense_clarification": "принимать решение сделать что-либо"
                            },
                            {
                                "target_lang_word": "предпочитать",
                                "target_lang_sense_clarification": "отдавать предпочтение одному варианту перед другим"
                            }
                        ]
                    }
                }
            ]
        },

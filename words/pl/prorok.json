                    "sense_id": "1cd0d9bd-39c6-324d-885f-7ed270900d8e",
                    "sense_definition": "charyzmatyczny przywódca religijny lub osoba natchniona przez Boga, która przekazuje jego wolę",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Biblijny <w>prorok</w> Izajasz ostrzegał swój lud przed nadchodzącym niebezpieczeństwem.",
                            "target_lang_translations": {
                                "ru": "Библейский <w>пророк</w> Исаия предупреждал свой народ о грядущей опасности."
                            }
                        },
                        {
                            "text": "Wielu ludzi gromadziło się na placu, aby słuchać słów, które głosił nowy <w>prorok</w>.",
                            "target_lang_translations": {
                                "ru": "Множество людей собиралось на площади, чтобы послушать слова, которые провозглашал новый <w>пророк</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "profeta"
                    ],
                    "common_phrases": [
                        "fałszywy prorok",
                        "prorok mniejszy",
                        "prorok większy",
                        "księgi proroków"
                    ],
                    "target_lang_definitions": {
                        "ru": "харизматичный религиозный лидер или человек, вдохновленный Богом, передающий его волю"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пророк",

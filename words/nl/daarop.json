{
    "word_family": [
        "daaropvolgend"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e22493eb-2d3a-3b84-90b3-f21dae1fa118",
                    "sense_definition": "Op dat of op die; verwijst naar een object, plaats of zaak die daar is of eerder genoemd is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "referential",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zie je die grote tafel in de hoek? <w>Daarop</w> liggen al mijn boeken.",
                            "target_lang_translations": {
                                "en": "Do you see that big table in the corner? All my books are <w>on it</w>.",
                                "ru": "Ты видишь тот большой стол в углу? <w>На нём</w> лежат все мои книги."
                            }
                        },
                        {
                            "text": "Hij stelde een moeilijke vraag, maar het antwoord <w>daarop</w> wist niemand.",
                            "target_lang_translations": {
                                "en": "He asked a difficult question, but no one knew the answer <w>to it</w>.",
                                "ru": "Он задал трудный вопрос, но ответа <w>на него</w> никто не знал."
                            }
                        }
                    ],
                    "synonyms": [
                        "erop"
                    ],
                    "antonyms": [
                        "hierop"
                    ],
                    "common_phrases": [
                        "het antwoord daarop",
                        "de kans daarop",
                        "het zicht daarop"
                    ],
                    "target_lang_definitions": {
                        "en": "On that or on it; refers to an object, place, or matter that is there or was mentioned earlier.",
                        "ru": "На этом или на том; относится к объекту, месту или предмету, который находится там или был упомянут ранее."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on it",
                                "target_lang_sense_clarification": "referring to a physical object or surface"
                            },
                            {
                                "target_lang_word": "on that",
                                "target_lang_sense_clarification": "emphatic reference to a specific object"
                            },
                            {
                                "target_lang_word": "to that",
                                "target_lang_sense_clarification": "used when the English verb requires 'to' (e.g., answer to that)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на этом",
                                "target_lang_sense_clarification": "указание на местоположение"
                            },
                            {
                                "target_lang_word": "на это",
                                "target_lang_sense_clarification": "ссылка на абстрактный объект или вопрос"
                            },
                            {
                                "target_lang_word": "на нём",
                                "target_lang_sense_clarification": "замена существительного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3bd68a1-b027-35f9-87a3-8d53c69668d0",
                    "sense_definition": "Op een moment dat direct volgt op wat zojuist genoemd of gebeurd is.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er klonk een harde knal en vlak <w>daarop</w> ging het brandalarm af.",
                            "target_lang_translations": {
                                "en": "There was a loud bang and right <w>after that</w> the fire alarm went off.",
                                "ru": "Раздался громкий хлопок, и сразу <w>после этого</w> сработала пожарная сигнализация."
                            }
                        },
                        {
                            "text": "De voorzitter opende de vergadering en <w>daarop</w> begon de eerste spreker.",
                            "target_lang_translations": {
                                "en": "The chairman opened the meeting and <w>thereupon</w> the first speaker began.",
                                "ru": "Председатель открыл собрание, и <w>затем</w> начал выступать первый оратор."
                            }
                        }
                    ],
                    "synonyms": [
                        "vervolgens",
                        "daarna",
                        "toen"
                    ],
                    "antonyms": [
                        "daarvoor",
                        "tevoren"
                    ],
                    "common_phrases": [
                        "kort daarop",
                        "vlak daarop",
                        "meteen daarop"
                    ],
                    "target_lang_definitions": {
                        "en": "At a moment directly following what was just mentioned or happened.",
                        "ru": "В момент, непосредственно следующий за тем, что только что было упомянуто или произошло."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "after that",
                                "target_lang_sense_clarification": "indicating temporal sequence"
                            },
                            {
                                "target_lang_word": "thereupon",
                                "target_lang_sense_clarification": "formal, indicating immediate consequence or sequence"
                            },
                            {
                                "target_lang_word": "then",
                                "target_lang_sense_clarification": "simple sequence of events"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "затем",
                                "target_lang_sense_clarification": "наречие времени"
                            },
                            {
                                "target_lang_word": "после этого",
                                "target_lang_sense_clarification": "временная последовательность"
                            },
                            {
                                "target_lang_word": "вслед за этим",
                                "target_lang_sense_clarification": "непосредственно после"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
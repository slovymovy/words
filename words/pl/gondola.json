            "senses": [
                {
                    "sense_id": "6e96f343-8b12-334d-87ed-5b2a508cdd29",
                    "sense_definition": "Wąska, długa łódź wiosłowa o charakterystycznym kształcie, tradycyjnie używana w Wenecji do transportu pasażerów kanałami.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Wenecji zakochane pary często odbywają romantyczne przejażdżki <w>gondolami</w>."
                        },
                        {
                            "text": "Turysta wynajął <w>gondolę</w>, aby zobaczyć zabytkowe miasto z perspektywy wody."
                        }
                    ],
                    "common_phrases": [
                        "wenecka gondola",
                        "płynąć gondolą",
                        "wynająć gondolę"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "associated with Venice"
                        }
                    ]
                },
                {
                    "sense_id": "0bc26aff-8a3c-3a20-8819-7995e948a099",
                    "sense_definition": "Zamknięty wagonik kolejki linowej, służący do transportu ludzi, często spotykany w ośrodkach narciarskich.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_cable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do <w>gondoli</w> wsiadło ośmiu narciarzy i kolejka zawiozła ich na ośnieżony stok."
                        },
                        {
                            "text": "Widok z przeszklonej <w>gondoli</w> na górskie szczyty był absolutnie niesamowity."
                        }
                    ],
                    "common_phrases": [
                        "kolejka z gondolami",
                        "wsiąść do gondoli"
                    ],
                    "traits": [
                        {
                            "trait_type": "technical",
                            "comment": "transport"
                        }
                    ]
                },
                {
                    "sense_id": "bf06ac06-eb25-3670-b534-8dab5e991994",
                    "sense_definition": "Górna część wózka dziecięcego, w której niemowlę przewożone jest w pozycji leżącej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "childcare",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez pierwsze miesiące życia dziecko należy wozić na codzienne spacery w <w>gondoli</w>."
                        },
                        {
                            "text": "Kupiliśmy nowoczesny wózek, który posiada dużą i wygodną <w>gondolę</w> dla noworodka."
                        }
                    ],
                    "common_phrases": [
                        "wózek z gondolą",
                        "głęboka gondola"
                    ]
                },
                {
                    "sense_id": "b65b5278-3fff-3f0b-83b8-d83d93fdbfe4",
                    "sense_definition": "Kosz podwieszony pod balonem lub sterowcem, przeznaczony dla załogi i pasażerów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "transport_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Balon nagle odleciał, ale nikt nie zdążył jeszcze wsiąść do jego <w>gondoli</w>."
                        },
                        {
                            "text": "Podróżnicy spędzili w ciasnej <w>gondoli</w> wiele godzin podczas lotu nad oceanem."
                        }
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa692e42-ce57-355c-8866-176ff391618e",
                    "sense_definition": "Jadalny owoc egzotyczny o gruszkowatym kształcie, zielonej lub ciemnej skórce i maślanym miąższu, znany powszechnie jako awokado.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fruit_tree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do sałatki dodałem pokrojoną w kostkę <w>smaczliwkę</w>.",
                            "target_lang_translations": {
                                "ru": "В салат я добавил нарезанное кубиками <w>авокадо</w>."
                            }
                        },
                        {
                            "text": "Dojrzała <w>smaczliwka</w> ma bardzo delikatny i kremowy miąższ.",
                            "target_lang_translations": {
                                "ru": "Спелое <w>авокадо</w> имеет очень нежную и кремовую мякоть."
                            }
                        }
                    ],
                    "synonyms": [
                        "awokado"
                    ],
                    "common_phrases": [
                        "dojrzała smaczliwka",
                        "olej ze smaczliwki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный экзотический фрукт грушевидной формы с зеленой или темной кожурой и маслянистой мякотью, широко известный как авокадо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "авокадо",
                                "target_lang_sense_clarification": "фрукт"
                            },
                            {
                                "target_lang_word": "аллигаторова груша",
                                "target_lang_sense_clarification": "устаревшее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7f7a7e5-e5b0-3396-9109-eb6a2930eec3",
                    "sense_definition": "Wiecznie zielone drzewo tropikalne z rodziny wawrzynowatych, które rodzi owoce zwane awokado.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fruit_tree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ogrodzie botanicznym rośnie wysoka <w>smaczliwka</w>.",
                            "target_lang_translations": {
                                "ru": "В ботаническом саду растет высокое <w>авокадо</w>."
                            }
                        },
                        {
                            "text": "Liście <w>smaczliwki</w> są ciemnozielone i błyszczące.",
                            "target_lang_translations": {
                                "ru": "Листья <w>авокадо</w> темно-зеленые и блестящие."
                            }
                        }
                    ],
                    "synonyms": [
                        "awokado"
                    ],
                    "common_phrases": [
                        "uprawa smaczliwki",
                        "drzewo smaczliwki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вечнозеленое тропическое дерево семейства лавровых, приносящее плоды, называемые авокадо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "авокадо",
                                "target_lang_sense_clarification": "дерево"
                            },
                            {
                                "target_lang_word": "персея",
                                "target_lang_sense_clarification": "ботаническое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c1d9500-b50c-31ac-91b9-baba8d9554a2",
                    "sense_definition": "Rodzaj roślin drzewiastych należący do rodziny wawrzynowatych, obejmujący wiele gatunków.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "genus",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Smaczliwka</w> wdzięczna to gatunek pochodzący z Ameryki Środkowej.",
                            "target_lang_translations": {
                                "ru": "<w>Персея</w> американская — это вид, происходящий из Центральной Америки."
                            }
                        },
                        {
                            "text": "Botanicy opisali wiele różnych gatunków należących do rodzaju <w>smaczliwka</w>.",
                            "target_lang_translations": {
                                "ru": "Ботаники описали множество различных видов, принадлежащих к роду <w>Персея</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rodzaj smaczliwka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Род древесных растений семейства лавровых, включающий множество видов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Персея",
                                "target_lang_sense_clarification": "род растений"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "passwordless"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "85023942-13a6-3eea-a599-5b9cf855c00d",
                    "sense_definition": "A secret string of characters used to log in to a computer, network, or account to prove identity and gain access.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please enter your username and <w>password</w> to access the website.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, введите имя пользователя и <w>пароль</w> для доступа к сайту."
                            }
                        },
                        {
                            "text": "A strong <w>password</w> usually contains a mix of letters, numbers, and symbols.",
                            "target_lang_translations": {
                                "ru": "Надежный <w>пароль</w> обычно содержит сочетание букв, цифр и символов."
                            }
                        }
                    ],
                    "synonyms": [
                        "passcode",
                        "pass",
                        "pwd"
                    ],
                    "common_phrases": [
                        "enter password",
                        "forgot password",
                        "reset password",
                        "strong password"
                    ],
                    "target_lang_definitions": {
                        "ru": "Секретный набор символов, используемый для входа в компьютер, сеть или учетную запись."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пароль",
                                "target_lang_sense_clarification": "код доступа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc7a0ccd-b017-3218-b337-d11f1d7e9203",
                    "sense_definition": "A secret word or phrase spoken to a guard or sentry to gain permission to enter a restricted place.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The guard refused to open the gate because the soldier did not know the <w>password</w>.",
                            "target_lang_translations": {
                                "ru": "Охранник отказался открывать ворота, так как солдат не знал <w>пароля</w>."
                            }
                        },
                        {
                            "text": "You must whisper the secret <w>password</w> to get into the underground club.",
                            "target_lang_translations": {
                                "ru": "Вы должны прошептать секретный <w>пароль</w>, чтобы попасть в подпольный клуб."
                            }
                        }
                    ],
                    "synonyms": [
                        "watchword"
                    ],
                    "common_phrases": [
                        "secret password",
                        "give the password"
                    ],
                    "target_lang_definitions": {
                        "ru": "Секретное слово или фраза, которую называют охране для получения разрешения на проход."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пароль",
                                "target_lang_sense_clarification": "секретное слово"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "768ba7da-7965-3d27-82cf-fa8c982cf5cc",
                    "sense_definition": "To protect a file, device, or system by requiring a password for access.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I decided to <w>password</w> the folder containing my financial documents.",
                            "target_lang_translations": {
                                "ru": "Я решил <w>защитить паролем</w> папку с моими финансовыми документами."
                            }
                        },
                        {
                            "text": "The system is <w>passworded</w> to prevent unauthorized users from making changes.",
                            "target_lang_translations": {
                                "ru": "Система <w>защищена паролем</w>, чтобы предотвратить внесение изменений посторонними пользователями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "password protect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защищать файл, устройство или систему, требуя ввода пароля для доступа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защитить паролем",
                                "target_lang_sense_clarification": "установить защиту"
                            },
                            {
                                "target_lang_word": "запаролить",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
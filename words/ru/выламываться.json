    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "211bcb45-9ce9-372b-8b13-b18dbb6bbc02",
                    "sense_definition": "Сломавшись, отделяться от основания, смещаться или выпадать наружу.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_break",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старые доски начали со скрипом <w>выламываться</w> из деревянного забора.",
                            "target_lang_translations": {
                                "en": "The old boards began to creakily <w>break out</w> of the wooden fence."
                            }
                        },
                        {
                            "text": "Под сильным напором ветра сухие ветки стали <w>выламываться</w> из ствола.",
                            "target_lang_translations": {
                                "en": "Under the strong pressure of the wind, dry branches began to <w>break off</w> from the trunk."
                            }
                        }
                    ],
                    "synonyms": [
                        "выпадать",
                        "вываливаться"
                    ],
                    "common_phrases": [
                        "выламываться из стены",
                        "выламываться с корнем"
                    ],
                    "target_lang_definitions": {
                        "en": "To separate from a base, shift, or fall out after breaking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break out",
                                "target_lang_sense_clarification": "to detach forcibly from a structure"
                            },
                            {
                                "target_lang_word": "break off",
                                "target_lang_sense_clarification": "to become separated by breaking"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5df1620-61f9-30b8-9d30-56d87773fdb0",
                    "sense_definition": "Вести себя неестественно, жеманиться, кривляться или притворяться с целью произвести впечатление.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Подросток продолжал <w>выламываться</w> перед сверстниками, пытаясь привлечь их внимание.",
                            "target_lang_translations": {
                                "en": "The teenager continued to <w>show off</w> in front of his peers, trying to attract their attention."
                            }
                        },
                        {
                            "text": "Тебе не стоит так <w>выламываться</w>, просто будь собой.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>put on airs</w> like that, just be yourself."
                            }
                        }
                    ],
                    "synonyms": [
                        "кривляться",
                        "ломаться",
                        "паясничать",
                        "жеманиться"
                    ],
                    "antonyms": [
                        "вести себя естественно"
                    ],
                    "common_phrases": [
                        "хватит выламываться",
                        "не надо выламываться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To behave unnaturally, mince, grimace, or pretend in order to impress."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show off",
                                "target_lang_sense_clarification": "to behave boastfully to attract attention"
                            },
                            {
                                "target_lang_word": "put on airs",
                                "target_lang_sense_clarification": "to act superior or unnatural"
                            },
                            {
                                "target_lang_word": "act up",
                                "target_lang_sense_clarification": "to behave in an unruly or strange manner"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c4b95c3-b40d-3d3e-a7fd-426b2a2f6721",
                    "sense_definition": "С большим трудом привыкать к новым условиям или подчиняться чьей-либо воле (переламывать себя).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "adaptation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему пришлось долго <w>выламываться</w> под строгие армейские порядки.",
                            "target_lang_translations": {
                                "en": "He had to <w>bend</w> to the strict army regulations for a long time."
                            }
                        },
                        {
                            "text": "Сотрудник не хотел <w>выламываться</w> под требования нового начальства.",
                            "target_lang_translations": {
                                "en": "The employee didn't want to <w>conform</w> to the new management's demands."
                            }
                        }
                    ],
                    "synonyms": [
                        "приспосабливаться",
                        "подчиняться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "сниженная лексика"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To adapt to new conditions or submit to someone's will with great difficulty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bend",
                                "target_lang_sense_clarification": "to force oneself to submit or adapt"
                            },
                            {
                                "target_lang_word": "conform",
                                "target_lang_sense_clarification": "to comply with rules or standards"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bc4ca57-fb90-313e-8c4e-2d3571f77cf6",
                    "sense_definition": "Изгибаться высокой дугой (обычно о бровях при выражении эмоций).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её тонкие брови начали удивлённо <w>выламываться</w> при виде этого зрелища.",
                            "target_lang_translations": {
                                "en": "Her thin eyebrows began to <w>arch</w> in surprise at the sight."
                            }
                        },
                        {
                            "text": "Брови актёра умели комично <w>выламываться</w>, выражая скепсис.",
                            "target_lang_translations": {
                                "en": "The actor's eyebrows could <w>arch</w> comically, expressing skepticism."
                            }
                        }
                    ],
                    "synonyms": [
                        "изгибаться",
                        "приподниматься"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To curve into a high arch (usually regarding eyebrows)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "arch",
                                "target_lang_sense_clarification": "to form a curved shape"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d23458cf-901f-3436-ad66-5a3679e7b171",
                    "sense_definition": "Команда немедленно прекратить движение или действие.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "command_halt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель автобуса громко крикнул: «<w>Стоп</w>!»",
                            "target_lang_translations": {
                                "en": "The bus driver shouted loudly: \"<w>Stop</w>!\""
                            }
                        },
                        {
                            "text": "<w>Стоп</w>, машина, мы приехали.",
                            "target_lang_translations": {
                                "en": "<w>Stop</w> the car, we have arrived."
                            }
                        }
                    ],
                    "synonyms": [
                        "стой",
                        "останови́сь"
                    ],
                    "antonyms": [
                        "давай",
                        "марш",
                        "старт"
                    ],
                    "common_phrases": [
                        "стоп машина",
                        "полный стоп"
                    ],
                    "target_lang_definitions": {
                        "en": "A command to immediately stop moving or acting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "command to halt"
                            },
                            {
                                "target_lang_word": "halt",
                                "target_lang_sense_clarification": "military or formal command"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2249f1f8-d09e-31fb-9933-40520588aad8",
                    "sense_definition": "Возглас, означающий призыв прекратить разговор или действие, так как этого достаточно.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "command_enough",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Всё, <w>стоп</w>, хватит спорить об этом.",
                            "target_lang_translations": {
                                "en": "That's it, <w>stop</w>, enough arguing about this."
                            }
                        },
                        {
                            "text": "<w>Стоп</w>, я больше не хочу это слушать.",
                            "target_lang_translations": {
                                "en": "<w>Stop</w>, I don't want to listen to this anymore."
                            }
                        }
                    ],
                    "synonyms": [
                        "хватит"
                    ],
                    "common_phrases": [
                        "всё стоп",
                        "стоп игра"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation calling to stop a conversation or action because it is enough."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "cease activity"
                            },
                            {
                                "target_lang_word": "cut it out",
                                "target_lang_sense_clarification": "informal phrase"
                            }
                        ]
                    }
                }
            ]
        },

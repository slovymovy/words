    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "132e2836-7ec0-3e8f-82b8-4687e2bae5a7",
                    "sense_definition": "Wzbudzający politowanie lub pogardę z powodu swojej marności, nieudolności lub bardzo złej jakości.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quality_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego zachowanie podczas spotkania było po prostu <w>żałosne</w> i wszyscy czuliśmy wstyd.",
                            "target_lang_translations": {
                                "ru": "Его поведение во время встречи было просто <w>жалким</w>, и нам всем было стыдно."
                            }
                        },
                        {
                            "text": "To była <w>żałosna</w> próba usprawiedliwienia swojego błędu przed szefem.",
                            "target_lang_translations": {
                                "ru": "Это была <w>жалкая</w> попытка оправдать свою ошибку перед начальником."
                            }
                        }
                    ],
                    "synonyms": [
                        "beznadziejny",
                        "marny",
                        "nędzny",
                        "tragiczny",
                        "słaby",
                        "kiepski"
                    ],
                    "antonyms": [
                        "wspaniały",
                        "znakomity",
                        "świetny"
                    ],
                    "common_phrases": [
                        "żałosny widok",
                        "żałosna wymówka",
                        "żałosny poziom",
                        "żałosny koniec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий жалость или презрение из-за своей ничтожности, неумелости или очень низкого качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалкий",
                                "target_lang_sense_clarification": "вызывающий презрение или сочувствие"
                            },
                            {
                                "target_lang_word": "убогий",
                                "target_lang_sense_clarification": "крайне низкого качества, ничтожный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db1e7c4f-d26d-3561-93bc-d875bbe212a6",
                    "sense_definition": "Wyrażający głęboki smutek, żal lub cierpienie, często w odniesieniu do głosu lub wyglądu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_sadness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zza ściany dobiegał cichy, <w>żałosny</w> płacz małego dziecka.",
                            "target_lang_translations": {
                                "ru": "Из-за стены доносился тихий, <w>жалобный</w> плач маленького ребёнка."
                            }
                        },
                        {
                            "text": "Pies wydał z siebie <w>żałosny</w> skowyt, gdy jego właściciel zamknął drzwi.",
                            "target_lang_translations": {
                                "ru": "Собака издала <w>жалобный</w> вой, когда её хозяин закрыл дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "smutny",
                        "rozpaczliwy",
                        "boleściwy",
                        "smętny",
                        "tęskny"
                    ],
                    "antonyms": [
                        "radosny",
                        "wesoły"
                    ],
                    "common_phrases": [
                        "żałosny jęk",
                        "żałosny głos",
                        "żałosne wycie",
                        "żałosne westchnienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражающий глубокую грусть, скорбь или страдание, часто в отношении голоса или внешнего вида."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалобный",
                                "target_lang_sense_clarification": "выражающий страдание или мольбу (о голосе, звуке)"
                            },
                            {
                                "target_lang_word": "жалостный",
                                "target_lang_sense_clarification": "вызывающий сострадание, печальный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
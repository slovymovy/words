    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "423445cd-fb31-3cd3-956f-38da13d99857",
                    "sense_definition": "Me, as the object of a verb or preposition when I am also the subject.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I taught <w>myself</w> how to play the guitar.",
                            "target_lang_translations": {
                                "ru": "Я <w>сам</w> научился играть на гитаре."
                            }
                        },
                        {
                            "text": "I like to spend time by <w>myself</w> in the garden.",
                            "target_lang_translations": {
                                "ru": "Мне нравится проводить время <w>наедине с собой</w> в саду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "by myself",
                        "for myself",
                        "to myself",
                        "do it myself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Себя (как объект действия, совершаемого самим говорящим)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            },
                            {
                                "target_lang_word": "себе",
                                "target_lang_sense_clarification": "дательный/предложный падеж"
                            },
                            {
                                "target_lang_word": "собой",
                                "target_lang_sense_clarification": "творительный падеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4fc4d58-5623-38a9-9a0f-0ca28b31e5b5",
                    "sense_definition": "Personally; used to emphasize that I performed the action, not someone else.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>myself</w> witnessed the strange event yesterday.",
                            "target_lang_translations": {
                                "ru": "Я <w>сам</w> видел это странное событие вчера."
                            }
                        },
                        {
                            "text": "I wrote the entire report <w>myself</w>.",
                            "target_lang_translations": {
                                "ru": "Я написал весь отчет <w>сам</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "me"
                    ],
                    "common_phrases": [
                        "I myself"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лично; используется для подчеркивания того, что действие выполнил я, а не кто-то другой"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сам",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "сама",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc0873e4-a4ab-35f0-8f9e-17aa5f947c75",
                    "sense_definition": "Me; used as an object in place of 'me', often joined with another name or pronoun.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please send the final documents to John or <w>myself</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отправьте окончательные документы Джону или <w>мне</w>."
                            }
                        },
                        {
                            "text": "They invited my wife and <w>myself</w> to the dinner party.",
                            "target_lang_translations": {
                                "ru": "Они пригласили мою жену и <w>меня</w> на ужин."
                            }
                        }
                    ],
                    "synonyms": [
                        "me"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often considered non-standard in formal grammar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Меня/мне (используется как дополнение вместо \"me\", часто в сочетании с другим именем)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меня",
                                "target_lang_sense_clarification": "винительный падеж"
                            },
                            {
                                "target_lang_word": "мне",
                                "target_lang_sense_clarification": "дательный падеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75d3e491-c7bb-3060-8f50-cb1e6c0ee442",
                    "sense_definition": "In my normal state of health, mind, or character.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I do not feel like <w>myself</w> today because of the flu.",
                            "target_lang_translations": {
                                "ru": "Я сегодня <w>сам не свой</w> из-за гриппа."
                            }
                        },
                        {
                            "text": "After a good night of sleep, I was back to being <w>myself</w>.",
                            "target_lang_translations": {
                                "ru": "После хорошего сна я снова стал <w>самим собой</w>."
                            }

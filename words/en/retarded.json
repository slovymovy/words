                {
                    "sense_id": "2c9ad72b-e73f-35b7-8d12-9b9e94e43012",
                    "sense_definition": "Extremely stupid, foolish, or socially unacceptable.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot believe we have to wait three hours for a table; that is completely <w>retarded</w>.",

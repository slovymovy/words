    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8ad95b46-6aaa-3dd4-bda3-c1f962c697b6",
                    "sense_definition": "Обращаться к кому-либо с целью получить что-либо, побудить сделать что-либо или разрешить что-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "request_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>прошу</w> тебя помочь мне с домашним заданием.",
                            "target_lang_translations": {
                                "en": "I <w>ask</w> you to help me with my homework."
                            }
                        },
                        {
                            "text": "Студент <w>просил</w> преподавателя объяснить правило ещё раз.",
                            "target_lang_translations": {
                                "en": "The student <w>asked</w> the teacher to explain the rule one more time."
                            }
                        },
                        {
                            "text": "Мы будем <w>просить</w> прощения за наше опоздание.",
                            "target_lang_translations": {
                                "en": "We will <w>ask</w> for forgiveness for our lateness."
                            }
                        }
                    ],
                    "synonyms": [
                        "умолять",
                        "клянчить"
                    ],
                    "antonyms": [
                        "приказывать",
                        "требовать",
                        "заставлять"
                    ],
                    "common_phrases": [
                        "просить помощи",
                        "просить прощения",
                        "просить совета",
                        "просить разрешения",
                        "просить денег"
                    ],
                    "target_lang_definitions": {
                        "en": "To address someone in order to obtain something, induce them to do something, or permit something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to request an action, object, or permission"
                            },
                            {
                                "target_lang_word": "request",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "beg",
                                "target_lang_sense_clarification": "to ask earnestly, e.g., for forgiveness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13dcb517-76c0-3cbf-a681-812837271fdc",
                    "sense_definition": "Назначать определённую цену за продаваемый товар или услугу.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "trade_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сколько вы <w>просите</w> за этот старый автомобиль?",
                            "target_lang_translations": {
                                "en": "How much are you <w>asking</w> for this old car?"
                            }
                        },
                        {
                            "text": "Продавец на рынке <w>просил</w> слишком высокую цену за фрукты.",
                            "target_lang_translations": {
                                "en": "The seller at the market was <w>asking</w> too high a price for the fruit."
                            }
                        }
                    ],
                    "synonyms": [
                        "запрашивать",
                        "назначать цену"
                    ],
                    "common_phrases": [
                        "просить дорого",
                        "просить цену",
                        "много просить"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in context of bargaining or sales"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To set a specific price for a product or service being sold."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to demand a price"
                            },
                            {
                                "target_lang_word": "charge",
                                "target_lang_sense_clarification": "to set a price for a service or good"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1e756d7-a882-3f7d-94e9-cb0e7489baef",
                    "sense_definition": "Хлопотать и вступаться за кого-либо, пытаясь помочь в решении проблемы или смягчить наказание.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "intercession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она пришла <w>просить</w> за своего сына, который попал в беду.",
                            "target_lang_translations": {
                                "en": "She came to <w>plead</w> for her son, who had gotten into trouble."
                            }
                        },
                        {
                            "text": "Я не люблю <w>просить</w> за других людей перед начальством.",
                            "target_lang_translations": {
                                "en": "I don't like <w>interceding</w> for other people in front of the management."
                            }
                        }
                    ],
                    "synonyms": [
                        "ходатайствовать",
                        "вступаться",
                        "хлопотать"
                    ],
                    "common_phrases": [
                        "просить за друга",
                        "просить за сына",
                        "просить за себя"
                    ],
                    "target_lang_definitions": {
                        "en": "To bustle about and intercede for someone, trying to help solve a problem or mitigate punishment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plead",
                                "target_lang_sense_clarification": "to argue or appeal on someone's behalf"
                            },
                            {
                                "target_lang_word": "intercede",
                                "target_lang_sense_clarification": "to intervene for someone"
                            },
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to request on behalf of someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "999d61a3-ae04-3fd2-866c-cc7353cb7079",
                    "sense_definition": "Вежливо приглашать кого-либо войти, сесть или принять участие в трапезе.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "invitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хозяйка улыбнулась и <w>просила</w> всех гостей к столу.",
                            "target_lang_translations": {
                                "en": "The hostess smiled and <w>invited</w> all the guests to the table."
                            }
                        },
                        {
                            "text": "Милости <w>просим</w> в наш новый дом.",
                            "target_lang_translations": {
                                "en": "We kindly <w>welcome</w> you to our new home."
                            }
                        }
                    ],
                    "synonyms": [
                        "приглашать",
                        "звать"
                    ],
                    "antonyms": [
                        "выгонять"
                    ],
                    "common_phrases": [
                        "милости просим",
                        "просим к столу",
                        "просить садиться"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly used in fixed polite phrases or literature"
                        },
                        {
                            "trait_type": "form",
                            "comment": "polite etiquette"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To politely invite someone to enter, sit down, or take part in a meal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "invite",
                                "target_lang_sense_clarification": "to ask someone to come or participate"
                            },
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to invite someone in or to a table"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "432f1cea-47f1-3eca-9487-18eec6dc5ace",
                    "sense_definition": "Собирать милостыню, жить подаянием.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "begging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старик сидел у ворот церкви и молча <w>просил</w>.",
                            "target_lang_translations": {
                                "en": "The old man sat by the church gate and silently <w>begged</w>."
                            }
                        },
                        {
                            "text": "Ему было стыдно <w>просить</w> милостыню на улице.",
                            "target_lang_translations": {
                                "en": "He was ashamed to <w>beg</w> for alms on the street."
                            }
                        }
                    ],
                    "synonyms": [
                        "побираться",
                        "нищенствовать"
                    ],
                    "antonyms": [
                        "подавать"
                    ],
                    "common_phrases": [
                        "просить милостыню",
                        "просить на хлеб",
                        "просить Христа ради"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies begging for alms"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To collect alms, to live by begging."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beg",
                                "target_lang_sense_clarification": "to ask for money or food as charity"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
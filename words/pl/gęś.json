            "senses": [
                {
                    "sense_id": "df760581-9d99-38f0-8225-4117e038083a",
                    "sense_definition": "Duży ptak wodny z rodziny kaczkowatych, dziki lub hodowany w gospodarstwach dla mięsa i pierza.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wiejskim podwórku głośno gęgała biała <w>gęś</w>.",
                            "target_lang_translations": {
                                "ru": "На деревенском дворе громко гоготал белый <w>гусь</w>."
                            }
                        },
                        {
                            "text": "Jesienią dzikie <w>gęsi</w> odlatują do ciepłych krajów w poszukiwaniu pożywienia.",
                            "target_lang_translations": {
                                "ru": "Осенью дикие <w>гуси</w> улетают в теплые края в поисках пищи."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) ganś, gynś"
                    ],
                    "common_phrases": [
                        "dzika gęś",
                        "szara gęś",
                        "gęś domowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная водоплавающая птица семейства утиных, дикая или разводимая в хозяйствах ради мяса и перьев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гусь",
                                "target_lang_sense_clarification": "птица"
                            },
                            {
                                "target_lang_word": "гусыня",
                                "target_lang_sense_clarification": "самка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4dfc61e6-5d28-3392-aa7d-d7d83b9f4c27",
                    "sense_definition": "Mięso z gęsi lub potrawa przyrządzona z tego mięsa.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na uroczysty obiad podano pieczoną <w>gęś</w> z jabłkami.",
                            "target_lang_translations": {
                                "ru": "На праздничный обед подали запеченного <w>гуся</w> с яблоками."
                            }
                        },
                        {
                            "text": "W tej restauracji serwują wyśmienitą <w>gęś</w> z modrą kapustą.",
                            "target_lang_translations": {
                                "ru": "В этом ресторане подают отменного <w>гуся</w> с красной капустой."
                            }
                        }
                    ],
                    "synonyms": [
                        "gęsina"
                    ],
                    "common_phrases": [
                        "pieczona gęś",
                        "gęś z jabłkami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясо гуся или блюдо, приготовленное из этого мяса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гусь",
                                "target_lang_sense_clarification": "блюдо"
                            },
                            {

                },
                {
                    "sense_id": "0a4606e4-5135-3ba4-94f9-36f9b64f49e1",
                    "sense_definition": "Een verplichte heffing of belasting die aan onderworpenen wordt opgelegd.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

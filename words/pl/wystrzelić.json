                {
                    "sense_id": "feeb2127-b972-31f4-ae74-897062973575",
                    "sense_definition": "Spowodować wyrzucenie pocisku, strzały lub rakiety z dużą siłą, używając do tego broni lub specjalnego urządzenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shooting_launching",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz wycelował dokładnie i <w>wystrzelił</w> w kierunku tarczy.",
                            "target_lang_translations": {
                                "ru": "Солдат точно прицелился и <w>выстрелил</w> в сторону мишени."
                            }
                        },
                        {
                            "text": "W noc sylwestrową o północy wspólnie <w>wystrzelimy</w> fajerwerki.",
                            "target_lang_translations": {
                                "ru": "В новогоднюю ночь в полночь мы вместе <w>запустим</w> фейерверки."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpalić",
                        "wypuścić"
                    ],
                    "common_phrases": [
                        "wystrzelić pocisk",
                        "wystrzelić rakietę",
                        "wystrzelić z pistoletu",
                        "wystrzelić w powietrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпустить снаряд, стрелу или ракету с большой силой, используя оружие или специальное устройство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выстрелить",
                                "target_lang_sense_clarification": "из оружия"
                            },
                            {
                                "target_lang_word": "запустить",
                                "target_lang_sense_clarification": "ракету, фейерверк"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d6c8e25-6635-3270-a5ba-27b3c61f7821",
                    "sense_definition": "Poruszyć się bardzo szybko i gwałtownie w określonym kierunku lub nagle się pojawić.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rapid_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samochód wyścigowy <w>wystrzelił</w> jak z procy, gdy tylko zapaliło się zielone światło.",
                            "target_lang_translations": {
                                "ru": "Гоночный автомобиль <w>рванул</w> как из рогатки, как только загорелся зеленый свет."
                            }
                        },
                        {
                            "text": "Nagle z rękawa iluzjonisty <w>wystrzeliło</w> coś srebrzystego i upadło na podłogę.",
                            "target_lang_translations": {
                                "ru": "Внезапно из рукава иллюзиониста <w>вылетело</w> что-то серебристое и упало на пол."
                            }
                        },
                        {
                            "text": "Ceny mieszkań w ostatnim roku gwałtownie <w>wystrzeliły</w> w górę.",
                            "target_lang_translations": {
                                "ru": "Цены на квартиры в прошлом году резко <w>взлетели</w> вверх."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyskoczyć",
                        "pomknąć",
                        "wyrwać się"
                    ],
                    "common_phrases": [
                        "wystrzelić jak z procy",
                        "wystrzelić w górę",
                        "wystrzelić do przodu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень быстро и резко двинуться в определенном направлении или внезапно появиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рвануть",
                                "target_lang_sense_clarification": "резко двинуться с места"
                            },
                            {
                                "target_lang_word": "взлететь",
                                "target_lang_sense_clarification": "о ценах, резко подняться"
                            },
                            {
                                "target_lang_word": "вылететь",
                                "target_lang_sense_clarification": "быстро выскочить"
                            }
                        ]
                    }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d39df440-cf7e-344c-b450-3fa2f41ba37e",
                    "sense_definition": "To run or move at top speed for a short distance or period.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rapid_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I had to <w>sprint</w> to catch the bus before it left the station.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось <w>рвануть</w>, чтобы успеть на автобус, пока он не уехал со станции."
                            }
                        },
                        {
                            "text": "The players <w>sprint</w> across the field during their warm-up routine.",
                            "target_lang_translations": {
                                "ru": "Игроки <w>спринтуют</w> по полю во время разминки."
                            }
                        }
                    ],
                    "synonyms": [
                        "dash",
                        "bolt",
                        "race",
                        "rush"
                    ],
                    "antonyms": [
                        "jog",
                        "plod",
                        "amble"
                    ],
                    "common_phrases": [
                        "sprint to the finish",
                        "sprint away",
                        "sprint for the bus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бежать или двигаться с максимальной скоростью на короткое расстояние или в течение короткого времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рвануть",
                                "target_lang_sense_clarification": "резко побежать"
                            },
                            {
                                "target_lang_word": "спринтовать",
                                "target_lang_sense_clarification": "в спорте"
                            },
                            {
                                "target_lang_word": "мчаться",
                                "target_lang_sense_clarification": "быстро двигаться"
                            }
                        ]
                    }
                }
            ]
        },
        {

                    "sense_id": "daa18b45-fe9c-30e1-b898-94e96f3bb0a6",
                    "sense_definition": "Выход жидкости, газа или сыпучего вещества из ёмкости вследствие повреждения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_leak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В подвале дома произошла <w>утечка</w> воды из старой трубы.",
                            "target_lang_translations": {
                                "en": "There was a water <w>leak</w> from an old pipe in the basement of the house."
                            }
                        },
                        {
                            "text": "Специалисты быстро устранили опасную <w>утечку</w> газа.",
                            "target_lang_translations": {
                                "en": "Specialists quickly fixed the dangerous gas <w>leak</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "проте́чка",
                        "течь",
                        "вытекание",
                        "утекание"
                    ],
                    "antonyms": [
                        "притекание"
                    ],
                    "common_phrases": [
                        "утечка газа",
                        "утечка воды",
                        "утечка нефти",
                        "обнаружить утечку"
                    ],
                    "target_lang_definitions": {
                        "en": "The escape of liquid, gas, or loose substance from a container due to damage."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leak",
                                "target_lang_sense_clarification": "hole or escape of fluid"
                            },
                            {

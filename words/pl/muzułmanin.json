            "senses": [
                {
                    "sense_id": "bc6d5a44-eaf3-3862-9061-8c3b18a99b0c",
                    "sense_definition": "wyznawca islamu",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Według praw islamu, jeśli ojcem jest <w>muzułmanin</w>, dziecko też jest muzułmaninem.",
                            "target_lang_translations": {
                                "ru": "Согласно законам ислама, если отец — <w>мусульманин</w>, ребёнок тоже является мусульманином."
                            }
                        },
                        {
                            "text": "Każdy dorosły <w>muzułmanin</w> powinien przestrzegać postu w czasie świętego miesiąca ramadanu.",
                            "target_lang_translations": {
                                "ru": "Каждый взрослый <w>мусульманин</w> должен соблюдать пост во время священного месяца Рамадан."
                            }
                        }
                    ],
                    "synonyms": [
                        "mahometanin"
                    ],
                    "common_phrases": [
                        "praktykujący muzułmanin",
                        "pobożny muzułmanin"
                    ],
                    "target_lang_definitions": {
                        "ru": "последователь ислама"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мусульманин",
                                "target_lang_sense_clarification": "религия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "291b7fe9-4314-3780-a76c-db2fee849831",
                    "sense_definition": "przedstawiciel grupy etnicznej w Bośni",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ethnicity",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "I wreszcie <w>Muzułmanie</w> i Chorwaci stanowią dla serbskich nacjonalistów przykład narodów, które swą prawdziwą tożsamość zdradziły.",
                            "target_lang_translations": {
                                "ru": "И наконец, <w>мусульмане</w> и хорваты являются для сербских националистов примером народов, предавших свою истинную идентичность."
                            }
                        },
                        {
                            "text": "Wielu etnicznych <w>Muzułmanów</w> zamieszkuje tereny dzisiejszej Bośni i Hercegowiny.",
                            "target_lang_translations": {
                                "ru": "Многие этнические <w>мусульмане</w> населяют территории современной Боснии и Герцеговины."
                            }
                        }
                    ],
                    "synonyms": [
                        "Boszniak"
                    ],
                    "target_lang_definitions": {
                        "ru": "представитель этнической группы в Боснии (славянин-мусульманин)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мусульманин",
                                "target_lang_sense_clarification": "этническая принадлежность"
                            },
                            {
                                "target_lang_word": "бошняк",
                                "target_lang_sense_clarification": "современное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d01e1c2d-4389-3822-8df1-6595c6875572",
                    "sense_definition": "ekstremalnie wygłodzony więzień obozu koncentracyjnego",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "history_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więźniowie nazywali go <w>muzułmaninem</w>, gdyż stracił wolę życia z powodu skrajnego głodu.",
                            "target_lang_translations": {
                                "ru": "Заключённые называли его <w>мусульманином</w>, так как он потерял волю к жизни из-за крайнего голода."
                            }
                        },
                        {
                            "text": "Ten <w>muzułmanin</w> był tak wycieńczony, że nie mógł już wstać z pryczy.",
                            "target_lang_translations": {
                                "ru": "Этот <w>мусульманин</w> был так истощён, что не мог уже встать с нар."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gwara obozowa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "крайне истощённый узник концлагеря, потерявший волю к жизни"
                    },

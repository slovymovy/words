    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "976f0cf2-2f9e-3cdb-a73c-1e41f2914022",
                    "sense_definition": "The process of keeping a machine, building, or system in good condition by checking or repairing it regularly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "upkeep_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old bridge requires constant <w>maintenance</w> to remain safe for traffic.",
                            "target_lang_translations": {
                                "ru": "Старый мост требует постоянного <w>обслуживания</w>, чтобы оставаться безопасным для движения."
                            }
                        },
                        {
                            "text": "Regular <w>maintenance</w> can extend the life of your car engine significantly.",
                            "target_lang_translations": {
                                "ru": "Регулярное <w>техническое обслуживание</w> может значительно продлить срок службы двигателя вашего автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "upkeep",
                        "servicing",
                        "repair",
                        "care"
                    ],
                    "antonyms": [
                        "neglect"
                    ],
                    "common_phrases": [
                        "regular maintenance",
                        "maintenance costs",
                        "high maintenance",
                        "low maintenance",
                        "preventive maintenance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс поддержания машины, здания или системы в исправном состоянии посредством регулярных проверок или ремонта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обслуживание",
                                "target_lang_sense_clarification": "технический уход, сервис"
                            },
                            {
                                "target_lang_word": "ремонт",
                                "target_lang_sense_clarification": "починка, устранение неисправностей"
                            },
                            {
                                "target_lang_word": "содержание",
                                "target_lang_sense_clarification": "уход за зданием или имуществом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "065a911c-d2da-3447-9d39-8895bcc5442b",
                    "sense_definition": "Work done on software or computer systems to fix errors, improve performance, or ensure stability.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "upkeep_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The website will be offline tonight for scheduled server <w>maintenance</w>.",
                            "target_lang_translations": {
                                "ru": "Сегодня ночью веб-сайт будет недоступен из-за планового <w>обслуживания</w> сервера."
                            }
                        },
                        {
                            "text": "This <w>maintenance</w> release fixes several critical security bugs.",
                            "target_lang_translations": {
                                "ru": "Этот выпуск <w>технической поддержки</w> исправляет несколько критических ошибок безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "update",
                        "patching"
                    ],
                    "common_phrases": [
                        "scheduled maintenance",
                        "maintenance mode",
                        "maintenance release"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Работы, проводимые с программным обеспечением или компьютерными системами для исправления ошибок, улучшения производительности или обеспечения стабильности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обслуживание",
                                "target_lang_sense_clarification": "серверов или оборудования"
                            },
                            {
                                "target_lang_word": "поддержка",
                                "target_lang_sense_clarification": "техническая поддержка ПО"
                            },
                            {
                                "target_lang_word": "сопровождение",
                                "target_lang_sense_clarification": "долгосрочная поддержка системы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96d82ce5-e7ef-3991-a732-bb0ed7dea955",
                    "sense_definition": "Money that a person is legally ordered to pay regularly to support a former spouse or children after a separation or divorce.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He pays a significant amount of monthly <w>maintenance</w> for his three children.",
                            "target_lang_translations": {
                                "ru": "Он платит значительную сумму ежемесячных <w>алиментов</w> на троих детей."
                            }
                        },
                        {
                            "text": "The court decided the amount of spousal <w>maintenance</w> based on her income needs.",
                            "target_lang_translations": {
                                "ru": "Суд определил размер <w>содержания</w> супруги, основываясь на ее финансовых потребностях."
                            }
                        }
                    ],
                    "synonyms": [
                        "alimony",
                        "support",
                        "upkeep",
                        "subsistence"
                    ],
                    "common_phrases": [
                        "child maintenance",
                        "spousal maintenance",
                        "pay maintenance"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "law"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежные средства, которые лицо по закону обязано регулярно выплачивать для обеспечения бывшего супруга или детей после расставания или развода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алименты",
                                "target_lang_sense_clarification": "выплаты на детей или супруга"
                            },
                            {
                                "target_lang_word": "содержание",
                                "target_lang_sense_clarification": "материальное обеспечение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "667013bd-dc93-3be6-af46-691794ecaf84",
                    "sense_definition": "The unlawful act of supporting a lawsuit by a person who has no legitimate interest in the case.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer was accused of <w>maintenance</w> for funding the litigation of a stranger.",
                            "target_lang_translations": {
                                "ru": "Адвоката обвинили в <w>незаконной поддержке</w> иска за финансирование судебного процесса незнакомца."
                            }
                        },
                        {
                            "text": "Laws against <w>maintenance</w> were originally designed to prevent powerful nobles from interfering in justice.",
                            "target_lang_translations": {
                                "ru": "Законы против <w>вмешательства в тяжбы</w> изначально были разработаны для предотвращения влияния могущественных дворян на правосудие."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Противоправное действие по поддержке судебного иска лицом, не имеющим законного интереса в данном деле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддержка",
                                "target_lang_sense_clarification": "незаконное финансирование тяжбы"
                            }
                        ]
                    }
                }
            ]
        }
    ]

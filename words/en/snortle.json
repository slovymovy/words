    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5135fecd-919c-3909-85d3-fc8a41ef73d1",
                    "sense_definition": "To laugh loudly and heartily while making a snorting sound through the nose.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hannah looked elegant until she <w>snortled</w> at the dinner table.",
                            "target_lang_translations": {
                                "ru": "Ханна выглядела элегантно, пока не <w>фыркнула от смеха</w> за обеденным столом."
                            }
                        },
                        {
                            "text": "He tried to keep a straight face but <w>snortled</w> with laughter at the joke.",
                            "target_lang_translations": {
                                "ru": "Он пытался сохранить невозмутимое лицо, но <w>прыснул</w> со смеху над шуткой."
                            }
                        }
                    ],
                    "synonyms": [
                        "chortle"
                    ],
                    "common_phrases": [
                        "snortle with laughter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громко и от души смеяться, издавая носом фыркающий звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фыркнуть от смеха",
                                "target_lang_sense_clarification": "издать звук носом при смехе"
                            },
                            {
                                "target_lang_word": "прыснуть",
                                "target_lang_sense_clarification": "внезапно и громко рассмеяться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f6e9ec99-ad99-3e5d-9292-404bc24f25bf",
                    "sense_definition": "A loud, hearty laugh accompanied by a snorting sound.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A sudden <w>snortle</w> escaped him during the quiet meeting.",
                            "target_lang_translations": {
                                "ru": "Внезапный <w>смешок с фырканьем</w> вырвался у него во время тихого собрания."
                            }
                        },
                        {
                            "text": "Her infectious <w>snortle</w> made everyone else in the room start giggling.",
                            "target_lang_translations": {
                                "ru": "Её заразительный <w>фыркающий смех</w> заставил всех остальных в комнате захихикать."
                            }
                        }
                    ],
                    "synonyms": [
                        "chortle"
                    ],
                    "common_phrases": [
                        "let out a snortle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громкий, искренний смех, сопровождаемый фырканьем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фыркающий смех",
                                "target_lang_sense_clarification": "смех, сопровождаемый звуками носом"
                            },
                            {
                                "target_lang_word": "смешок с фырканьем",
                                "target_lang_sense_clarification": "короткий смех с характерным звуком"
                            }
                        ]
                    }
                }
            ]
        }
    ]

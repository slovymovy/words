    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4fe15dee-a421-3dcf-992b-b3f46e15e4f0",
                    "sense_definition": "Осуществляемый руками, действующий при помощи рук, а не автоматически или с помощью сложных механизмов.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hand_operated",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель забыл опустить <ручной> тормоз перед началом движения.",
                            "target_lang_translations": {
                                "en": "The driver forgot to release the <hand> brake before starting to move."
                            }
                        },
                        {
                            "text": "В сельском хозяйстве до сих пор часто используется тяжёлый <ручной> труд.",
                            "target_lang_translations": {
                                "en": "Heavy <manual> labor is still often used in agriculture."
                            }
                        }
                    ],
                    "antonyms": [
                        "автоматический",
                        "автоматизированный",
                        "механизированный",
                        "промышленный"
                    ],
                    "common_phrases": [
                        "ручной труд",
                        "ручной тормоз",
                        "ручное управление",
                        "ручной привод"
                    ],
                    "target_lang_definitions": {
                        "en": "Done or operated by hand rather than automatically or by machine."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "manual",
                                "target_lang_sense_clarification": "involving physical work or operation by hand"
                            },
                            {
                                "target_lang_word": "hand",
                                "target_lang_sense_clarification": "used in compounds like 'hand brake'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "665ce98b-2006-326f-9ebb-73eab37e54cf",
                    "sense_definition": "Изготовленный руками, кустарным способом, а не на фабрике.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На ярмарке продавались красивые сувениры <ручной> работы.",
                            "target_lang_translations": {
                                "en": "Beautiful <handmade> souvenirs were sold at the fair."
                            }
                        },
                        {
                            "text": "Этот свитер <ручной> вязки стоит очень дорого.",
                            "target_lang_translations": {
                                "en": "This <hand-knitted> sweater is very expensive."
                            }
                        }
                    ],
                    "synonyms": [
                        "рукодельный"
                    ],
                    "antonyms": [
                        "серийный",
                        "промышленный",
                        "дикий"
                    ],
                    "common_phrases": [
                        "ручная работа",
                        "изделие ручной работы",
                        "ручная вязка"
                    ],
                    "target_lang_definitions": {
                        "en": "Made by hand rather than in a factory; artisanal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "handmade",
                                "target_lang_sense_clarification": "made by hand"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cc9b51d-9104-3d40-9d43-ab4cfd88b79d",
                    "sense_definition": "О багаже: небольшой, предназначенный для переноски в руках и размещения в салоне транспортного средства.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "hand_operated",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В самолёт разрешено брать только одну сумку <ручной> клади.",
                            "target_lang_translations": {
                                "en": "You are allowed to take only one bag of <hand> luggage on the plane."
                            }
                        },
                        {
                            "text": "Ваша <ручная> кладь должна свободно помещаться под сиденьем.",
                            "target_lang_translations": {
                                "en": "Your <carry-on> luggage must fit freely under the seat."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ручная кладь"
                    ],
                    "target_lang_definitions": {
                        "en": "Small luggage designed to be carried by hand and taken into the vehicle cabin."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hand",
                                "target_lang_sense_clarification": "as in 'hand luggage'"
                            },
                            {
                                "target_lang_word": "carry-on",
                                "target_lang_sense_clarification": "luggage taken into the cabin"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1918d378-0b1d-3bec-9a62-0948ad4cb836",
                    "sense_definition": "Приручённый, привыкший к человеку, не дикий (о животных).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В городском парке живут абсолютно <ручные> белки, которые берут еду с ладони.",
                            "target_lang_translations": {
                                "en": "Absolutely <tame> squirrels live in the city park, taking food from your palm."
                            }
                        },
                        {
                            "text": "У лесника жил <ручной> волк, который вёл себя как добрая собака.",
                            "target_lang_translations": {
                                "en": "The forester had a <tame> wolf that behaved like a kind dog."
                            }
                        }
                    ],
                    "synonyms": [
                        "приручённый"
                    ],
                    "antonyms": [
                        "дикий"
                    ],
                    "common_phrases": [
                        "ручной зверь",
                        "стать ручным"
                    ],
                    "target_lang_definitions": {
                        "en": "Accustomed to humans, not wild (referring to animals)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tame",
                                "target_lang_sense_clarification": "docile, not afraid of humans"
                            },
                            {
                                "target_lang_word": "domesticated",
                                "target_lang_sense_clarification": "bred to live with humans"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1131b011-1665-3c05-b711-84eb5caeb742",
                    "sense_definition": "Предназначенный для держания в руках или использования руками (об оружии, инструментах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hand_operated",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдат научился точно бросать <ручную> гранату в цель.",
                            "target_lang_translations": {
                                "en": "The soldier learned to throw a <hand> grenade accurately at the target."
                            }
                        },
                        {
                            "text": "Для мелкого ремонта дома мне нужна обычная <ручная> пила.",
                            "target_lang_translations": {
                                "en": "For minor home repairs, I need an ordinary <hand> saw."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ручная граната",
                        "ручной пулемёт",
                        "ручная пила"
                    ],
                    "target_lang_definitions": {
                        "en": "Designed to be held or used with the hands (referring to tools or weapons)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hand",
                                "target_lang_sense_clarification": "as in 'hand grenade' or 'hand saw'"
                            },
                            {
                                "target_lang_word": "hand-held",
                                "target_lang_sense_clarification": "portable, held in the hand"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1221b7b-acc4-3069-96e2-4b9aa46d413b",
                    "sense_definition": "Послушный, полностью подчиняющийся чужой воле, зависимый (о человеке).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор хотел сделать из своих заместителей <ручных> исполнителей.",
                            "target_lang_translations": {
                                "en": "The director wanted to turn his deputies into <tame> executors."
                            }
                        },
                        {
                            "text": "У этого влиятельного политика есть свои <ручные> журналисты.",
                            "target_lang_translations": {
                                "en": "This influential politician has his own <tame> journalists."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "переносное значение"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Submissive, fully controlled by another's will (referring to a person)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tame",
                                "target_lang_sense_clarification": "figurative: submissive, compliant"
                            },
                            {
                                "target_lang_word": "compliant",
                                "target_lang_sense_clarification": "obedient to others"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
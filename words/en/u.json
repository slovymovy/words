{
    "word_family": [
        "non-U",
        "U-turn"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "653dfa25-c481-3c7f-bc23-16f09e9307a1",
                    "sense_definition": "Abbreviation of the word \"you\", commonly used in text messaging and internet slang.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will text <w>u</w> later when I finish my work.",
                            "target_lang_translations": {
                                "ru": "Я напишу <w>тебе</w> позже, когда закончу работу."
                            }
                        },
                        {
                            "text": "Are <w>u</w> coming to the party tonight?",
                            "target_lang_translations": {
                                "ru": "<w>Ты</w> идешь сегодня на вечеринку?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "how r u",
                        "nice to meet u",
                        "see u later"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang and text messaging"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от слова \"you\" (ты/вы), часто используемое в текстовых сообщениях и интернет-сленге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ты",
                                "target_lang_sense_clarification": "неформальное обращение"
                            },
                            {
                                "target_lang_word": "вы",
                                "target_lang_sense_clarification": "формальное или множественное число"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "058e7410-e97b-33ef-a630-9b7fba6b038f",
                    "sense_definition": "The twenty-first letter of the English alphabet.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "letter_u",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The word \"umbrella\" starts with the letter <w>u</w>.",
                            "target_lang_translations": {
                                "ru": "Слово \"umbrella\" начинается с буквы <w>u</w>."
                            }
                        },
                        {
                            "text": "There is a silent <w>u</w> in the word \"guess\".",
                            "target_lang_translations": {
                                "ru": "В слове \"guess\" есть непроизносимая <w>u</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "letter u"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двадцать первая буква английского алфавита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ю",
                                "target_lang_sense_clarification": "название буквы в алфавите"
                            },
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "звучание или транслитерация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8484089-4d04-3477-806a-7d332b464592",
                    "sense_definition": "An object, curve, or turn shaped like the letter U.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "letter_u",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pipe under the sink forms a <w>U</w> to trap water.",
                            "target_lang_translations": {
                                "ru": "Труба под раковиной образует <w>колено</w>, чтобы задерживать воду."
                            }
                        },
                        {
                            "text": "The river makes a wide <w>U</w> around the peninsula.",
                            "target_lang_translations": {
                                "ru": "Река делает широкую <w>петлю</w> вокруг полуострова."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, изгиб или поворот в форме буквы U."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуга",
                                "target_lang_sense_clarification": "форма"
                            },
                            {
                                "target_lang_word": "петля",
                                "target_lang_sense_clarification": "изгиб дороги или реки"
                            },
                            {
                                "target_lang_word": "колено",
                                "target_lang_sense_clarification": "часть трубы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c91041bb-4075-3ec6-a08f-54791eec3a67",
                    "sense_definition": "A U-turn; a 180-degree turn made by a vehicle to reverse direction.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The driver pulled a quick <w>U</w> in the middle of the empty street.",
                            "target_lang_translations": {
                                "ru": "Водитель сделал быстрый <w>разворот</w> посреди пустой улицы."
                            }
                        },
                        {
                            "text": "We missed the exit, so we had to do a <w>U</w> at the next intersection.",
                            "target_lang_translations": {
                                "ru": "Мы пропустили съезд, поэтому пришлось делать <w>разворот</w> на следующем перекрестке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do a U",
                        "pull a U"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разворот на 180 градусов; маневр автомобиля для смены направления на противоположное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разворот",
                                "target_lang_sense_clarification": "маневр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "767f5ce4-efde-3dbc-a4a0-dd6434b16af2",
                    "sense_definition": "An honorific title used before the name of an adult man in Myanmar (Burma), similar to \"Mr.\".",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>U</w> Thant served as the Secretary-General of the United Nations.",
                            "target_lang_translations": {
                                "ru": "<w>У</w> Тан служил Генеральным секретарем Организации Объединенных Наций."
                            }
                        },
                        {
                            "text": "The letter was addressed to <w>U</w> Kyaw, a respected elder in the village.",
                            "target_lang_translations": {
                                "ru": "Письмо было адресовано <w>У</w> Чжо, уважаемому старейшине деревни."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Myanmar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Почтительное обращение к взрослому мужчине в Мьянме (Бирме), аналогичное \"господин\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "У",
                                "target_lang_sense_clarification": "бирманское обращение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "44fd8b00-007b-379a-9f4d-a6bc1501f6c0",
                    "sense_definition": "Abbreviation of \"under\", used in sports and classifications to indicate an age group below a specific number.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was selected to play for the national <w>U</w>19 hockey team.",
                            "target_lang_translations": {
                                "ru": "Ее отобрали в национальную хоккейную команду <w>до</w> 19 лет."
                            }
                        },
                        {
                            "text": "The tournament is open to all <w>U</w>16 players in the region.",
                            "target_lang_translations": {
                                "ru": "Турнир открыт для всех игроков <w>младше</w> 16 лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "U-21 team",
                        "U-18 squad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"under\" (младше), используется в спорте для обозначения возрастных категорий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "возрастное ограничение"
                            },
                            {
                                "target_lang_word": "младше",
                                "target_lang_sense_clarification": "возраст"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d367317f-b25b-34da-913e-dcae00db6052",
                    "sense_definition": "Characteristic of the upper classes, particularly regarding social etiquette and language usage.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "According to the essay, using a napkin is <w>U</w>, while using a serviette is not.",
                            "target_lang_translations": {
                                "ru": "Согласно эссе, использование салфетки — это <w>аристократично</w>, а не простонародно."
                            }
                        },
                        {
                            "text": "She was very concerned with whether her vocabulary was considered <w>U</w> or non-U.",
                            "target_lang_translations": {
                                "ru": "Ее очень волновало, считается ли ее лексика <w>светской</w> или нет."
                            }
                        }
                    ],
                    "antonyms": [
                        "non-U"
                    ],
                    "common_phrases": [
                        "U and non-U",
                        "U speaker"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "sociolinguistics"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Характерный для высших слоев общества (о языке и манерах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аристократический",
                                "target_lang_sense_clarification": "о манерах или речи"
                            },
                            {
                                "target_lang_word": "светский",
                                "target_lang_sense_clarification": "принятый в высшем обществе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "742a6579-f946-38ef-a5ff-8bed02f74ea9",
                    "sense_definition": "Abbreviation of \"universal\"; a film certification indicating the movie is suitable for all ages.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The animated movie was given a <w>U</w> certificate by the censors.",
                            "target_lang_translations": {
                                "ru": "Цензоры присвоили мультфильму категорию <w>U</w> (без ограничений)."
                            }
                        },
                        {
                            "text": "Most family comedies are rated <w>U</w> so that children can watch them.",
                            "target_lang_translations": {
                                "ru": "Большинство семейных комедий имеют рейтинг <w>U</w>, чтобы их могли смотреть дети."
                            }
                        }
                    ],
                    "common_phrases": [
                        "U certificate",
                        "rated U"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"universal\"; категория фильмов без возрастных ограничений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "без ограничений",
                                "target_lang_sense_clarification": "рейтинг фильма"
                            },
                            {
                                "target_lang_word": "универсальный",
                                "target_lang_sense_clarification": "для всех возрастов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "39a25359-a3be-3c8a-907a-72588c24967e",
                    "sense_definition": "An Austroasiatic language spoken by the P'uman people in China.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Linguists are studying the phonology of the <w>U</w> language.",
                            "target_lang_translations": {
                                "ru": "Лингвисты изучают фонологию языка <w>у</w>."
                            }
                        },
                        {
                            "text": "The <w>U</w> language is spoken in the Yunnan province of China.",
                            "target_lang_translations": {
                                "ru": "На языке <w>у</w> говорят в китайской провинции Юньнань."
                            }
                        }
                    ],
                    "synonyms": [
                        "P'uman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Австроазиатский язык, на котором говорит народ пуман в Китае."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "название языка"
                            }
                        ]
                    }
                }
            ]
        }
    ]

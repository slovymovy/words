{
    "word_family": [
        "odpadać",
        "odpad",
        "odpadki",
        "odpadnięcie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9846209a-e25d-32d4-88b1-284fde191c45",
                    "sense_definition": "Odłączyć się od całości lub powierzchni, do której było się przymocowanym, i spaść na dół.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary tynk <odpadł> ze ściany, odsłaniając surowe cegły.",
                            "target_lang_translations": {
                                "ru": "Старая штукатурка <отвалилась> со стены, обнажив сырые кирпичи."
                            }
                        },
                        {
                            "text": "Zauważyłem, że guzik <odpadł> mi od płaszcza w drodze do pracy.",
                            "target_lang_translations": {
                                "ru": "Я заметил, что пуговица <оторвалась> от моего пальто по дороге на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "oderwać się",
                        "odlecieć"
                    ],
                    "antonyms": [
                        "przywrzeć",
                        "trzymać się"
                    ],
                    "common_phrases": [
                        "tynk odpadł",
                        "guzik odpadł",
                        "koło odpadło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отделиться от целого или поверхности, к которой было прикреплено, и упасть вниз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвалиться",
                                "target_lang_sense_clarification": "о штукатурке, деталях, колесах"
                            },
                            {
                                "target_lang_word": "отпасть",
                                "target_lang_sense_clarification": "отделиться (о пуговице, куске)"
                            },
                            {
                                "target_lang_word": "оторваться",
                                "target_lang_sense_clarification": "о пуговице, ручке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21a4a4b7-86a9-346d-9980-a32568984bfb",
                    "sense_definition": "Zostać wyeliminowanym z gry, konkursu lub rywalizacji; przestać brać udział w dalszym etapie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "elimination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza reprezentacja <odpadła> z turnieju już w fazie grupowej.",
                            "target_lang_translations": {
                                "ru": "Наша сборная <выбыла> из турнира уже на групповом этапе."
                            }
                        },
                        {
                            "text": "Wielu kandydatów <odpadło> podczas trudnego egzaminu wstępnego na studia.",
                            "target_lang_translations": {
                                "ru": "Многие кандидаты <отсеялись> во время сложного вступительного экзамена в вуз."
                            }
                        }
                    ],
                    "synonyms": [
                        "przegrać",
                        "zostać wyeliminowanym"
                    ],
                    "antonyms": [
                        "awansować",
                        "zakwalifikować się"
                    ],
                    "common_phrases": [
                        "odpaść z gry",
                        "odpaść w przedbiegach",
                        "odpaść z turnieju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть исключенным из игры, конкурса или соревнования; прекратить участие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбыть",
                                "target_lang_sense_clarification": "из игры, турнира"
                            },
                            {
                                "target_lang_word": "вылететь",
                                "target_lang_sense_clarification": "разговорное; проиграть и покинуть турнир"
                            },
                            {
                                "target_lang_word": "отсеяться",
                                "target_lang_sense_clarification": "о кандидатах в процессе отбора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6d8df46-06be-376e-9588-e68ec13e2a4f",
                    "sense_definition": "Wykarmić i utuczyć zwierzęta hodowlane; aspekt dokonany od <odpasać>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "feeding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz <odpasł> bydło na soczystej trawie przed jesienną sprzedażą.",
                            "target_lang_translations": {
                                "ru": "Хозяин <откормил> скот на сочной траве перед осенней продажей."
                            }
                        },
                        {
                            "text": "Trzeba dbać o zwierzęta, by dobrze <odpasły> się na zimę.",
                            "target_lang_translations": {
                                "ru": "Нужно заботиться о животных, чтобы они хорошо <отъелись> к зиме."
                            }
                        }
                    ],
                    "synonyms": [
                        "utuczyć",
                        "wykarmić"
                    ],
                    "antonyms": [
                        "odchudzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Откормить и сделать тучными сельскохозяйственных животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откормить",
                                "target_lang_sense_clarification": "о скоте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49848148-ffbc-34b0-bc62-08180cb28734",
                    "sense_definition": "Zgubić coś.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "regional_loss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdzieś na rynku <odpadłem> portfel z dokumentami.",
                            "target_lang_translations": {
                                "ru": "Где-то на рынке я <потерял> кошелек с документами."
                            }
                        },
                        {
                            "text": "Pilnuj czapki, żebyś jej nie <odpadł> w tym tłumie.",
                            "target_lang_translations": {
                                "ru": "Следи за шапкой, чтобы ты её не <потерял> в этой толпе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgubić",
                        "stracić"
                    ],
                    "antonyms": [
                        "znaleźć"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Kresy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерять что-либо (региональное употребление)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потерять",
                                "target_lang_sense_clarification": "утратить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
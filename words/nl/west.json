    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1002a10d-901f-3caf-84cc-703d03711099",
                    "sense_definition": "De windstreek waar de zon ondergaat, tegenover het oosten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction_cardinal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De storm beweegt zich langzaam van oost naar <w>west</w>.",
                            "target_lang_translations": {
                                "en": "The storm moves slowly from east to <w>west</w>.",
                                "ru": "Шторм медленно движется с востока на <w>запад</w>."
                            }
                        },
                        {
                            "text": "Deze traditie is in zowel oost als <w>west</w> bekend.",
                            "target_lang_translations": {
                                "en": "This tradition is known in both east and <w>west</w>.",
                                "ru": "Эта традиция известна как на востоке, так и на <w>западе</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "oost",
                        "oosten"
                    ],
                    "common_phrases": [
                        "oost west, thuis best",
                        "van oost naar west",
                        "noord, oost, zuid, west"
                    ],
                    "target_lang_definitions": {
                        "en": "The cardinal direction where the sun sets, opposite to the east.",
                        "ru": "Сторона света, где заходит солнце, противоположная востоку."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "west",
                                "target_lang_sense_clarification": "cardinal direction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "запад",
                                "target_lang_sense_clarification": "сторона света"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f81297c8-c038-337f-a29a-31baebf96a44",
                    "sense_definition": "In westelijke richting.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "direction_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het schip koerste <w>west</w> om de storm te ontwijken.",
                            "target_lang_translations": {
                                "en": "The ship headed <w>west</w> to avoid the storm.",
                                "ru": "Корабль взял курс на <w>запад</w>, чтобы избежать шторма."
                            }
                        },
                        {
                            "text": "We rijden <w>west</w> totdat we de kust bereiken.",
                            "target_lang_translations": {
                                "en": "We drive <w>west</w> until we reach the coast.",
                                "ru": "Мы едем на <w>запад</w>, пока не достигнем побережья."
                            }
                        }
                    ],
                    "synonyms": [
                        "westwaarts"
                    ],
                    "antonyms": [
                        "oost"
                    ],
                    "target_lang_definitions": {
                        "en": "Towards the west.",
                        "ru": "В западном направлении."
                    },
                    "translations": {
                        "en": [
                            {

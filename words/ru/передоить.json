            "senses": [
                {
                    "sense_id": "34cd6ca9-a54d-31d1-bfce-af3223d0366a",
                    "sense_definition": "Подоить всех или многих животных одного за другим.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дояркам нужно успеть <w>передоить</w> всех коров до обеденного перерыва.",
                            "target_lang_translations": {
                                "en": "The milkmaids need to have time to <w>milk</w> all the cows before the lunch break."
                            }
                        },
                        {
                            "text": "За утро бабушка сама смогла <w>передоить</w> целое стадо коз.",
                            "target_lang_translations": {
                                "en": "In the morning, grandma was able to <w>milk</w> a whole herd of goats by herself."
                            }
                        }
                    ],
                    "common_phrases": [
                        "передоить всех коров",
                        "передоить стадо"
                    ],
                    "target_lang_definitions": {
                        "en": "To milk all or many animals one after another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "milk",
                                "target_lang_sense_clarification": "all or many animals"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52d8673d-9523-3969-b572-b8ac92e5dc62",
                    "sense_definition": "Доить слишком долго или часто, утомляя животное или причиняя ему вред.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если случайно <w>передоить</w> корову, она может заболеть и перестать давать молоко.",
                            "target_lang_translations": {
                                "en": "If you accidentally <w>overmilk</w> a cow, she might get sick and stop giving milk."
                            }
                        },
                        {
                            "text": "Неопытный работник боялся <w>передоить</w> животное и повредить вымя.",
                            "target_lang_translations": {
                                "en": "The inexperienced worker was afraid to <w>overmilk</w> the animal and damage the udder."
                            }
                        }
                    ],
                    "antonyms": [
                        "недодоить"
                    ],
                    "target_lang_definitions": {
                        "en": "To milk for too long or too often, potentially causing harm."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "overmilk",
                                "target_lang_sense_clarification": "milk excessively"
                            }
                        ]
                    }
                }
            ]
        }

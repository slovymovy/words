    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "45609136-72ef-33b1-bff4-bc69cec0fecb",
                    "sense_definition": "Закрыть что-либо не полностью, оставив небольшую щель, или плотно затворить (дверь, окно); сомкнуть (веки, глаза).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "closing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пожалуйста, <прикрой> за собой дверь, когда будешь выходить из комнаты.",
                            "target_lang_translations": {
                                "en": "Please <close> the door behind you when you leave the room."
                            }
                        },
                        {
                            "text": "Она устало <прикрыла> глаза и попыталась немного отдохнуть.",
                            "target_lang_translations": {
                                "en": "She wearily <closed> her eyes and tried to rest a little."
                            }
                        },
                        {
                            "text": "Из-за сильного сквозняка нам пришлось плотно <прикрыть> окно на кухне.",
                            "target_lang_translations": {
                                "en": "Because of the strong draft, we had to <shut> the kitchen window tightly."
                            }
                        }
                    ],
                    "synonyms": [
                        "притворить",
                        "закрыть",
                        "сомкнуть"
                    ],
                    "antonyms": [
                        "открыть",
                        "распахнуть"
                    ],
                    "common_phrases": [
                        "прикрыть дверь",
                        "прикрыть глаза",
                        "плотно прикрыть",
                        "прикрыть за собой"
                    ],
                    "target_lang_definitions": {
                        "en": "To close something partially or tightly (such as a door or window); to close (eyes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "close",
                                "target_lang_sense_clarification": "general action for doors, windows, or eyes"
                            },
                            {
                                "target_lang_word": "shut",
                                "target_lang_sense_clarification": "often implies closing tightly or securely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0f8123d-03e0-3f95-a0a7-dc4cfd675014",
                    "sense_definition": "Положить что-либо сверху или спереди объекта, чтобы спрятать его, сделать невидимым или защитить от внешних воздействий.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама решила <прикрыть> спящего ребёнка тёплым одеялом.",
                            "target_lang_translations": {
                                "en": "Mom decided to <cover> the sleeping child with a warm blanket."
                            }
                        },
                        {
                            "text": "Садовник посоветовал <прикрыть> молодые растения плёнкой от ночных заморозков.",
                            "target_lang_translations": {
                                "en": "The gardener advised to <cover> the young plants with film against night frosts."
                            }
                        }
                    ],
                    "synonyms": [
                        "накрыть",
                        "укрыть",
                        "завесить"
                    ],
                    "antonyms": [
                        "открыть",
                        "обнажить"
                    ],
                    "common_phrases": [
                        "прикрыть одеялом",
                        "прикрыть рукой",
                        "прикрыть от дождя"
                    ],
                    "target_lang_definitions": {
                        "en": "To place something on top of or in front of an object to hide or protect it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cover",
                                "target_lang_sense_clarification": "to place something over an object"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73cb6e3b-a551-3c2d-9d06-fd1849072ec1",
                    "sense_definition": "Защитить кого-либо от нападения или опасности, часто обеспечивая безопасность своими действиями или огнём (в военном контексте).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир приказал солдатам <прикрыть> отход основной группы огнём.",
                            "target_lang_translations": {
                                "en": "The commander ordered the soldiers to <cover> the main group's retreat with fire."
                            }
                        },
                        {
                            "text": "Ты иди вперёд, а я <прикрою> тебя со спины, если что-то случится.",
                            "target_lang_translations": {
                                "en": "You go ahead, and I will <cover> your back if anything happens."
                            }
                        }
                    ],
                    "synonyms": [
                        "защитить",
                        "заслонить",
                        "оборонить"
                    ],
                    "antonyms": [
                        "подставить"
                    ],
                    "common_phrases": [
                        "прикрыть спину",
                        "прикрыть отход",
                        "прикрыть собой"
                    ],
                    "target_lang_definitions": {
                        "en": "To protect someone from attack or danger, often by providing safety or fire support."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cover",
                                "target_lang_sense_clarification": "military or tactical protection"
                            },
                            {
                                "target_lang_word": "protect",
                                "target_lang_sense_clarification": "to shield from danger"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f690f5d2-28d5-3ad3-976e-8ca0eabd2c7e",
                    "sense_definition": "Скрыть, утаить нечестные действия, проступки, ошибки или преступление.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "concealment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нечестный бухгалтер пытался <прикрыть> недостачу фальшивыми отчётами.",
                            "target_lang_translations": {
                                "en": "The dishonest accountant tried to <cover up> the shortage with fake reports."
                            }
                        },
                        {
                            "text": "Никто из коллег не захотел <прикрыть> его ошибку перед начальством.",
                            "target_lang_translations": {
                                "en": "None of the colleagues wanted to <cover up> his mistake in front of the boss."
                            }
                        }
                    ],
                    "synonyms": [
                        "скрыть",
                        "утаить",
                        "замаскировать"
                    ],
                    "antonyms": [
                        "разоблачить",
                        "выдать"
                    ],
                    "common_phrases": [
                        "прикрыть грехи",
                        "прикрыть преступление"
                    ],
                    "target_lang_definitions": {
                        "en": "To hide or conceal dishonest actions, mistakes, or crimes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cover up",
                                "target_lang_sense_clarification": "to conceal a mistake or crime"
                            },
                            {
                                "target_lang_word": "shield",
                                "target_lang_sense_clarification": "to protect someone from consequences"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00859ba3-ace0-398c-a6f1-24b53f7db361",
                    "sense_definition": "Прекратить деятельность какой-либо организации, предприятия или незаконного дела.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shutting_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция давно собиралась <прикрыть> этот подпольный игорный клуб.",
                            "target_lang_translations": {
                                "en": "The police had long intended to <shut down> this underground gambling club."
                            }
                        },
                        {
                            "text": "Из-за финансовых проблем владельцы решили <прикрыть> свой бизнес.",
                            "target_lang_translations": {
                                "en": "Due to financial problems, the owners decided to <close down> their business."
                            }
                        }
                    ],
                    "synonyms": [
                        "закрыть",
                        "ликвидировать"
                    ],
                    "antonyms": [
                        "открыть",
                        "основать"
                    ],
                    "common_phrases": [
                        "прикрыть лавочку",
                        "прикрыть дело"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used when talking about shutting down businesses or schemes"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stop the operations of an organization, business, or illegal activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shut down",
                                "target_lang_sense_clarification": "to close a business or operation permanently"
                            },
                            {
                                "target_lang_word": "close down",
                                "target_lang_sense_clarification": "synonym for shutting down a business"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
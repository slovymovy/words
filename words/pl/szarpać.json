{
    "word_family": [
        "szarpnąć",
        "szarpnięcie",
        "poszarpać",
        "wyszarpać",
        "nadszarpnąć",
        "szarpany"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f0d4cac-900a-3ce6-9bca-98838fc56ba0",
                    "sense_definition": "Ciągnąć kogoś lub coś gwałtownymi, krótkimi i silnymi ruchami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pies mocno <w>szarpał</w> smycz, próbując wyrwać się właścicielowi.",
                            "target_lang_translations": {
                                "ru": "Собака сильно <w>дёргала</w> поводок, пытаясь вырваться от хозяина."
                            }
                        },
                        {
                            "text": "Nie wolno <w>szarpać</w> za klamkę, bo zamek w drzwiach może się zaciąć.",
                            "target_lang_translations": {
                                "ru": "Нельзя <w>дёргать</w> за ручку, так как замок в двери может заесть."
                            }
                        }
                    ],
                    "synonyms": [
                        "rwać",
                        "podrywać"
                    ],
                    "common_phrases": [
                        "szarpać za włosy",
                        "szarpać za rękaw",
                        "szarpać struny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тянуть кого-то или что-то резкими, короткими и сильными движениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дёргать",
                                "target_lang_sense_clarification": "резко тянуть"
                            },
                            {
                                "target_lang_word": "теребить",
                                "target_lang_sense_clarification": "постоянно трогать или тянуть"
                            },
                            {
                                "target_lang_word": "рвать",
                                "target_lang_sense_clarification": "сильно тянуть к себе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e37f6f1c-3f5d-3a27-8836-b80e3c69c959",
                    "sense_definition": "Wykonywać gwałtowne ruchy, walcząc z kimś, próbując się uwolnić lub zmagając się z trudną sytuacją (zwykle w formie zwrotnej).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "struggle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złodziej zaczął się <w>szarpać</w> z policjantami podczas aresztowania.",
                            "target_lang_translations": {
                                "ru": "Вор начал <w>вырываться</w> от полицейских во время ареста."
                            }
                        },
                        {
                            "text": "Muszę się <w>szarpać</w> z tym starym zamkiem, żeby otworzyć drzwi.",
                            "target_lang_translations": {
                                "ru": "Мне приходится <w>возиться</w> с этим старым замком, чтобы открыть дверь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szarpać się z życiem",
                        "szarpać się z myślami",
                        "szarpać się z kimś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершать резкие движения, борясь с кем-то или пытаясь освободиться (обычно возвратный глагол)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырываться",
                                "target_lang_sense_clarification": "пытаться освободиться"
                            },
                            {
                                "target_lang_word": "бороться",
                                "target_lang_sense_clarification": "сражаться с кем-то или чем-то"
                            },
                            {
                                "target_lang_word": "дёргаться",
                                "target_lang_sense_clarification": "делать резкие движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2265ede-091d-3d9e-9f0a-c4a4b73d0ca3",
                    "sense_definition": "Rozrywać coś na kawałki lub odrywać części czegoś przy użyciu siły.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny wiatr <w>szarpał</w> flagę zawieszoną na wysokim maszcie.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер <w>трепал</w> флаг, висящий на высокой мачте."
                            }
                        },
                        {
                            "text": "Drapieżnik upolował ofiarę i zaczął <w>szarpać</w> mięso na mniejsze kawałki.",
                            "target_lang_translations": {
                                "ru": "Хищник поймал жертву и начал <w>рвать</w> мясо на более мелкие куски."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdzierać",
                        "odrywać",
                        "rwać"
                    ],
                    "common_phrases": [
                        "szarpać na strzępy",
                        "szarpać na kawałki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрывать что-то на куски или отрывать части чего-то с применением силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трепать",
                                "target_lang_sense_clarification": "о ветре или ткани"
                            },
                            {
                                "target_lang_word": "рвать",
                                "target_lang_sense_clarification": "разрывать на части"
                            },
                            {
                                "target_lang_word": "терзать",
                                "target_lang_sense_clarification": "раздирать (о хищниках)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59ed9a60-ca54-33d0-8741-d4581b0574ac",
                    "sense_definition": "Nękać kogoś psychicznie, denerwować lub sprawiać komuś przykrość.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "psychological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta stresująca sytuacja w pracy zaczyna mi <w>szarpać</w> nerwy.",
                            "target_lang_translations": {
                                "ru": "Эта стрессовая ситуация на работе начинает <w>трепать</w> мне нервы."
                            }
                        },
                        {
                            "text": "Nie powinieneś <w>szarpać</w> dobrego imienia swojej rodziny przez takie zachowanie.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <w>порочить</w> доброе имя своей семьи таким поведением."
                            }
                        }
                    ],
                    "synonyms": [
                        "nękać",
                        "męczyć",
                        "niepokoić",
                        "dokuczać"
                    ],
                    "common_phrases": [
                        "szarpać nerwy",
                        "szarpać opinię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изводить кого-то морально, нервировать или причинять неприятности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трепать",
                                "target_lang_sense_clarification": "нервы или имя"
                            },
                            {
                                "target_lang_word": "изматывать",
                                "target_lang_sense_clarification": "психологически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4251288c-f207-3a34-a7d7-d3a154542407",
                    "sense_definition": "Pracować nierówno, wykonywać skokowe ruchy lub dławić się (o silniku, pojeździe, urządzeniu).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy zmieniam bieg na wyższy, samochód zaczyna dziwnie <w>szarpać</w>.",
                            "target_lang_translations": {
                                "ru": "Когда я переключаю передачу на более высокую, машина начинает странно <w>дёргаться</w>."
                            }
                        },
                        {
                            "text": "Stary silnik kosiarki <w>szarpał</w> i gasł co kilka minut.",
                            "target_lang_translations": {
                                "ru": "Старый двигатель газонокосилки <w>работал рывками</w> и глох каждые несколько минут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "silnik szarpie",
                        "auto szarpie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Работать неровно, двигаться рывками или глохнуть (о двигателе, механизме)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дёргаться",
                                "target_lang_sense_clarification": "двигаться рывками"
                            },
                            {
                                "target_lang_word": "барахлить",
                                "target_lang_sense_clarification": "работать с перебоями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86c23c43-6423-38b9-93f4-4db5947abab4",
                    "sense_definition": "Powodować ostry, rwący i nawracający ból (o ranie lub dolegliwości).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głęboka rana na nodze <w>szarpała</w> go przy każdym kroku.",
                            "target_lang_translations": {
                                "ru": "Глубокая рана на ноге <w>дёргала</w> его при каждом шаге."
                            }
                        },
                        {
                            "text": "Mimo leków przeciwbólowych, chory ząb wciąż nieprzyjemnie <w>szarpał</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на обезболивающие, больной зуб всё ещё неприятно <w>дёргал</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rwać"
                    ],
                    "common_phrases": [
                        "rana szarpie",
                        "ból szarpie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать острую, пульсирующую и повторяющуюся боль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дёргать",
                                "target_lang_sense_clarification": "о пульсирующей боли"
                            },
                            {
                                "target_lang_word": "стрелять",
                                "target_lang_sense_clarification": "об острой боли"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "govern",
        "government",
        "governor",
        "governable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "598ec701-21d7-348a-b36f-5a5fb8595974",
                    "sense_definition": "The process, or the power, of governing; government or administration.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "process_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The citizens demanded better <w>governance</w> from their elected leaders.",
                            "target_lang_translations": {
                                "ru": "Граждане требовали лучшего <w>управления</w> от своих избранных лидеров."
                            }
                        },
                        {
                            "text": "Transparency and accountability are essential for good <w>governance</w>.",
                            "target_lang_translations": {
                                "ru": "Прозрачность и подотчетность необходимы для надлежащего <w>управления</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "administration",
                        "government",
                        "management"
                    ],
                    "common_phrases": [
                        "good governance",
                        "global governance",
                        "weak governance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс или полномочия по управлению; правительство или администрация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управление",
                                "target_lang_sense_clarification": "процесс осуществления власти"
                            },
                            {
                                "target_lang_word": "руководство",
                                "target_lang_sense_clarification": "деятельность по управлению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4235b29b-04f1-31c7-a988-a20a0b2f09e3",
                    "sense_definition": "Accountability for consistent, cohesive policies, processes and decision rights, especially within an organization.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "process_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The board of directors is responsible for the corporate <w>governance</w> of the company.",
                            "target_lang_translations": {
                                "ru": "Совет директоров несет ответственность за корпоративное <w>управление</w> компанией."
                            }
                        },
                        {
                            "text": "Effective data <w>governance</w> ensures that customer information remains secure.",
                            "target_lang_translations": {
                                "ru": "Эффективное <w>управление</w> данными гарантирует безопасность информации о клиентах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "corporate governance",
                        "data governance",
                        "clinical governance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система подотчетности, последовательной политики и процессов принятия решений, особенно внутри организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "управление",
                                "target_lang_sense_clarification": "в контексте корпоративных правил и процедур"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2837b222-3c2b-3890-94ae-00684bee2da6",
                    "sense_definition": "The group of people who make up an administrative body.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university <w>governance</w> met to discuss the budget for the next academic year.",
                            "target_lang_translations": {
                                "ru": "<w>Руководство</w> университета встретилось, чтобы обсудить бюджет на следующий учебный год."
                            }
                        },
                        {
                            "text": "She was elected to join the school's <w>governance</w> as a parent representative.",
                            "target_lang_translations": {
                                "ru": "Она была избрана в школьное <w>руководство</w> в качестве представителя родителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "administration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, составляющих административный орган."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руководство",
                                "target_lang_sense_clarification": "совокупность руководителей"
                            },
                            {
                                "target_lang_word": "администрация",
                                "target_lang_sense_clarification": "орган управления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b195702-4ec3-39e0-a0ab-0c4abd9b724e",
                    "sense_definition": "The state of being governed.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The territory remained under the <w>governance</w> of the empire for two centuries.",
                            "target_lang_translations": {
                                "ru": "Территория оставалась под <w>правлением</w> империи в течение двух столетий."
                            }
                        },
                        {
                            "text": "They lived peacefully under the strict <w>governance</w> of the new regime.",
                            "target_lang_translations": {
                                "ru": "Они жили мирно под строгим <w>правлением</w> нового режима."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние нахождения под управлением или властью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правление",
                                "target_lang_sense_clarification": "период или режим власти"
                            },
                            {
                                "target_lang_word": "власть",
                                "target_lang_sense_clarification": "контроль над территорией или людьми"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
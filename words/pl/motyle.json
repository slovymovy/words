    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ffbc13ab-37fc-3775-b636-02cdd0393ceb",
                    "sense_definition": "Forma liczby mnogiej rzeczownika <w>motyl</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem na kwiecistej łące latają kolorowe <w>motyle</w>.",
                            "target_lang_translations": {
                                "ru": "Летом на цветочном лугу летают разноцветные <w>бабочки</w>."
                            }
                        },
                        {
                            "text": "Kiedy go widzę, od razu czuję <w>motyle</w> w brzuchu.",
                            "target_lang_translations": {
                                "ru": "Когда я его вижу, я сразу чувствую <w>бабочек</w> в животе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czuć motyle",
                        "kolorowe motyle",
                        "łapać motyle"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число существительного motyl (бабочка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бабочки",
                                "target_lang_sense_clarification": "насекомое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1f2f5a1-bda2-3107-a8d7-b52e27667a81",
                    "sense_definition": "Rząd owadów, do którego należą motyle dzienne i ćmy, charakteryzujący się dwiema parami skrzydeł pokrytych łuskami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zaraz po chrząszczach <w>motyle</w> stanowią drugą pod względem liczebności grupę owadów.",
                            "target_lang_translations": {
                                "ru": "Сразу после жуков <w>бабочки</w> (чешуекрылые) составляют вторую по численности группу насекомых."
                            }
                        },
                        {
                            "text": "Wszystkie <w>motyle</w> przechodzą przeobrażenie zupełne w cyklu rozwojowym.",
                            "target_lang_translations": {
                                "ru": "Все <w>бабочки</w> проходят полное превращение в цикле развития."
                            }
                        }
                    ],
                    "synonyms": [
                        "łuskoskrzydłe"
                    ],
                    "common_phrases": [
                        "rząd motyli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отряд насекомых, к которому относятся дневные бабочки и мотыльки (чешуекрылые)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чешуекрылые",
                                "target_lang_sense_clarification": "научный термин"
                            },
                            {
                                "target_lang_word": "бабочки",
                                "target_lang_sense_clarification": "общее название отряда"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9007a565-903b-38ba-8eb4-3615a5b40ed1",
                    "sense_definition": "Forma fleksyjna przymiotnika <w>motyli</w> (odnoszącego się do motyla lub go przypominającego).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wystawie w muzeum podziwialiśmy piękne <w>motyle</w> skrzydła.",
                            "target_lang_translations": {
                                "ru": "На выставке в музее мы любовались красивыми крыльями <w>бабочек</w>."
                            }
                        },
                        {
                            "text": "Jej duże, <w>motyle</w> okulary zasłaniały połowę twarzy.",
                            "target_lang_translations": {
                                "ru": "Ее большие очки-<w>бабочки</w> закрывали половину лица."
                            }
                        }
                    ],
                    "common_phrases": [
                        "motyle skrzydła",
                        "motyle okulary"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "neuter singular or nonvirile plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прилагательного motyli (относящийся к бабочке или напоминающий её)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бабочки",
                                "target_lang_sense_clarification": "в составе составных существительных или в родительном падеже (напр. крылья бабочки)"
                            },
                            {
                                "target_lang_word": "мотыльковые",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They took a ferry to the Isle of <w>Man</w> for their summer holiday.",
                            "target_lang_translations": {
                                "ru": "Они сели на паром до острова <w>Мэн</w> для летнего отдыха."
                            }

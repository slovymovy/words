{
    "word_family": [],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a6d35a3f-e26d-3da3-88dd-7d5563253046",
                    "sense_definition": "Used to indicate the time after noon, specifically the hours between 12:00 midday and 12:00 midnight.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The meeting is scheduled for 2 <w>pm</w> tomorrow.",
                            "target_lang_translations": {
                                "ru": "Встреча запланирована на 2 часа <w>дня</w> завтра."
                            }
                        },
                        {
                            "text": "Most shops in the city close at 9 <w>pm</w>.",
                            "target_lang_translations": {
                                "ru": "Большинство магазинов в городе закрываются в 9 часов <w>вечера</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "am"
                    ],
                    "common_phrases": [
                        "at 5 pm",
                        "10 pm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначение времени после полудня (с 12:00 до полуночи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "после полудня",
                                "target_lang_sense_clarification": "формальное обозначение времени"
                            },
                            {
                                "target_lang_word": "вечера",
                                "target_lang_sense_clarification": "при указании времени (напр. 6 pm - 6 вечера)"
                            },
                            {
                                "target_lang_word": "дня",
                                "target_lang_sense_clarification": "при указании времени (напр. 2 pm - 2 дня)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2ec82e0a-a927-3910-bff2-81c525f24887",
                    "sense_definition": "Abbreviation for <w>prime minister</w>, the head of an elected government in some countries.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "political_role",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "The <w>PM</w> announced a new policy on education this morning.",
                            "target_lang_translations": {
                                "ru": "<w>Премьер-министр</w> объявил о новой политике в области образования сегодня утром."
                            }
                        },
                        {
                            "text": "She hopes to become the next <w>PM</w> after the election.",
                            "target_lang_translations": {
                                "ru": "Она надеется стать следующим <w>премьер-министром</w> после выборов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the current PM",
                        "former PM"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «премьер-министр», глава правительства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "премьер-министр",
                                "target_lang_sense_clarification": "политическая должность"
                            },
                            {
                                "target_lang_word": "премьер",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "445b9f1e-bc8a-3b50-989b-ff724ff832b0",
                    "sense_definition": "Abbreviation for <w>private message</w> or <w>personal message</w>, used in online communication.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "digital_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I sent you a <w>PM</w> with the details of the event.",
                            "target_lang_translations": {
                                "ru": "Я отправил тебе <w>личное сообщение</w> с деталями мероприятия."
                            }
                        },
                        {
                            "text": "Please check your <w>PMs</w> when you have a moment.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверь свои <w>личные сообщения</w>, когда будет время."
                            }
                        }
                    ],
                    "synonyms": [
                        "DM"
                    ],
                    "common_phrases": [
                        "send a PM",
                        "check PMs"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «личное сообщение», используется в интернет-переписке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "личное сообщение",
                                "target_lang_sense_clarification": "стандартный термин"
                            },
                            {
                                "target_lang_word": "ЛС",
                                "target_lang_sense_clarification": "аббревиатура"
                            },
                            {
                                "target_lang_word": "личка",
                                "target_lang_sense_clarification": "интернет-сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "051cddd2-8423-30d9-b493-7665a9c72c28",
                    "sense_definition": "Abbreviation for <w>project manager</w> or <w>product manager</w>, a professional role responsible for planning and execution.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "job_title",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "Our <w>PM</w> organized a meeting to discuss the timeline.",
                            "target_lang_translations": {
                                "ru": "Наш <w>менеджер проекта</w> организовал встречу, чтобы обсудить сроки."
                            }
                        },
                        {
                            "text": "He has been working as a technical <w>PM</w> for five years.",
                            "target_lang_translations": {
                                "ru": "Он работает техническим <w>менеджером продукта</w> уже пять лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "senior PM",
                        "technical PM"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «менеджер проекта» или «менеджер продукта»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "менеджер проекта",
                                "target_lang_sense_clarification": "project manager"
                            },
                            {
                                "target_lang_word": "менеджер продукта",
                                "target_lang_sense_clarification": "product manager"
                            },
                            {
                                "target_lang_word": "ПМ",
                                "target_lang_sense_clarification": "профессиональный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1a6a562-654a-366e-a8ad-b6d6f62e2f87",
                    "sense_definition": "Abbreviation for <w>post mortem</w>, an examination of a body after death or an analysis of a project after its completion.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team held a <w>PM</w> to understand why the software failed.",
                            "target_lang_translations": {
                                "ru": "Команда провела <w>анализ итогов</w>, чтобы понять, почему произошел сбой программного обеспечения."
                            }
                        },
                        {
                            "text": "A preliminary <w>PM</w> report was issued by the coroner.",
                            "target_lang_translations": {
                                "ru": "Коронер выпустил предварительный отчет о <w>вскрытии</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "PM report"
                    ],
                    "target_lang_definitions": {
                        "ru": "Анализ причин неудачи проекта или медицинское вскрытие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вскрытие",
                                "target_lang_sense_clarification": "медицинский термин"
                            },
                            {
                                "target_lang_word": "анализ итогов",
                                "target_lang_sense_clarification": "бизнес-контекст"
                            },
                            {
                                "target_lang_word": "постмортем",
                                "target_lang_sense_clarification": "IT-сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4027a78-1c80-3785-af6c-29b981b87b6e",
                    "sense_definition": "Abbreviation for <w>particulate matter</w>, microscopic solid or liquid matter suspended in the atmosphere.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "High levels of <w>PM</w> in the air can cause respiratory problems.",
                            "target_lang_translations": {
                                "ru": "Высокий уровень <w>взвешенных частиц</w> в воздухе может вызвать проблемы с дыханием."
                            }
                        },
                        {
                            "text": "The environmental agency monitors <w>PM</w> 2.5 concentrations daily.",
                            "target_lang_translations": {
                                "ru": "Агентство по охране окружающей среды ежедневно отслеживает концентрацию <w>твердых частиц</w> PM 2.5."
                            }
                        }
                    ],
                    "common_phrases": [
                        "PM levels"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "scientific"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Взвешенные в воздухе твердые микрочастицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взвешенные частицы",
                                "target_lang_sense_clarification": "экология"
                            },
                            {
                                "target_lang_word": "твердые частицы",
                                "target_lang_sense_clarification": "загрязнение воздуха"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "57614e2b-21f8-35d0-8a21-6ad7b6868140",
                    "sense_definition": "To send a private message to someone, usually on a social media platform.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you are interested in buying the item, please <w>PM</w> me.",
                            "target_lang_translations": {
                                "ru": "Если вы заинтересованы в покупке товара, пожалуйста, <w>напишите мне в личку</w>."
                            }
                        },
                        {
                            "text": "She <w>PMed</w> him to ask for the address.",
                            "target_lang_translations": {
                                "ru": "Она <w>написала ему в личку</w>, чтобы спросить адрес."
                            }
                        }
                    ],
                    "synonyms": [
                        "DM"
                    ],
                    "common_phrases": [
                        "PM me"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправить кому-либо личное сообщение (обычно в соцсетях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "написать в личку",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "отправить ЛС",
                                "target_lang_sense_clarification": "интернет-сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
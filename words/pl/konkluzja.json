    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "55214f85-9bef-3671-87db-7351cfd2af5c",
                    "sense_definition": "Ostateczny wynik rozumowania, dyskusji lub badań; wniosek końcowy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_conclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu godzinach burzliwej debaty nasza wspólna <w>konkluzja</w> była zaskakująco prosta.",
                            "target_lang_translations": {
                                "ru": "После многих часов бурных дебатов наш общий <w>вывод</w> оказался на удивление простым."
                            }
                        },
                        {
                            "text": "Muszę przyznać, że twoja logiczna <w>konkluzja</w> rzuca zupełnie nowe światło na ten problem.",
                            "target_lang_translations": {
                                "ru": "Должен признать, что твой логический <w>вывод</w> проливает совершенно новый свет на эту проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "wniosek"
                    ],
                    "common_phrases": [
                        "dojść do konkluzji",
                        "logiczna konkluzja",
                        "smutna konkluzja",
                        "trafna konkluzja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окончательный результат рассуждений, дискуссии или исследований; итоговый вывод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вывод",
                                "target_lang_sense_clarification": "итог рассуждения"
                            },
                            {
                                "target_lang_word": "заключение",
                                "target_lang_sense_clarification": "формальный итог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98d8fdec-5e3b-332c-97a7-0e96266f53ee",
                    "sense_definition": "Twierdzenie w logice, które wynika w sposób konieczny z przyjętych wcześniej przesłanek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_conclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student filozofii musiał wykazać, w jaki sposób przesłanki prowadzą do tej konkretnej <w>konkluzji</w>.",
                            "target_lang_translations": {
                                "ru": "Студент философского факультета должен был показать, каким образом посылки ведут к этому конкретному <w>выводу</w>."
                            }
                        },
                        {
                            "text": "Jeśli pierwsza przesłanka jest fałszywa, to ostateczna <w>konkluzja</w> również może być błędna.",
                            "target_lang_translations": {
                                "ru": "Если первая посылка ложна, то окончательный <w>вывод</w> также может быть ошибочным."
                            }
                        }
                    ],
                    "synonyms": [
                        "wniosek"
                    ],
                    "common_phrases": [
                        "wyciągnąć konkluzję",
                        "błędna konkluzja"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "logic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Утверждение в логике, которое с необходимостью следует из принятых ранее посылок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вывод",
                                "target_lang_sense_clarification": "логическое следствие"
                            },
                            {
                                "target_lang_word": "заключение",
                                "target_lang_sense_clarification": "термин логики"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c185fac-37c2-3449-a0d0-ae6c9a638d1e",
                    "sense_definition": "Końcowe żądanie procesowe sformułowane przez stronę w piśmie sądowym lub mowie końcowej.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_conclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Adwokat przedstawił sądowi ostateczną <w>konkluzję</w> obrony, wnosząc o uniewinnienie swojego klienta.",
                            "target_lang_translations": {
                                "ru": "Адвокат представил суду окончательное <w>заключение</w> защиты, ходатайствуя об оправдании своего клиента."
                            }
                        },
                        {
                            "text": "Sędzia uważnie przeanalizował <w>konkluzję</w> zawartą w piśmie procesowym powoda.",
                            "target_lang_translations": {
                                "ru": "Судья внимательно проанализировал <w>вывод</w>, содержащийся в процессуальном документе истца."
                            }
                        }
                    ],
                    "synonyms": [
                        "wniosek"
                    ],
                    "common_phrases": [
                        "konkluzja pozwu",
                        "konkluzja oskarżenia"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Окончательное процессуальное требование, сформулированное стороной в судебном документе или заключительной речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключение",
                                "target_lang_sense_clarification": "юридический итог"
                            },
                            {
                                "target_lang_word": "вывод",
                                "target_lang_sense_clarification": "резюмирующая часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7d4b608-8be6-366e-808c-8475159ace6c",
                    "sense_definition": "Historyczny napis trumienny z XVII lub XVIII wieku, zawierający sentencję mowy pogrzebowej, często umieszczany na tkaninie wraz z portretem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_artifact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum diecezjalnym oglądaliśmy starą, ozdobną <w>konkluzję</w> zachowaną z siedemnastowiecznego pogrzebu.",
                            "target_lang_translations": {
                                "ru": "В епархиальном музее мы рассматривали старинную, украшенную <w>конклюзию</w>, сохранившуюся с похорон семнадцатого века."
                            }
                        },
                        {
                            "text": "Historyk sztuki badał treść, jaką zawierała ta barokowa <w>konkluzja</w> herbowa znaleziona w krypcie.",
                            "target_lang_translations": {
                                "ru": "Искусствовед изучал содержание, которое несла эта барочная гербовая <w>конклюзия</w>, найденная в склепе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Историческая надпись на гробу XVII или XVIII века, содержащая сентенцию из надгробной речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конклюзия",
                                "target_lang_sense_clarification": "исторический термин"
                            },
                            {
                                "target_lang_word": "эпитафия",

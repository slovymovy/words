    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c004b1e-cb03-360e-9254-6892673f5096",
                    "sense_definition": "Zdrobnienie od: <w>dzikus</w>; pieszczotliwe lub żartobliwe określenie dziecka albo małego zwierzęcia, które jest nieśmiałe, płochliwe i stroni od ludzi.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój syn to straszny <w>dzikusek</w>, zawsze chowa się za mną, gdy widzi kogoś nowego.",
                            "target_lang_translations": {
                                "ru": "Мой сын — ужасный <w>дичок</w>, он всегда прячется за мной, когда видит кого-то нового."
                            }
                        },
                        {
                            "text": "Ten kot to mały <w>dzikusek</w>, który ucieka na widok wyciągniętej ręki.",
                            "target_lang_translations": {
                                "ru": "Этот кот — маленький <w>дикарёнок</w>, который убегает при виде протянутой руки."
                            }
                        }
                    ],
                    "synonyms": [
                        "wstydzioch",
                        "odludek"
                    ],
                    "antonyms": [
                        "śmiałek"
                    ],
                    "common_phrases": [
                        "mały dzikusek",
                        "straszny dzikusek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często pieszczotliwie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительно-ласкательная форма от слова \"dzikus\" (дикарь); ласковое или шутливое определение ребенка или маленького животного, которое застенчиво, пугливо и избегает людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дикарёнок",
                                "target_lang_sense_clarification": "ласково о ребенке или зверьке"
                            },
                            {
                                "target_lang_word": "дичок",
                                "target_lang_sense_clarification": "о пугливом, нелюдимом существе"
                            },
                            {
                                "target_lang_word": "бука",
                                "target_lang_sense_clarification": "о необщительном, замкнутом ребенке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83643555-9ee6-34bb-be6f-160c6b2f6434",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>dzikuska</w>.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej grupie przedszkolnej nie było żadnych <w>dzikusek</w>, wszystkie dziewczynki chętnie się bawiły.",
                            "target_lang_translations": {
                                "ru": "В этой группе детского сада не было никаких <w>дикарок</w>, все девочки охотно играли."
                            }
                        },
                        {
                            "text": "Unikał towarzystwa tych małych <w>dzikusek</w>, które biegały po podwórku.",
                            "target_lang_translations": {
                                "ru": "Он избегал общества этих маленьких <w>дикарок</w>, которые бегали по двору."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "dopełniacz liczby mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного \"dzikuska\" (дикарка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дикарок",
                                "target_lang_sense_clarification": "форма слова \"дикарка\""
                            }
                        ]
                    }
                }
            ]
        }
    ]

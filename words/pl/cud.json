            "senses": [
                {
                    "sense_id": "3b98c4c4-dda2-364a-b6bf-04602f90077b",
                    "sense_definition": "Zdarzenie lub rzecz niezwykła, niespodziewana i budząca podziw, często oznaczająca szczęśliwy zbieg okoliczności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "figurative_wonder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To prawdziwy <w>cud</w>, że nikt nie zginął w tym groźnym wypadku.",
                            "target_lang_translations": {
                                "ru": "Это настоящее <w>чудо</w>, что никто не погиб в этой страшной аварии."
                            }
                        },
                        {
                            "text": "Jakimś <w>cudem</w> zdążyliśmy na ostatni pociąg do domu.",
                            "target_lang_translations": {
                                "ru": "Каким-то <w>чудом</w> мы успели на последний поезд домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "fenomen",
                        "rewelacja"
                    ],
                    "common_phrases": [
                        "jakimś cudem",
                        "cud techniki",
                        "cud świata",
                        "nie liczyć na cud"
                    ],
                    "target_lang_definitions": {
                        "ru": "Необычное, неожиданное событие, вызывающее восхищение; часто счастливое стечение обстоятельств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чудо",
                                "target_lang_sense_clarification": "удивительное событие, удача"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80845a75-4527-3b0f-94c7-cf7364e01927",
                    "sense_definition": "Niezwykły akt lub zdarzenie przypisywane działaniu siły nadprzyrodzonej lub Boga, naruszające znane prawa natury.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "supernatural_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierni zgromadzili się w kościele, aby modlić się o <w>cud</w> uzdrowienia.",
                            "target_lang_translations": {
                                "ru": "Верующие собрались в церкви, чтобы молиться о <w>чуде</w> исцеления."
                            }
                        },
                        {
                            "text": "Według legendy święty ten potrafił czynić wielkie <w>cuda</w>.",
                            "target_lang_translations": {
                                "ru": "Согласно легенде, этот святой мог творить великие <w>чудеса</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czynić cuda",
                        "wierzyć w cuda",
                        "oczekiwać cudu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сверхъестественное событие или действие, приписываемое божественной силе и нарушающее законы природы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чудо",
                                "target_lang_sense_clarification": "сверхъестественное явление"
                            }
                        ]
                    }
                }
            ]
        }
    ]

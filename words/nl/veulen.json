    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a82aebe8-3c27-3590-925c-307113116186",
                    "sense_definition": "Een jong van bepaalde hoefdieren, zoals een paard, ezel, kameel of zebra.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het pasgeboren <w>veulen</w> stond wankel op zijn dunne benen.",
                            "target_lang_translations": {
                                "ru": "Новорожденный <w>жеребёнок</w> неуверенно стоял на своих тонких ногах."
                            }
                        },
                        {
                            "text": "In de wei loopt een merrie samen met haar <w>veulen</w> te grazen.",
                            "target_lang_translations": {
                                "ru": "На лугу пасётся кобыла вместе со своим <w>жеребёнком</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een dartel veulen",
                        "pasgeboren veulen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Детёныш лошади или некоторых других копытных животных (например, осла, верблюда или зебры)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жеребёнок",
                                "target_lang_sense_clarification": "детёныш лошади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6a80924-30dc-3577-887b-50136ec50754",
                    "sense_definition": "Een jong, onervaren persoon die zich vaak naïef of onbezonnen gedraagt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gedraagt zich op het werk nog als een jong <w>veulen</w>.",
                            "target_lang_translations": {
                                "ru": "На работе он всё ещё ведёт себя как молодой <w>жеребёнок</w>."
                            }
                        },
                        {
                            "text": "De nieuwe politicus is nog een onervaren <w>veulen</w> in het vak.",
                            "target_lang_translations": {
                                "ru": "Новый политик — всё ещё неопытный <w>юнец</w> в этом деле."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een jong veulen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодой, неопытный человек, который часто ведет себя наивно или безрассудно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юнец",
                                "target_lang_sense_clarification": "неопытный молодой человек"
                            },
                            {
                                "target_lang_word": "жеребёнок",
                                "target_lang_sense_clarification": "перен. молодой, резвый или неопытный человек"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "27ba4bef-8d78-3d33-8f7f-629e96ea12cf",
                    "sense_definition": "Een dorp in de gemeente Venray, in de Nederlandse provincie Limburg.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mijn grootouders wonen al jaren in het dorp <w>Veulen</w>.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка уже много лет живут в деревне <w>Вёлен</w>."
                            }
                        },
                        {
                            "text": "De carnavalsvereniging van <w>Veulen</w> organiseert elk jaar een optocht.",
                            "target_lang_translations": {
                                "ru": "Карнавальная ассоциация <w>Вёлена</w> каждый год организует шествие."
                            }
                        }
                    ],
                    "synonyms": [
                        "'t Väöle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревня в муниципалитете Венрай, в нидерландской провинции Лимбург."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Вёлен",
                                "target_lang_sense_clarification": "название деревни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f75a3e5b-fd55-37c2-897c-ecce81ce9656",
                    "sense_definition": "Een klein sterrenbeeld aan de noordelijke hemel, ook bekend als Equuleus.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Het sterrenbeeld <w>Veulen</w> is moeilijk te zien zonder telescoop.",
                            "target_lang_translations": {
                                "ru": "Созвездие <w>Малый Конь</w> трудно увидеть без телескопа."
                            }
                        },
                        {
                            "text": "Astronomen hebben nieuwe sterren ontdekt in <w>Veulen</w>.",
                            "target_lang_translations": {
                                "ru": "Астрономы обнаружили новые звёзды в <w>Малом Коне</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Equ",
                        "Equl"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькое созвездие в северном полушарии, также известное как Малый Конь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Малый Конь",
                                "target_lang_sense_clarification": "созвездие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
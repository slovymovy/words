{
    "word_family": [
        "roguish",
        "roguery",
        "roguishly",
        "roguishness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f23735a1-4bd2-30e4-ba6c-8913e72f6c39",
                    "sense_definition": "A dishonest, unprincipled, or unreliable person, often used to describe a man who is charming despite his faults.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "person_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The charming <w>rogue</w> managed to talk his way out of paying the bill.",
                            "target_lang_translations": {
                                "ru": "Этот обаятельный <w>плут</w> умудрился заговорить зубы и не заплатить по счёту."
                            }
                        },
                        {
                            "text": "He is an old <w>rogue</w> who has swindled half the town, yet everyone still likes him.",
                            "target_lang_translations": {
                                "ru": "Он старый <w>мошенник</w>, обманувший полгорода, но все его всё равно любят."
                            }
                        }
                    ],
                    "synonyms": [
                        "scoundrel",
                        "rascal",
                        "villain",
                        "knave",
                        "blackguard"
                    ],
                    "antonyms": [
                        "gentleman",
                        "hero"
                    ],
                    "common_phrases": [
                        "lovable rogue",
                        "old rogue",
                        "gallery of rogues"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often used in a literary or slightly humorous context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечестный, беспринципный человек; мошенник или плут (часто используется для описания обаятельного мужчины, несмотря на его недостатки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плут",
                                "target_lang_sense_clarification": "хитрый, но часто обаятельный обманщик"
                            },
                            {
                                "target_lang_word": "мошенник",
                                "target_lang_sense_clarification": "человек, занимающийся обманом"
                            },
                            {
                                "target_lang_word": "пройдоха",
                                "target_lang_sense_clarification": "пронырливый, хитрый человек"
                            },
                            {
                                "target_lang_word": "негодяй",
                                "target_lang_sense_clarification": "подлый, низкий человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adcd7a59-c168-3744-a8e4-3433d1dab512",
                    "sense_definition": "A playfully mischievous person, especially a child.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Come here, you little <w>rogue</w>, and give your grandma a hug.",
                            "target_lang_translations": {
                                "ru": "Иди сюда, маленький <w>проказник</w>, и обними бабушку."
                            }
                        },
                        {
                            "text": "The little <w>rogue</w> hid my keys just to see me search for them.",
                            "target_lang_translations": {
                                "ru": "Этот маленький <w>шалун</w> спрятал мои ключи, просто чтобы посмотреть, как я буду их искать."
                            }
                        }
                    ],
                    "synonyms": [
                        "scamp",
                        "rascal",
                        "imp"
                    ],
                    "common_phrases": [
                        "little rogue",
                        "cheeky rogue"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used affectionately"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Игривый, озорной человек, особенно ребёнок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проказник",
                                "target_lang_sense_clarification": "тот, кто совершает мелкие шалости"
                            },
                            {
                                "target_lang_word": "шалун",
                                "target_lang_sense_clarification": "баловник, озорник"
                            },
                            {
                                "target_lang_word": "сорванец",
                                "target_lang_sense_clarification": "бойкий, озорной ребенок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aeaba3f5-03df-30d4-b7ae-8421b00c1fbe",
                    "sense_definition": "A wild animal, especially an elephant, that lives apart from the herd and is often aggressive or destructive.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers were warned about a <w>rogue</w> that had been trampling crops near the forest edge.",
                            "target_lang_translations": {
                                "ru": "Жителей деревни предупредили о <w>слоне-бродяге</w>, который вытаптывал посевы у кромки леса."
                            }
                        },
                        {
                            "text": "Hunters tracked the <w>rogue</w> for days to prevent it from attacking the settlement.",
                            "target_lang_translations": {
                                "ru": "Охотники выслеживали <w>зверя-одиночку</w> несколько дней, чтобы не дать ему напасть на поселение."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zoology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дикое животное (обычно слон), живущее отдельно от стада и отличающееся агрессивным поведением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бродяга",
                                "target_lang_sense_clarification": "о животном, отбившемся от стада"
                            },
                            {
                                "target_lang_word": "одиночка",
                                "target_lang_sense_clarification": "животное, живущее обособленно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bbcaabd-52b2-3f4b-a58d-fcc55b2f403c",
                    "sense_definition": "A character class in role-playing games specializing in stealth, lockpicking, and sneak attacks.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gaming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I usually play as a <w>rogue</w> because I enjoy sneaking past enemies rather than fighting them.",
                            "target_lang_translations": {
                                "ru": "Я обычно играю за <w>разбойника</w>, потому что мне нравится прокрадываться мимо врагов, а не сражаться с ними."
                            }
                        },
                        {
                            "text": "The party needs a <w>rogue</w> to disarm the traps in the dungeon.",
                            "target_lang_translations": {
                                "ru": "Группе нужен <w>вор</w>, чтобы обезвредить ловушки в подземелье."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "gaming"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Класс персонажа в ролевых играх, специализирующийся на скрытности, взломе замков и внезапных атаках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разбойник",
                                "target_lang_sense_clarification": "игровой класс (Rogue)"
                            },
                            {
                                "target_lang_word": "вор",
                                "target_lang_sense_clarification": "альтернативное название класса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ecc472c-16eb-333b-b039-388f9640558a",
                    "sense_definition": "Malicious software that deceives users, often by pretending to be a legitimate security program.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The IT department warned us about a new <w>rogue</w> that installs itself when you click on fake ads.",
                            "target_lang_translations": {
                                "ru": "IT-отдел предупредил нас о новом <w>лжеантивирусе</w>, который устанавливается при клике на поддельную рекламу."
                            }
                        },
                        {
                            "text": "Scanning your computer regularly can help detect <w>rogues</w> before they steal your data.",
                            "target_lang_translations": {
                                "ru": "Регулярное сканирование компьютера поможет обнаружить <w>вредоносные программы</w> до того, как они украдут ваши данные."
                            }
                        }
                    ],
                    "synonyms": [
                        "malware",
                        "scareware"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вредоносное программное обеспечение, которое обманывает пользователей, часто выдавая себя за легитимную защитную программу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лжеантивирус",
                                "target_lang_sense_clarification": "поддельное защитное ПО"
                            },
                            {
                                "target_lang_word": "вредоносная программа",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "dbbf2ade-d7ac-304f-aad3-773b7fda793a",
                    "sense_definition": "Operating outside normal or desirable controls, often behaving in a dangerous, destructive, or unauthorized way.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "uncontrolled",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government imposed sanctions on the <w>rogue</w> state for developing illegal weapons.",
                            "target_lang_translations": {
                                "ru": "Правительство наложило санкции на <w>страну-изгоя</w> за разработку незаконного оружия."
                            }
                        },
                        {
                            "text": "A <w>rogue</w> wave suddenly appeared and nearly capsized the small fishing boat.",
                            "target_lang_translations": {
                                "ru": "Внезапно появилась <w>блуждающая</w> волна и чуть не перевернула маленькую рыбацкую лодку."
                            }
                        },
                        {
                            "text": "The detective decided to go <w>rogue</w> and investigate the case without his captain's permission.",
                            "target_lang_translations": {
                                "ru": "Детектив решил <w>действовать самовольно</w> и расследовать дело без разрешения капитана."
                            }
                        }
                    ],
                    "synonyms": [
                        "uncontrolled",
                        "unprincipled",
                        "renegade",
                        "unauthorized"
                    ],
                    "antonyms": [
                        "disciplined",
                        "controlled",
                        "obedient"
                    ],
                    "common_phrases": [
                        "go rogue",
                        "rogue state",
                        "rogue wave",
                        "rogue trader",
                        "rogue elephant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действующий бесконтрольно, часто опасным или несанкционированным образом; вышедший из-под контроля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неконтролируемый",
                                "target_lang_sense_clarification": "вышедший из-под контроля"
                            },
                            {
                                "target_lang_word": "самовольный",
                                "target_lang_sense_clarification": "действующий без разрешения"
                            },
                            {
                                "target_lang_word": "изгой",
                                "target_lang_sense_clarification": "в контексте 'rogue state' (страна-изгой)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ef632fe7-b786-3f0d-aeb0-154cf0bd918d",
                    "sense_definition": "To remove inferior, diseased, or unwanted plants from a crop to ensure the quality of the harvest.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Farmers spend days <w>roguing</w> the fields to remove any plants that do not meet the standard.",
                            "target_lang_translations": {
                                "ru": "Фермеры целыми днями <w>пропалывают</w> поля, удаляя растения, не соответствующие стандарту."
                            }
                        },
                        {
                            "text": "It is important to <w>rogue</w> the crop before pollination to maintain the purity of the seed.",
                            "target_lang_translations": {
                                "ru": "Важно <w>отбраковать</w> посевы до опыления, чтобы сохранить чистоту семян."
                            }
                        }
                    ],
                    "synonyms": [
                        "cull"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "agriculture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять некачественные, больные или нежелательные растения из посевов для обеспечения качества урожая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "браковать",
                                "target_lang_sense_clarification": "отсеивать негодное"
                            },
                            {
                                "target_lang_word": "пропалывать",
                                "target_lang_sense_clarification": "удалять сорняки или больные растения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "28341d5c-f9fd-3826-974c-fc67c3ad13f9",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Rogues</w> was listed in the old town directory.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Рогс</w> был указан в старом городском справочнике."
                            }
                        },
                        {
                            "text": "We met a family named <w>Rogues</w> during our trip to France.",
                            "target_lang_translations": {
                                "ru": "Во время нашей поездки во Францию мы встретили семью по фамилии <w>Рогс</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Рогс",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
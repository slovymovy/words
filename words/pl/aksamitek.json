            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2b3fbd81-edf5-31ed-889f-91f1a3f17597",
                    "sense_definition": "Roślina ozdobna z rodzaju Tagetes o pomarańczowych, żółtych lub czerwonawych kwiatach; męska forma nazwy aksamitka.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W doniczce na balkonie wyrósł piękny, pomarańczowy <w>aksamitek</w>.",
                            "target_lang_translations": {
                                "ru": "В горшке на балконе вырос красивый оранжевый <w>бархатец</w>."
                            }
                        },
                        {
                            "text": "Ogrodnik zalecił, aby posadzić <w>aksamitek</w> w pobliżu warzyw dla ochrony przed szkodnikami.",
                            "target_lang_translations": {
                                "ru": "Садовник посоветовал посадить <w>бархатцы</w> рядом с овощами для защиты от вредителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "aksamitka",
                        "śmierdziuszek"
                    ],
                    "common_phrases": [
                        "żółty aksamitek",
                        "kwitnący aksamitek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративное растение рода Tagetes с оранжевыми, желтыми или красноватыми цветами; бархатцы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бархатцы",
                                "target_lang_sense_clarification": "растение"
                            },
                            {
                                "target_lang_word": "бархатец",
                                "target_lang_sense_clarification": "один цветок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9948698-f077-3c9d-940c-8f0aa345e2e4",
                    "sense_definition": "Zdrobnienie od słowa <w>aksamit</w>; delikatna, miękka tkanina lub jej niewielki kawałek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fabrics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej kołnierzyk był obszyty czarnym, lśniącym <w>aksamitkiem</w>.",
                            "target_lang_translations": {
                                "ru": "Её воротничок был обшит черной блестящей <w>бархаткой</w>."
                            }
                        },
                        {
                            "text": "W dotyku ten materiał przypominał miękki <w>aksamitek</w>.",
                            "target_lang_translations": {
                                "ru": "На ощупь этот материал напоминал мягкий <w>бархат</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czarny aksamitek",
                        "miękki aksamitek"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова «бархат»; мягкая ткань или бархатная лента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бархат",
                                "target_lang_sense_clarification": "ткань"
                            },
                            {
                                "target_lang_word": "бархатка",
                                "target_lang_sense_clarification": "лента"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36344578-ad2b-33a1-9876-d6b04c7a27da",
                    "sense_definition": "Pająk z rodzaju Clubiona, charakteryzujący się aksamitnym wyglądem odwłoka, niebudujący sieci łownych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas spaceru po lesie zauważyłem, jak mały <w>aksamitek</w> chowa się pod liściem.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки по лесу я заметил, как маленькая <w>клубиона</w> прячется под листом."
                            }
                        },
                        {
                            "text": "<w>Aksamitek</w> jest pająkiem aktywnie polującym nocą na drobne owady.",
                            "target_lang_translations": {
                                "ru": "<w>Клубиона</w> — это паук, активно охотящийся ночью на мелких насекомых."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Паук из рода Clubiona с бархатистым брюшком, не плетущий ловчих сетей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клубиона",
                                "target_lang_sense_clarification": "паук"
                            },
                            {
                                "target_lang_word": "паук-мешкопряд",
                                "target_lang_sense_clarification": "семейство"
                            }
                        ]
                    }
                }

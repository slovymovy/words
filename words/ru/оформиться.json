    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6503e633-cda0-3860-a2e3-890d21d0bfb7",
                    "sense_definition": "Выполнить необходимые официальные процедуры для получения статуса, должности или права (например, при приёме на работу).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я завтра пойду в отдел кадров, чтобы официально <w>оформиться</w> на новую должность.",
                            "target_lang_translations": {
                                "en": "I'm going to the HR department tomorrow to officially <w>complete the paperwork</w> for the new position."
                            }
                        },
                        {
                            "text": "Перед приёмом у врача пациенту необходимо <w>оформиться</w> в регистратуре.",
                            "target_lang_translations": {
                                "en": "Before the doctor's appointment, the patient needs to <w>register</w> at the reception desk."
                            }
                        }
                    ],
                    "synonyms": [
                        "устроиться",
                        "зарегистрироваться"
                    ],
                    "antonyms": [
                        "уволиться"
                    ],
                    "common_phrases": [
                        "оформиться на работу",
                        "оформиться в штат",
                        "оформиться официально",
                        "быстро оформиться"
                    ],
                    "target_lang_definitions": {
                        "en": "To complete the necessary official procedures to obtain a status, position, or right (e.g., when getting a job)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get hired",
                                "target_lang_sense_clarification": "specifically regarding employment procedures"
                            },
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "at a reception, hotel, or institution"
                            },
                            {
                                "target_lang_word": "complete the paperwork",
                                "target_lang_sense_clarification": "focus on the administrative process"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1db87621-ec8c-35af-870f-d294d536814e",
                    "sense_definition": "Принять законченный вид, стать определённым и ясным (о мыслях, идеях, явлениях).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У писателя в голове наконец <w>оформилась</w> идея для нового романа.",
                            "target_lang_translations": {
                                "en": "An idea for a new novel finally <w>took shape</w> in the writer's head."
                            }
                        },
                        {
                            "text": "Их случайное знакомство со временем <w>оформилось</w> в крепкую дружбу.",
                            "target_lang_translations": {
                                "en": "Their chance meeting eventually <w>developed</w> into a strong friendship."
                            }
                        }
                    ],
                    "synonyms": [
                        "сформироваться",
                        "сложиться",
                        "определиться"
                    ],
                    "common_phrases": [
                        "идея оформилась",
                        "окончательно оформиться",
                        "мысль оформилась"
                    ],
                    "target_lang_definitions": {
                        "en": "To take on a finished form; to become definite and clear (regarding thoughts, ideas, or phenomena)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take shape",
                                "target_lang_sense_clarification": "idiomatic, for ideas or plans"
                            },
                            {
                                "target_lang_word": "form",
                                "target_lang_sense_clarification": "general sense of coming into existence"
                            },
                            {
                                "target_lang_word": "crystallize",
                                "target_lang_sense_clarification": "metaphorical, becoming very clear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea4496ca-56e0-3914-be64-93535a357c94",
                    "sense_definition": "Страдательный залог к глаголу <w>оформить</w>: быть составленным, зарегистрированным или декорированным.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ваш заказ <w>оформился</w> автоматически, и скоро вы получите подтверждение.",
                            "target_lang_translations": {
                                "en": "Your order <w>was processed</w> automatically, and you will receive a confirmation soon."
                            }
                        },
                        {
                            "text": "Этот документ <w>оформится</w> в течение трёх рабочих дней.",
                            "target_lang_translations": {
                                "en": "This document <w>will be processed</w> within three business days."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "указывает на пассивное действие или результат"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive voice of 'оформить': to be drawn up, registered, or processed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be processed",
                                "target_lang_sense_clarification": "for orders, requests, or documents"
                            },
                            {
                                "target_lang_word": "be issued",
                                "target_lang_sense_clarification": "for official documents"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
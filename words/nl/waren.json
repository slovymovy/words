                    "sense_id": "7d9a5e35-a6bd-3612-9952-2cc449263bd2",
                    "sense_definition": "Vervoeging van <w>zijn</w>: verleden tijd meervoud.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>waren</w> gisteren de hele dag thuis.",
                            "target_lang_translations": {
                                "en": "We <w>were</w> home all day yesterday.",
                                "ru": "Мы <w>были</w> вчера весь день дома."
                            }
                        },
                        {
                            "text": "Er <w>waren</w> veel mensen op het feest aanwezig.",
                            "target_lang_translations": {
                                "en": "There <w>were</w> many people present at the party.",
                                "ru": "На вечеринке <w>было</w> много людей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "er waren",
                        "wij waren",
                        "zij waren"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugation of <w>zijn</w>: past tense plural.",
                        "ru": "Прошедшее время множественного числа глагола \"zijn\" (быть)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "were",
                                "target_lang_sense_clarification": "past tense of 'to be'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "были",
                                "target_lang_sense_clarification": "прошедшее время мн. ч."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f1ae113-7893-337d-ae0c-86a425c0ea0c",
                    "sense_definition": "Doelloos en rusteloos ronddwalen, vaak gezegd van geesten of gedachten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgens de oude legende <w>waren</w> er 's nachts geesten door het kasteel.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "58be799c-2614-3f11-acfd-8051169099fe",
                    "sense_definition": "Оценка ситуации как невозможной, недопустимой или нереальной.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Появление ошибок в этой компьютерной программе полностью <w>исключено</w>.",
                            "target_lang_translations": {
                                "en": "The appearance of errors in this computer program is completely <w>ruled out</w>."
                            }
                        },
                        {
                            "text": "Это абсолютно <w>исключено</w>, мы не можем так рисковать деньгами компании.",
                            "target_lang_translations": {
                                "en": "That is absolutely <w>out of the question</w>; we cannot risk the company's money like that."
                            }
                        },
                        {
                            "text": "Совсем не <w>исключено</w>, что завтра погода резко испортится.",
                            "target_lang_translations": {
                                "en": "It is not at all <w>impossible</w> that the weather will turn bad tomorrow."
                            }
                        }
                    ],
                    "synonyms": [
                        "невозможно",
                        "недопустимо",
                        "нереально"
                    ],
                    "antonyms": [
                        "возможно",
                        "допустимо",
                        "вероятно"
                    ],
                    "common_phrases": [
                        "не исключено",
                        "абсолютно исключено",
                        "полностью исключено",
                        "это исключено"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate that a situation is impossible, unacceptable, or not to be considered."
                    },
                    "translations": {
                        "en": [
                            {

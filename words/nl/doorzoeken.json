            "senses": [
                {
                    "sense_id": "802b9d07-1222-3705-99b7-a684a019cd42",
                    "sense_definition": "Grondig onderzoeken of zich iets ergens bevindt",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie <w>doorzocht</w> het huis van de verdachte op verboden middelen.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>обыскала</w> дом подозреваемого в поисках запрещенных веществ.",
                                "en": "The police <w>searched</w> the suspect's house for banned substances."
                            }
                        },
                        {
                            "text": "Hij <w>doorzocht</w> alle lades van het bureau om zijn paspoort te vinden.",
                            "target_lang_translations": {
                                "ru": "Он <w>обыскал</w> все ящики стола, чтобы найти свой паспорт.",
                                "en": "He <w>searched</w> all the drawers of the desk to find his passport."
                            }
                        }
                    ],
                    "synonyms": [
                        "inspecteren",
                        "uitkammen",
                        "nagaan"
                    ],
                    "common_phrases": [
                        "het huis doorzoeken",
                        "alles doorzoeken",
                        "zakken doorzoeken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательно осматривать место или человека, чтобы найти что-то спрятанное или потерянное",
                        "en": "To thoroughly investigate a place or person to find something hidden or lost"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обыскивать",
                                "target_lang_sense_clarification": "проводить осмотр с целью находки"
                            },
                            {
                                "target_lang_word": "осматривать",
                                "target_lang_sense_clarification": "внимательно изучать взглядом или проверять"
                            },
                            {
                                "target_lang_word": "прочесывать",
                                "target_lang_sense_clarification": "тщательно обследовать местность"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "search",
                                "target_lang_sense_clarification": "to look somewhere carefully in order to find something"
                            },
                            {
                                "target_lang_word": "search through",
                                "target_lang_sense_clarification": "to look inside something thoroughly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf223ece-69c5-3473-b15a-1a6c63394d66",
                    "sense_definition": "Voortgaan met zoeken; niet stoppen met zoeken",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De reddingswerkers <w>zochten</w> de hele nacht <w>door</w> naar de vermiste wandelaars.",
                            "target_lang_translations": {
                                "ru": "Спасатели всю ночь <w>продолжали искать</w> пропавших туристов.",
                                "en": "The rescue workers <w>kept searching</w> all night for the missing hikers."
                            }
                        },
                        {
                            "text": "We moeten <w>doorzoeken</w> tot we de oorzaak van het probleem hebben gevonden.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>продолжать искать</w>, пока не найдем причину проблемы.",
                                "en": "We must <w>continue searching</w> until we have found the cause of the problem."
                            }
                        }
                    ],
                    "synonyms": [
                        "verder zoeken"
                    ],
                    "antonyms": [
                        "stoppen",
                        "opgeven"
                    ],
                    "common_phrases": [
                        "blijven doorzoeken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжать поиски; не останавливаться в процессе поиска",
                        "en": "To continue searching; not to stop the search"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продолжать искать",
                                "target_lang_sense_clarification": "не прекращать действие поиска"
                            },
                            {
                                "target_lang_word": "искать дальше",
                                "target_lang_sense_clarification": "продолжать поиск после перерыва или неудачи"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "keep searching",
                                "target_lang_sense_clarification": "to continue the action of searching"
                            },
                            {
                                "target_lang_word": "continue searching",
                                "target_lang_sense_clarification": "to persist with a search"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "78fd3238-5bd5-3fd8-9843-c489754a3534",
                    "sense_definition": "Een regelmatige herhaling van geluiden, tonen of bewegingen, vooral in muziek.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De drummer speelde een snel <w>ritme</w> waar iedereen op begon te dansen.",
                            "target_lang_translations": {
                                "en": "The drummer played a fast <w>rhythm</w> that everyone started dancing to.",
                                "ru": "Барабанщик играл быстрый <w>ритм</w>, под который все начали танцевать."
                            }
                        },
                        {
                            "text": "Hij tikte zachtjes met zijn voet mee op het <w>ritme</w> van het liedje.",
                            "target_lang_translations": {
                                "en": "He tapped his foot gently to the <w>rhythm</w> of the song.",
                                "ru": "Он тихонько отбивал ногой <w>ритм</w> песни."
                            }
                        }
                    ],
                    "synonyms": [
                        "maat",
                        "cadans",
                        "tempo"
                    ],
                    "common_phrases": [
                        "het ritme volgen",
                        "in de maat",
                        "strak ritme"
                    ],
                    "target_lang_definitions": {
                        "en": "A regular repetition of sounds, tones, or movements, especially in music.",
                        "ru": "Регулярное повторение звуков, тонов или движений, особенно в музыке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rhythm",
                                "target_lang_sense_clarification": "musical pattern"
                            },
                            {
                                "target_lang_word": "beat",
                                "target_lang_sense_clarification": "pulse or tempo"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ритм",
                                "target_lang_sense_clarification": "музыкальный"
                            },
                            {
                                "target_lang_word": "такт",
                                "target_lang_sense_clarification": "музыкальный размер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "717a5719-50d3-3bf9-bf7c-55772b10abea",
                    "sense_definition": "Een vaste regelmaat waarin gebeurtenissen of handelingen elkaar opvolgen in de tijd.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de lange vakantie moest ik weer wennen aan het dagelijkse <w>ritme</w> van werken en slapen.",
                            "target_lang_translations": {
                                "en": "After the long vacation, I had to get used to the daily <w>rhythm</w> of working and sleeping again.",
                                "ru": "После долгого отпуска мне пришлось снова привыкать к ежедневному <w>ритму</w> работы и сна."
                            }
                        },
                        {
                            "text": "Het is belangrijk voor je gezondheid om een vast <w>ritme</w> aan te houden.",
                            "target_lang_translations": {
                                "en": "It is important for your health to maintain a regular <w>rhythm</w>.",
                                "ru": "Для здоровья важно придерживаться постоянного <w>ритма</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "regelmaat",
                        "patroon"
                    ],
                    "common_phrases": [
                        "biologisch ritme",
                        "dagelijks ritme",
                        "in een ritme komen"
                    ],
                    "target_lang_definitions": {
                        "en": "A fixed regularity in which events or actions follow each other in time.",
                        "ru": "Установленный порядок, в котором события или действия следуют друг за другом во времени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rhythm",
                                "target_lang_sense_clarification": "pattern of recurrence"
                            },
                            {
                                "target_lang_word": "routine",
                                "target_lang_sense_clarification": "regular schedule"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ритм",
                                "target_lang_sense_clarification": "темп жизни, регулярность"
                            },
                            {
                                "target_lang_word": "режим",
                                "target_lang_sense_clarification": "распорядок"
                            }
                        ]
                    }
                }
            ]
        }

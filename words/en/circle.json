{
    "word_family": [
        "circular",
        "semicircle",
        "encircle",
        "circulate",
        "circulation",
        "circuit"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "51ef3026-8a75-3f59-8f3b-935debdeca3b",
                    "sense_definition": "A perfectly round flat shape, or a line representing the edge of such a shape.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geometry_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used a compass to draw a perfect <w>circle</w> on the paper.",
                            "target_lang_translations": {
                                "ru": "Она использовала циркуль, чтобы нарисовать на бумаге идеальный <w>круг</w>."
                            }
                        },
                        {
                            "text": "The children cut a large <w>circle</w> out of the red cardboard.",
                            "target_lang_translations": {
                                "ru": "Дети вырезали большой <w>круг</w> из красного картона."
                            }
                        }
                    ],
                    "synonyms": [
                        "disc",
                        "disk",
                        "ring",
                        "round"
                    ],
                    "common_phrases": [
                        "draw a circle",
                        "in a circle",
                        "full circle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Идеально круглая плоская фигура или линия, обозначающая край такой фигуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "геометрическая фигура"
                            },
                            {
                                "target_lang_word": "окружность",
                                "target_lang_sense_clarification": "замкнутая кривая линия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b83ec7d9-9d7f-34f2-8b30-f9161672a00e",
                    "sense_definition": "An arrangement of people or objects in a round shape.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "geometry_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The students stood in a <w>circle</w> holding hands.",
                            "target_lang_translations": {
                                "ru": "Студенты встали в <w>круг</w>, держась за руки."
                            }
                        },
                        {
                            "text": "Please form a <w>circle</w> so everyone can see the demonstration.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, встаньте в <w>круг</w>, чтобы все могли видеть демонстрацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "ring"
                    ],
                    "common_phrases": [
                        "form a circle",
                        "sit in a circle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположение людей или предметов в форме круга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "расположение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb53561f-ee96-312e-bef9-c5639414b399",
                    "sense_definition": "A specific group of people who share interests, professions, or social connections.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a large <w>circle</w> of friends at the university.",
                            "target_lang_translations": {
                                "ru": "У неё большой <w>круг</w> друзей в университете."
                            }
                        },
                        {
                            "text": "The news spread quickly within political <w>circles</w>.",
                            "target_lang_translations": {
                                "ru": "Новости быстро распространились в политических <w>кругах</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "group",
                        "bunch",
                        "gang"
                    ],
                    "common_phrases": [
                        "circle of friends",
                        "inner circle",
                        "social circle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенная группа людей, связанных общими интересами, профессией или социальными связями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "общество, группа людей"
                            },
                            {
                                "target_lang_word": "кружок",
                                "target_lang_sense_clarification": "группа по интересам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1955610b-e260-3535-a838-5f4c9a67f8c3",
                    "sense_definition": "A road junction where traffic moves in one direction around a central island.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "traffic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Take the second exit at the traffic <w>circle</w>.",
                            "target_lang_translations": {
                                "ru": "Сверните на втором съезде на <w>кольце</w>."
                            }
                        },
                        {
                            "text": "He drove around the <w>circle</w> twice because he missed his turn.",
                            "target_lang_translations": {
                                "ru": "Он проехал по <w>кольцу</w> дважды, потому что пропустил свой поворот."
                            }
                        }
                    ],
                    "synonyms": [
                        "round"
                    ],
                    "common_phrases": [
                        "traffic circle"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дорожная развязка, где движение транспорта осуществляется в одном направлении вокруг центрального острова."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "дорожная развязка"
                            },
                            {
                                "target_lang_word": "круговое движение",
                                "target_lang_sense_clarification": "тип перекрестка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90ee57e6-3d29-3062-9a73-ae67e78c0573",
                    "sense_definition": "Dark marks or puffy skin under the eyes, usually caused by lack of sleep.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "body_feature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had dark <w>circles</w> under his eyes after working all night.",
                            "target_lang_translations": {
                                "ru": "У него были темные <w>круги</w> под глазами после того, как он проработал всю ночь."
                            }
                        },
                        {
                            "text": "Fatigue left deep <w>circles</w> on her face.",
                            "target_lang_translations": {
                                "ru": "Усталость оставила глубокие <w>круги</w> на её лице."
                            }
                        }
                    ],
                    "common_phrases": [
                        "circles under eyes",
                        "dark circles"
                    ],
                    "target_lang_definitions": {
                        "ru": "Темные пятна или припухлости под глазами, обычно вызванные недосыпанием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круги",
                                "target_lang_sense_clarification": "под глазами"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f1cf9692-2045-3019-89a1-1966775f554c",
                    "sense_definition": "To draw a line around something, often to select or highlight it.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "marking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>circle</w> the correct answer on your test paper.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>обведите</w> правильный ответ в вашей контрольной работе."
                            }
                        },
                        {
                            "text": "I <w>circled</w> the important dates on the calendar in red ink.",
                            "target_lang_translations": {
                                "ru": "Я <w>обвел</w> важные даты в календаре красными чернилами."
                            }
                        }
                    ],
                    "synonyms": [
                        "ring"
                    ],
                    "common_phrases": [
                        "circle the answer",
                        "circle the date"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рисовать линию вокруг чего-либо, часто чтобы выделить или отметить это."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обводить",
                                "target_lang_sense_clarification": "рисовать круг вокруг"
                            },
                            {
                                "target_lang_word": "обвести",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54929ef5-0552-3584-8b49-6a6518b2e8c0",
                    "sense_definition": "To move in a circle or go around something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The plane had to <w>circle</w> the airport for twenty minutes before landing.",
                            "target_lang_translations": {
                                "ru": "Самолету пришлось <w>кружить</w> над аэропортом двадцать минут перед посадкой."
                            }
                        },
                        {
                            "text": "Sharks began to <w>circle</w> the small boat.",
                            "target_lang_translations": {
                                "ru": "Акулы начали <w>кружить</w> вокруг маленькой лодки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "circle around",
                        "circle overhead"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться по кругу или обходить что-либо вокруг."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружить",
                                "target_lang_sense_clarification": "двигаться кругами"
                            },
                            {
                                "target_lang_word": "кружиться",
                                "target_lang_sense_clarification": "вращаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2fd42069-33b6-385c-bee4-22c181ad7be9",
                    "sense_definition": "To surround or enclose something on all sides.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "enclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A high stone fence <w>circles</w> the entire estate.",
                            "target_lang_translations": {
                                "ru": "Высокий каменный забор <w>окружает</w> всё поместье."
                            }
                        },
                        {
                            "text": "The enemy troops <w>circled</w> the city during the siege.",
                            "target_lang_translations": {
                                "ru": "Вражеские войска <w>окружили</w> город во время осады."
                            }
                        }
                    ],
                    "synonyms": [
                        "ring"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окружать или заключать что-либо со всех сторон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окружать",
                                "target_lang_sense_clarification": "находиться вокруг"
                            },
                            {
                                "target_lang_word": "окружить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                }
            ]
        },

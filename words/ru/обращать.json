            "senses": [
                {
                    "sense_id": "64efe044-2e09-3a36-81f2-e5d12d7ab5cc",
                    "sense_definition": "Направлять взгляд, лицо или внимание на кого-либо или что-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "direction_focus",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старайся не <обращать> внимание на мелкие недостатки.",
                            "target_lang_translations": {
                                "en": "Try not to <pay> attention to minor flaws."
                            }
                        },
                        {
                            "text": "Он любил <обращать> свой взор к звездному небу.",
                            "target_lang_translations": {
                                "en": "He loved to <turn> his gaze toward the starry sky."
                            }
                        }
                    ],
                    "synonyms": [
                        "направлять",
                        "устремлять",
                        "поворачивать"
                    ],
                    "antonyms": [
                        "отворачивать",
                        "отвлекать"
                    ],
                    "common_phrases": [
                        "обращать внимание",
                        "обращать взор",
                        "обращать взгляд"
                    ],
                    "target_lang_definitions": {
                        "en": "To direct one's gaze, face, or attention toward someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "used with 'attention'"
                            },
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "to direct one's gaze or face"
                            },
                            {
                                "target_lang_word": "direct",
                                "target_lang_sense_clarification": "to focus attention or gaze"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82e059e8-21c1-341e-9b5a-f435b79a40d0",
                    "sense_definition": "Превращать, переводить из одного вида или состояния в другое.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный жар может <обращать> воду в пар.",
                            "target_lang_translations": {
                                "en": "Intense heat can <turn> water into steam."
                            }
                        },
                        {
                            "text": "Он умел <обращать> любую ситуацию в шутку.",
                            "target_lang_translations": {
                                "en": "He knew how to <turn> any situation into a joke."
                            }

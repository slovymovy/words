{
    "word_family": [
        "запрет",
        "запрещение",
        "запретный",
        "запрещать"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "11dc130b-f421-33e9-8aeb-f1e60df1beda",
                    "sense_definition": "Не позволить кому-либо совершать какое-либо действие; наложить запрет.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "prohibition_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач строго <запретил> пациенту пить крепкий кофе.",
                            "target_lang_translations": {
                                "en": "The doctor strictly <forbade> the patient to drink strong coffee."
                            }
                        },
                        {
                            "text": "Родители <запретили> детям гулять поздно вечером.",
                            "target_lang_translations": {
                                "en": "The parents <forbade> the children to go for a walk late in the evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "воспретить"
                    ],
                    "antonyms": [
                        "разрешить",
                        "позволить"
                    ],
                    "common_phrases": [
                        "строго запретить",
                        "категорически запретить",
                        "запретить подходить"
                    ],
                    "target_lang_definitions": {
                        "en": "To not allow someone to perform an action; to impose a ban."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forbid",
                                "target_lang_sense_clarification": "general or personal context"
                            },
                            {
                                "target_lang_word": "prohibit",
                                "target_lang_sense_clarification": "formal context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51f5173a-bdd4-38f5-9279-b4728f39ef2b",
                    "sense_definition": "Официально признать недопустимым к использованию, распространению или применению.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "prohibition_official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство <запретило> ввоз некоторых товаров из-за границы.",
                            "target_lang_translations": {
                                "en": "The government <banned> the import of certain goods from abroad."
                            }
                        },
                        {
                            "text": "В городе <запретили> продажу алкоголя после десяти вечера.",
                            "target_lang_translations": {
                                "en": "The city <banned> the sale of alcohol after ten in the evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "воспретить"
                    ],
                    "antonyms": [
                        "разрешить"
                    ],
                    "common_phrases": [
                        "запретить въезд",
                        "запретить деятельность",
                        "запретить продажу"
                    ],
                    "target_lang_definitions": {
                        "en": "To officially recognize as inadmissible for use, distribution, or application."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ban",
                                "target_lang_sense_clarification": "official or commercial context"
                            },
                            {
                                "target_lang_word": "prohibit",
                                "target_lang_sense_clarification": "by law or regulation"
                            },
                            {
                                "target_lang_word": "outlaw",
                                "target_lang_sense_clarification": "to make illegal"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
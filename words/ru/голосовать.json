{
    "word_family": [
        "голос",
        "голосование",
        "проголосовать"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f27eb64d-e8e3-3008-832e-61a6a68ef780",
                    "sense_definition": "Отдавать свой голос на выборах или при принятии общего решения.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "voting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все граждане страны должны <голосовать> на выборах.",
                            "target_lang_translations": {
                                "en": "All citizens of the country must <w>vote</w> in the elections."
                            }
                        },
                        {
                            "text": "Я решил <голосовать> за другого кандидата.",
                            "target_lang_translations": {
                                "en": "I decided to <w>vote</w> for another candidate."
                            }
                        }
                    ],
                    "synonyms": [
                        "избирать"
                    ],
                    "antonyms": [
                        "воздерживаться"
                    ],
                    "common_phrases": [
                        "голосовать за",
                        "голосовать против",
                        "голосовать на выборах"
                    ],
                    "target_lang_definitions": {
                        "en": "To cast a vote in an election or during a collective decision-making process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vote",
                                "target_lang_sense_clarification": "to express a choice in an election or meeting"
                            },
                            {
                                "target_lang_word": "cast a ballot",
                                "target_lang_sense_clarification": "formal phrase for the act of voting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5d36e04-2986-3120-9132-a451bf8cab9a",
                    "sense_definition": "Останавливать попутную машину жестом руки.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы вышли на трассу и начали <голосовать>.",
                            "target_lang_translations": {
                                "en": "The tourists went out onto the highway and started to <w>hitchhike</w>."
                            }
                        },
                        {
                            "text": "В дождь очень трудно <голосовать> на дороге.",
                            "target_lang_translations": {
                                "en": "It is very difficult to <w>hitchhike</w> on the road in the rain."
                            }
                        }
                    ],
                    "common_phrases": [
                        "голосовать на дороге",
                        "голосовать рукой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stop a passing car with a hand gesture to get a free ride."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hitchhike",
                                "target_lang_sense_clarification": "to travel by getting free rides in passing vehicles"
                            },
                            {
                                "target_lang_word": "thumb a ride",
                                "target_lang_sense_clarification": "informal, referring to the specific hand gesture used"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cab209a-5710-3880-91e6-62c8f4750255",
                    "sense_definition": "Выражать своё отношение к чему-либо действием, например покупкой или уходом.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Покупатели стали <голосовать> рублём за качественные продукты.",
                            "target_lang_translations": {
                                "en": "Shoppers began to <w>vote</w> with their wallets for quality products."
                            }
                        },
                        {
                            "text": "Инвесторы предпочитают <голосовать> ногами, если условия бизнеса ухудшаются.",
                            "target_lang_translations": {
                                "en": "Investors prefer to <w>vote</w> with their feet if business conditions deteriorate."
                            }
                        }
                    ],
                    "common_phrases": [
                        "голосовать рублём",
                        "голосовать ногами"
                    ],
                    "target_lang_definitions": {
                        "en": "To express one's attitude or preference through actions rather than a formal ballot, such as by spending money or leaving."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vote",
                                "target_lang_sense_clarification": "used metaphorically in idioms like 'vote with one's feet' or 'vote with one's wallet'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
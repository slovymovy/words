                {
                    "sense_id": "10c01ae4-7495-3ba4-ad99-e2372c407611",
                    "sense_definition": "Tijdelijk het werk van iemand anders overnemen, bijvoorbeeld omdat diegene ziek of verhinderd is",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "substitute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>val</w> vandaag <w>in</w> voor een zieke collega.",
                            "target_lang_translations": {
                                "en": "I am <w>filling in</w> for a sick colleague today.",
                                "ru": "Я сегодня <w>подменяю</w> больного коллегу."
                            }
                        },
                        {
                            "text": "De leraar is ziek, dus er moet een andere docent <w>invallen</w>.",
                            "target_lang_translations": {
                                "en": "The teacher is sick, so another teacher has to <w>fill in</w>.",
                                "ru": "Учитель болен, поэтому другой преподаватель должен его <w>заменить</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vervangen",
                        "waarnemen"

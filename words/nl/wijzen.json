                {
                    "sense_id": "48ab9dcf-545e-3c89-8a4c-a0c90f8ad0e6",
                    "sense_definition": "Met de vinger, hand of een voorwerp een richting of object aanduiden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pointing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is niet beleefd om met je vinger naar mensen te <w>wijzen</w>.",
                            "target_lang_translations": {
                                "en": "It is not polite to <w>point</w> at people with your finger.",
                                "ru": "Невежливо <w>показывать</w> пальцем на людей."
                            }
                        },
                        {
                            "text": "De pijl op het bord <w>wijst</w> naar de nooduitgang.",
                            "target_lang_translations": {
                                "en": "The arrow on the sign <w>points</w> to the emergency exit.",
                                "ru": "Стрелка на знаке <w>указывает</w> на запасной выход."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanwijzen",
                        "duiden"
                    ],
                    "common_phrases": [
                        "naar iets wijzen",
                        "met de vinger wijzen"
                    ],
                    "target_lang_definitions": {
                        "en": "To indicate a direction or object using a finger, hand, or object.",
                        "ru": "Указывать пальцем, рукой или предметом на направление или объект."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "point",
                                "target_lang_sense_clarification": "to indicate with a finger or object"
                            },
                            {
                                "target_lang_word": "indicate",
                                "target_lang_sense_clarification": "to show direction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "указывать",
                                "target_lang_sense_clarification": "пальцем или предметом"
                            },
                            {
                                "target_lang_word": "показывать",
                                "target_lang_sense_clarification": "направление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88789f63-0d49-3665-8889-9f244e34603a",
                    "sense_definition": "Iemand laten zien waar hij of zij naartoe moet gaan.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "directing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kunt u mij de weg naar het station <w>wijzen</w>?",
                            "target_lang_translations": {
                                "en": "Can you <w>show</w> me the way to the station?",
                                "ru": "Вы можете <w>показать</w> мне дорогу к вокзалу?"
                            }
                        },
                        {
                            "text": "Omdat hij zich misdroeg, werd hem de deur <w>gewezen</w>.",
                            "target_lang_translations": {
                                "en": "Because he misbehaved, he was <w>shown</w> the door.",
                                "ru": "Так как он плохо себя вел, ему <w>указали</w> на дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "tonen",
                        "gidsen"
                    ],
                    "common_phrases": [
                        "de weg wijzen",
                        "de deur wijzen",
                        "terecht wijzen"
                    ],
                    "target_lang_definitions": {
                        "en": "To show someone where they need to go.",
                        "ru": "Показывать кому-либо, куда нужно идти."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "to guide or demonstrate the way"

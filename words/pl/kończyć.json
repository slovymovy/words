                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заканчивать",
                                "target_lang_sense_clarification": "наиболее употребительное"
                            },
                            {
                                "target_lang_word": "завершать",
                                "target_lang_sense_clarification": "более формальное"
                            },
                            {
                                "target_lang_word": "кончать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "864d2d7f-c574-3412-8f2d-42e3da9c2942",
                    "sense_definition": "Dobiegać końca, mieć swoje zakończenie (o czasie, wydarzeniach, dziełach); używane z zaimkiem „się”.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "state_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film <kończy> się bardzo smutną sceną.",
                            "target_lang_translations": {
                                "ru": "Этот фильм <заканчивается> очень грустной сценой."
                            }
                        },
                        {
                            "text": "Wakacje niestety szybko się <kończą>.",
                            "target_lang_translations": {
                                "ru": "Каникулы, к сожалению, быстро <заканчиваются>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mijać",
                        "upływać",
                        "wygasać"
                    ],
                    "antonyms": [
                        "zaczynać",
                        "rozpoczynać"
                    ],
                    "common_phrases": [
                        "kończyć się dobrze",
                        "wszystko się kończy",
                        "rok się kończy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходить к концу, завершаться (о времени, событиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заканчиваться",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "кончаться",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a4009e9-3f0e-3a18-8a7f-9c5fb246e950",

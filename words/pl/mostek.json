    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "74798c2b-4c58-3228-a914-e44143258035",
                    "sense_definition": "Mały <w>most</w> lub kładka nad niewielką przeszkodą wodną lub lądową.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spacerowaliśmy przez park i przeszliśmy przez drewniany <w>mostek</w>.",
                            "target_lang_translations": {
                                "ru": "Мы гуляли по парку и перешли через деревянный <w>мостик</w>."
                            }
                        },
                        {
                            "text": "Pod <w>mostkiem</w> płynął cichy, czysty strumyk.",
                            "target_lang_translations": {
                                "ru": "Под <w>мостиком</w> тек тихий, чистый ручеек."
                            }
                        }
                    ],
                    "synonyms": [
                        "mosteczek",
                        "kładka"
                    ],
                    "common_phrases": [
                        "drewniany mostek",
                        "przejść przez mostek",
                        "mostek nad rzeką"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zdrobnienie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький мост или настил над небольшим водным или наземным препятствием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мостик",
                                "target_lang_sense_clarification": "уменьш. от мост"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af0de9d5-eb0f-3d80-b968-94b2a02368eb",
                    "sense_definition": "Figura gimnastyczna polegająca na mocnym wygięciu pleców w łuk do tyłu, opierając się na rękach i stopach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lekcji wf-u nauczyciel kazał nam zrobić <w>mostek</w> na materacu.",
                            "target_lang_translations": {
                                "ru": "На уроке физкультуры учитель велел нам сделать <w>мостик</w> на мате."
                            }
                        },
                        {
                            "text": "Gimnastyczka wykonała idealny <w>mostek</w>, pokazując swoją elastyczność.",
                            "target_lang_translations": {
                                "ru": "Гимнастка выполнила идеальный <w>мостик</w>, показав свою гибкость."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zrobić mostek",
                        "zejście do mostka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гимнастическая фигура, состоящая в сильном прогибе спины дугой назад с опорой на руки и ноги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мостик",
                                "target_lang_sense_clarification": "гимнастика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d4323ec-bfc7-3921-803b-edf529cd9886",
                    "sense_definition": "Płaska kość pośrodku klatki piersiowej, łącząca żebra.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz uciskał mój <w>mostek</w>, sprawdzając reakcję na ból.",
                            "target_lang_translations": {
                                "ru": "Врач надавил мне на <w>грудину</w>, проверяя реакцию на боль."
                            }
                        },
                        {
                            "text": "Serce znajduje się za <w>mostkiem</w>, nieco po lewej stronie.",
                            "target_lang_translations": {
                                "ru": "Сердце находится за <w>грудиной</w>, немного слева."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ból mostka",
                        "za mostkiem",
                        "okolice mostka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская кость в середине грудной клетки, соединяющая ребра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грудина",
                                "target_lang_sense_clarification": "анатомия"
                            },
                            {
                                "target_lang_word": "грудная кость",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08773839-3388-32f3-9b44-8741b8ac3b22",
                    "sense_definition": "Mięso wołowe lub cielęce pochodzące z przedniej części tuszy, z okolic klatki piersiowej.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na niedzielny obiad upiekłam pyszny <w>mostek</w> wołowy w sosie.",
                            "target_lang_translations": {
                                "ru": "На воскресный обед я запекла вкусную говяжью <w>грудинку</w> в соусе."
                            }
                        },
                        {
                            "text": "W sklepie mięsnym kupiłem kilogram <w>mostka</w> na zupę.",
                            "target_lang_translations": {
                                "ru": "В мясном магазине я купил килограмм <w>грудинки</w> на суп."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mostek wołowy",
                        "mostek cielęcy",
                        "pieczony mostek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говядина или телятина из передней части туши, из области грудной клетки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грудинка",
                                "target_lang_sense_clarification": "мясо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cfb88ee-0ad1-3bb5-bb14-9d22f2dbff97",
                    "sense_definition": "Podwyższone miejsce na statku, wyposażone w urządzenia sterownicze, skąd kapitan dowodzi jednostką.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapitan obserwował wzburzone morze, stojąc na <w>mostku</w>.",
                            "target_lang_translations": {
                                "ru": "Капитан наблюдал за бурным морем, стоя на <w>мостике</w>."
                            }
                        },
                        {
                            "text": "Oficer dyżurny nie może opuścić <w>mostka</w> bez zmiennika.",
                            "target_lang_translations": {
                                "ru": "Дежурный офицер не может покинуть <w>мостик</w> без сменщика."
                            }
                        }
                    ],
                    "synonyms": [
                        "sterówka",
                        "pomost"
                    ],
                    "common_phrases": [
                        "mostek kapitański",
                        "wejść na mostek",
                        "wachta na mostku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвышенное место на судне, оборудованное приборами управления, откуда капитан командует судном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мостик",
                                "target_lang_sense_clarification": "на судне"
                            },
                            {
                                "target_lang_word": "капитанский мостик",
                                "target_lang_sense_clarification": "на судне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f279e069-7a57-3d29-905c-fb67a8582ba5",
                    "sense_definition": "Stałe uzupełnienie protetyczne zastępujące jeden lub kilka brakujących zębów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stomatolog zaproponował mi <w>mostek</w> zamiast drogiego implantu.",
                            "target_lang_translations": {
                                "ru": "Стоматолог предложил мне <w>мост</w> вместо дорогого импланта."
                            }
                        },
                        {
                            "text": "Muszę iść do dentysty, bo obluzował mi się <w>mostek</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно к стоматологу, потому что у меня расшатался <w>мост</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mostek porcelanowy",
                        "założyć mostek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несъемный зубной протез, замещающий один или несколько отсутствующих зубов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мост",
                                "target_lang_sense_clarification": "стоматология"
                            },
                            {
                                "target_lang_word": "мостовидный протез",
                                "target_lang_sense_clarification": "стоматология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8546f65e-f746-3103-8fed-600f88376b28",
                    "sense_definition": "Część roweru łącząca kierownicę z widelcem ramy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wymieniłem <w>mostek</w> w rowerze na krótszy, żeby siedzieć wygodniej.",
                            "target_lang_translations": {
                                "ru": "Я заменил <w>вынос</w> на велосипеде на более короткий, чтобы сидеть удобнее."
                            }
                        },
                        {
                            "text": "Musisz dokręcić śruby przy <w>mostku</w>, bo kierownica się rusza.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно подтянуть болты на <w>выносе</w>, а то руль шатается."
                            }
                        }
                    ],
                    "synonyms": [
                        "wspornik kierownicy"
                    ],
                    "common_phrases": [
                        "krótki mostek",
                        "regulowany mostek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть велосипеда, соединяющая руль с вилкой рамы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вынос",
                                "target_lang_sense_clarification": "велосипед"
                            },
                            {
                                "target_lang_word": "вынос руля",
                                "target_lang_sense_clarification": "велосипед"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b7e5dd5-20db-3b8b-a20f-5e4a06d31176",
                    "sense_definition": "Element instrumentu strunowego podtrzymujący struny i przenoszący ich drgania na pudło rezonansowe.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lutnik musiał wymienić pęknięty <w>mostek</w> w moich skrzypcach.",
                            "target_lang_translations": {
                                "ru": "Скрипичный мастер должен был заменить треснувшую <w>подставку</w> на моей скрипке."
                            }
                        },
                        {
                            "text": "Struny opierają się o <w>mostek</w>, co wpływa na brzmienie instrumentu.",
                            "target_lang_translations": {
                                "ru": "Струны опираются на <w>подставку</w>, что влияет на звучание инструмента."
                            }
                        }
                    ],
                    "synonyms": [
                        "podstawek"
                    ],
                    "common_phrases": [
                        "mostek skrzypcowy",
                        "mostek gitarowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент струнного инструмента, поддерживающий струны и передающий их колебания на резонансный корпус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подставка",
                                "target_lang_sense_clarification": "музыкальный инструмент"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
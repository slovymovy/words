                    "synonyms": [
                        "stel",
                        "paar",
                        "duo"
                    ],
                    "common_phrases": [
                        "een jong koppel",
                        "een getrouwd koppel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Два человека, состоящие в любовных отношениях или образующие единство.",
                        "en": "Two people who have a romantic relationship or form a unit."

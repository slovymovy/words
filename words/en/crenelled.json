            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0465401e-003f-3ed6-8eae-039285298d72",
                    "sense_definition": "Having battlements or square notches along the top of a wall, typically for defense or decoration.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "architecture_defense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tourists took photos of the <w>crenelled</w> walls surrounding the ancient castle.",
                            "target_lang_translations": {
                                "ru": "Туристы фотографировали <w>зубчатые</w> стены, окружающие старинный замок."
                            }
                        },
                        {
                            "text": "Archers used to stand behind the <w>crenelled</w> parapet to fire arrows at invaders.",
                            "target_lang_translations": {
                                "ru": "Лучники обычно стояли за <w>зубчатым</w> парапетом, чтобы стрелять в захватчиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "crenellated",
                        "battlemented",
                        "fortified"
                    ],
                    "antonyms": [
                        "unfortified",
                        "plain"
                    ],
                    "common_phrases": [
                        "crenelled wall",
                        "crenelled tower",
                        "crenelled parapet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий зубцы или прямоугольные вырезы по верху стены, обычно для обороны или украшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зубчатый",
                                "target_lang_sense_clarification": "о стенах или башнях"
                            },
                            {
                                "target_lang_word": "увенчанный зубцами",
                                "target_lang_sense_clarification": "имеющий крепостные зубцы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55fcb39d-7171-364a-ad59-9b1b09cefcfc",
                    "sense_definition": "Having a notched, indented, or scalloped edge, similar to the shape of certain leaves or coins.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "shape_texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The designer chose a <w>crenelled</w> pattern for the border of the fabric.",
                            "target_lang_translations": {
                                "ru": "Дизайнер выбрал <w>зубчатый</w> узор для каймы ткани."
                            }
                        },
                        {
                            "text": "He ran his finger along the <w>crenelled</w> edge of the commemorative coin.",
                            "target_lang_translations": {
                                "ru": "Он провел пальцем по <w>зазубренному</w> краю памятной монеты."
                            }
                        }
                    ],
                    "synonyms": [

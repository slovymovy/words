                },
                {
                    "sense_id": "e1d0c1b2-d71c-3f14-ab95-d528e950c464",
                    "sense_definition": "Geeft aan wat men draagt (kleding) of waaruit iets bestaat (materiaal).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "materiaal_kleding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gastheer verzoekt u vriendelijk <w>in</w> jacquet te verschijnen.",
                            "target_lang_translations": {
                                "en": "The host kindly requests you to appear <w>in</w> a morning coat."
                            }
                        },
                        {
                            "text": "Het kunstwerk is volledig uitgevoerd <w>in</w> hout en koper.",
                            "target_lang_translations": {
                                "en": "The artwork is executed entirely <w>in</w> wood and copper."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in burger"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates what someone is wearing or the material something consists of."

{
    "word_family": [
        "policeman",
        "policewoman",
        "policing"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba766804-fa77-3bb8-900c-95263134eba7",
                    "sense_definition": "The official organization responsible for preventing crime, enforcing laws, and maintaining public order.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "law_enforcement_org",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Someone stole my bicycle, so I went to the <w>police</w> station to report it.",
                            "target_lang_translations": {
                                "ru": "Кто-то украл мой велосипед, поэтому я пошел в <w>полицию</w>, чтобы заявить об этом."
                            }
                        },
                        {
                            "text": "If you see anything suspicious, you should call the <w>police</w> immediately.",
                            "target_lang_translations": {
                                "ru": "Если вы увидите что-либо подозрительное, вам следует немедленно позвонить в <w>полицию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "police force",
                        "police department",
                        "law enforcement",
                        "constabulary",
                        "the law"
                    ],
                    "common_phrases": [
                        "call the police",
                        "police officer",
                        "police station",
                        "police car",
                        "police force"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually treated as a plural noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная организация, ответственная за предотвращение преступлений, обеспечение соблюдения законов и поддержание общественного порядка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полиция",
                                "target_lang_sense_clarification": "правоохранительные органы"
                            },
                            {
                                "target_lang_word": "милиция",
                                "target_lang_sense_clarification": "историческое название или в некоторых странах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e749ada8-b839-3fcc-bd75-12a5f99fd770",
                    "sense_definition": "A group of people who strictly enforce standards or rules in a specific area, often used humorously or metaphorically.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_enforcement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My sister acts like the grammar <w>police</w> whenever I send her a text message.",
                            "target_lang_translations": {
                                "ru": "Моя сестра ведет себя как грамматическая <w>полиция</w>, когда я отправляю ей сообщение."
                            }
                        },
                        {
                            "text": "You do not need to worry about the fashion <w>police</w> criticizing your comfortable clothes.",
                            "target_lang_translations": {
                                "ru": "Тебе не нужно беспокоиться о том, что <w>полиция</w> моды раскритикует твою удобную одежду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fashion police",
                        "grammar police",
                        "thought police"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in compound terms like 'fashion police'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, строго следящая за соблюдением стандартов или правил в определенной области (часто используется в шутку или метафорически)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полиция",
                                "target_lang_sense_clarification": "в переносном смысле (напр. полиция моды)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ae667b80-cc8a-3d53-b3e0-f34bca525e70",
                    "sense_definition": "To control or monitor a situation, area, or group of people to ensure that rules and laws are obeyed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "law_enforcement_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government hired extra security guards to <w>police</w> the crowd during the festival.",
                            "target_lang_translations": {
                                "ru": "Правительство наняло дополнительных охранников, чтобы <w>контролировать</w> толпу во время фестиваля."
                            }
                        },
                        {
                            "text": "It is very difficult to effectively <w>police</w> the internet for illegal content.",
                            "target_lang_translations": {
                                "ru": "Очень сложно эффективно <w>контролировать</w> интернет на предмет незаконного контента."
                            }
                        }
                    ],
                    "synonyms": [
                        "patrol",
                        "monitor",
                        "guard",
                        "supervise"
                    ],
                    "common_phrases": [
                        "police the streets",
                        "police the border",
                        "police the area"
                    ],
                    "target_lang_definitions": {
                        "ru": "Контролировать или наблюдать за ситуацией, территорией или группой людей для обеспечения соблюдения правил и законов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контролировать",
                                "target_lang_sense_clarification": "следить за соблюдением правил"
                            },
                            {
                                "target_lang_word": "охранять",
                                "target_lang_sense_clarification": "обеспечивать безопасность"
                            },
                            {
                                "target_lang_word": "патрулировать",
                                "target_lang_sense_clarification": "обходить территорию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "651b6fd8-ab17-32dc-bcc8-1da580a644be",
                    "sense_definition": "To clean or tidy a specific area, typically as a duty.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sergeant ordered the soldiers to <w>police</w> the barracks before the inspection began.",
                            "target_lang_translations": {
                                "ru": "Сержант приказал солдатам <w>привести в порядок</w> казармы перед началом проверки."
                            }
                        },
                        {
                            "text": "We spent the entire morning helping to <w>police</w> up the trash left on the field.",
                            "target_lang_translations": {
                                "ru": "Мы провели все утро, помогая <w>убирать</w> мусор, оставленный на поле."
                            }
                        }
                    ],
                    "synonyms": [
                        "clean up",
                        "tidy"
                    ],
                    "common_phrases": [
                        "police up",
                        "police the area"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "primarily military usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Убирать или приводить в порядок определенную территорию, обычно в качестве обязанности (часто в военном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убирать",
                                "target_lang_sense_clarification": "наводить чистоту"
                            },
                            {
                                "target_lang_word": "приводить в порядок",
                                "target_lang_sense_clarification": "делать уборку"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
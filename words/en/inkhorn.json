    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "95d310a5-17f1-3b4f-b2ce-f8ed41662b16",
                    "sense_definition": "A small portable container, often made of horn, used to carry ink.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scribe dipped his quill into the <w>inkhorn</w> hanging from his belt.",
                            "target_lang_translations": {
                                "ru": "Писец обмакнул перо в <w>чернильный рожок</w>, висевший у него на поясе."
                            }
                        },
                        {
                            "text": "She admired the antique <w>inkhorn</w> displayed in the museum case.",
                            "target_lang_translations": {
                                "ru": "Она любовалась старинной <w>чернильницей</w>, выставленной в музейной витрине."
                            }
                        }
                    ],
                    "synonyms": [
                        "inkwell",
                        "inkpot"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical object"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой переносной сосуд для чернил, часто изготовленный из рога."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чернильница",
                                "target_lang_sense_clarification": "сосуд для чернил"
                            },
                            {
                                "target_lang_word": "чернильный рожок",
                                "target_lang_sense_clarification": "исторический сосуд из рога"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5708d20b-6d96-3547-9bd2-2b0b0735fe12",
                    "sense_definition": "Something or someone pedantic or obscurely scholarly; often used to describe pretentious vocabulary.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "pedantry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author was criticized for using an <w>inkhorn</w> term instead of a simple English word.",
                            "target_lang_translations": {
                                "ru": "Автора критиковали за использование <w>вычурного</w> термина вместо простого английского слова."
                            }
                        },
                        {
                            "text": "His speech was filled with <w>inkhorn</w> language that confused the audience.",
                            "target_lang_translations": {
                                "ru": "Его речь была наполнена <w>заумными</w> выражениями, которые сбили аудиторию с толку."
                            }
                        }
                    ],
                    "synonyms": [
                        "pedant"
                    ],
                    "common_phrases": [
                        "inkhorn term",
                        "inkhorn word"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly used attributively in historical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Педантичный, вычурный или малопонятный (обычно о лексике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заумный",
                                "target_lang_sense_clarification": "сложный для понимания"
                            },
                            {
                                "target_lang_word": "вычурный",
                                "target_lang_sense_clarification": "претенциозный, неестественный"
                            },
                            {
                                "target_lang_word": "мудрёный",
                                "target_lang_sense_clarification": "замысловатый"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9f6acd8a-21a8-3032-8e95-135678ee2159",
                    "sense_definition": "Воспринимать написанный или напечатанный текст, знакомиться с содержанием книг, статей или писем.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я очень люблю <w>читать</w> исторические романы по вечерам.",
                            "target_lang_translations": {
                                "en": "I really love to <w>read</w> historical novels in the evenings."
                            }
                        },
                        {
                            "text": "Бабушка надела очки, чтобы <w>читать</w> свежую газету.",
                            "target_lang_translations": {
                                "en": "Grandma put on her glasses to <w>read</w> the fresh newspaper."
                            }
                        }
                    ],
                    "synonyms": [
                        "прочитывать"
                    ],
                    "antonyms": [
                        "записывать",
                        "смотреть телевизор"
                    ],
                    "common_phrases": [
                        "читать книгу",
                        "читать газету",
                        "читать про себя",
                        "читать запоем"
                    ],
                    "target_lang_definitions": {
                        "en": "To perceive written or printed text, to get acquainted with the content of books, articles, or letters."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "read",
                                "target_lang_sense_clarification": "text, books"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46fa7919-c902-3d87-b66d-47ba573957a8",
                    "sense_definition": "Владеть навыком чтения, уметь различать буквы и складывать их в слова.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой младший брат научился <w>читать</w> в пять лет.",
                            "target_lang_translations": {
                                "en": "My younger brother learned to <w>read</w> at the age of five."
                            }
                        },
                        {
                            "text": "Этот ребёнок ещё не умеет <w>читать</w> и писать.",
                            "target_lang_translations": {
                                "en": "This child does not know how to <w>read</w> and write yet."
                            }
                        }
                    ],
                    "common_phrases": [
                        "уметь читать",
                        "учиться читать",
                        "научить читать"
                    ],
                    "target_lang_definitions": {
                        "en": "To possess the skill of reading, to be able to distinguish letters and put them into words."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "read",
                                "target_lang_sense_clarification": "ability"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7455559d-cf64-377d-87ef-b703cea8e48b",
                    "sense_definition": "Произносить текст вслух, декламировать художественные произведения (стихи, прозу) перед слушателями.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vocalization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Актёр начал <w>читать</w> стихи Пушкина с большим чувством.",
                            "target_lang_translations": {
                                "en": "The actor began to <w>recite</w> Pushkin's poems with great feeling."
                            }
                        },
                        {
                            "text": "Учитель попросил ученика <w>читать</w> текст вслух для всего класса.",
                            "target_lang_translations": {
                                "en": "The teacher asked the student to <w>read</w> the text aloud for the whole class."
                            }
                        }
                    ],
                    "synonyms": [
                        "декламировать",
                        "зачитывать"
                    ],
                    "common_phrases": [
                        "читать стихи",
                        "читать вслух",
                        "читать с выражением",
                        "читать рэп",
                        "читать молитвы"
                    ],
                    "target_lang_definitions": {
                        "en": "To pronounce text aloud, to recite artistic works (poems, prose) before listeners."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "recite",
                                "target_lang_sense_clarification": "poems, from memory"
                            },
                            {
                                "target_lang_word": "read",
                                "target_lang_sense_clarification": "aloud"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1726dd78-27c2-3e26-abde-6108e8704c45",
                    "sense_definition": "Получать и распознавать информацию с цифрового носителя (о компьютере или программе).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компьютер не может <w>читать</w> этот повреждённый диск.",
                            "target_lang_translations": {
                                "en": "The computer cannot <w>read</w> this damaged disk."
                            }
                        },
                        {
                            "text": "Программа начинает <w>читать</w> данные сразу после запуска.",
                            "target_lang_translations": {
                                "en": "The program starts to <w>read</w> data immediately after launch."
                            }
                        }
                    ],
                    "synonyms": [
                        "считывать"
                    ],
                    "antonyms": [
                        "записывать",
                        "сохранять"
                    ],
                    "common_phrases": [
                        "читать диск",
                        "читать файл",
                        "читать данные",
                        "ошибка при чтении"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To receive and recognize information from digital media (about a computer or program)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "read",
                                "target_lang_sense_clarification": "data, computer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "326680ea-7031-3164-b6f6-da7b2bcc972e",
                    "sense_definition": "Вести учебный курс, выступать с лекциями перед аудиторией.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vocalization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Профессор будет <w>читать</w> лекции по истории искусств в этом семестре.",
                            "target_lang_translations": {
                                "en": "The professor will <w>give</w> lectures on art history this semester."
                            }
                        },
                        {
                            "text": "Известный учёный приехал <w>читать</w> доклад на конференции.",
                            "target_lang_translations": {
                                "en": "A famous scientist arrived to <w>deliver</w> a report at the conference."
                            }
                        }
                    ],
                    "synonyms": [
                        "проводить"
                    ],
                    "common_phrases": [
                        "читать лекцию",
                        "читать курс",
                        "читать доклад"
                    ],
                    "target_lang_definitions": {
                        "en": "To conduct an educational course, to speak with lectures before an audience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "lectures"
                            },
                            {
                                "target_lang_word": "deliver",
                                "target_lang_sense_clarification": "a speech or lecture"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee087977-78ab-3a23-9a7d-1f7e9a870136",
                    "sense_definition": "Угадывать мысли или чувства человека по его внешнему виду, глазам или поведению.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она умела <w>читать</w> мысли своего мужа по одному его взгляду.",
                            "target_lang_translations": {
                                "en": "She knew how to <w>read</w> her husband's thoughts by just one look."
                            }
                        },
                        {
                            "text": "Опытный игрок может <w>читать</w> эмоции соперников за столом.",
                            "target_lang_translations": {
                                "en": "An experienced player can <w>read</w> the emotions of opponents at the table."
                            }
                        }
                    ],
                    "common_phrases": [
                        "читать мысли",
                        "читать по глазам",
                        "читать между строк",
                        "читать в душе"
                    ],
                    "target_lang_definitions": {
                        "en": "To guess a person's thoughts or feelings by their appearance, eyes, or behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "read",
                                "target_lang_sense_clarification": "minds, emotions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a774b340-59cf-37db-9f74-631b27ab361d",
                    "sense_definition": "Понимать и расшифровывать специальные знаки, символы, чертежи или карты.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "visual_perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый турист должен уметь <w>читать</w> карту местности.",
                            "target_lang_translations": {
                                "en": "Every tourist must know how to <w>read</w> a map of the area."
                            }
                        },
                        {
                            "text": "Инженер легко может <w>читать</w> сложные строительные чертежи.",
                            "target_lang_translations": {
                                "en": "The engineer can easily <w>read</w> complex construction blueprints."
                            }
                        }
                    ],
                    "synonyms": [
                        "распознавать"
                    ],
                    "common_phrases": [
                        "читать карту",
                        "читать чертёж",
                        "читать схему",
                        "читать по губам",
                        "читать следы"
                    ],
                    "target_lang_definitions": {
                        "en": "To understand and decipher special signs, symbols, blueprints, or maps."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "read",
                                "target_lang_sense_clarification": "maps, blueprints"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebb9ac62-e1dd-33c6-ad9e-7c26fb8d8a18",
                    "sense_definition": "Назидательно говорить, поучать кого-либо, выражая недовольство.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "vocalization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама начала <w>читать</w> нотации сыну за плохие оценки.",
                            "target_lang_translations": {
                                "en": "Mom started to <w>lecture</w> her son for bad grades."
                            }
                        },
                        {
                            "text": "Не надо мне <w>читать</w> мораль, я сам знаю, что делать.",
                            "target_lang_translations": {
                                "en": "Don't <w>preach</w> to me, I know what to do myself."
                            }
                        }
                    ],
                    "common_phrases": [
                        "читать нотации",
                        "читать мораль",
                        "читать наставления"
                    ],
                    "target_lang_definitions": {
                        "en": "To speak didactically, to teach someone, expressing dissatisfaction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lecture",
                                "target_lang_sense_clarification": "scold"
                            },
                            {
                                "target_lang_word": "preach",
                                "target_lang_sense_clarification": "moralize"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "a8caa92b-4b75-3378-9812-892fadde1c77",
                    "sense_definition": "Verwijst naar iets dat dichtbij is of net genoemd is, als vervanging van <w>voor</w> + <w>dit</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben <w>hiervoor</w> niet verantwoordelijk.",
                            "target_lang_translations": {
                                "en": "I am not responsible <w>for this</w>.",
                                "ru": "Я не несу ответственности <w>за это</w>."
                            }
                        },
                        {
                            "text": "Heb je <w>hiervoor</w> al betaald?",
                            "target_lang_translations": {
                                "en": "Have you already paid <w>for this</w>?",
                                "ru": "Ты уже заплатил <w>за это</w>?"
                            }
                        }
                    ],
                    "antonyms": [
                        "daarvoor"
                    ],
                    "common_phrases": [
                        "hiervoor kiezen",
                        "hiervoor zorgen",
                        "hiervoor danken"
                    ],
                    "target_lang_definitions": {
                        "en": "Refers to something close by or just mentioned, replacing \"for\" + \"this\".",
                        "ru": "Указывает на что-то, находящееся рядом или только что упомянутое (заменяет \"voor\" + \"dit\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for this",
                                "target_lang_sense_clarification": "referring to a specific thing or situation"
                            },
                            {
                                "target_lang_word": "for it",
                                "target_lang_sense_clarification": "referring to an object or abstract concept"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "за это",
                                "target_lang_sense_clarification": "объект действия (платить, отвечать)"
                            },
                            {
                                "target_lang_word": "для этого",
                                "target_lang_sense_clarification": "предназначение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d57eb39-ff49-33ea-bf2f-ec4e8f038018",
                    "sense_definition": "Met dit specifieke doel of om deze reden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe regels zijn speciaal <w>hiervoor</w> gemaakt.",
                            "target_lang_translations": {
                                "en": "The new rules were made especially <w>for this</w>.",
                                "ru": "Новые правила были созданы специально <w>для этого</w>."
                            }
                        },
                        {
                            "text": "Wij hebben <w>hiervoor</w> helaas geen tijd.",
                            "target_lang_translations": {
                                "en": "Unfortunately, we have no time <w>for this</w>.",
                                "ru": "К сожалению, у нас нет времени <w>на это</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hiervoor bedoeld",
                        "hiervoor geschikt"
                    ],
                    "target_lang_definitions": {
                        "en": "With this specific purpose or for this reason.",
                        "ru": "С этой конкретной целью или по этой причине."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for this",
                                "target_lang_sense_clarification": "indicating purpose"
                            },
                            {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "02987dd3-166f-3a60-ab46-32eaf2a481cb",
                    "sense_definition": "Zdarzyć się, odbyć się lub nadejść jako skutek pewnych wydarzeń.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niespodziewanie <w>nastąpiła</w> awaria zasilania i w całym biurze zgasło światło.",
                            "target_lang_translations": {
                                "ru": "Неожиданно <w>произошла</w> авария электросети, и во всем офисе погас свет."
                            }
                        },
                        {
                            "text": "Jeśli nie zapłacisz rachunku w terminie, <w>nastąpi</w> odłączenie usługi internetowej.",
                            "target_lang_translations": {
                                "ru": "Если ты не оплатишь счет вовремя, <w>произойдет</w> отключение интернет-услуги."
                            }
                        }
                    ],
                    "synonyms": [
                        "nastać"
                    ],
                    "common_phrases": [
                        "nastąpiła cisza",
                        "nastąpiła zmiana",
                        "wkrótce nastąpi",
                        "nastąpił koniec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случиться, состояться или прийти как следствие определенных событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наступить",
                                "target_lang_sense_clarification": "о событии, времени или состоянии"
                            },
                            {
                                "target_lang_word": "произойти",
                                "target_lang_sense_clarification": "случиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b2a7831-6cb0-3f87-96d9-50c459ee0552",
                    "sense_definition": "Pojawić się bezpośrednio po czymś innym w kolejności czasowej lub logicznej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po gwałtownej burzy <w>nastąpiła</w> piękna i słoneczna pogoda.",
                            "target_lang_translations": {
                                "ru": "После сильной бури <w>наступила</w> прекрасная и солнечная погода."
                            }
                        },
                        {
                            "text": "Zaraz po przemówieniu dyrektora <w>nastąpi</w> wręczenie nagród dla pracowników.",
                            "target_lang_translations": {
                                "ru": "Сразу после речи директора <w>последует</w> вручение наград сотрудникам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nastąpić po sobie",
                        "jak następuje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появиться непосредственно после чего-либо в хронологическом или логическом порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "последовать",
                                "target_lang_sense_clarification": "произойти следом за чем-то"
                            },
                            {
                                "target_lang_word": "наступить",
                                "target_lang_sense_clarification": "прийти на смену чему-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fec9a84-b80b-348b-b423-ec6af0c9e21a",
                    "sense_definition": "Postawić stopę na czymś; przycisnąć coś nogą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ciemnym korytarzu przypadkiem <w>nastąpiłem</w> na leżącą na podłodze zabawkę.",
                            "target_lang_translations": {
                                "ru": "В темном коридоре я случайно <w>наступил</w> на лежащую на полу игрушку."
                            }
                        },
                        {
                            "text": "Uważaj, żeby w tłumie nie <w>nastąpić</w> komuś na nogę.",
                            "target_lang_translations": {
                                "ru": "Осторожно, чтобы в толпе не <w>наступить</w> кому-нибудь на ногу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nastąpić na odcisk",
                        "nastąpić na nogę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поставить ногу на что-либо; прижать что-либо ногой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наступить",
                                "target_lang_sense_clarification": "на ногу или предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a689f4de-54d7-3974-9d03-56af1b87b012",
                    "sense_definition": "Gwałtownie zbliżyć się do kogoś w celu ataku lub wywarcia presji (znaczenie dawne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "aggression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz śmiało <w>nastąpił</w> na przeciwnika, unosząc wysoko swój miecz.",
                            "target_lang_translations": {
                                "ru": "Рыцарь смело <w>наступил</w> на противника, высоко подняв свой меч."
                            }
                        },
                        {
                            "text": "Wróg znienacka <w>nastąpił</w> na nasze oddziały stacjonujące w dolinie.",
                            "target_lang_translations": {
                                "ru": "Враг внезапно <w>напал</w> на наши отряды, размещенные в долине."
                            }
                        }
                    ],
                    "synonyms": [
                        "podejść",
                        "zbliżyć się"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane głównie w literaturze historycznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко приблизиться к кому-либо с целью нападения или давления (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напасть",
                                "target_lang_sense_clarification": "атаковать"
                            },
                            {
                                "target_lang_word": "наступить",
                                "target_lang_sense_clarification": "двинуться в атаку (уст.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "coruscation",
        "coruscant"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6817a777-8971-30d7-9761-9004905e4b1b",
                    "sense_definition": "To give off light; to reflect in flashes; to sparkle.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "light_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The diamond necklace seemed to <w>coruscate</w> brilliantly under the ballroom lights.",
                            "target_lang_translations": {
                                "ru": "Бриллиантовое ожерелье, казалось, ослепительно <w>сверкало</w> в огнях бального зала."
                            }
                        },
                        {
                            "text": "We watched the fireworks <w>coruscate</w> against the dark canvas of the night sky.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали, как фейерверки <w>сверкают</w> на темном полотне ночного неба."
                            }
                        }
                    ],
                    "synonyms": [
                        "sparkle",
                        "glitter",
                        "scintillate",
                        "twinkle",
                        "glisten"
                    ],
                    "common_phrases": [
                        "coruscating-light",
                        "coruscating-colors"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испускать свет; отражать вспышки; сверкать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сверкать",
                                "target_lang_sense_clarification": "излучать яркий, часто прерывистый свет"
                            },
                            {
                                "target_lang_word": "искриться",
                                "target_lang_sense_clarification": "испускать искры или мелкие вспышки света"
                            },
                            {
                                "target_lang_word": "блистать",
                                "target_lang_sense_clarification": "ярко светиться или отражать свет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32f7da19-576f-353c-b1c6-c605c77ef137",
                    "sense_definition": "To exhibit brilliant technique, style, or wit.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "brilliance_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author was famous for his <w>coruscating</w> wit and sharp humor.",
                            "target_lang_translations": {
                                "ru": "Автор был известен своим <w>искромётным</w> остроумием и острым юмором."
                            }
                        },
                        {
                            "text": "She delivered a <w>coruscating</w> performance on the piano that thrilled the entire audience.",
                            "target_lang_translations": {
                                "ru": "Она выдала <w>блестящее</w> исполнение на фортепиано, которое привело в восторг всю публику."
                            }
                        }
                    ],
                    "synonyms": [
                        "scintillate"
                    ],
                    "common_phrases": [
                        "coruscating-wit",
                        "coruscating-intelligence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Демонстрировать блестящую технику, стиль или остроумие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блистать",
                                "target_lang_sense_clarification": "выделяться выдающимися способностями, умом или красотой"
                            },
                            {
                                "target_lang_word": "искриться",
                                "target_lang_sense_clarification": "быть полным живости, веселья или остроумия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
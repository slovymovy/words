    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8139eda5-5b2b-32a4-9c38-443f924a453f",
                    "sense_definition": "Geleidelijk verdwijnen of voorbijgaan, vaak gezegd van abstracte zaken zoals tijd, herinneringen, hoop of gevoelens.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "verdwijnen_figuurlijk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat is de tijd snel <w>vervlogen</w> sinds we elkaar voor het laatst zagen!",
                            "target_lang_translations": {
                                "en": "How quickly time has <w>flown by</w> since we last saw each other!",
                                "ru": "Как быстро <w>пролетело</w> время с тех пор, как мы виделись в последний раз!"
                            }
                        },
                        {
                            "text": "Al onze hoop op een goede afloop was in één klap <w>vervlogen</w>.",
                            "target_lang_translations": {
                                "en": "All our hope for a good outcome had <w>vanished</w> in one fell swoop.",
                                "ru": "Вся наша надежда на хороший исход <w>улетучилась</w> в один миг."
                            }
                        },
                        {
                            "text": "De mooie herinneringen aan die zomer zullen nooit helemaal <w>vervliegen</w>.",
                            "target_lang_translations": {
                                "en": "The beautiful memories of that summer will never completely <w>fade away</w>.",
                                "ru": "Прекрасные воспоминания о том лете никогда полностью не <w>исчезнут</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdwijnen",
                        "wegebben",
                        "voorbijgaan",
                        "vergaan"
                    ],
                    "antonyms": [
                        "blijven",
                        "beklijven",
                        "aanhouden"
                    ],
                    "common_phrases": [
                        "vervlogen tijden",
                        "de hoop is vervlogen",
                        "vervlogen jaren"
                    ],
                    "target_lang_definitions": {
                        "en": "To gradually disappear or pass by, often said of abstract things like time, memories, hope, or feelings.",
                        "ru": "Постепенно исчезать или проходить (часто об абстрактных вещах, таких как время, воспоминания, надежда или чувства)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fly by",
                                "target_lang_sense_clarification": "regarding time"
                            },
                            {
                                "target_lang_word": "vanish",
                                "target_lang_sense_clarification": "regarding hope or chances"
                            },
                            {
                                "target_lang_word": "fade away",
                                "target_lang_sense_clarification": "regarding memories or feelings"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пролетать",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "улетучиваться",
                                "target_lang_sense_clarification": "о надежде, чувствах"
                            },
                            {
                                "target_lang_word": "исчезать",
                                "target_lang_sense_clarification": "об исчезновении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5bc0d77-7547-3aea-81a3-68afcc740ae6",
                    "sense_definition": "Overgaan van vloeibare vorm in gasvorm (verdampen), of het langzaam verdwijnen van een geur.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "verdampen_fysiek",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je de fles niet goed afsluit, zal de alcohol snel <w>vervliegen</w>.",
                            "target_lang_translations": {
                                "en": "If you don't close the bottle properly, the alcohol will quickly <w>evaporate</w>.",
                                "ru": "Если плохо закрыть бутылку, спирт быстро <w>испарится</w>."
                            }
                        },
                        {
                            "text": "De sterke geur van de verf was na een paar uur gelukkig <w>vervlogen</w>.",
                            "target_lang_translations": {
                                "en": "Fortunately, the strong smell of the paint had <w>dissipated</w> after a few hours.",
                                "ru": "К счастью, сильный запах краски через несколько часов <w>выветрился</w>."
                            }
                        },
                        {
                            "text": "De ether was langzaam aan het <w>vervliegen</w> in de warme kamer.",
                            "target_lang_translations": {
                                "en": "The ether was slowly <w>evaporating</w> in the warm room.",
                                "ru": "Эфир медленно <w>испарялся</w> в теплой комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdampen",
                        "vervluchtigen"
                    ],
                    "antonyms": [
                        "condenseren",
                        "neerslaan"
                    ],
                    "common_phrases": [
                        "snel vervliegen",
                        "de geur vervliegt"
                    ],
                    "target_lang_definitions": {
                        "en": "To change from liquid to gas (evaporate), or the slow disappearance of a scent.",
                        "ru": "Переходить из жидкого состояния в газообразное (испаряться) или постепенно исчезать (о запахе)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "evaporate",
                                "target_lang_sense_clarification": "liquids turning into gas"
                            },
                            {
                                "target_lang_word": "dissipate",
                                "target_lang_sense_clarification": "scents or fumes disappearing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "испаряться",
                                "target_lang_sense_clarification": "о жидкости"
                            },
                            {
                                "target_lang_word": "выветриваться",
                                "target_lang_sense_clarification": "о запахе"
                            },
                            {
                                "target_lang_word": "улетучиваться",
                                "target_lang_sense_clarification": "о газе или запахе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
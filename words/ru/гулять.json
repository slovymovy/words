    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4eed703f-f0bf-3b54-9337-fc2dc31daf0b",
                    "sense_definition": "Ходить пешком на свежем воздухе ради отдыха или удовольствия.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В хорошую погоду мы любим <гулять> в городском парке."
                        },
                        {
                            "text": "По вечерам соседи выходят <гулять> со своими собаками."
                        }
                    ],
                    "synonyms": [
                        "сойтись",
                        "веселиться",
                        "развлекаться",
                        "кутить"
                    ],
                    "antonyms": [
                        "стоять",
                        "лежать"
                    ],
                    "common_phrases": [
                        "пойти гулять",
                        "гулять с собакой",
                        "гулять в парке",
                        "гулять пешком"
                    ]
                },
                {
                    "sense_id": "4b9c4e97-47ae-33ce-ac50-6cbca41504e0",
                    "sense_definition": "Веселиться, развлекаться, отмечать праздник (часто с употреблением алкоголя).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "leisure_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вся деревня три дня <гуляла> на шумной свадьбе."
                        },
                        {
                            "text": "Студенты сдали последнюю сессию и <гуляли> до самого утра."
                        }
                    ],
                    "synonyms": [
                        "веселиться",
                        "развлекаться",
                        "кутить"
                    ],
                    "common_phrases": [
                        "гулять на свадьбе",
                        "гулять всю ночь",
                        "гулять на все"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "79635a60-cc3a-3a1d-91b5-d961a77128de",
                    "sense_definition": "Быть свободным от работы или учёбы, иметь выходной день.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leisure_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Праздник пришёлся на четверг, так что мы <гуляем> четыре дня подряд."
                        },
                        {
                            "text": "Начальник подписал отпуск, и теперь я <гуляю> целый месяц."
                        }
                    ],
                    "common_phrases": [
                        "завтра гуляем",
                        "гулять два дня"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "d88c1efe-f719-349d-a1ce-228c7cdba7c9",
                    "sense_definition": "Встречаться с кем-либо, иметь романтические отношения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relationships_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она <гуляла> с этим парнем ещё в старших классах школы."
                        },
                        {
                            "text": "Родители строго запрещали дочери <гулять> допоздна."
                        }
                    ],
                    "common_phrases": [
                        "гулять с парнем",
                        "гулять с девушкой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "654c9e57-dfe7-38b9-8e15-d02578725961",
                    "sense_definition": "Изменять супругу или постоянному партнёру, иметь связи на стороне.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relationships_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все соседи знали, что её муж давно <гуляет>."
                        },
                        {
                            "text": "Он был примерным семьянином и никогда не <гулял> от жены."
                        }
                    ],
                    "common_phrases": [
                        "муж гуляет",
                        "гулять от жены",
                        "гулять налево"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "fac5b09e-2fce-3247-ab16-112f4c875d21",
                    "sense_definition": "Свободно перемещаться в разных направлениях (о ветре, сквозняке, волнах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старом деревянном доме по полу постоянно <гуляли> сквозняки."
                        },
                        {
                            "text": "Холодный ветер свободно <гулял> по пустому полю."
                        }
                    ],
                    "antonyms": [
                        "застыть"
                    ],
                    "common_phrases": [
                        "ветер гуляет",
                        "сквозняк гуляет",
                        "волны гуляют"
                    ]
                },
                {
                    "sense_id": "55c2efc9-315d-32cd-bfd4-86f44a8525c7",
                    "sense_definition": "Находиться в бездействии, не использоваться (о механизмах, земле или помещении).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дорогой станок <гуляет> без работы уже целую неделю."
                        },
                        {
                            "text": "Плодородная земля не должна <гулять> без обработки."
                        }
                    ],
                    "common_phrases": [
                        "земля гуляет",
                        "станок гуляет"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "bd1020a7-ccb0-372d-8604-59c31dff71ae",
                    "sense_definition": "Не спать, бодрствовать в то время, когда положено спать (обычно о маленьких детях).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Маленький ребёнок ночью долго <гулял> и не давал родителям спать."
                        },
                        {
                            "text": "Если младенец выспится днём, то ночью он будет <гулять>."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ]
                }
            ]

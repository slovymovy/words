    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "18fb954f-8942-38cf-86a2-29714287a4bc",
                    "sense_definition": "Сделать более длительным, увеличить время протекания чего-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи сделали всё возможное, чтобы <w>продлить</w> жизнь пациента.",
                            "target_lang_translations": {
                                "en": "The doctors did everything possible to <w>prolong</w> the patient's life."
                            }
                        },
                        {
                            "text": "Мы решили <w>продлить</w> наше путешествие и остаться на море ещё на неделю.",
                            "target_lang_translations": {
                                "en": "We decided to <w>extend</w> our trip and stay by the sea for another week."
                            }
                        }
                    ],
                    "synonyms": [
                        "удлинить",
                        "растянуть"
                    ],
                    "antonyms": [
                        "сократить",
                        "укоротить"
                    ],
                    "common_phrases": [
                        "продлить жизнь",
                        "продлить удовольствие",
                        "продлить молчание",
                        "продлить отпуск"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something last longer or increase its duration."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prolong",
                                "target_lang_sense_clarification": "time, life, silence"
                            },
                            {
                                "target_lang_word": "extend",
                                "target_lang_sense_clarification": "duration of an event or activity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13087077-48bc-302f-abdc-7b0be6e40e7c",
                    "sense_definition": "Увеличить официальный срок действия документа, договора или соглашения.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "validity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно срочно <w>продлить</w> визу, чтобы легально оставаться в стране.",
                            "target_lang_translations": {
                                "en": "I urgently need to <w>extend</w> my visa to legally remain in the country."
                            }
                        },
                        {
                            "text": "Компания предложила сотруднику <w>продлить</w> рабочий контракт на два года.",
                            "target_lang_translations": {
                                "en": "The company offered the employee to <w>renew</w> the employment contract for two years."
                            }
                        }
                    ],
                    "synonyms": [
                        "пролонгировать",
                        "возобновить"
                    ],
                    "antonyms": [
                        "аннулировать",
                        "расторгнуть"
                    ],
                    "common_phrases": [
                        "продлить договор",
                        "продлить визу",
                        "продлить подписку",
                        "продлить паспорт"
                    ],
                    "target_lang_definitions": {
                        "en": "To increase the validity period of a document, contract, or agreement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "renew",
                                "target_lang_sense_clarification": "subscription, contract"
                            },
                            {
                                "target_lang_word": "extend",
                                "target_lang_sense_clarification": "visa, deadline, validity"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        "vunsigheid"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "62c18609-70e6-3756-8982-1368c8705443",
                    "sense_definition": "De eigenschap van muf of bedompt zijn, vaak met betrekking tot een ruimte waar geen frisse lucht komt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "atmosphere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>vunsheid</w> in de afgesloten kelder sloeg ons direct op de adem.",
                            "target_lang_translations": {
                                "en": "The <w>mustiness</w> in the sealed cellar immediately caught in our throats.",
                                "ru": "<w>Затхлость</w> в закрытом подвале сразу перехватила нам дыхание."
                            }
                        },
                        {
                            "text": "Na de vakantie hing er een lichte <w>vunsheid</w> in het huis door de gesloten ramen.",
                            "target_lang_translations": {
                                "en": "After the vacation, a slight <w>stuffiness</w> hung in the house due to the closed windows.",
                                "ru": "После отпуска в доме стояла легкая <w>спертость</w> из-за закрытых окон."
                            }
                        }
                    ],
                    "synonyms": [
                        "mufheid",
                        "bedomptheid"
                    ],
                    "antonyms": [
                        "frisheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality of being musty or stuffy, often regarding a room without fresh air.",
                        "ru": "Свойство быть затхлым или спертым, часто относящееся к помещению без свежего воздуха."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mustiness",
                                "target_lang_sense_clarification": "stale, moldy smell or state"
                            },
                            {
                                "target_lang_word": "stuffiness",
                                "target_lang_sense_clarification": "lack of fresh air"
                            },
                            {
                                "target_lang_word": "fustiness",
                                "target_lang_sense_clarification": "stale, old smell"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "затхлость",
                                "target_lang_sense_clarification": "запах или состояние воздуха"
                            },
                            {
                                "target_lang_word": "спертость",
                                "target_lang_sense_clarification": "о несвежем воздухе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d935885-8872-3835-82ac-c7386c85acb0",
                    "sense_definition": "Een toestand van viezigheid, verwaarlozing of slordigheid.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>vunsheid</w> van het oude gebouw was duidelijk zichtbaar aan de muren.",
                            "target_lang_translations": {
                                "en": "The <w>filthiness</w> of the old building was clearly visible on the walls.",
                                "ru": "<w>Запущенность</w> старого здания была отчетливо видна по стенам."
                            }
                        },
                        {
                            "text": "Hij kon niet wennen aan de <w>vunsheid</w> en rommel in de studentenwoning.",
                            "target_lang_translations": {
                                "en": "He could not get used to the <w>squalor</w> and mess in the student house.",
                                "ru": "Он не мог привыкнуть к <w>грязи</w> и беспорядку в студенческом доме."
                            }
                        }
                    ],
                    "synonyms": [
                        "smerigheid",
                        "vuiligheid"
                    ],
                    "antonyms": [
                        "netheid",
                        "schoonheid"
                    ],
                    "target_lang_definitions": {
                        "en": "A state of filthiness, neglect, or sloppiness.",
                        "ru": "Состояние грязи, запущенности или неряшливости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "filthiness",
                                "target_lang_sense_clarification": "state of being dirty"
                            },
                            {
                                "target_lang_word": "squalor",
                                "target_lang_sense_clarification": "state of neglect and dirt"
                            },
                            {
                                "target_lang_word": "sordidness",
                                "target_lang_sense_clarification": "dirty or morally ignoble state"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "грязь",
                                "target_lang_sense_clarification": "физическая нечистота"
                            },
                            {
                                "target_lang_word": "запущенность",
                                "target_lang_sense_clarification": "состояние небрежности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ff05c55-ff89-3662-ae75-c2d63ed2b9c5",
                    "sense_definition": "Een onaangename mate van vochtigheid of klamheid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "atmosphere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>vunsheid</w> van de regenachtige dag trok langzaam in onze kleren.",
                            "target_lang_translations": {
                                "en": "The <w>clamminess</w> of the rainy day slowly seeped into our clothes.",
                                "ru": "<w>Сырость</w> дождливого дня медленно проникала в нашу одежду."
                            }
                        },
                        {
                            "text": "Door de aanhoudende <w>vunsheid</w> begon het houtwerk in de schuur te rotten.",
                            "target_lang_translations": {
                                "en": "Due to the persistent <w>dankness</w>, the woodwork in the shed began to rot.",
                                "ru": "Из-за постоянной <w>сырости</w> деревянные конструкции в сарае начали гнить."
                            }
                        }
                    ],
                    "synonyms": [
                        "klamheid",
                        "vochtigheid"
                    ],
                    "antonyms": [
                        "droogte"
                    ],
                    "target_lang_definitions": {
                        "en": "An unpleasant degree of humidity or clamminess.",
                        "ru": "Неприятная степень влажности или сырости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dankness",
                                "target_lang_sense_clarification": "unpleasant, cold dampness"
                            },
                            {
                                "target_lang_word": "clamminess",
                                "target_lang_sense_clarification": "sticky, unpleasant dampness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сырость",
                                "target_lang_sense_clarification": "неприятная влажность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
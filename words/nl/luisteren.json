    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "349f45d4-d5b8-3060-896e-7c19da329ae6",
                    "sense_definition": "Met aandacht horen om geluiden, muziek of spraak waar te nemen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>luister</w> elke ochtend tijdens het ontbijt naar de radio.",
                            "target_lang_translations": {
                                "en": "I <w>listen</w> to the radio every morning during breakfast.",
                                "ru": "Я <w>слушаю</w> радио каждое утро за завтраком."
                            }
                        },
                        {
                            "text": "Kun je even stil zijn en goed naar mij <w>luisteren</w>?",
                            "target_lang_translations": {
                                "en": "Can you be quiet for a moment and <w>listen</w> to me carefully?",
                                "ru": "Ты можешь помолчать минутку и внимательно меня <w>послушать</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "toehoren"
                    ],
                    "common_phrases": [
                        "naar muziek luisteren",
                        "goed luisteren",
                        "luisteren naar"
                    ],
                    "target_lang_definitions": {
                        "en": "To hear with attention in order to perceive sounds, music, or speech.",
                        "ru": "Слушать внимательно, чтобы воспринимать звуки, музыку или речь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "listen",
                                "target_lang_sense_clarification": "to pay attention to sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "слушать",
                                "target_lang_sense_clarification": "воспринимать звуковую информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b825803-71bc-3692-ba31-fe9193795618",
                    "sense_definition": "Doen wat iemand vraagt, zegt of beveelt; gehoorzamen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "obedience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond <w>luistert</w> heel goed naar de commando's van zijn baas.",
                            "target_lang_translations": {
                                "en": "The dog <w>listens</w> very well to its owner's commands.",
                                "ru": "Собака очень хорошо <w>слушается</w> команд своего хозяина."
                            }
                        },
                        {
                            "text": "Als je niet naar je ouders <w>luistert</w>, krijg je straf.",
                            "target_lang_translations": {
                                "en": "If you do not <w>listen</w> to your parents, you will be punished.",
                                "ru": "Если ты не будешь <w>слушаться</w> родителей, тебя накажут."
                            }
                        }
                    ],
                    "synonyms": [
                        "gehoorzamen"
                    ],
                    "common_phrases": [
                        "niet willen luisteren",
                        "naar je ouders luisteren"
                    ],
                    "target_lang_definitions": {
                        "en": "To do what someone asks, says, or orders; to obey.",
                        "ru": "Делать то, что кто-то просит, говорит или приказывает; повиноваться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "listen",
                                "target_lang_sense_clarification": "to follow advice or instructions"
                            },
                            {
                                "target_lang_word": "obey",
                                "target_lang_sense_clarification": "to comply with commands"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "слушаться",
                                "target_lang_sense_clarification": "повиноваться, быть послушным"
                            },
                            {
                                "target_lang_word": "слушать",
                                "target_lang_sense_clarification": "следовать советам или указаниям"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bf067f5-d7ff-3441-98e0-b6ab9bbfff58",
                    "sense_definition": "Glans, schoonheid of waardigheid verlenen aan iets.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "splendor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aanwezigheid van de koning <w>luisterde</w> de plechtige viering.",
                            "target_lang_translations": {
                                "en": "The king's presence <w>graced</w> the solemn celebration.",
                                "ru": "Присутствие короля <w>украсило</w> торжественную церемонию."
                            }
                        },
                        {
                            "text": "Dit prachtige muziekstuk <w>luisterde</w> de avond op bijzondere wijze.",
                            "target_lang_translations": {
                                "en": "This beautiful piece of music <w>graced</w> the evening in a special way.",
                                "ru": "Это прекрасное музыкальное произведение по-особому <w>украсило</w> вечер."
                            }
                        }
                    ],
                    "antonyms": [
                        "ontluisteren"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "In modern Nederlands wordt hiervoor meestal 'opluisteren' gebruikt."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To lend luster, beauty, or dignity to something.",
                        "ru": "Придавать блеск, красоту или достоинство чему-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grace",
                                "target_lang_sense_clarification": "to add dignity or honor to an occasion"
                            },
                            {
                                "target_lang_word": "add luster to",
                                "target_lang_sense_clarification": "to make something more splendid"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "украшать",
                                "target_lang_sense_clarification": "делать событие более торжественным"
                            },
                            {
                                "target_lang_word": "придавать блеск",
                                "target_lang_sense_clarification": "делать что-то более великолепным"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
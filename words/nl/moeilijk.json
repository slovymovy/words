    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0439805f-ff96-3090-ada1-5253b5732612",
                    "sense_definition": "Niet gemakkelijk; iets wat veel inspanning, geduld of vaardigheid vraagt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "difficulty_effort",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>moeilijk</w> om een nieuwe taal goed te leren.",
                            "target_lang_translations": {
                                "en": "It is <w>difficult</w> to learn a new language well.",
                                "ru": "<w>Трудно</w> хорошо выучить новый язык."
                            }
                        },
                        {
                            "text": "Deze wiskundeopgave is te <w>moeilijk</w> voor de leerlingen.",
                            "target_lang_translations": {
                                "en": "This math problem is too <w>hard</w> for the students.",
                                "ru": "Эта задача по математике слишком <w>сложная</w> для учеников."
                            }
                        }
                    ],
                    "synonyms": [
                        "lastig",
                        "ingewikkeld"
                    ],
                    "antonyms": [
                        "makkelijk",
                        "gemakkelijk"
                    ],
                    "common_phrases": [
                        "moeilijk doen",
                        "moeilijk te zeggen",
                        "het moeilijk hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "Not easy; requiring much effort, patience, or skill.",

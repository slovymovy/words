        "securities"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a85fc731-d1a6-3959-afb0-41f46255dfcc",
                    "sense_definition": "Protected from danger, harm, or attack.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "safety_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children feel <w>secure</w> when their parents are nearby.",
                            "target_lang_translations": {
                                "ru": "Дети чувствуют себя <w>в безопасности</w>, когда их родители рядом."
                            }
                        },
                        {
                            "text": "We need to find a <w>secure</w> place to hide the money.",
                            "target_lang_translations": {
                                "ru": "Нам нужно найти <w>надёжное</w> место, чтобы спрятать деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "safe",
                        "protected"
                    ],
                    "antonyms": [
                        "insecure",
                        "insecure"
                    ],
                    "common_phrases": [
                        "feel secure",
                        "secure place",
                        "secure environment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защищённый от опасности или вреда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безопасный",
                                "target_lang_sense_clarification": "не представляющий угрозы"
                            },
                            {
                                "target_lang_word": "надёжный",
                                "target_lang_sense_clarification": "внушающий доверие, защищённый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb101314-c0f5-3be7-86e4-e159a146252f",
                    "sense_definition": "Firmly attached or fixed so that it will not move or fall.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stability_fixation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure the ladder is <w>secure</w> before you try to climb it.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что лестница <w>устойчива</w>, прежде чем пытаться залезть на неё."
                            }
                        },
                        {
                            "text": "The knot was not <w>secure</w>, so the boat drifted away from the dock.",
                            "target_lang_translations": {
                                "ru": "Узел был <w>ненадёжным</w>, поэтому лодка отплыла от причала."
                            }
                        }
                    ],
                    "synonyms": [
                        "stable",
                        "fixed",
                        "firm"
                    ],
                    "antonyms": [
                        "insecure",
                        "insecure"
                    ],
                    "common_phrases": [
                        "secure connection",
                        "secure footing",
                        "secure grip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочно прикреплённый или зафиксированный, устойчивый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надёжный",
                                "target_lang_sense_clarification": "прочный, хорошо закреплённый"
                            },
                            {
                                "target_lang_word": "устойчивый",
                                "target_lang_sense_clarification": "стабильный, не шаткий"
                            },
                            {
                                "target_lang_word": "прочный",
                                "target_lang_sense_clarification": "крепкий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3302f551-eaf9-3fda-b6a4-d6d95a45060b",
                    "sense_definition": "Protected against unauthorized access or theft, especially regarding information or technology.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "safety_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please ensure you are using a <w>secure</w> connection when checking your bank account.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, убедитесь, что вы используете <w>безопасное</w> соединение при проверке банковского счёта."
                            }
                        },
                        {
                            "text": "The confidential documents are kept in a highly <w>secure</w> database.",
                            "target_lang_translations": {
                                "ru": "Конфиденциальные документы хранятся в хорошо <w>защищённой</w> базе данных."
                            }
                        }
                    ],
                    "synonyms": [
                        "encrypted",
                        "private"
                    ],
                    "antonyms": [
                        "insecure",
                        "insecure"
                    ],
                    "common_phrases": [
                        "secure network",
                        "secure website",
                        "secure line"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защищённый от несанкционированного доступа (особенно в информационных технологиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защищённый",
                                "target_lang_sense_clarification": "о канале связи или данных"
                            },
                            {
                                "target_lang_word": "безопасный",
                                "target_lang_sense_clarification": "не несущий угрозы утечки информации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3be7fc20-fcbf-32b8-8640-d0584ad97a1e",
                    "sense_definition": "Feeling confident and free from fear, anxiety, or doubt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "confidence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She finally felt <w>secure</w> enough to speak in front of the large crowd.",
                            "target_lang_translations": {
                                "ru": "Она наконец почувствовала себя достаточно <w>уверенной</w>, чтобы выступить перед большой толпой."
                            }
                        },
                        {
                            "text": "A loving family helps a child grow up feeling emotionally <w>secure</w>.",
                            "target_lang_translations": {
                                "ru": "Любящая семья помогает ребёнку расти эмоционально <w>защищённым</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "confident",
                        "assured"
                    ],
                    "antonyms": [
                        "insecure",
                        "insecure"
                    ],
                    "common_phrases": [
                        "feel secure",
                        "emotionally secure",
                        "secure future"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий уверенность, спокойствие и отсутствие страха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уверенный",
                                "target_lang_sense_clarification": "в себе или в будущем"
                            },
                            {
                                "target_lang_word": "спокойный",
                                "target_lang_sense_clarification": "не испытывающий тревоги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92e06a0b-14fb-35a1-b34e-cca7828b1fc5",
                    "sense_definition": "Certain to happen or be achieved; guaranteed.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With such a large lead in the polls, their victory seemed <w>secure</w>.",
                            "target_lang_translations": {
                                "ru": "С таким большим отрывом в опросах их победа казалась <w>обеспеченной</w>."
                            }
                        },
                        {
                            "text": "His place in history is <w>secure</w> thanks to his amazing discoveries.",
                            "target_lang_translations": {
                                "ru": "Его место в истории <w>гарантировано</w> благодаря его удивительным открытиям."
                            }
                        }
                    ],
                    "synonyms": [
                        "certain",
                        "assured",
                        "guaranteed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несомненный, гарантированный, который обязательно произойдёт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гарантированный",
                                "target_lang_sense_clarification": "обеспеченный"
                            },
                            {
                                "target_lang_word": "несомненный",
                                "target_lang_sense_clarification": "верный, бесспорный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8b9b99b8-6951-3c75-bfb9-e2022c70cfc9",
                    "sense_definition": "To succeed in getting something, especially after a lot of effort.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company managed to <w>secure</w> a major contract with the government.",
                            "target_lang_translations": {
                                "ru": "Компании удалось <w>получить</w> крупный контракт с правительством."
                            }
                        },
                        {
                            "text": "He worked very hard to <w>secure</w> a place at a top university.",
                            "target_lang_translations": {
                                "ru": "Он очень много работал, чтобы <w>добиться</w> места в престижном университете."
                            }
                        }
                    ],
                    "synonyms": [
                        "obtain",
                        "acquire",
                        "gain"
                    ],
                    "common_phrases": [
                        "secure a deal",
                        "secure funding",
                        "secure a job"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить или достичь чего-либо, приложив усилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добиться",
                                "target_lang_sense_clarification": "достичь цели усилиями"
                            },
                            {
                                "target_lang_word": "получить",
                                "target_lang_sense_clarification": "приобрести, стать обладателем"
                            },
                            {
                                "target_lang_word": "обеспечить",
                                "target_lang_sense_clarification": "гарантировать получение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7904af0-faf0-3545-aec8-af614f00d59a",
                    "sense_definition": "To fasten something firmly or lock it to prevent it from moving or being opened.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "stability_fixation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>secure</w> all doors and windows before leaving the house.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>заприте</w> все двери и окна перед выходом из дома."
                            }
                        },
                        {
                            "text": "The heavy cargo was <w>secured</w> to the deck of the ship with ropes.",
                            "target_lang_translations": {
                                "ru": "Тяжёлый груз был <w>закреплён</w> на палубе корабля верёвками."
                            }
                        }
                    ],
                    "synonyms": [
                        "fasten",
                        "lock",
                        "fix"
                    ],
                    "common_phrases": [
                        "secure the door",
                        "secure the area",
                        "secure the rope"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочно закрепить что-либо или запереть на замок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрепить",
                                "target_lang_sense_clarification": "зафиксировать неподвижно"
                            },
                            {
                                "target_lang_word": "запереть",
                                "target_lang_sense_clarification": "закрыть на замок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed34cb6c-0d90-3f4c-87e3-82fdde586b4d",
                    "sense_definition": "To protect something from harm or danger.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "safety_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Soldiers were sent to <w>secure</w> the border against potential threats.",
                            "target_lang_translations": {
                                "ru": "Солдаты были отправлены, чтобы <w>обезопасить</w> границу от потенциальных угроз."
                            }
                        },
                        {
                            "text": "We must take steps to <w>secure</w> our computer network against hackers.",
                            "target_lang_translations": {
                                "ru": "Мы должны принять меры, чтобы <w>защитить</w> нашу компьютерную сеть от хакеров."
                            }
                        }
                    ],
                    "synonyms": [
                        "protect",
                        "guard",
                        "defend"
                    ],
                    "common_phrases": [
                        "secure the border",
                        "secure the perimeter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охранять или защищать что-либо от опасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обезопасить",
                                "target_lang_sense_clarification": "сделать безопасным"
                            },
                            {
                                "target_lang_word": "защитить",
                                "target_lang_sense_clarification": "оборонять"
                            }
                        ]
                    }
                }
            ]
        }
    ]

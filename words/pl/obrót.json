                {
                    "sense_id": "9f134217-88db-3988-ae86-495c86737283",
                    "sense_definition": "Ruch obiektu dookoła własnej osi lub wokół innego punktu; pełna zmiana pozycji w ten sposób.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ziemia wykonuje pełny <w>obrót</w> wokół własnej osi w ciągu jednej doby.",
                            "target_lang_translations": {
                                "ru": "Земля совершает полный <w>оборот</w> вокруг собственной оси за одни сутки."
                            }
                        },
                        {
                            "text": "Tancerka wykonała szybki <w>obrót</w> i ukłoniła się publiczności.",
                            "target_lang_translations": {
                                "ru": "Танцовщица сделала быстрый <w>оборот</w> и поклонилась публике."
                            }
                        }
                    ],
                    "synonyms": [
                        "rotacja",
                        "piruet",
                        "młyniec"
                    ],
                    "common_phrases": [

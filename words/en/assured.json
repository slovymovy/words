{
    "word_family": [
        "assurance",
        "reassurance",
        "reassure",
        "self-assured"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17e6eb1d-2898-30cd-817b-d1a70a7a0026",
                    "sense_definition": "Simple past and past participle of <w>assure</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>assured</w> us that the project would be finished on time.",
                            "target_lang_translations": {
                                "ru": "Он <w>заверил</w> нас, что проект будет завершен вовремя."
                            }
                        },
                        {
                            "text": "We were <w>assured</w> that our luggage was safe.",
                            "target_lang_translations": {
                                "ru": "Нас <w>уверили</w>, что наш багаж в безопасности."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола assure (уверять, гарантировать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заверил",
                                "target_lang_sense_clarification": "прошедшее время, обещание или гарантия"
                            },
                            {
                                "target_lang_word": "уверил",
                                "target_lang_sense_clarification": "прошедшее время, убеждение кого-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6fa52419-dd99-3027-b78e-1c1bf45233c3",
                    "sense_definition": "Certain to happen or be achieved; guaranteed.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can rest <w>assured</w> that we will do everything possible to help.",
                            "target_lang_translations": {
                                "ru": "Вы можете быть <w>уверены</w>, что мы сделаем все возможное, чтобы помочь."
                            }
                        },
                        {
                            "text": "With such a strong team, victory seemed almost <w>assured</w>.",
                            "target_lang_translations": {
                                "ru": "С такой сильной командой победа казалась почти <w>обеспеченной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sicker",
                        "insured"
                    ],
                    "antonyms": [
                        "uncertain",
                        "doubtful"
                    ],
                    "common_phrases": [
                        "rest assured",
                        "mutually assured destruction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несомненный, гарантированный; то, что обязательно произойдет или будет достигнуто."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гарантированный",
                                "target_lang_sense_clarification": "обеспеченный, несомненный"
                            },
                            {
                                "target_lang_word": "обеспеченный",
                                "target_lang_sense_clarification": "о будущем успехе или результате"
                            },
                            {
                                "target_lang_word": "уверенный",
                                "target_lang_sense_clarification": "в выражении 'rest assured' (быть уверенным)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcc50dca-b091-3ff2-9f1f-ad7367d49e08",
                    "sense_definition": "Showing confidence in one's own abilities or character.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young actor gave a remarkably <w>assured</w> performance on stage.",
                            "target_lang_translations": {
                                "ru": "Молодой актер продемонстрировал на сцене удивительно <w>уверенную</w> игру."
                            }
                        },
                        {
                            "text": "She spoke in a calm and <w>assured</w> manner that made everyone listen.",
                            "target_lang_translations": {
                                "ru": "Она говорила в спокойной и <w>уверенной</w> манере, которая заставила всех слушать."
                            }
                        }
                    ],
                    "synonyms": [
                        "sicker",
                        "insured"
                    ],
                    "antonyms": [
                        "insecure",
                        "hesitant"
                    ],
                    "common_phrases": [
                        "self-assured",
                        "assured manner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Демонстрирующий уверенность в своих способностях или характере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уверенный",
                                "target_lang_sense_clarification": "выражающий спокойствие и компетентность"
                            },
                            {
                                "target_lang_word": "самоуверенный",
                                "target_lang_sense_clarification": "уверенный в своих силах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "45edabe3-b76b-3f1a-b3fe-dac62a9d3f32",
                    "sense_definition": "A person who is protected by an insurance policy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>assured</w> must notify the company immediately in the event of a loss.",
                            "target_lang_translations": {
                                "ru": "<w>Застрахованное лицо</w> обязано немедленно уведомить компанию в случае убытка."
                            }
                        },
                        {
                            "text": "Life insurance provides financial security for the family of the <w>assured</w>.",
                            "target_lang_translations": {
                                "ru": "Страхование жизни обеспечивает финансовую безопасность семье <w>застрахованного</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sicker",
                        "insured"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, защищенное страховым полисом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застрахованное лицо",
                                "target_lang_sense_clarification": "юридический термин"
                            },
                            {
                                "target_lang_word": "страхователь",
                                "target_lang_sense_clarification": "сторона в договоре страхования"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "clerical",
        "clerkship"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c976526-10e4-3379-8f60-8c0a50357983",
                    "sense_definition": "An employee who performs general office duties such as keeping records, filing, and typing.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "job_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She works as a junior <w>clerk</w> in a busy insurance office.",
                            "target_lang_translations": {
                                "ru": "Она работает младшим <w>клерком</w> в оживленном страховом офисе."
                            }
                        },
                        {
                            "text": "The filing <w>clerk</w> organized the documents alphabetically.",
                            "target_lang_translations": {
                                "ru": "<w>Делопроизводитель</w> расставил документы в алфавитном порядке."
                            }
                        }
                    ],
                    "synonyms": [
                        "secretary"
                    ],
                    "common_phrases": [
                        "junior clerk",
                        "filing clerk",
                        "office clerk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служащий, выполняющий общие офисные обязанности, такие как ведение записей, архивирование и набор текста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клерк",
                                "target_lang_sense_clarification": "офисный работник"
                            },
                            {
                                "target_lang_word": "служащий",
                                "target_lang_sense_clarification": "сотрудник учреждения"
                            },
                            {
                                "target_lang_word": "делопроизводитель",
                                "target_lang_sense_clarification": "ответственный за документы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6eb08de-2f57-3420-b03f-4f248922a9c4",
                    "sense_definition": "A person who serves customers in a shop or store.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "job_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I asked the <w>clerk</w> if they had this shirt in a smaller size.",
                            "target_lang_translations": {
                                "ru": "Я спросил у <w>продавца</w>, есть ли у них эта рубашка меньшего размера."
                            }
                        },
                        {
                            "text": "The store <w>clerk</w> scanned my items quickly at the counter.",
                            "target_lang_translations": {
                                "ru": "<w>Продавец</w> магазина быстро просканировал мои товары на кассе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sales clerk",
                        "store clerk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обслуживающий покупателей в магазине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продавец",
                                "target_lang_sense_clarification": "в магазине"
                            },
                            {
                                "target_lang_word": "консультант",
                                "target_lang_sense_clarification": "торговый работник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa5f9d90-66ca-3659-a108-da415edd4ec8",
                    "sense_definition": "An employee at a hotel reception desk who deals with guests.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "job_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The night <w>clerk</w> gave us our room keys when we arrived late.",
                            "target_lang_translations": {
                                "ru": "Ночной <w>портье</w> выдал нам ключи от номера, когда мы приехали поздно."
                            }
                        },
                        {
                            "text": "Please leave your key with the desk <w>clerk</w> when you go out.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, оставляйте ключ у <w>администратора</w>, когда уходите."
                            }
                        }
                    ],
                    "common_phrases": [
                        "desk clerk",
                        "hotel clerk",
                        "front desk clerk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сотрудник стойки регистрации в отеле, работающий с гостями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "администратор",
                                "target_lang_sense_clarification": "на ресепшене"
                            },
                            {
                                "target_lang_word": "портье",
                                "target_lang_sense_clarification": "дежурный в отеле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01448954-ba28-3f37-8d6e-c3f33bb2aee6",
                    "sense_definition": "An assistant to a judge or lawyer who helps with legal research and documentation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "job_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After graduating, he worked as a <w>clerk</w> for a federal judge.",
                            "target_lang_translations": {
                                "ru": "После окончания учебы он работал <w>секретарем</w> у федерального судьи."
                            }
                        },
                        {
                            "text": "The court <w>clerk</w> called the room to order before the trial began.",
                            "target_lang_translations": {
                                "ru": "Судебный <w>секретарь</w> призвал всех к порядку перед началом процесса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "law clerk",
                        "court clerk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помощник судьи или адвоката, помогающий с правовыми исследованиями и документацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секретарь",
                                "target_lang_sense_clarification": "судебный"
                            },
                            {
                                "target_lang_word": "помощник",
                                "target_lang_sense_clarification": "юриста или судьи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e14d8d9a-b9a0-36fe-9cd3-2a4fcd2375f3",
                    "sense_definition": "A member of the clergy or a cleric.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "job_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old manuscript described a <w>clerk</w> in holy orders.",
                            "target_lang_translations": {
                                "ru": "В старинной рукописи описывался <w>священнослужитель</w>, принявший сан."
                            }
                        },
                        {
                            "text": "In medieval times, a <w>clerk</w> was often a man of learning and religion.",
                            "target_lang_translations": {
                                "ru": "В средневековье <w>клирик</w> часто был образованным и религиозным человеком."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член духовенства или священнослужитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "священнослужитель",
                                "target_lang_sense_clarification": "духовное лицо"
                            },
                            {
                                "target_lang_word": "клирик",
                                "target_lang_sense_clarification": "историческое"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "68ca51fd-abd0-33d5-8ca6-d51e01e5b5a2",
                    "sense_definition": "To work as a clerk, performing administrative or sales duties.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>clerked</w> at a grocery store to pay for his college tuition.",
                            "target_lang_translations": {
                                "ru": "Он <w>работал продавцом</w> в продуктовом магазине, чтобы оплатить обучение в колледже."
                            }
                        },
                        {
                            "text": "She <w>clerked</w> for a law firm during the summer break.",
                            "target_lang_translations": {
                                "ru": "Она <w>работала помощником</w> в юридической фирме во время летних каникул."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clerk for",
                        "clerk at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Работать в качестве клерка, выполняя административные обязанности или обязанности по продажам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работать клерком",
                                "target_lang_sense_clarification": "выполнять обязанности клерка"
                            },
                            {
                                "target_lang_word": "служить",
                                "target_lang_sense_clarification": "быть на службе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bb6bd12-44cb-3052-85f1-91c50ce25916",
                    "sense_definition": "To record a patient's medical history and perform an examination upon admission.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The junior doctor had to <w>clerk</w> five new patients last night.",
                            "target_lang_translations": {
                                "ru": "Младшему врачу пришлось <w>оформлять</w> пять новых пациентов прошлой ночью."
                            }
                        },
                        {
                            "text": "Medical students learn how to <w>clerk</w> patients effectively on the wards.",
                            "target_lang_translations": {
                                "ru": "Студенты-медики учатся эффективно <w>собирать анамнез</w> у пациентов в палатах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clerk a patient",
                        "clerk in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Записывать историю болезни пациента и проводить осмотр при поступлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оформлять",
                                "target_lang_sense_clarification": "пациента при поступлении"
                            },
                            {
                                "target_lang_word": "собирать анамнез",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "db168712-df3b-305e-8d40-e4c4f08db26d",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Clerk</w> signed the document.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Клерк</w> подписал документ."
                            }
                        },
                        {
                            "text": "We invited the <w>Clerk</w> family to the gathering.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили семью <w>Клерк</w> на собрание."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Клерк",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
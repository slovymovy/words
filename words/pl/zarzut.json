                {
                    "sense_id": "c31ae4de-66b6-35ad-9ff8-e6aeb4055ac0",
                    "sense_definition": "Słownie sformułowane zastrzeżenie wobec kogoś lub czyjegoś zachowania; krytyczna uwaga.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "criticism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego praca została wykonana <w>bez zarzutu</w> i szef był bardzo zadowolony.",
                            "target_lang_translations": {
                                "ru": "Его работа была выполнена <w>без нареканий</w>, и начальник был очень доволен."
                            }
                        },
                        {
                            "text": "Mam do ciebie mały <w>zarzut</w>, że rzadko do nas dzwonisz.",
                            "target_lang_translations": {
                                "ru": "У меня к тебе маленький <w>упрёк</w>: ты редко нам звонишь."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymówka"
                    ],
                    "antonyms": [
                        "pochwała",
                        "komplement"
                    ],
                    "common_phrases": [
                        "mieć zarzuty",
                        "odeprzeć zarzuty",
                        "bez zarzutu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высказанное вслух недовольство кем-либо или чьим-либо поведением; критическое замечание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упрёк",
                                "target_lang_sense_clarification": "выражение неодобрения"
                            },
                            {
                                "target_lang_word": "претензия",
                                "target_lang_sense_clarification": "жалоба, требование"
                            },
                            {
                                "target_lang_word": "нарекание",
                                "target_lang_sense_clarification": "обычно во мн.ч. или в выражении «без нареканий»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e06c169c-c3e8-36df-af27-7fe0bcdf2c5d",
                    "sense_definition": "Oficjalne oskarżenie o popełnienie przestępstwa lub wykroczenia, będące częścią aktu oskarżenia.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja zatrzymała mężczyznę, któremu prokurator chce postawić <w>zarzuty</w> kradzieży.",
                            "target_lang_translations": {
                                "ru": "Полиция задержала мужчину, которому прокурор хочет предъявить <w>обвинения</w> в краже."
                            }
                        },
                        {
                            "text": "Podejrzany usłyszał <w>zarzuty</w> korupcyjne i został aresztowany na trzy miesiące.",
                            "target_lang_translations": {
                                "ru": "Подозреваемому предъявили <w>обвинения</w> в коррупции, и он был арестован на три месяца."
                            }
                        }
                    ],
                    "synonyms": [
                        "oskarżenie"
                    ],
                    "common_phrases": [
                        "postawić zarzuty",
                        "usłyszeć zarzuty",
                        "oddalić zarzuty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное обвинение в совершении преступления или правонарушения."
                    },

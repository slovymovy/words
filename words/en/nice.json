        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3698486b-490e-3500-a539-bf6d507693aa",
                    "sense_definition": "Pleasant, satisfactory, or enjoyable.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pleasantness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The weather is very <w>nice</w> today.",
                            "target_lang_translations": {
                                "ru": "Сегодня очень <w>хорошая</w> погода."
                            }
                        },
                        {
                            "text": "It was <w>nice</w> to see you again after so long.",
                            "target_lang_translations": {
                                "ru": "Было <w>приятно</w> снова увидеть тебя спустя столько времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "pleasant",
                        "delightful",
                        "lovely"
                    ],
                    "antonyms": [
                        "horrible",
                        "nasty",
                        "awful",
                        "horrid"
                    ],
                    "common_phrases": [
                        "have a nice day",
                        "nice to meet you",
                        "looks nice"
                    ],

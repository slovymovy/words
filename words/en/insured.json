    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "707ab0bd-5ed2-3fc3-8853-45d87fb1bb0b",
                    "sense_definition": "Covered by an insurance policy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>insured</w> vehicle was repaired at no extra cost.",
                            "target_lang_translations": {
                                "ru": "<w>Застрахованный</w> автомобиль был отремонтирован без дополнительных затрат."
                            }
                        },
                        {
                            "text": "It is important to have an <w>insured</w> value for your jewelry.",
                            "target_lang_translations": {
                                "ru": "Важно определить <w>страховую</w> стоимость для ваших ювелирных изделий."
                            }
                        }
                    ],
                    "antonyms": [
                        "uninsured"
                    ],
                    "common_phrases": [
                        "fully insured",
                        "insured value",
                        "insured person"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытый страховым полисом; имеющий страховку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застрахованный",
                                "target_lang_sense_clarification": "покрытый страховкой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8bb17ad7-aaa2-361a-9e6b-87d099364c07",
                    "sense_definition": "Simple past and past participle of <w>insure</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>insured</w> our luggage before the trip.",
                            "target_lang_translations": {
                                "ru": "Мы <w>застраховали</w> наш багаж перед поездкой."
                            }
                        },
                        {
                            "text": "She <w>insured</w> her car against theft and damage.",
                            "target_lang_translations": {
                                "ru": "Она <w>застраховала</w> свою машину от угона и повреждений."
                            }
                        }
                    ],
                    "common_phrases": [
                        "insured against",
                        "insured for"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола «страховать»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застраховал",
                                "target_lang_sense_clarification": "глагол в прошедшем времени"
                            },
                            {
                                "target_lang_word": "застрахованный",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "44f4b84c-1128-355e-886a-adba33a65e2c",
                    "sense_definition": "The person or persons whose interests are protected by an insurance policy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>insured</w> must file the claim within thirty days.",
                            "target_lang_translations": {
                                "ru": "<w>Застрахованное лицо</w> должно подать заявление в течение тридцати дней."
                            }
                        },
                        {
                            "text": "Any damage caused by the <w>insured</w> is covered.",
                            "target_lang_translations": {
                                "ru": "Любой ущерб, причиненный <w>застрахованным</w>, покрывается."
                            }
                        }
                    ],
                    "synonyms": [
                        "insuree",
                        "insurant"
                    ],
                    "common_phrases": [
                        "named insured",
                        "additional insured"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или лица, чьи интересы защищены страховым полисом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застрахованное лицо",
                                "target_lang_sense_clarification": "официальный термин"
                            },
                            {
                                "target_lang_word": "застрахованный",
                                "target_lang_sense_clarification": "субстантивированное прилагательное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

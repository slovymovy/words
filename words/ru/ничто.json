    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "e46d95ce-bf14-318f-9fff-de02e9a3ea3d",
                    "sense_definition": "Отсутствие какого-либо предмета, явления или дела; ни один предмет.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой комнате <w>ничто</w> не напоминало о прежних жильцах.",
                            "target_lang_translations": {
                                "en": "In this room, <w>nothing</w> reminded one of the former tenants."
                            }
                        },
                        {
                            "text": "<w>Ничто</w> не вечно в этом мире.",
                            "target_lang_translations": {
                                "en": "<w>Nothing</w> lasts forever in this world."
                            }
                        }
                    ],
                    "antonyms": [
                        "всё",
                        "что-то",
                        "кое-что"
                    ],
                    "common_phrases": [
                        "ничто не вечно",
                        "ничто иное",
                        "ничто человеческое"
                    ],
                    "target_lang_definitions": {
                        "en": "The absence of any object, phenomenon, or matter; not a single thing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nothing",
                                "target_lang_sense_clarification": "pronoun indicating absence of anything"
                            },
                            {
                                "target_lang_word": "not anything",
                                "target_lang_sense_clarification": "phrase used for negation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97fd4533-f855-374a-9533-1b65ce591e61",
                    "sense_definition": "Предмет или явление, не имеющие никакой ценности или значения; пустяк.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "substantivized",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все твои проблемы — это <w>ничто</w> по сравнению с его горем.",
                            "target_lang_translations": {
                                "en": "All your problems are <w>nothing</w> compared to his grief."
                            }
                        },
                        {
                            "text": "Деньги для него — <w>ничто</w>, главное — это честь.",
                            "target_lang_translations": {
                                "en": "Money is <w>nothing</w> to him; the main thing is honor."
                            }
                        }
                    ],
                    "synonyms": [
                        "ерунда",
                        "пустяк",
                        "мелочь",
                        "безделица"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "indeclinable"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An object or phenomenon having no value or significance; a trifle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nothing",
                                "target_lang_sense_clarification": "something of no importance"
                            },
                            {
                                "target_lang_word": "trifle",
                                "target_lang_sense_clarification": "a thing of little value or importance"
                            },
                            {
                                "target_lang_word": "triviality",
                                "target_lang_sense_clarification": "insignificant detail"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b39b73f5-0005-33a2-9616-c0825725cdb7",
                    "sense_definition": "Человек, не имеющий никаких достоинств или значимости.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "substantivized",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он возомнил себя великим начальником, а на самом деле он полное <w>ничто</w>.",
                            "target_lang_translations": {
                                "en": "He fancied himself a great boss, but in reality he is a complete <w>nobody</w>."
                            }
                        },
                        {
                            "text": "Без своей должности и денег этот человек просто <w>ничто</w>.",
                            "target_lang_translations": {
                                "en": "Without his position and money, this person is just <w>nothing</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ничтожество",
                        "ноль",
                        "пустое место"
                    ],
                    "common_phrases": [
                        "полное ничто"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "indeclinable"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person having no merits or significance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nobody",
                                "target_lang_sense_clarification": "an insignificant person"
                            },
                            {
                                "target_lang_word": "nonentity",
                                "target_lang_sense_clarification": "a person of no importance"
                            },
                            {
                                "target_lang_word": "nothing",
                                "target_lang_sense_clarification": "a person of no consequence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8536ff14-452e-33d3-a318-3f91e0c917c6",
                    "sense_definition": "Полное отсутствие бытия, пустота, небытие.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "substantivized",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Согласно этой теории, Вселенная возникла из абсолютного <w>ничто</w>.",
                            "target_lang_translations": {
                                "en": "According to this theory, the Universe emerged from absolute <w>nothingness</w>."
                            }
                        },
                        {
                            "text": "Его сознание медленно погружалось в темное и холодное <w>ничто</w>.",
                            "target_lang_translations": {
                                "en": "His consciousness slowly sank into a dark and cold <w>void</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "небытие",
                        "пустота",
                        "вакуум"
                    ],
                    "antonyms": [
                        "всё"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "indeclinable"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Complete absence of being, emptiness, non-existence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nothingness",
                                "target_lang_sense_clarification": "the state of non-existence"
                            },
                            {
                                "target_lang_word": "void",
                                "target_lang_sense_clarification": "complete emptiness"
                            },
                            {
                                "target_lang_word": "nothing",
                                "target_lang_sense_clarification": "absolute absence of being"
                            }
                        ]
                    }
                }
            ]
        }

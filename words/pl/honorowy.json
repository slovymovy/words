                {
                    "sense_id": "5bffe576-b6db-3e2c-a317-36408049ec28",
                    "sense_definition": "Mający poczucie godności osobistej, postępujący szlachetnie i dotrzymujący słowa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek był bardzo <w>honorowym</w> człowiekiem i zawsze dotrzymywał złożonych obietnic.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был очень <w>благородным</w> человеком и всегда сдерживал данные обещания."
                            }
                        },
                        {
                            "text": "To bardzo <w>honorowe</w> podejście do sprawy, które budzi mój szczery podziw.",
                            "target_lang_translations": {
                                "ru": "Это очень <w>благородный</w> подход к делу, который вызывает моё искреннее восхищение."
                            }
                        }
                    ],
                    "synonyms": [
                        "szlachetny",
                        "godny",
                        "rycerski"
                    ],
                    "antonyms": [
                        "niehonorowy",
                        "nikczemny",
                        "haniebny"
                    ],
                    "common_phrases": [
                        "człowiek honorowy",
                        "być honorowym",
                        "zachowanie honorowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий чувством собственного достоинства, поступающий благородно и держащий слово."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "благородный",
                                "target_lang_sense_clarification": "о характере или поступках"
                            },
                            {
                                "target_lang_word": "честный",
                                "target_lang_sense_clarification": "порядочный, добросовестный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02d04242-28b2-3146-ba4c-7dc5b2edb5ce",
                    "sense_definition": "Nadawany jako wyróżnienie za zasługi, często bez wiążących się z tym obowiązków lub wynagrodzenia.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "title",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany pisarz został <w>honorowym</w> obywatelem naszego miasta za promowanie kultury.",
                            "target_lang_translations": {
                                "ru": "Известный писатель стал <w>почётным</w> гражданином нашего города за продвижение культуры."
                            }
                        },
                        {
                            "text": "Prezydent objął patronat <w>honorowy</w> nad tym ważnym wydarzeniem sportowym.",
                            "target_lang_translations": {
                                "ru": "Президент взял <w>почётный</w> патронат над этим важным спортивным событием."
                            }
                        }
                    ],
                    "synonyms": [
                        "tytularny"
                    ],
                    "common_phrases": [
                        "honorowy obywatel",
                        "członek honorowy",
                        "patronat honorowy",
                        "prezes honorowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Присваиваемый в знак признания заслуг, часто без связанных с этим обязанностей или вознаграждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почётный",
                                "target_lang_sense_clarification": "о звании или титуле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb421ca3-4d69-33ad-81d3-3e0ff72fc29f",
                    "sense_definition": "Wynikający z chęci zachowania dobrego imienia lub zasad, często o charakterze symbolicznym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "symbolic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza drużyna przegrała mecz, ale w ostatniej minucie strzeliła bramkę <w>honorową</w>.",
                            "target_lang_translations": {
                                "ru": "Наша команда проиграла матч, но на последней минуте забила гол <w>престижа</w>."
                            }
                        },
                        {
                            "text": "Żołnierze oddali salwę <w>honorową</w> podczas uroczystości pogrzebowych generała.",
                            "target_lang_translations": {
                                "ru": "Солдаты дали <w>почётный</w> залп во время похоронной церемонии генерала."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bramka honorowa",
                        "runda honorowa",
                        "salwa honorowa",
                        "rozwiązanie honorowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с желанием сохранить доброе имя или принципы, часто носящий символический характер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почётный",
                                "target_lang_sense_clarification": "символический (круг почёта, караул)"
                            },
                            {
                                "target_lang_word": "престижа",
                                "target_lang_sense_clarification": "в спорте (гол престижа)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a76767e5-e2cd-3ea0-8af6-b351aad283b3",
                    "sense_definition": "Niedający zysków materialnych lub niepobierający zapłaty za swoje działanie; wykonywany społecznie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "voluntary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <w>honorowy</w> dawca krwi otrzymuje posiłek regeneracyjny i dzień wolny od pracy.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>почётный</w> донор крови получает восстановительное питание и выходной день."
                            }
                        },
                        {
                            "text": "Mój brat jest <w>honorowym</w> krwiodawcą i regularnie pomaga potrzebującym w szpitalu.",
                            "target_lang_translations": {
                                "ru": "Мой брат — <w>почётный</w> донор, он регулярно помогает нуждающимся в больнице."
                            }
                        }
                    ],
                    "common_phrases": [
                        "honorowy dawca krwi",
                        "honorowe krwiodawstwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не приносящий материальной выгоды или не предусматривающий оплаты; выполняемый на общественных началах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почётный",
                                "target_lang_sense_clarification": "в устойчивых сочетаниях (почётный донор)"
                            },
                            {
                                "target_lang_word": "безвозмездный",
                                "target_lang_sense_clarification": "бесплатный, добровольный"
                            }
                        ]
                    }
                }
            ]
        }

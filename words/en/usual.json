            "senses": [
                {
                    "sense_id": "698683be-301f-3f94-9b6b-6841643acb93",
                    "sense_definition": "Most commonly occurring; typical.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He sat in his <w>usual</w> chair by the window.",
                            "target_lang_translations": {
                                "ru": "Он сел на свой <w>обычный</w> стул у окна."
                            }
                        },
                        {
                            "text": "It is <w>usual</w> to shake hands when you meet someone new.",
                            "target_lang_translations": {
                                "ru": "При знакомстве <w>принято</w> пожимать руки."
                            }
                        }
                    ],
                    "synonyms": [
                        "typical",
                        "common",
                        "normal",
                        "regular",
                        "standard",
                        "ordinary"
                    ],
                    "antonyms": [
                        "unusual",
                        "atypical"
                    ],
                    "common_phrases": [
                        "as usual",
                        "usual way",
                        "usual time",
                        "usual place"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наиболее часто встречающийся; типичный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обычный",
                                "target_lang_sense_clarification": "наиболее распространенный, стандартный"
                            },
                            {
                                "target_lang_word": "привычный",
                                "target_lang_sense_clarification": "ставший привычкой, знакомый"
                            },
                            {
                                "target_lang_word": "обыкновенный",
                                "target_lang_sense_clarification": "ничем не выделяющийся, простой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e5992bf-b8f0-38d4-9535-de45a98fb63c",
                    "sense_definition": "A specific good or service (e.g. a drink) that someone typically orders.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The waiter brought me the <w>usual</w> without asking.",
                            "target_lang_translations": {
                                "ru": "Официант принёс мне <w>обычное</w>, не спрашивая."
                            }
                        },
                        {
                            "text": "Do you want the <w>usual</w> or something different today?",
                            "target_lang_translations": {
                                "ru": "Ты хочешь <w>обычное</w> или что-то другое сегодня?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "the usual"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Used to refer to a regular order."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный товар или услуга (например, напиток), которые кто-то обычно заказывает."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обычное",
                                "target_lang_sense_clarification": "субстантивированное прилагательное, означающее привычный заказ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c02d915e-a214-3063-883b-43ce5f3cc639",
                    "sense_definition": "The typical state of something, or something that is typical.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the fire drill, it was business as <w>usual</w> in the office.",
                            "target_lang_translations": {
                                "ru": "После учебной пожарной тревоги работа в офисе пошла своим <w>обычным</w> чередом."
                            }
                        },
                        {
                            "text": "The quiet atmosphere is the <w>usual</w> here on Sundays.",
                            "target_lang_translations": {
                                "ru": "Тихая атмосфера здесь — <w>обычное</w> явление по воскресеньям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "business as usual",
                        "per usual"
                    ],
                    "target_lang_definitions": {
                        "ru": "Типичное состояние чего-либо; то, что является нормой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обычное дело",
                                "target_lang_sense_clarification": "разговорное выражение для обозначения нормальной ситуации"
                            },
                            {
                                "target_lang_word": "норма",
                                "target_lang_sense_clarification": "общепринятый стандарт"
                            }
                        ]
                    }
                }
            ]
        }
    ]

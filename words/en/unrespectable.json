            "senses": [
                {
                    "sense_id": "a8d1ad86-373e-32a6-bd84-e20604c14a77",
                    "sense_definition": "A person who is considered to lack social standing or good reputation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The novel focuses on the lives of the <w>unrespectables</w> struggling to survive in the city slums.",
                            "target_lang_translations": {
                                "ru": "Роман повествует о жизни <w>людей с дурной репутацией</w>, пытающихся выжить в городских трущобах."
                            }
                        },
                        {
                            "text": "Victorian society often ignored the plight of the so-called <w>unrespectables</w> living on the margins.",
                            "target_lang_translations": {
                                "ru": "Викторианское общество часто игнорировало бедственное положение так называемых <w>нереспектабельных личностей</w>, живущих на обочине жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "outcast",
                        "pariah"
                    ],
                    "target_lang_definitions": {

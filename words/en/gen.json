        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3b8452f3-3e72-37e2-8ba0-795004250e15",
                    "sense_definition": "To create or produce something using an automated process, especially in computing; short for <w>generate</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system <w>genned</w> a new configuration file after the update.",
                            "target_lang_translations": {
                                "ru": "Система <w>сгенерировала</w> новый файл конфигурации после обновления."
                            }
                        },
                        {
                            "text": "We need to wait while the report is being <w>genned</w> by the server.",
                            "target_lang_translations": {
                                "ru": "Нам нужно подождать, пока отчет <w>генерируется</w> сервером."

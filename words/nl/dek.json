                {
                    "sense_id": "f6fe8a15-346a-397a-8518-a927e9ffe9a7",
                    "sense_definition": "een vloer in de openlucht op een schip, of een verdieping binnenin een schip",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De passagiers stonden op het <w>dek</w> naar de haven te zwaaien.",
                            "target_lang_translations": {
                                "en": "The passengers stood on the <w>deck</w> waving at the harbor.",
                                "ru": "Пассажиры стояли на <w>палубе</w> и махали в сторону гавани."
                            }
                        },
                        {
                            "text": "Tijdens de storm mocht niemand zich op het bovenste <w>dek</w> begeven.",
                            "target_lang_translations": {
                                "en": "During the storm, no one was allowed on the upper <w>deck</w>.",
                                "ru": "Во время шторма никому не разрешалось выходить на верхнюю <w>палубу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheepsdek"
                    ],
                    "common_phrases": [
                        "aan dek",
                        "op het dek",
                        "het dek schrobben"
                    ],
                    "target_lang_definitions": {
                        "en": "a floor in the open air on a ship, or a level inside a ship",
                        "ru": "Пол на открытом воздухе на корабле или этаж внутри корабля"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deck",
                                "target_lang_sense_clarification": "of a ship"
                            }

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "35dd2c23-0a99-38f5-b52f-6b1c8defebb6",
                    "sense_definition": "To what degree, quantity, or extent.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree_extent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>How</w> often do you practice the piano?",
                            "target_lang_translations": {
                                "ru": "<w>Как</w> часто ты занимаешься на пианино?"
                            }
                        },
                        {
                            "text": "I wonder <w>how</w> soon we can leave for the airport.",
                            "target_lang_translations": {
                                "ru": "Интересно, <w>как</w> скоро мы сможем выехать в аэропорт."
                            }
                        },
                        {
                            "text": "<w>How</w> much does this jacket cost?",
                            "target_lang_translations": {
                                "ru": "<w>Сколько</w> стоит эта куртка?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "how much",
                        "how many",
                        "how often",
                        "how long",
                        "how far"
                    ],
                    "target_lang_definitions": {

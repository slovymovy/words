    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f76822d-dcd5-3125-b0da-3a78a881eb5e",
                    "sense_definition": "The total amount of money, time, or other resources that is spent.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "spending_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The total <w>expenditure</w> for the new building exceeded the initial budget.",
                            "target_lang_translations": {
                                "ru": "Общие <w>расходы</w> на новое здание превысили первоначальный бюджет."
                            }
                        },
                        {
                            "text": "We need to reduce our annual <w>expenditure</w> to ensure the company stays profitable.",
                            "target_lang_translations": {
                                "ru": "Нам нужно сократить наши ежегодные <w>расходы</w>, чтобы компания оставалась прибыльной."
                            }
                        }
                    ],
                    "synonyms": [
                        "expense",
                        "cost",
                        "outlay",
                        "charge"
                    ],
                    "antonyms": [
                        "income",
                        "revenue",
                        "profit"
                    ],
                    "common_phrases": [
                        "total expenditure",
                        "annual expenditure",
                        "government expenditure",
                        "capital expenditure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общая сумма денег, времени или других потраченных ресурсов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расходы",
                                "target_lang_sense_clarification": "денежные траты или издержки"
                            },
                            {
                                "target_lang_word": "затраты",
                                "target_lang_sense_clarification": "стоимость ресурсов, необходимых для чего-либо"
                            },
                            {
                                "target_lang_word": "издержки",
                                "target_lang_sense_clarification": "финансовые потери или выплаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1b847fb-0ffe-328d-ae30-d3e1f329f22e",
                    "sense_definition": "The act of spending or using up resources such as money, time, or energy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "spending_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>expenditure</w> of effort required to complete this task is immense.",
                            "target_lang_translations": {
                                "ru": "<w>Затраты</w> усилий, необходимых для выполнения этой задачи, огромны."
                            }
                        },
                        {
                            "text": "Proper <w>expenditure</w> of public funds is the main responsibility of the treasurer.",
                            "target_lang_translations": {
                                "ru": "Правильное <w>расходование</w> государственных средств является главной обязанностью казначея."
                            }
                        }
                    ],
                    "synonyms": [
                        "payment",
                        "disbursement"
                    ],
                    "antonyms": [
                        "saving",
                        "accumulation",
                        "hoarding"
                    ],
                    "common_phrases": [
                        "expenditure of energy",
                        "expenditure of time",
                        "expenditure of effort"
                    ],
                    "target_lang_definitions": {

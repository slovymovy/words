    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5a0b5a7e-a289-3841-a415-07209aa720c8",
                    "sense_definition": "Een nachtelijke dienst waarbij men toezicht houdt, vaak ervaren als zwaar of vervelend.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "night_shift",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De agenten hielden de hele nacht de <w>hondenwacht</w> in hun koude auto.",
                            "target_lang_translations": {
                                "ru": "Полицейские всю ночь несли <w>ночное дежурство</w> в своей холодной машине."
                            }
                        },
                        {
                            "text": "Tijdens het kamp moesten de leiders om de beurt de <w>hondenwacht</w> houden.",
                            "target_lang_translations": {
                                "ru": "Во время лагеря вожатые должны были по очереди нести <w>ночную вахту</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nachtdienst"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ночная смена, часто воспринимаемая как тяжелая или неприятная."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ночное дежурство",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "ночная вахта",
                                "target_lang_sense_clarification": "дежурство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1056113-3f23-339c-9e3e-6cba4a58b33f",
                    "sense_definition": "Een functionaris die controleert of hondenbezitters zich aan de regels houden.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>hondenwacht</w> gaf de man een boete omdat zijn hond losliep.",
                            "target_lang_translations": {
                                "ru": "<w>Инспектор по контролю за собаками</w> оштрафовал мужчину, потому что его собака гуляла без поводка."
                            }
                        },
                        {
                            "text": "In deze gemeente controleert de <w>hondenwacht</w> streng op het betalen van hondenbelasting.",
                            "target_lang_translations": {
                                "ru": "В этом муниципалитете <w>служба контроля за собаками</w> строго проверяет уплату налога на собак."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Должностное лицо, контролирующее соблюдение правил владельцами собак."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инспектор по контролю за собаками",
                                "target_lang_sense_clarification": "должность"
                            },
                            {
                                "target_lang_word": "собачий патруль",
                                "target_lang_sense_clarification": "служба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69d9cea0-e71d-39bb-95d6-bbd0103eef60",
                    "sense_definition": "De wachtdienst aan boord van een schip van middernacht tot vier uur 's ochtends.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "night_shift",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stuurman vroeg wie vannacht de <w>hondenwacht</w> wilde lopen.",
                            "target_lang_translations": {
                                "ru": "Штурман спросил, кто хочет сегодня ночью стоять <w>собачью вахту</w>."
                            }
                        },
                        {
                            "text": "Het is vaak erg stil en donker op zee tijdens de <w>hondenwacht</w>.",
                            "target_lang_translations": {
                                "ru": "Во время <w>собачьей вахты</w> на море часто очень тихо и темно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de hondenwacht lopen",
                        "de hondenwacht hebben"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вахта на корабле с полуночи до четырех часов утра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собачья вахта",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
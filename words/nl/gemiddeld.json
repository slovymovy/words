    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c9d1b0e5-dd04-3484-95f4-654d46d454a9",
                    "sense_definition": "Bepaald door de som van een reeks getallen te delen door het aantal getallen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>gemiddelde</w> temperatuur was in juli hoger dan normaal.",
                            "target_lang_translations": {
                                "en": "The <w>average</w> temperature in July was higher than normal.",
                                "ru": "<w>Средняя</w> температура в июле была выше нормы."
                            }
                        },
                        {
                            "text": "Met een <w>gemiddelde</w> snelheid van honderd kilometer per uur kwamen we op tijd aan.",
                            "target_lang_translations": {
                                "en": "We arrived on time with an <w>average</w> speed of one hundred kilometers per hour.",
                                "ru": "Со <w>средней</w> скоростью сто километров в час мы прибыли вовремя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gemiddelde snelheid",
                        "gemiddelde leeftijd",
                        "gemiddelde temperatuur"
                    ],
                    "target_lang_definitions": {
                        "en": "Calculated by dividing the sum of a set of numbers by the count of numbers.",
                        "ru": "Определяемый путем деления суммы ряда чисел на их количество."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "average",
                                "target_lang_sense_clarification": "mathematical value"
                            },
                            {

{
    "word_family": [
        "pochłaniacz",
        "pochłonięcie",
        "chłonąć",
        "chłonny",
        "chłonność"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "80ad5aa9-e874-3fb0-8107-a4cb4c4b680a",
                    "sense_definition": "Zużywać duże ilości zasobów, takich jak czas, energia lub pieniądze.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "consumption_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Remont starego domu <w>pochłania</w> wszystkie nasze oszczędności.",
                            "target_lang_translations": {
                                "ru": "Ремонт старого дома <w>поглощает</w> все наши сбережения."
                            }
                        },
                        {
                            "text": "Ta nowa praca <w>pochłania</w> mi mnóstwo czasu i energii.",
                            "target_lang_translations": {
                                "ru": "Эта новая работа <w>отнимает</w> у меня кучу времени и сил."
                            }
                        }
                    ],
                    "synonyms": [
                        "pożerać"
                    ],
                    "common_phrases": [
                        "pochłaniać mnóstwo czasu",
                        "pochłaniać energię",
                        "pochłaniać majątek",
                        "pochłaniać budżet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расходовать большое количество ресурсов, таких как время, энергия или деньги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "о ресурсах, деньгах"
                            },
                            {
                                "target_lang_word": "отнимать",
                                "target_lang_sense_clarification": "о времени, силах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cc54d2b-91b5-302b-bbf4-788b2a27f076",
                    "sense_definition": "Zajmować czyjąś uwagę lub czas całkowicie; angażować kogoś bez reszty.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "engagement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowe hobby całkowicie go <w>pochłania</w>, więc rzadko do nas dzwoni.",
                            "target_lang_translations": {
                                "ru": "Новое хобби полностью его <w>поглощает</w>, поэтому он редко нам звонит."
                            }
                        },
                        {
                            "text": "Przygotowania do ślubu <w>pochłaniają</w> jej każdą wolną chwilę.",
                            "target_lang_translations": {
                                "ru": "Подготовка к свадьбе <w>поглощает</w> каждую её свободную минуту."
                            }
                        }
                    ],
                    "synonyms": [
                        "absorbować",
                        "chłonąć"
                    ],
                    "common_phrases": [
                        "pochłaniać uwagę",
                        "pochłaniać bez reszty",
                        "praca go pochłania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью занимать чье-то внимание или время; увлекать кого-то без остатка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "занимать целиком"
                            },
                            {
                                "target_lang_word": "увлекать",
                                "target_lang_sense_clarification": "вызывать сильный интерес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "346eae29-5036-387f-836b-c005188c2c54",
                    "sense_definition": "Wchłaniać coś do swojego wnętrza; sprawiać, że coś znika wewnątrz czegoś innego (często w kontekście fizycznym).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_absorption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czarne dziury w kosmosie <w>pochłaniają</w> wszystko, co znajdzie się w ich pobliżu.",
                            "target_lang_translations": {
                                "ru": "Черные дыры в космосе <w>поглощают</w> всё, что оказывается поблизости."
                            }
                        },
                        {
                            "text": "Gąbka szybko <w>pochłania</w> rozlaną na stole wodę.",
                            "target_lang_translations": {
                                "ru": "Губка быстро <w>впитывает</w> разлитую на столе воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "wchłaniać",
                        "chłonąć",
                        "absorbować"
                    ],
                    "common_phrases": [
                        "pochłaniać wilgoć",
                        "pochłaniać światło",
                        "pochłaniać promieniowanie",
                        "pochłaniać ciepło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вбирать что-то внутрь себя; заставлять исчезнуть внутри чего-то другого (обычно физически)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "впитывать",
                                "target_lang_sense_clarification": "о жидкости, влаге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb0a3de7-6dd0-3880-a1f0-0b72a9ecc1a0",
                    "sense_definition": "Czytać książki lub przyswajać wiedzę w bardzo szybkim tempie i z dużym zainteresowaniem.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "intellectual_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako dziecko <w>pochłaniałem</w> jedną książkę przygodową za drugą.",
                            "target_lang_translations": {
                                "ru": "В детстве я <w>глотал</w> одну приключенческую книгу за другой."
                            }
                        },
                        {
                            "text": "Studenci przed sesją egzaminacyjną <w>pochłaniają</w> ogromne ilości wiedzy.",
                            "target_lang_translations": {
                                "ru": "Студенты перед сессией <w>поглощают</w> огромные объемы знаний."
                            }
                        }
                    ],
                    "synonyms": [
                        "chłonąć"
                    ],
                    "common_phrases": [
                        "pochłaniać książki",
                        "pochłaniać wiedzę",
                        "pochłaniać lektury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Читать книги или усваивать знания в очень быстром темпе и с большим интересом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глотать",
                                "target_lang_sense_clarification": "о книгах (разг.)"
                            },
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "о знаниях, информации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1078f1d0-d4ee-3acb-87f4-d5fa26a0c2d8",
                    "sense_definition": "Jeść lub pić coś bardzo szybko, łapczywie i w dużych ilościach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "eating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głodni turyści <w>pochłaniali</w> gorącą pizzę w mgnieniu oka.",
                            "target_lang_translations": {
                                "ru": "Голодные туристы <w>уплетали</w> горячую пиццу в мгновение ока."
                            }
                        },
                        {
                            "text": "Nie powinieneś tak szybko <w>pochłaniać</w> obiadu, bo rozboli cię brzuch.",
                            "target_lang_translations": {
                                "ru": "Тебе не стоит так быстро <w>поглощать</w> обед, иначе разболится живот."
                            }
                        }
                    ],
                    "synonyms": [
                        "pożerać",
                        "wtranżalać",
                        "wtrząchać",
                        "wrąbywać",
                        "wtrajać"
                    ],
                    "common_phrases": [
                        "pochłaniać jedzenie",
                        "pochłaniać słodycze",
                        "szybko pochłaniać"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in expressive contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Есть или пить что-то очень быстро, жадно и в больших количествах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "есть в больших количествах"
                            },
                            {
                                "target_lang_word": "уплетать",
                                "target_lang_sense_clarification": "есть с аппетитом (разг.)"
                            },
                            {
                                "target_lang_word": "пожирать",
                                "target_lang_sense_clarification": "есть жадно, грубо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac46188e-f923-36c6-9bf1-0e887b8d2584",
                    "sense_definition": "Powodować zniszczenie lub śmierć wielu osób (używane w kontekście żywiołów, wojen lub katastrof).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szalejący pożar lasu <w>pochłaniał</w> kolejne hektary drzew.",
                            "target_lang_translations": {
                                "ru": "Бушующий лесной пожар <w>поглощал</w> гектар за гектаром деревья."
                            }
                        },
                        {
                            "text": "Druga wojna światowa <w>pochłonęła</w> miliony istnień ludzkich.",
                            "target_lang_translations": {
                                "ru": "Вторая мировая война <w>унесла</w> миллионы человеческих жизней."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pochłaniać ofiary",
                        "ogień pochłania",
                        "wojna pochłania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить к разрушениям или гибели многих людей (о стихиях, войнах или катастрофах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уносить",
                                "target_lang_sense_clarification": "о жизнях"
                            },
                            {
                                "target_lang_word": "уничтожать",
                                "target_lang_sense_clarification": "разрушать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
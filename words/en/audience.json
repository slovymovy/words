{
    "word_family": [
        "audio",
        "audible",
        "auditory",
        "audition",
        "auditorium",
        "auditor"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b83adf76-19b6-358d-96d1-8561a49d3e50",
                    "sense_definition": "A group of people gathered together to watch or listen to a performance, speech, or public event.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "people_listening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>audience</w> clapped loudly at the end of the play.",
                            "target_lang_translations": {
                                "ru": "<w>Зрители</w> громко аплодировали в конце пьесы."
                            }
                        },
                        {
                            "text": "We joined the live <w>audience</w> just before the show started.",
                            "target_lang_translations": {
                                "ru": "Мы присоединились к <w>зрителям</w> в зале прямо перед началом шоу."
                            }
                        }
                    ],
                    "synonyms": [
                        "spectators",
                        "crowd"
                    ],
                    "common_phrases": [
                        "live audience",
                        "audience member",
                        "captive audience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, собравшихся вместе, чтобы посмотреть или послушать представление, речь или публичное мероприятие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публика",
                                "target_lang_sense_clarification": "люди на мероприятии, общество"
                            },
                            {
                                "target_lang_word": "зрители",
                                "target_lang_sense_clarification": "те, кто смотрит представление"
                            },
                            {
                                "target_lang_word": "аудитория",
                                "target_lang_sense_clarification": "слушатели в зале"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76ccbebf-2f99-304e-9ca0-7422e69575d1",
                    "sense_definition": "The people who watch, read, or listen to a particular television programme, radio show, book, or artist.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "people_listening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The television show attracts a huge <w>audience</w> every week.",
                            "target_lang_translations": {
                                "ru": "Телешоу собирает огромную <w>аудиторию</w> каждую неделю."
                            }
                        },
                        {
                            "text": "The author writes books specifically for a younger <w>audience</w>.",
                            "target_lang_translations": {
                                "ru": "Автор пишет книги специально для более молодой <w>аудитории</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "listenership",
                        "hearership"
                    ],
                    "common_phrases": [
                        "target audience",
                        "wider audience",
                        "reach an audience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди, которые смотрят, читают или слушают определенную телепрограмму, радиопередачу, книгу или артиста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аудитория",
                                "target_lang_sense_clarification": "целевая группа, охват"
                            },
                            {
                                "target_lang_word": "зрители",
                                "target_lang_sense_clarification": "телезрители или кинозрители"
                            },
                            {
                                "target_lang_word": "читатели",
                                "target_lang_sense_clarification": "аудитория книг или изданий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2488632c-c695-3ec6-8d1e-3eafc5ada04f",
                    "sense_definition": "A formal meeting with a very important person, such as a monarch or religious leader.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ambassador requested a private <w>audience</w> with the Queen.",
                            "target_lang_translations": {
                                "ru": "Посол попросил о частной <w>аудиенции</w> у королевы."
                            }
                        },
                        {
                            "text": "She was lucky enough to grant an <w>audience</w> to the visiting diplomat.",
                            "target_lang_translations": {
                                "ru": "Ей посчастливилось дать <w>аудиенцию</w> приезжему дипломату."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grant an audience",
                        "seek an audience",
                        "private audience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная встреча с очень важной персоной, например, монархом или религиозным лидером."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аудиенция",
                                "target_lang_sense_clarification": "официальный прием у высокопоставленного лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12ac84de-2134-3151-bdbd-65d0f0e88a87",
                    "sense_definition": "The act or state of hearing or listening to someone.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He proclaimed the important news in the <w>audience</w> of the entire town.",
                            "target_lang_translations": {
                                "ru": "Он провозгласил важную новость в <w>присутствии</w> всего города."
                            }
                        },
                        {
                            "text": "Her words found no <w>audience</w> among the angry crowd.",
                            "target_lang_translations": {
                                "ru": "Ее слова не нашли <w>слушателей</w> в разъяренной толпе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly found in older literature"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или состояние слышания или слушания кого-либо (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слушание",
                                "target_lang_sense_clarification": "процесс восприятия речи"
                            },
                            {
                                "target_lang_word": "внимание",
                                "target_lang_sense_clarification": "готовность выслушать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
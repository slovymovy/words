                    "sense_id": "146ae2e8-fd1d-33f2-adff-ea2ec3d43b90",
                    "sense_definition": "Duża odległość w przestrzeni lub czasie; stan bycia daleko od kogoś lub czegoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz dom znajduje się w znacznym <w>oddaleniu</w> od głównej drogi.",
                            "target_lang_translations": {
                                "ru": "Наш дом находится в значительном <w>отдалении</w> от главной дороги."
                            }
                        },
                        {
                            "text": "Obserwowaliśmy nadchodzącą burzę z bezpiecznego <w>oddalenia</w>.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали за приближающейся грозой с безопасного <w>расстояния</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "odległość",
                        "dystans",
                        "dalekość"
                    ],
                    "common_phrases": [
                        "w oddaleniu",
                        "znaczne oddalenie",
                        "z oddalenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое расстояние в пространстве или времени; состояние нахождения далеко от кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдаление",
                                "target_lang_sense_clarification": "состояние нахождения вдали"

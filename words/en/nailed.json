            "senses": [
                {
                    "sense_id": "7495e028-ae8a-320a-8c34-4784f21f566b",
                    "sense_definition": "Having nails, often of a specific type or color.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The witch pointed a long-<nailed> finger at the frightened children.",
                            "target_lang_translations": {
                                "ru": "Ведьма указала пальцем с длинным <ногтем> на испуганных детей."
                            }
                        },
                        {
                            "text": "He looked at his dirty-<nailed> hands after working in the garden all day.",
                            "target_lang_translations": {
                                "ru": "Он посмотрел на свои руки с грязными <ногтями> после работы в саду весь день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "long-nailed",
                        "red-nailed",
                        "sharp-nailed"
                    ],
                    "target_lang_definitions": {

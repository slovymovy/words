                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "business/technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность ресурсов информационных технологий и инфраструктуры, принадлежащих организации."

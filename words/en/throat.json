{
    "word_family": [
        "throaty",
        "cutthroat",
        "throttle"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dfaa682d-e4e2-384b-a209-a53501aade54",
                    "sense_definition": "The passage inside the neck that leads from the back of the mouth down to the stomach and lungs.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_internal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have a sore <w>throat</w> and cannot swallow easily.",
                            "target_lang_translations": {
                                "ru": "У меня болит <w>горло</w>, и мне трудно глотать."
                            }
                        },
                        {
                            "text": "He cleared his <w>throat</w> loudly to get our attention.",
                            "target_lang_translations": {
                                "ru": "Он громко прочистил <w>горло</w>, чтобы привлечь наше внимание."
                            }
                        }
                    ],
                    "synonyms": [
                        "gullet",
                        "esophagus",
                        "windpipe",
                        "trachea"
                    ],
                    "common_phrases": [
                        "sore throat",
                        "clear one's throat",
                        "lump in one's throat",
                        "frog in one's throat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проход внутри шеи, ведущий от задней части рта к желудку и легким."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "го́рло",
                                "target_lang_sense_clarification": "часть тела"
                            },
                            {
                                "target_lang_word": "гло́тка",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6118ffdb-b028-3a34-bb71-27560e330d77",
                    "sense_definition": "The front outer part of the neck.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_external",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a beautiful silver necklace around her <w>throat</w>.",
                            "target_lang_translations": {
                                "ru": "На <w>шее</w> у неё было красивое серебряное ожерелье."
                            }
                        },
                        {
                            "text": "The animal had white fur on its <w>throat</w> and chest.",
                            "target_lang_translations": {
                                "ru": "У животного был белый мех на <w>горле</w> и груди."
                            }
                        }
                    ],
                    "synonyms": [
                        "neck"
                    ],
                    "common_phrases": [
                        "grab by the throat",
                        "slit someone's throat",
                        "cut-throat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя внешняя часть шеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "го́рло",
                                "target_lang_sense_clarification": "передняя часть шеи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8317faef-1ba9-3cce-b7a7-b71d844f709a",
                    "sense_definition": "A narrow opening or passage in a vessel or object, such as a bottle or chimney.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wine poured slowly from the narrow <w>throat</w> of the bottle.",
                            "target_lang_translations": {
                                "ru": "Вино медленно лилось из узкого <w>горлышка</w> бутылки."
                            }
                        },
                        {
                            "text": "Smoke rose up through the <w>throat</w> of the chimney.",
                            "target_lang_translations": {
                                "ru": "Дым поднимался через <w>горловину</w> дымохода."
                            }
                        }
                    ],
                    "synonyms": [
                        "neck",
                        "bottleneck"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкое отверстие или проход в сосуде или объекте, например, в бутылке или дымоходе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "го́рлышко",
                                "target_lang_sense_clarification": "бутылки"
                            },
                            {
                                "target_lang_word": "горлови́на",
                                "target_lang_sense_clarification": "техническое отверстие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a64a8544-09a3-3af4-9d12-99752a885e7a",
                    "sense_definition": "To make a sound, speak, or sing using the throat.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bird <w>throated</w> a cheerful song from the tree branch.",
                            "target_lang_translations": {
                                "ru": "Птица <w>пропела</w> веселую песню с ветки дерева."
                            }
                        },
                        {
                            "text": "He <w>throated</w> a low warning to the others before disappearing into the dark.",
                            "target_lang_translations": {
                                "ru": "Он <w>прохрипел</w> тихое предупреждение остальным, прежде чем исчезнуть в темноте."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly literary or archaic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать звук, говорить или петь, используя горло."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "17eefcd1-2a14-3653-91ae-942cb9d546ef",
                    "sense_definition": "Adwokat, radca prawny lub inna osoba wykonująca zawód prawniczy; często używane jako tytuł grzecznościowy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <mecenas> doradził mi, abym nie podpisywał tej niekorzystnej umowy.",
                            "target_lang_translations": {
                                "ru": "Мой <адвокат> посоветовал мне не подписывать этот невыгодный договор."
                            }
                        },
                        {
                            "text": "W sądzie reprezentował go doświadczony <mecenas>, który wygrał wiele trudnych spraw.",
                            "target_lang_translations": {
                                "ru": "В суде его представлял опытный <адвокат>, который выиграл много сложных дел."
                            }
                        }
                    ],
                    "synonyms": [
                        "adwokat",
                        "prawnik"
                    ],
                    "common_phrases": [
                        "pan mecenas",
                        "pani mecenas",
                        "znany mecenas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Адвокат, юрисконсульт или другое лицо, занимающееся юридической практикой; часто используется как вежливое обращение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "адвокат",
                                "target_lang_sense_clarification": "юрист, оказывающий профессиональную правовую помощь"
                            },
                            {
                                "target_lang_word": "юрист",
                                "target_lang_sense_clarification": "специалист в области права"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7707b93-32f3-32f8-9166-ebfc6752bf7d",
                    "sense_definition": "Osoba lub instytucja, która finansuje artystów, naukowców lub działalność kulturalną.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stanisław August Poniatowski był wielkim <mecenasem> kultury i wspierał wielu malarzy.",
                            "target_lang_translations": {
                                "ru": "Станислав Август Понятовский был великим <меценатом> культуры и поддерживал многих художников."
                            }
                        },
                        {
                            "text": "Dzięki prywatnym <mecenasom> muzeum mogło zakupić nowe dzieła sztuki do swojej kolekcji.",
                            "target_lang_translations": {
                                "ru": "Благодаря частным <меценатам> музей смог приобрести новые произведения искусства для своей коллекции."
                            }
                        }
                    ],
                    "synonyms": [
                        "sponsor",
                        "patron",
                        "opiekun"
                    ],
                    "common_phrases": [
                        "mecenas sztuki",
                        "mecenas kultury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или организация, оказывающие финансовую поддержку художникам, ученым или культурным проектам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меценат",
                                "target_lang_sense_clarification": "покровитель искусств и наук"
                            },
                            {
                                "target_lang_word": "покровитель",
                                "target_lang_sense_clarification": "защитник, спонсор"
                            }
                        ]
                    }
                }
            ]
        }
    ]

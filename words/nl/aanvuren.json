    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3d4fd4e6-9486-329e-9aee-694a1f81c39d",
                    "sense_definition": "Iemand enthousiast aanmoedigen of aansporen om een prestatie te leveren, vaak door te roepen of toe te juichen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "encourage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De supporters bleven hun team luidkeels <w>aanvuren</w> tot het laatste fluitsignaal.",
                            "target_lang_translations": {
                                "en": "The supporters kept <w>cheering on</w> their team loudly until the final whistle.",
                                "ru": "Болельщики продолжали громко <w>подбадривать</w> свою команду до финального свистка."
                            }
                        },
                        {
                            "text": "Een goede coach weet precies hoe hij zijn spelers moet <w>aanvuren</w> voor de wedstrijd.",
                            "target_lang_translations": {
                                "en": "A good coach knows exactly how to <w>fire up</w> his players before the match.",
                                "ru": "Хороший тренер точно знает, как <w>воодушевить</w> своих игроков перед матчем."
                            }
                        }
                    ],
                    "synonyms": [
                        "aansporen",
                        "stimuleren",
                        "aanporren",
                        "oppeppen",
                        "aanzetten",
                        "opporren"
                    ],
                    "common_phrases": [
                        "het team aanvuren",
                        "elkaar aanvuren"
                    ],
                    "target_lang_definitions": {
                        "en": "To enthusiastically encourage or urge someone to perform, often by shouting or cheering.",
                        "ru": "Воодушевлять или побуждать кого-то к действию, часто криками поддержки или аплодисментами."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "encourage",
                                "target_lang_sense_clarification": "to give support, confidence, or hope to someone"
                            },
                            {
                                "target_lang_word": "cheer on",
                                "target_lang_sense_clarification": "to shout encouragement to a team or person"
                            },
                            {
                                "target_lang_word": "spur on",
                                "target_lang_sense_clarification": "to urge someone to continue or try harder"
                            },
                            {
                                "target_lang_word": "fire up",
                                "target_lang_sense_clarification": "to fill with energy or enthusiasm"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подбадривать",
                                "target_lang_sense_clarification": "ободрять, выражать поддержку"
                            },
                            {
                                "target_lang_word": "воодушевлять",
                                "target_lang_sense_clarification": "вдохновлять на действия"
                            },
                            {
                                "target_lang_word": "подстегивать",
                                "target_lang_sense_clarification": "побуждать к активности, стимулировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "587f08ce-7984-3074-87d2-d19eb58233cb",
                    "sense_definition": "Een vuur, strijd of emotie heviger maken of doen oplaaien.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "kindle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De harde wind zal het bosbrandje snel verder <w>aanvuren</w>.",
                            "target_lang_translations": {
                                "en": "The strong wind will quickly <w>fan</w> the small forest fire further.",
                                "ru": "Сильный ветер быстро <w>раздует</w> лесной пожар."
                            }
                        },
                        {
                            "text": "Zijn provocerende opmerkingen dienden alleen om de ruzie <w>aan te vuren</w>.",
                            "target_lang_translations": {
                                "en": "His provocative remarks only served to <w>fuel</w> the argument.",
                                "ru": "Его провокационные замечания послужили лишь тому, чтобы <w>разжечь</w> ссору."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanwakkeren",
                        "opstoken",
                        "prikkelen"
                    ],
                    "common_phrases": [
                        "een conflict aanvuren",
                        "het vuur aanvuren"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a fire, conflict, or emotion more intense or cause it to flare up.",
                        "ru": "Усиливать огонь, конфликт или эмоции; заставлять разгораться сильнее."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fan",
                                "target_lang_sense_clarification": "to make a fire or emotion stronger (often 'fan the flames')"
                            },
                            {
                                "target_lang_word": "fuel",
                                "target_lang_sense_clarification": "to intensify a conflict, argument, or feeling"
                            },
                            {
                                "target_lang_word": "stir up",
                                "target_lang_sense_clarification": "to provoke or incite trouble or conflict"
                            },
                            {
                                "target_lang_word": "stoke",
                                "target_lang_sense_clarification": "to encourage or incite strong emotions or conflict"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раздувать",
                                "target_lang_sense_clarification": "усиливать пламя или конфликт"
                            },
                            {
                                "target_lang_word": "разжигать",
                                "target_lang_sense_clarification": "способствовать возгоранию или обострению ситуации"
                            }
                        ]
                    }
                }
            ]
        }
    ]

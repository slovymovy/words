                {
                    "sense_id": "8add6418-3f81-39da-9ddb-b4ab016f6488",
                    "sense_definition": "An English topographic surname for someone who lived by a small wood or copse.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Shaw</w> has been appointed as the new director of the company.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Шоу</w> был назначен новым директором компании."
                            }
                        },
                        {
                            "text": "We are going to visit the <w>Shaw</w> family this weekend.",
                            "target_lang_translations": {
                                "ru": "Мы собираемся навестить семью <w>Шоу</w> в эти выходные."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Английская топонимическая фамилия, обозначающая человека, жившего у небольшого леса или рощи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Шоу",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "021d49a0-eaff-3eec-8fc2-8371ebff1b38",
                    "sense_definition": "A place name used for various towns, villages, and neighborhoods in the United Kingdom and the United States.",

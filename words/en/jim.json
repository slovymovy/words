            "senses": [
                {
                    "sense_id": "40244a4b-1887-34bf-a178-9a4576b236d5",
                    "sense_definition": "A common short form of the male given name <w>James</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My neighbor <w>Jim</w> helped me fix the fence yesterday.",
                            "target_lang_translations": {
                                "ru": "Мой сосед <w>Джим</w> вчера помог мне починить забор."
                            }
                        },
                        {
                            "text": "Is <w>Jim</w> coming to the party with us tonight?",
                            "target_lang_translations": {
                                "ru": "<w>Джим</w> пойдет с нами сегодня вечером на вечеринку?"
                            }
                        }
                    ],
                    "synonyms": [
                        "Jimmy",
                        "Jimmie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенная краткая форма мужского имени Джеймс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Джим",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "18643611-e4e2-3549-b181-eaf669ac8700",
                    "sense_definition": "To force something open or to tamper with a mechanism.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to <w>jim</w> the lock with a thin piece of wire.",
                            "target_lang_translations": {
                                "ru": "Он попытался <w>вскрыть</w> замок тонким куском проволоки."
                            }
                        },
                        {
                            "text": "Be careful not to <w>jim</w> the gears by forcing the lever.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, чтобы не <w>испортить</w> шестерни, налегая на рычаг."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as 'jimmy'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вскрывать что-либо силой или вмешиваться в работу механизма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взламывать",
                                "target_lang_sense_clarification": "вскрывать силой"
                            },
                            {
                                "target_lang_word": "поддевать",
                                "target_lang_sense_clarification": "рычагом или инструментом"
                            }
                        ]
                    }
                }
            ]
        },
        {

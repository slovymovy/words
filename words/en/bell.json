    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "90b6875d-915c-32ea-a041-eb28f1bb399b",
                    "sense_definition": "A hollow metal object, typically shaped like a cup, that makes a ringing sound when struck by a clapper inside or a hammer outside.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "instrument_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The church <w>bell</w> rings every Sunday morning to call people to the service.",
                            "target_lang_translations": {
                                "ru": "Церковный <w>колокол</w> звонит каждое воскресное утро, созывая людей на службу."
                            }
                        },
                        {
                            "text": "She rang the small silver <w>bell</w> on the counter to get the clerk's attention.",
                            "target_lang_translations": {
                                "ru": "Она позвонила в маленький серебряный <w>колокольчик</w> на прилавке, чтобы привлечь внимание клерка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ring a bell",
                        "church bell",
                        "alarm bell",
                        "door bell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полый металлический предмет, обычно в форме чаши, издающий звон при ударе языком изнутри или молоточком снаружи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колокол",
                                "target_lang_sense_clarification": "большой, например, церковный"
                            },
                            {
                                "target_lang_word": "колокольчик",
                                "target_lang_sense_clarification": "маленький, ручной"
                            },
                            {
                                "target_lang_word": "звонок",
                                "target_lang_sense_clarification": "дверной или велосипедный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bf35738-6ba3-3269-b20a-7157fb2eb87f",
                    "sense_definition": "The sound produced by a bell, often used as a signal to mark a specific time or event.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "instrument_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The students packed their bags as soon as they heard the <w>bell</w>.",
                            "target_lang_translations": {
                                "ru": "Студенты собрали сумки, как только услышали <w>звонок</w>."
                            }
                        },
                        {
                            "text": "He was saved by the <w>bell</w> just before he had to answer the difficult question.",
                            "target_lang_translations": {
                                "ru": "Его спас <w>звонок</w> как раз перед тем, как ему пришлось отвечать на трудный вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "alert",
                        "beep"
                    ],
                    "common_phrases": [
                        "saved by the bell",
                        "answer the bell",
                        "hear the bell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, издаваемый колоколом, часто используемый как сигнал для обозначения определенного времени или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонок",
                                "target_lang_sense_clarification": "сигнал (школьный, театральный)"
                            },
                            {
                                "target_lang_word": "звон",
                                "target_lang_sense_clarification": "звучание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "196ab192-af84-3e7b-9aef-4e8724e8a48e",
                    "sense_definition": "Anything shaped like a bell, such as the cup of a flower or the flared end of a musical instrument.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The trumpet has a shiny brass <w>bell</w> that amplifies the sound.",
                            "target_lang_translations": {
                                "ru": "У трубы есть блестящий латунный <w>раструб</w>, который усиливает звук."
                            }
                        },
                        {
                            "text": "She admired the delicate blue <w>bell</w> of the flower in the garden.",
                            "target_lang_translations": {
                                "ru": "Она любовалась нежным голубым <w>колокольчиком</w> цветка в саду."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-либо, имеющее форму колокола, например, чашечка цветка или расширяющийся конец музыкального инструмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раструб",
                                "target_lang_sense_clarification": "музыкального инструмента"
                            },
                            {
                                "target_lang_word": "колокольчик",
                                "target_lang_sense_clarification": "цветок или форма цветка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "639b1942-e086-3ca9-b096-d26310c3ebe6",
                    "sense_definition": "A telephone call.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will give you a <w>bell</w> later this afternoon to discuss the plans.",
                            "target_lang_translations": {
                                "ru": "Я <w>позвоню</w> тебе позже днем, чтобы обсудить планы."
                            }
                        },
                        {
                            "text": "Thanks for the <w>bell</w>; it was good to catch up with you.",
                            "target_lang_translations": {
                                "ru": "Спасибо за <w>звонок</w>; было приятно пообщаться с тобой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "give someone a bell"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "British, Australian, New Zealand"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Телефонный звонок (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонок",
                                "target_lang_sense_clarification": "по телефону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf75ef97-f96d-3e40-ae30-279a2da55448",
                    "sense_definition": "The loud cry or bellow of certain animals, particularly a stag during the breeding season.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We could hear the deep <w>bell</w> of a stag echoing through the forest.",
                            "target_lang_translations": {
                                "ru": "Мы слышали глубокий <w>рёв</w> оленя, эхом разносившийся по лесу."
                            }
                        },
                        {
                            "text": "The hunter listened for the <w>bell</w> to locate the herd.",
                            "target_lang_translations": {
                                "ru": "Охотник прислушивался к <w>рёву</w>, чтобы определить местоположение стада."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "hunting or zoology context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Громкий крик или рев некоторых животных, особенно оленя в брачный период."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рёв",
                                "target_lang_sense_clarification": "оленя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "72543532-e042-358e-af17-e223ed65c6d6",
                    "sense_definition": "To telephone someone.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>bell</w> me when you get home so I know you are safe?",
                            "target_lang_translations": {
                                "ru": "Можешь <w>позвонить</w> мне, когда доберешься домой, чтобы я знал, что ты в безопасности?"
                            }
                        },
                        {
                            "text": "He <w>belled</w> his friend to arrange a meeting for the weekend.",
                            "target_lang_translations": {
                                "ru": "Он <w>позвонил</w> другу, чтобы договориться о встрече на выходных."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Звонить кому-либо по телефону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонить",
                                "target_lang_sense_clarification": "по телефону"
                            },
                            {
                                "target_lang_word": "звякнуть",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98319487-9210-37da-9675-e669d1112848",
                    "sense_definition": "To attach a bell to someone or something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_attach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mice held a meeting to decide who would <w>bell</w> the cat.",
                            "target_lang_translations": {
                                "ru": "Мыши собрались на совещание, чтобы решить, кто <w>повесит колокольчик</w> на кота."
                            }
                        },
                        {
                            "text": "Farmers often <w>bell</w> their livestock to keep track of them in the fields.",
                            "target_lang_translations": {
                                "ru": "Фермеры часто <w>вешают колокольчики</w> на скот, чтобы следить за ним в полях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bell the cat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикреплять колокольчик к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вешать колокольчик",
                                "target_lang_sense_clarification": "на шею животному"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0a8ee5b-2f26-3c5f-954e-e458cc79bf7e",
                    "sense_definition": "To shape something so that it flares out like a bell.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to <w>bell</w> the end of the tube to make it fit over the connector.",
                            "target_lang_translations": {
                                "ru": "Вам нужно <w>расширить</w> конец трубки, чтобы надеть её на соединитель."
                            }
                        },
                        {
                            "text": "The skirt <w>bells</w> out beautifully when she spins around.",
                            "target_lang_translations": {
                                "ru": "Юбка красиво <w>расширяется колоколом</w>, когда она кружится."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать чему-либо форму колокола (расширять книзу)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расширять",
                                "target_lang_sense_clarification": "делать раструб"
                            },
                            {
                                "target_lang_word": "развальцовывать",
                                "target_lang_sense_clarification": "техническое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf9dd8f3-471e-3ea0-9103-641acdc70f9c",
                    "sense_definition": "To make a loud, deep cry or roar, typically said of a stag.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The stag <w>belled</w> loudly, challenging his rivals in the valley.",
                            "target_lang_translations": {
                                "ru": "Олень громко <w>ревел</w>, бросая вызов соперникам в долине."
                            }
                        },
                        {
                            "text": "As dawn broke, the animals <w>belled</w> in the distance.",
                            "target_lang_translations": {
                                "ru": "На рассвете животные <w>ревели</w> вдалеке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "hunting or zoology context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать громкий, глубокий крик или рев (обычно об олене)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "реветь",
                                "target_lang_sense_clarification": "об олене"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "8be7e823-550f-3eac-8c88-e64dc35cbc71",
                    "sense_definition": "A surname of Scottish and northern English origin.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Bell</w> is the new teacher at our school.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Белл</w> — новый учитель в нашей школе."
                            }
                        },
                        {
                            "text": "The <w>Bell</w> family has lived in this town for generations.",
                            "target_lang_translations": {
                                "ru": "Семья <w>Белл</w> живет в этом городе уже несколько поколений."
                            }

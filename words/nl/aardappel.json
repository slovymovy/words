            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa888882-b40c-34fa-9743-f66a06b2efeb",
                    "sense_definition": "De eetbare knol van de aardappelplant, die wereldwijd als belangrijk voedingsmiddel wordt gebruikt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_tuber",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Nederland eten veel mensen 's avonds vlees, groente en gekookte <w>aardappelen</w>.",
                            "target_lang_translations": {
                                "ru": "В Нидерландах многие люди едят на ужин мясо, овощи и вареный <w>картофель</w>."
                            }
                        },
                        {
                            "text": "Wil jij vanavond de <w>aardappels</w> schillen voor het avondeten?",
                            "target_lang_translations": {
                                "ru": "Ты почистишь <w>картошку</w> к ужину сегодня вечером?"
                            }
                        }
                    ],
                    "synonyms": [
                        "pieper",
                        "patat"
                    ],
                    "common_phrases": [
                        "aardappels schillen",
                        "gekookte aardappels",
                        "gebakken aardappels",
                        "nieuwe aardappels"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный клубень растения картофель, который широко используется во всем мире как важный продукт питания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "картофель",
                                "target_lang_sense_clarification": "овощ"
                            },
                            {
                                "target_lang_word": "картошка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef06051a-5248-3708-946e-522628eac1f3",
                    "sense_definition": "De plant *Solanum tuberosum* uit de nachtschadefamilie waaraan de eetbare knollen groeien.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "plant_biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het groene loof van de <w>aardappel</w> is giftig voor mensen en dieren.",
                            "target_lang_translations": {
                                "ru": "Зеленая ботва <w>картофеля</w> ядовита для людей и животных."
                            }
                        },
                        {
                            "text": "In het voorjaar gaan de boeren de nieuwe <w>aardappels</w> poten op het land.",
                            "target_lang_translations": {
                                "ru": "Весной фермеры сажают новый <w>картофель</w> на полях."
                            }
                        }
                    ],
                    "synonyms": [
                        "aardappelplant"
                    ],
                    "common_phrases": [
                        "aardappels poten",
                        "aardappels rooien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растение Solanum tuberosum из семейства пасленовых, на котором растут съедобные клубни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "картофель",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4303ea6-7215-3a7e-a70a-ed06303e9862",
                    "sense_definition": "Een dom of onhandig persoon.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kijk toch uit waar je loopt, jij onhandige <w>aardappel</w>!",
                            "target_lang_translations": {
                                "ru": "Смотри, куда идешь, ты, неуклюжий <w>тюфяк</w>!"
                            }
                        },
                        {
                            "text": "Hij voelde zich een echte <w>aardappel</w> toen hij de afspraak was vergeten.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя полным <w>болваном</w>, когда забыл о встрече."
                            }
                        }
                    ],
                    "synonyms": [
                        "sufferd"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used as a mild insult"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Глупый или неуклюжий человек."
                    },
                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "8682567a-3017-3e8f-ba82-bb2e05504dff",
                    "sense_definition": "Een benaming voor planten uit het geslacht Cyclamen en hun knollen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany_cyclamen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de volksmond wordt deze plant soms een <w>aardappel</w> genoemd vanwege de knol.",
                            "target_lang_translations": {
                                "ru": "В просторечии это растение иногда называют <w>картофелем</w> из-за его клубня."
                            }
                        },
                        {
                            "text": "De cyclaam staat ook wel bekend als varkensbrood of <w>aardappel</w>.",
                            "target_lang_translations": {
                                "ru": "Цикламен также известен как дряква или <w>картофель</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cyclaam",
                        "varkensbrood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Название растений из рода Цикламен и их клубней."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }

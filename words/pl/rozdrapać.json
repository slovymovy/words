    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fe09ee51-10dc-3b53-830d-8e22b1354d23",
                    "sense_definition": "Spowodować uszkodzenie skóry lub ponowne otwarcie rany poprzez mocne drapanie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko tak długo dotykało kolana, aż <rozdrapało> sobie strup.",
                            "target_lang_translations": {
                                "ru": "Ребёнок так долго трогал колено, пока не <расковырял> себе болячку."
                            }
                        },
                        {
                            "text": "Musisz uważać, żeby nie <rozdrapać> tego miejsca do krwi.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть осторожен, чтобы не <расчесать> это место до крови."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rozdrapać ranę",
                        "rozdrapać strup",
                        "rozdrapać do krwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повредить кожу или снова открыть рану сильным чесанием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расчесать",
                                "target_lang_sense_clarification": "повредить кожу ногтями при зуде"
                            },
                            {
                                "target_lang_word": "расковырять",
                                "target_lang_sense_clarification": "содрать корку с раны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72c7aa29-eb70-3e4b-abb8-c24f1ab69ca8",
                    "sense_definition": "Szybko i chciwie zabrać, wykupić lub ukraść wszystkie dostępne rzeczy (zwykle o grupie ludzi).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klienci wpadli do sklepu i w kilka minut <rozdrapali> promocyjne towary.",
                            "target_lang_translations": {
                                "ru": "Клиенты ворвались в магазин и за несколько минут <расхватали> акционные товары."
                            }
                        },
                        {
                            "text": "Po śmierci milionera rodzina natychmiast <rozdrapała> jego majątek.",
                            "target_lang_translations": {
                                "ru": "После смерти миллионера семья немедленно <растащила> его состояние."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozchwycić",
                        "rozkraść"
                    ],
                    "common_phrases": [
                        "rozdrapać majątek",
                        "rozdrapać wszystko"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро и жадно разобрать, раскупить или растащить всё доступное (обычно о группе людей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растащить",
                                "target_lang_sense_clarification": "разобрать, присвоить по частям"
                            },
                            {
                                "target_lang_word": "расхватать",
                                "target_lang_sense_clarification": "быстро разобрать, раскупить"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a54c76f7-de49-33f1-a406-57c65218796a",
                    "sense_definition": "Быстро, точно и с большим умением (о движениях или действиях).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вратарь <w>ловко</w> поймал летящий мяч в прыжке.",
                            "target_lang_translations": {
                                "en": "The goalkeeper <w>deftly</w> caught the flying ball in a jump."
                            }
                        },
                        {
                            "text": "Она <w>ловко</w> управляется с вязальными спицами, создавая узоры.",
                            "target_lang_translations": {
                                "en": "She <w>skillfully</w> handles the knitting needles, creating patterns."
                            }
                        }
                    ],
                    "synonyms": [
                        "искусно",
                        "проворно"
                    ],
                    "antonyms": [
                        "неуклюже",
                        "неловко"
                    ],
                    "common_phrases": [
                        "ловко сделать",
                        "ловко поймать",
                        "ловко управлять"
                    ],
                    "target_lang_definitions": {
                        "en": "In a quick, accurate, and skillful manner, especially regarding movements or physical actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deftly",
                                "target_lang_sense_clarification": "implies quick and neat physical movement"
                            },
                            {
                                "target_lang_word": "skillfully",
                                "target_lang_sense_clarification": "with expertise and competence"
                            },
                            {
                                "target_lang_word": "nimbly",
                                "target_lang_sense_clarification": "with lightness and speed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f58d19e-e93e-35a0-830e-f4f882f2fb46",
                    "sense_definition": "Хитро, изворотливо и с умом (о поведении, речи или решении проблем).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Андрей <w>ловко</w> вывернулся из сложной ситуации, придумав оправдание.",
                            "target_lang_translations": {
                                "en": "Andrey <w>cleverly</w> wriggled out of the difficult situation by inventing an excuse."
                            }
                        },
                        {
                            "text": "Мошенник <w>ловко</w> обманул доверчивых туристов на рынке.",
                            "target_lang_translations": {
                                "en": "The scammer <w>slickly</w> deceived the gullible tourists at the market."
                            }
                        }
                    ],
                    "synonyms": [
                        "искусно"
                    ],
                    "common_phrases": [
                        "ловко придумать",
                        "ловко выкрутиться",
                        "ловко обмануть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implied by usage context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "In a cunning, resourceful, or clever way, often regarding behavior or problem-solving."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cleverly",
                                "target_lang_sense_clarification": "showing intelligence or resourcefulness"
                            },
                            {
                                "target_lang_word": "cunningly",
                                "target_lang_sense_clarification": "in a tricky or deceitful way"
                            },
                            {
                                "target_lang_word": "slickly",
                                "target_lang_sense_clarification": "smoothly but often deceptively"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e1e80dd-0dcb-3473-a449-24c486b625fd",
                    "sense_definition": "Возглас или оценка, выражающие восхищение чьим-либо умением, удачным действием или сноровкой.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— <w>Ловко</w> ты это придумал! — с восхищением сказал отец.",
                            "target_lang_translations": {
                                "en": "\"You came up with that <w>cleverly</w>!\" said father with admiration."
                            }
                        },
                        {
                            "text": "— Даже не ожидал, что вы так быстро справитесь, <w>ловко</w>!",
                            "target_lang_translations": {
                                "en": "\"I didn't even expect you to manage so quickly, <w>neatly done</w>!\""
                            }
                        }
                    ],
                    "common_phrases": [
                        "это ловко",
                        "как ловко"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation or evaluation expressing admiration for someone's skill, luck, or successful action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cleverly",
                                "target_lang_sense_clarification": "praising the ingenuity of an action"
                            },
                            {
                                "target_lang_word": "neatly",
                                "target_lang_sense_clarification": "praising the execution of an action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c789d4e7-c390-3763-8a34-1a131b32bf0d",
                    "sense_definition": "Удобно, хорошо по фигуре (обычно об одежде или обуви).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый костюм сидит на тебе очень <w>ловко</w>.",
                            "target_lang_translations": {
                                "en": "The new suit fits you very <w>well</w>."
                            }
                        },
                        {
                            "text": "Сапоги сшиты <w>ловко</w>, нигде не жмут и смотрятся красиво.",
                            "target_lang_translations": {
                                "en": "The boots are made <w>well</w>, they don't pinch anywhere and look beautiful."
                            }
                        }
                    ],
                    "synonyms": [
                        "удобно"
                    ],
                    "antonyms": [
                        "неловко"
                    ],
                    "common_phrases": [
                        "сидеть ловко"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "low colloquial usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Fitting well, comfortably, or looking good (usually regarding clothes or shoes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "fitting properly"
                            },
                            {
                                "target_lang_word": "snugly",
                                "target_lang_sense_clarification": "fitting closely and comfortably"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bdbfe753-9eaa-3215-b1a7-c873d15e21cd",
                    "sense_definition": "Краткая форма среднего рода прилагательного <w>ловкий</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Движение гимнаста было <w>ловко</w> и невероятно грациозно.",
                            "target_lang_translations": {
                                "en": "The gymnast's movement was <w>agile</w> and incredibly graceful."
                            }
                        },
                        {
                            "text": "Его решение проблемы было <w>ловко</w>, хотя и рискованно.",
                            "target_lang_translations": {
                                "en": "His solution to the problem was <w>clever</w>, although risky."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "short neuter singular"
                        }
                    ],
                    "target_lang_definitions": {

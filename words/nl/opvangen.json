            "senses": [
                {
                    "sense_id": "147feda1-8479-3402-885f-8b3111a75ab5",
                    "sense_definition": "Iets dat valt of beweegt grijpen, tegenhouden of de kracht ervan absorberen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_catch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De keeper kon de bal gelukkig nog net op tijd <w>opvangen</w>.",
                            "target_lang_translations": {
                                "en": "Fortunately, the goalkeeper was able to <w>catch</w> the ball just in time.",
                                "ru": "К счастью, вратарь смог <w>поймать</w> мяч как раз вовремя."
                            }
                        },
                        {
                            "text": "Zonnepanelen op het dak <w>vangen</w> het zonlicht <w>op</w> om energie op te wekken.",
                            "target_lang_translations": {
                                "en": "Solar panels on the roof <w>capture</w> the sunlight to generate energy.",
                                "ru": "Солнечные панели на крыше <w>улавливают</w> солнечный свет для выработки энергии."
                            }
                        }
                    ],
                    "synonyms": [
                        "vangen",
                        "tegenhouden",
                        "stoppen"
                    ],
                    "antonyms": [
                        "laten vallen",
                        "missen"
                    ],
                    "common_phrases": [
                        "een bal opvangen",
                        "een klap opvangen",
                        "signalen opvangen"
                    ],
                    "target_lang_definitions": {
                        "en": "To catch, stop, or absorb the force of something falling or moving.",
                        "ru": "Схватить что-то падающее или движущееся, остановить или поглотить силу воздействия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "catch",
                                "target_lang_sense_clarification": "to seize something moving through the air"
                            },
                            {
                                "target_lang_word": "absorb",
                                "target_lang_sense_clarification": "to take the impact of a blow or force"
                            },
                            {
                                "target_lang_word": "intercept",
                                "target_lang_sense_clarification": "to stop or catch something in progress"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поймать",
                                "target_lang_sense_clarification": "схватить летящий предмет"
                            },
                            {
                                "target_lang_word": "улавливать",

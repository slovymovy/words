    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "84cbcad8-1faf-3755-85a2-aadbdf0817c5",
                    "sense_definition": "Przestać istnieć, trwać lub dziać się.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy silny wiatr w końcu <ustał>, mogliśmy wyjść na spacer.",
                            "target_lang_translations": {
                                "ru": "Когда сильный ветер наконец <утих>, мы смогли выйти на прогулку."
                            }
                        },
                        {
                            "text": "Miejmy nadzieję, że ten uciążliwy hałas wkrótce <ustanie>.",
                            "target_lang_translations": {
                                "ru": "Будем надеяться, что этот назойливый шум вскоре <прекратится>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakończyć się"
                    ],
                    "common_phrases": [
                        "deszcz ustał",
                        "wiatr ustał",
                        "ból ustał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать существовать, длиться или происходить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прекратиться",
                                "target_lang_sense_clarification": "перестать происходить"
                            },
                            {
                                "target_lang_word": "утихнуть",
                                "target_lang_sense_clarification": "о ветре, шуме или боли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae08362e-3f3d-3a97-ab80-fef228db1972",
                    "sense_definition": "Utrzymać równowagę i nie przewrócić się, mimo trudnych warunków.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Było tak ślisko na chodniku, że ledwo <ustałem> na nogach.",
                            "target_lang_translations": {
                                "ru": "На тротуаре было так скользко, что я едва <устоял> на ногах."
                            }
                        },
                        {
                            "text": "Mimo silnego uderzenia, zawodnik <ustał> i kontynuował grę.",
                            "target_lang_translations": {
                                "ru": "Несмотря на сильный удар, игрок <устоял> и продолжил игру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ledwo ustać na nogach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удержать равновесие и не упасть, несмотря на трудные условия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устоять",
                                "target_lang_sense_clarification": "сохранить равновесие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e627b46a-1e0f-3b08-be3e-3d41dc31a4fb",
                    "sense_definition": "Opaść z sił i bardzo się zmęczyć, zazwyczaj od długiego stania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu godzinach pracy w sklepie moje nogi zupełnie <ustały>.",
                            "target_lang_translations": {
                                "ru": "После многих часов работы в магазине мои ноги совершенно <устали>."
                            }
                        },
                        {
                            "text": "Starsza pani <ustała> po długim oczekiwaniu w kolejce do lekarza.",
                            "target_lang_translations": {
                                "ru": "Пожилая женщина <устала> после долгого ожидания в очереди к врачу."
                            }
                        }
                    ],
                    "synonyms": [
                        "znużyć się"
                    ],
                    "common_phrases": [
                        "nogi mi ustały"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишиться сил и сильно утомиться, обычно от долгого стояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устать",
                                "target_lang_sense_clarification": "утомиться"
                            },
                            {
                                "target_lang_word": "отняться",
                                "target_lang_sense_clarification": "о ногах: потерять чувствительность от усталости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fa8f0bf-dc3a-37bb-bcd8-82e36536e04f",
                    "sense_definition": "Stać się klarownym i przejrzystym po opadnięciu osadu na dno (o cieczy).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz poczekać, aż zmącona woda w wiadrze <ustanie>.",
                            "target_lang_translations": {
                                "ru": "Ты должен подождать, пока мутная вода в ведре <отстоится>."
                            }
                        },
                        {
                            "text": "Kiedy sok z owoców <ustanie>, będzie miał piękny i czysty kolor.",
                            "target_lang_translations": {
                                "ru": "Когда фруктовый сок <отстоится>, у него будет красивый и чистый цвет."
                            }
                        }
                    ],
                    "synonyms": [
                        "sklarować się"
                    ],
                    "common_phrases": [
                        "woda ustała"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать прозрачным и чистым после оседания осадка (о жидкости)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отстояться",
                                "target_lang_sense_clarification": "о жидкости"
                            },
                            {
                                "target_lang_word": "устояться",
                                "target_lang_sense_clarification": "прийти в норму, осесть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bcbcd5c-87da-3f83-9cc7-fc0ef739eb2f",
                    "sense_definition": "Podnieść się z pozycji siedzącej lub leżącej na stojącą.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz kazał wszystkim gościom <ustać> od stołu.",
                            "target_lang_translations": {
                                "ru": "Хозяин велел всем гостям <встать> из-за стола."
                            }
                        },
                        {
                            "text": "Chłopiec powoli <ustał> z łóżka, przecierając zaspane oczy.",
                            "target_lang_translations": {
                                "ru": "Мальчик медленно <встал> с кровати, протирая заспанные глаза."
                            }
                        }
                    ],
                    "synonyms": [
                        "wstać"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "używane w gwarach i dialektach zamiast 'wstać'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подняться из сидячего или лежачего положения (региональное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встать",
                                "target_lang_sense_clarification": "подняться на ноги"
                            }
                        ]
                    }
                }
            ]
        }
    ]

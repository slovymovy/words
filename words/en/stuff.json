    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f526e141-5d34-3c25-8ed7-258103c5c686",
                    "sense_definition": "Miscellaneous items, objects, or personal belongings, often referred to in a general or unspecified way.",

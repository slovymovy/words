                            {
                                "target_lang_word": "суматоха",
                                "target_lang_sense_clarification": "беспорядок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e76eef7b-46eb-352f-abb5-79f8ad5a5a1b",
                    "sense_definition": "Een algemene beweging of verplaatsing.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elk <w>gebaar</w> dat hij maakte deed pijn aan zijn rug.",
                            "target_lang_translations": {
                                "en": "Every <w>movement</w> he made hurt his back.",
                                "ru": "Каждое <w>движение</w>, которое он делал, отдавалось болью в спине."
                            }
                        },
                        {
                            "text": "Hij volgde het trage <w>gebaar</w> van de wolken in de lucht.",
                            "target_lang_translations": {
                                "en": "He followed the slow <w>motion</w> of the clouds in the sky.",
                                "ru": "Он следил за медленным <w>движением</w> облаков в небе."
                            }
                        }
                    ],
                    "synonyms": [
                        "beweging"
                    ],
                    "target_lang_definitions": {
                        "en": "A general movement or displacement.",
                        "ru": "Общее движение или перемещение."
                    },
                    "translations": {

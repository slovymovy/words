            "senses": [
                {
                    "sense_id": "13c3aea6-6822-357c-8d03-5b30598860a5",
                    "sense_definition": "Текст, написанный или отправленный кому-либо для сообщения информации (на бумаге или в электронном виде).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я вчера получил долгожданное <письмо> от своего старого друга.",
                            "target_lang_translations": {
                                "en": "Yesterday I received a long-awaited <letter> from my old friend."
                            }
                        },
                        {
                            "text": "Тебе нужно срочно отправить это заказное <письмо> по почте.",
                            "target_lang_translations": {
                                "en": "You need to send this registered <letter> by mail immediately."
                            }
                        }
                    ],
                    "synonyms": [
                        "послание",
                        "сообщение"
                    ],
                    "common_phrases": [
                        "написать письмо",
                        "отправить письмо",
                        "получить письмо",
                        "электронное письмо",
                        "заказное письмо"
                    ],
                    "target_lang_definitions": {
                        "en": "A written message sent to someone, either on paper or electronically."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "letter",
                                "target_lang_sense_clarification": "written message"
                            },
                            {
                                "target_lang_word": "email",
                                "target_lang_sense_clarification": "electronic message"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c39e1e3-5cbd-3e07-8422-6984683abac9",
                    "sense_definition": "Система графических знаков, принятая для фиксации речи в каком-либо языке.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Древнее иероглифическое <письмо> было очень сложным для изучения.",
                            "target_lang_translations": {
                                "en": "Ancient hieroglyphic <writing> was very difficult to learn."
                            }
                        },
                        {
                            "text": "Славянское <письмо> развивалось на протяжении многих веков.",
                            "target_lang_translations": {
                                "en": "Slavic <script> developed over many centuries."

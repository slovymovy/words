            "pos": "noun",
            "senses": [
                {
                    "sense_id": "80ad2c6c-1813-3710-8072-9772d858fb3b",
                    "sense_definition": "Rytmiczny ruch tętnic wyczuwalny dotykiem, będący skutkiem bicia serca i przepływu krwi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz przyłożył palce do mojego nadgarstka, aby sprawdzić <w>puls</w>.",
                            "target_lang_translations": {
                                "ru": "Врач приложил пальцы к моему запястью, чтобы проверить <w>пульс</w>."
                            }
                        },
                        {
                            "text": "Po szybkim biegu mój <w>puls</w> był bardzo wysoki i długo nie spadał.",
                            "target_lang_translations": {
                                "ru": "После быстрого бега мой <w>пульс</w> был очень высоким и долго не падал."
                            }
                        }
                    ],
                    "synonyms": [
                        "tętno"
                    ],
                    "common_phrases": [
                        "mierzyć puls",
                        "szybki puls",
                        "wyczuć puls",
                        "słaby puls"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ритмическое движение артерий, ощущаемое при прикосновении, вызванное сердцебиением и током крови."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пульс",
                                "target_lang_sense_clarification": "физиологический"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4cee0ee2-1728-391e-9d0e-0d1a24294981",
                    "sense_definition": "Rytm, dynamika lub energia charakterystyczna dla jakiegoś miejsca, społeczności lub działania.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W centrum wielkiego miasta można poczuć szybki <w>puls</w> nocnego życia.",
                            "target_lang_translations": {
                                "ru": "В центре большого города можно почувствовать быстрый <w>пульс</w> ночной жизни."
                            }
                        },
                        {
                            "text": "Dziennikarz musi trzymać rękę na <w>pulsie</w>, aby wiedzieć o wszystkich ważnych wydarzeniach.",
                            "target_lang_translations": {
                                "ru": "Журналист должен держать руку на <w>пульсе</w>, чтобы знать обо всех важных событиях."
                            }
                        }
                    ],
                    "synonyms": [
                        "tętno"
                    ],
                    "common_phrases": [
                        "trzymać rękę na pulsie",
                        "puls miasta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ритм, динамика или энергия, характерная для какого-либо места, сообщества или действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пульс",
                                "target_lang_sense_clarification": "переносное значение: ритм жизни"
                            },
                            {
                                "target_lang_word": "ритм",
                                "target_lang_sense_clarification": "темп, динамика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31aeafc4-a2ca-36ea-a218-081e0176f926",
                    "sense_definition": "Część głowy pomiędzy czołem a uchem; skroń.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starszy pan narzekał na ból głowy i uciskał prawy <w>puls</w>.",
                            "target_lang_translations": {
                                "ru": "Пожилой мужчина жаловался на головную боль и сжимал правый <w>висок</w>."
                            }
                        },
                        {
                            "text": "Uderzył się w <w>puls</w>, gdy przechodził przez niskie drzwi.",
                            "target_lang_translations": {
                                "ru": "Он ударился <w>виском</w>, когда проходил через низкую дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) pulst"
                    ],
                    "traits": [
                        {

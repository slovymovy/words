                                "target_lang_sense_clarification": "mischievous person or child"
                            },
                            {
                                "target_lang_word": "scallywag",
                                "target_lang_sense_clarification": "playful rogue"
                            },
                            {
                                "target_lang_word": "prankster",
                                "target_lang_sense_clarification": "someone who plays tricks"
                            }
                        ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "09c8ac71-5d1d-3791-aabe-01d413d496ec",
                    "sense_definition": "The duration of the life of someone or something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The average <w>lifetime</w> of a house cat is around fifteen years.",
                            "target_lang_translations": {
                                "ru": "Средняя <w>продолжительность жизни</w> домашней кошки составляет около пятнадцати лет."
                            }
                        },
                        {
                            "text": "This high-quality tool comes with a limited <w>lifetime</w> warranty.",
                            "target_lang_translations": {
                                "ru": "Этот высококачественный инструмент поставляется с ограниченной гарантией на весь <w>срок службы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "lifespan"
                    ],
                    "common_phrases": [
                        "lifetime warranty",
                        "once in a lifetime",
                        "lifetime achievement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжительность жизни или период существования кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продолжительность жизни",
                                "target_lang_sense_clarification": "живого существа"
                            },
                            {
                                "target_lang_word": "срок службы",
                                "target_lang_sense_clarification": "техники, изделия"
                            },
                            {
                                "target_lang_word": "время жизни",
                                "target_lang_sense_clarification": "объекта, частицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7506519d-4a77-39ac-88a0-2203344d6f41",
                    "sense_definition": "A long period of time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It feels like I have been waiting a <w>lifetime</w> for you to arrive.",
                            "target_lang_translations": {
                                "ru": "Кажется, будто я ждал твоего приезда <w>целую вечность</w>."
                            }
                        },
                        {
                            "text": "These wonderful memories will surely last a <w>lifetime</w>.",
                            "target_lang_translations": {
                                "ru": "Эти чудесные воспоминания, несомненно, останутся на <w>всю жизнь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ages",
                        "eternity",
                        "years"
                    ],
                    "common_phrases": [
                        "wait a lifetime",
                        "last a lifetime"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used hyperbolically"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень долгий период времени; вечность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целая вечность",
                                "target_lang_sense_clarification": "разг., преувеличение"
                            },
                            {
                                "target_lang_word": "вся жизнь",
                                "target_lang_sense_clarification": "очень долгий срок"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "729b1958-55ee-32dd-b601-1306bb74c017",
                    "sense_definition": "Вид мягкой мебели с длинной спинкой и ручками, предназначенный для сидения и лежания.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В центре нашей гостиной стоит большой и удобный <w>диван</w>.",
                            "target_lang_translations": {
                                "en": "A large and comfortable <w>sofa</w> stands in the center of our living room."
                            }
                        },
                        {
                            "text": "После работы папа любит отдыхать и читать газету на <w>диване</w>.",
                            "target_lang_translations": {
                                "en": "After work, Dad likes to relax and read the newspaper on the <w>couch</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ложе"
                    ],
                    "common_phrases": [
                        "лежать на диване",
                        "купить диван",
                        "удобный диван",
                        "раскладной диван"
                    ],
                    "target_lang_definitions": {
                        "en": "A piece of upholstered furniture with a back and armrests, designed for seating two or more people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sofa",
                                "target_lang_sense_clarification": "common term"
                            },
                            {
                                "target_lang_word": "couch",
                                "target_lang_sense_clarification": "often implies comfort or casual use"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e281a07-d390-368b-b6d3-9f02594933d9",
                    "sense_definition": "Высший орган исполнительной или законодательной власти в ряде исламских государств.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Султан собрал <w>диван</w> для решения важных государственных вопросов.",
                            "target_lang_translations": {
                                "en": "The Sultan convened the <w>divan</w> to resolve important state issues."
                            }
                        },
                        {
                            "text": "Визири вошли в <w>диван</w> и заняли свои места для обсуждения.",
                            "target_lang_translations": {
                                "en": "The viziers entered the <w>divan</w> and took their seats for the discussion."
                            }
                        }
                    ],
                    "common_phrases": [
                        "созвать диван",
                        "члены дивана"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "исторический термин"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A high governmental council or assembly in certain Islamic states, particularly the Ottoman Empire."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "divan",
                                "target_lang_sense_clarification": "historical council"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e86705c-df4d-320e-9cf0-8e212fa5256a",
                    "sense_definition": "Собрание лирических стихотворений одного поэта в литературах Ближнего и Среднего Востока.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

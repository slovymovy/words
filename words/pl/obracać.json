            "senses": [
                {
                    "sense_id": "1d4b35bc-6bb3-3fcf-8a26-4cbb2d872acc",
                    "sense_definition": "Wprawiać przedmiot w ruch dookoła jego własnej osi lub wokół innego punktu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_rotation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechanik powoli <obracał> kołem, aby sprawdzić hamulce.",
                            "target_lang_translations": {
                                "ru": "Механик медленно <вращал> колесо, чтобы проверить тормоза."
                            }
                        },
                        {
                            "text": "Dziecko <obracało> globusem, szukając na nim Polski.",
                            "target_lang_translations": {
                                "ru": "Ребенок <крутил> глобус, ища на нем Польшу."
                            }
                        }
                    ],
                    "synonyms": [
                        "kręcić",
                        "wirować"
                    ],
                    "antonyms": [
                        "zatrzymywać"
                    ],
                    "common_phrases": [
                        "obracać głowę",
                        "obracać klucz",
                        "obracać kołem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить предмет в движение вокруг его оси или другой точки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вращать",
                                "target_lang_sense_clarification": "вокруг оси"
                            },
                            {
                                "target_lang_word": "крутить",
                                "target_lang_sense_clarification": "руками или механизмом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ae47c70-378f-3683-baef-89981861c9d8",
                    "sense_definition": "Poruszać się ruchem obrotowym wokół własnej osi (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_rotation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ziemia <obraca się> wokół własnej osi każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Земля <вращается> вокруг собственной оси каждый день."
                            }
                        },
                        {
                            "text": "Tancerka <obracała się> tak szybko, że publiczność była zachwycona.",
                            "target_lang_translations": {
                                "ru": "Танцовщица <крутилась> так быстро, что публика была в восторге."
                            }
                        }

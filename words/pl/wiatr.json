                    "frequency": "High",
                    "semantic_group_id": "weather_phenomena",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziś na dworze wieje bardzo silny i zimny <w>wiatr</w>.",
                            "target_lang_translations": {

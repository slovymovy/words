{
    "word_family": [
        "bliski",
        "blisko",
        "zbliżenie",
        "przybliżać",
        "najbliższy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3476be78-1536-3e82-a938-319322fa6a06",
                    "sense_definition": "Podchodzić do kogoś lub czegoś; zmniejszać dystans fizyczny dzielący od celu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Zbliżył się</w> do okna i zobaczył, że niebo jest zachmurzone.",
                            "target_lang_translations": {
                                "ru": "Он <w>подошел</w> к окну и увидел, что небо затянуто облаками."
                            }
                        },
                        {
                            "text": "Pociąg powoli <w>zbliżał się</w> do stacji kolejowej.",
                            "target_lang_translations": {
                                "ru": "Поезд медленно <w>приближался</w> к железнодорожной станции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbliżać się do miasta",
                        "zbliżać się do celu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходить к кому-либо или чему-либо; сокращать физическое расстояние до цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приближаться",
                                "target_lang_sense_clarification": "сокращать дистанцию"
                            },
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "приближаться шагом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94e966c6-7ec0-3552-9fc2-ad69a4f7f3b5",
                    "sense_definition": "O czasie, zjawiskach lub wydarzeniach: nadchodzić, mieć wkrótce nastąpić.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_events",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chyba <w>zbliża się</w> gwałtowna burza.",
                            "target_lang_translations": {
                                "ru": "Кажется, <w>надвигается</w> сильная гроза."
                            }
                        },
                        {
                            "text": "Szybko <w>zbliża się</w> termin oddania ważnych dokumentów.",
                            "target_lang_translations": {
                                "ru": "Быстро <w>приближается</w> срок сдачи важных документов."
                            }
                        }
                    ],
                    "synonyms": [
                        "nadchodzić"
                    ],
                    "common_phrases": [
                        "zbliża się noc",
                        "zbliża się koniec",
                        "czas się zbliża"
                    ],
                    "target_lang_definitions": {
                        "ru": "О времени, явлениях или событиях: наступать, скоро произойти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приближаться",
                                "target_lang_sense_clarification": "о времени или событии"
                            },
                            {
                                "target_lang_word": "надвигаться",
                                "target_lang_sense_clarification": "о чем-то неизбежном или угрожающем (напр. буря)"
                            },
                            {
                                "target_lang_word": "наступать",
                                "target_lang_sense_clarification": "приходить (о времени)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cde16631-6ee7-3d8b-98f6-a41a3a4e0f98",
                    "sense_definition": "Zmniejszać odległość między obiektami poprzez przesuwanie ich ku sobie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna powoli <w>zbliżyła</w> rękę do ust.",
                            "target_lang_translations": {
                                "ru": "Девушка медленно <w>поднесла</w> руку ко рту."
                            }
                        },
                        {
                            "text": "Ostrożnie <w>zbliżał</w> magnes do metalowej płytki.",
                            "target_lang_translations": {
                                "ru": "Он осторожно <w>приближал</w> магнит к металлической пластине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbliżać do siebie",
                        "zbliżać rękę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшать расстояние между объектами, перемещая их друг к другу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приближать",
                                "target_lang_sense_clarification": "перемещать ближе"
                            },
                            {
                                "target_lang_word": "подносить",
                                "target_lang_sense_clarification": "поднимать или перемещать близко к чему-то (напр. руку)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da4297cf-109b-32ba-9a0f-5b07ec943553",
                    "sense_definition": "Nawiązywać bliższe relacje, pogłębiać więzi emocjonalne lub stawać się sobie bliższym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relationships",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wspólne wakacje bardzo ich do siebie <w>zbliżyły</w>.",
                            "target_lang_translations": {
                                "ru": "Совместный отпуск их очень <w>сблизил</w>."
                            }
                        },
                        {
                            "text": "W trudnych chwilach ludzie często <w>zbliżają się</w> do rodziny.",
                            "target_lang_translations": {
                                "ru": "В трудные моменты люди часто <w>сближаются</w> с семьей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zbliżyć się do siebie",
                        "ludzie się zbliżają"
                    ],
                    "target_lang_definitions": {
                        "ru": "Налаживать более близкие отношения, углублять эмоциональные связи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сближать",
                                "target_lang_sense_clarification": "делать отношения более тесными"
                            },
                            {
                                "target_lang_word": "сближаться",
                                "target_lang_sense_clarification": "становиться ближе эмоционально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6153fad-5597-3844-8814-44f37b4b200c",
                    "sense_definition": "Stawać się podobnym do czegoś lub wykazywać cechy wspólne z czymś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "similarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowatorstwo myśli <w>zbliża</w> go do nurtu awangardowego.",
                            "target_lang_translations": {
                                "ru": "Новаторство мысли <w>сближает</w> его с авангардным течением."
                            }
                        },
                        {
                            "text": "Jego styl pisania <w>zbliża</w> tę powieść do reportażu.",
                            "target_lang_translations": {
                                "ru": "Его стиль письма <w>приближает</w> этот роман к репортажу."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться похожим на что-либо или проявлять общие черты с чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сближать",
                                "target_lang_sense_clarification": "находить или создавать сходство"
                            },
                            {
                                "target_lang_word": "делать похожим",
                                "target_lang_sense_clarification": "придавать сходство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
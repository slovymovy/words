    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a569d737-d3b7-3ea9-8faa-e4b02031c74c",
                    "sense_definition": "The present participle and gerund of <w>pack</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is currently <w>packing</w> her suitcase for the holiday.",
                            "target_lang_translations": {
                                "ru": "Она сейчас <w>укладывает</w> свой чемодан для отпуска."
                            }
                        },
                        {
                            "text": "They were busy <w>packing</w> the books into large boxes.",
                            "target_lang_translations": {
                                "ru": "Они были заняты <w>упаковыванием</w> книг в большие коробки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола pack."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упаковывание",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "укладывание",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c0ef10b-40db-3b4c-ba2f-0f1499a13ecc",
                    "sense_definition": "The action of putting clothes and other items into bags or cases for a journey.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to finish my <w>packing</w> tonight because the flight is early tomorrow.",
                            "target_lang_translations": {
                                "ru": "Мне нужно закончить <w>сборы</w> сегодня вечером, потому что вылет завтра рано."
                            }
                        },
                        {
                            "text": "He hates <w>packing</w> and always leaves it until the last minute.",
                            "target_lang_translations": {
                                "ru": "Он ненавидит <w>собирать вещи</w> и всегда откладывает это на последнюю минуту."
                            }
                        }
                    ],
                    "antonyms": [
                        "unpacking"
                    ],
                    "common_phrases": [
                        "do the packing",
                        "finish packing",
                        "start packing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по укладыванию одежды и других вещей в сумки или чемоданы для поездки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сборы",
                                "target_lang_sense_clarification": "приготовления к отъезду"
                            },
                            {
                                "target_lang_word": "укладывание вещей",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08aec0d9-a597-34df-864f-9220563fa421",
                    "sense_definition": "Material used to wrap or protect goods that are being transported or sold.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "material_protection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The vase arrived broken because there was not enough <w>packing</w> in the box.",
                            "target_lang_translations": {
                                "ru": "Ваза пришла разбитой, потому что в коробке было недостаточно <w>упаковочного материала</w>."
                            }
                        },
                        {
                            "text": "Please remove all the foam <w>packing</w> before you turn on the machine.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, удалите всю пенопластовую <w>упаковку</w> перед включением машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "packaging",
                        "wrapping",
                        "cushioning"
                    ],
                    "common_phrases": [
                        "packing material",
                        "protective packing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материал, используемый для обертывания или защиты товаров при транспортировке или продаже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упаковочный материал",
                                "target_lang_sense_clarification": "защитный материал"
                            },
                            {
                                "target_lang_word": "набивка",
                                "target_lang_sense_clarification": "заполнитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4fff24d-aefe-305b-8ab0-0dd552c1845e",
                    "sense_definition": "A fee charged to cover the costs of wrapping and preparing goods for delivery.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The total price includes five dollars for postage and <w>packing</w>.",
                            "target_lang_translations": {
                                "ru": "Общая цена включает пять долларов за пересылку и <w>упаковку</w>."
                            }
                        },
                        {
                            "text": "We offer free <w>packing</w> on all orders over a certain amount.",
                            "target_lang_translations": {
                                "ru": "Мы предлагаем бесплатную <w>упаковку</w> для всех заказов на определенную сумму."
                            }
                        }
                    ],
                    "common_phrases": [
                        "postage and packing",
                        "shipping and packing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плата, взимаемая за покрытие расходов на упаковку и подготовку товаров к доставке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упаковка",
                                "target_lang_sense_clarification": "услуга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b65bb35-9c7c-3ffc-8b7a-8346b4ce66f6",
                    "sense_definition": "Material used to fill a space in machinery to make it watertight or airtight.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material_sealant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic had to replace the worn <w>packing</w> to stop the leak.",
                            "target_lang_translations": {
                                "ru": "Механику пришлось заменить изношенную <w>набивку</w>, чтобы остановить течь."
                            }
                        },
                        {
                            "text": "Water was escaping through the defective <w>packing</w> around the valve stem.",
                            "target_lang_translations": {
                                "ru": "Вода вытекала через неисправное <w>уплотнение</w> вокруг штока клапана."
                            }
                        }
                    ],
                    "synonyms": [
                        "stuffing",
                        "seal"
                    ],
                    "common_phrases": [
                        "gland packing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материал, используемый для заполнения пространства в механизмах для обеспечения водонепроницаемости или герметичности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набивка",
                                "target_lang_sense_clarification": "сальниковая"
                            },
                            {
                                "target_lang_word": "уплотнение",
                                "target_lang_sense_clarification": "герметизация"
                            }
                        ]
                    }
                }
            ]
        }
    ]

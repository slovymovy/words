    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "728cc48e-f87d-3c65-9cda-2a07e55c6974",
                    "sense_definition": "w ciągu jednego dnia, w przeliczeniu na dobę; określa ilość lub częstotliwość zdarzeń przypadających na jeden dzień",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił mi przyjmowanie tego leku dwa razy <w>dziennie</w>.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал мне принимать это лекарство два раза <w>в день</w>."
                            }
                        },
                        {
                            "text": "Mój brat pracuje osiem godzin <w>dziennie</w> w dużej firmie.",
                            "target_lang_translations": {
                                "ru": "Мой брат работает по восемь часов <w>в день</w> в крупной фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "na dzień"
                    ],
                    "common_phrases": [
                        "raz dziennie",
                        "dwa razy dziennie",
                        "godzin dziennie",
                        "kilka razy dziennie"
                    ],
                    "target_lang_definitions": {
                        "ru": "в день, в течение дня, в расчете на сутки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в день",
                                "target_lang_sense_clarification": "указывает на частоту или количество в течение одного дня"
                            },
                            {
                                "target_lang_word": "ежедневно",
                                "target_lang_sense_clarification": "каждый день"
                            },
                            {
                                "target_lang_word": "в сутки",
                                "target_lang_sense_clarification": "в течение 24 часов (часто в медицинском контексте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb89e2a4-92b1-33bc-9d56-6a1e420af4c6",
                    "sense_definition": "w trybie stacjonarnym; dotyczy systemu nauki odbywającej się w ciągu dnia, a nie wieczorami lub w weekendy",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Większość moich znajomych studiuje <w>dziennie</w>, więc mają wolne wieczory.",
                            "target_lang_translations": {
                                "ru": "Большинство моих знакомых учится <w>на дневном</w>, поэтому вечера у них свободны."
                            }
                        },
                        {
                            "text": "Ona zdecydowała się uczyć <w>dziennie</w>, aby szybciej skończyć studia.",
                            "target_lang_translations": {
                                "ru": "Она решила учиться <w>на дневном</w>, чтобы быстрее закончить учёбу."
                            }
                        }
                    ],
                    "antonyms": [
                        "zaocznie",
                        "wieczorowo"
                    ],
                    "common_phrases": [
                        "studiować dziennie",
                        "uczyć się dziennie"
                    ],
                    "target_lang_definitions": {
                        "ru": "на дневном отделении, очно (об учебе)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на дневном",
                                "target_lang_sense_clarification": "форма обучения в дневное время (сокращение от «на дневном отделении»)"
                            },
                            {
                                "target_lang_word": "очно",
                                "target_lang_sense_clarification": "форма обучения с личным присутствием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33dd3a45-5734-3d3a-b450-9058b193c921",
                    "sense_definition": "każdego dnia, codziennie (używane w gwarze)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz <w>dziennie</w> rano wychodził w pole, by doglądać upraw.",
                            "target_lang_translations": {
                                "ru": "Хозяин <w>каждый день</w> утром выходил в поле, чтобы присматривать за посевами."
                            }
                        },
                        {
                            "text": "Oni przychodzą tu <w>dziennie</w>, żeby porozmawiać z sąsiadami.",
                            "target_lang_translations": {
                                "ru": "Они приходят сюда <w>каждый день</w>, чтобы поговорить с соседями."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Warmia"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "gwara"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "каждый день (региональное или устаревшее значение)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каждый день",
                                "target_lang_sense_clarification": "синоним слова «codziennie»"
                            }
                        ]
                    }
                }
            ]
        }
    ]

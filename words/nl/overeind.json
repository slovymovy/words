    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3b83fa01-e897-32d0-af54-d136cd27d3aa",
                    "sense_definition": "In een verticale, rechtopstaande positie; niet omgevallen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state_vertical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het oude gebouw staat na al die jaren nog steeds <w>overeind</w>.",
                            "target_lang_translations": {
                                "en": "The old building is still <w>standing</w> after all those years.",
                                "ru": "Старое здание после всех этих лет всё ещё <w>стоит</w>."
                            }
                        },
                        {
                            "text": "Ondanks de zware storm bleef de grote boom in de tuin <w>overeind</w>.",
                            "target_lang_translations": {
                                "en": "Despite the heavy storm, the large tree in the garden remained <w>standing</w>.",
                                "ru": "Несмотря на сильный шторм, большое дерево в саду <w>устояло</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rechtop",
                        "staande"
                    ],
                    "antonyms": [
                        "omver",
                        "plat"
                    ],
                    "common_phrases": [
                        "overeind blijven",
                        "overeind staan",
                        "overeind houden"
                    ],
                    "target_lang_definitions": {
                        "en": "In a vertical, upright position; not fallen over.",
                        "ru": "В вертикальном, стоячем положении; не упав."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "upright",
                                "target_lang_sense_clarification": "vertical position"
                            },
                            {
                                "target_lang_word": "standing",
                                "target_lang_sense_clarification": "remaining vertical"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стоя",
                                "target_lang_sense_clarification": "находиться в вертикальном положении"
                            },
                            {
                                "target_lang_word": "вертикально",
                                "target_lang_sense_clarification": "прямо, перпендикулярно земле"
                            },
                            {
                                "target_lang_word": "на ногах",
                                "target_lang_sense_clarification": "устоять, не упасть (часто переносно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98484942-fb59-3613-abe0-274ae617b736",
                    "sense_definition": "Een beweging makend naar een verticale positie; opstaan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_vertical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De speler kwam na de overtreding moeizaam weer <w>overeind</w>.",
                            "target_lang_translations": {
                                "en": "The player struggled to get back <w>up</w> after the foul.",
                                "ru": "Игрок с трудом <w>поднялся</w> после нарушения."
                            }
                        },
                        {
                            "text": "Ze schrok wakker van het geluid en zat direct <w>overeind</w> in haar bed.",
                            "target_lang_translations": {
                                "en": "She was startled awake by the noise and immediately sat <w>up</w> in her bed.",
                                "ru": "Она проснулась от звука и тут же <w>села</w> в кровати."
                            }
                        }
                    ],
                    "synonyms": [
                        "omhoog",
                        "op"
                    ],
                    "antonyms": [
                        "neer",
                        "omlaag"
                    ],
                    "common_phrases": [
                        "overeind komen",
                        "overeind krabbelen",
                        "overeind helpen",
                        "overeind schieten"
                    ],
                    "target_lang_definitions": {
                        "en": "Moving into a vertical position; getting up.",
                        "ru": "Движение в вертикальное положение; вставание или приподнимание."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "up",
                                "target_lang_sense_clarification": "movement to vertical position"
                            },
                            {
                                "target_lang_word": "to one's feet",
                                "target_lang_sense_clarification": "standing up"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "на ноги",
                                "target_lang_sense_clarification": "встать, подняться"
                            },
                            {
                                "target_lang_word": "вверх",
                                "target_lang_sense_clarification": "направление движения при вставании"
                            },
                            {
                                "target_lang_word": "приподняться",

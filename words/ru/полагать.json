    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f1d520d5-b3fe-3458-97a5-ca5c572fb8f8",
                    "sense_definition": "Думать, считать, иметь мнение или суждение о чём-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>полагаю</w>, что мы сможем решить эту проблему завтра.",
                            "target_lang_translations": {
                                "en": "I <w>suppose</w> that we can solve this problem tomorrow."
                            }
                        },
                        {
                            "text": "Надо <w>полагать</w>, что погода скоро улучшится и мы поедем.",
                            "target_lang_translations": {
                                "en": "One must <w>suppose</w> that the weather will improve soon and we will go."
                            }
                        }
                    ],
                    "synonyms": [
                        "думать",
                        "считать"
                    ],
                    "common_phrases": [
                        "надо полагать",
                        "смею полагать",
                        "есть основания полагать"
                    ],
                    "target_lang_definitions": {
                        "en": "To think, consider, or have an opinion or judgment about something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suppose",
                                "target_lang_sense_clarification": "to assume or consider probable"
                            },
                            {
                                "target_lang_word": "believe",
                                "target_lang_sense_clarification": "to hold as an opinion"
                            },
                            {
                                "target_lang_word": "deem",
                                "target_lang_sense_clarification": "formal - to consider or judge"
                            },
                            {
                                "target_lang_word": "reckon",
                                "target_lang_sense_clarification": "informal - to think or suppose"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fee5f74-1283-3102-84a6-2d646e888a73",
                    "sense_definition": "Устанавливать, определять (обычно в сочетании с существительными «начало», «конец», «предел»).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это радостное событие <w>полагает</w> начало новой исторической эпохе.",
                            "target_lang_translations": {
                                "en": "This joyful event <w>marks</w> the beginning of a new historical era."
                            }
                        },
                        {
                            "text": "Смерть неизбежно <w>полагает</w> конец всем земным страданиям.",
                            "target_lang_translations": {
                                "en": "Death inevitably <w>puts</w> an end to all earthly sufferings."
                            }
                        }
                    ],
                    "synonyms": [
                        "класть",
                        "определять"
                    ],
                    "common_phrases": [
                        "полагать начало",
                        "полагать конец",
                        "полагать предел"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish or determine (usually in combination with nouns like \"beginning\", \"end\", \"limit\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put",
                                "target_lang_sense_clarification": "used in phrases like 'put an end to'"
                            },
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "to establish limits or boundaries"
                            },
                            {
                                "target_lang_word": "mark",
                                "target_lang_sense_clarification": "to indicate a beginning or end"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e0a6091-4c03-3f26-9ab1-ea4673809345",
                    "sense_definition": "Прикладывать, затрачивать (силы, старания, усилия).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>полагал</w> огромные усилия для достижения своей заветной цели.",
                            "target_lang_translations": {
                                "en": "He <w>exerted</w> huge efforts to achieve his cherished goal."
                            }
                        },
                        {
                            "text": "Не стоит <w>полагать</w> столько труда на это бесполезное дело.",
                            "target_lang_translations": {
                                "en": "It is not worth <w>expending</w> so much labor on this useless matter."
                            }
                        }
                    ],
                    "synonyms": [
                        "прилагать",
                        "затрачивать"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "устаревшее значение"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To apply or expend (strength, efforts, endeavors)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expend",
                                "target_lang_sense_clarification": "to use up resources or effort"
                            },
                            {
                                "target_lang_word": "exert",
                                "target_lang_sense_clarification": "to put forth strength or effort"
                            },
                            {
                                "target_lang_word": "put",
                                "target_lang_sense_clarification": "to apply effort or labor"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
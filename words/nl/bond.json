            "senses": [
                {
                    "sense_id": "4f914c1b-a4a6-3095-884a-e045f7c8a0c6",
                    "sense_definition": "Een vereniging of organisatie waarin mensen of groepen samenwerken voor een gemeenschappelijk doel of belang, zoals een vakbond of sportbond.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_association",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader is al jaren lid van de <w>bond</w>.",
                            "target_lang_translations": {
                                "en": "My father has been a member of the <w>union</w> for years.",
                                "ru": "Мой отец уже много лет является членом <w>союза</w>."
                            }
                        },
                        {
                            "text": "De <w>bond</w> heeft opgeroepen tot een staking voor betere lonen.",
                            "target_lang_translations": {
                                "en": "The <w>union</w> has called for a strike for better wages.",
                                "ru": "<w>Профсоюз</w> призвал к забастовке ради повышения зарплат."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbond"
                    ],
                    "common_phrases": [
                        "bij de bond",
                        "lid van de bond"
                    ],
                    "target_lang_definitions": {
                        "en": "An organization or association where people or groups work together for a common goal, such as a trade union or sports federation.",
                        "ru": "Объединение или организация, в которой люди или группы сотрудничают ради общей цели, например, профсоюз или спортивная федерация."
                    },
                    "translations": {

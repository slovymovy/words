{
    "word_family": [],
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "0425525c-e092-3a0c-84de-7e0dfb80417e",
                    "sense_definition": "Used to connect words, phrases, or parts of sentences that belong together.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I like apples <w>and</w> oranges.",
                            "target_lang_translations": {
                                "ru": "Я люблю яблоки <w>и</w> апельсины."
                            }
                        },
                        {
                            "text": "She bought a shirt <w>and</w> a pair of shoes.",
                            "target_lang_translations": {
                                "ru": "Она купила рубашку <w>и</w> пару туфель."
                            }
                        }
                    ],
                    "synonyms": [
                        "as well as",
                        "together with",
                        "in addition to",
                        "&",
                        "'n'",
                        "+",
                        "x"
                    ],
                    "common_phrases": [
                        "black and white",
                        "bread and butter",
                        "come and go",
                        "wait and see"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для соединения слов, фраз или частей предложения, которые связаны по смыслу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "соединительный союз"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "разговорный или фольклорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4861c583-9864-3f2c-a23b-8a7f43a447cd",
                    "sense_definition": "Used to join two clauses or sentences, often to show sequence or result.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He opened the door <w>and</w> walked out.",
                            "target_lang_translations": {
                                "ru": "Он открыл дверь <w>и</w> вышел."
                            }
                        },
                        {
                            "text": "The sun came out <w>and</w> the ice melted.",
                            "target_lang_translations": {
                                "ru": "Выглянуло солнце, <w>и</w> лед растаял."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для соединения двух предложений, часто указывая на последовательность действий или результат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "последовательность"
                            },
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "противопоставление или смена действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d1f9bcd-0ada-3bd6-800e-854d698ca49c",
                    "sense_definition": "Used to add numbers together.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Two <w>and</w> two make four.",
                            "target_lang_translations": {
                                "ru": "Два <w>и</w> два будет четыре."
                            }
                        },
                        {
                            "text": "It cost five pounds <w>and</w> fifty pence.",
                            "target_lang_translations": {
                                "ru": "Это стоило пять фунтов <w>и</w> пятьдесят пенсов."
                            }
                        }
                    ],
                    "synonyms": [
                        "+"
                    ],
                    "common_phrases": [
                        "two and two"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется при сложении чисел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "в математических выражениях"
                            },
                            {
                                "target_lang_word": "плюс",
                                "target_lang_sense_clarification": "математический знак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29b826de-30e3-349e-9d4c-9a8d4bd1dfe7",
                    "sense_definition": "Used instead of 'to' after verbs like 'try', 'come', or 'go' to indicate a purpose.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "idiomatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will try <w>and</w> finish this today.",
                            "target_lang_translations": {
                                "ru": "Я постараюсь закончить это сегодня."
                            }
                        },
                        {
                            "text": "Come <w>and</w> see what I found.",
                            "target_lang_translations": {
                                "ru": "Приходи <w>и</w> посмотри, что я нашел."
                            }
                        }
                    ],
                    "common_phrases": [
                        "try and",
                        "come and",
                        "go and",
                        "wait and"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется вместо частицы 'to' после глаголов типа 'try', 'come' или 'go' для указания цели действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "после глаголов движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a02dda7d-007e-3eba-bf54-33c00f7ef645",
                    "sense_definition": "Used to connect the same word twice to emphasize duration, repetition, or intensity.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "idiomatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He ran miles <w>and</w> miles.",
                            "target_lang_translations": {
                                "ru": "Он бежал многие <w>и</w> многие мили."
                            }
                        },
                        {
                            "text": "It rained <w>and</w> rained all night.",
                            "target_lang_translations": {
                                "ru": "Дождь всё шел <w>и</w> шел всю ночь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "again and again",
                        "on and on",
                        "over and over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для соединения повторяющихся слов с целью подчеркнуть длительность или интенсивность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "усиление значения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2aa49d88-529f-3d69-82f0-98a11bef7921",
                    "sense_definition": "If; provided that.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_conditional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>And</w> it please you, I will leave now.",
                            "target_lang_translations": {
                                "ru": "<w>Если</w> вам будет угодно, я сейчас уйду."
                            }
                        },
                        {
                            "text": "<w>And</w> I were a rich man, I would travel the world.",
                            "target_lang_translations": {
                                "ru": "<w>Если</w> бы я был богачом, я бы путешествовал по миру."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Used in older literature or dialects."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Если; при условии, что (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "если",
                                "target_lang_sense_clarification": "устаревшее значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f9902a93-0a25-323b-9ade-ec6aca8674c0",
                    "sense_definition": "A logical operator that is true only if all inputs are true.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The proof examines the definition of both OR and <w>AND</w>.",
                            "target_lang_translations": {
                                "ru": "Доказательство рассматривает определение как ИЛИ, так и <w>И</w>."
                            }
                        },
                        {
                            "text": "In boolean logic, the <w>AND</w> function outputs true only when both operands are true.",
                            "target_lang_translations": {
                                "ru": "В булевой логике функция <w>И</w> возвращает истину только тогда, когда оба операнда истинны."
                            }
                        }
                    ],
                    "antonyms": [
                        "NAND"
                    ],
                    "target_lang_definitions": {
                        "ru": "Логический оператор, который истинен только в том случае, если истинны все входные данные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "И",
                                "target_lang_sense_clarification": "логический оператор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9423fb78-f860-37f9-b691-9f8de2a45224",
                    "sense_definition": "The second half of a divided beat in rhythm.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musicians often count one <w>and</w> two <w>and</w>.",
                            "target_lang_translations": {
                                "ru": "Музыканты часто считают: раз <w>и</w> два <w>и</w>."
                            }
                        },
                        {
                            "text": "The note starts on the <w>and</w> of the third beat.",
                            "target_lang_translations": {
                                "ru": "Нота начинается на счет <w>и</w> третьей доли."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вторая половина доли в музыкальном ритме (слабая доля)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и",
                                "target_lang_sense_clarification": "счет в музыке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6d852016-1f93-3033-b2e0-12430ef88996",
                    "sense_definition": "To combine a value with another value using the logical AND operator.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software <w>ANDs</w> the two binary values.",
                            "target_lang_translations": {
                                "ru": "Программа <w>выполняет логическое И</w> над двумя двоичными значениями."
                            }
                        },
                        {
                            "text": "The mask is <w>ANDed</w> with the key to filter the results.",
                            "target_lang_translations": {
                                "ru": "Маска <w>логически умножается</w> с ключом для фильтрации результатов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединять значения с помощью логического оператора И."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выполнять логическое И",
                                "target_lang_sense_clarification": "информатика"
                            },
                            {
                                "target_lang_word": "логически умножать",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e0c62bc6-89a5-3475-9061-d99907007d02",
                    "sense_definition": "Faza Księżyca, w której jego tarcza jest w całości oświetlona przez Słońce i widoczna z Ziemi",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "astronomy_phase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziś w nocy będzie <w>pełnia</w>, więc niebo będzie bardzo jasne.",
                            "target_lang_translations": {
                                "ru": "Сегодня ночью будет <w>полнолуние</w>, поэтому небо будет очень светлым."
                            }
                        },
                        {
                            "text": "Często nie mogę spać, gdy na niebie występuje <w>pełnia</w>.",
                            "target_lang_translations": {
                                "ru": "Я часто не могу спать, когда на небе <w>полнолуние</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "nów"
                    ],
                    "common_phrases": [
                        "pełnia Księżyca",
                        "podczas pełni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фаза Луны, при которой её диск полностью освещён Солнцем и виден с Земли"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полнолуние",
                                "target_lang_sense_clarification": "фаза луны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a47bc7c-f546-31cd-8579-304130d20826",
                    "sense_definition": "Najwyższy stopień nasilenia jakiegoś zjawiska, uczucia lub szczytowy moment trwania czegoś",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_fullness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dniu ślubu widać było, że panna młoda jest w <w>pełni</w> szczęścia.",
                            "target_lang_translations": {
                                "ru": "В день свадьбы было видно, что невеста ощущает <w>полноту</w> счастья."
                            }
                        },
                        {
                            "text": "Mamy teraz <w>pełnię</w> lata, więc dni są bardzo długie i gorące.",
                            "target_lang_translations": {
                                "ru": "Сейчас у нас <w>разгар</w> лета, поэтому дни очень длинные и жаркие."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pełnia lata",
                        "pełnia szczęścia",
                        "pełnia sezonu",
                        "w pełni sił"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высшая степень проявления какого-либо явления, чувства или пиковый момент чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полнота",
                                "target_lang_sense_clarification": "чувства, ощущения"
                            },
                            {
                                "target_lang_word": "разгар",
                                "target_lang_sense_clarification": "сезона, события"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10bb7bd1-5ef9-3114-ac5d-b2a9252d26b7",
                    "sense_definition": "Stan kompletności, w którym niczego nie brakuje i wszystko stanowi całość",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_fullness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dopiero po wielu latach docenił <w>pełnię</w> możliwości, jakie dała mu ta praca.",
                            "target_lang_translations": {
                                "ru": "Только спустя много лет он оценил <w>полноту</w> возможностей, которые дала ему эта работа."
                            }
                        },
                        {
                            "text": "Nowy prezydent przejął wczoraj <w>pełnię</w> władzy w państwie.",
                            "target_lang_translations": {
                                "ru": "Новый президент вчера принял на себя всю <w>полноту</w> власти в государстве."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w całej pełni",
                        "pełnia praw",
                        "pełnia władzy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние целостности, когда ничего не не хватает и всё составляет единое целое"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полнота",
                                "target_lang_sense_clarification": "власти, прав, возможностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c43ebe6-3620-3a5c-af9c-bda4c220b25b",
                    "sense_definition": "Otwarta, szeroka przestrzeń wodna na morzu lub jeziorze, znajdująca się z dala od brzegu",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mała łódka rybacka odważyła się wypłynąć na szeroką <w>pełnię</w>.",
                            "target_lang_translations": {
                                "ru": "Маленькая рыбацкая лодка отважилась выйти в <w>открытое море</w>."
                            }
                        },
                        {
                            "text": "Z wysokiego klifu widać było statki płynące przez morską <w>pełnię</w>.",
                            "target_lang_translations": {
                                "ru": "С высокого утеса было видно корабли, плывущие через морской <w>простор</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wypłynąć na pełnię"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытое, широкое водное пространство на море или озере, вдали от берега"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытое море",
                                "target_lang_sense_clarification": "водное пространство"
                            },
                            {
                                "target_lang_word": "простор",
                                "target_lang_sense_clarification": "широкое пространство"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "diep",
        "diepte",
        "verdiepen",
        "uitdiepen",
        "diepgang",
        "diepzee"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "090647a4-9f72-34bf-ae22-a561f09e65c6",
                    "sense_definition": "Het moment of de toestand waarin een situatie op zijn slechtst is.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_low",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De sfeer in het team bereikte een <w>dieptepunt</w> na de derde nederlaag op rij.",
                            "target_lang_translations": {
                                "en": "The atmosphere in the team reached a <w>low point</w> after the third defeat in a row.",
                                "ru": "Атмосфера в команде достигла <w>нижней точки</w> после третьего поражения подряд."
                            }
                        },
                        {
                            "text": "Zijn ontslag was voor hem het absolute <w>dieptepunt</w> van het afgelopen jaar.",
                            "target_lang_translations": {
                                "en": "His dismissal was the absolute <w>low point</w> of the past year for him.",
                                "ru": "Его увольнение стало для него абсолютным <w>дном</w> прошедшего года."
                            }
                        }
                    ],
                    "antonyms": [
                        "hoogtepunt"
                    ],
                    "common_phrases": [
                        "een dieptepunt bereiken",
                        "een historisch dieptepunt",
                        "absoluut dieptepunt"
                    ],
                    "target_lang_definitions": {
                        "en": "The moment or state when a situation is at its worst.",
                        "ru": "Момент или состояние, когда ситуация находится в наихудшем положении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "low point",
                                "target_lang_sense_clarification": "worst moment in a situation"
                            },
                            {
                                "target_lang_word": "rock bottom",
                                "target_lang_sense_clarification": "absolute worst state"
                            },
                            {
                                "target_lang_word": "nadir",
                                "target_lang_sense_clarification": "formal; lowest point"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нижняя точка",
                                "target_lang_sense_clarification": "наихудший момент в развитии ситуации"
                            },
                            {
                                "target_lang_word": "дно",
                                "target_lang_sense_clarification": "предельно низкое, плохое состояние (переносное значение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4e337a6-575c-33c0-beef-6c87772059a0",
                    "sense_definition": "De laagste waarde, stand of fysieke positie die iets bereikt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_low",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De temperatuur daalde vannacht tot een <w>dieptepunt</w> van min tien graden.",
                            "target_lang_translations": {
                                "en": "The temperature dropped last night to a <w>low</w> of minus ten degrees.",
                                "ru": "Прошлой ночью температура опустилась до <w>минимума</w> в минус десять градусов."
                            }
                        },
                        {
                            "text": "De koers van het aandeel zakte gisteren naar een nieuw <w>dieptepunt</w>.",
                            "target_lang_translations": {
                                "en": "The share price dropped yesterday to a new <w>low</w>.",
                                "ru": "Вчера курс акций упал до нового <w>минимума</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "hoogtepunt"
                    ],
                    "common_phrases": [
                        "op een dieptepunt staan"
                    ],
                    "target_lang_definitions": {
                        "en": "The lowest value, level, or physical position reached by something.",
                        "ru": "Самое низкое значение, уровень или физическая позиция, достигнутая чем-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "low",
                                "target_lang_sense_clarification": "lowest level or value (e.g. temperature, stocks)"
                            },
                            {
                                "target_lang_word": "minimum",
                                "target_lang_sense_clarification": "lowest quantity or degree"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "минимум",
                                "target_lang_sense_clarification": "самое низкое значение (температуры, цен и т.д.)"
                            },
                            {
                                "target_lang_word": "нижний предел",
                                "target_lang_sense_clarification": "самая низкая возможная точка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
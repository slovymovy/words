                {
                    "sense_id": "56f35afa-0c1d-3ba1-92c2-8d8dbd172b81",
                    "sense_definition": "Complete, absolute, or total; used to emphasize a quality or state, usually a negative one.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "adj_absolute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The party was an <w>utter</w> disaster from start to finish.",
                            "target_lang_translations": {
                                "ru": "Вечеринка была <w>по́лной</w> катастрофой от начала до конца."
                            }
                        },
                        {
                            "text": "She looked at him in <w>utter</w> amazement when he told the story.",
                            "target_lang_translations": {
                                "ru": "Она смотрела на него в <w>по́лном</w> изумлении, когда он рассказывал эту историю."
                            }
                        },
                        {
                            "text": "It is <w>utter</w> nonsense to suggest that the earth is flat.",
                            "target_lang_translations": {
                                "ru": "Это <w>абсолю́тная</w> чепуха — предполагать, что земля плоская."
                            }
                        }
                    ],
                    "synonyms": [
                        "absolute",
                        "complete",
                        "total",
                        "sheer",
                        "thorough",
                        "unmitigated",
                        "downright",
                        "out-and-out",
                        "arrant",
                        "pure",
                        "rank"
                    ],
                    "common_phrases": [
                        "utter chaos",
                        "utter nonsense",
                        "utter darkness",
                        "utter silence",
                        "utter despair"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный, абсолютный или тотальный; используется для усиления качества или состояния, обычно негативного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по́лный",
                                "target_lang_sense_clarification": "всеобъемлющий, завершённый"
                            },
                            {
                                "target_lang_word": "абсолю́тный",
                                "target_lang_sense_clarification": "безусловный, совершенный"
                            },
                            {
                                "target_lang_word": "соверше́нный",
                                "target_lang_sense_clarification": "истинный, настоящий (часто с негативным оттенком)"

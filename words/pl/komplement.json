                {
                    "sense_id": "c80d7f09-265a-32bb-bfa2-7add8dece90c",
                    "sense_definition": "Grzecznościowa wypowiedź wyrażająca uznanie, podziw lub pochwałę dla kogoś, często mająca na celu sprawienie przyjemności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "praise_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna prawił jej <komplementy> przez cały wieczór, co bardzo jej schlebiało.",
                            "target_lang_translations": {
                                "ru": "Мужчина говорил ей <комплименты> весь вечер, что ей очень льстило."
                            }
                        },
                        {
                            "text": "To był najmilszy <komplement>, jaki kiedykolwiek usłyszałam od szefa.",
                            "target_lang_translations": {
                                "ru": "Это был самый приятный <комплимент>, который я когда-либо слышала от начальника."
                            }
                        }
                    ],
                    "synonyms": [
                        "pochwała",
                        "pochlebstwo",
                        "duser"
                    ],
                    "antonyms": [
                        "krytyka",
                        "nagana"
                    ],
                    "common_phrases": [
                        "prawić komplementy",
                        "sypać komplementami",
                        "łowić komplementy",
                        "fałszywy komplement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливое высказывание, выражающее признание, восхищение или похвалу кому-либо, часто с целью сделать приятное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комплимент",
                                "target_lang_sense_clarification": "похвала, любезность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afd2a4ad-dba0-39e7-8bdb-194764ec0527",
                    "sense_definition": "W immunologii: zespół białek osocza krwi biorący udział w mechanizmach odporności nieswoistej organizmu.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biology_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aktywacja <komplementu> jest kluczowym elementem odpowiedzi immunologicznej na infekcję.",

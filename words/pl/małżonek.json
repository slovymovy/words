    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ed70072-3e63-399d-8719-cd88eff8935b",
                    "sense_definition": "Mężczyzna pozostający w związku małżeńskim; formalne lub uprzejme określenie męża.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "husband",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pani ambasador z <w>małżonkiem</w> zaprosili nas na bal.",
                            "target_lang_translations": {
                                "ru": "Госпожа посол с <w>супругом</w> пригласили нас на бал."
                            }
                        },
                        {
                            "text": "Jej <w>małżonek</w> pracuje jako lekarz w szpitalu miejskim.",
                            "target_lang_translations": {
                                "ru": "Её <w>супруг</w> работает врачом в городской больнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "mąż"
                    ],
                    "antonyms": [
                        "żona"
                    ],
                    "common_phrases": [
                        "drogi małżonek",
                        "przyszły małżonek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, состоящий в браке; официальное или вежливое название мужа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "супруг",
                                "target_lang_sense_clarification": "официально"
                            },
                            {
                                "target_lang_word": "муж",
                                "target_lang_sense_clarification": "нейтрально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b122fa88-a9a4-3a39-9eb3-9509cda1fc3e",
                    "sense_definition": "Osoba pozostająca w związku małżeńskim (mąż lub żona); termin używany często w kontekście urzędowym i prawnym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spouse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tę rubrykę trzeba wpisać dane <w>małżonka</w>.",
                            "target_lang_translations": {
                                "ru": "В эту графу нужно вписать данные <w>супруга</w>."
                            }
                        },
                        {
                            "text": "Do wniosku kredytowego wymagana jest pisemna zgoda <w>małżonka</w>.",
                            "target_lang_translations": {
                                "ru": "Для заявки на кредит требуется письменное согласие <w>супруга</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "współmałżonek"
                    ],
                    "common_phrases": [
                        "dane małżonka",
                        "podpis małżonka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, состоящее в браке (муж или жена); термин, часто используемый в официальном и юридическом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "супруг",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8984b833-0e06-3c8f-9888-14a11950904a",
                    "sense_definition": "Dopełniacz liczby mnogiej od słowa <w>małżonka</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na przyjęciu nie było wielu <w>małżonek</w> pracowników.",
                            "target_lang_translations": {
                                "ru": "На приёме не было многих <w>супруг</w> сотрудников."
                            }
                        },
                        {
                            "text": "To spotkanie jest przeznaczone wyłącznie dla <w>małżonek</w> dyplomatów.",
                            "target_lang_translations": {
                                "ru": "Эта встреча предназначена исключительно для <w>супруг</w> дипломатов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа от слова «małżonka» (супруга)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "супруга",
                                "target_lang_sense_clarification": "форма мн. ч. род. п. — супруг"
                            }
                        ]
                    }
                }
            ]
        }
    ]

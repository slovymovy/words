    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e715384-f19b-3df8-98a5-f4f8ad33ac1c",
                    "sense_definition": "The actual facts or information about something, rather than what is imagined, invented, or guessed.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reality_facts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please tell me the <w>truth</w> about what happened yesterday.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, расскажи мне <w>правду</w> о том, что случилось вчера."
                            }
                        },
                        {
                            "text": "The <w>truth</w> is that we cannot afford a new car right now.",
                            "target_lang_translations": {
                                "ru": "<w>Правда</w> в том, что мы не можем позволить себе новую машину прямо сейчас."
                            }
                        }
                    ],
                    "synonyms": [
                        "straight goods",
                        "verity",
                        "truth"
                    ],
                    "antonyms": [
                        "lie",
                        "falsehood",
                        "untruth",
                        "nonsense"
                    ],
                    "common_phrases": [
                        "tell the truth",
                        "truth be told",
                        "the whole truth",
                        "moment of truth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Реальные факты или информация о чем-либо, в отличие от вымысла или догадок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "то, что соответствует действительности"
                            },
                            {
                                "target_lang_word": "истина",
                                "target_lang_sense_clarification": "верное утверждение, факты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "275c075d-932a-3003-acfa-b262ed5520fd",
                    "sense_definition": "The quality or state of being true, accurate, and in accordance with fact or reality.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "reality_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is no <w>truth</w> in the rumors spreading around the office.",
                            "target_lang_translations": {
                                "ru": "В слухах, распространяющихся по офису, нет ни слова <w>правды</w>."
                            }
                        },
                        {
                            "text": "We need to check the <w>truth</w> of his statement before we publish it.",
                            "target_lang_translations": {
                                "ru": "Нам нужно проверить <w>истинность</w> его заявления перед публикацией."
                            }
                        }
                    ],
                    "synonyms": [
                        "veracity",
                        "veraciousness",
                        "veridicality",
                        "verity"
                    ],
                    "antonyms": [
                        "falsity",
                        "falseness",
                        "falsehood"
                    ],
                    "common_phrases": [
                        "grain of truth",
                        "ring of truth",
                        "in truth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство быть верным, точным и соответствующим фактам или реальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "истинность",
                                "target_lang_sense_clarification": "соответствие истине"
                            },
                            {
                                "target_lang_word": "правдивость",
                                "target_lang_sense_clarification": "свойство быть правдивым"
                            },
                            {
                                "target_lang_word": "достоверность",
                                "target_lang_sense_clarification": "точность и надежность информации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d90df33d-9b02-3caf-b6b0-a5fad22bf6dc",
                    "sense_definition": "A fact, principle, or belief that is accepted as true or real.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reality_facts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a universal <w>truth</w> that everyone wants to be happy.",
                            "target_lang_translations": {
                                "ru": "Это прописная <w>истина</w>, что каждый хочет быть счастливым."
                            }
                        },
                        {
                            "text": "She had to face some hard <w>truths</w> about her health.",
                            "target_lang_translations": {
                                "ru": "Ей пришлось столкнуться с неприятными <w>истинами</w> о своем здоровье."
                            }
                        }
                    ],
                    "synonyms": [
                        "verity"
                    ],
                    "antonyms": [
                        "lie",
                        "untruth",
                        "half-truth"
                    ],
                    "common_phrases": [
                        "universal truth",
                        "home truth",
                        "eternal truth"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт, принцип или убеждение, которые принимаются как верные или реальные."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "истина",
                                "target_lang_sense_clarification": "непреложный факт или догма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f496f117-697b-3b60-9ab5-b1b658496b36",
                    "sense_definition": "An option in the party game 'Truth or Dare' where a player must answer a question honestly.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "game",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When it was her turn to play, she chose <w>truth</w> instead of dare.",
                            "target_lang_translations": {
                                "ru": "Когда пришла её очередь играть, она выбрала <w>правду</w>, а не действие."
                            }
                        },
                        {
                            "text": "He picked <w>truth</w> and had to reveal his secret crush.",
                            "target_lang_translations": {
                                "ru": "Он выбрал <w>правду</w> и был вынужден раскрыть свою тайную влюбленность."
                            }
                        }
                    ],
                    "common_phrases": [
                        "truth or dare"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вариант выбора в игре «Правда или действие», когда игрок должен честно ответить на вопрос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "в игре «Правда или действие»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fe410d9-eef8-3e66-8836-4914e82e8974",
                    "sense_definition": "The state of being sincere, loyal, or faithful to someone or something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "loyalty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>truth</w> to his own principles is what makes him a great leader.",
                            "target_lang_translations": {
                                "ru": "Его <w>верность</w> собственным принципам — вот что делает его великим лидером."
                            }
                        },
                        {
                            "text": "She questioned the <w>truth</w> of his friendship after he betrayed her trust.",
                            "target_lang_translations": {
                                "ru": "Она усомнилась в <w>искренности</w> его дружбы после того, как он предал её доверие."
                            }
                        }
                    ],
                    "antonyms": [
                        "falseness"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "Modern usage often prefers 'fidelity' or 'integrity' in this context."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние искренности, верности или преданности кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верность",
                                "target_lang_sense_clarification": "преданность принципам или людям"
                            },
                            {
                                "target_lang_word": "искренность",
                                "target_lang_sense_clarification": "честность в отношениях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "acf2c7eb-fd4d-3541-8801-eec02feb5385",
                    "sense_definition": "To verify data or information by checking the actual situation in the real world (often used in the phrase 'ground truth').",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The satellite data was ground <w>truthed</w> by a team of field researchers.",
                            "target_lang_translations": {
                                "ru": "Спутниковые данные были <w>проверены</w> на местности группой полевых исследователей."
                            }
                        },
                        {
                            "text": "We need to <w>truth</w> these figures against the original documents.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>сверить</w> эти цифры с оригиналами документов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ground truth"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Often used in technical or scientific contexts."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверять данные или информацию путем сверки с реальной ситуацией (часто в контексте «наземной проверки»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверять",
                                "target_lang_sense_clarification": "сверять с реальностью"
                            },
                            {
                                "target_lang_word": "верифицировать",
                                "target_lang_sense_clarification": "подтверждать истинность данных"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "ogonek",
        "ogonowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f24ce4df-b473-36fe-a79d-06d9854cfa89",
                    "sense_definition": "Tylna część ciała zwierzęcia, będąca przedłużeniem kręgosłupa, często pokryta sierścią lub piórami; służy do utrzymywania równowagi, komunikacji lub odganiania owadów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pies radośnie merdał <w>ogonem</w> na widok swojego właściciela.",
                            "target_lang_translations": {
                                "ru": "Собака радостно виляла <w>хвостом</w> при виде своего хозяина."
                            }
                        },
                        {
                            "text": "Kot siedział na płocie i czyścił swój puszysty <w>ogon</w>.",
                            "target_lang_translations": {
                                "ru": "Кот сидел на заборе и чистил свой пушистый <w>хвост</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kita",
                        "tył"
                    ],
                    "antonyms": [
                        "przód"
                    ],
                    "common_phrases": [
                        "merdać ogonem",
                        "podkulić ogon",
                        "machać ogonem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задняя часть тела животного, являющаяся продолжением позвоночника; служит для равновесия, общения или защиты от насекомых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "часть тела животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60b91296-d019-3fcb-8dc9-448b3b9497b8",
                    "sense_definition": "Długa kolejka ludzi lub pojazdów oczekujących na coś jeden za drugim.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "lineup",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed kasą biletową ustawił się długi <w>ogon</w> zniecierpliwionych klientów.",
                            "target_lang_translations": {
                                "ru": "Перед билетной кассой выстроился длинный <w>хвост</w> нетерпеливых клиентов."
                            }
                        },
                        {
                            "text": "Musieliśmy stać w <w>ogonie</w> przez ponad godzinę, żeby wejść do muzeum.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось стоять в <w>хвосте</w> больше часа, чтобы попасть в музей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stać w ogonie",
                        "ciągnąć się w ogonie",
                        "koniec ogona"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная вереница людей или транспортных средств, ожидающих чего-либо друг за другом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "разговорное: очередь"
                            },
                            {
                                "target_lang_word": "очередь",
                                "target_lang_sense_clarification": "вереница людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bf54adf-7ba5-370e-b1ec-c308c60ab12d",
                    "sense_definition": "Tylna, końcowa część jakiegoś przedmiotu, pojazdu lub zjawiska.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechanicy naprawiali uszkodzony <w>ogon</w> samolotu pasażerskiego.",
                            "target_lang_translations": {
                                "ru": "Механики ремонтировали поврежденный <w>хвост</w> пассажирского самолета."
                            }
                        },
                        {
                            "text": "Na nocnym niebie pojawiła się jasna kometa z długim <w>ogonem</w>.",
                            "target_lang_translations": {
                                "ru": "В ночном небе появилась яркая комета с длинным <w>хвостом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tył"
                    ],
                    "antonyms": [
                        "przód"
                    ],
                    "common_phrases": [
                        "ogon samolotu",
                        "ogon komety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задняя, конечная часть какого-либо предмета, транспортного средства или явления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "самолета, кометы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9969885e-d7a0-3f54-9178-72d6eb4665dc",
                    "sense_definition": "Osoba, grupa osób lub pojazd śledzący kogoś w ukryciu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "spy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prywatny detektyw zorientował się, że ma <w>ogon</w> i skręcił w boczną uliczkę.",
                            "target_lang_translations": {
                                "ru": "Частный детектив понял, что у него <w>хвост</w>, и свернул в переулок."
                            }
                        },
                        {
                            "text": "Policjanci zgubili <w>ogon</w>, aby nie doprowadzić przestępców do kryjówki.",
                            "target_lang_translations": {
                                "ru": "Полицейские сбросили <w>хвост</w>, чтобы не привести преступников к убежищу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć ogon",
                        "zgubić ogon"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "police or criminal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или группа людей, тайно следящих за кем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "слежка, преследователь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5100011d-3eb0-3df7-ae0c-e1ceff7cab84",
                    "sense_definition": "Rola drugoplanowa w sztuce teatralnej lub filmie, często niema lub mało znacząca.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "theater",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody aktor musiał grać <w>ogony</w>, zanim reżyser powierzył mu poważniejsze zadanie.",
                            "target_lang_translations": {
                                "ru": "Молодому актеру приходилось играть <w>эпизодические роли</w>, прежде чем режиссер доверил ему более серьезную задачу."
                            }
                        },
                        {
                            "text": "Nikt nie lubi grać <w>ogonów</w>, bo nie dają szansy na wykazanie się talentem.",
                            "target_lang_translations": {
                                "ru": "Никто не любит играть <w>в массовке</w>, потому что это не дает шанса проявить талант."
                            }
                        }
                    ],
                    "antonyms": [
                        "główna rola"
                    ],
                    "common_phrases": [
                        "grać ogony"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "theatrical slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Второстепенная, незначительная роль в театре или кино, часто без слов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эпизодическая роль",
                                "target_lang_sense_clarification": "незначительная роль"
                            },
                            {
                                "target_lang_word": "массовка",
                                "target_lang_sense_clarification": "участие в массовых сценах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d93e94d-33de-3889-8f45-0b981b2737a6",
                    "sense_definition": "Wulgarnie o męskim członku.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "body_part_vulgar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdzie <w>ogon</w> rządzi, tam głowa błądzi.",
                            "target_lang_translations": {
                                "ru": "Где <w>член</w> правит, там голова блуждает."
                            }
                        },
                        {
                            "text": "W sprośnych żartach często pojawia się słowo <w>ogon</w> w znaczeniu anatomicznym.",
                            "target_lang_translations": {
                                "ru": "В пошлых шутках часто встречается слово <w>хвост</w> в анатомическом значении."
                            }
                        }
                    ],
                    "synonyms": [
                        "kuśka",
                        "korzeń"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарное обозначение мужского полового члена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "член",
                                "target_lang_sense_clarification": "анатомическое/вульгарное"
                            },
                            {
                                "target_lang_word": "хер",
                                "target_lang_sense_clarification": "грубое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b245af12-1b43-3d2f-89f8-00b39e79549c",
                    "sense_definition": "Pojedyncza sztuka bydła (używane w kontekście rolniczym lub łowieckim).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_count",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz z dumą mówił, że ma trzy <w>ogony</w> w oborze.",
                            "target_lang_translations": {
                                "ru": "Хозяин с гордостью говорил, что у него в хлеву три <w>головы</w> скота."
                            }
                        }
                    ],
                    "synonyms": [
                        "bydło"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "or dialectal/hunting"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица счета скота (используется в сельском хозяйстве или охотничьем контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голова",
                                "target_lang_sense_clarification": "единица счета скота"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [

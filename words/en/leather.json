    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "700eae96-c596-3152-ba30-709be74557aa",
                    "sense_definition": "A tough material produced from the skin of animals by tanning, used for making shoes, clothes, and furniture.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My new boots are made of genuine <w>leather</w>.",
                            "target_lang_translations": {
                                "ru": "Мои новые ботинки сделаны из натуральной <w>кожи</w>."
                            }
                        },
                        {
                            "text": "You should polish the <w>leather</w> regularly to keep it soft.",
                            "target_lang_translations": {
                                "ru": "Вам следует регулярно полировать <w>кожу</w>, чтобы она оставалась мягкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "leathern",
                        "leathern"
                    ],
                    "common_phrases": [
                        "genuine leather",
                        "shoe leather",
                        "leather goods"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочный материал, получаемый из шкур животных путем дубления, используемый для изготовления обуви, одежды и мебели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кожа",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1a92d38-b285-3da3-ad50-842e32adae65",
                    "sense_definition": "Clothing made from animal skin, especially a jacket and trousers worn by motorcycle riders.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The motorcyclist put on his black <w>leathers</w> before the race.",
                            "target_lang_translations": {
                                "ru": "Мотоциклист надел свой черный <w>кожаный костюм</w> перед гонкой."
                            }
                        },
                        {
                            "text": "It is safer to ride in full <w>leathers</w> to protect your skin.",
                            "target_lang_translations": {
                                "ru": "Безопаснее ездить в полной <w>кожаной экипировке</w>, чтобы защитить кожу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "motorcycle leathers",
                        "wear leathers"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда из кожи, особенно куртка и брюки, которые носят мотоциклисты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кожаная экипировка",
                                "target_lang_sense_clarification": "одежда мотоциклиста"
                            },
                            {
                                "target_lang_word": "кожаный костюм",
                                "target_lang_sense_clarification": "одежда"
                            }
                        ]

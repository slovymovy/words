            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8716ad7-70fc-32d4-b1a6-4dd65c3702c3",
                    "sense_definition": "Iets wat men denkt; een product van het denkproces of de geest.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mental_product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>gedachte</w> aan de naderende vakantie maakt me erg vrolijk.",
                            "target_lang_translations": {
                                "en": "The <w>thought</w> of the approaching holiday makes me very cheerful."
                            }
                        },
                        {
                            "text": "Hij was zo diep in <w>gedachten</w> verzonken dat hij de bel niet hoorde.",
                            "target_lang_translations": {
                                "en": "He was so lost in <w>thought</w> that he didn't hear the doorbell."
                            }
                        },
                        {
                            "text": "Soms heb ik het gevoel dat ik een <w>gedachte</w> van mijn beste vriendin kan lezen.",
                            "target_lang_translations": {
                                "en": "Sometimes I have the feeling that I can read a <w>thought</w> of my best friend."
                            }
                        }
                    ],
                    "synonyms": [
                        "idee",
                        "inval",
                        "overpeinzing"
                    ],
                    "common_phrases": [
                        "in gedachten",
                        "van gedachten wisselen",
                        "op twee gedachten hinken"
                    ],
                    "target_lang_definitions": {
                        "en": "Something one thinks; a product of the thinking process or the mind."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thought",
                                "target_lang_sense_clarification": "mental representation or product of thinking"
                            },
                            {
                                "target_lang_word": "idea",
                                "target_lang_sense_clarification": "concept, notion, or plan"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2657616-35de-3407-9b84-d8bb1c754f85",
                    "sense_definition": "Een mening, oordeel of opvatting over een bepaald onderwerp.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat is jouw <w>gedachte</w> over het nieuwe beleid van de gemeente?",
                            "target_lang_translations": {
                                "en": "What are your <w>thoughts</w> on the municipality's new policy?"
                            }
                        },
                        {
                            "text": "Naar mijn <w>gedachte</w> is dit niet de juiste oplossing voor het probleem.",
                            "target_lang_translations": {
                                "en": "In my <w>opinion</w>, this is not the right solution to the problem."
                            }
                        }
                    ],
                    "synonyms": [
                        "mening",
                        "opinie",
                        "opvatting",
                        "oordeel"
                    ],
                    "common_phrases": [
                        "naar mijn gedachte",
                        "van gedachten veranderen"
                    ],
                    "target_lang_definitions": {
                        "en": "An opinion, judgment, or view on a particular subject."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opinion",
                                "target_lang_sense_clarification": "personal belief or judgment"
                            },
                            {
                                "target_lang_word": "view",
                                "target_lang_sense_clarification": "perspective or way of regarding something"
                            },
                            {
                                "target_lang_word": "thoughts",
                                "target_lang_sense_clarification": "ideas or reflections on a topic"
                            }
                        ]
                    }
                }
            ]

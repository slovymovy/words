                    "frequency": "Middle",
                    "semantic_group_id": "room_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>acoustics</w> of the new theater are excellent, allowing every whisper to be heard.",
                            "target_lang_translations": {
                                "ru": "<w>Акустика</w> нового театра превосходна, что позволяет услышать каждый шепот."
                            }
                        },
                        {
                            "text": "We need to improve the room's <w>acoustics</w> before we can record any music here.",
                            "target_lang_translations": {
                                "ru": "Нам нужно улучшить <w>акустику</w> помещения, прежде чем мы сможем записывать здесь музыку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "good acoustics",
                        "poor acoustics",
                        "room acoustics"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качества помещения или здания, определяющие, как в нем распространяется звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акустика",
                                "target_lang_sense_clarification": "свойства помещения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b11fb344-7bee-3aa2-8094-be8c66944d96",

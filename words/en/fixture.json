                            {
                                "target_lang_word": "встреча",
                                "target_lang_sense_clarification": "спортивное состязание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "105752d3-8ed0-3d4c-b5ee-b91ad65b10e9",
                    "sense_definition": "A person who is regularly present at a particular place or event, often to the point of seeming like a permanent part of it.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Over the years, the old man had become a <w>fixture</w> at the neighborhood pub.",
                            "target_lang_translations": {
                                "ru": "С годами старик стал <w>завсегдатаем</w> местного паба."
                            }
                        },
                        {
                            "text": "She is a permanent <w>fixture</w> in the library, studying there almost every evening.",
                            "target_lang_translations": {
                                "ru": "Она — <w>постоянный посетитель</w> библиотеки, занимается там почти каждый вечер."
                            }
                        }
                    ],
                    "synonyms": [
                        "regular",
                        "habitué"
                    ],
                    "common_phrases": [
                        "permanent fixture",
                        "become a fixture"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который постоянно присутствует в определенном месте или на мероприятии, словно являясь его неотъемлемой частью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завсегдатай",
                                "target_lang_sense_clarification": "постоянный посетитель"

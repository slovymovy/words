    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "012e54fb-cb29-343f-b4e3-57e61aed8844",
                    "sense_definition": "Rustig worden; ophouden met onrustig, wild of emotioneel zijn.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "calm_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De storm is gelukkig wat <w>gekalmeerd</w>.",
                            "target_lang_translations": {
                                "ru": "К счастью, шторм немного <w>утих</w>."
                            }
                        },
                        {
                            "text": "Je moet eerst even <w>kalmeren</w> voordat je het verhaal vertelt.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно сначала <w>успокоиться</w>, прежде чем рассказывать историю."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedaren",
                        "bedaren",
                        "zinnigen"
                    ],
                    "antonyms": [
                        "opwinden",
                        "uitbarsten"
                    ],
                    "common_phrases": [
                        "vanzelf kalmeren",
                        "langzaam kalmeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться спокойным; перестать нервничать, бушевать или проявлять сильные эмоции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "успокаиваться",
                                "target_lang_sense_clarification": "приходить в состояние покоя (о людях)"
                            },
                            {
                                "target_lang_word": "стихать",
                                "target_lang_sense_clarification": "ослабевать (о ветре, буре, боли)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd2f8e58-b527-3931-bca6-719589890f32",
                    "sense_definition": "Iemand of iets rustig maken; zorgen dat de onrust of emotie afneemt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "calm_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De groepsleider <w>kalmeerde</w> de jongen met een rustige stem.",
                            "target_lang_translations": {
                                "ru": "Руководитель группы <w>успокоил</w> мальчика тихим голосом."
                            }
                        },
                        {
                            "text": "Het was moeilijk om de boze menigte te <w>kalmeren</w>.",
                            "target_lang_translations": {
                                "ru": "Было трудно <w>успокоить</w> разъяренную толпу."
                            }
                        }
                    ],
                    "synonyms": [
                        "bedaren",
                        "zinnigen",
                        "zinnigen"
                    ],
                    "antonyms": [
                        "opjutten",
                        "provoceren"
                    ],
                    "common_phrases": [
                        "de gemoederen kalmeren",
                        "iemand proberen te kalmeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать кого-то или что-то спокойным; способствовать уменьшению беспокойства или волнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "успокаивать",
                                "target_lang_sense_clarification": "возвращать душевное равновесие"
                            },
                            {
                                "target_lang_word": "усмирять",
                                "target_lang_sense_clarification": "подавлять буйство или агрессию"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
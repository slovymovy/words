            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2be0fba-9850-3276-8911-4d4db33387c2",
                    "sense_definition": "Охладить что-либо до очень низкой температуры или до превращения в лёд.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <w>заморозить</w> свежую клубнику на зиму, чтобы сохранить витамины.",
                            "target_lang_translations": {
                                "en": "We decided to <w>freeze</w> fresh strawberries for the winter to preserve vitamins."
                            }
                        },
                        {
                            "text": "В этой камере можно быстро <w>заморозить</w> мясо для длительного хранения.",
                            "target_lang_translations": {
                                "en": "In this chamber, you can quickly <w>freeze</w> meat for long-term storage."
                            }
                        }
                    ],
                    "antonyms": [
                        "разморозить"
                    ],
                    "common_phrases": [
                        "заморозить мясо",
                        "заморозить ягоды",
                        "заморозить воду",
                        "заморозить продукты"
                    ],
                    "target_lang_definitions": {
                        "en": "To cool something to a very low temperature or until it turns into ice."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "freeze",
                                "target_lang_sense_clarification": "to turn into ice or preserve by cold"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdb79ec9-e712-3e3d-b421-8e65ce5f8536",
                    "sense_definition": "Приостановить действие, развитие или изменение чего-либо (цен, счетов, проектов).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство планирует <w>заморозить</w> цены на основные продукты питания.",
                            "target_lang_translations": {
                                "en": "The government plans to <w>freeze</w> prices on basic food products."
                            }
                        },
                        {
                            "text": "Банк был вынужден <w>заморозить</w> счета компании из-за подозрительных операций.",
                            "target_lang_translations": {
                                "en": "The bank was forced to <w>freeze</w> the company's accounts due to suspicious transactions."
                            }
                        }
                    ],
                    "synonyms": [
                        "приостановить"
                    ],
                    "antonyms": [
                        "разморозить"
                    ],
                    "common_phrases": [
                        "заморозить цены",
                        "заморозить счета",
                        "заморозить проект",
                        "заморозить строительство"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop the progress, change, or movement of something (such as prices, accounts, or projects)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "freeze",
                                "target_lang_sense_clarification": "prices, assets, or projects"
                            },
                            {
                                "target_lang_word": "suspend",
                                "target_lang_sense_clarification": "operations or projects"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bfbabf7-6dde-3152-b17d-f0407286b5b4",
                    "sense_definition": "Временно заблокировать доступ к учётной записи пользователя в интернете.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Администрация социальной сети может <w>заморозить</w> вашу страницу за спам.",
                            "target_lang_translations": {
                                "en": "The social network administration can <w>freeze</w> your page for spam."
                            }
                        },
                        {
                            "text": "Мне пришлось временно <w>заморозить</w> аккаунт, чтобы разобраться с настройками безопасности.",
                            "target_lang_translations": {
                                "en": "I had to temporarily <w>freeze</w> my account to sort out security settings."
                            }
                        }
                    ],
                    "synonyms": [
                        "заблокировать"
                    ],
                    "antonyms": [
                        "разморозить"
                    ],
                    "common_phrases": [
                        "заморозить страницу",
                        "заморозить аккаунт",
                        "заморозить профиль"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "компьютерный"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To temporarily block access to an online user account."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "freeze",
                                "target_lang_sense_clarification": "an account or profile"
                            },
                            {
                                "target_lang_word": "suspend",
                                "target_lang_sense_clarification": "an account temporarily"
                            }
                        ]
                    }
                }
            ]
        }

                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many <w>Friends</w> are actively involved in peace movements around the world.",
                            "target_lang_translations": {

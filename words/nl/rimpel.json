{
    "word_family": [
        "rimpelen",
        "rimpeling",
        "gerimpeld"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "664507ee-c40e-36f1-ae80-9182561f0f69",
                    "sense_definition": "Een oppervlakkige vouw of golving, in het bijzonder van de menselijke huid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "skin_fold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij smeert elke avond crème op haar gezicht om elke <w>rimpel</w> te voorkomen.",
                            "target_lang_translations": {
                                "ru": "Она каждый вечер наносит крем на лицо, чтобы предотвратить появление любой <w>морщины</w>."
                            }
                        },
                        {
                            "text": "Toen hij lachte, verscheen er een diepe <w>rimpel</w> op zijn voorhoofd.",
                            "target_lang_translations": {
                                "ru": "Когда он смеялся, на его лбу появлялась глубокая <w>морщина</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vouw",
                        "plooi"
                    ],
                    "common_phrases": [
                        "rimpels krijgen",
                        "diepe rimpels"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неглубокая складка или неровность, особенно на коже человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морщина",
                                "target_lang_sense_clarification": "на коже лица или тела"
                            },
                            {
                                "target_lang_word": "складка",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d03799d4-2c5d-38f6-833d-f90167f85887",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>rimpelen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>rimpel</w> mijn voorhoofd als ik diep aan het nadenken ben.",
                            "target_lang_translations": {
                                "ru": "Я <w>морщу</w> лоб, когда глубоко задумываюсь."
                            }
                        },
                        {
                            "text": "Als ik de stof <w>rimpel</w>, valt de rok veel mooier.",
                            "target_lang_translations": {
                                "ru": "Если я <w>собираю</w> ткань <w>в складки</w>, юбка сидит гораздо лучше."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма 1-го лица единственного числа настоящего времени от глагола rimpelen (морщить, сборить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морщить",
                                "target_lang_sense_clarification": "хмурить лоб, нос"
                            },
                            {
                                "target_lang_word": "сборить",
                                "target_lang_sense_clarification": "делать складки на ткани"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0dbb7f4-8c79-3288-9120-dca6e34f4147",
                    "sense_definition": "Gebiedende wijs van <w>rimpelen</w>.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Rimpel</w> je neus niet zo als je je groenten moet eten.",
                            "target_lang_translations": {
                                "ru": "Не <w>морщи</w> нос, когда тебе нужно есть овощи."
                            }
                        },
                        {
                            "text": "<w>Rimpel</w> het water niet door er stenen in te gooien.",
                            "target_lang_translations": {
                                "ru": "Не <w>создавай рябь</w> на воде, бросая в неё камни."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола rimpelen."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морщить",
                                "target_lang_sense_clarification": "приказ или просьба"
                            },
                            {
                                "target_lang_word": "рябить",
                                "target_lang_sense_clarification": "создавать волнение на воде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d06a4df-10b5-3cef-ac29-842c301ea955",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>rimpelen</w> (bij inversie).",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waarom <w>rimpel</w> je je voorhoofd als ik dat zeg?",
                            "target_lang_translations": {
                                "ru": "Почему ты <w>морщишь</w> лоб, когда я это говорю?"
                            }
                        },
                        {
                            "text": "<w>Rimpel</w> je de stof met de hand of met de naaimachine?",
                            "target_lang_translations": {
                                "ru": "Ты <w>собираешь</w> ткань <w>в складки</w> вручную или на швейной машинке?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма 2-го лица единственного числа настоящего времени от глагола rimpelen (при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морщить",
                                "target_lang_sense_clarification": "действие собеседника"
                            },
                            {
                                "target_lang_word": "собирать в складки",
                                "target_lang_sense_clarification": "ткань"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
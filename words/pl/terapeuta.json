    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eac12b2c-c87d-325a-b233-a3be08f8291a",
                    "sense_definition": "Specjalista zajmujący się prowadzeniem terapii, pomagający pacjentom w powrocie do zdrowia lub sprawności za pomocą określonych metod.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_specialist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <terapeuta> zalecił mi serię ćwiczeń na kręgosłup.",
                            "target_lang_translations": {
                                "ru": "Мой <терапевт> порекомендовал мне комплекс упражнений для позвоночника."
                            }
                        },
                        {
                            "text": "Ten <terapeuta> specjalizuje się w pracy z dziećmi niepełnosprawnymi.",
                            "target_lang_translations": {
                                "ru": "Этот <терапевт> специализируется на работе с детьми с ограниченными возможностями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "terapeuta manualny",
                        "terapeuta zajęciowy",
                        "doświadczony terapeuta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист, занимающийся проведением терапии и помогающий пациентам восстановить здоровье или физическую форму с помощью определенных методов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терапевт",
                                "target_lang_sense_clarification": "специалист, проводящий лечение (например, мануальный)"
                            },
                            {
                                "target_lang_word": "реабилитолог",
                                "target_lang_sense_clarification": "специалист по восстановлению утраченных функций"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d1456d2-75c2-3806-92ee-1799b1ca0c96",
                    "sense_definition": "<w>Psychoterapeuta</w>; osoba zajmująca się leczeniem zaburzeń psychicznych i emocjonalnych, zazwyczaj poprzez rozmowę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "medical_specialist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po traumatycznym wydarzeniu zdecydował się na szczerą rozmowę z <terapeutą>.",
                            "target_lang_translations": {
                                "ru": "После травматического события он решился на откровенный разговор с <терапевтом>."
                            }
                        },
                        {
                            "text": "Dobry <terapeuta> potrafi słuchać pacjenta bez oceniania go.",
                            "target_lang_translations": {
                                "ru": "Хороший <терапевт> умеет слушать пациента, не осуждая его."
                            }
                        }
                    ],
                    "synonyms": [
                        "psychoterapeuta"
                    ],
                    "common_phrases": [
                        "terapeuta uzależnień",
                        "wizyta u terapeuty",
                        "terapeuta rodzinny"
                    ],
                    "target_lang_definitions": {
                        "ru": "<w>Психотерапевт</w>; специалист, занимающийся лечением психических и эмоциональных расстройств, как правило, посредством беседы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "психотерапевт",
                                "target_lang_sense_clarification": "специалист по лечению ментальных расстройств"
                            },
                            {
                                "target_lang_word": "терапевт",
                                "target_lang_sense_clarification": "разг. то же, что психотерапевт"
                            }
                        ]
                    }
                }
            ]
        }
    ]

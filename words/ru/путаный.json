    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f4a7612c-7d05-37e0-b8ea-7289c8c8f54f",
                    "sense_definition": "Лишённый ясности, логической последовательности; сбивчивый, непонятный.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unclear_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент дал <w>путаный</w> и неуверенный ответ на экзамене.",
                            "target_lang_translations": {
                                "en": "The student gave a <w>confused</w> and uncertain answer on the exam."
                            }
                        },
                        {
                            "text": "Его <w>путаный</w> рассказ о происшествии никто не мог понять.",
                            "target_lang_translations": {
                                "en": "No one could understand his <w>incoherent</w> story about the incident."
                            }
                        }
                    ],
                    "synonyms": [
                        "сби́вчивый",
                        "нелоги́чный",
                        "нея́сный"
                    ],
                    "common_phrases": [
                        "путаный ответ",
                        "путаные мысли",
                        "путаная речь",
                        "путаный рассказ"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking clarity or logical sequence; incoherent, incomprehensible."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "confused",
                                "target_lang_sense_clarification": "lacking order or clarity"
                            },
                            {
                                "target_lang_word": "incoherent",
                                "target_lang_sense_clarification": "lacking logical connection"
                            },
                            {
                                "target_lang_word": "muddled",
                                "target_lang_sense_clarification": "disordered or mixed up"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b62b91d7-b960-36bc-83a5-ca03c40496c0",
                    "sense_definition": "Беспорядочно переплетённый, имеющий много петель или изгибов (о нитках, волосах, следах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_tangle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охотник долго пытался разобрать <w>путаный</w> след зайца на снегу.",
                            "target_lang_translations": {
                                "en": "The hunter tried for a long time to figure out the hare's <w>winding</w> tracks in the snow."
                            }
                        },
                        {
                            "text": "В старой корзине лежали <w>путаные</w> клубки шерсти.",
                            "target_lang_translations": {
                                "en": "<w>Tangled</w> balls of wool lay in the old basket."
                            }
                        }
                    ],
                    "common_phrases": [
                        "путаный след",
                        "путаные нитки",
                        "путаные волосы"
                    ],
                    "target_lang_definitions": {
                        "en": "Disorderly intertwined, having many loops or bends (of threads, hair, tracks)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tangled",
                                "target_lang_sense_clarification": "intertwined in a messy way"
                            },
                            {
                                "target_lang_word": "winding",
                                "target_lang_sense_clarification": "having many twists and turns"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52ef6fcd-0c89-3596-870b-928837c839ce",
                    "sense_definition": "Бестолковый, не умеющий ясно мыслить или действовать (о человеке).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "unclear_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "С этим <w>путаным</w> сотрудником очень сложно вести дела.",
                            "target_lang_translations": {
                                "en": "It is very difficult to do business with this <w>muddle-headed</w> employee."
                            }
                        },
                        {
                            "text": "Он какой-то <w>путаный</w> и странный человек, которому нельзя доверять.",
                            "target_lang_translations": {
                                "en": "He is a somewhat <w>confused</w> and strange person who cannot be trusted."
                            }
                        }
                    ],
                    "synonyms": [
                        "бестолко́вый"
                    ],
                    "common_phrases": [
                        "путаный человек",
                        "путаный народ"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in spoken language to describe a person"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Muddle-headed, unable to think or act clearly (about a person)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "muddle-headed",
                                "target_lang_sense_clarification": "mentally disorganized"
                            },
                            {
                                "target_lang_word": "scatterbrained",
                                "target_lang_sense_clarification": "lacking concentration or organization"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
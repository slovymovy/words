                {
                    "sense_id": "09db2b28-12cb-359d-9388-3fb2de90b263",
                    "sense_definition": "drobny węgiel kamienny, używany głównie do ogrzewania domów",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym roku musimy kupić dwie tony <miału> na zimę.",

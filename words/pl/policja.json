    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f49ad0b1-a0ee-3e07-909e-acf1d78d9b35",
                    "sense_definition": "Instytucja państwowa powołana do ochrony bezpieczeństwa ludzi i mienia, utrzymywania porządku publicznego oraz ścigania przestępstw.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "modern_police",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę natychmiast zadzwonić na <w>policję</w>, ponieważ byłem świadkiem wypadku.",
                            "target_lang_translations": {
                                "ru": "Мне нужно немедленно позвонить в <w>полицию</w>, так как я стал свидетелем аварии."
                            }
                        },
                        {
                            "text": "Lokalna <w>policja</w> prowadzi śledztwo w sprawie kradzieży samochodu.",
                            "target_lang_translations": {
                                "ru": "Местная <w>полиция</w> ведет расследование по делу о краже автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "milicja"
                    ],
                    "antonyms": [
                        "półświatek",
                        "przestępca"
                    ],
                    "common_phrases": [
                        "dzwonić na policję",
                        "patrol policji",
                        "wóz policji",
                        "komendant policji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственное учреждение, созданное для охраны безопасности людей и имущества, поддержания общественного порядка и преследования преступности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полиция",
                                "target_lang_sense_clarification": "государственный орган"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d3f4371-5b2b-3948-b08f-3c368192b412",
                    "sense_definition": "Ogół funkcjonariuszy pracujących w tej instytucji; grupa policjantów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "modern_police",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy przyjechała <w>policja</w>, sprawcy zdążyli już uciec z miejsca zdarzenia.",
                            "target_lang_translations": {
                                "ru": "Когда приехала <w>полиция</w>, виновники уже успели скрыться с места происшествия."
                            }
                        },
                        {
                            "text": "Pod stadionem stała <w>policja</w>, aby pilnować porządku podczas meczu.",
                            "target_lang_translations": {
                                "ru": "У стадиона стояла <w>полиция</w>, чтобы следить за порядком во время матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "gliny",
                        "gliniarze",
                        "psiarnia",
                        "bagieta",
                        "smutni panowie",
                        "pały",
                        "psy"
                    ],
                    "common_phrases": [
                        "policja przyjechała",
                        "uciekać przed policją",
                        "bać się policji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность сотрудников, работающих в этом учреждении; группа полицейских."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полиция",
                                "target_lang_sense_clarification": "сотрудники"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d51bceb3-0e5a-37b7-b94e-16de7a325ffe",
                    "sense_definition": "Budynek będący siedzibą jednostki tej instytucji; komisariat lub komenda.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "modern_police",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zatrzymany mężczyzna został przewieziony na <w>policję</w> w celu złożenia wyjaśnień.",
                            "target_lang_translations": {
                                "ru": "Задержанного мужчину отвезли в <w>полицию</w> для дачи объяснений."
                            }
                        },
                        {
                            "text": "Jutro muszę iść na <w>policję</w>, żeby odebrać zgubione dokumenty.",
                            "target_lang_translations": {
                                "ru": "Завтра мне нужно пойти в <w>полицию</w>, чтобы забрать утерянные документы."
                            }
                        }
                    ],
                    "synonyms": [
                        "smutni panowie",
                        "gliny",
                        "gliniarze",
                        "pały",
                        "psy",
                        "psiarnia",
                        "bagieta",
                        "kurwy",
                        "mendy"
                    ],
                    "common_phrases": [
                        "iść na policję",
                        "zgłosić na policję",
                        "zabrać na policję"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane potocznie zamiast słowa komisariat lub komenda"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание, являющееся штаб-квартирой подразделения этого учреждения; участок или отделение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полиция",
                                "target_lang_sense_clarification": "разг. здание"
                            },
                            {
                                "target_lang_word": "отделение полиции",
                                "target_lang_sense_clarification": "участок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4efecad0-21c1-3fbf-94dc-f5fcd94c8167",
                    "sense_definition": "Dawne określenie na dobre obyczaje, ogładę towarzyską lub uporządkowane życie społeczne.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych wiekach szlachta ceniła sobie <w>policję</w> i kulturę osobistą.",
                            "target_lang_translations": {
                                "ru": "В прошлые века шляхта ценила <w>благочиние</w> и личную культуру."
                            }
                        },
                        {
                            "text": "Książka opisuje dawną <w>policję</w> jako zbiór zasad dobrego wychowania.",
                            "target_lang_translations": {
                                "ru": "Книга описывает старинное <w>благочиние</w> как свод правил хорошего воспитания."
                            }
                        }
                    ],

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3cbc2092-585f-3676-b191-2d3e91899f5b",
                    "sense_definition": "Говорить очень низким, густым и грубым голосом.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дедушка любит <w>басить</w>, когда рассказывает внукам страшные истории.",
                            "target_lang_translations": {
                                "en": "My grandfather likes to <w>speak in a deep voice</w> when telling his grandchildren scary stories."
                            }
                        },
                        {
                            "text": "Незнакомец начал громко <w>басить</w> в телефонную трубку, требуя позвать начальника.",
                            "target_lang_translations": {
                                "en": "The stranger started to <w>boom</w> loudly into the phone receiver, demanding to speak to the boss."
                            }
                        }
                    ],
                    "antonyms": [
                        "пищать"
                    ],
                    "common_phrases": [
                        "громко басить",
                        "басить в ответ",
                        "басить по телефону"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To speak in a very low, deep, and rough voice."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "speak in a deep voice",
                                "target_lang_sense_clarification": "general description of the action"
                            },
                            {
                                "target_lang_word": "boom",
                                "target_lang_sense_clarification": "to speak with a deep, resonant, and often loud voice"
                            },
                            {
                                "target_lang_word": "rumble",
                                "target_lang_sense_clarification": "to speak in a deep, rolling voice"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "874db315-f0fb-301f-b9e6-bf12cdf1234b",
                    "sense_definition": "Петь низким голосом, исполнять вокальную партию баса.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sound_production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В церковном хоре дьякон <w>басил</w> так мощно, что его голос перекрывал всех остальных.",
                            "target_lang_translations": {
                                "en": "In the church choir, the deacon <w>sang bass</w> so powerfully that his voice drowned out everyone else."
                            }
                        },
                        {
                            "text": "Пан Харитон старательно <w>басил</w> в лад с музыкой, удивляя гостей своим талантом.",
                            "target_lang_translations": {
                                "en": "Pan Khariton diligently <w>sang in a deep voice</w> in time with the music, surprising the guests with his talent."
                            }
                        }
                    ],
                    "antonyms": [
                        "пищать"
                    ],
                    "common_phrases": [
                        "басить в хоре",
                        "басить песню"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To sing in a low voice, to perform a bass part."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sing bass",
                                "target_lang_sense_clarification": "musical context"
                            },
                            {
                                "target_lang_word": "sing in a deep voice",
                                "target_lang_sense_clarification": "general description"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bcdb349a-b983-346d-bf8e-037688c8c7df",
                    "sense_definition": "Издавать низкие, гудящие звуки (о насекомых, музыкальных инструментах или механизмах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_production",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Над цветущим лугом <w>басят</w> мохнатые шмели, собирая сладкий нектар.",
                            "target_lang_translations": {
                                "en": "Furry bumblebees <w>drone</w> over the flowering meadow, gathering sweet nectar."
                            }
                        },
                        {
                            "text": "Старый контрабас <w>басил</w> в углу оркестровой ямы, создавая ритмическую основу мелодии.",
                            "target_lang_translations": {
                                "en": "The old double bass <w>rumbled</w> in the corner of the orchestra pit, creating the rhythmic foundation of the melody."
                            }
                        }
                    ],
                    "antonyms": [
                        "звенеть"
                    ],
                    "common_phrases": [
                        "шмели басят",
                        "струны басят"
                    ],
                    "target_lang_definitions": {
                        "en": "To make low, buzzing, or humming sounds (about insects, musical instruments, or mechanisms)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drone",
                                "target_lang_sense_clarification": "to make a continuous low humming sound"
                            },
                            {
                                "target_lang_word": "buzz deeply",
                                "target_lang_sense_clarification": "specifically for insects making a low-pitched sound"
                            },
                            {
                                "target_lang_word": "rumble",
                                "target_lang_sense_clarification": "for instruments or mechanisms producing deep sounds"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "128e5167-8cd1-319a-b421-9d277fa911a6",
                    "sense_definition": "Наряжаться, одеваться в красивую и богатую одежду.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_regional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Деревенские девушки любили <w>басить</w> перед большими праздниками, доставая из сундуков лучшие наряды.",
                            "target_lang_translations": {
                                "en": "The village girls loved to <w>dress up</w> before big holidays, taking their best outfits out of the chests."
                            }
                        },
                        {
                            "text": "Хватит тебе <w>басить</w> без повода, одевайся проще и иди работать.",
                            "target_lang_translations": {
                                "en": "Stop <w>decking yourself out</w> for no reason, dress simpler and go to work."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Карелия"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To dress up in beautiful and rich clothes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dress up",
                                "target_lang_sense_clarification": "to put on special clothes"
                            },
                            {
                                "target_lang_word": "deck oneself out",
                                "target_lang_sense_clarification": "to dress in a showy way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "840e35b4-ab5c-314b-9183-217016f247a5",
                    "sense_definition": "Украшать одежду баской, лентами или оборками.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_regional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Местная швея умела искусно <w>басить</w> подолы юбок разноцветными лентами.",
                            "target_lang_translations": {
                                "en": "The local seamstress knew how to skillfully <w>trim</w> the hems of skirts with colorful ribbons."
                            }
                        },
                        {
                            "text": "Она решила <w>басить</w> своё новое платье, чтобы оно выглядело наряднее.",
                            "target_lang_translations": {
                                "en": "She decided to <w>decorate</w> her new dress so it would look fancier."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "рег."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To decorate clothes with ribbons, frills, or a peplum."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trim",
                                "target_lang_sense_clarification": "to decorate clothing with ribbons or lace"
                            },
                            {
                                "target_lang_word": "adorn",
                                "target_lang_sense_clarification": "to make more beautiful or attractive"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
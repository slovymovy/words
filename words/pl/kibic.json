    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea4cf50b-4e73-3d43-b05b-0adeefa704a5",
                    "sense_definition": "Osoba interesująca się danym sportem, oglądająca zawody i dopingująca ulubioną drużynę lub zawodnika.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sports_fan",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Janek jest wielkim <w>kibicem</w> Interu Mediolan.",
                            "target_lang_translations": {
                                "ru": "Янек — большой <w>болельщик</w> миланского «Интера»."
                            }
                        },
                        {
                            "text": "Wierny <w>kibic</w> zawsze wspiera swój klub, nawet gdy ten przegrywa mecze.",
                            "target_lang_translations": {
                                "ru": "Верный <w>болельщик</w> всегда поддерживает свой клуб, даже когда тот проигрывает матчи."
                            }
                        }
                    ],
                    "synonyms": [
                        "fan",
                        "sympatyk klubu"
                    ],
                    "common_phrases": [
                        "wierny kibic",
                        "kibic piłkarski",
                        "doping kibiców"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, интересующийся определенным видом спорта, посещающий соревнования и поддерживающий любимую команду или спортсмена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болельщик",
                                "target_lang_sense_clarification": "поклонник спортивной команды или спортсмена"
                            },
                            {
                                "target_lang_word": "фанат",
                                "target_lang_sense_clarification": "разговорное; страстный, преданный поклонник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f46dd0c9-d9ca-3eaa-9fe8-da9ca8afbe05",
                    "sense_definition": "Osoba biernie przyglądająca się jakiejś sytuacji lub grze towarzyskiej (np. w karty), nie biorąc w niej czynnego udziału.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "observer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas turnieju szachowego każdy <w>kibic</w> musi zachować absolutną ciszę.",
                            "target_lang_translations": {
                                "ru": "Во время шахматного турнира каждый <w>зритель</w> должен соблюдать абсолютную тишину."
                            }
                        },
                        {
                            "text": "Nie lubię, gdy jakiś <w>kibic</w> zagląda mi w karty i komentuje moje ruchy.",
                            "target_lang_translations": {
                                "ru": "Я не люблю, когда какой-нибудь <w>наблюдатель</w> заглядывает мне в карты и комментирует мои ходы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, пассивно наблюдающий за какой-либо ситуацией или игрой (например, в карты), не принимая в ней активного участия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зритель",
                                "target_lang_sense_clarification": "тот, кто смотрит за происходящим"
                            },
                            {
                                "target_lang_word": "наблюдатель",
                                "target_lang_sense_clarification": "тот, кто следит за ходом действий со стороны"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "szczebiot",
        "zaszczebiotać",
        "szczebiotliwy",
        "szczebiotka"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ccc024c7-f087-35a6-b824-e4954d7ca178",
                    "sense_definition": "Wydawać szybkie, wysokie i przyjemne dla ucha dźwięki, charakterystyczne dla małych ptaków.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_birds",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kolorowe ptaki <szczebiotały> głośno w koronie starego dębu.",
                            "target_lang_translations": {
                                "ru": "Разноцветные птицы громко <щебетали> в кроне старого дуба."
                            }
                        },
                        {
                            "text": "Lubię słuchać, jak wróble <szczebioczą> o wschodzie słońca.",
                            "target_lang_translations": {
                                "ru": "Я люблю слушать, как воробьи <щебечут> на рассвете."
                            }
                        }
                    ],
                    "synonyms": [
                        "ćwierkać",
                        "świergotać"
                    ],
                    "common_phrases": [
                        "ptaki szczebioczą",
                        "wesoło szczebiotać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать быстрые, высокие и приятные звуки, характерные для маленьких птиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щебетать",
                                "target_lang_sense_clarification": "о пении птиц"
                            },
                            {
                                "target_lang_word": "чирикать",
                                "target_lang_sense_clarification": "издавать короткие звуки (о воробьях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e73c6e3a-d0e0-3f67-aa9d-5d4b0335df1a",
                    "sense_definition": "Mówić szybko, dużo i wysokim głosem, zazwyczaj w sposób wesoły i beztroski.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mała dziewczynka <szczebiotała> radośnie, opowiadając o swojej nowej lalce.",
                            "target_lang_translations": {
                                "ru": "Маленькая девочка радостно <щебетала>, рассказывая о своей новой кукле."
                            }
                        },
                        {
                            "text": "Przyjaciółki siedziały w kawiarni i <szczebiotały> o ostatnich wydarzeniach.",
                            "target_lang_translations": {
                                "ru": "Подруги сидели в кафе и <щебетали> о последних событиях."
                            }
                        }
                    ],
                    "synonyms": [
                        "paplać",
                        "trajkotać"
                    ],
                    "common_phrases": [
                        "szczebiotać radośnie",
                        "słodko szczebiotać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить быстро, много и высоким голосом, обычно весело и беззаботно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щебетать",
                                "target_lang_sense_clarification": "оживленно и весело болтать"
                            },
                            {
                                "target_lang_word": "тараторить",
                                "target_lang_sense_clarification": "говорить очень быстро"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "вход",
        "входной",
        "вхождение"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ae3f43d-64db-3027-85f1-401749ef47af",
                    "sense_definition": "Перемещаться внутрь помещения, здания или другого пространства; переступать порог.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_entry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое утро учитель <входит> в класс ровно в девять часов.",
                            "target_lang_translations": {
                                "en": "Every morning the teacher <enters> the classroom at exactly nine o'clock."
                            }
                        },
                        {
                            "text": "Пожалуйста, <входите>, дверь не заперта.",
                            "target_lang_translations": {
                                "en": "Please <come in>, the door is not locked."
                            }
                        }
                    ],
                    "antonyms": [
                        "выходить"
                    ],
                    "common_phrases": [
                        "входить в комнату",
                        "входить в дом",
                        "входить без стука",
                        "входить в здание"
                    ],
                    "target_lang_definitions": {
                        "en": "To move into a room, building, or other space; to cross a threshold."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "general / formal"
                            },
                            {
                                "target_lang_word": "come in",
                                "target_lang_sense_clarification": "when the speaker is inside"
                            },
                            {
                                "target_lang_word": "go in",
                                "target_lang_sense_clarification": "when the speaker is outside"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc7a36a4-046e-3676-8679-55d3eae251bf",
                    "sense_definition": "Быть составной частью чего-либо, включаться в список или группу.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В стоимость билета <входит> обед и напитки.",
                            "target_lang_translations": {
                                "en": "Lunch and drinks <are included> in the ticket price."
                            }
                        },
                        {
                            "text": "Этот талантливый игрок <входит> в состав национальной сборной.",
                            "target_lang_translations": {
                                "en": "This talented player <is part of> the national team."
                            }
                        }
                    ],
                    "antonyms": [
                        "выходить"
                    ],
                    "common_phrases": [
                        "входить в состав",
                        "входить в стоимость",
                        "входить в планы",
                        "входить в историю"
                    ],
                    "target_lang_definitions": {
                        "en": "To be a constituent part of something; to be included in a list or group."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be included",
                                "target_lang_sense_clarification": "in a price, list, or scope"
                            },
                            {
                                "target_lang_word": "be part of",
                                "target_lang_sense_clarification": "a group, team, or whole"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24225414-518a-3bb7-b6a6-b63ee5394067",
                    "sense_definition": "Проникать вглубь материала или поверхности (обычно об острых предметах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "penetration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Острый нож легко <входил> в мягкое масло.",
                            "target_lang_translations": {
                                "en": "The sharp knife easily <went into> the soft butter."
                            }
                        },
                        {
                            "text": "Гвоздь <входил> в дерево с большим трудом.",
                            "target_lang_translations": {
                                "en": "The nail <went into> the wood with great difficulty."
                            }
                        }
                    ],
                    "synonyms": [
                        "втыкаться",
                        "вонзаться"
                    ],
                    "antonyms": [
                        "выходить"
                    ],
                    "common_phrases": [
                        "легко входить",
                        "туго входить"
                    ],
                    "target_lang_definitions": {
                        "en": "To penetrate deep into a material or surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go in",
                                "target_lang_sense_clarification": "movement into a substance"
                            },
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "formal / technical"
                            },
                            {
                                "target_lang_word": "penetrate",
                                "target_lang_sense_clarification": "piercing deep into something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2a43663a-3b9c-3d23-a904-08a6bcadb64e",
                    "sense_definition": "A song of praise or loyalty, especially one officially adopted by a country or organization.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "song_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everyone in the stadium stood up to sing the national <w>anthem</w>.",
                            "target_lang_translations": {
                                "ru": "Все на стадионе встали, чтобы спеть национальный <w>гимн</w>."
                            }
                        },
                        {
                            "text": "The players felt proud listening to their country's <w>anthem</w> before the game.",
                            "target_lang_translations": {
                                "ru": "Игроки чувствовали гордость, слушая <w>гимн</w> своей страны перед матчем."
                            }
                        }
                    ],
                    "synonyms": [
                        "hymn"
                    ],
                    "common_phrases": [
                        "national anthem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественная песня хвалы или верности, особенно официально принятая страной или организацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гимн",
                                "target_lang_sense_clarification": "торжественная песня страны или организации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58b1099a-615f-34d7-b4f3-f35375169549",
                    "sense_definition": "A choral composition with sacred lyrics, often performed during a church service.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "song_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The choir practiced a beautiful <w>anthem</w> for the Sunday morning service.",
                            "target_lang_translations": {
                                "ru": "Хор репетировал красивое <w>песнопение</w> для воскресной утренней службы."
                            }
                        },
                        {
                            "text": "The composer wrote a complex choral <w>anthem</w> that was performed in the cathedral.",
                            "target_lang_translations": {
                                "ru": "Композитор написал сложное хоровое <w>песнопение</w>, которое было исполнено в соборе."
                            }
                        }
                    ],
                    "synonyms": [
                        "canticle",
                        "chorale"
                    ],
                    "common_phrases": [
                        "choral anthem",
                        "church anthem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хоровая композиция с духовным текстом, часто исполняемая во время церковной службы."
                    },
                    "translations": {
                        "ru": [
                            {

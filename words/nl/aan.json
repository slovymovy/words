    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "4d093755-e59b-3af4-b16e-d505d3bcdf3d",
                    "sense_definition": "Fysiek verbonden met, hangend aan of bevestigd tegen een oppervlak.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het mooie schilderij hangt <w>aan</w> de muur.",
                            "target_lang_translations": {
                                "ru": "Красивая картина висит <w>на</w> стене.",
                                "en": "The beautiful painting hangs <w>on</w> the wall."
                            }
                        },
                        {
                            "text": "Er zit een beetje modder <w>aan</w> je schoen.",
                            "target_lang_translations": {
                                "ru": "<w>На</w> твоем ботинке немного грязи.",
                                "en": "There is a bit of mud <w>on</w> your shoe."
                            }
                        }
                    ],
                    "synonyms": [
                        "tegen"
                    ],
                    "common_phrases": [
                        "aan de muur",
                        "aan elkaar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически соединен с чем-либо, висит на чем-либо или прикреплен к поверхности.",
                        "en": "Physically connected to, hanging on, or attached to a surface."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "указывает на нахождение на поверхности"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "positioned at the outer surface of"
                            },
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "attached or connected to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74547038-2937-3cb5-b07c-88324bbc73a7",
                    "sense_definition": "In de directe nabijheid van, grenzend aan of gelegen bij.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij zitten elke avond gezellig <w>aan</w> tafel.",
                            "target_lang_translations": {
                                "ru": "Мы каждый вечер уютно сидим <w>за</w> столом.",
                                "en": "We sit cozily <w>at</w> the table every evening."
                            }
                        },
                        {
                            "text": "Zij wonen in een groot huis <w>aan</w> zee.",
                            "target_lang_translations": {
                                "ru": "Они живут в большом доме <w>у</w> моря.",
                                "en": "They live in a big house <w>by</w> the sea."
                            }
                        }
                    ],
                    "synonyms": [
                        "tegen",
                        "in",
                        "voor"
                    ],
                    "common_phrases": [
                        "aan tafel",
                        "aan zee",
                        "aan de kust"
                    ],
                    "target_lang_definitions": {
                        "ru": "В непосредственной близости от, граничит с или расположен у.",
                        "en": "In the immediate vicinity of, bordering on, or located at."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "около, рядом с"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "используется в выражении «за столом»"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "используется с водоемами (на море)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "location (e.g., at the table)"
                            },
                            {
                                "target_lang_word": "by",
                                "target_lang_sense_clarification": "proximity (e.g., by the sea)"
                            },
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "location (e.g., on the coast)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c57dc80-ff16-3bf3-ab99-a799ba20e05d",
                    "sense_definition": "Geeft de ontvanger of het doelwit van een handeling aan (meewerkend voorwerp).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "recipient",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik geef het cadeau <w>aan</w> mijn beste vriend.",
                            "target_lang_translations": {
                                "ru": "Я дарю подарок <w>своему лучшему другу</w>.",
                                "en": "I give the gift <w>to</w> my best friend."
                            }
                        },
                        {
                            "text": "Heb je het nieuws al <w>aan</w> je ouders verteld?",
                            "target_lang_translations": {
                                "ru": "Ты уже рассказал новости <w>своим родителям</w>?",
                                "en": "Have you told the news <w>to</w> your parents yet?"
                            }
                        }
                    ],
                    "synonyms": [
                        "voor"
                    ],
                    "common_phrases": [
                        "aan wie",
                        "aan mij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на получателя или объект действия (соответствует дательному падежу).",
                        "en": "Indicates the recipient or target of an action."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "(дательный падеж)",
                                "target_lang_sense_clarification": "обычно переводится без предлога, изменением окончания слова"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "по направлению к кому-то"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "recipient of an action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "336fb169-5d75-3a08-90a0-3fe0df9cf2d9",
                    "sense_definition": "Onderdeel van een constructie die aangeeft dat een handeling op dit moment bezig is (gevolgd door 'het' en een werkwoord).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "continuous_aspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stoor mij niet, ik ben <w>aan</w> het werk.",
                            "target_lang_translations": {
                                "ru": "Не мешай мне, я <w>за</w> работой.",
                                "en": "Do not disturb me, I am <w>at</w> work."
                            }
                        },
                        {
                            "text": "Zij waren gisteren de hele dag <w>aan</w> het studeren.",
                            "target_lang_translations": {
                                "ru": "Они вчера весь день <w>учились</w>.",
                                "en": "They were <w>studying</w> all day yesterday."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan het werk",
                        "aan de gang"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть конструкции, указывающей на то, что действие происходит в данный момент.",
                        "en": "Part of a construction indicating that an action is currently in progress."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в процессе",
                                "target_lang_sense_clarification": "указывает на длительность действия"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "занятый чем-то (за работой)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "engaged in (e.g., at work)"
                            },
                            {
                                "target_lang_word": "-ing",
                                "target_lang_sense_clarification": "continuous aspect marker"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a142e25-f42c-390b-9a18-17732560a961",
                    "sense_definition": "Betreffende; met betrekking tot iets of iemand.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik weet niet wat er <w>aan</w> de hand is.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, что <w>происходит</w>.",
                                "en": "I don't know what is going <w>on</w>."
                            }
                        },
                        {
                            "text": "Ik twijfel soms <w>aan</w> zijn eerlijkheid.",
                            "target_lang_translations": {
                                "ru": "Я иногда сомневаюсь <w>в</w> его честности.",
                                "en": "I sometimes have doubts <w>about</w> his honesty."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan de hand",
                        "twijfelen aan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Касательно; по отношению к чему-либо или кому-либо.",
                        "en": "Concerning; with regard to something or someone."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "сомневаться в чем-то"
                            },
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "что случилось с..."
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "about",
                                "target_lang_sense_clarification": "concerning"
                            },
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "happening (e.g., going on)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ae24e4b-3e7a-384b-9ce8-fe200261ad69",
                    "sense_definition": "Om aan te geven wiens beurt of verantwoordelijkheid het is.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is nu <w>aan</w> jou om een beslissing te nemen.",
                            "target_lang_translations": {
                                "ru": "Теперь решение <w>за</w> тобой.",
                                "en": "It is now <w>up to</w> you to make a decision."
                            }
                        },
                        {
                            "text": "De volgende zet in het spel is <w>aan</w> de tegenstander.",
                            "target_lang_translations": {
                                "ru": "Следующий ход в игре <w>за</w> противником.",
                                "en": "The next move in the game is <w>up to</w> the opponent."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan de beurt",
                        "aan jou"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на чью-либо очередь или ответственность.",
                        "en": "Indicates whose turn or responsibility it is."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "очередь/решение за кем-то"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "up to",
                                "target_lang_sense_clarification": "responsibility/choice"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77d4232c-2c85-31e9-96a5-495e1a7322ad",
                    "sense_definition": "Als gevolg van; lijdend aan (vaak gebruikt bij ziektes of oorzaken).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude man is overleden <w>aan</w> een hartstilstand.",
                            "target_lang_translations": {
                                "ru": "Старик умер <w>от</w> остановки сердца.",
                                "en": "The old man died <w>of</w> cardiac arrest."
                            }
                        },
                        {
                            "text": "Veel mensen in deze regio lijden <w>aan</w> die ziekte.",
                            "target_lang_translations": {
                                "ru": "Многие люди в этом регионе страдают <w>от</w> этой болезни.",
                                "en": "Many people in this region suffer <w>from</w> that disease."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lijden aan",
                        "sterven aan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вследствие; страдать от (часто используется с болезнями или причинами).",
                        "en": "As a result of; suffering from (often used with diseases or causes)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "указывет на причину или болезнь"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "of",
                                "target_lang_sense_clarification": "cause (e.g., died of)"
                            },
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "source (e.g., suffer from)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {

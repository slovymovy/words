                    "frequency": "Middle",
                    "semantic_group_id": "heel_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb een pijnlijke blaar op mijn <w>hak</w>.",
                            "target_lang_translations": {

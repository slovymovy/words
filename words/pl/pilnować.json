                        },
                        {
                            "text": "Musimy <pilnować> czasu, żeby nie spóźnić się na pociąg.",
                            "target_lang_translations": {
                                "ru": "Мы должны <следить> за временем, чтобы не опоздать на поезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "dbać"
                    ],
                    "common_phrases": [

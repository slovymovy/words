{
    "word_family": [
        "назначение",
        "предназначить",
        "переназначить",
        "обозначить"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cbd39c98-8b0c-3190-82ca-1fdcec9f2f62",
                    "sense_definition": "Установить время или место для встречи или какого-либо события.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <назначить> нашу встречу на завтрашнее утро в офисе.",
                            "target_lang_translations": {
                                "en": "We decided to <schedule> our meeting for tomorrow morning at the office."
                            }
                        },
                        {
                            "text": "Молодой человек хотел <назначить> девушке свидание в парке у фонтана.",
                            "target_lang_translations": {
                                "en": "The young man wanted to <arrange> a date with the girl in the park by the fountain."
                            }
                        }
                    ],
                    "synonyms": [
                        "установить",
                        "наметить",
                        "определить"
                    ],
                    "antonyms": [
                        "отменить"
                    ],
                    "common_phrases": [
                        "назначить встречу",
                        "назначить свидание",
                        "назначить время",
                        "назначить дату"
                    ],
                    "target_lang_definitions": {
                        "en": "To set a time or place for a meeting or an event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "schedule",
                                "target_lang_sense_clarification": "an event or meeting"
                            },
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "a time or date"
                            },
                            {
                                "target_lang_word": "appoint",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "fix",
                                "target_lang_sense_clarification": "a date"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cbb1aec-111b-3ad8-9958-524a24286cc8",
                    "sense_definition": "Поставить кого-либо на какую-либо должность или поручить выполнение работы.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "job",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Совет директоров планирует <назначить> нового управляющего в следующем месяце.",
                            "target_lang_translations": {
                                "en": "The board of directors plans to <appoint> a new manager next month."
                            }
                        },
                        {
                            "text": "Президент подписал указ, чтобы <назначить> генерала командующим армией.",
                            "target_lang_translations": {
                                "en": "The president signed a decree to <appoint> the general as commander of the army."
                            }
                        }
                    ],
                    "synonyms": [
                        "поставить",
                        "утвердить"
                    ],
                    "antonyms": [
                        "уволить",
                        "снять",
                        "сместить"
                    ],
                    "common_phrases": [
                        "назначить на должность",
                        "назначить директором",
                        "назначить главным",
                        "назначить исполняющим обязанности"
                    ],
                    "target_lang_definitions": {
                        "en": "To place someone in a position or assign a task."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appoint",
                                "target_lang_sense_clarification": "to a position"
                            },
                            {
                                "target_lang_word": "assign",
                                "target_lang_sense_clarification": "a role or task"
                            },
                            {
                                "target_lang_word": "name",
                                "target_lang_sense_clarification": "to a post"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2dfeb62-0fe2-38a0-852d-20d2a09058ce",
                    "sense_definition": "Предписать применение лекарства или прохождение лечебных процедур (о враче).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После осмотра врач решил <назначить> пациенту курс антибиотиков.",
                            "target_lang_translations": {
                                "en": "After the examination, the doctor decided to <prescribe> a course of antibiotics for the patient."
                            }
                        },
                        {
                            "text": "Доктор должен <назначить> правильное лечение, чтобы избежать осложнений.",
                            "target_lang_translations": {
                                "en": "The doctor must <prescribe> the correct treatment to avoid complications."
                            }
                        }
                    ],
                    "synonyms": [
                        "прописать",
                        "выписать"
                    ],
                    "antonyms": [
                        "отменить"
                    ],
                    "common_phrases": [
                        "назначить лечение",
                        "назначить лекарство",
                        "назначить уколы",
                        "назначить анализы"
                    ],
                    "target_lang_definitions": {
                        "en": "To order the use of medicine or medical procedures."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prescribe",
                                "target_lang_sense_clarification": "medicine or treatment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18426ae5-f645-35e4-aa5b-755c35e78291",
                    "sense_definition": "Установить размер денежной выплаты, цены или определить меру наказания.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "determination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Судья может <назначить> преступнику наказание в виде крупного штрафа.",
                            "target_lang_translations": {
                                "en": "The judge can <impose> a punishment on the criminal in the form of a large fine."
                            }
                        },
                        {
                            "text": "Комиссия должна рассмотреть документы и <назначить> пенсию по возрасту.",
                            "target_lang_translations": {
                                "en": "The commission must review the documents and <assign> an old-age pension."
                            }
                        }
                    ],
                    "synonyms": [
                        "установить",
                        "определить"
                    ],
                    "common_phrases": [
                        "назначить цену",
                        "назначить пенсию",
                        "назначить штраф",
                        "назначить наказание"
                    ],
                    "target_lang_definitions": {
                        "en": "To determine an amount of money, price, or punishment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "a price or amount"
                            },
                            {
                                "target_lang_word": "impose",
                                "target_lang_sense_clarification": "a fine or punishment"
                            },
                            {
                                "target_lang_word": "assign",
                                "target_lang_sense_clarification": "a pension or benefit"
                            }
                        ]
                    }
                }
            ]
        }
    ]

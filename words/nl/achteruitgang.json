            "senses": [
                {
                    "sense_id": "879f6de5-a2cd-36b3-ab8f-367619d68ba7",
                    "sense_definition": "Een uitgang aan de achterkant van een gebouw of terrein.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bezoekers van het brandende café wisten via de <w>achteruitgang</w> te ontsnappen.",
                            "target_lang_translations": {
                                "en": "The visitors of the burning café managed to escape via the <w>back exit</w>.",
                                "ru": "Посетителям горящего кафе удалось спастись через <w>чёрный ход</w>."
                            }
                        },
                        {
                            "text": "Omdat de voordeur op slot zat, moesten we de <w>achteruitgang</w> gebruiken.",
                            "target_lang_translations": {
                                "en": "Because the front door was locked, we had to use the <w>rear exit</w>.",
                                "ru": "Поскольку входная дверь была заперта, нам пришлось воспользоваться <w>задним выходом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "via de achteruitgang",
                        "de achteruitgang nemen"
                    ],
                    "target_lang_definitions": {
                        "en": "An exit located at the back of a building or property.",
                        "ru": "Выход, расположенный с тыльной стороны здания или территории."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "back exit",
                                "target_lang_sense_clarification": "common term"
                            },
                            {
                                "target_lang_word": "rear exit",
                                "target_lang_sense_clarification": "slightly more formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чёрный ход",
                                "target_lang_sense_clarification": "служебный или задний вход в здание"
                            },
                            {
                                "target_lang_word": "задний выход",
                                "target_lang_sense_clarification": "буквальное обозначение выхода сзади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df2e190c-fb75-3593-bb8c-ae41290df740",
                    "sense_definition": "Het proces van slechter worden of in kwaliteit afnemen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ondanks haar lichamelijke <w>achteruitgang</w>, bleef ze heel actief.",
                            "target_lang_translations": {
                                "en": "Despite her physical <w>deterioration</w>, she remained very active.",
                                "ru": "Несмотря на физическое <w>ухудшение</w>, она оставалась очень активной."
                            }
                        },
                        {
                            "text": "De economische <w>achteruitgang</w> zorgde voor veel werkloosheid in de regio.",
                            "target_lang_translations": {
                                "en": "The economic <w>decline</w> caused a lot of unemployment in the region.",
                                "ru": "Экономический <w>спад</w> стал причиной высокой безработицы в регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "verslechtering",
                        "verergering"
                    ],
                    "antonyms": [
                        "vooruitgang",
                        "progressie"
                    ],
                    "common_phrases": [
                        "economische achteruitgang",
                        "lichamelijke achteruitgang",
                        "sterke achteruitgang"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of becoming worse or decreasing in quality.",
                        "ru": "Процесс ухудшения состояния или снижения качества."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decline",
                                "target_lang_sense_clarification": "general decrease in quality or status"
                            },
                            {
                                "target_lang_word": "deterioration",
                                "target_lang_sense_clarification": "worsening of physical condition"
                            },
                            {
                                "target_lang_word": "regression",
                                "target_lang_sense_clarification": "returning to a less developed state"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ухудшение",
                                "target_lang_sense_clarification": "общий термин для снижения качества или состояния"
                            },
                            {
                                "target_lang_word": "спад",
                                "target_lang_sense_clarification": "часто используется в экономическом контексте"
                            },
                            {
                                "target_lang_word": "регресс",
                                "target_lang_sense_clarification": "возвращение к менее развитому состоянию"

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f3dd555d-74ff-3eac-a236-5f44c1584cc2",
                    "sense_definition": "Goed bestand tegen gebruik en slijtage; van stevige en betrouwbare kwaliteit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quality_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die <w>degelijke</w> tent is bestand tegen de storm.",
                            "target_lang_translations": {
                                "ru": "Эта <w>добротная</w> палатка устойчива к шторму."
                            }
                        },
                        {
                            "text": "Mijn opa kocht altijd <w>degelijke</w> schoenen die jarenlang meegingen.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка всегда покупал <w>прочные</w> ботинки, которые носились годами."
                            }
                        }
                    ],
                    "synonyms": [
                        "robuust"
                    ],
                    "antonyms": [
                        "ondegelijk"
                    ],
                    "common_phrases": [
                        "een degelijke auto",
                        "degelijk materiaal",
                        "een degelijke fiets"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо выдерживающий использование и износ; прочного и надежного качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добротный",
                                "target_lang_sense_clarification": "о качестве материала или изделия"
                            },
                            {
                                "target_lang_word": "прочный",
                                "target_lang_sense_clarification": "стойкий к износу"
                            },
                            {
                                "target_lang_word": "надежный",
                                "target_lang_sense_clarification": "на которого можно положиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c864b82-e969-348d-98e5-fe6bdd33b3cc",
                    "sense_definition": "Betrouwbaar, eerlijk en serieus van aard, soms met de bijklank van saai of ouderwets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quality_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je niet met een <w>degelijk</w> iemand trouwt, zal ik je onterven!",
                            "target_lang_translations": {
                                "ru": "Если ты не вступишь в брак с <w>порядочным</w> человеком, я лишу тебя наследства!"
                            }
                        },
                        {
                            "text": "Hij komt uit een <w>degelijk</w> gezin waar hard werken de norm is.",
                            "target_lang_translations": {
                                "ru": "Он из <w>благопристойной</w> семьи, где упорный труд является нормой."
                            }
                        }
                    ],
                    "antonyms": [
                        "onbetrouwbaar",
                        "luchthartig",
                        "onbezorgd"
                    ],
                    "common_phrases": [
                        "een degelijk gezin",
                        "een degelijke opvoeding",
                        "een degelijke huisvader"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надежный, честный и серьезный по характеру, иногда с оттенком скуки или старомодности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порядочный",
                                "target_lang_sense_clarification": "честный и достойный уважения"
                            },
                            {
                                "target_lang_word": "благопристойный",
                                "target_lang_sense_clarification": "соответствующий нормам приличия"
                            },
                            {
                                "target_lang_word": "солидный",
                                "target_lang_sense_clarification": "серьезный и представительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1805cfd8-e9ee-3d51-bb54-770220049c3d",
                    "sense_definition": "Grondig, fundamenteel en goed onderbouwd.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quality_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wetenschapper heeft een <w>degelijk</w> onderzoek uitgevoerd naar de oorzaken.",
                            "target_lang_translations": {
                                "ru": "Ученый провел <w>основательное</w> исследование причин."
                            }
                        },
                        {
                            "text": "Er is geen <w>degelijk</w> bewijs gevonden voor zijn beweringen.",
                            "target_lang_translations": {
                                "ru": "Не было найдено никаких <w>весомых</w> доказательств его утверждений."
                            }
                        }
                    ],
                    "synonyms": [
                        "diepgaand"
                    ],
                    "common_phrases": [
                        "degelijk onderzoek",
                        "degelijke kennis",
                        "degelijk bewijs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обстоятельный, фундаментальный и хорошо обоснованный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основательный",
                                "target_lang_sense_clarification": "глубокий и серьезный"
                            },
                            {
                                "target_lang_word": "весомый",
                                "target_lang_sense_clarification": "убедительный (о доказательствах)"
                            },
                            {
                                "target_lang_word": "тщательный",
                                "target_lang_sense_clarification": "аккуратный и подробный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e353ff82-3379-3af5-ad27-696d51d34bf3",
                    "sense_definition": "Op een grondige, stevige of duidelijke wijze.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze hebben het probleem <w>degelijk</w> aangepakt om herhaling te voorkomen.",

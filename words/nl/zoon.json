    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0e5d2bc6-6d51-36db-9be4-01b149779ba6",
                    "sense_definition": "Een mannelijk kind in relatie tot zijn ouders.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn <w>zoon</w> gaat volgend jaar naar de universiteit.",
                            "target_lang_translations": {
                                "en": "My <w>son</w> is going to university next year.",
                                "ru": "Мой <w>сын</w> в следующем году поступает в университет."
                            }
                        },
                        {
                            "text": "Ze heeft drie zonen en een dochter.",
                            "target_lang_translations": {
                                "en": "She has three sons and a daughter.",
                                "ru": "У неё три сына и одна дочь."
                            }
                        }
                    ],
                    "antonyms": [
                        "dochter"
                    ],
                    "common_phrases": [
                        "oudste zoon",
                        "jongste zoon",
                        "verloren zoon"
                    ],
                    "target_lang_definitions": {
                        "en": "A male child in relation to his parents.",
                        "ru": "Лицо мужского пола по отношению к своим родителям."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "son",
                                "target_lang_sense_clarification": "male offspring"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сын",
                                "target_lang_sense_clarification": "ребенок мужского пола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1b58bad-84a0-3fac-aa80-82562127cabc",
                    "sense_definition": "Aanduiding van Jezus Christus als een van de drie personen van de God van het christendom.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "In de kerk wordt vaak gesproken over de Vader en de <w>Zoon</w>.",
                            "target_lang_translations": {
                                "en": "In church, people often speak about the Father and the <w>Son</w>.",
                                "ru": "В церкви часто говорят об Отце и <w>Сыне</w>."
                            }
                        },
                        {
                            "text": "Gelovigen bidden tot de Vader, de <w>Zoon</w> en de Heilige Geest.",
                            "target_lang_translations": {
                                "en": "Believers pray to the Father, the <w>Son</w>, and the Holy Spirit.",
                                "ru": "Верующие молятся Отцу, <w>Сыну</w> и Святому Духу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de Vader en de Zoon"
                    ],
                    "target_lang_definitions": {
                        "en": "Jesus Christ referred to as one of the three persons of the Christian God.",
                        "ru": "Иисус Христос как одна из ипостасей Бога в христианстве."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Son",
                                "target_lang_sense_clarification": "religious title"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "Сын",
                                "target_lang_sense_clarification": "Иисус Христос"
                            }
                        ]
                    }
                }
            ]
        }
    ]

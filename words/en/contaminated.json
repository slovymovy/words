            "senses": [
                {
                    "sense_id": "d79f9eab-aa7b-33d4-9893-d05701f1bb0f",
                    "sense_definition": "Made impure or unfit for use by the introduction of harmful or undesirable elements.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers were advised not to drink the <w>contaminated</w> water.",
                            "target_lang_translations": {
                                "ru": "Жителям деревни посоветовали не пить <w>загрязнённую</w> воду."
                            }
                        },
                        {
                            "text": "All <w>contaminated</w> instruments must be sterilized before they can be used again.",
                            "target_lang_translations": {
                                "ru": "Все <w>загрязнённые</w> инструменты должны быть простерилизованы перед повторным использованием."
                            }
                        }
                    ],
                    "synonyms": [
                        "polluted",
                        "tainted",
                        "adulterated"
                    ],
                    "antonyms": [
                        "pure"
                    ],
                    "common_phrases": [
                        "contaminated water",
                        "contaminated food",
                        "contaminated soil",
                        "heavily contaminated"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ставший нечистым или непригодным для использования из-за попадания вредных или нежелательных элементов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загрязнённый",
                                "target_lang_sense_clarification": "содержащий вредные примеси или грязь"
                            },
                            {
                                "target_lang_word": "заражённый",
                                "target_lang_sense_clarification": "содержащий инфекцию, вирусы или радиацию"
                            }
                        ]
                    }
                }
            ]
        },
        {

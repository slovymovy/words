{
    "word_family": [
        "enqueue",
        "dequeue",
        "queuer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9bb8db0a-f523-31db-adb5-cf10a1a0e72b",
                    "sense_definition": "A line of people, vehicles, or objects waiting for their turn to be attended to or to proceed.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We stood in a long <w>queue</w> to buy tickets for the concert.",
                            "target_lang_translations": {
                                "ru": "Мы стояли в длинной <w>очереди</w>, чтобы купить билеты на концерт."
                            }
                        },
                        {
                            "text": "There is always a <w>queue</w> at the post office during lunch hour.",
                            "target_lang_translations": {
                                "ru": "В обеденный перерыв на почте всегда <w>очередь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "line",
                        "lineup"
                    ],
                    "common_phrases": [
                        "jump the queue",
                        "join the queue",
                        "stand in a queue"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ряд людей, транспортных средств или объектов, ожидающих своей очереди для обслуживания или прохода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очередь",
                                "target_lang_sense_clarification": "люди или предметы, стоящие друг за другом"
                            },
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "разговорное; конец очереди или сама очередь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33044f14-d488-3425-8353-84e819f3d6dd",
                    "sense_definition": "A sequence of people or things waiting to be processed, often not in a physical line.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your document is third in the printer <w>queue</w>.",
                            "target_lang_translations": {
                                "ru": "Ваш документ третий в <w>очереди</w> на печать."
                            }
                        },
                        {
                            "text": "There is a <w>queue</w> of applicants waiting to be interviewed.",
                            "target_lang_translations": {
                                "ru": "Существует <w>очередь</w> из кандидатов, ожидающих собеседования."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in the queue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Последовательность людей или предметов, ожидающих обработки, часто не в физической линии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очередь",
                                "target_lang_sense_clarification": "порядок ожидания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb76585c-f893-39f2-b40e-aead2397f1a8",
                    "sense_definition": "A data structure where elements are usually added to the back and removed from the front.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software uses a priority <w>queue</w> to manage tasks efficiently.",
                            "target_lang_translations": {
                                "ru": "Программное обеспечение использует приоритетную <w>очередь</w> для эффективного управления задачами."
                            }
                        },
                        {
                            "text": "Items in a FIFO <w>queue</w> are processed in the exact order they arrive.",
                            "target_lang_translations": {
                                "ru": "Элементы в <w>очереди</w> FIFO обрабатываются в точном порядке их поступления."
                            }
                        }
                    ],
                    "common_phrases": [
                        "message queue",
                        "priority queue"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Структура данных, в которой элементы обычно добавляются в конец, а удаляются из начала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очередь",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "400af649-2777-3115-bd80-45ce999eec0a",
                    "sense_definition": "A hairstyle consisting of a long braid or ponytail worn at the back of the head.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the past, men in some cultures wore their hair in a long <w>queue</w>.",
                            "target_lang_translations": {
                                "ru": "В прошлом мужчины в некоторых культурах носили волосы, заплетенные в длинную <w>косу</w>."
                            }
                        },
                        {
                            "text": "He tied his hair back into a neat <w>queue</w>.",
                            "target_lang_translations": {
                                "ru": "Он собрал волосы сзади в аккуратный <w>хвост</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical fashion"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прическа, состоящая из длинной косы или хвоста, носимая на затылке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коса",
                                "target_lang_sense_clarification": "сплетенные волосы"
                            },
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "собранные волосы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3442b0d-ecc8-3e96-ad45-c3770f9a9569",
                    "sense_definition": "The tail of an animal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heraldic lion was depicted with a forked <w>queue</w>.",
                            "target_lang_translations": {
                                "ru": "Геральдический лев был изображен с раздвоенным <w>хвостом</w>."
                            }
                        },
                        {
                            "text": "The coat of arms features a beast with a golden <w>queue</w>.",
                            "target_lang_translations": {
                                "ru": "На гербе изображен зверь с золотым <w>хвостом</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "heraldry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хвост животного (в геральдике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "часть тела животного"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a01410f0-fe27-3e8f-845e-1f1201aa0c0a",
                    "sense_definition": "To wait in a line of people or vehicles.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We had to <w>queue</w> for over an hour to get into the museum.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>стоять в очереди</w> больше часа, чтобы попасть в музей."
                            }
                        },
                        {
                            "text": "Fans began to <w>queue</w> up outside the stadium early in the morning.",
                            "target_lang_translations": {
                                "ru": "Фанаты начали <w>выстраиваться в очередь</w> у стадиона рано утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "line up"
                    ],
                    "common_phrases": [
                        "queue up",
                        "queue for tickets"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ждать в ряду людей или транспортных средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоять в очереди",
                                "target_lang_sense_clarification": "находиться в ожидании"
                            },
                            {
                                "target_lang_word": "ждать в очереди",
                                "target_lang_sense_clarification": "процесс ожидания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "469fa78d-cfa3-32c3-a0a2-cfdae291cfb5",
                    "sense_definition": "To add an item to a data queue.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The server <w>queued</w> the request for later processing.",
                            "target_lang_translations": {
                                "ru": "Сервер <w>поставил</w> запрос <w>в очередь</w> для последующей обработки."
                            }
                        },
                        {
                            "text": "All print jobs are <w>queued</w> automatically by the system.",
                            "target_lang_translations": {
                                "ru": "Все задания на печать автоматически <w>ставятся в очередь</w> системой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Добавлять элемент в очередь данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ставить в очередь",
                                "target_lang_sense_clarification": "информатика"
                            },
                            {
                                "target_lang_word": "добавлять в очередь",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fb8ea1b-a602-38aa-9810-74ecaee8f434",
                    "sense_definition": "To fasten hair into a braid or ponytail.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldier <w>queued</w> his hair according to the regulation of the time.",
                            "target_lang_translations": {
                                "ru": "Солдат <w>заплел</w> волосы <w>в косу</w> в соответствии с уставом того времени."
                            }
                        },
                        {
                            "text": "He <w>queued</w> his long hair neatly behind his head.",
                            "target_lang_translations": {
                                "ru": "Он аккуратно <w>собрал</w> свои длинные волосы <w>в хвост</w> на затылке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заплетать волосы в косу или собирать в хвост."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заплетать в косу",
                                "target_lang_sense_clarification": "делать прическу"
                            },
                            {
                                "target_lang_word": "собирать в хвост",
                                "target_lang_sense_clarification": "делать прическу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
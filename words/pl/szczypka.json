            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9ab7d840-5caf-368c-8cdd-69654c0b4779",
                    "sense_definition": "Regionalny wyrób cukierniczy z Lubelszczyzny w postaci twardej, barwnej laseczki wykonanej z cukru, sprzedawany często na odpustach i jarmarkach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na straganie podczas odpustu kupiłem sobie słodką <w>szczypkę</w>.",
                            "target_lang_translations": {
                                "ru": "В ларьке на церковной ярмарке я купил себе сладкую <w>сахарную палочку</w>."
                            }
                        },
                        {
                            "text": "Tradycyjna lubelska <w>szczypka</w> jest bardzo twarda i ma intensywny kolor.",
                            "target_lang_translations": {
                                "ru": "Традиционная люблинская <w>сахарная палочка</w> очень твердая и имеет насыщенный цвет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szczypka z cukru",
                        "słodka szczypka"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Lubelszczyzna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное кондитерское изделие из Люблинского воеводства в виде твердой цветной сахарной палочки, часто продаваемое на церковных праздниках и ярмарках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сахарная палочка",
                                "target_lang_sense_clarification": "региональная сладость"
                            },
                            {
                                "target_lang_word": "леденец",
                                "target_lang_sense_clarification": "твердая конфета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7c9e6de-e3af-35c1-a00e-271df279aba9",
                    "sense_definition": "W gwarze śląskiej: młoda roślina wyhodowana z nasiona lub fragmentu innej rośliny, przeznaczona do posadzenia w ziemi; sadzonka.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta mała <w>szczypka</w> potrzebuje dużo wody, żeby się przyjąć.",
                            "target_lang_translations": {
                                "ru": "Этому маленькому <w>саженцу</w> нужно много воды, чтобы прижиться."
                            }
                        },
                        {
                            "text": "Ogrodnik przyniósł nową <w>szczypkę</w>, którą wsadził do doniczki.",
                            "target_lang_translations": {
                                "ru": "Садовник принес новый <w>отросток</w>, который посадил в цветочный горшок."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) ablyjger, flancka, szczypek"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Górny Śląsk"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В силезском диалекте: молодое растение, выращенное из семени или части другого растения, предназначенное для посадки в грунт; саженец."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "саженец",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fe313c7b-7ec4-3248-a39a-b1f833a2befc",
                    "sense_definition": "Any of the foraging non-Bantu ethnic groups of southwestern Africa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "san_people_group",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "The <w>San</w> are renowned for their ancient rock art found throughout the region.",
                            "target_lang_translations": {
                                "ru": "Народ <w>сан</w> известен своей древней наскальной живописью, встречающейся по всему региону."
                            }
                        },
                        {
                            "text": "Anthropologists have long studied the hunter-gatherer lifestyle of the <w>San</w>.",
                            "target_lang_translations": {
                                "ru": "Антропологи давно изучают образ жизни охотников-собирателей народа <w>сан</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Bushmen"
                    ],
                    "common_phrases": [
                        "San people",
                        "San culture",
                        "San rock art"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собирательное название коренных народов Южной Африки, традиционно занимающихся охотой и собирательством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сан",
                                "target_lang_sense_clarification": "народ"
                            },
                            {
                                "target_lang_word": "бушмены",
                                "target_lang_sense_clarification": "устаревшее или общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7d5ec10-5714-310b-82b5-085b88312cc8",
                    "sense_definition": "Acronym of storage area network.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company installed a new <w>SAN</w> to manage its growing database requirements.",
                            "target_lang_translations": {
                                "ru": "Компания установила новую <w>сеть хранения данных</w> для управления растущими потребностями базы данных."
                            }
                        },
                        {
                            "text": "A fibre channel <w>SAN</w> provides high-speed data transfer between servers and storage.",
                            "target_lang_translations": {
                                "ru": "<w>Сеть хранения данных</w> на базе Fibre Channel обеспечивает высокоскоростную передачу данных между серверами и хранилищем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fibre channel SAN",
                        "virtual SAN",
                        "SAN storage"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "acronym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сеть хранения данных; архитектура сети для подключения внешних устройств хранения к серверам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сеть хранения данных",
                                "target_lang_sense_clarification": "компьютерный термин"
                            },
                            {
                                "target_lang_word": "SAN",
                                "target_lang_sense_clarification": "аббревиатура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4eaba60f-f9aa-3b26-a337-d0dcfde1ba3c",
                    "sense_definition": "A sanatorium.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent several months recovering from tuberculosis in the <w>san</w>.",
                            "target_lang_translations": {
                                "ru": "Он провел несколько месяцев, восстанавливаясь от туберкулеза в <w>санатории</w>."
                            }
                        },
                        {
                            "text": "The old <w>san</w> on the hill has been converted into luxury apartments.",
                            "target_lang_translations": {
                                "ru": "Старый <w>санаторий</w> на холме был переделан в элитные квартиры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in the san"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное сокращение от слова «санаторий»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "санаторий",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a324268-a31d-38c3-94ac-6f74d0d4f125",
                    "sense_definition": "Acronym of sinoatrial node.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>SAN</w> acts as the natural pacemaker for the human heart.",
                            "target_lang_translations": {
                                "ru": "<w>Синоатриальный узел</w> действует как естественный кардиостимулятор человеческого сердца."
                            }
                        },
                        {
                            "text": "Electrical impulses originate in the <w>SAN</w> and travel through the atria.",
                            "target_lang_translations": {
                                "ru": "Электрические импульсы возникают в <w>синоатриальном узле</w> и проходят через предсердия."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "acronym"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Синоатриальный узел; естественный водитель ритма сердца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синоатриальный узел",
                                "target_lang_sense_clarification": "анатомия"
                            },
                            {
                                "target_lang_word": "СА-узел",
                                "target_lang_sense_clarification": "медицинская аббревиатура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "530758dd-fcae-3a38-bd12-bb7c85d9d91b",
                    "sense_definition": "Abbreviation of South African.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The study compares Australian and <w>SAn</w> agricultural practices.",
                            "target_lang_translations": {
                                "ru": "В исследовании сравниваются австралийские и <w>южноафриканские</w> методы ведения сельского хозяйства."
                            }
                        },
                        {
                            "text": "Many <w>SAns</w> living abroad still follow the local rugby team.",
                            "target_lang_translations": {
                                "ru": "Многие <w>южноафриканцы</w>, живущие за границей, по-прежнему следят за местной командой по регби."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «южноафриканский» или «житель Южной Африки»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "южноафриканский",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "южноафриканец",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ef39163-f7d9-38e5-805d-bff6b53286fa",
                    "sense_definition": "An Ancient Greek letter representing /s/ (uppercase Ϻ, lowercase ϻ), originally used in some dialects instead of sigma.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The archaic letter <w>san</w> eventually fell out of use in favor of sigma.",
                            "target_lang_translations": {
                                "ru": "Архаичная буква <w>сан</w> со временем вышла из употребления, уступив место сигме."
                            }
                        },
                        {
                            "text": "Epigraphers identified the symbol as <w>san</w> on the ancient tablet.",
                            "target_lang_translations": {
                                "ru": "Эпиграфисты идентифицировали символ на древней табличке как <w>сан</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Архаическая буква греческого алфавита, обозначавшая звук /s/."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сан",
                                "target_lang_sense_clarification": "греческая буква"
                            }
                        ]
                    }
                }
            ]

{
    "word_family": [
        "aanzienlijk",
        "aangezien"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a03cfbc7-67cf-36f6-9821-aa9e6b6dc0a3",
                    "sense_definition": "Iemand of iets ten onrechte houden voor een ander persoon of zaak; beschouwen als.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_interpret",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het donker <w>zag</w> ik de struik <w>aan</w> voor een inbreker.",
                            "target_lang_translations": {
                                "en": "In the dark I <w>mistook</w> the bush for a burglar.",
                                "ru": "В темноте я <w>принял</w> куст за грабителя."
                            }
                        },
                        {
                            "text": "Mijn excuses, ik <w>zag</w> u <w>aan</w> voor iemand anders.",
                            "target_lang_translations": {
                                "en": "My apologies, I <w>took</w> you for someone else.",
                                "ru": "Прошу прощения, я <w>принял</w> вас за кого-то другого."
                            }
                        }
                    ],
                    "synonyms": [
                        "houden voor",
                        "beschouwen als"
                    ],
                    "common_phrases": [
                        "iemand aanzien voor"
                    ],
                    "target_lang_definitions": {
                        "en": "To wrongly identify someone or something as another person or thing.",
                        "ru": "Ошибочно принять кого-то или что-то за другого человека или предмет; обознаться."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mistake for",
                                "target_lang_sense_clarification": "to wrongly identify as"
                            },
                            {
                                "target_lang_word": "take for",
                                "target_lang_sense_clarification": "to assume to be"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "принимать за",
                                "target_lang_sense_clarification": "ошибочно считать кем-то другим"
                            },
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "воспринимать как кого-то другого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "911d1229-9996-33e1-b653-9d337d186f40",
                    "sense_definition": "Een situatie geduldig afwachten om te zien hoe deze zich ontwikkelt, zonder direct in te grijpen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_wait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De arts wilde de werking van het medicijn nog even <w>aanzien</w>.",
                            "target_lang_translations": {
                                "en": "The doctor wanted to <w>wait and see</w> the effect of the medication for a while.",
                                "ru": "Врач хотел еще некоторое время <w>понаблюдать</w> за действием лекарства."
                            }
                        },
                        {
                            "text": "We zullen de situatie nog een week <w>aanzien</w> voordat we beslissen.",
                            "target_lang_translations": {
                                "en": "We will <w>watch</w> the situation for another week before we decide.",
                                "ru": "Мы <w>понаблюдаем</w> за ситуацией еще неделю, прежде чем принять решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "afwachten",
                        "aankijken",
                        "dulden"
                    ],
                    "antonyms": [
                        "ingrijpen"
                    ],
                    "common_phrases": [
                        "het even aanzien",
                        "de kat uit de boom kijken"
                    ],
                    "target_lang_definitions": {
                        "en": "To wait patiently to observe how a situation develops without intervening immediately.",
                        "ru": "Терпеливо ждать, наблюдая за развитием ситуации, не вмешиваясь немедленно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wait and see",
                                "target_lang_sense_clarification": "to delay action until the outcome is known"
                            },
                            {
                                "target_lang_word": "watch",
                                "target_lang_sense_clarification": "to observe a situation unfold"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выжидать",
                                "target_lang_sense_clarification": "ждать развития событий"
                            },
                            {
                                "target_lang_word": "наблюдать",
                                "target_lang_sense_clarification": "следить за ходом дела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c26715d4-3775-3beb-994d-de4939e5eb9a",
                    "sense_definition": "De blik richten op iemand of iets; kijken naar (vaak gebruikt in contexten van iets verdragen).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_look",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kon het verdriet van zijn kinderen niet langer <w>aanzien</w>.",
                            "target_lang_translations": {
                                "en": "He could no longer <w>bear to watch</w> the sorrow of his children.",
                                "ru": "Он больше не мог <w>смотреть</w> на горе своих детей."
                            }
                        },
                        {
                            "text": "Waarom zit je me de hele tijd zo <w>aan</w> te <w>zien</w>?",
                            "target_lang_translations": {
                                "en": "Why are you <w>looking</w> at me like that the whole time?",
                                "ru": "Почему ты все время так на меня <w>смотришь</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "aankijken",
                        "gadeslaan"
                    ],
                    "antonyms": [
                        "wegkijken"
                    ],
                    "common_phrases": [
                        "niet om aan te zien"
                    ],
                    "target_lang_definitions": {
                        "en": "To direct one's gaze at someone or something; often used in contexts of being able to tolerate seeing something.",
                        "ru": "Направлять взгляд на кого-то или что-то; часто используется в контексте способности выносить (или не выносить) вид чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look at",
                                "target_lang_sense_clarification": "to direct eyes towards"
                            },
                            {
                                "target_lang_word": "bear to watch",
                                "target_lang_sense_clarification": "to tolerate seeing something unpleasant"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "смотреть",
                                "target_lang_sense_clarification": "направлять взгляд"
                            },
                            {
                                "target_lang_word": "видеть",
                                "target_lang_sense_clarification": "воспринимать зрительно (обычно в отрицании: не мог видеть)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15e11441-75bf-3093-8143-e2afffb04e4a",
                    "sense_definition": "Aanschouwen of waarnemen (voornamelijk in oudere of plechtige teksten).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_look",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Aanziet</w> de leliën des velds, hoe zij groeien.",
                            "target_lang_translations": {
                                "en": "<w>Consider</w> the lilies of the field, how they grow.",
                                "ru": "<w>Посмотрите</w> на полевые лилии, как они растут."
                            }
                        },
                        {
                            "text": "Wij <w>aanzagen</w> het wonder met grote verbazing.",
                            "target_lang_translations": {
                                "en": "We <w>beheld</w> the miracle with great amazement.",
                                "ru": "Мы <w>созерцали</w> это чудо с великим изумлением."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanschouwen",
                        "waarnemen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "often biblical or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To look upon or observe (mainly in archaic, biblical, or solemn contexts).",
                        "ru": "Взирать или наблюдать (преимущественно в устаревших, библейских или торжественных контекстах)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "behold",
                                "target_lang_sense_clarification": "archaic: to see or observe"
                            },
                            {
                                "target_lang_word": "consider",
                                "target_lang_sense_clarification": "biblical: to look at attentively"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "созерцать",
                                "target_lang_sense_clarification": "наблюдать (книжн.)"
                            },
                            {
                                "target_lang_word": "взирать",
                                "target_lang_sense_clarification": "смотреть с вниманием (устар.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4fd709e8-5488-3b6f-a1c7-5140d7720a3c",
                    "sense_definition": "De goede naam, invloed en het respect dat iemand of iets geniet bij anderen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door zijn harde werk geniet hij veel <w>aanzien</w> in de gemeenschap.",
                            "target_lang_translations": {
                                "en": "Due to his hard work, he enjoys much <w>respect</w> in the community.",
                                "ru": "Благодаря своему упорному труду он пользуется большим <w>уважением</w> в обществе."
                            }
                        },
                        {
                            "text": "Het schandaal heeft zijn <w>aanzien</w> ernstig beschadigd.",
                            "target_lang_translations": {
                                "en": "The scandal has seriously damaged his <w>reputation</w>.",
                                "ru": "Скандал серьезно повредил его <w>репутации</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "prestige",
                        "status",
                        "reputatie",
                        "achting"
                    ],
                    "antonyms": [
                        "smaad",
                        "minachting"
                    ],
                    "common_phrases": [
                        "in aanzien staan",
                        "aanzien genieten",
                        "zonder aanzien des persoons"
                    ],
                    "target_lang_definitions": {
                        "en": "The high standing, influence, and respect that someone or something enjoys among others.",
                        "ru": "Высокое положение, влияние и уважение, которым пользуется кто-то или что-то среди других."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prestige",
                                "target_lang_sense_clarification": "widespread respect and admiration"
                            },
                            {
                                "target_lang_word": "respect",
                                "target_lang_sense_clarification": "esteem or high regard"
                            },
                            {
                                "target_lang_word": "standing",
                                "target_lang_sense_clarification": "status or reputation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уважение",
                                "target_lang_sense_clarification": "почтение"
                            },
                            {
                                "target_lang_word": "авторитет",
                                "target_lang_sense_clarification": "влияние и признание"
                            },
                            {
                                "target_lang_word": "репутация",
                                "target_lang_sense_clarification": "общественное мнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65ea768c-503c-3fda-b763-f3e4331dbd45",
                    "sense_definition": "Betrekking of relatie; uitsluitend gebruikt in vaste voorzetseluitdrukkingen om een verband aan te geven.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het beleid ten <w>aanzien</w> van roken is strenger geworden.",
                            "target_lang_translations": {
                                "en": "The policy with <w>regard</w> to smoking has become stricter.",
                                "ru": "Политика в <w>отношении</w> курения стала строже."
                            }
                        },
                        {
                            "text": "Wat is uw standpunt ten <w>aanzien</w> van dit voorstel?",
                            "target_lang_translations": {
                                "en": "What is your position <w>regarding</w> this proposal?",
                                "ru": "Какова ваша позиция в <w>отношении</w> этого предложения?"
                            }
                        }
                    ],
                    "synonyms": [
                        "betrekking"
                    ],
                    "common_phrases": [
                        "ten aanzien van"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "used in prepositional phrases"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Relation or connection; used exclusively in fixed prepositional phrases.",
                        "ru": "Отношение или связь; используется исключительно в устойчивых предложных оборотах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regard",
                                "target_lang_sense_clarification": "in the phrase 'with regard to'"
                            },
                            {
                                "target_lang_word": "respect",
                                "target_lang_sense_clarification": "in the phrase 'with respect to'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отношение",
                                "target_lang_sense_clarification": "в выражении 'ten aanzien van' (в отношении)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a678f11b-d298-3f0b-920d-39eb1732c96c",
                    "sense_definition": "De uiterlijke verschijning of het voorkomen van een persoon of object.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de renovatie is het <w>aanzien</w> van de straat verbeterd.",
                            "target_lang_translations": {
                                "en": "The renovation has improved the <w>appearance</w> of the street.",
                                "ru": "Благодаря ремонту внешний <w>вид</w> улицы улучшился."
                            }
                        },
                        {
                            "text": "Het gebouw heeft een modern en strak <w>aanzien</w> gekregen.",
                            "target_lang_translations": {
                                "en": "The building has been given a modern and sleek <w>look</w>.",
                                "ru": "Здание приобрело современный и строгий <w>облик</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uiterlijk",
                        "voorkomen",
                        "aanblik"
                    ],
                    "common_phrases": [
                        "het aanzien waard"
                    ],
                    "target_lang_definitions": {
                        "en": "The physical look or external appearance of a person or object.",
                        "ru": "Внешний вид или облик человека или предмета."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appearance",
                                "target_lang_sense_clarification": "the way something looks"
                            },
                            {
                                "target_lang_word": "look",
                                "target_lang_sense_clarification": "visual aspect"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "внешность"
                            },
                            {
                                "target_lang_word": "облик",
                                "target_lang_sense_clarification": "внешний образ"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
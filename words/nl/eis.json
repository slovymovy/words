    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3142a64f-8f4b-3e83-af04-9ec48e559287",
                    "sense_definition": "Een dwingende voorwaarde of verlangen waaraan moet worden voldaan",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "demand_requirement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De belangrijkste <w>eis</w> voor deze functie is dat je vloeiend Nederlands spreekt.",
                            "target_lang_translations": {
                                "en": "The most important <w>requirement</w> for this position is that you speak fluent Dutch.",
                                "ru": "Самое важное <w>требование</w> к этой должности — свободное владение нидерландским языком."
                            }
                        },
                        {
                            "text": "Hij kon niet aan de zware <w>eis</w> voldoen en moest de wedstrijd verlaten.",
                            "target_lang_translations": {
                                "en": "He could not meet the tough <w>demand</w> and had to leave the match.",
                                "ru": "Он не смог выполнить суровое <w>требование</w> и был вынужден покинуть соревнование."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorwaarde",
                        "vereiste",
                        "conditie"
                    ],
                    "antonyms": [
                        "verzoek",
                        "aanbod"
                    ],
                    "common_phrases": [
                        "eisen stellen",
                        "aan de eisen voldoen",
                        "harde eis"
                    ],
                    "target_lang_definitions": {
                        "en": "A mandatory condition or desire that must be met",
                        "ru": "Обязательное условие или желание, которое должно быть выполнено"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "requirement",
                                "target_lang_sense_clarification": "something necessary or compulsory"
                            },
                            {
                                "target_lang_word": "demand",
                                "target_lang_sense_clarification": "a forceful request or condition"
                            },
                            {
                                "target_lang_word": "condition",
                                "target_lang_sense_clarification": "a stipulation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "требование",
                                "target_lang_sense_clarification": "настойчивая просьба, правило или норма"
                            },
                            {
                                "target_lang_word": "условие",
                                "target_lang_sense_clarification": "обстоятельство, от которого что-то зависит"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76a40e49-1051-3566-a849-fc24f6f8e6ee",
                    "sense_definition": "De strafmaat die de officier van justitie voorstelt aan de rechter in een rechtszaak",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_demand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter ging niet mee in de <w>eis</w> van het openbaar ministerie.",
                            "target_lang_translations": {
                                "en": "The judge did not go along with the <w>demand</w> of the Public Prosecution Service.",
                                "ru": "Судья не согласился с <w>требованием</w> прокуратуры."
                            }
                        },
                        {
                            "text": "Na het horen van de getuigen formuleerde de officier zijn definitieve <w>eis</w>.",
                            "target_lang_translations": {
                                "en": "After hearing the witnesses, the prosecutor formulated his final <w>sentence demand</w>.",
                                "ru": "Заслушав свидетелей, прокурор сформулировал свое окончательное <w>требование</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "strafeis"
                    ],
                    "common_phrases": [
                        "de eis luidt",
                        "conform de eis"
                    ],
                    "target_lang_definitions": {
                        "en": "The sentence or penalty proposed by the public prosecutor to the judge in a court case",
                        "ru": "Мера наказания, предлагаемая прокурором судье в судебном процессе"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sentence demand",
                                "target_lang_sense_clarification": "legal proposal by prosecutor"
                            },
                            {
                                "target_lang_word": "demand",
                                "target_lang_sense_clarification": "legal context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "требование",
                                "target_lang_sense_clarification": "юридический запрос прокурора о наказании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "69c7f2eb-5a10-361f-be76-b79bbd02c7e9",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>eisen</w>",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form_eisen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>eis</w> dat er onmiddellijk naar mijn klacht wordt geluisterd.",
                            "target_lang_translations": {
                                "en": "I <w>demand</w> that my complaint be listened to immediately.",
                                "ru": "Я <w>требую</w>, чтобы мою жалобу выслушали немедленно."
                            }
                        },
                        {
                            "text": "Als betalende klant <w>eis</w> ik een betere service dan dit.",
                            "target_lang_translations": {
                                "en": "As a paying customer, I <w>demand</w> better service than this.",
                                "ru": "Как платящий клиент, я <w>требую</w> лучшего обслуживания."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of <w>to demand</w>",
                        "ru": "Первое лицо единственного числа настоящего времени от глагола <w>eisen</w>"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demand",
                                "target_lang_sense_clarification": "to request forcefully"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "требую",
                                "target_lang_sense_clarification": "форма глагола 'требовать'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ad09053-fc05-3ccd-b4eb-e8dc517c7339",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>eisen</w> (bij inversie)",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_form_eisen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Eis</w> je nu echt dat we alles opnieuw gaan doen?",
                            "target_lang_translations": {
                                "en": "Do you really <w>demand</w> that we do everything all over again?",
                                "ru": "Ты действительно <w>требуешь</w>, чтобы мы всё переделали?"
                            }
                        },
                        {
                            "text": "Waarom <w>eis</w> je zoveel aandacht van je collega's?",
                            "target_lang_translations": {
                                "en": "Why do you <w>demand</w> so much attention from your colleagues?",
                                "ru": "Почему ты <w>требуешь</w> столько внимания от коллег?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Second person singular present tense of <w>to demand</w> (in inversion)",
                        "ru": "Второе лицо единственного числа настоящего времени от глагола <w>eisen</w> (при инверсии)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demand",
                                "target_lang_sense_clarification": "to request forcefully"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "требуешь",
                                "target_lang_sense_clarification": "форма глагола 'требовать'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "300fbc9a-a694-3f75-a2fa-420b741cea34",
                    "sense_definition": "Gebiedende wijs van <w>eisen</w>",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form_eisen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Eis</w> je geld terug als het product binnen een week kapot gaat.",
                            "target_lang_translations": {
                                "en": "<w>Demand</w> your money back if the product breaks within a week.",
                                "ru": "<w>Требуй</w> возврата денег, если товар сломается в течение недели."
                            }
                        },
                        {
                            "text": "<w>Eis</w> respect van anderen door zelf ook respectvol te zijn.",
                            "target_lang_translations": {
                                "en": "<w>Demand</w> respect from others by being respectful yourself.",
                                "ru": "<w>Требуй</w> уважения от других, проявляя уважение сам."
                            }
                        }
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e745329d-4314-3927-b077-bedd26884d9d",
                    "sense_definition": "The country where a person was born or which they regard as their home.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "country_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After living abroad for decades, he finally returned to his <w>homeland</w>.",
                            "target_lang_translations": {
                                "ru": "Прожив десятилетия за границей, он наконец вернулся на <w>родину</w>."
                            }
                        },
                        {
                            "text": "The soldiers were willing to die to defend their <w>homeland</w> from invaders.",
                            "target_lang_translations": {
                                "ru": "Солдаты были готовы умереть, защищая свою <w>родину</w> от захватчиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "bantustan"
                    ],
                    "common_phrases": [
                        "defend the homeland",
                        "return to the homeland",
                        "homeland security"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страна, где человек родился или которую считает своим домом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родина",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "отечество",
                                "target_lang_sense_clarification": "высокий стиль"
                            },
                            {
                                "target_lang_word": "отчизна",
                                "target_lang_sense_clarification": "поэтическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e71726d1-4719-3725-9a94-9bd3ea3bcdc6",
                    "sense_definition": "The traditional territory historically inhabited by a specific ethnic group.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "country_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tribe has lived in this mountainous <w>homeland</w> for thousands of years.",
                            "target_lang_translations": {
                                "ru": "Племя жило на этой горной <w>родине</w> тысячи лет."
                            }
                        },
                        {
                            "text": "Many people were displaced from their ancestral <w>homeland</w> during the war.",
                            "target_lang_translations": {
                                "ru": "Многие люди были изгнаны со своей исторической <w>родины</w> во время войны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ancestral homeland",
                        "traditional homeland"
                    ],
                    "target_lang_definitions": {
                        "ru": "Традиционная территория, исторически населенная определенной этнической группой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родина",
                                "target_lang_sense_clarification": "место происхождения"
                            },
                            {
                                "target_lang_word": "историческая родина",
                                "target_lang_sense_clarification": "исконные земли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b87a96d-82f1-3175-995c-7acc1a190841",
                    "sense_definition": "An area set aside for Black South Africans under the policy of apartheid.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "During apartheid, millions were forcibly moved to a designated <w>homeland</w>.",
                            "target_lang_translations": {
                                "ru": "Во время апартеида миллионы были насильно переселены в специально отведенный <w>бантустан</w>."
                            }
                        },
                        {
                            "text": "The government claimed the <w>homeland</w> was independent, but it relied heavily on outside support.",
                            "target_lang_translations": {
                                "ru": "Правительство заявляло, что <w>бантустан</w> независим, но он сильно зависел от внешней поддержки."
                            }
                        }
                    ],
                    "synonyms": [
                        "bantustan"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "South Africa"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical context of apartheid"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория, отведенная для чернокожего населения ЮАР во времена политики апартеида."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бантустан",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                }
            ]
        },

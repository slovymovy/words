    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6ee8c48b-783b-3eaa-bb01-13522c38b654",
                    "sense_definition": "Небольшой, редкий лес или полоса леса, соединяющая лесные массивы.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За деревней начинался небольшой берёзовый <w>пролесок</w>.",
                            "target_lang_translations": {
                                "en": "A small birch <w>copse</w> began beyond the village."
                            }
                        },
                        {
                            "text": "Мы решили отдохнуть, войдя в тенистый <w>пролесок</w>.",
                            "target_lang_translations": {
                                "en": "We decided to rest upon entering the shady <w>small wood</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "перелесок"
                    ],
                    "common_phrases": [
                        "берёзовый пролесок",
                        "редкий пролесок"
                    ],
                    "target_lang_definitions": {
                        "en": "A small, sparse forest or a strip of woodland connecting larger forest areas."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "copse",
                                "target_lang_sense_clarification": "a small group of trees"
                            },
                            {
                                "target_lang_word": "small wood",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "grove",
                                "target_lang_sense_clarification": "a small wood or group of trees"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f4c4e1e-fc1b-399c-b0ac-b6ea979c49bf",
                    "sense_definition": "Форма родительного падежа множественного числа существительного <w>пролеска</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В весеннем лесу расцвело множество голубых <w>пролесок</w>.",
                            "target_lang_translations": {
                                "en": "A multitude of blue <w>scillas</w> blossomed in the spring forest."
                            }
                        },
                        {
                            "text": "На столе стоял маленький букет первых <w>пролесок</w>.",
                            "target_lang_translations": {
                                "en": "A small bouquet of the first <w>squills</w> stood on the table."
                            }
                        }
                    ],
                    "common_phrases": [
                        "букет пролесок",
                        "поляна пролесок"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "родительный падеж множественного числа"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive plural form of the noun \"scilla\" (a blue spring flower)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scillas",
                                "target_lang_sense_clarification": "plural of the flower name"
                            },
                            {
                                "target_lang_word": "squills",
                                "target_lang_sense_clarification": "common name for the flower"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8b4f981-43bf-3efd-86df-6486165b73c8",
                    "sense_definition": "Узкая дорога в лесу или очищенная от деревьев полоса.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тропинка свернула в узкий <w>пролесок</w>, ведущий к реке.",
                            "target_lang_translations": {
                                "en": "The path turned into a narrow <w>forest cut</w> leading to the river."
                            }
                        },
                        {
                            "text": "Через густую чащу был прорублен длинный <w>пролесок</w>.",
                            "target_lang_translations": {
                                "en": "A long <w>clearing</w> had been cut through the dense thicket."
                            }
                        }
                    ],
                    "synonyms": [
                        "просека"
                    ],
                    "common_phrases": [
                        "узкий пролесок",
                        "заросший пролесок"
                    ],
                    "target_lang_definitions": {
                        "en": "A narrow path in a forest or a strip cleared of trees."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forest path",
                                "target_lang_sense_clarification": "narrow way through woods"
                            },
                            {
                                "target_lang_word": "clearing",
                                "target_lang_sense_clarification": "strip of land cleared of trees"
                            },
                            {
                                "target_lang_word": "cut",
                                "target_lang_sense_clarification": "a passage cut through a forest"
                            }
                        ]
                    }
                }
            ]
        }
    ]

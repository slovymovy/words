    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0cb4fa96-e9b5-3b91-aaa6-e00f296e7785",
                    "sense_definition": "Verschillende delen, mensen of groepen stevig met elkaar verbinden tot een eenheid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "uniting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur wil de twee afdelingen tot één efficiënt team <w>samensmeden</w>.",
                            "target_lang_translations": {
                                "en": "The director wants to <w>forge</w> the two departments into one efficient team."
                            }
                        },
                        {
                            "text": "De rebellen hebben in het geheim een plan <w>samengesmeed</w> om de regering omver te werpen.",
                            "target_lang_translations": {
                                "en": "The rebels secretly <w>forged</w> a plan to overthrow the government."
                            }
                        }
                    ],
                    "synonyms": [
                        "verenigen",
                        "samenvoegen",
                        "verbinden"
                    ],
                    "antonyms": [
                        "scheiden",
                        "splitsen"
                    ],
                    "common_phrases": [
                        "een plan samensmeden",
                        "tot een geheel samensmeden",
                        "een bondgenootschap samensmeden"
                    ],
                    "target_lang_definitions": {
                        "en": "To firmly connect different parts, people, or groups into a unity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forge",
                                "target_lang_sense_clarification": "alliances, teams, or plans"
                            },
                            {
                                "target_lang_word": "weld together",
                                "target_lang_sense_clarification": "people or groups"
                            },
                            {
                                "target_lang_word": "unite",
                                "target_lang_sense_clarification": "general"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31b3f119-aff4-348a-9f58-67ab9f2a14cf",
                    "sense_definition": "Twee of meer stukken metaal door verhitting en hameren tot één geheel maken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "uniting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De smid moest het gebroken zwaard opnieuw <w>samensmeden</w> in het hete vuur.",
                            "target_lang_translations": {
                                "en": "The blacksmith had to <w>forge</w> the broken sword <w>together</w> again in the hot fire."
                            }
                        },
                        {
                            "text": "Je kunt goud en zilver niet zomaar <w>samensmeden</w> zonder de juiste techniek.",
                            "target_lang_translations": {
                                "en": "You cannot simply <w>forge</w> gold and silver <w>together</w> without the right technique."
                            }
                        }
                    ],
                    "synonyms": [
                        "aaneensmeden"
                    ],
                    "common_phrases": [
                        "ijzer samensmeden",
                        "metaal samensmeden"
                    ],
                    "target_lang_definitions": {
                        "en": "To combine two or more pieces of metal into a whole by heating and hammering."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forge together",
                                "target_lang_sense_clarification": "metal"
                            },
                            {
                                "target_lang_word": "weld",
                                "target_lang_sense_clarification": "metalworking"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "frequency": "High",
                    "semantic_group_id": "requirement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The contract <w>stipulates</w> that all payments must be made by the end of the month."
                        },
                        {
                            "text": "State laws <w>stipulate</w> the maximum number of hours a driver can work without a break."
                        }
                    ],
                    "synonyms": [
                        "specify",
                        "require",
                        "demand"
                    ],
                    "common_phrases": [

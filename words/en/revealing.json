            "senses": [
                {
                    "sense_id": "b8afb106-09bb-39ff-b573-65c5fa578d69",
                    "sense_definition": "Present participle and gerund of <w>reveal</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is <w>revealing</w> the new design to the public tomorrow.",
                            "target_lang_translations": {
                                "ru": "Завтра компания <w>раскроет</w> общественности новый дизайн."
                            }
                        },
                        {
                            "text": "By <w>revealing</w> the truth, he lifted a heavy burden from his shoulders.",
                            "target_lang_translations": {
                                "ru": "<w>Раскрыв</w> правду, он снял тяжкий груз со своих плеч."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола reveal."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрывающий",
                                "target_lang_sense_clarification": "причастие"
                            },
                            {
                                "target_lang_word": "раскрывая",
                                "target_lang_sense_clarification": "деепричастие"
                            }

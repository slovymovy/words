                            },
                            {
                                "target_lang_word": "regard",
                                "target_lang_sense_clarification": "in phrases indicating reference"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отношение",
                                "target_lang_sense_clarification": "связь, касательство"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "взаимосвязь"
                            }

                    "sense_id": "d26b916f-71bf-3ad4-8dd0-93a613a35811",
                    "sense_definition": "Een korte, nauwsluitende onderbroek zonder pijpen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "underwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij kocht gisteren een nieuwe katoenen <w>slip</w>.",
                            "target_lang_translations": {
                                "en": "She bought new cotton <w>briefs</w> yesterday.",
                                "ru": "Вчера она купила новые хлопковые <w>трусики</w>."
                            }

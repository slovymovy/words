    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fed4d60a-921b-310a-95ab-887354fa33bd",
                    "sense_definition": "To throw something forcefully in a specific direction.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "throwing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fisherman <w>cast</w> his line far out into the lake.",
                            "target_lang_translations": {
                                "ru": "Рыбак <w>забросил</w> леску далеко в озеро."
                            }
                        },
                        {
                            "text": "She <w>cast</w> the stone into the river and watched the ripples.",
                            "target_lang_translations": {
                                "ru": "Она <w>бросила</w> камень в реку и смотрела на рябь."
                            }
                        }
                    ],
                    "synonyms": [
                        "throw",
                        "toss",
                        "fling",
                        "hurl",
                        "pitch"
                    ],
                    "antonyms": [
                        "catch",
                        "hold"
                    ],
                    "common_phrases": [
                        "cast aside",
                        "cast away",
                        "cast off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бросать что-либо с силой в определенном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "закидывать",
                                "target_lang_sense_clarification": "о удочке или неводе"
                            },
                            {
                                "target_lang_word": "кидать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "998de7b3-ed3e-3b69-95a7-483ff85ee4e5",
                    "sense_definition": "Zacząć odczuwać sympatię do kogoś lub upodobanie do czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że moi rodzice szybko cię <polubią>.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, что мои родители быстро тебя <полюбят>."
                            }
                        },
                        {
                            "text": "Po przeczytaniu pierwszego rozdziału od razu <polubiłem> tę książkę.",
                            "target_lang_translations": {
                                "ru": "Прочитав первую главу, я сразу <полюбил> эту книгу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zagustować",
                        "rozsmakować się"
                    ],
                    "antonyms": [
                        "znielubić"
                    ],
                    "common_phrases": [
                        "dać się polubić",
                        "szczerze polubić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать испытывать симпатию к кому-либо или вкус к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полюбить",
                                "target_lang_sense_clarification": "начать испытывать привязанность или глубокую симпатию"
                            },
                            {
                                "target_lang_word": "понравиться",
                                "target_lang_sense_clarification": "вызвать симпатию (конструкция: кто-то нравится кому-то)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43617fca-0a8e-3ec4-89ec-6a4677f522c5",
                    "sense_definition": "Wyrazić aprobatę dla treści w internecie, zaznaczając ją odpowiednią opcją (np. klikając przycisk).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli podoba ci się ten film, nie zapomnij go <polubić>.",
                            "target_lang_translations": {
                                "ru": "Если тебе нравится этот фильм, не забудь его <лайкнуть>."
                            }
                        },
                        {
                            "text": "Wiele osób <polubiło> jej zdjęcie z wakacji na portalu społecznościowym.",
                            "target_lang_translations": {
                                "ru": "Многие люди <лайкнули> её фотографию из отпуска в социальной сети."
                            }
                        }
                    ],
                    "synonyms": [
                        "lajknąć",
                        "polajkować",
                        "oserduszkować",
                        "zalajkować",
                        "zaserduszkować"
                    ],
                    "common_phrases": [
                        "polubić post",
                        "polubić stronę",
                        "polubić zdjęcie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выразить одобрение контента в интернете, нажав соответствующую кнопку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лайкнуть",
                                "target_lang_sense_clarification": "интернет-сленг"
                            },
                            {
                                "target_lang_word": "поставить лайк",
                                "target_lang_sense_clarification": "устойчивое выражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "248fff97-3cda-3b07-9a6e-1617b46e2690",
                    "sense_definition": "Zacząć darzyć się wzajemną sympatią.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo trudnego początku, w końcu się <polubili> i zostali przyjaciółmi.",
                            "target_lang_translations": {
                                "ru": "Несмотря на трудное начало, в конце концов они <понравились друг другу> и стали друзьями."
                            }
                        },
                        {
                            "text": "Sąsiedzi szybko się <polubili> i często odwiedzają się nawzajem.",
                            "target_lang_translations": {
                                "ru": "Соседи быстро <подружились> и часто ходят друг к другу в гости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "polubić się z kimś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать испытывать взаимную симпатию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понравиться друг другу",
                                "target_lang_sense_clarification": "о взаимной симпатии"
                            },
                            {
                                "target_lang_word": "подружиться",
                                "target_lang_sense_clarification": "стать друзьями"
                            }
                        ]
                    }
                }
            ]
        }
    ]

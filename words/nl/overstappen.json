    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7feb7576-ec78-33a7-a4fb-7dab33371856",
                    "sense_definition": "Met een ander vervoermiddel de reis vervolgen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je naar het centrum wilt, moet je hier <w>overstappen</w> op de tram.",
                            "target_lang_translations": {
                                "en": "If you want to go to the center, you have to <w>transfer</w> to the tram here.",
                                "ru": "Если вы хотите попасть в центр, вам нужно здесь <w>пересесть</w> на трамвай."
                            }
                        },
                        {
                            "text": "Reizigers uit het zuiden kunnen in Utrecht makkelijk <w>overstappen</w>.",
                            "target_lang_translations": {
                                "en": "Travelers from the south can easily <w>transfer</w> in Utrecht.",
                                "ru": "Путешественники с юга могут легко <w>сделать пересадку</w> в Утрехте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "overstappen op",
                        "overstappen in",
                        "moeten overstappen"
                    ],
                    "target_lang_definitions": {
                        "en": "To continue a journey using a different vehicle or mode of transport.",
                        "ru": "Продолжить путешествие на другом транспортном средстве."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "transfer",
                                "target_lang_sense_clarification": "transport"
                            },
                            {
                                "target_lang_word": "change",
                                "target_lang_sense_clarification": "trains/buses"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пересаживаться",
                                "target_lang_sense_clarification": "транспорт"
                            },
                            {
                                "target_lang_word": "делать пересадку",
                                "target_lang_sense_clarification": "транспорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e6914de-7bd3-38b7-80ec-78f231d9f9e0",
                    "sense_definition": "Veranderen van keuze, zoals van studie, baan, leverancier of politieke partij.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel consumenten willen <w>overstappen</w> naar een goedkopere energieleverancier.",
                            "target_lang_translations": {
                                "en": "Many consumers want to <w>switch</w> to a cheaper energy supplier.",
                                "ru": "Многие потребители хотят <w>перейти</w> к более дешевому поставщику энергии."
                            }
                        },
                        {
                            "text": "Na twee jaar rechten te hebben gestudeerd, besloot hij <w>over te stappen</w> naar economie.",
                            "target_lang_translations": {
                                "en": "After studying law for two years, he decided to <w>switch</w> to economics.",
                                "ru": "Проучившись два года на юридическом, он решил <w>перейти</w> на экономику."
                            }
                        }
                    ],
                    "synonyms": [
                        "wisselen",
                        "switchen"
                    ],
                    "common_phrases": [
                        "overstappen naar",
                        "overstappen van"
                    ],
                    "target_lang_definitions": {
                        "en": "To change a choice, such as a study program, job, service provider, or political party.",
                        "ru": "Изменить выбор, например, учебы, работы, поставщика услуг или политической партии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "switch",
                                "target_lang_sense_clarification": "provider, job, or study"
                            },
                            {
                                "target_lang_word": "change",
                                "target_lang_sense_clarification": "general"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "переходить",
                                "target_lang_sense_clarification": "к другому провайдеру, на другую работу"
                            },
                            {
                                "target_lang_word": "менять",
                                "target_lang_sense_clarification": "поставщика, работу, учебу"
                            }
                        ]
                    }
                }
            ]
        },

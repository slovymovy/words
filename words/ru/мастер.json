    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8d0885e-5ab5-3cfd-bb8e-c77837dc0d78",
                    "sense_definition": "Квалифицированный работник, профессионально занимающийся каким-либо ремеслом или ремонтом.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "skill_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы вызвали <w>мастера</w>, чтобы он починил сломанную стиральную машину.",
                            "target_lang_translations": {
                                "en": "We called a <w>repairman</w> to fix the broken washing machine."
                            }
                        },
                        {
                            "text": "В этой маленькой мастерской работает опытный часовой <w>мастер</w>.",
                            "target_lang_translations": {
                                "en": "An experienced <w>watchmaker</w> works in this small workshop."
                            }
                        }
                    ],
                    "common_phrases": [
                        "часовой мастер",
                        "вызвать мастера",
                        "мастер по ремонту"
                    ],
                    "target_lang_definitions": {
                        "en": "A qualified worker professionally engaged in a specific craft or repair work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "repairman",
                                "target_lang_sense_clarification": "person who repairs machines or appliances"
                            },
                            {
                                "target_lang_word": "craftsman",
                                "target_lang_sense_clarification": "skilled artisan"
                            },
                            {
                                "target_lang_word": "master",
                                "target_lang_sense_clarification": "skilled tradesman"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4c9fedc-fcc2-39ff-b778-602ae0d3a431",
                    "sense_definition": "Специалист, достигший высокого искусства, совершенства и признания в своём деле.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "skill_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот художник — настоящий <w>мастер</w> своего дела, его картины известны во всём мире.",
                            "target_lang_translations": {
                                "en": "This artist is a true <w>master</w> of his craft; his paintings are known all over the world."
                            }
                        },
                        {
                            "text": "Только великий <w>мастер</w> мог создать такую сложную и красивую скульптуру.",
                            "target_lang_translations": {
                                "en": "Only a great <w>master</w> could create such a complex and beautiful sculpture."
                            }
                        }
                    ],
                    "synonyms": [
                        "профессионал",
                        "специалист"
                    ],
                    "common_phrases": [
                        "мастер своего дела",
                        "дело мастера боится",
                        "великий мастер"
                    ],
                    "target_lang_definitions": {
                        "en": "A specialist who has achieved a high level of skill, perfection, and recognition in their field."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "master",
                                "target_lang_sense_clarification": "expert or virtuoso"
                            },
                            {
                                "target_lang_word": "expert",
                                "target_lang_sense_clarification": "highly skilled person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f52a510e-e7d8-3ccd-9028-9268b131f9a1",
                    "sense_definition": "Руководитель отдельного производственного участка или цеха на заводе или фабрике.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "role_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое утро сменный <w>мастер</w> распределяет задания между рабочими бригады.",
                            "target_lang_translations": {
                                "en": "Every morning, the shift <w>foreman</w> distributes tasks among the brigade workers."
                            }
                        },
                        {
                            "text": "Строгий <w>мастер</w> следил за соблюдением техники безопасности в цехе.",
                            "target_lang_translations": {
                                "en": "The strict <w>foreman</w> monitored compliance with safety regulations in the shop."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сменный мастер",
                        "мастер цеха",
                        "старший мастер"
                    ],
                    "target_lang_definitions": {
                        "en": "A supervisor or leader of a specific production section or workshop in a factory."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "foreman",
                                "target_lang_sense_clarification": "factory supervisor"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bdc9db2-c72c-32d8-92d3-b0c7ad7cfb94",
                    "sense_definition": "Официальное спортивное звание, присваиваемое за выдающиеся достижения, а также спортсмен, имеющий это звание.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "title_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Через пять лет упорных тренировок он выполнил норматив и стал <w>мастером</w> спорта.",
                            "target_lang_translations": {
                                "en": "After five years of hard training, he met the standard and became a <w>Master</w> of Sports."
                            }
                        },
                        {
                            "text": "На соревнования приехал заслуженный <w>мастер</w> спорта по фигурному катанию.",
                            "target_lang_translations": {
                                "en": "An Honored <w>Master</w> of Sports in figure skating arrived at the competition."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мастер спорта",
                        "заслуженный мастер спорта",
                        "норматив мастера"
                    ],
                    "target_lang_definitions": {
                        "en": "An official sports title awarded for outstanding achievements, or an athlete holding this title."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Master",
                                "target_lang_sense_clarification": "sports title (Master of Sports)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68c01343-710a-3121-a847-4147e6dff210",
                    "sense_definition": "Ловкий, умелый и искусный в каком-либо занятии человек (часто с оттенком восхищения или иронии).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "skill_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш сосед — <w>мастер</w> на все руки, он умеет чинить всё: от утюга до автомобиля.",
                            "target_lang_translations": {
                                "en": "Our neighbor is a <w>jack</w> of all trades; he knows how to fix everything: from an iron to a car."
                            }
                        },
                        {
                            "text": "Он большой <w>мастер</w> рассказывать смешные истории и развлекать гостей.",
                            "target_lang_translations": {
                                "en": "He is a great <w>master</w> at telling funny stories and entertaining guests."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мастер на все руки",
                        "большой мастер"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who is adroit, skillful, and clever in some activity (often used with admiration or irony)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "master",
                                "target_lang_sense_clarification": "skilled person"
                            },
                            {
                                "target_lang_word": "expert",
                                "target_lang_sense_clarification": "proficient person"
                            },
                            {
                                "target_lang_word": "whiz",
                                "target_lang_sense_clarification": "colloquial, skilled person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd21c65a-312d-3bb0-b46d-34b7eb2f6464",
                    "sense_definition": "Компьютерная программа, которая помогает пользователю выполнить сложную задачу пошагово.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "software_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Мастер</w> установки поможет вам быстро и правильно настроить новую программу.",
                            "target_lang_translations": {
                                "en": "The installation <w>wizard</w> will help you quickly and correctly set up the new program."
                            }
                        },
                        {
                            "text": "Если вы не знаете, как подключить принтер, запустите <w>мастер</w> настройки оборудования.",
                            "target_lang_translations": {
                                "en": "If you don't know how to connect the printer, run the hardware setup <w>wizard</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "помо́щник"
                    ],
                    "common_phrases": [
                        "мастер установки",
                        "мастер настройки",
                        "мастер подключения"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A computer program that guides the user through a complex task step-by-step."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wizard",
                                "target_lang_sense_clarification": "software interface"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "08bbd537-b900-3c5a-a63b-35ff7e83ea2d",
                    "sense_definition": "Мужское имя.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Вчера я познакомился с интересным человеком, которого зовут <w>Мастер</w>.",
                            "target_lang_translations": {
                                "en": "Yesterday I met an interesting man called the <w>Master</w>."
                            }
                        },
                        {
                            "text": "<w>Мастер</w> не любил, когда его редкое имя произносили неправильно.",
                            "target_lang_translations": {
                                "en": "The <w>Master</w> did not like it when his rare name was pronounced incorrectly."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A specific name or title used to address or refer to a person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Master",
                                "target_lang_sense_clarification": "name or title"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
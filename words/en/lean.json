                    "semantic_group_id": "opinion_tendency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am starting to <w>lean</w> towards the second option we discussed yesterday.",
                            "target_lang_translations": {
                                "ru": "Я начинаю <w>склоняться</w> к второму варианту, который мы обсуждали вчера."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f3ef41b3-d87b-3b2d-8009-ae92fa128d11",
                    "sense_definition": "Fakt bycia obecnym i aktywnym w jakimś wydarzeniu, grupie lub działaniu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "participation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy uczniowie muszą wziąć <w>udział</w> w szkolnej akademii.",
                            "target_lang_translations": {
                                "ru": "Все ученики должны принять <w>участие</w> в школьном мероприятии."
                            }
                        },
                        {
                            "text": "Dziękujemy państwu za liczny <w>udział</w> w naszym spotkaniu.",
                            "target_lang_translations": {
                                "ru": "Благодарим вас за активное <w>участие</w> в нашей встрече."
                            }
                        }
                    ],
                    "synonyms": [
                        "uczestnictwo",
                        "partycypacja",
                        "współudział",
                        "współuczestnictwo",
                        "zaangażowanie"
                    ],
                    "common_phrases": [
                        "brać udział",
                        "wziąć udział",
                        "aktywny udział",
                        "zgłosić udział"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт присутствия и активной деятельности в каком-либо событии, группе или действии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участие",
                                "target_lang_sense_clarification": "в событии или действии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cced0ea1-0480-3221-8e39-56d2381e20c0",
                    "sense_definition": "Osobisty wkład pracy, wpływ lub rola w realizacji jakiegoś przedsięwzięcia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "contribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat miał spory <w>udział</w> w przygotowaniu tej ważnej wystawy.",
                            "target_lang_translations": {
                                "ru": "Мой брат принял немалое <w>участие</w> в подготовке этой важной выставки."
                            }
                        },
                        {
                            "text": "To wielki sukces, w którym każdy pracownik ma swój <w>udział</w>.",
                            "target_lang_translations": {
                                "ru": "Это большой успех, в который каждый сотрудник внёс свой <w>вклад</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wkład",
                        "współpraca"
                    ],
                    "common_phrases": [
                        "mieć udział",
                        "znaczący udział",
                        "swój udział",
                        "pośredni udział"
                    ],
                    "target_lang_definitions": {
                        "ru": "Личный вклад, влияние или роль в реализации какого-либо дела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участие",
                                "target_lang_sense_clarification": "роль в деле"
                            },
                            {
                                "target_lang_word": "вклад",
                                "target_lang_sense_clarification": "результат усилий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7ce2c30-e75e-33c0-b88b-b65505d9e37f",
                    "sense_definition": "Część kapitału w spółce lub przedsiębiorstwie, dająca właścicielowi określone prawa i zyski.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zagraniczny inwestor nabył większościowe <w>udziały</w> w nowej firmie technologicznej.",
                            "target_lang_translations": {
                                "ru": "Иностранный инвестор приобрёл контрольную <w>долю</w> в новой технологической компании."
                            }
                        },
                        {
                            "text": "Sprzedaż <w>udziałów</w> przyniosła mu w tym roku ogromne zyski.",
                            "target_lang_translations": {
                                "ru": "Продажа <w>долей</w> принесла ему в этом году огромную прибыль."
                            }
                        }
                    ],
                    "synonyms": [
                        "akcja",
                        "część"
                    ],
                    "common_phrases": [
                        "udziały w spółce",
                        "sprzedaż udziałów",
                        "kupno udziałów",
                        "pakiet udziałów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть капитала в компании или предприятии, дающая владельцу определенные права и прибыль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доля",
                                "target_lang_sense_clarification": "часть капитала"
                            },
                            {
                                "target_lang_word": "пай",
                                "target_lang_sense_clarification": "взнос в кооператив"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "425bd416-3e04-3a46-b624-9a16cb052a94",
                    "sense_definition": "To, co kogoś spotyka w życiu (los) lub co otrzymuje w wyniku podziału.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fate_portion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej <w>udziałem</w> stały się niestety same zmartwienia i kłopoty.",
                            "target_lang_translations": {
                                "ru": "К сожалению, на её <w>долю</w> выпали одни лишь заботы и неприятности."
                            }
                        },
                        {
                            "text": "Główna nagroda niespodziewanie przypadła w <w>udziale</w> młodemu pisarzowi.",
                            "target_lang_translations": {
                                "ru": "Главная награда неожиданно выпала на <w>долю</w> молодого писателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "część",
                        "porcja"
                    ],
                    "common_phrases": [
                        "przypaść w udziale",
                        "stać się udziałem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что выпадает кому-то в жизни (судьба), или то, что получено в результате раздела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доля",
                                "target_lang_sense_clarification": "судьба или часть при разделе"
                            },
                            {
                                "target_lang_word": "удел",
                                "target_lang_sense_clarification": "судьба, участь (книжное)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

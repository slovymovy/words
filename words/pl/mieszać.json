    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5b0b1d76-ae30-3177-9b4a-9eaa88c42d56",
                    "sense_definition": "Łączyć ze sobą różne składniki lub substancje, zazwyczaj wykonując ruchy okrężne łyżką lub innym narzędziem, aby stworzyć jednolitą całość.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "mixing_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz powoli <w>mieszać</w> zupę, żeby się nie przypaliła.",
                            "target_lang_translations": {
                                "ru": "Ты должен медленно <w>мешать</w> суп, чтобы он не пригорел."
                            }
                        },
                        {
                            "text": "Kucharz zaczął dokładnie <w>mieszać</w> wszystkie składniki na ciasto.",
                            "target_lang_translations": {
                                "ru": "Повар начал тщательно <w>перемешивать</w> все ингредиенты для теста."
                            }
                        }
                    ],
                    "synonyms": [
                        "bełtać"
                    ],
                    "antonyms": [
                        "rozdzielać",
                        "oddzielać"
                    ],
                    "common_phrases": [
                        "mieszać herbatę",
                        "mieszać łyżką",
                        "dokładnie mieszać",
                        "mieszać składniki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединять ингредиенты или вещества, обычно круговыми движениями ложки или другого инструмента, для получения однородной массы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мешать",
                                "target_lang_sense_clarification": "совершать круговые движения (ложкой)"
                            },
                            {
                                "target_lang_word": "перемешивать",
                                "target_lang_sense_clarification": "тщательно соединять компоненты"
                            },
                            {
                                "target_lang_word": "смешивать",
                                "target_lang_sense_clarification": "соединять разные вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8edf35b0-76c3-3ecd-b0dc-046a1054bc16",
                    "sense_definition": "Mylić ze sobą różne osoby, rzeczy, fakty lub pojęcia; brać omyłkowo jedno za drugie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "confusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze zaczynam <w>mieszać</w> te dwa podobne do siebie słowa.",
                            "target_lang_translations": {
                                "ru": "Я всегда начинаю <w>путать</w> эти два похожих слова."
                            }
                        },
                        {
                            "text": "Proszę nie <w>mieszać</w> fikcji literackiej z prawdą historyczną.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не надо <w>смешивать</w> литературный вымысел с исторической правдой."
                            }
                        }
                    ],
                    "synonyms": [
                        "mylić"
                    ],
                    "common_phrases": [
                        "mieszać pojęcia",
                        "mieszać fakty",
                        "mieszać imiona"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путать людей, вещи, факты или понятия; ошибочно принимать одно за другое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путать",
                                "target_lang_sense_clarification": "ошибаться, принимая одно за другое"
                            },
                            {
                                "target_lang_word": "смешивать",
                                "target_lang_sense_clarification": "соединять понятия, которые должны быть разделены"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0dab70b-72df-3889-ac55-dc561805441e",
                    "sense_definition": "Angażować się w cudze sprawy, spory lub sytuacje, które nas nie dotyczą (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "interference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obiecałem sobie nie <w>mieszać</w> się do ich małżeńskich kłótni.",
                            "target_lang_translations": {
                                "ru": "Я пообещал себе не <w>вмешиваться</w> в их супружеские ссоры."
                            }
                        },
                        {
                            "text": "Lepiej nie <w>mieszać</w> się w sprawy, o których nie masz pojęcia.",
                            "target_lang_translations": {
                                "ru": "Лучше не <w>вмешиваться</w> в дела, о которых ты не имеешь понятия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nie mieszać się",
                        "mieszać się w sprawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участвовать в чужих делах, спорах или ситуациях, которые нас не касаются."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вмешиваться",
                                "target_lang_sense_clarification": "принимать участие в чужих делах"
                            },
                            {
                                "target_lang_word": "лезть",
                                "target_lang_sense_clarification": "разг. назойливо вмешиваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b39c896-aaca-3da4-851e-1535abcee819",
                    "sense_definition": "Wciągać kogoś w kłopotliwą sytuację, intrygę lub konflikt wbrew jego woli.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "implication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinieneś <w>mieszać</w> swoich przyjaciół w te brudne interesy.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <w>впутывать</w> своих друзей в эти грязные дела."
                            }
                        },
                        {
                            "text": "Próbował <w>mieszać</w> w to policję, ale nikt mu nie uwierzył.",
                            "target_lang_translations": {
                                "ru": "Он пытался <w>вмешать</w> в это полицию, но никто ему не поверил."
                            }
                        }
                    ],
                    "synonyms": [
                        "wplątywać",
                        "wikłać",
                        "wkopywać"
                    ],
                    "common_phrases": [
                        "mieszać kogoś w aferę",
                        "mieszać w to policję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вовлекать кого-то в неприятную ситуацию, интригу или конфликт против его воли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "впутывать",
                                "target_lang_sense_clarification": "вовлекать в неприятности"
                            },
                            {
                                "target_lang_word": "вмешивать",
                                "target_lang_sense_clarification": "делать участником чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "850a4030-9354-3c77-9d9d-ad31cb9b6a97",
                    "sense_definition": "Łączyć się w jedną całość, przenikać się nawzajem, tworząc nową jakość (często o dźwiękach, zapachach, tłumie; używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mixing_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zapach świeżych kwiatów zaczął <w>mieszać</w> się z dymem papierosowym.",
                            "target_lang_translations": {
                                "ru": "Запах свежих цветов начал <w>смешиваться</w> с сигаретным дымом."
                            }
                        },
                        {
                            "text": "Jej radosny śmiech zaczął <w>mieszać</w> się z głośną muzyką.",
                            "target_lang_translations": {
                                "ru": "Её радостный смех начал <w>сливаться</w> с громкой музыкой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieszać się z tłumem",
                        "mieszać się z powietrzem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединяться в единое целое, проникать друг в друга (часто о звуках, запахах, толпе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смешиваться",
                                "target_lang_sense_clarification": "соединяться, образуя смесь"
                            },
                            {
                                "target_lang_word": "сливаться",
                                "target_lang_sense_clarification": "объединяться в одно целое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59e4e2f9-8077-339f-a04c-da102e5e80c3",
                    "sense_definition": "Wprawiać kogoś w zakłopotanie, powodować utratę pewności siebie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego intensywne i badawcze spojrzenie zaczęło mnie <w>mieszać</w>.",
                            "target_lang_translations": {
                                "ru": "Его пристальный и изучающий взгляд начал меня <w>смущать</w>."
                            }
                        },
                        {
                            "text": "To podchwytliwe pytanie miało na celu <w>mieszać</w> świadka w sądzie.",
                            "target_lang_translations": {
                                "ru": "Этот каверзный вопрос имел целью <w>сбить с толку</w> свидетеля в суде."
                            }
                        }
                    ],
                    "synonyms": [
                        "peszyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить кого-то в замешательство, лишать уверенности в себе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смущать",
                                "target_lang_sense_clarification": "приводить в замешательство"
                            },
                            {
                                "target_lang_word": "сбивать с толку",
                                "target_lang_sense_clarification": "запутывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "654d607c-30da-39d0-a6b2-53707ff3e552",
                    "sense_definition": "Tworzyć stado (kierdel) z owiec należących do różnych właścicieli w celu wspólnego wypasu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "farming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górale zaczęli <w>mieszać</w> owce na hali, aby rozpocząć sezon wypasu.",
                            "target_lang_translations": {
                                "ru": "Горцы начали <w>смешивать</w> овец на пастбище, чтобы начать сезон выпаса."
                            }
                        },
                        {
                            "text": "Tradycyjnie na wiosnę trzeba <w>mieszać</w> stada przed wyjściem w góry.",
                            "target_lang_translations": {
                                "ru": "Традиционно весной нужно <w>смешивать</w> стада перед выходом в горы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieszać owce"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Śląsk Cieszyński"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "gwara góralska"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединять овец разных владельцев в одно стадо для совместного выпаса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смешивать",
                                "target_lang_sense_clarification": "объединять животных в стадо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
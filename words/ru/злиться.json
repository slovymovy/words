    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "141672df-90ab-3ea6-8be1-1702a9831ed8",
                    "sense_definition": "Испытывать чувство злости, раздражения или гнева по отношению к кому-либо или чему-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не нужно <злиться> на друга из-за такой мелочи.",
                            "target_lang_translations": {
                                "en": "There is no need <to get angry> at a friend over such a trifle."
                            }
                        },
                        {
                            "text": "Я начинаю <злиться>, когда автобус долго не приходит.",
                            "target_lang_translations": {
                                "en": "I start <to get angry> when the bus takes a long time to arrive."
                            }
                        }
                    ],
                    "synonyms": [
                        "серди́ться"
                    ],
                    "common_phrases": [
                        "злиться на себя",
                        "злиться на весь мир",
                        "злиться из-за пустяков"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel anger, irritation, or rage towards someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to get angry",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "to be angry",
                                "target_lang_sense_clarification": "state"
                            },
                            {
                                "target_lang_word": "to be mad",
                                "target_lang_sense_clarification": "informal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0121c9a-257b-3404-b5bc-369dbdcd2d3a",
                    "sense_definition": "Бушевать, проявлять сильную активность или разрушительную силу (о явлениях природы: ветре, вьюге, холоде).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За окном всю ночь <злилась> зимняя вьюга.",
                            "target_lang_translations": {
                                "en": "A winter blizzard <raged> outside the window all night."
                            }
                        },
                        {
                            "text": "Холодный ветер <злился> и срывал последние листья с деревьев.",
                            "target_lang_translations": {
                                "en": "The cold wind <raged> and tore the last leaves from the trees."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ветер злится",
                        "вьюга злится",
                        "мороз злится"
                    ],
                    "target_lang_definitions": {
                        "en": "To rage or show violent force (referring to natural phenomena like wind, blizzards, or cold)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to rage",
                                "target_lang_sense_clarification": "weather phenomena"
                            }
                        ]
                    }
                }
            ]
        }
    ]

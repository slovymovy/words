{
    "word_family": [
        "enigheid",
        "enigszins",
        "verenigen",
        "enigermate"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "675b95b1-ae89-3cc2-8660-228c07907d55",
                    "sense_definition": "Waar er maar één van is; uniek of alleenstaand.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "unique",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat was zijn <w>enige</w> kans om de wedstrijd te winnen.",
                            "target_lang_translations": {
                                "en": "That was his <w>only</w> chance to win the match.",
                                "ru": "Это был его <w>единственный</w> шанс выиграть матч."
                            }
                        },
                        {
                            "text": "Omdat ze geen broers of zussen heeft, is ze een <w>enig</w> kind.",
                            "target_lang_translations": {
                                "en": "Because she has no brothers or sisters, she is an <w>only</w> child.",
                                "ru": "Так как у нее нет братьев и сестер, она <w>единственный</w> ребенок."
                            }
                        }
                    ],
                    "synonyms": [
                        "enigst"
                    ],
                    "common_phrases": [
                        "enig kind",
                        "de enige",
                        "enige optie"
                    ],
                    "target_lang_definitions": {
                        "en": "Being the only one of its kind; unique or standing alone.",
                        "ru": "Тот, который существует в единственном экземпляре; уникальный или единственный в своем роде."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "only",
                                "target_lang_sense_clarification": "being the single one in a category"
                            },
                            {
                                "target_lang_word": "unique",
                                "target_lang_sense_clarification": "being one of a kind"
                            },
                            {
                                "target_lang_word": "sole",
                                "target_lang_sense_clarification": "single; not shared with others"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "единственный",
                                "target_lang_sense_clarification": "существующий только один"
                            },
                            {
                                "target_lang_word": "уникальный",
                                "target_lang_sense_clarification": "неповторимый, исключительный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe9290e9-fb65-3ce8-a485-0821c9b150e7",
                    "sense_definition": "Heel leuk, mooi of schattig om te zien.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "nice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat een <w>enig</w> hondje loopt daar in het park!",
                            "target_lang_translations": {
                                "en": "What a <w>cute</w> little dog is walking there in the park!",
                                "ru": "Какая <w>милая</w> собачка бегает там в парке!"
                            }
                        },
                        {
                            "text": "Ze heeft een <w>enige</w> jurk gekocht voor het feest.",
                            "target_lang_translations": {
                                "en": "She bought a <w>lovely</w> dress for the party.",
                                "ru": "Она купила <w>прелестное</w> платье для вечеринки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wat enig",
                        "enig idee"
                    ],
                    "target_lang_definitions": {
                        "en": "Very nice, beautiful, or cute to look at.",
                        "ru": "Очень милый, красивый или приятный для глаз."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cute",
                                "target_lang_sense_clarification": "attractive in a dainty or youthful way"
                            },
                            {
                                "target_lang_word": "lovely",
                                "target_lang_sense_clarification": "very pleasant or attractive"
                            },
                            {
                                "target_lang_word": "charming",
                                "target_lang_sense_clarification": "delightful and pleasing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "милый",
                                "target_lang_sense_clarification": "приятный и привлекательный"
                            },
                            {
                                "target_lang_word": "прелестный",
                                "target_lang_sense_clarification": "очень красивый, восхитительный"
                            },
                            {
                                "target_lang_word": "очаровательный",
                                "target_lang_sense_clarification": "производящий сильное приятное впечатление"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "5c432326-6757-3490-a425-2913d4755aea",
                    "sense_definition": "Een onbepaald aantal of een zekere hoeveelheid; niet veel, maar ook niet niets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft <w>enige</w> ervaring opgedaan in zijn vorige baan.",
                            "target_lang_translations": {
                                "en": "He gained <w>some</w> experience in his previous job.",
                                "ru": "Он приобрел <w>некоторый</w> опыт на своей предыдущей работе."
                            }
                        },
                        {
                            "text": "<w>Enige</w> voorzichtigheid wordt wel op prijs gesteld in deze situatie.",
                            "target_lang_translations": {
                                "en": "<w>Some</w> caution is appreciated in this situation.",
                                "ru": "В этой ситуации приветствуется <w>некоторая</w> осторожность."
                            }
                        }
                    ],
                    "synonyms": [
                        "een aantal",
                        "een paar",
                        "wat"
                    ],
                    "common_phrases": [
                        "enige tijd",
                        "enige mate",
                        "enige jaren"
                    ],
                    "target_lang_definitions": {
                        "en": "An indefinite number or a certain amount; not a lot, but not nothing either.",
                        "ru": "Неопределенное число или некоторое количество; не много, но и не мало."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "some",
                                "target_lang_sense_clarification": "an indefinite quantity or degree"
                            },
                            {
                                "target_lang_word": "a certain",
                                "target_lang_sense_clarification": "specific but unspecified amount"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "некоторый",
                                "target_lang_sense_clarification": "определенное, но не названное количество или качество"
                            },
                            {
                                "target_lang_word": "несколько",
                                "target_lang_sense_clarification": "небольшое, неопределенное количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9144c953-0b5b-3d29-aa01-271bdfd1361c",
                    "sense_definition": "Welk(e) dan ook; het maakt niet uit welk(e).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "indefinite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je <w>enig</w> idee hoe laat de trein vertrekt?",
                            "target_lang_translations": {
                                "en": "Do you have <w>any</w> idea what time the train leaves?",
                                "ru": "У тебя есть <w>какое-либо</w> представление, во сколько отправляется поезд?"
                            }
                        },
                        {
                            "text": "Zonder <w>enige</w> twijfel nam hij de moeilijke beslissing.",
                            "target_lang_translations": {
                                "en": "Without <w>any</w> doubt, he made the difficult decision.",
                                "ru": "Без <w>всякого</w> сомнения он принял это трудное решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "enigerlei"
                    ],
                    "common_phrases": [
                        "zonder enige twijfel",
                        "te eniger tijd",
                        "op enig moment"
                    ],
                    "target_lang_definitions": {
                        "en": "Whichever one; it does not matter which.",
                        "ru": "Какой бы то ни было; неважно какой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "any",
                                "target_lang_sense_clarification": "no matter which or what kind"
                            },
                            {
                                "target_lang_word": "whatsoever",
                                "target_lang_sense_clarification": "at all; used for emphasis"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "какой-либо",
                                "target_lang_sense_clarification": "какой угодно, любой"
                            },
                            {
                                "target_lang_word": "малейший",
                                "target_lang_sense_clarification": "самый незначительный (для усиления)"
                            },
                            {
                                "target_lang_word": "всякий",
                                "target_lang_sense_clarification": "любой, каждый (часто в отрицаниях)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
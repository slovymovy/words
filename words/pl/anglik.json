    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b3d86db4-a792-31c6-a345-9df556cea5b7",
                    "sense_definition": "Mężczyzna lub chłopiec narodowości angielskiej; mieszkaniec Anglii.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "nationality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy sąsiad to <w>Anglik</w>, który przeprowadził się do Polski.",
                            "target_lang_translations": {
                                "ru": "Мой новый сосед — <w>англичанин</w>, который переехал в Польшу."
                            }
                        },
                        {
                            "text": "Każdy <w>Anglik</w>, którego znam, pije herbatę z mlekiem.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>англичанин</w>, которого я знаю, пьет чай с молоком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "typowy Anglik",
                        "rodowity Anglik",
                        "znajomy Anglik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина или мальчик английской национальности; житель Англии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "англичанин",
                                "target_lang_sense_clarification": "национальность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dfa7b7cf-0bde-3851-bdc7-3bfb324e4d26",
                    "sense_definition": "Potoczne określenie języka angielskiego jako przedmiotu szkolnego lub lekcji tego języka.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę się dużo uczyć, bo jutro mam sprawdzian z <w>anglika</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно много учиться, потому что завтра у меня контрольная по <w>английскому</w>."
                            }
                        },
                        {
                            "text": "Nasza nauczycielka od <w>anglika</w> jest bardzo wymagająca.",
                            "target_lang_translations": {
                                "ru": "Наша учительница <w>английского</w> очень требовательная."
                            }
                        }
                    ],
                    "synonyms": [
                        "angielski",
                        "język angielski"
                    ],
                    "common_phrases": [
                        "uczyć się na anglika",
                        "zadanie z anglika",
                        "korepetycje z anglika"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "szkolny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название английского языка как школьного предмета или урока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "английский",
                                "target_lang_sense_clarification": "школьный предмет"
                            },
                            {
                                "target_lang_word": "инглиш",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4b58d42-ea53-38c0-a726-2f9ae1cd9619",
                    "sense_definition": "Samochód przystosowany do ruchu lewostronnego, posiadający kierownicę po prawej stronie (zwykle sprowadzony z Wielkiej Brytanii).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "automotive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rejestracja <w>anglika</w> w Polsce jest teraz znacznie łatwiejsza.",
                            "target_lang_translations": {
                                "ru": "Регистрация <w>англичанина</w> (праворульного авто) в Польше теперь стала значительно проще."
                            }
                        },
                        {
                            "text": "Mój brat kupił taniego <w>anglika</w> i przerobił go na wersję europejską.",
                            "target_lang_translations": {
                                "ru": "Мой брат купил дешевого <w>англичанина</w> и переделал его под европейскую версию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zarejestrować anglika",
                        "jeździć anglikiem",
                        "przeróbka anglika"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "motoryzacyjny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Автомобиль с правым рулем, предназначенный для левостороннего движения (обычно ввезенный из Великобритании)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "англичанин",
                                "target_lang_sense_clarification": "автомобильный сленг"
                            },
                            {
                                "target_lang_word": "праворульная машина",
                                "target_lang_sense_clarification": "автомобиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f427ab37-ec98-38a6-bb46-fb12c945b733",
                    "sense_definition": "Koń pełnej krwi angielskiej; rasa konia wyścigowego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na torze wyścigowym piękne <w>angliki</w> szykowały się do startu.",
                            "target_lang_translations": {
                                "ru": "На ипподроме прекрасные <w>английские скакуны</w> готовились к старту."
                            }
                        },
                        {
                            "text": "Ta stadnina słynie z hodowli szybkich i wytrzymałych <w>anglików</w>.",
                            "target_lang_translations": {
                                "ru": "Этот конный завод славится разведением быстрых и выносливых <w>английских чистокровных</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "folblut"
                    ],
                    "common_phrases": [
                        "hodowla anglików",
                        "stado anglików"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лошадь английской чистокровной верховой породы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "английская чистокровная",
                                "target_lang_sense_clarification": "порода лошадей"
                            },
                            {
                                "target_lang_word": "скакун-англичанин",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afecfb9c-5dd4-3e33-9db2-db84eaaf1549",
                    "sense_definition": "Rodzaj rozjazdu kolejowego (rozjazd krzyżowy).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "railway",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pociąg przejechał powoli przez stary <w>anglik</w> na bocznicy.",
                            "target_lang_translations": {
                                "ru": "Поезд медленно проехал через старую <w>английскую стрелку</w> на запасном пути."
                            }
                        },
                        {
                            "text": "Technicy musieli naprawić uszkodzony <w>anglik</w> przed przyjazdem składu.",
                            "target_lang_translations": {
                                "ru": "Техники должны были починить поврежденную <w>английскую стрелку</w> до прибытия состава."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "kolejowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид железнодорожной стрелки (перекрестный съезд)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "английская стрелка",
                                "target_lang_sense_clarification": "железнодорожный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d8f3a80e-ba5a-392f-ac6d-a9cc2eece564",
                    "sense_definition": "To be a sign that something good or bad will happen in the future.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company's rising profits <w>augur</w> well for the upcoming financial year.",
                            "target_lang_translations": {
                                "ru": "Растущая прибыль компании <w>сулит</w> успех в предстоящем финансовом году."
                            }
                        },
                        {
                            "text": "Constant disagreements during the negotiations did not <w>augur</w> well for a peaceful resolution.",
                            "target_lang_translations": {
                                "ru": "Постоянные разногласия в ходе переговоров не <w>предвещали</w> мирного разрешения ситуации."
                            }
                        },
                        {
                            "text": "These dark clouds <w>augur</w> a heavy storm later in the afternoon.",
                            "target_lang_translations": {
                                "ru": "Эти темные тучи <w>предвещают</w> сильную грозу во второй половине дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "bode",
                        "portend",
                        "foreshadow",
                        "predict",
                        "promise"
                    ],
                    "common_phrases": [
                        "augur well",
                        "augur ill",
                        "augur badly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служить предзнаменованием того, что в будущем произойдет что-то хорошее или плохое; указывать на исход событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предвещать",
                                "target_lang_sense_clarification": "служить признаком наступления чего-либо"
                            },
                            {
                                "target_lang_word": "сулить",
                                "target_lang_sense_clarification": "обещать или предсказывать (часто об удаче или неудаче)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "09e377c6-02b4-3be7-831e-e5032501137a",
                    "sense_definition": "A religious official in ancient Rome who predicted the future by interpreting signs and omens, such as the behavior of birds.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general refused to attack until the <w>augur</w> declared the signs favorable.",
                            "target_lang_translations": {
                                "ru": "Полководец отказывался наступать, пока <w>авгур</w> не объявит знамения благоприятными."
                            }
                        },
                        {
                            "text": "An <w>augur</w> was consulted to interpret the strange flight pattern of the eagles.",
                            "target_lang_translations": {
                                "ru": "Чтобы истолковать странный полет орлов, был приглашен <w>авгур</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "soothsayer",
                        "diviner",
                        "seer",
                        "prophet"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Ancient Rome"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жрец в Древнем Риме, который предсказывал будущее, толкуя знаки и знамения, например, полет птиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "авгур",
                                "target_lang_sense_clarification": "исторический термин; древнеримский жрец"
                            },
                            {
                                "target_lang_word": "прорицатель",
                                "target_lang_sense_clarification": "общее название предсказателя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "afa2dc95-f8f7-3cd7-b8e4-a89031f09553",
                    "sense_definition": "Оказывать помощь, содействие, выражать согласие или выступать в чью-либо защиту.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы всегда готовы <w>поддерживать</w> своих друзей в трудную минуту.",
                            "target_lang_translations": {
                                "en": "We are always ready to <w>support</w> our friends in difficult times."
                            }
                        },
                        {
                            "text": "Большинство сотрудников <w>поддерживают</w> новую стратегию развития компании.",
                            "target_lang_translations": {
                                "en": "Most employees <w>support</w> the company's new development strategy."
                            }
                        }
                    ],
                    "synonyms": [
                        "помогать",
                        "защищать"
                    ],
                    "antonyms": [
                        "противодействовать",
                        "противиться"
                    ],
                    "common_phrases": [
                        "поддерживать идею",
                        "поддерживать морально",
                        "поддерживать предложение",
                        "поддерживать кандидата"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide help, assistance, express agreement, or act in someone's defense."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "support",
                                "target_lang_sense_clarification": "to agree with or approve of; to help"
                            },
                            {
                                "target_lang_word": "back",
                                "target_lang_sense_clarification": "to give support to someone or something"
                            },
                            {
                                "target_lang_word": "back up",
                                "target_lang_sense_clarification": "to support or assist someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1928a978-2683-351f-ab06-8d6f2bf5d718",
                    "sense_definition": "Сохранять что-либо в определённом состоянии, не давать прекратиться или исчезнуть.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Очень важно <w>поддерживать</w> чистоту и порядок в своём доме.",
                            "target_lang_translations": {
                                "en": "It is very important to <w>maintain</w> cleanliness and order in one's home."
                            }
                        },
                        {
                            "text": "Они стараются <w>поддерживать</w> тёплые отношения с дальними родственниками.",
                            "target_lang_translations": {
                                "en": "They try to <w>maintain</w> warm relations with distant relatives."
                            }
                        }
                    ],
                    "synonyms": [
                        "сохранять",
                        "хранить"
                    ],
                    "antonyms": [
                        "прекращать"
                    ],
                    "common_phrases": [
                        "поддерживать разговор",
                        "поддерживать порядок",
                        "поддерживать связь",
                        "поддерживать температуру"
                    ],
                    "target_lang_definitions": {
                        "en": "To keep something in a certain state, preventing it from stopping or disappearing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "maintain",
                                "target_lang_sense_clarification": "to keep in an existing state"
                            },
                            {
                                "target_lang_word": "keep up",
                                "target_lang_sense_clarification": "to continue or sustain"
                            },
                            {
                                "target_lang_word": "preserve",
                                "target_lang_sense_clarification": "to keep safe or in a particular condition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f838a6ec-f430-3427-92df-802fede2797d",
                    "sense_definition": "Служить физической опорой кому-либо или чему-либо, не давая упасть.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старые деревянные столбы с трудом <w>поддерживают</w> крышу сарая.",
                            "target_lang_translations": {
                                "en": "Old wooden posts barely <w>support</w> the shed's roof."
                            }
                        },
                        {
                            "text": "Медсестра бережно <w>поддерживала</w> пациента, пока он шёл по коридору.",
                            "target_lang_translations": {
                                "en": "The nurse gently <w>supported</w> the patient while he walked down the corridor."
                            }
                        }
                    ],
                    "synonyms": [

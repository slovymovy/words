                },
                {
                    "sense_id": "c5e04bbd-91cd-314e-9dfa-28a4e2219df9",
                    "sense_definition": "The capital city of the state of Nebraska, United States.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "place_lincoln_us",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "She moved to <w>Lincoln</w> to attend the state university.",
                            "target_lang_translations": {
                                "ru": "Она переехала в <w>Линкольн</w>, чтобы учиться в университете штата."
                            }
                        },
                        {
                            "text": "The government offices in <w>Lincoln</w> are closed on public holidays.",
                            "target_lang_translations": {
                                "ru": "Правительственные учреждения в <w>Линкольне</w> закрыты в праздничные дни."
                            }

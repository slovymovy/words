                {
                    "sense_id": "24034320-7108-3ae8-89f6-cda4c141f6d6",
                    "sense_definition": "Stan chorobowy, w którym narząd lub układ organizmu nie jest w stanie prawidłowo wykonywać swoich funkcji fizjologicznych.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zdiagnozował u pacjenta przewlekłą <w>niewydolność</w> nerek.",
                            "target_lang_translations": {
                                "ru": "Врач диагностировал у пациента хроническую почечную <w>недостаточность</w>."
                            }
                        },
                        {
                            "text": "Nieleczona <w>niewydolność</w> serca może prowadzić do poważnych powikłań zdrowotnych.",
                            "target_lang_translations": {
                                "ru": "Нелеченая сердечная <w>недостаточность</w> может привести к серьезным осложнениям здоровья."
                            }
                        }
                    ],
                    "synonyms": [
                        "niedomoga"
                    ],
                    "antonyms": [
                        "wydolność",
                        "sprawność"
                    ],
                    "common_phrases": [
                        "niewydolność serca",
                        "niewydolność nerek",
                        "niewydolność oddechowa",
                        "niewydolność krążenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Патологическое состояние, при котором орган или система организма не способны нормально выполнять свои физиологические функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недостаточность",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "501241cc-fd28-3316-8e38-6f33f2235e72",

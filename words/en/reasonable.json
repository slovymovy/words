    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a7292101-53ef-3bb4-886b-41826663d940",
                    "sense_definition": "Fair, practical, and showing good judgment.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>reasonable</w> to expect a reply within two business days.",
                            "target_lang_translations": {
                                "ru": "<w>Разумно</w> ожидать ответа в течение двух рабочих дней."
                            }
                        },
                        {
                            "text": "Please be <w>reasonable</w> and listen to my side of the story.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, будь <w>благоразумным</w> и выслушай мою версию событий."
                            }
                        }
                    ],
                    "synonyms": [
                        "fair",
                        "sensible",
                        "just",
                        "equitable"
                    ],
                    "antonyms": [
                        "unreasonable",
                        "unjust",
                        "absurd",
                        "arbitrary"
                    ],
                    "common_phrases": [
                        "be reasonable",
                        "reasonable request",
                        "reasonable doubt",
                        "reasonable approach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Справедливый, практичный и проявляющий здравый смысл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разумный",
                                "target_lang_sense_clarification": "основанный на здравом смысле"
                            },
                            {
                                "target_lang_word": "благоразумный",
                                "target_lang_sense_clarification": "рассудительный (о человеке)"
                            },
                            {
                                "target_lang_word": "справедливый",
                                "target_lang_sense_clarification": "обоснованный (о требовании)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "026822e5-72e5-3b5b-a92f-65ba514d3c4e",
                    "sense_definition": "Not too expensive; sold at a fair price.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The restaurant serves delicious food at very <w>reasonable</w> prices.",
                            "target_lang_translations": {
                                "ru": "В этом ресторане подают вкусную еду по очень <w>разумным</w> ценам."
                            }
                        },
                        {
                            "text": "We managed to find a hotel with <w>reasonable</w> rates near the city center.",
                            "target_lang_translations": {
                                "ru": "Нам удалось найти отель с <w>приемлемыми</w> ценами недалеко от центра города."
                            }
                        }
                    ],
                    "synonyms": [
                        "affordable",
                        "inexpensive",
                        "economical",
                        "competitive"
                    ],
                    "antonyms": [
                        "expensive",
                        "extortionate"
                    ],
                    "common_phrases": [
                        "reasonable price",
                        "reasonable rates",
                        "reasonable cost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не слишком дорогой; продаваемый по справедливой цене."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приемлемый",
                                "target_lang_sense_clarification": "о цене"
                            },
                            {
                                "target_lang_word": "умеренный",
                                "target_lang_sense_clarification": "средний, невысокий"
                            },
                            {
                                "target_lang_word": "недорогой",
                                "target_lang_sense_clarification": "доступный по цене"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cb65022-06aa-3e44-a16e-2526b00f2b10",
                    "sense_definition": "Acceptable or satisfactory, though not outstanding.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team has a <w>reasonable</w> chance of winning the championship this year.",
                            "target_lang_translations": {
                                "ru": "У команды есть <w>неплохие</w> шансы выиграть чемпионат в этом году."
                            }
                        },
                        {
                            "text": "She did a <w>reasonable</w> job painting the fence, considering it was her first time.",
                            "target_lang_translations": {
                                "ru": "Она сделала <w>неплохую</w> работу по покраске забора, учитывая, что это был её первый раз."
                            }
                        }
                    ],
                    "synonyms": [
                        "acceptable",
                        "satisfactory",
                        "adequate",
                        "decent",
                        "passable"
                    ],
                    "antonyms": [
                        "unsatisfactory"
                    ],
                    "common_phrases": [
                        "reasonable chance",
                        "reasonable job",
                        "reasonable condition",
                        "reasonable standard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приемлемый или удовлетворительный, хотя и не выдающийся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приемлемый",
                                "target_lang_sense_clarification": "допустимый"

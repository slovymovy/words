                            {
                                "target_lang_word": "последний срок",
                                "target_lang_sense_clarification": "буквальное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2dba4170-427a-3dfe-bb2c-c735f243b7f4",
                    "sense_definition": "A boundary around a prison, prisoners crossing which would be shot.",

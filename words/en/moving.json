    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ca927d32-6bda-3865-89cc-da0c9e18e52a",
                    "sense_definition": "The present participle and gerund of <w>move</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are <w>moving</w> to a new apartment in the city next month.",
                            "target_lang_translations": {
                                "ru": "В следующем месяце мы <w>переезжаем</w> в новую квартиру в городе."
                            }
                        },
                        {
                            "text": "The traffic is finally <w>moving</w> after being stuck for an hour.",
                            "target_lang_translations": {
                                "ru": "Транспорт наконец-то <w>движется</w> после того, как простоял час в пробке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "keep moving",
                        "get moving",
                        "start moving"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола move (двигаться, переезжать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переезжать",
                                "target_lang_sense_clarification": "менять место жительства"
                            },
                            {
                                "target_lang_word": "двигаться",
                                "target_lang_sense_clarification": "совершать движение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "00f40e19-cdb7-3ee6-b0d4-d1baeab5a3c4",
                    "sense_definition": "In motion; changing position or place; not stationary.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to touch any <w>moving</w> parts inside the machine.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны, не касайтесь <w>движущихся</w> частей внутри машины."
                            }
                        },
                        {
                            "text": "It is very difficult to hit a fast <w>moving</w> target.",
                            "target_lang_translations": {
                                "ru": "Очень трудно попасть в быстро <w>движущуюся</w> цель."
                            }
                        }
                    ],
                    "synonyms": [
                        "mobile",
                        "dynamic",
                        "in motion"
                    ],
                    "antonyms": [
                        "stationary"
                    ],
                    "common_phrases": [
                        "moving parts",
                        "moving target",
                        "fast moving"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в движении; меняющий положение; не стационарный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "движущийся",
                                "target_lang_sense_clarification": "находящийся в процессе движения"
                            },
                            {
                                "target_lang_word": "подвижный",
                                "target_lang_sense_clarification": "способный двигаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3a6d3e0-a4ae-336b-b45e-a96a80f6ad5a",
                    "sense_definition": "Causing strong feelings of sadness, sympathy, or admiration.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The director gave a deeply <w>moving</w> speech at the awards ceremony.",
                            "target_lang_translations": {
                                "ru": "Режиссер произнес глубоко <w>трогательную</w> речь на церемонии награждения."
                            }
                        },
                        {
                            "text": "It was a <w>moving</w> story about a family reunited after the war.",
                            "target_lang_translations": {
                                "ru": "Это была <w>трогательная</w> история о семье, воссоединившейся после войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "touching",
                        "affecting",
                        "poignant",
                        "emotional",
                        "gripping"
                    ],
                    "antonyms": [
                        "unaffecting",
                        "nonemotional"
                    ],
                    "common_phrases": [
                        "deeply moving",
                        "moving experience",
                        "moving tribute"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий сильные чувства грусти, сочувствия или восхищения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трогательный",
                                "target_lang_sense_clarification": "вызывающий умиление или сочувствие"
                            },
                            {
                                "target_lang_word": "волнующий",
                                "target_lang_sense_clarification": "вызывающий сильные эмоции"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5e81fb14-7934-384f-a27d-054b36cef2a9",
                    "sense_definition": "The act of changing one's residence or place of business.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>moving</w> took much longer than we had anticipated.",
                            "target_lang_translations": {
                                "ru": "<w>Переезд</w> занял гораздо больше времени, чем мы ожидали."
                            }
                        },
                        {
                            "text": "We hired a truck to help with the <w>moving</w> of our furniture.",
                            "target_lang_translations": {
                                "ru": "Мы наняли грузовик для <w>перевозки</w> нашей мебели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "moving day",
                        "moving company",
                        "moving expenses"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс смены места жительства или работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переезд",
                                "target_lang_sense_clarification": "смена места жительства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8e17453-f3ef-3854-93d6-81a9e7631a85",
                    "sense_definition": "A specific instance of movement or motion.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cat watched the tiny <w>movings</w> of the mouse in the grass.",
                            "target_lang_translations": {
                                "ru": "Кошка наблюдала за крошечными <w>движениями</w> мыши в траве."
                            }
                        },
                        {
                            "text": "She studied the complex <w>movings</w> of the celestial bodies.",
                            "target_lang_translations": {
                                "ru": "Она изучала сложные <w>движения</w> небесных тел."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный случай движения или перемещения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "движение",
                                "target_lang_sense_clarification": "действие или процесс"
                            },
                            {
                                "target_lang_word": "перемещение",
                                "target_lang_sense_clarification": "смена положения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

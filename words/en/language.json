                {
                    "sense_id": "1c3e0dd0-dbdf-3385-a5b8-1016f612633d",
                    "sense_definition": "Rude, offensive, or vulgar words.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "profanity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher scolded the student for using bad <w>language</w> in class.",
                            "target_lang_translations": {

                            {
                                "target_lang_word": "bags",
                                "target_lang_sense_clarification": "flexible containers for carrying things"
                            },
                            {
                                "target_lang_word": "pockets",
                                "target_lang_sense_clarification": "pouches sewn into clothing"
                            },
                            {
                                "target_lang_word": "sacks",
                                "target_lang_sense_clarification": "large bags made of strong material"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мешки",
                                "target_lang_sense_clarification": "гибкие контейнеры для переноски вещей"
                            },
                            {
                                "target_lang_word": "карманы",
                                "target_lang_sense_clarification": "отделения, вшитые в одежду"
                            },
                            {
                                "target_lang_word": "пакеты",
                                "target_lang_sense_clarification": "сумки из пластика или бумаги"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
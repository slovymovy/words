    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1532d6ab-2525-302c-b91a-43594b28bf58",
                    "sense_definition": "Zakończyć wykonywanie jakiejś czynności lub przerwać trwanie jakiegoś stanu",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_cease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz natychmiast <w>przestać</w> palić papierosy, jeśli chcesz być zdrowy.",
                            "target_lang_translations": {
                                "ru": "Ты должен немедленно <w>перестать</w> курить сигареты, если хочешь быть здоровым."
                            }
                        },
                        {
                            "text": "Kiedy wreszcie <w>przestanie</w> padać ten ulewny deszcz?",
                            "target_lang_translations": {
                                "ru": "Когда наконец <w>перестанет</w> идти этот проливной дождь?"
                            }
                        },
                        {
                            "text": "Dziecko <w>przestało</w> płakać, gdy tylko zobaczyło swoją mamę.",
                            "target_lang_translations": {
                                "ru": "Ребёнок <w>перестал</w> плакать, как только увидел свою маму."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgodzić się",
                        "przystać",
                        "poprzestać",
                        "zostać"
                    ],
                    "antonyms": [
                        "zacząć",
                        "rozpocząć",
                        "kontynuować"
                    ],
                    "common_phrases": [
                        "przestać palić",
                        "przestać mówić",
                        "przestać się martwić",
                        "przestać płakać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекратить выполнение какого-либо действия или прервать состояние"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перестать",
                                "target_lang_sense_clarification": "прекратить действие"
                            },
                            {
                                "target_lang_word": "прекратить",
                                "target_lang_sense_clarification": "остановить, положить конец"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96cbca5b-90d0-3c08-87fd-a6436290ccb2",
                    "sense_definition": "Spędzić określony czas w pozycji stojącej",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_stance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy <w>przestać</w> w długiej kolejce prawie dwie godziny, żeby kupić bilety.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>простоять</w> в длинной очереди почти два часа, чтобы купить билеты."
                            }
                        },
                        {
                            "text": "Nogi mnie bolą, bo <w>przestałem</w> cały koncert pod samą sceną.",
                            "target_lang_translations": {
                                "ru": "У меня болят ноги, потому что я <w>простоял</w> весь концерт у самой сцены."
                            }
                        }
                    ],
                    "synonyms": [
                        "zostać"
                    ],
                    "common_phrases": [
                        "przestać w kolejce",
                        "przestać całą mszę",
                        "przestać na nogach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Провести определённое время в положении стоя"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простоять",
                                "target_lang_sense_clarification": "провести время стоя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54d41ac8-7b5f-357c-83a4-3c14ed1db5d0",
                    "sense_definition": "O cieczy lub potrawie: stracić świeżość lub zmienić smak wskutek długiego stania w jednym miejscu",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "state_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie pij tej herbaty, bo <w>przestała</w> na stole całą noc i jest niesmaczna.",
                            "target_lang_translations": {
                                "ru": "Не пей этот чай, так как он <w>простоял</w> на столе всю ночь и стал невкусным."
                            }
                        },
                        {
                            "text": "Woda w otwartym dzbanku <w>przestała</w> kilka dni i nadaje się tylko do podlania kwiatów.",
                            "target_lang_translations": {
                                "ru": "Вода в открытом кувшине <w>простояла</w> несколько дней и годится только для полива цветов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "woda przestała",
                        "zupa przestała"
                    ],
                    "target_lang_definitions": {
                        "ru": "О жидкости или еде: потерять свежесть или изменить вкус из-за долгого стояния"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простоять",
                                "target_lang_sense_clarification": "находиться где-либо долгое время"
                            },
                            {
                                "target_lang_word": "застояться",
                                "target_lang_sense_clarification": "испортиться от времени"
                            }
                        ]
                    }
                }
            ]
        }
    ]

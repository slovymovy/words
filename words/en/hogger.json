                },
                {
                    "sense_id": "564cd69c-4373-3ac1-957c-457ee5ce01d2",
                    "sense_definition": "A marijuana cigarette.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_drug",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was caught smoking a <w>hogger</w> behind the garage.",

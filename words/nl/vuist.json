                {
                    "sense_id": "272b3a2b-1efe-33bd-8188-9bf30e6c940a",
                    "sense_definition": "Een hand waarvan de vingers stevig naar binnen gebogen zijn tegen de handpalm.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij sloeg hard met zijn <w>vuist</w> op de houten tafel.",
                            "target_lang_translations": {
                                "ru": "Он сильно ударил <w>кулаком</w> по деревянному столу."
                            }
                        },
                        {
                            "text": "Zij balde haar hand tot een <w>vuist</w> om haar woede te tonen.",
                            "target_lang_translations": {
                                "ru": "Она сжала руку в <w>кулак</w>, чтобы показать свой гнев."
                            }
                        }
                    ],
                    "common_phrases": [
                        "met de vuist op tafel slaan",
                        "voor de vuist weg",
                        "in het vuistje lachen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кисть руки, пальцы которой плотно прижаты к ладони."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кулак",
                                "target_lang_sense_clarification": "сжатая кисть руки"
                            }
                        ]

{
    "word_family": [
        "transparency",
        "transparently",
        "nontransparent",
        "semitransparent"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bb6cb64a-5372-3528-8aeb-7eae457ed9a5",
                    "sense_definition": "Allowing light to pass through so that objects behind can be distinctly seen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The water in the mountain lake is so <w>transparent</w> that you can see every stone on the bottom.",
                            "target_lang_translations": {
                                "ru": "Вода в горном озере настолько <w>прозрачная</w>, что видно каждый камень на дне."
                            }
                        },
                        {
                            "text": "She placed the photo inside a <w>transparent</w> plastic sleeve to protect it from damage.",
                            "target_lang_translations": {
                                "ru": "Она поместила фотографию в <w>прозрачный</w> пластиковый конверт, чтобы защитить её от повреждений."
                            }
                        }
                    ],
                    "synonyms": [
                        "see-through",
                        "clear",
                        "crystalline",
                        "limpid",
                        "sheer"
                    ],
                    "antonyms": [
                        "opaque",
                        "nontransparent"
                    ],
                    "common_phrases": [
                        "transparent glass",
                        "transparent material",
                        "transparent layer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пропускающий свет так, что предметы позади отчетливо видны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачный",
                                "target_lang_sense_clarification": "физическое свойство"
                            },
                            {
                                "target_lang_word": "просвечивающий",
                                "target_lang_sense_clarification": "пропускающий свет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c4a14b5-4312-3f45-b318-caa79347cf14",
                    "sense_definition": "Open and honest, without secrets; allowing processes, actions, or rules to be easily seen and understood by the public.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The organization promised a fully <w>transparent</w> hiring process to ensure fairness for all candidates.",
                            "target_lang_translations": {
                                "ru": "Организация пообещала полностью <w>прозрачный</w> процесс найма, чтобы обеспечить справедливость для всех кандидатов."
                            }
                        },
                        {
                            "text": "Citizens are demanding more <w>transparent</w> government policies regarding the use of tax money.",
                            "target_lang_translations": {
                                "ru": "Граждане требуют более <w>прозрачной</w> государственной политики в отношении использования налоговых средств."
                            }
                        }
                    ],
                    "synonyms": [
                        "clear",
                        "apparent"
                    ],
                    "antonyms": [
                        "opaque"
                    ],
                    "common_phrases": [
                        "transparent process",
                        "transparent system",
                        "fully transparent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открытый и честный, без секретов; позволяющий легко видеть и понимать процессы или правила."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачный",
                                "target_lang_sense_clarification": "метафорически: понятный, честный"
                            },
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "доступный для общественности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d937d49-8c94-3c13-ad28-1b53c786e5d8",
                    "sense_definition": "Easy to perceive, detect, or understand; obvious (often describing a lie, excuse, or emotion).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His excuse for missing the meeting was a <w>transparent</w> lie that fooled absolutely no one.",
                            "target_lang_translations": {
                                "ru": "Его оправдание пропуска собрания было <w>очевидной</w> ложью, которая никого не обманула."
                            }
                        },
                        {
                            "text": "It was a <w>transparent</w> attempt to distract us from the real issue at hand.",
                            "target_lang_translations": {
                                "ru": "Это была <w>явная</w> попытка отвлечь нас от реальной проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "obvious",
                        "apparent",
                        "clear"
                    ],
                    "antonyms": [
                        "obscure"
                    ],
                    "common_phrases": [
                        "transparent lie",
                        "transparent excuse",
                        "transparent attempt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкий для восприятия или понимания; очевидный (часто о лжи, оправданиях или эмоциях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очевидный",
                                "target_lang_sense_clarification": "несомненный"
                            },
                            {
                                "target_lang_word": "явный",
                                "target_lang_sense_clarification": "нескрываемый"
                            },
                            {
                                "target_lang_word": "прозрачный",
                                "target_lang_sense_clarification": "о намеках или намерениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd52ec14-8d9c-3385-9b38-07b0ea262879",
                    "sense_definition": "Functioning without the user being aware of its presence or operation; invisible to the user.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technical_computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software update was designed to be <w>transparent</w> to the user, requiring no manual restart.",
                            "target_lang_translations": {
                                "ru": "Обновление программного обеспечения было разработано так, чтобы быть <w>прозрачным</w> для пользователя и не требовать ручной перезагрузки."
                            }
                        },
                        {
                            "text": "Browsers often make data caching <w>transparent</w> so that pages load faster without input.",
                            "target_lang_translations": {
                                "ru": "Браузеры часто делают кэширование данных <w>прозрачным</w> (незаметным), чтобы страницы загружались быстрее без вмешательства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "transparent to users",
                        "transparent operation"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Функционирующий незаметно для пользователя; не требующий внимания при работе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачный",
                                "target_lang_sense_clarification": "компьютерный термин"
                            },
                            {
                                "target_lang_word": "незаметный",
                                "target_lang_sense_clarification": "скрытый от пользователя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3c665913-5bc5-3c59-bdea-eb7c17a82679",
                    "sense_definition": "An object or material that allows light to pass through.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist used various <w>transparents</w> and opaques to create a textured effect in the glasswork.",
                            "target_lang_translations": {
                                "ru": "Художник использовал различные <w>прозрачные материалы</w> и непрозрачные краски для создания текстурного эффекта в работе со стеклом."
                            }
                        },
                        {
                            "text": "Care must be taken when adding the <w>transparents</w> to the mold so they do not overflow.",
                            "target_lang_translations": {
                                "ru": "Нужно быть осторожным при добавлении <w>прозрачных компонентов</w> в форму, чтобы они не перелились через край."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical or rare"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет или материал, который пропускает свет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачный материал",
                                "target_lang_sense_clarification": "субстантив"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f2194e9-4b18-3625-912c-b5cc020ff8b2",
                    "sense_definition": "A transgender or transsexual parent.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The support group provides a safe space for <w>transparents</w> to discuss their experiences raising children.",
                            "target_lang_translations": {
                                "ru": "Группа поддержки предоставляет безопасное пространство для <w>транс-родителей</w>, чтобы они могли обсудить свой опыт воспитания детей."
                            }
                        },
                        {
                            "text": "Books about <w>transparents</w> are becoming more available to help families understand diverse identities.",
                            "target_lang_translations": {
                                "ru": "Книги о <w>транс-родителях</w> становятся всё более доступными, помогая семьям понять разнообразие идентичностей."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "neologism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Родитель, являющийся трансгендером или транссексуалом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "транс-родитель",
                                "target_lang_sense_clarification": "неологизм"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
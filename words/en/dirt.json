            "senses": [
                {
                    "sense_id": "691495c7-0353-337b-ad37-50e9d384671f",
                    "sense_definition": "Soil or earth.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children sat in the <w>dirt</w> and played with their toy trucks.",
                            "target_lang_translations": {
                                "ru": "Дети сидели в <w>грязи</w> и играли со своими игрушечными грузовиками."
                            }
                        },
                        {
                            "text": "We drove down a long <w>dirt</w> road to get to the farm.",
                            "target_lang_translations": {
                                "ru": "Мы ехали по длинной <w>грунтовой</w> дороге, чтобы добраться до фермы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dirt road",
                        "pile of dirt",
                        "dig in the dirt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Почва или земля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "земля",
                                "target_lang_sense_clarification": "почва, грунт"
                            },
                            {
                                "target_lang_word": "грязь",
                                "target_lang_sense_clarification": "размокшая почва"
                            },
                            {
                                "target_lang_word": "грунт",
                                "target_lang_sense_clarification": "почва, основание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d14f16b-55e0-3783-8a14-1fafc1e46987",
                    "sense_definition": "A stain, spot, or any foreign substance that makes something unclean.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please wash the <w>dirt</w> off your hands before you eat dinner.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, смой <w>грязь</w> с рук перед ужином."
                            }
                        },
                        {
                            "text": "His clothes were covered in <w>dirt</w> after working in the garden.",
                            "target_lang_translations": {
                                "ru": "Его одежда была покрыта <w>грязью</w> после работы в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "filth"
                    ],
                    "common_phrases": [
                        "covered in dirt",
                        "wash off dirt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пятно, нечистоты или любое вещество, делающее предмет грязным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грязь",
                                "target_lang_sense_clarification": "то, что пачкает"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3473e2ca-5f89-3ddf-8552-61efcad7c5e7",
                    "sense_definition": "Previously unknown facts or rumors about a person, often of a scandalous nature.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "information",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The reporter tried to dig up <w>dirt</w> on the famous businessman.",
                            "target_lang_translations": {
                                "ru": "Репортёр пытался накопать <w>компромат</w> на известного бизнесмена."
                            }
                        },
                        {
                            "text": "She loves to hear all the latest <w>dirt</w> about her neighbors.",
                            "target_lang_translations": {
                                "ru": "Она обожает слушать все последние <w>сплетни</w> о своих соседях."
                            }
                        }
                    ],
                    "synonyms": [
                        "gossip",
                        "kompromat"
                    ],
                    "common_phrases": [
                        "dig up dirt",
                        "dish the dirt"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in informal contexts regarding gossip"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ранее неизвестные факты или слухи о человеке, часто скандального характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компромат",
                                "target_lang_sense_clarification": "компрометирующие материалы"
                            },
                            {
                                "target_lang_word": "сплетни",
                                "target_lang_sense_clarification": "слухи"
                            },
                            {
                                "target_lang_word": "грязь",

                {
                    "sense_id": "633e64b0-116e-3a12-a551-f77c184cff9f",
                    "sense_definition": "Iemand die kippen steelt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer heeft gisternacht een <w>kippendief</w> op zijn erf betrapt.",
                            "target_lang_translations": {
                                "ru": "Прошлой ночью фермер поймал <w>куриного вора</w> на своем дворе."
                            }
                        },
                        {
                            "text": "De politie zoekt naar de <w>kippendief</w> die al weken actief is in de regio.",
                            "target_lang_translations": {
                                "ru": "Полиция разыскивает <w>куриного вора</w>, который уже несколько недель орудует в регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoenderdief"
                    ],
                    "common_phrases": [
                        "sluwe kippendief"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто крадет кур."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куриный вор",
                                "target_lang_sense_clarification": "человек, совершающий кражу"
                            },
                            {
                                "target_lang_word": "похититель кур",
                                "target_lang_sense_clarification": "более формальный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8daff90c-a96b-36f3-bf14-1008a8e8bafd",
                    "sense_definition": "Een vos, vaak zo genoemd omdat deze dieren op pluimvee jagen.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de fabel staat de <w>kippendief</w> bekend om zijn sluwe streken.",
                            "target_lang_translations": {
                                "ru": "В басне <w>лис</w> известен своими хитрыми проделками."
                            }
                        },
                        {
                            "text": "De jager zag de rode <w>kippendief</w> langs de bosrand sluipen.",
                            "target_lang_translations": {
                                "ru": "Охотник увидел, как рыжая <w>лиса</w> крадется вдоль опушки леса."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лиса; часто так называют из-за того, что эти животные охотятся на домашнюю птицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лиса",
                                "target_lang_sense_clarification": "животное"
                            },
                            {
                                "target_lang_word": "лис",
                                "target_lang_sense_clarification": "самец лисы, персонаж сказок"
                            }
                        ]
                    }
                }
            ]
        }

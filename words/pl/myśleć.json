            "senses": [
                {
                    "sense_id": "5624039e-fa9e-3638-8daa-65fec3eb2566",
                    "sense_definition": "Przetwarzać informacje w umyśle, zastanawiać się nad czymś, analizować fakty w celu zrozumienia czegoś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cognitive_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę chwilę <myśleć>, zanim odpowiem na to trudne pytanie.",
                            "target_lang_translations": {
                                "ru": "Мне нужно немного <подумать>, прежде чем я отвечу на этот трудный вопрос."
                            }
                        },
                        {
                            "text": "Detektyw intensywnie <myślał> nad rozwiązaniem tej skomplikowanej zagadki.",
                            "target_lang_translations": {
                                "ru": "Детектив напряженно <размышлял> над решением этой сложной загадки."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozumować",
                        "rozważać",
                        "analizować",
                        "rozmyślać"
                    ],
                    "common_phrases": [
                        "myśleć logicznie",
                        "głośno myśleć",
                        "intensywnie myśleć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обрабатывать информацию в уме, размышлять над чем-то, анализировать факты для понимания сути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "думать",
                                "target_lang_sense_clarification": "общая мыслительная деятельность"
                            },
                            {
                                "target_lang_word": "размышлять",
                                "target_lang_sense_clarification": "глубоко обдумывать, анализировать"
                            },
                            {
                                "target_lang_word": "мыслить",
                                "target_lang_sense_clarification": "обладать способностью рассуждать, строить умозаключения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d228b2b8-2505-335c-a603-6fe3b2c5e795",
                    "sense_definition": "Mieć określone zdanie, opinię lub pogląd na dany temat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Myślę>, że to jest bardzo dobry pomysł na spędzenie weekendu.",
                            "target_lang_translations": {
                                "ru": "<Думаю>, что это очень хорошая идея для проведения выходных."
                            }
                        },
                        {
                            "text": "Co <myślisz> o nowym pracowniku naszej firmy?",
                            "target_lang_translations": {
                                "ru": "Что ты <думаешь> о новом сотруднике нашей фирмы?"
                            }
                        }
                    ],
                    "synonyms": [
                        "sądzić",
                        "uważać",
                        "mniemać"
                    ],
                    "common_phrases": [
                        "co o tym myślisz",
                        "tak myślę",
                        "myśleć inaczej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенное мнение, взгляд или суждение по какому-либо вопросу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "думать",
                                "target_lang_sense_clarification": "иметь мнение"
                            },
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "быть уверенным в своем мнении"
                            },
                            {
                                "target_lang_word": "полагать",
                                "target_lang_sense_clarification": "формально: допускать, считать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18cb6b03-3c3f-37e2-9716-6136dd1bbfaa",
                    "sense_definition": "Mieć zamiar coś zrobić, planować podjęcie jakiegoś działania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "intention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od dawna <myślałem> o nauce języka hiszpańskiego.",
                            "target_lang_translations": {
                                "ru": "Я давно <думал> об изучении испанского языка."
                            }
                        },
                        {
                            "text": "Ona poważnie <myśli> o założeniu własnej działalności gospodarczej.",
                            "target_lang_translations": {
                                "ru": "Она всерьез <думает> об открытии собственного бизнеса."
                            }
                        }
                    ],
                    "synonyms": [
                        "zamierzać",
                        "planować",
                        "chcieć",
                        "postanawiać"
                    ],
                    "common_phrases": [
                        "myśleć o przyszłości",
                        "myśleć o wyjeździe",
                        "myśleć o zmianie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь намерение что-то сделать, планировать какие-либо действия или перемены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "думать",
                                "target_lang_sense_clarification": "обдумывать план, намереваться (обычно с предлогом 'о')"
                            },
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "готовиться совершить действие"
                            },
                            {
                                "target_lang_word": "подумывать",
                                "target_lang_sense_clarification": "размышлять о возможности что-то сделать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47f9b628-2ddf-37bd-a6f8-4b8b5f26d06a",
                    "sense_definition": "Skupiać myśli i uwagę na kimś lub na czymś, nie zapominać o czymś.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "cognitive_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas ważnego egzaminu uczeń <myślał> tylko o poprawnych odpowiedziach.",
                            "target_lang_translations": {
                                "ru": "Во время важного экзамена ученик <думал> только о правильных ответах."
                            }
                        },
                        {
                            "text": "Zawsze ciepło <myślę> o moich dziadkach, którzy mieszkają daleko.",
                            "target_lang_translations": {
                                "ru": "Я всегда с теплотой <думаю> о моих дедушке и бабушке, которые живут далеко."
                            }
                        }
                    ],
                    "common_phrases": [
                        "myśleć o kimś",
                        "ciągle o czymś myśleć",
                        "myśleć o wszystkim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосредоточивать мысли и внимание на ком-то или чем-то, хранить в памяти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "думать",
                                "target_lang_sense_clarification": "направлять мысли на объект, заботиться"
                            },
                            {
                                "target_lang_word": "вспоминать",
                                "target_lang_sense_clarification": "воспроизводить в памяти"
                            }
                        ]
                    }
                }
            ]

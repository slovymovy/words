                            },
                            {
                                "target_lang_word": "opposite",
                                "target_lang_sense_clarification": "contrary in nature or effect"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обратный",
                                "target_lang_sense_clarification": "противоположный порядок или направление"
                            },
                            {
                                "target_lang_word": "противоположный",
                                "target_lang_sense_clarification": "находящийся напротив или действующий иначе"
                            }
                        ]

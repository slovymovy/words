    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad840477-62ef-3bb6-88c0-2f681e414818",
                    "sense_definition": "Een symbolische weergave van een wiskundige regel, vergelijking of de samenstelling van een stof",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar schreef de ingewikkelde <w>formule</w> op het schoolbord.",
                            "target_lang_translations": {
                                "ru": "Учитель написал сложную <w>формулу</w> на школьной доске."
                            }
                        },
                        {
                            "text": "Water heeft de bekende chemische <w>formule</w> H2O.",
                            "target_lang_translations": {
                                "ru": "Вода имеет известную химическую <w>формулу</w> H2O."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergelijking"
                    ],
                    "common_phrases": [
                        "wiskundige formule",
                        "chemische formule"
                    ],
                    "target_lang_definitions": {
                        "ru": "Символическое представление математического правила, уравнения или состава вещества"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формула",
                                "target_lang_sense_clarification": "математическая или химическая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6cae1f9-f4da-31fa-bdba-a9b938be5ed6",
                    "sense_definition": "Een gestandaardiseerde opzet of werkwijze, vaak gebruikt bij winkelketens of televisieprogramma's",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De supermarktketen heeft veel succes met deze nieuwe <w>formule</w>.",
                            "target_lang_translations": {
                                "ru": "Сеть супермаркетов имеет большой успех с этой новой <w>концепцией</w>."
                            }
                        },
                        {
                            "text": "Het televisieprogramma hanteert al jaren dezelfde vaste <w>formule</w>.",
                            "target_lang_translations": {
                                "ru": "Телепрограмма уже много лет использует один и тот же <w>формат</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "concept",
                        "opzet",
                        "werkwijze"
                    ],
                    "common_phrases": [
                        "succesvolle formule",
                        "vaste formule"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандартизированная концепция или метод работы, часто используемые в торговых сетях или телепрограммах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "концепция",
                                "target_lang_sense_clarification": "бизнес-модель"
                            },
                            {
                                "target_lang_word": "формат",
                                "target_lang_sense_clarification": "структура или организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27be2978-5112-3f51-a8cb-0400ee21eba3",
                    "sense_definition": "Een manier of methode die men volgt om een bepaald resultaat te bereiken",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hard werken is voor haar de <w>formule</w> voor succes.",
                            "target_lang_translations": {
                                "ru": "Упорный труд для неё — это <w>формула</w> успеха."
                            }
                        },
                        {
                            "text": "We zoeken nog naar de juiste <w>formule</w> om dit probleem op te lossen.",
                            "target_lang_translations": {
                                "ru": "Мы всё ещё ищем правильный <w>рецепт</w> для решения этой проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "recept",
                        "methode"
                    ],
                    "common_phrases": [
                        "formule voor succes",
                        "gouden formule"
                    ],
                    "target_lang_definitions": {

            "senses": [
                {
                    "sense_id": "b5a879a5-181e-3673-b695-ebb4b657e2ef",
                    "sense_definition": "Solitary; having no companion or others present.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>lone</w> wolf wandered silently through the snowy forest.",
                            "target_lang_translations": {
                                "ru": "<w>Одинокий</w> волк молча бродил по заснеженному лесу."
                            }
                        },
                        {
                            "text": "She sat as a <w>lone</w> figure on the bench, watching the sunset.",
                            "target_lang_translations": {
                                "ru": "Она сидела на скамейке как <w>одинокая</w> фигура, наблюдая за закатом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lone wolf",
                        "lone ranger",
                        "lone gunman"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одинокий; не имеющий спутников или других людей рядом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одинокий",
                                "target_lang_sense_clarification": "solitary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fa476ce-3196-3b38-9db8-477214f0ee51",
                    "sense_definition": "Sole; being the only one of a specific type in a group or place.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was the <w>lone</w> survivor of the terrible plane crash.",
                            "target_lang_translations": {
                                "ru": "Он был <w>единственным</w> выжившим в ужасной авиакатастрофе."
                            }
                        },
                        {
                            "text": "A <w>lone</w> voice in the crowd shouted out a question to the speaker.",
                            "target_lang_translations": {
                                "ru": "<w>Одинокий</w> голос из толпы выкрикнул вопрос оратору."
                            }
                        }
                    ],
                    "synonyms": [
                        "only"
                    ],
                    "common_phrases": [
                        "lone survivor",
                        "lone voice",
                        "lone parent"
                    ],
                    "target_lang_definitions": {

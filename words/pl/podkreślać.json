            "senses": [
                {
                    "sense_id": "28d9973a-2fb8-3f64-8094-f4150f232edc",
                    "sense_definition": "Kłaść nacisk na coś w wypowiedzi lub zachowaniu, aby zwrócić na to uwagę i zaznaczyć tego ważność.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor na każdym spotkaniu <w>podkreśla</w> znaczenie punktualności w pracy.",
                            "target_lang_translations": {
                                "ru": "Директор на каждом собрании <w>подчёркивает</w> важность пунктуальности в работе."
                            }
                        },
                        {
                            "text": "W swojej prezentacji musisz wyraźnie <w>podkreślać</w> najważniejsze wnioski z badań.",
                            "target_lang_translations": {
                                "ru": "В своей презентации ты должен чётко <w>подчёркивать</w> самые важные выводы исследования."
                            }
                        }
                    ],
                    "synonyms": [
                        "akcentować",
                        "zaznaczać",
                        "uwydatniać",
                        "kłaść nacisk"
                    ],
                    "antonyms": [
                        "bagatelizować",
                        "umniejszać",
                        "pomijać"
                    ],
                    "common_phrases": [
                        "podkreślać znaczenie",
                        "podkreślać wagę",
                        "wyraźnie podkreślać",
                        "wielokrotnie podkreślać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращать особое внимание на что-либо в речи или поведении, выделять как важное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подчёркивать",
                                "target_lang_sense_clarification": "выделять важность"
                            },
                            {
                                "target_lang_word": "акцентировать",
                                "target_lang_sense_clarification": "делать упор"
                            },
                            {
                                "target_lang_word": "выделять",
                                "target_lang_sense_clarification": "обозначать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25afa9be-24a3-3e3e-b194-89141356f17d",
                    "sense_definition": "Rysować linię pod tekstem lub wyrazem, aby go wyróżnić graficznie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił uczniów, aby zaczęli <w>podkreślać</w> czasowniki w zdaniach.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников начать <w>подчёркивать</w> глаголы в предложениях."
                            }
                        },
                        {
                            "text": "Lubię <w>podkreślać</w> na czerwono błędy, które muszę poprawić.",
                            "target_lang_translations": {
                                "ru": "Я люблю <w>подчёркивать</w> красным ошибки, которые мне нужно исправить."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaznaczać"
                    ],
                    "antonyms": [
                        "wykreślać",
                        "zamazywać"
                    ],
                    "common_phrases": [

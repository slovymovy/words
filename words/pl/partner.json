            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d5ba99d-65c6-3075-8f1f-f3c9794e90e6",
                    "sense_definition": "Osoba, z którą pozostaje się w stałym związku uczuciowym, małżeńskim lub seksualnym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relationships",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>partner</w> przygotował dziś pyszną kolację dla całej rodziny.",
                            "target_lang_translations": {
                                "ru": "Мой <w>партнёр</w> приготовил сегодня вкусный ужин для всей семьи."
                            }
                        },
                        {
                            "text": "Szukam <w>partnera</w>, który będzie podzielał moje pasje i zainteresowania.",
                            "target_lang_translations": {
                                "ru": "Я ищу <w>партнёра</w>, который будет разделять мои увлечения и интересы."
                            }
                        }
                    ],
                    "synonyms": [
                        "towarzysz życia",
                        "druga połówka"
                    ],
                    "common_phrases": [
                        "partner życiowy",
                        "stały partner",
                        "potencjalny partner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, с которым состоят в постоянных романтических или семейных отношениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "партнёр",
                                "target_lang_sense_clarification": "в отношениях"
                            },
                            {
                                "target_lang_word": "спутник жизни",
                                "target_lang_sense_clarification": "поэтическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53da2842-6e78-32ca-a0d1-72daa65df36a",
                    "sense_definition": "Współuczestnik w grze, rozmowie, interesach lub innym wspólnym przedsięwzięciu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cooperation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz nowy <w>partner</w> biznesowy pochodzi z Francji i ma duże doświadczenie.",
                            "target_lang_translations": {
                                "ru": "Наш новый бизнес-<w>партнёр</w> родом из Франции и имеет большой опыт."
                            }
                        },
                        {
                            "text": "W tenisie deblowym trzeba doskonale rozumieć swojego <w>partnera</w> na korcie.",
                            "target_lang_translations": {
                                "ru": "В парном теннисе нужно отлично понимать своего <w>партнёра</w> на корте."
                            }
                        }
                    ],
                    "synonyms": [
                        "wspólnik",
                        "współpracownik",
                        "kompan",
                        "współgracz"
                    ],
                    "antonyms": [
                        "rywal",
                        "przeciwnik",
                        "konkurent"
                    ],
                    "common_phrases": [
                        "partner biznesowy",
                        "partner handlowy",
                        "partner do rozmowy",
                        "partner w zbrodni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участник совместной деятельности, игры, бизнеса или разговора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "партнёр",
                                "target_lang_sense_clarification": "в деле, игре"
                            },
                            {
                                "target_lang_word": "компаньон",
                                "target_lang_sense_clarification": "в бизнесе"
                            },
                            {
                                "target_lang_word": "напарник",
                                "target_lang_sense_clarification": "по работе, команде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74b573c5-a2ca-3f8c-856c-a0e206114210",
                    "sense_definition": "Osoba lub podmiot traktowany jako równy sobie, posiadający równorzędny status.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polska jest ważnym i wiarygodnym <w>partnerem</w> w Unii Europejskiej.",
                            "target_lang_translations": {
                                "ru": "Польша является важным и надёжным <w>партнёром</w> в Европейском союзе."
                            }
                        },
                        {
                            "text": "Chcę być traktowany jak <w>partner</w>, a nie jak podwładny wykonujący rozkazy.",
                            "target_lang_translations": {
                                "ru": "Я хочу, чтобы ко мне относились как к <w>партнёру</w>, а не как к подчинённому, выполняющему приказы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "godny partner",
                        "równorzędny partner",
                        "wiarygodny partner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или организация, рассматриваемые как равные, имеющие равный статус."
                    },

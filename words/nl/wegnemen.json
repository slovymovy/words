{
    "word_family": [
        "wegneming"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dca39e6a-75f4-3fc0-ad76-0fdf6d56b1f7",
                    "sense_definition": "Iets pakken en meenemen, of iets van iemand afpakken zodat diegene het niet meer heeft.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze <w>nam</w> zijn speelgoed <w>weg</w> omdat hij stout was.",
                            "target_lang_translations": {
                                "ru": "Она <w>отобрала</w> его игрушки, потому что он плохо себя вел.",
                                "en": "She <w>took</w> his toys <w>away</w> because he was naughty."
                            }
                        },
                        {
                            "text": "Je mag niet zomaar spullen van anderen <w>wegnemen</w> zonder te vragen.",
                            "target_lang_translations": {
                                "ru": "Нельзя просто так <w>забирать</w> чужие вещи без спроса.",
                                "en": "You cannot just <w>take</w> other people's things <w>away</w> without asking."
                            }
                        }
                    ],
                    "synonyms": [
                        "afpakken",
                        "meenemen",
                        "weghalen"
                    ],
                    "antonyms": [
                        "teruggeven",
                        "geven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять что-то и унести, или отобрать у кого-то, лишая его этого.",
                        "en": "To take something and carry it away, or to take something from someone so they no longer have it."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забирать",
                                "target_lang_sense_clarification": "брать с собой или лишать кого-то чего-то"
                            },
                            {
                                "target_lang_word": "отнимать",
                                "target_lang_sense_clarification": "отбирать силой или против воли"
                            },
                            {
                                "target_lang_word": "уносить",
                                "target_lang_sense_clarification": "убирать прочь, перемещать в другое место"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "take away",
                                "target_lang_sense_clarification": "to remove physically or deprive someone of something"
                            },
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "to move something from where it is"
                            },
                            {
                                "target_lang_word": "confiscate",
                                "target_lang_sense_clarification": "to take something away with authority"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e08b86f-0310-3f39-9138-d4ebbcc7cfc4",
                    "sense_definition": "Zorgen dat iets, zoals een probleem, gevoel of obstakel, verdwijnt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een goed gesprek kan veel onzekerheid en twijfel <w>wegnemen</w>.",
                            "target_lang_translations": {
                                "ru": "Хороший разговор может <w>развеять</w> большую неуверенность и сомнения.",
                                "en": "A good conversation can <w>take away</w> a lot of uncertainty and doubt."
                            }
                        },
                        {
                            "text": "De dokter gaf hem medicijnen om de pijn <w>weg</w> te <w>nemen</w>.",
                            "target_lang_translations": {
                                "ru": "Врач дал ему лекарства, чтобы <w>снять</w> боль.",
                                "en": "The doctor gave him medicine to <w>take away</w> the pain."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwijderen",
                        "verhelpen",
                        "elimineren"
                    ],
                    "antonyms": [
                        "veroorzaken"
                    ],
                    "common_phrases": [
                        "twijfel wegnemen",
                        "angst wegnemen",
                        "pijn wegnemen",
                        "zorgen wegnemen",
                        "obstakels wegnemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить что-то исчезнуть, например, проблему, чувство или препятствие.",
                        "en": "To ensure that something, such as a problem, feeling, or obstacle, disappears."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устранять",
                                "target_lang_sense_clarification": "избавляться от проблем или препятствий"
                            },
                            {
                                "target_lang_word": "снимать",
                                "target_lang_sense_clarification": "избавлять от боли, напряжения или симптомов"
                            },
                            {
                                "target_lang_word": "развеивать",
                                "target_lang_sense_clarification": "избавлять от сомнений или страхов"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "to eliminate a problem, obstacle, or cause"
                            },
                            {
                                "target_lang_word": "relieve",
                                "target_lang_sense_clarification": "to alleviate pain, suffering, or distress"
                            },
                            {
                                "target_lang_word": "dispel",
                                "target_lang_sense_clarification": "to make doubts, fears, or feelings disappear"
                            },
                            {
                                "target_lang_word": "take away",
                                "target_lang_sense_clarification": "general term for making a feeling or sensation go away"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
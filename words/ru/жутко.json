                    ],
                    "synonyms": [
                        "страшно"
                    ],
                    "common_phrases": [
                        "мне жутко",
                        "стало жутко",
                        "жутко смотреть"
                    ],
                    "target_lang_definitions": {
                        "en": "Describing a state of fear, horror, or anxiety (used as a predicate)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "creepy",
                                "target_lang_sense_clarification": "causing an unpleasant feeling of fear or unease"
                            },
                            {
                                "target_lang_word": "scary",
                                "target_lang_sense_clarification": "causing fear"
                            },
                            {
                                "target_lang_word": "terrifying",
                                "target_lang_sense_clarification": "causing extreme fear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc0b29de-2e72-3a00-87cb-d2ba2d95346d",
                    "sense_definition": "Таким образом, что становится страшно; пугающе, зловеще.",

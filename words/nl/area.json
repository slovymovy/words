                    "sense_id": "f04203fe-fe23-386c-a504-58b07a8dc27d",
                    "sense_definition": "Een specifiek gebied binnen een orgaan, met name in de hersenen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy_zone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Inmiddels weten we dat Broca's <w>area</w> ook in de hersenen van dieren te vinden is.",
                            "target_lang_translations": {
                                "en": "We now know that Broca's <w>area</w> can also be found in the brains of animals.",
                                "ru": "Теперь мы знаем, что <w>зону</w> Брока можно найти и в мозге животных."
                            }
                        },
                        {
                            "text": "De neuroloog wees de beschadigde <w>area</w> aan op de scan van de patiënt.",
                            "target_lang_translations": {
                                "en": "The neurologist pointed out the damaged <w>area</w> on the patient's scan.",
                                "ru": "Невролог указал на поврежденную <w>область</w> на снимке пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "gebied",
                        "zone",
                        "regio"
                    ],
                    "common_phrases": [
                        "Broca's area",
                        "Wernicke's area"
                    ],
                    "target_lang_definitions": {

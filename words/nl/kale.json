    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9c0abcce-91d4-3d03-b1d8-467a21b32216",
                    "sense_definition": "Verbogen vorm van <w>kaal</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurman is een <kale> man die altijd een hoed draagt.",
                            "target_lang_translations": {
                                "en": "My neighbor is a <bald> man who always wears a hat.",
                                "ru": "Мой сосед — <лысый> мужчина, который всегда носит шляпу."
                            }
                        },
                        {
                            "text": "Van een <kale> kip kun je niet plukken.",
                            "target_lang_translations": {
                                "en": "You cannot pluck a <bald> chicken.",
                                "ru": "С <голой> курицы перьев не нащиплешь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een kale kip",
                        "kale plek",
                        "kale vlakte"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Inflected form of 'kaal' (bald, bare).",
                        "ru": "Склоняемая форма прилагательного 'kaal' (лысый, голый)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bald",
                                "target_lang_sense_clarification": "lacking hair"
                            },
                            {
                                "target_lang_word": "bare",
                                "target_lang_sense_clarification": "uncovered or empty"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лысый",
                                "target_lang_sense_clarification": "не имеющий волос"
                            },
                            {
                                "target_lang_word": "голый",
                                "target_lang_sense_clarification": "непокрытый, пустой (о местности или предметах)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "36fde5d2-abc6-39c3-8303-e0585b964ef0",
                    "sense_definition": "Iemand die een kaal hoofd heeft.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die <kale> daar aan de bar is mijn oom.",
                            "target_lang_translations": {
                                "en": "That <baldy> over there at the bar is my uncle.",
                                "ru": "Тот <лысый> у барной стойки — мой дядя."
                            }
                        },
                        {
                            "text": "De <kale> werd boos toen iemand een grap over zijn haar maakte.",
                            "target_lang_translations": {
                                "en": "The <baldy> got angry when someone made a joke about his hair.",
                                "ru": "<Лысый> разозлился, когда кто-то пошутил по поводу его волос."
                            }
                        }
                    ],
                    "synonyms": [
                        "kaalkop"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person with a bald head.",
                        "ru": "Человек с лысой головой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "baldy",
                                "target_lang_sense_clarification": "informal, often derogatory"
                            },
                            {
                                "target_lang_word": "baldhead",
                                "target_lang_sense_clarification": "informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лысый",
                                "target_lang_sense_clarification": "субстантивированное прилагательное"
                            },
                            {
                                "target_lang_word": "лысак",
                                "target_lang_sense_clarification": "разговорное, иногда пренебрежительное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d55c67bc-6f86-38d5-85d0-356c6ca9edff",
                    "sense_definition": "Aanvoegende wijs van <w>kalen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Men hoopte dat het dier niet verder <kale> door de ziekte.",
                            "target_lang_translations": {
                                "en": "One hoped that the animal would not <lose hair> further due to the disease.",
                                "ru": "Надеялись, что животное не будет дальше <лысеть> из-за болезни."
                            }
                        },
                        {
                            "text": "Moge de oude boom niet volledig <kale> in de winter.",
                            "target_lang_translations": {
                                "en": "May the old tree not completely <become bare> in the winter.",
                                "ru": "Пусть старое дерево не <оголится> полностью зимой."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "subjunctive"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Subjunctive form of 'kalen' (to become bald or bare).",
                        "ru": "Сослагательное наклонение глагола 'kalen' (лысеть, оголяться)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "become bald",
                                "target_lang_sense_clarification": "to lose hair"
                            },
                            {
                                "target_lang_word": "become bare",
                                "target_lang_sense_clarification": "to lose leaves or covering"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лысеть",
                                "target_lang_sense_clarification": "терять волосы"
                            },
                            {
                                "target_lang_word": "оголяться",
                                "target_lang_sense_clarification": "терять листву или покрытие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5bf6d61a-de5a-3fa3-8cf3-3a9da3b92a37",
                    "sense_definition": "Zachowywać się niegrzecznie, swawolić lub prowadzić hulaszczy tryb życia (używane z zaimkiem zwrotnym „się”)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wakacji dzieci zaczęły się <bisurmanić> i zupełnie nie słuchały rodziców.",
                            "target_lang_translations": {
                                "ru": "Во время каникул дети начали <распускаться> и совсем не слушались родителей."
                            }
                        },
                        {
                            "text": "Młodzi szlachcice lubili się <bisurmanić> w karczmach aż do białego rana.",
                            "target_lang_translations": {
                                "ru": "Молодые шляхтичи любили <кутить> в корчмах до самого утра."
                            }
                        }
                    ],
                    "synonyms": [
                        "turczyć się",
                        "turczyć",
                        "turczyć się"
                    ],
                    "antonyms": [
                        "statkować się",
                        "grzecznieć"
                    ],
                    "common_phrases": [
                        "bisurmanić się za młodu",
                        "strasznie się bisurmanić"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести себя невоспитанно, шалить или вести разгульный образ жизни"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распускаться",
                                "target_lang_sense_clarification": "становиться недисциплинированным, терять сдержанность"
                            },
                            {
                                "target_lang_word": "безобразничать",
                                "target_lang_sense_clarification": "вести себя возмутительно, нарушать порядок"
                            },
                            {
                                "target_lang_word": "кутить",
                                "target_lang_sense_clarification": "вести разгульную жизнь, пьянствовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd9a5629-170e-3572-baba-5a2370531564",
                    "sense_definition": "Wpływać na kogoś demoralizująco, psuć czyjeś obyczaje lub charakter",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złe towarzystwo potrafi szybko <bisurmanić> młodego i niedoświadczonego człowieka.",
                            "target_lang_translations": {
                                "ru": "Дурная компания может быстро <испортить> молодого и неопытного человека."
                            }
                        },
                        {
                            "text": "Nie pozwól, by łatwe pieniądze zaczęły cię <bisurmanić>.",
                            "target_lang_translations": {
                                "ru": "Не позволь легким деньгам начать тебя <развращать>."
                            }
                        }
                    ],
                    "synonyms": [
                        "turczyć",
                        "turczyć się"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

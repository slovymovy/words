    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5aedf0f3-0509-31c1-85ff-2018f4f98428",
                    "sense_definition": "Uczta skoncentrowana wokół picia alkoholu w dużych ilościach.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiedzi wezwali policję, ponieważ w mieszkaniu obok trwała głośna <w>libacja</w>.",
                            "target_lang_translations": {
                                "ru": "Соседи вызвали полицию, так как в квартире рядом шла шумная <w>пьянка</w>."
                            }
                        },
                        {
                            "text": "Ta całonocna <w>libacja</w> alkoholowa skończyła się dla niego potwornym bólem głowy.",
                            "target_lang_translations": {
                                "ru": "Эта ночная <w>попойка</w> закончилась для него ужасной головной болью."
                            }
                        }
                    ],
                    "synonyms": [
                        "pijatyka",
                        "popijawa",
                        "balanga",
                        "melanż"
                    ],
                    "common_phrases": [
                        "głośna libacja",
                        "libacja alkoholowa",
                        "urządzić libację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Застолье или вечеринка, сопровождающаяся употреблением большого количества алкоголя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пьянка",
                                "target_lang_sense_clarification": "разговорное, шумное застолье"
                            },
                            {
                                "target_lang_word": "попойка",
                                "target_lang_sense_clarification": "разговорное, обильное употребление спиртного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3773c005-a8cf-3093-ae94-45ec166d55d5",
                    "sense_definition": "Zwyczaj wylewania alkoholu, oliwy lub innych cennych płynów na ofiarę bogu.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapłan dokonał uroczystej <w>libacji</w>, wylewając wino na ołtarz.",
                            "target_lang_translations": {
                                "ru": "Жрец совершил торжественное <w>возлияние</w>, вылив вино на алтарь."
                            }
                        },
                        {
                            "text": "W starożytnej Grecji <w>libacja</w> była ważnym elementem domowych obrzędów religijnych.",
                            "target_lang_translations": {
                                "ru": "В Древней Греции <w>возлияние</w> было важным элементом домашних религиозных обрядов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dokonać libacji",
                        "ofiarna libacja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обряд жертвоприношения божеству путем выливания вина, масла или других жидкостей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возлияние",
                                "target_lang_sense_clarification": "религиозный ритуал"
                            }
                        ]
                    }
                }
            ]
        }
    ]

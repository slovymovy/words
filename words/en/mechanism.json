            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad2ac18c-302a-3e55-a604-3dab88d40e31",
                    "sense_definition": "A mechanical part or system of parts that work together to perform a function.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The locking <w>mechanism</w> on the front door is jammed and needs repair.",
                            "target_lang_translations": {
                                "ru": "Запорный <w>механизм</w> входной двери заклинило, и он требует ремонта."
                            }
                        },
                        {
                            "text": "Engineers studied the internal <w>mechanism</w> of the clock to understand how it kept time.",
                            "target_lang_translations": {
                                "ru": "Инженеры изучали внутренний <w>механизм</w> часов, чтобы понять, как они отсчитывают время."
                            }
                        }
                    ],
                    "synonyms": [
                        "device",
                        "apparatus",
                        "gear",
                        "machinery"
                    ],
                    "common_phrases": [
                        "locking mechanism",
                        "internal mechanism",
                        "safety mechanism",
                        "firing mechanism"
                    ],
                    "target_lang_definitions": {
                        "ru": "Механическая деталь или система деталей, работающих вместе для выполнения определенной функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "механизм",
                                "target_lang_sense_clarification": "устройство, прибор"
                            },
                            {
                                "target_lang_word": "устройство",
                                "target_lang_sense_clarification": "приспособление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acd42dd3-3a33-3c57-90cd-5a4b6f8e9a7d",
                    "sense_definition": "A method, process, or system for achieving a result or handling a situation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Humor is often used as a coping <w>mechanism</w> during difficult times.",
                            "target_lang_translations": {
                                "ru": "Юмор часто используется как защитный <w>механизм</w> в трудные времена."
                            }
                        },
                        {
                            "text": "The company established a new <w>mechanism</w> for resolving customer complaints efficiently.",
                            "target_lang_translations": {
                                "ru": "Компания создала новый <w>механизм</w> для эффективного разрешения жалоб клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "process",
                        "procedure",
                        "system",
                        "method",
                        "technique"
                    ],
                    "common_phrases": [
                        "coping mechanism",
                        "defense mechanism",
                        "support mechanism",
                        "market mechanism"
                    ],
                    "target_lang_definitions": {
                        "ru": "Метод, процесс или система для достижения результата или разрешения ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "механизм",
                                "target_lang_sense_clarification": "система, способ действия"
                            },
                            {
                                "target_lang_word": "метод",
                                "target_lang_sense_clarification": "способ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97ee36a4-847c-38ce-b718-2193353105d1",
                    "sense_definition": "The philosophical theory that all natural phenomena can be explained by physical causes.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The debate between vitalism and <w>mechanism</w> was central to the history of biology.",
                            "target_lang_translations": {
                                "ru": "Спор между витализмом и <w>механицизмом</w> занимал центральное место в истории биологии."
                            }
                        },
                        {
                            "text": "Strict <w>mechanism</w> suggests that the universe operates like a complex machine without spiritual intervention.",
                            "target_lang_translations": {
                                "ru": "Строгий <w>механицизм</w> предполагает, что вселенная функционирует как сложная машина без духовного вмешательства."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "philosophy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Философская теория, объясняющая все природные явления физическими причинами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "механицизм",
                                "target_lang_sense_clarification": "философское направление"
                            }
                        ]
                    }
                }
            ]
        }
    ]

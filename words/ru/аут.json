    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2fb29b27-8995-31ff-af99-e53341c29ea7",
                    "sense_definition": "Пространство за чертой, ограничивающей игровое поле, а также положение мяча или шайбы в этом пространстве.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После сильного удара соперника мяч улетел в <w>аут</w>.",
                            "target_lang_translations": {
                                "en": "After the opponent's strong kick, the ball flew <w>out of bounds</w>."
                            }
                        },
                        {
                            "text": "Судья остановил игру и назначил вбрасывание из <w>аута</w>.",
                            "target_lang_translations": {
                                "en": "The referee stopped the game and awarded a throw-in from <w>out of bounds</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мяч в ауте",
                        "уйти в аут",
                        "вбрасывание из аута"
                    ],
                    "target_lang_definitions": {
                        "en": "The area outside the boundary lines of a playing field, or the state of the ball being in that area."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "out of bounds",
                                "target_lang_sense_clarification": "sports area outside the field"
                            },
                            {
                                "target_lang_word": "out",
                                "target_lang_sense_clarification": "sports state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc93c5c5-d625-3691-a26d-5122958e03b5",
                    "sense_definition": "Психологическое состояние отрешённости, сильной усталости, шока или потери сознания.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После сдачи сложного экзамена студент был в полном <w>ауте</w>.",
                            "target_lang_translations": {
                                "en": "After passing the difficult exam, the student was completely <w>out of it</w>."
                            }
                        },
                        {
                            "text": "От невероятной усталости он просто ушёл в <w>аут</w> и проспал весь день.",
                            "target_lang_translations": {
                                "en": "From incredible fatigue, he just passed <w>out</w> and slept all day."
                            }
                        }
                    ],
                    "synonyms": [
                        "отрешённость",
                        "отключка",
                        "фрустрация"
                    ],
                    "common_phrases": [
                        "быть в ауте",
                        "полный аут",
                        "уйти в аут"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A state of extreme fatigue, shock, detachment, or unconsciousness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "out of it",
                                "target_lang_sense_clarification": "mental state of detachment or fatigue"
                            },
                            {
                                "target_lang_word": "knocked out",
                                "target_lang_sense_clarification": "unconscious or extremely tired"
                            },
                            {
                                "target_lang_word": "in a daze",
                                "target_lang_sense_clarification": "shocked or confused state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ff5eb01-f9fb-3f84-a01c-559b8776f201",
                    "sense_definition": "Карта в покере, которая может улучшить комбинацию игрока до выигрышной.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытный игрок всегда считает свои <w>ауты</w> перед принятием решения.",
                            "target_lang_translations": {
                                "en": "An experienced player always counts their <w>outs</w> before making a decision."
                            }
                        },
                        {
                            "text": "Ему нужен был всего один <w>аут</w>, чтобы собрать самую сильную комбинацию.",
                            "target_lang_translations": {
                                "en": "He needed just one <w>out</w> to complete the strongest hand."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A card in poker that can improve a player's hand to a winning one."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "out",
                                "target_lang_sense_clarification": "poker term"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "62dabe00-c5a9-3ed4-adf1-69c1be22c7ea",
                    "sense_definition": "Сделанный из камня, состоящий из камня или кирпича",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "literal_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили построить большой <w>каменный</w> дом для всей нашей семьи.",
                            "target_lang_translations": {
                                "en": "We decided to build a large <w>stone</w> house for our whole family."
                            }
                        },
                        {
                            "text": "Вокруг старого замка была возведена высокая <w>каменная</w> стена.",
                            "target_lang_translations": {
                                "en": "A high <w>stone</w> wall was erected around the old castle."
                            }
                        }
                    ],
                    "antonyms": [
                        "деревянный"
                    ],
                    "common_phrases": [
                        "каменный дом",
                        "каменная стена",
                        "каменный пол",
                        "каменный век",
                        "каменный уголь"
                    ],
                    "target_lang_definitions": {
                        "en": "Made of stone; consisting of stone or brick"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stone",
                                "target_lang_sense_clarification": "Used attributively to describe material (e.g., stone wall, stone house)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a04fccd4-1586-34f4-91e4-0c83b95f65a4",
                    "sense_definition": "Неподвижный, застывший, ничего не выражающий (обычно о лице или взгляде)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он слушал плохие новости с абсолютно <w>каменным</w> лицом, не показывая эмоций.",
                            "target_lang_translations": {
                                "en": "He listened to the bad news with an absolutely <w>stone</w> face, showing no emotion."
                            }
                        },
                        {
                            "text": "Её взгляд стал <w>каменным</w>, когда она узнала горькую правду.",
                            "target_lang_translations": {
                                "en": "Her gaze became <w>stony</w> when she found out the bitter truth."
                            }
                        }
                    ],
                    "synonyms": [
                        "неподвижный",
                        "застывший",
                        "безжизненный"
                    ],
                    "antonyms": [
                        "живой",
                        "выразительный"
                    ],
                    "common_phrases": [
                        "каменное лицо",
                        "каменное выражение",
                        "с каменным видом"
                    ],
                    "target_lang_definitions": {
                        "en": "Motionless, frozen, or expressionless (usually referring to a face or gaze)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stony",
                                "target_lang_sense_clarification": "Lacking warmth or emotion (e.g., stony stare, stony silence)"
                            },
                            {
                                "target_lang_word": "stone",
                                "target_lang_sense_clarification": "Used in the idiom 'stone face' (expressionless)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a825ac22-5b03-30ad-bb78-e28efba36ea1",
                    "sense_definition": "Равнодушный, безжалостный, не способный к состраданию",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этого человека <w>каменное</w> сердце, он никогда никому не помогает.",
                            "target_lang_translations": {
                                "en": "This man has a <w>stone</w> heart; he never helps anyone."
                            }
                        },
                        {
                            "text": "Не будь таким <w>каменным</w>, прояви хоть немного сочувствия к людям.",
                            "target_lang_translations": {
                                "en": "Don't be so <w>hard-hearted</w>, show at least a little sympathy for people."
                            }
                        }
                    ],
                    "synonyms": [
                        "жестокий",
                        "безжа́лостный",
                        "бесчу́вственный",
                        "равнодушный"
                    ],
                    "antonyms": [
                        "чуткий",
                        "мягкий",
                        "отзывчивый"
                    ],
                    "common_phrases": [
                        "каменное сердце",
                        "каменная душа"
                    ],
                    "target_lang_definitions": {
                        "en": "Indifferent, ruthless, or incapable of compassion"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hard",
                                "target_lang_sense_clarification": "Severe, harsh, or lacking compassion (e.g., hard heart)"
                            },
                            {
                                "target_lang_word": "stony",
                                "target_lang_sense_clarification": "Unfeeling or cruel (e.g., stony heart)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a19d830-c761-35eb-9c17-4dd51ab57f00",
                    "sense_definition": "Твёрдый, непоколебимый, очень прочный (обычно о характере или принципах)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_firmness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <w>каменное</w> спокойствие в такой сложной ситуации удивляло всех окружающих.",
                            "target_lang_translations": {
                                "en": "His <w>stony</w> calm in such a difficult situation surprised everyone around."
                            }
                        },
                        {
                            "text": "Благодаря своей <w>каменной</w> воле он смог преодолеть все жизненные трудности.",
                            "target_lang_translations": {
                                "en": "Thanks to his <w>iron</w> will, he was able to overcome all life's hardships."
                            }
                        }
                    ],
                    "synonyms": [
                        "непоколебимый",
                        "стойкий",
                        "твёрдый"
                    ],
                    "antonyms": [
                        "слабый",
                        "неустойчивый"
                    ],
                    "common_phrases": [
                        "каменная воля",
                        "каменное спокойствие"
                    ],
                    "target_lang_definitions": {
                        "en": "Firm, unshakable, or very durable (usually referring to character or principles)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "immovable",
                                "target_lang_sense_clarification": "Steadfast and unyielding in purpose"
                            },
                            {
                                "target_lang_word": "iron",
                                "target_lang_sense_clarification": "Idiomatic translation for 'will' (iron will)"
                            },
                            {
                                "target_lang_word": "stony",
                                "target_lang_sense_clarification": "Unshakable (e.g., stony calm)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "732cf5a4-42c7-3e4c-8fcc-b642eb3a75b9",
                    "sense_definition": "The action of protecting someone or something from attack, danger, or criticism.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He carried a heavy stick for <w>defense</w> while walking home at night.",
                            "target_lang_translations": {
                                "ru": "Он носил с собой тяжелую палку для <w>защиты</w>, когда возвращался домой ночью."
                            }
                        },
                        {
                            "text": "She was quick to come to the <w>defense</w> of her friend during the argument.",
                            "target_lang_translations": {
                                "ru": "Она быстро встала на <w>защиту</w> своей подруги во время спора."
                            }
                        }
                    ],
                    "synonyms": [
                        "protection",
                        "safeguard",
                        "shield",
                        "preservation"
                    ],
                    "antonyms": [
                        "attack"
                    ],
                    "common_phrases": [
                        "self-defense",
                        "in defense of",
                        "come to someone's defense",
                        "line of defense"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "American English spelling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по защите кого-либо или чего-либо от нападения, опасности или критики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защита",
                                "target_lang_sense_clarification": "общее понятие: ограждение от опасности"
                            },
                            {
                                "target_lang_word": "оборона",
                                "target_lang_sense_clarification": "активное сопротивление нападению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7da5215-0ba3-3fa2-b29a-55682eef7e19",
                    "sense_definition": "The military resources, systems, and organizations used by a country to protect itself.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government decided to increase the budget for national <w>defense</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство решило увеличить бюджет на национальную <w>оборону</w>."
                            }
                        },
                        {
                            "text": "They are testing a new air <w>defense</w> system to intercept missiles.",
                            "target_lang_translations": {
                                "ru": "Они испытывают новую систему противовоздушной <w>обороны</w> для перехвата ракет."
                            }
                        }
                    ],
                    "common_phrases": [

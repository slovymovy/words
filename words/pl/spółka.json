                            {
                                "target_lang_word": "совместное дело",
                                "target_lang_sense_clarification": "общие действия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "362fa519-9784-34a2-9d2d-74b2e36b5d21",
                    "sense_definition": "Tradycyjna potrawa regionalna przygotowywana z gotowanego grochu i kaszy lub pęczaku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na obiad babcia podała nam pyszną <w>spółkę</w> z dużą ilością majeranku.",

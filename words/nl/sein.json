    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b5aa9ac9-e514-3ca8-9fa8-06b038145270",
                    "sense_definition": "Een aanduiding, bericht of signaal dat aangeeft dat een actie mag beginnen of moet gebeuren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Eindelijk krijgt hij het <w>sein</w> om te beginnen met de werkzaamheden.",
                            "target_lang_translations": {
                                "en": "Finally he gets the <w>signal</w> to start the work.",
                                "ru": "Наконец он получает <w>сигнал</w> начать работу."
                            }
                        },
                        {
                            "text": "De scheidsrechter gaf het <w>sein</w> voor de start van de wedstrijd.",
                            "target_lang_translations": {
                                "en": "The referee gave the <w>signal</w> for the start of the match.",
                                "ru": "Судья дал <w>сигнал</w> к началу матча."
                            }
                        }
                    ],
                    "synonyms": [
                        "signaal",
                        "teken"
                    ],
                    "common_phrases": [
                        "het sein geven",
                        "wachten op het sein",
                        "op groen staan"
                    ],
                    "target_lang_definitions": {
                        "en": "An indication, message, or signal indicating that an action may start or must occur.",
                        "ru": "Указание, сообщение или сигнал, означающий, что действие может начаться или должно произойти."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "signal",
                                "target_lang_sense_clarification": "indication to start"
                            },
                            {
                                "target_lang_word": "cue",
                                "target_lang_sense_clarification": "prompt for action"
                            },
                            {
                                "target_lang_word": "sign",
                                "target_lang_sense_clarification": "indication"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сигнал",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "отмашка",
                                "target_lang_sense_clarification": "разрешение начать"
                            },
                            {
                                "target_lang_word": "знак",
                                "target_lang_sense_clarification": "условное обозначение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae190afd-fdf1-3059-a6c9-53d9167c6794",
                    "sense_definition": "Een installatie met lichten of borden langs een spoorweg die instructies geeft aan treinbestuurders.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het driehoekige groene <w>sein</w> gaf een maximumsnelheid voor het baanvlak aan.",
                            "target_lang_translations": {
                                "en": "The triangular green <w>signal</w> indicated a maximum speed for the track section.",
                                "ru": "Треугольный зеленый <w>сигнал</w> указывал максимальную скорость для участка пути."
                            }
                        },
                        {
                            "text": "De machinist moest direct remmen omdat het <w>sein</w> plotseling op rood sprong.",
                            "target_lang_translations": {
                                "en": "The train driver had to brake immediately because the <w>signal</w> suddenly turned red.",
                                "ru": "Машинист должен был немедленно затормозить, так как <w>сигнал</w> внезапно сменился на красный."
                            }
                        }
                    ],
                    "synonyms": [
                        "signaal"
                    ],
                    "common_phrases": [
                        "sein op rood",
                        "sein op groen",
                        "onveilig sein"
                    ],
                    "target_lang_definitions": {
                        "en": "An installation with lights or signs along a railway that gives instructions to train drivers.",
                        "ru": "Устройство с огнями или знаками вдоль железной дороги, дающее инструкции машинистам."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "signal",

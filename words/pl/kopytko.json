    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "99815322-507d-363f-a8a0-1e733ee7afd9",
                    "sense_definition": "pojedyncza kluska z ciasta ziemniaczanego, zazwyczaj w kształcie rombu, gotowana w wodzie",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia źle się czuje i zjadła na obiad dosłownie tylko jedno <w>kopytko</w>.",
                            "target_lang_translations": {
                                "ru": "Бабушка плохо себя чувствует и съела на обед буквально только одну <w>клецку</w>."
                            }
                        },
                        {
                            "text": "To ostatnie <w>kopytko</w> na talerzu wygląda bardzo apetycznie.",
                            "target_lang_translations": {
                                "ru": "Эта последняя <w>клецка</w> на тарелке выглядит очень аппетитно."
                            }
                        }
                    ],
                    "synonyms": [
                        "kluska"
                    ],
                    "common_phrases": [
                        "kopytka z sosem",
                        "kopytka z bułką",
                        "domowe kopytka"
                    ],
                    "target_lang_definitions": {
                        "ru": "одна клецка из картофельного теста, обычно в форме ромба, сваренная в воде"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клецка",
                                "target_lang_sense_clarification": "кулинария"
                            },
                            {
                                "target_lang_word": "копытка",
                                "target_lang_sense_clarification": "польское блюдо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9692b42-3c26-35d8-8a5b-701e174d05f4",
                    "sense_definition": "zdrobnienie od: <w>kopyto</w>; mała rogowa osłona palców u niektórych ssaków, zwłaszcza młodych lub małych zwierząt",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mała kózka zraniła się w przednie <w>kopytko</w> podczas biegania po kamieniach.",
                            "target_lang_translations": {
                                "ru": "Маленькая козочка поранила переднее <w>копытце</w>, бегая по камням."
                            }
                        },
                        {
                            "text": "Źrebak niepewnie stawia swoje chwiejne <w>kopytka</w> na trawie.",
                            "target_lang_translations": {
                                "ru": "Жеребенок неуверенно ставит свои шаткие <w>копытца</w> на траву."
                            }
                        }
                    ],
                    "synonyms": [
                        "raciczka"
                    ],
                    "antonyms": [
                        "kopyto"
                    ],
                    "common_phrases": [
                        "końskie kopytko",
                        "małe kopytko"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "уменьшительная форма от: <w>копыто</w>; маленькое роговое покрытие пальцев у некоторых млекопитающих"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копытце",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdfa5a80-cbf7-33cc-b180-d0f5066ed2ac",
                    "sense_definition": "skarpetka wykonana z grubej wełny, chroniąca stopę przed zimnem",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W górach zimą najlepiej nosić ciepłe wełniane <w>kopytka</w>.",
                            "target_lang_translations": {
                                "ru": "В горах зимой лучше всего носить теплые шерстяные <w>носки</w>."
                            }
                        },
                        {
                            "text": "Babcia zrobiła mi na drutach grube <w>kopytka</w> do chodzenia po domu.",
                            "target_lang_translations": {
                                "ru": "Бабушка связала мне толстые <w>носки</w> для хождения по дому."
                            }
                        }
                    ],
                    "synonyms": [
                        "skarpetka"
                    ],
                    "common_phrases": [
                        "wełniane kopytka",
                        "ciepłe kopytka"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "często używane w gwarach góralskich"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "носок из толстой шерсти, защищающий ногу от холода"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шерстяной носок",
                                "target_lang_sense_clarification": "одежда"

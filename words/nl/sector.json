    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b60a2fcc-6dc7-3d04-b7fb-5b757febb648",
                    "sense_definition": "Een specifiek deel van de maatschappij of economie, zoals een bepaalde bedrijfstak of vakgebied.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "economy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel mensen werken tegenwoordig in de dienstverlenende <w>sector</w>.",
                            "target_lang_translations": {
                                "ru": "Многие люди сегодня работают в <w>сфере</w> услуг."
                            }
                        },
                        {
                            "text": "De overheid wil meer investeren in de publieke <w>sector</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство хочет больше инвестировать в государственный <w>сектор</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "branche",
                        "bedrijfstak",
                        "vakgebied"
                    ],
                    "common_phrases": [
                        "publieke sector",
                        "agrarische sector",
                        "financiële sector",
                        "tertiaire sector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическая часть общества или экономики, например, определенная отрасль или сфера деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сектор",
                                "target_lang_sense_clarification": "часть экономики"
                            },
                            {
                                "target_lang_word": "отрасль",
                                "target_lang_sense_clarification": "сфера деятельности"
                            },
                            {
                                "target_lang_word": "сфера",
                                "target_lang_sense_clarification": "область"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9d806bc-5d64-3120-96cc-1f7550595fb3",
                    "sense_definition": "Een deel van een cirkeloppervlak in de vorm van een taartpunt, of een specifiek afgebakend gebied.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geometry_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de wiskundeles moesten we de oppervlakte van de <w>sector</w> berekenen.",
                            "target_lang_translations": {
                                "ru": "На уроке математики нам нужно было вычислить площадь <w>сектора</w>."
                            }
                        },
                        {
                            "text": "De stad was na de oorlog verdeeld in verschillende militaire <w>sectoren</w>.",
                            "target_lang_translations": {
                                "ru": "После войны город был разделен на различные военные <w>сектора</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "taartpunt",
                        "cirkelsector"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть круга, ограниченная дугой и двумя радиусами, или определенная территория."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сектор",
                                "target_lang_sense_clarification": "геометрия, зона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2dddc723-cc89-3af6-8de7-cd68177897e1",
                    "sense_definition": "De kleinste eenheid op een opslagmedium, zoals een harde schijf, die in één keer kan worden gelezen of geschreven.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als een harde schijf een beschadigde <w>sector</w> heeft, kunnen er gegevens verloren gaan.",
                            "target_lang_translations": {
                                "ru": "Если на жестком диске есть поврежденный <w>сектор</w>, данные могут быть потеряны."
                            }
                        },
                        {
                            "text": "Het programma controleert de schijf op elke slechte <w>sector</w>.",
                            "target_lang_translations": {
                                "ru": "Программа проверяет диск на наличие сбойных <w>секторов</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "slechte sector"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Минимальная единица хранения данных на носителе информации, например, на жестком диске."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сектор",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
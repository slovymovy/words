    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1612cbec-c9ce-3e17-8476-437d17d40952",
                    "sense_definition": "Газовая оболочка, окружающая планету или другое небесное тело.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У Земли есть плотная <w>атмосфера</w>, которая защищает всё живое.",
                            "target_lang_translations": {
                                "en": "Earth has a dense <w>atmosphere</w> that protects all living things."
                            }
                        },
                        {
                            "text": "Космический корабль вошёл в верхние слои <w>атмосферы</w>.",
                            "target_lang_translations": {
                                "en": "The spaceship entered the upper layers of the <w>atmosphere</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "воздух"
                    ],
                    "antonyms": [
                        "вакуум"
                    ],
                    "common_phrases": [
                        "атмосфера Земли",
                        "загрязнение атмосферы",
                        "слои атмосферы"
                    ],
                    "target_lang_definitions": {
                        "en": "The gaseous envelope surrounding a planet or other celestial body."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "atmosphere",
                                "target_lang_sense_clarification": "planetary/physical"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7be8a3ec-6c7d-363d-9e27-f3db5858007e",
                    "sense_definition": "Окружающая обстановка, настроение или дух в каком-либо месте или коллективе.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем коллективе сложилась здоровая и дружеская <w>атмосфера</w>.",
                            "target_lang_translations": {
                                "en": "A healthy and friendly <w>atmosphere</w> has developed in our team."
                            }
                        },
                        {
                            "text": "Тихая музыка и свечи создавали уютную <w>атмосферу</w> праздника.",
                            "target_lang_translations": {
                                "en": "Quiet music and candles created a cozy holiday <w>atmosphere</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "дух",
                        "среда"
                    ],
                    "common_phrases": [
                        "дружеская атмосфера",
                        "рабочая атмосфера",
                        "разрядить атмосферу",
                        "домашняя атмосфера"
                    ],
                    "target_lang_definitions": {
                        "en": "The surrounding environment, mood, or spirit in a particular place or group."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "atmosphere",
                                "target_lang_sense_clarification": "mood or spirit"
                            },
                            {
                                "target_lang_word": "environment",
                                "target_lang_sense_clarification": "surroundings or conditions"
                            },
                            {
                                "target_lang_word": "ambiance",
                                "target_lang_sense_clarification": "mood of a place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c9c727a-9014-36c4-82fc-ca62ef5879b3",
                    "sense_definition": "Внесистемная единица измерения давления, приблизительно равная давлению воздуха на уровне моря.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В испытательной камере давление поднялось до десяти <w>атмосфер</w>.",
                            "target_lang_translations": {
                                "en": "In the test chamber, the pressure rose to ten <w>atmospheres</w>."
                            }
                        },
                        {
                            "text": "Для этих шин рекомендуется давление в две <w>атмосферы</w>.",
                            "target_lang_translations": {
                                "en": "A pressure of two <w>atmospheres</w> is recommended for these tires."
                            }
                        }
                    ],
                    "common_phrases": [
                        "давление в одну атмосферу",
                        "техническая атмосфера"
                    ],
                    "target_lang_definitions": {
                        "en": "A non-SI unit of pressure measurement, approximately equal to air pressure at sea level."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "atmosphere",
                                "target_lang_sense_clarification": "unit of pressure"
                            }
                        ]
                    }
                }
            ]
        }
    ]

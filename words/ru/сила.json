    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "79136d0a-caa0-3c05-8646-bc48b20e3520",
                    "sense_definition": "Физическая способность живого существа действовать, производить движения, поднимать тяжести.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_strength",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У спортсмена должна быть большая физическая <w>сила</w>.",
                            "target_lang_translations": {
                                "en": "An athlete must have great physical <w>strength</w>."
                            }
                        },
                        {
                            "text": "После долгой болезни к нему медленно возвращались <w>силы</w>.",
                            "target_lang_translations": {
                                "en": "After a long illness, his <w>strength</w> slowly returned to him."
                            }
                        }
                    ],
                    "synonyms": [
                        "мощь"
                    ],
                    "antonyms": [
                        "слабость",
                        "немощность"
                    ],
                    "common_phrases": [
                        "изо всех сил",
                        "нет сил",
                        "набраться сил",
                        "своими силами",
                        "не по силам"
                    ],
                    "target_lang_definitions": {
                        "en": "Physical power or capacity of a living being to act, move, or lift things."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strength",
                                "target_lang_sense_clarification": "physical capacity"
                            },
                            {
                                "target_lang_word": "might",
                                "target_lang_sense_clarification": "great physical strength"
                            },
                            {
                                "target_lang_word": "power",
                                "target_lang_sense_clarification": "physical capability"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3937b751-d3eb-3257-aba0-b53d1e6e6905",
                    "sense_definition": "Внутренняя энергия, душевная стойкость, способность к умственному или духовному напряжению.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_strength",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её <w>сила</w> духа помогла пережить трудные времена.",
                            "target_lang_translations": {
                                "en": "Her <w>strength</w> of spirit helped her survive difficult times."
                            }
                        },
                        {
                            "text": "Я не в <w>силах</w> отказаться от этого заманчивого предложения.",
                            "target_lang_translations": {
                                "en": "I do not have the <w>strength</w> to refuse this tempting offer."
                            }
                        }
                    ],
                    "synonyms": [
                        "энергия"
                    ],
                    "antonyms": [
                        "слабость"
                    ],
                    "common_phrases": [
                        "сила воли",
                        "сила духа",
                        "быть в силах",
                        "не в силах"
                    ],
                    "target_lang_definitions": {
                        "en": "Inner mental or spiritual energy, resilience, and willpower."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strength",
                                "target_lang_sense_clarification": "mental or spiritual fortitude"
                            },
                            {
                                "target_lang_word": "willpower",
                                "target_lang_sense_clarification": "strength of will"
                            },
                            {
                                "target_lang_word": "power",
                                "target_lang_sense_clarification": "capacity to do something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eace4357-1c48-3a8d-af5f-5dd177a087ad",
                    "sense_definition": "Мера механического воздействия на тело со стороны других тел (физический термин).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Сила</w> земного притяжения удерживает нас на поверхности планеты.",
                            "target_lang_translations": {
                                "en": "The <w>force</w> of gravity keeps us on the planet's surface."
                            }
                        },
                        {
                            "text": "Для движения автомобиля нужна <w>сила</w> трения колес о дорогу.",
                            "target_lang_translations": {
                                "en": "For the car to move, the friction <w>force</w> of the wheels against the road is needed."
                            }
                        }
                    ],
                    "synonyms": [
                        "тяга"
                    ],
                    "common_phrases": [
                        "сила тяжести",
                        "сила трения",
                        "лошадиная сила",
                        "центробежная сила"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A physical quantity representing the mechanical interaction between bodies."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "force",
                                "target_lang_sense_clarification": "physics term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89844cab-6440-3abc-9267-e3a0deb243a7",
                    "sense_definition": "Могущество, власть, авторитет или значительное влияние.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "power_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Знание — это великая <w>сила</w>, открывающая любые двери.",
                            "target_lang_translations": {
                                "en": "Knowledge is a great <w>power</w> that opens any doors."
                            }
                        },
                        {
                            "text": "Вся <w>сила</w> государства опирается на доверие граждан.",
                            "target_lang_translations": {
                                "en": "The entire <w>power</w> of the state relies on the trust of the citizens."
                            }
                        }
                    ],
                    "synonyms": [
                        "мощь",
                        "влиятельность"
                    ],
                    "antonyms": [
                        "слабость"
                    ],
                    "common_phrases": [
                        "великая сила",
                        "иметь силу"
                    ],
                    "target_lang_definitions": {
                        "en": "Political or social authority, control, or significant influence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "power",
                                "target_lang_sense_clarification": "authority or influence"
                            },
                            {
                                "target_lang_word": "might",
                                "target_lang_sense_clarification": "imposing power"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40900a9c-59d1-349b-bac6-9c5c3db2ad6e",
                    "sense_definition": "Применение физического принуждения, насилие.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Они решили действовать грубой <w>силой</w>, а не дипломатией.",
                            "target_lang_translations": {
                                "en": "They decided to act with brute <w>force</w>, not diplomacy."
                            }
                        },
                        {
                            "text": "Захватчики взяли город <w>силой</w> после долгой осады.",
                            "target_lang_translations": {
                                "en": "The invaders took the city by <w>force</w> after a long siege."
                            }
                        }
                    ],
                    "synonyms": [
                        "принуждение"
                    ],
                    "antonyms": [
                        "добровольность",
                        "полюбовность"
                    ],
                    "common_phrases": [
                        "взять силой",
                        "с позиции силы",
                        "грубая сила"
                    ],
                    "target_lang_definitions": {
                        "en": "The use of physical coercion or violence to compel someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "force",
                                "target_lang_sense_clarification": "coercion or violence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82097764-4f0f-3a4c-b7f7-3f4c143a2359",
                    "sense_definition": "Группа людей, общественная организация или фактор, оказывающие активное влияние на ситуацию.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_factor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Оппозиция стала серьезной политической <w>силой</w> в парламенте.",
                            "target_lang_translations": {
                                "en": "The opposition has become a serious political <w>force</w> in parliament."
                            }
                        },
                        {
                            "text": "Мы верим, что высшие <w>силы</w> оберегают наш дом.",
                            "target_lang_translations": {
                                "en": "We believe that higher <w>powers</w> protect our home."
                            }
                        }
                    ],
                    "synonyms": [
                        "фактор"
                    ],
                    "common_phrases": [
                        "политическая сила",
                        "движущая сила",
                        "высшие силы",
                        "третья сила"
                    ],
                    "target_lang_definitions": {
                        "en": "An organized group, organization, or factor influencing a situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "force",
                                "target_lang_sense_clarification": "political or social group"
                            },
                            {
                                "target_lang_word": "power",
                                "target_lang_sense_clarification": "influential entity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f58a9e81-40af-3bc2-b88e-3b7a59ccdb56",
                    "sense_definition": "Законность, обязательность исполнения документа или решения (юридический термин).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый закон вступает в <w>силу</w> с первого января следующего года.",
                            "target_lang_translations": {
                                "en": "The new law comes into <w>force</w> on the first of January of next year."
                            }
                        },
                        {
                            "text": "Этот договор имеет юридическую <w>силу</w> во всех странах союза.",
                            "target_lang_translations": {
                                "en": "This treaty has legal <w>force</w> in all countries of the union."
                            }
                        }
                    ],
                    "synonyms": [
                        "правомочность"
                    ],
                    "common_phrases": [
                        "вступить в силу",
                        "иметь силу",
                        "законная сила",
                        "юридическая сила"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Legal validity or the binding effect of a document or law."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "force",
                                "target_lang_sense_clarification": "legal validity"
                            }
                        ]
                    }
                }
            ]
        },

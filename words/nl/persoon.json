                    "sense_id": "c443792b-e5f2-36a8-88bb-919097509484",
                    "sense_definition": "Een menselijk individu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "human_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is in deze auto plaats voor maximaal vijf <w>personen</w>.",
                            "target_lang_translations": {

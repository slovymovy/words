    "entries": [
        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "524cce95-887c-3137-98d4-07b4edea1014",
                    "sense_definition": "Большое, значительное количество кого-либо или чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У нас сегодня очень <много> работы.",
                            "target_lang_translations": {
                                "en": "We have so <much> work today."
                            }
                        },
                        {
                            "text": "На площади собралось <много> людей.",
                            "target_lang_translations": {
                                "en": "<Many> people gathered in the square."
                            }
                        },
                        {
                            "text": "В этом году в саду выросло <много> яблок.",
                            "target_lang_translations": {
                                "en": "<A lot of> apples grew in the garden this year."
                            }
                        }
                    ],
                    "synonyms": [
                        "немало",
                        "мо́ре",
                        "куры не клюю́т",
                        "дофига"
                    ],
                    "antonyms": [
                        "ма́ло",
                        "немного"
                    ],
                    "common_phrases": [
                        "много раз",
                        "много лет",
                        "очень много",
                        "так много",
                        "как много"
                    ],
                    "target_lang_definitions": {
                        "en": "A large number or amount of people or things."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "many",
                                "target_lang_sense_clarification": "used with countable nouns (e.g., people, apples)"
                            },
                            {
                                "target_lang_word": "much",
                                "target_lang_sense_clarification": "used with uncountable nouns (e.g., work, water)"
                            },
                            {
                                "target_lang_word": "a lot of",
                                "target_lang_sense_clarification": "general use, suitable for both countable and uncountable nouns"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "da99432a-0e4f-3239-b38e-137bbbbfcc19",
                    "sense_definition": "В значительной степени, интенсивно, часто или долго (о действии).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат <много> работает и редко отдыхает.",
                            "target_lang_translations": {
                                "en": "My brother works <a lot> and rarely rests."
                            }
                        },
                        {
                            "text": "Ты слишком <много> говоришь, но мало делаешь.",
                            "target_lang_translations": {
                                "en": "You talk too <much>, but do little."
                            }
                        }
                    ],
                    "synonyms": [
                        "о́чень ()",
                        "очень"
                    ],
                    "antonyms": [
                        "ма́ло"
                    ],
                    "common_phrases": [
                        "много знать",
                        "много думать",
                        "много читать",
                        "много спать"
                    ],
                    "target_lang_definitions": {
                        "en": "To a great extent, frequently, or for a long time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "a lot",
                                "target_lang_sense_clarification": "common adverb for frequency or intensity"
                            },
                            {
                                "target_lang_word": "much",
                                "target_lang_sense_clarification": "often used in negative sentences, questions, or with 'too/so'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e919206e-b52f-3ee3-8a6e-d0cf5201ed06",
                    "sense_definition": "Больше, чем нужно или желательно; чрезмерно.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "excess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пять тысяч рублей за такой обед — это <много>.",
                            "target_lang_translations": {
                                "en": "Five thousand rubles for such a lunch is <a lot>."
                            }
                        },
                        {
                            "text": "Для одного человека целого торта будет <много>.",
                            "target_lang_translations": {
                                "en": "A whole cake will be <too much> for one person."
                            }
                        }
                    ],
                    "synonyms": [
                        "чрезмерно",
                        "в избы́тке"
                    ],
                    "antonyms": [
                        "ма́ло",
                        "недостаточно"
                    ],
                    "common_phrases": [
                        "этого много",
                        "слишком много"
                    ],
                    "target_lang_definitions": {
                        "en": "A large amount; more than is reasonable or required."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "a lot",
                                "target_lang_sense_clarification": "indicating a large quantity"
                            },
                            {
                                "target_lang_word": "too much",
                                "target_lang_sense_clarification": "indicating an excessive amount"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cf954d4-1efe-3e95-852f-40268007e1a1",
                    "sense_definition": "Значительно, гораздо (используется для усиления сравнительной степени прилагательных или наречий).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "comparative_modifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот путь оказался <много> длиннее, чем мы ожидали.",
                            "target_lang_translations": {
                                "en": "This path turned out to be <much> longer than we expected."
                            }
                        },
                        {
                            "text": "Его новая книга <много> интереснее предыдущей.",
                            "target_lang_translations": {
                                "en": "His new book is <much> more interesting than the previous one."
                            }
                        }
                    ],
                    "synonyms": [
                        "гораздо",
                        "значительно"
                    ],
                    "antonyms": [
                        "незначительно"
                    ],
                    "common_phrases": [
                        "много лучше",
                        "много больше",
                        "много хуже"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often replaced by 'гораздо' in modern speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Significantly; used to emphasize a comparative adjective or adverb."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "much",
                                "target_lang_sense_clarification": "standard intensifier for comparatives"
                            },
                            {
                                "target_lang_word": "far",

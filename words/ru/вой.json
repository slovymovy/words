    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e886598f-5ab5-3153-9feb-57c1bdf5d6ad",
                    "sense_definition": "Продолжительный, протяжный звук, издаваемый некоторыми животными (волком, собакой и т. п.).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В лесу раздался страшный <w>вой</w> волков.",
                            "target_lang_translations": {
                                "en": "A terrible <w>howl</w> of wolves rang out in the forest."
                            }
                        },
                        {
                            "text": "Собачий <w>вой</w> не давал нам спать всю ночь.",
                            "target_lang_translations": {
                                "en": "The dog's <w>howling</w> kept us awake all night."
                            }
                        }
                    ],
                    "synonyms": [
                        "завывание"
                    ],
                    "antonyms": [
                        "писк"
                    ],
                    "common_phrases": [
                        "волчий вой",
                        "вой собаки",
                        "слышать вой"
                    ],
                    "target_lang_definitions": {
                        "en": "A prolonged, drawn-out sound made by certain animals (such as a wolf or dog)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "howl",
                                "target_lang_sense_clarification": "sound of a wolf or dog"
                            },
                            {
                                "target_lang_word": "howling",
                                "target_lang_sense_clarification": "continuous action or sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a010f298-aa78-3f5d-b851-cd79f38bb0ba",
                    "sense_definition": "Протяжные, заунывные или тревожные звуки, напоминающие крик животного (о ветре, сирене, механизмах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За окном стоял жуткий <w>вой</w> ветра.",
                            "target_lang_translations": {
                                "en": "There was a terrible <w>howling</w> of the wind outside the window."
                            }
                        },
                        {
                            "text": "Вдруг послышался громкий <w>вой</w> полицейской сирены.",
                            "target_lang_translations": {
                                "en": "Suddenly, the loud <w>wail</w> of a police siren was heard."
                            }
                        }
                    ],
                    "synonyms": [
                        "завывание"
                    ],
                    "common_phrases": [
                        "вой ветра",
                        "вой сирены",
                        "вой метели"
                    ],
                    "target_lang_definitions": {
                        "en": "Drawn-out, mournful, or alarming sounds resembling an animal's cry (referring to wind, sirens, or mechanisms)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "howl",
                                "target_lang_sense_clarification": "of wind"
                            },
                            {
                                "target_lang_word": "wail",
                                "target_lang_sense_clarification": "of a siren"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5abf8fde-3168-34dc-bf1b-f8f930adf1f0",
                    "sense_definition": "Громкий, протяжный плач или вопль человека.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_human",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Женщины подняли <w>вой</w>, узнав о случившемся.",
                            "target_lang_translations": {
                                "en": "The women raised a <w>wail</w> upon learning what had happened."
                            }
                        },
                        {
                            "text": "Её <w>вой</w> был слышен даже на улице.",
                            "target_lang_translations": {
                                "en": "Her <w>wailing</w> could be heard even on the street."
                            }
                        }
                    ],
                    "synonyms": [
                        "стон"
                    ],
                    "common_phrases": [
                        "поднять вой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Loud, prolonged weeping or wailing of a person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wailing",
                                "target_lang_sense_clarification": "loud crying"
                            },
                            {
                                "target_lang_word": "howling",
                                "target_lang_sense_clarification": "intense, loud crying"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdffb8a0-13e4-385b-96b6-09087a9440d7",
                    "sense_definition": "Шумное выражение недовольства, возмущения или протеста.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "protest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В прессе поднялся настоящий <w>вой</w> по поводу нового закона.",
                            "target_lang_translations": {
                                "en": "A real <w>outcry</w> arose in the press regarding the new law."
                            }
                        },
                        {
                            "text": "Не обращай внимания на этот <w>вой</w> недовольных.",
                            "target_lang_translations": {
                                "en": "Don't pay attention to this <w>howling</w> of the dissatisfied."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A noisy expression of dissatisfaction, indignation, or protest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "outcry",
                                "target_lang_sense_clarification": "public expression of protest"
                            },
                            {
                                "target_lang_word": "uproar",
                                "target_lang_sense_clarification": "noisy disturbance or complaint"
                            }
                        ]
                    }
                }
            ]
        },
        {

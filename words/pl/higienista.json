                {
                    "sense_id": "c60b02d1-6f28-3b3f-9235-c79876a23b85",
                    "sense_definition": "Specjalista w zakresie higieny, zajmujący się wpływem środowiska na zdrowie człowieka i profilaktyką chorób.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medical_profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W szpitalu zatrudniono nowego <w>higienistę</w>, aby poprawić standardy sanitarne.",
                            "target_lang_translations": {
                                "ru": "В больницу наняли нового <w>гигиениста</w>, чтобы улучшить санитарные стандарты."
                            }
                        },
                        {
                            "text": "Lekarz <w>higienista</w> przeprowadził kontrolę czystości w naszej stołówce.",
                            "target_lang_translations": {
                                "ru": "Врач-<w>гигиенист</w> провел проверку чистоты в нашей столовой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lekarz higienista",
                        "doświadczony higienista"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист в области гигиены, занимающийся влиянием окружающей среды на здоровье человека и профилактикой заболеваний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гигиенист",
                                "target_lang_sense_clarification": "специалист"
                            },
                            {
                                "target_lang_word": "врач-гигиенист",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf042880-4234-3e3a-91ab-a4403a659e27",
                    "sense_definition": "Osoba, która przesadnie dba o czystość i skrupulatnie przestrzega zasad higieny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat to straszny <w>higienista</w>, myje ręce kilkanaście razy dziennie.",
                            "target_lang_translations": {
                                "ru": "Мой брат — ужасный <w>чистюля</w>, он моет руки по несколько раз в день."
                            }
                        },
                        {
                            "text": "Nie bądź takim <w>higienistą</w>, odrobina kurzu na półce nikomu nie zaszkodzi.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>чистюлей</w>, немного пыли на полке никому не повредит."
                            }
                        }
                    ],
                    "synonyms": [
                        "czyścioch"
                    ],
                    "antonyms": [
                        "brudas"
                    ],
                    "common_phrases": [
                        "wielki higienista",
                        "przesadny higienista"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который чрезмерно заботится о чистоте и скрупулезно соблюдает правила гигиены."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чистюля",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "блюститель чистоты",
                                "target_lang_sense_clarification": "ироничное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

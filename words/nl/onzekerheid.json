    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f42ff378-a91d-3138-85ce-ac8fe400a005",
                    "sense_definition": "De toestand waarin men niet zeker is van een situatie, de toekomst of de waarheid; een gevoel van twijfel.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "situation_doubt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politieke crisis zorgde voor veel <w>onzekerheid</w> bij de bevolking.",
                            "target_lang_translations": {
                                "en": "The political crisis caused a lot of <w>uncertainty</w> among the population.",
                                "ru": "Политический кризис вызвал сильную <w>неопределённость</w> среди населения."
                            }
                        },
                        {
                            "text": "Na weken van <w>onzekerheid</w> kregen we eindelijk goed nieuws.",
                            "target_lang_translations": {
                                "en": "After weeks of <w>uncertainty</w>, we finally received good news.",
                                "ru": "После недель <w>неизвестности</w> мы наконец получили хорошие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "twijfeling"
                    ],
                    "antonyms": [
                        "zekerheid"
                    ],
                    "common_phrases": [
                        "in onzekerheid verkeren",
                        "een tijd van onzekerheid",
                        "totale onzekerheid"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of not being sure about a situation, the future, or the truth; a feeling of doubt.",
                        "ru": "Состояние неуверенности в ситуации, будущем или истине; чувство сомнения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "uncertainty",
                                "target_lang_sense_clarification": "general state of not knowing"
                            },
                            {
                                "target_lang_word": "doubt",
                                "target_lang_sense_clarification": "feeling of hesitation or lack of conviction"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "неопределённость",
                                "target_lang_sense_clarification": "отсутствие ясности в ситуации или будущем"
                            },
                            {
                                "target_lang_word": "неуверенность",
                                "target_lang_sense_clarification": "внутреннее чувство сомнения"
                            },
                            {
                                "target_lang_word": "неизвестность",
                                "target_lang_sense_clarification": "отсутствие информации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89fcac19-ac1e-303b-a962-89e401977967",
                    "sense_definition": "Een gebrek aan zelfvertrouwen of geloof in eigen kunnen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "personal_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Haar <w>onzekerheid</w> weerhield haar ervan om haar mening te geven.",
                            "target_lang_translations": {
                                "en": "Her <w>insecurity</w> kept her from giving her opinion.",
                                "ru": "Её <w>неуверенность</w> мешала ей высказать своё мнение."
                            }
                        },
                        {
                            "text": "Hij probeert zijn <w>onzekerheid</w> vaak te verbergen met humor.",
                            "target_lang_translations": {
                                "en": "He often tries to hide his <w>insecurity</w> with humor.",
                                "ru": "Он часто пытается скрыть свою <w>неуверенность</w> за юмором."
                            }
                        }
                    ],
                    "synonyms": [
                        "schuchterheid",
                        "bedeesdheid"
                    ],
                    "antonyms": [
                        "zekerheid"
                    ],
                    "common_phrases": [
                        "last hebben van onzekerheid",
                        "gevoelens van onzekerheid"
                    ],
                    "target_lang_definitions": {
                        "en": "A lack of self-confidence or belief in one's own abilities.",
                        "ru": "Недостаток уверенности в себе или веры в собственные силы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "insecurity",
                                "target_lang_sense_clarification": "lack of self-confidence"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "неуверенность",
                                "target_lang_sense_clarification": "в себе или своих силах"
                            },
                            {
                                "target_lang_word": "закомплексованность",
                                "target_lang_sense_clarification": "наличие комплексов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19a787d2-aaba-31d7-95fa-48d617efb09a",
                    "sense_definition": "De mate waarin een meting en de werkelijke waarde van elkaar kunnen afwijken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De meting is nauwkeurig met een <w>onzekerheid</w> van slechts één millimeter.",
                            "target_lang_translations": {
                                "en": "The measurement is accurate with an <w>uncertainty</w> of just one millimeter.",
                                "ru": "Измерение точное, с <w>погрешностью</w> всего в один миллиметр."
                            }
                        },
                        {
                            "text": "In het wetenschappelijke rapport werd de statistische <w>onzekerheid</w> duidelijk vermeld.",
                            "target_lang_translations": {
                                "en": "The statistical <w>uncertainty</w> was clearly stated in the scientific report.",
                                "ru": "В научном отчете была четко указана статистическая <w>погрешность</w>."

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8315bcb6-326e-347a-8480-f8da7084c798",
                    "sense_definition": "Weggaan van een plaats om ergens anders naartoe te reizen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De trein naar Amsterdam zal over vijf minuten <w>vertrekken</w>.",
                            "target_lang_translations": {
                                "en": "The train to Amsterdam will <w>depart</w> in five minutes.",
                                "ru": "Поезд в Амстердам <w>отправится</w> через пять минут."
                            }
                        },
                        {
                            "text": "Wij zijn gisteren al vroeg in de ochtend <w>vertrokken</w>.",
                            "target_lang_translations": {
                                "en": "We <w>left</w> early yesterday morning.",
                                "ru": "Мы <w>выехали</w> вчера рано утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "weggaan",
                        "afreizen",
                        "opstappen"
                    ],
                    "antonyms": [
                        "aankomen"
                    ],
                    "common_phrases": [
                        "op tijd vertrekken",
                        "met de trein vertrekken",
                        "naar huis vertrekken"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave a place to travel to another destination.",
                        "ru": "Уезжать или уходить из одного места, чтобы отправиться в другое."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "depart",
                                "target_lang_sense_clarification": "formal or transport context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отправляться",
                                "target_lang_sense_clarification": "начинать путь"
                            },
                            {
                                "target_lang_word": "уезжать",
                                "target_lang_sense_clarification": "покидать место"
                            },
                            {
                                "target_lang_word": "отходить",
                                "target_lang_sense_clarification": "о транспорте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9b3cc3b-baff-3b07-ab7d-d299e86ae991",
                    "sense_definition": "Stoppen met het uitoefenen van een functie of baan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "employment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur gaat na tien jaar dienst bij het bedrijf <w>vertrekken</w>.",
                            "target_lang_translations": {
                                "en": "The director is going to <w>leave</w> the company after ten years of service.",
                                "ru": "Директор собирается <w>покинуть</w> компанию после десяти лет службы."
                            }
                        },
                        {
                            "text": "Hij is <w>vertrokken</w> omdat hij een betere baan heeft gevonden.",
                            "target_lang_translations": {
                                "en": "He <w>left</w> because he found a better job.",
                                "ru": "Он <w>ушел</w>, потому что нашел работу получше."
                            }
                        }
                    ],
                    "synonyms": [
                        "aftreden",
                        "opstappen",
                        "ontslag nemen"
                    ],
                    "common_phrases": [
                        "bij een bedrijf vertrekken"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop holding a position or job.",
                        "ru": "Прекращать выполнение обязанностей на должности или работе."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "leave",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "resign",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "с работы или должности"
                            },
                            {
                                "target_lang_word": "увольняться",
                                "target_lang_sense_clarification": "официально прекращать работу"
                            },
                            {
                                "target_lang_word": "покидать",
                                "target_lang_sense_clarification": "оставлять компанию или пост"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b0d0fe1-5973-382a-a759-399fd4f8de40",
                    "sense_definition": "Het veranderen van de gezichtsuitdrukking, meestal door pijn of een sterke emotie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn gezicht <w>vertrok</w> van woede toen hij het nieuws hoorde.",
                            "target_lang_translations": {
                                "en": "His face <w>contorted</w> with anger when he heard the news.",
                                "ru": "Его лицо <w>исказилось</w> от гнева, когда он услышал новости."
                            }
                        },
                        {
                            "text": "Zij luisterde naar het vonnis zonder een spier te <w>vertrekken</w>.",
                            "target_lang_translations": {
                                "en": "She listened to the verdict without <w>moving</w> a muscle.",
                                "ru": "Она слушала приговор, и ни один мускул на её лице не <w>дрогнул</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "geen spier vertrekken",
                        "van pijn vertrekken"
                    ],
                    "target_lang_definitions": {
                        "en": "To change facial expression, usually due to pain or strong emotion.",
                        "ru": "Изменение выражения лица, обычно из-за боли или сильной эмоции."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "contort",
                                "target_lang_sense_clarification": "of the face"
                            },
                            {
                                "target_lang_word": "twist",
                                "target_lang_sense_clarification": "facial expression"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "искажаться",
                                "target_lang_sense_clarification": "о чертах лица"
                            },
                            {
                                "target_lang_word": "перекашиваться",
                                "target_lang_sense_clarification": "разг. о лице"
                            }
                        ]
                    }
                }
            ]
